{
    "T01n889u_001:0": " mañjuvajrastotram",
    "T01n889u_001:1": " mañjugarbhakṛtam",
    "T01n889u_001:2": " namaḥ śrīmañjuvajrāya",
    "T01n889u_001:3": " śaśadharamiva śubhraṃ khaḍgapustākapāṇiṃ",
    "T01n889u_001:4": " suruciramaliśāntaṃ pañcacīraṃ kumāram |",
    "T01n889u_001:5": " pṛthutaravaramokṣaṃ padmapatrāyatākṣaṃ",
    "T01n889u_001:6": " kumatidahanadakṣaṃ mañjughoṣaṃ namāmi || 1 ||",
    "T01n889u_001:7": " kṛtamṛgaripuyānaṃ dattabhaktapradānaṃ",
    "T01n889u_001:8": " suradanujanṛyānaṃ bodhisattvapradhānam |",
    "T01n889u_001:9": " akhilaguṇanidhānaṃ sarvavidyāvitānaṃ",
    "T01n889u_001:10": " karasarasijabāṇaṃ mañjughoṣaṃ namāmi || 2 ||",
    "T01n889u_001:11": " vibhṛtasakalakoṣaṃ kṣālitājñānadoṣaṃ",
    "T01n889u_001:12": " smaraṇabhajanatoṣaṃ dūrarāgādidoṣam |",
    "T01n889u_001:13": " vihitasasurapoṣaṃ siddhidākhyānayoṣaṃ",
    "T01n889u_001:14": " kṛtajaḍapariśoṣaṃ mañjughoṣaṃ namāmi || 3 ||",
    "T01n889u_001:15": " gaṇapatiśarajanmaśrīmahākālasiṃhaiḥ",
    "T01n889u_001:16": " parivṛtamiva candrābhābhamindīvarākṣam |",
    "T01n889u_001:17": " asi-śara-japamālā-pustakaṃ saṃvahantam",
    "T01n889u_001:18": " urasi lalitamālaṃ mañjughoṣaṃ namāmi || 4 ||",
    "T01n889u_001:19": " surapatiśamanāyāpyeṣa mitrāgnirakṣaḥ-",
    "T01n889u_001:20": " pavanapramathapālaiḥ saṃvṛtaṃ smeravaktram |",
    "T01n889u_001:21": " khagapatirathagātraṃ brahmavandyaṃ ramomā-",
    "T01n889u_001:22": " vihitacaraṇabhaktaṃ mañjughoṣaṃ namāmi || 5 ||",
    "T01n889u_001:23": " yadasikaṭhinadhārācchedamārgābhivāhā (haḥ)",
    "T01n889u_001:24": " sakalasaliladhārāpātitāṅgāṅgamantraḥ |",
    "T01n889u_001:25": " bahuparicayasthalyāṃ bhūminadyāpi reje",
    "T01n889u_001:26": " bahutaramahimānaṃ mañjughoṣaṃ namāmi || 6 ||",
    "T01n889u_001:27": " bhavadabhinavanutyā toṣitā guhyadevī",
    "T01n889u_001:28": " nikhilanigamasārā suprakāśā'tireje |",
    "T01n889u_001:29": " bhavajalanidhipāraṃ dānakalpadrumāgraṃ",
    "T01n889u_001:30": " galitabahumahograṃ mañjughoṣaṃ namāmi || 7 ||",
    "T01n889u_001:31": " vibhajati janaloko dharmadhātuṃ maheśaṃ",
    "T01n889u_001:32": " daśaśatadalapadme saṃsthitaṃ jyotiraiśam |",
    "T01n889u_001:33": " tadapi tava praśastaṃ devamāhātmyamīśaṃ",
    "T01n889u_001:34": " vibhajati bhujageśaṃ mañjughoṣaṃ namāmi || 8 ||",
    "T01n889u_001:35": " paṭhati yadidamiṣṭaṃ mālinīpadyabandhaṃ",
    "T01n889u_001:36": " sa bhavati kavirājo vādisiṃhāsanasthaḥ |",
    "T01n889u_001:37": " sakalavisabhāsu projjvaladvāksudhāraḥ",
    "T01n889u_001:38": " kalitasakalavidyo bhūṣaṇo bhavyadakṣaḥ || 9 ||",
    "T01n889u_001:39": " svayambhūpurāṇoddhṛtaṃ mañjugarbhaviracittaṃ",
    "T01n889u_001:40": " mañjuvajrastotraṃ samāptam |"
}