{
    "T06bsa065_u:0": " ācārya vasubandhuviracitam Pañcaskandhaprakaraṇam",
    "T06bsa065_u:1": " prathamādhikāraḥ - skandhoddeśanirdeśakaḥ",
    "T06bsa065_u:2": " pañcaskandhāḥ",
    "T06bsa065_u:3": " rūpaskandhaḥ vedanāskandhaḥ saṃjñāskandhaḥ saṃskāraskandhaḥ vijñānaskandhaśca // 1 //",
    "T06bsa065_u:4": " rūpasya dvau bhedau",
    "T06bsa065_u:5": " rūpaṃ katamat /",
    "T06bsa065_u:6": " yat kiñcid rūpaṃ sarvaṃ taccatvāri mahābhūtāni catvāri ca mahābhūtānyupādāya // 2 //",
    "T06bsa065_u:7": " mahābhūtanāmāni",
    "T06bsa065_u:8": " katamāni catvāri mahābhūtāni /",
    "T06bsa065_u:9": " pṛthivī dhātuḥ abdhātuḥ tejodhātuḥvāyudhātuśca // 3 //",
    "T06bsa065_u:10": " mahābhūtalakṣaṇāni",
    "T06bsa065_u:11": " tatra pṛthivīdhātuḥ katamaḥ / kaṭhinatā // 4 //",
    "T06bsa065_u:12": " abdhātuḥ katamaḥ /",
    "T06bsa065_u:13": " niṣyandatā // 5 //",
    "T06bsa065_u:14": " tejodhātuḥ katamaḥ uṣṇatā // 6 //",
    "T06bsa065_u:15": " vāyudhātuḥ katamaḥ /",
    "T06bsa065_u:16": " paunaḥ punyakampanatā // 7 //",
    "T06bsa065_u:17": " upādāyarūpanāmāni",
    "T06bsa065_u:18": " upādayarūpaṃ katamat /",
    "T06bsa065_u:19": " cakṣurindriyam,",
    "T06bsa065_u:20": " śrotrendriyam,",
    "T06bsa065_u:21": " ghraṇendriyam,",
    "T06bsa065_u:22": " jihvendriyam,",
    "T06bsa065_u:23": " kāyendriyam,",
    "T06bsa065_u:24": " rūpam,",
    "T06bsa065_u:25": " śabdaḥ,",
    "T06bsa065_u:26": " gandhaḥ,",
    "T06bsa065_u:27": " rasaḥ,",
    "T06bsa065_u:28": " spraṣṭavyaikadeśaḥ,",
    "T06bsa065_u:29": " abijñaptiśca // 8 //",
    "T06bsa065_u:30": " upādāyarūpalakṣaṇāni cakṣurindrayaṃ katamat /",
    "T06bsa065_u:31": " varṇabiṣayo rūpaprasādaḥ // 9 //",
    "T06bsa065_u:32": " śrotrendrayaṃ katamat /",
    "T06bsa065_u:33": " śabdaviṣayo rūpaprasādaḥ // 10 //",
    "T06bsa065_u:34": " ghrāṇendriyam katamat /",
    "T06bsa065_u:35": " gandhaviṣayo rūpaprasādaḥ // 11 //",
    "T06bsa065_u:36": " jihvendriyam katamat /",
    "T06bsa065_u:37": " rasaviṣayo rūpaprasādaḥ // 12 //",
    "T06bsa065_u:38": " kāyendriyam katamat /",
    "T06bsa065_u:39": " spraṣṭavyaviṣayo rūpaprasādaḥ // 13 //",
    "T06bsa065_u:40": " rūpaṃ katamat /",
    "T06bsa065_u:41": " cakṣurgocaraḥ /",
    "T06bsa065_u:42": " varṇaḥ saṃsthānam bijñaptiśca // 14 //",
    "T06bsa065_u:43": " śabdaḥ katamaḥ /",
    "T06bsa065_u:44": " śrotragocaraḥ /",
    "T06bsa065_u:45": " niṣpannḥ aniṣpannaḥ ubhayaḥ /",
    "T06bsa065_u:46": " upāttacatumarhābhutahetukaḥ anupāttacaturmahābhūtahetukaḥ ubhayacaturmahābhūtahetukaśca // 15 //",
    "T06bsa065_u:47": " gandhaḥ katamaḥ /",
    "T06bsa065_u:48": " ghrāṇagocaraḥ /",
    "T06bsa065_u:49": " surabhiḥ asurabhiḥ tadanyaśca // 16 //",
    "T06bsa065_u:50": " rasaḥ katamaḥjihvāgocaraḥ /",
    "T06bsa065_u:51": " madhuraḥ amalaḥ lavaṇaḥ kaṭuḥ tiktaḥ kaṣāyaśc // 17 //",
    "T06bsa065_u:52": " spraṣṭavyaḥ katamaḥ /",
    "T06bsa065_u:53": " kāyagocaraḥ /",
    "T06bsa065_u:54": " mahābhūtānyupādāya ślakṣṇatvam karkasatbam gurutvam laghutvam śītatvam jighatsā pipāsā ca // 18 //",
    "T06bsa065_u:55": " abijñaptirūpaṃ katamat /",
    "T06bsa065_u:56": " vijñaptisamādhisaṃbhūtaṃ rūpam anidarśanam apratigham // 19 //",
    "T06bsa065_u:57": " rūpaskandhaḥ soniddeśaḥ pariniṣṭhitaḥ /",
    "T06bsa065_u:58": " atha vedanā salakṣaṇaprabhedā vedanā katamā /",
    "T06bsa065_u:59": " trividho 'nubhavaḥ /",
    "T06bsa065_u:60": " sukhaḥ duḥkhaḥ aduḥkhāsukhaśca // 20 //",
    "T06bsa065_u:61": " sukho yasmin niruddhe saṃyogecchā jāyate // 21 //",
    "T06bsa065_u:62": " duḥkho yasminnutpanne viyogecchā jāyate // 22 //",
    "T06bsa065_u:63": " aduḥkhāsukho yasminnutpanne nobhayecchā jāyane // 23 //",
    "T06bsa065_u:64": " vedanaskandhaḥ sonideśanirdeśaḥ parinisṭhataḥ /",
    "T06bsa065_u:65": " atha saṃjñā lakṣaṇa prabhedā saṃjñā katamā /",
    "T06bsa065_u:66": " viṣayanimittod grahaṇam // 24 //",
    "T06bsa065_u:67": " sā tridhā parittā mahadgatā apramāṇā ca // 25 //",
    "T06bsa065_u:68": " saṃjñā skandhaḥ soddeśanirdeśaḥ parinisṭhitaḥ /",
    "T06bsa065_u:69": " saṃskārāṇāṃ dvaubhedau",
    "T06bsa065_u:70": " saṃskārāḥ katame /",
    "T06bsa065_u:71": " vedanāsaṃjñābhyāmanye caitāḥ cittaviprayuktāśca // 26 //",
    "T06bsa065_u:72": " tatra caitadharmāḥ katame /",
    "T06bsa065_u:73": " ye dharmārścittena saṃprayuktāḥ // 27 //",
    "T06bsa065_u:74": " caitaparisaṃkhyānam",
    "T06bsa065_u:75": " te katame / sparśaḥ manaskāraḥ vedanā saṃjñā cetanā chandaḥ adhimokṣaḥ smṛtiḥ samādhiḥ",
    "T06bsa065_u:76": " prajñā śraddhā hrīḥ apatrapā alobhaḥ kuśalamūlam adveṣaḥkuśalamūlam amohaḥkuśalamūlam",
    "T06bsa065_u:77": " vīryam praśrabdhiḥ apramādaḥ upekṣā avihiṃsā rāga pratighaḥ mānaḥ avidyā dṛṣṭiḥ vicikitsā",
    "T06bsa065_u:78": " krodha upanāhaḥ mrakṣaḥ pradāśaḥ īrṣyā mātsaryarm māyā śāṭhyam madaḥ ahiṃsā āhrikyam",
    "T06bsa065_u:79": " anapatrāpyam styānam,",
    "T06bsa065_u:80": " auddhatyam,",
    "T06bsa065_u:81": " āśraddhyam,",
    "T06bsa065_u:82": " kauśīddhyam pramādaḥ muṣitāsmṛti,",
    "T06bsa065_u:83": " bikṣepaḥ,",
    "T06bsa065_u:84": " aprasajanyam,",
    "T06bsa065_u:85": " kaukrityam,",
    "T06bsa065_u:86": " middham,",
    "T06bsa065_u:87": " vitarkaḥ, vicāraśca // 28 //",
    "T06bsa065_u:88": " eteśāṃ paśca sarvatragāḥ /",
    "T06bsa065_u:89": " pañcapratiniyataviṣayā /",
    "T06bsa065_u:90": " ekādaśakuśalāḥ /",
    "T06bsa065_u:91": " ṣaṭkleśāḥ /",
    "T06bsa065_u:92": " śiṣṭā upakleśāḥ /",
    "T06bsa065_u:93": " catvāro 'nyatrāpi bhavantītyaniyatāḥ // 29 //",
    "T06bsa065_u:94": " caittalakṣaṇāni",
    "T06bsa065_u:95": " sparśaḥkatamaḥ /",
    "T06bsa065_u:96": " trikasaṃnipāte indriyavikāraparicchedaḥ // 30 //",
    "T06bsa065_u:97": " manaskāraḥ katamaḥ /",
    "T06bsa065_u:98": " cetasa ābhogaḥ // 31 //",
    "T06bsa065_u:99": " cetanā katamā /",
    "T06bsa065_u:100": " guṇadoṣānubhayeṣu cittābhisaṃskāro manaskarma // 32 //",
    "T06bsa065_u:101": " chandaḥ katamaḥ /",
    "T06bsa065_u:102": " abhiprete vastunyabhilāṣaḥ // 33 //",
    "T06bsa065_u:103": " adhimokṣaḥ katamaḥ /",
    "T06bsa065_u:104": " niścite vastuni tathaivāvadhāraṇam // 34 //",
    "T06bsa065_u:105": " smṛtiḥ katamā /",
    "T06bsa065_u:106": " saṃstute vastunyasaṃpramoṣaḥ // 35 //",
    "T06bsa065_u:107": " samādhiḥ katamaḥ /",
    "T06bsa065_u:108": " upaparīkṣye vastuni cittasyaikāgratā // 36 //",
    "T06bsa065_u:109": " prajñā katamā /",
    "T06bsa065_u:110": " tatraiva pravicayo yogāyogavihito 'nyathā ca // 37 //",
    "T06bsa065_u:111": " śraddā katamā /",
    "T06bsa065_u:112": " karmaphalāsatyaratneṣvabhisaṃpratyayaḥ /",
    "T06bsa065_u:113": " cetaso 'bhilāṣaḥ cetasaḥ prasādaḥ // 38 //",
    "T06bsa065_u:114": " hrīḥ / katamā ātmānaṃ dharmaṃ vādhipatiṃ kṛtvā svayamavayena lajjā // 39 //",
    "T06bsa065_u:115": " apatrapā katamā /",
    "T06bsa065_u:116": " lokamadhipatiṃ kṛtvā parato 'vadhena lajjā // 40 //",
    "T06bsa065_u:117": " alobhaḥ katamaḥ /",
    "T06bsa065_u:118": " lobha pratipakṣaḥ /",
    "T06bsa065_u:119": " cetaso dānakṣetrāparigrahaḥ // 41 //",
    "T06bsa065_u:120": " adveṣaḥ katamaḥ /",
    "T06bsa065_u:121": " dveṣapratipakṣaḥ / maitri // 42 //",
    "T06bsa065_u:122": " amohaḥ katamaḥ /",
    "T06bsa065_u:123": " mohapratipakṣaḥ /",
    "T06bsa065_u:124": " yathābhūtasaṃpratipattiḥ // 43 //",
    "T06bsa065_u:125": " vīryaṃ katamat kausīddyapratipakṣaḥ kuśale cetaso 'bhyutsāhaḥ // 44 //",
    "T06bsa065_u:126": " praśrabdhiḥ katamā /",
    "T06bsa065_u:127": " dauṣṭhulyapratipakṣaḥ kāyacittakarmaṇyatā // 45 //",
    "T06bsa065_u:128": " apramādaḥ katamaḥ /",
    "T06bsa065_u:129": " pramādapratipakṣaḥ /",
    "T06bsa065_u:130": " alobhādīn vīryāntān niśritya akuśalānāṃ dharmāṇāṃ prahāṇam /",
    "T06bsa065_u:131": " tatpratipakṣāṇāṃ kuśalānāṃ dharmāṇāṃ bhāvanā // 46 //",
    "T06bsa065_u:132": " upekṣā katamā /",
    "T06bsa065_u:133": " alobhādīn vīryantān niśritya cittasamatā /",
    "T06bsa065_u:134": " cittapraśastā /",
    "T06bsa065_u:135": " cittānābhogatā /",
    "T06bsa065_u:136": " yayākliṣṭadharmānavakāśenākliṣṭeṣu sthitiḥ // 47 //",
    "T06bsa065_u:137": " avihiṃsā katamā /",
    "T06bsa065_u:138": " vihiṃsāpratipakṣaḥ karuṇā // 48 ///",
    "T06bsa065_u:139": " rāgaḥ katamaḥ /",
    "T06bsa065_u:140": " upādānaskandheṣvabhyarthanā /",
    "T06bsa065_u:141": " adhyavasānam // 49 //",
    "T06bsa065_u:142": " pratighaḥ katamaḥ /",
    "T06bsa065_u:143": " satveṣvāghātacittatā // 50 //",
    "T06bsa065_u:144": " mānaḥ katamaḥ /",
    "T06bsa065_u:145": " mānaḥ saptavidhaḥ /",
    "T06bsa065_u:146": " atimānaḥ, mānātimānaḥ asmimānaḥ,",
    "T06bsa065_u:147": " abhimānaḥ,",
    "T06bsa065_u:148": " ūnamānaḥ,",
    "T06bsa065_u:149": " mithyāmānaśca // 51 //",
    "T06bsa065_u:150": " atimānaḥ katamaḥ /",
    "T06bsa065_u:151": " hīnāt śreyānahamasmi sadṛśena vā sadṛśo 'smīti cintayato yā cittasyonnatiḥ // 53 //",
    "T06bsa065_u:152": " mānātimānaḥ katamaḥ /",
    "T06bsa065_u:153": " śreyaso 'hameva śreyāniti cintayato yā cittasyonnatiḥ // 54 //",
    "T06bsa065_u:154": " asmimānaḥ katamaḥ /",
    "T06bsa065_u:155": " pañcasūpādānaskandheṣvātmānamātmīyaṃ vā paśyato yā cittasyonnati // 55 //",
    "T06bsa065_u:156": " abhimānaḥ katamaḥ /",
    "T06bsa065_u:157": " aprāpta uttare viśeṣādhigame prāpto mayeti cintayato yā cittasyonnatiḥ // 56 //",
    "T06bsa065_u:158": " ūnamānaḥ katamaḥ /",
    "T06bsa065_u:159": " bahṛvantaraviśiṣṭādalpāntarahino 'smīti cintayato yā cittasyonnatiḥ // 57 //",
    "T06bsa065_u:160": " mithyāmānaḥ katamaḥ /",
    "T06bsa065_u:161": " aguṇavato guṇavānahamasmīti cintayato yā cittasyonnatiḥ // 58 //",
    "T06bsa065_u:162": " aviddhyā katamā /",
    "T06bsa065_u:163": " karmaphalasatyaratnānāmajñānam /",
    "T06bsa065_u:164": " sā punaḥ sahajā parikalpitā ca // 59 //",
    "T06bsa065_u:165": " rāgacaritasya rāgapratighayoḥ rāgacaritasyāviddyāyāścetyakuśalānāṃ trīṇī mūlāni /",
    "T06bsa065_u:166": " lobho 'kuśalamūlaṃ ddheṣo 'kuśalamūlaṃ,",
    "T06bsa065_u:167": " moho 'kuśalamūlaṃ ca // 60 //",
    "T06bsa065_u:168": " dṛṣṭiḥ katamā pañcadṛṣṭyaḥ - satkāyadṛṣṭiḥ,",
    "T06bsa065_u:169": " antargrāhyadṛṣṭiḥ,",
    "T06bsa065_u:170": " mithyādṛṣṭiḥ,",
    "T06bsa065_u:171": " dṛṣṭiparāmarśaḥ,",
    "T06bsa065_u:172": " śīlavrataparāmarśaśca // 61 //",
    "T06bsa065_u:173": " satkāyadṛṣṭiḥ katamā /",
    "T06bsa065_u:174": " pañcopādāna - skandhānātmata ātmīyato vā paśyato yā kliṣṭā prajñā // 62 //",
    "T06bsa065_u:175": " antargrāhyadṛṣṭiḥ katamā /",
    "T06bsa065_u:176": " tānevādhipatiṃ kṛtvā śāśvatata ucchedato vā paśyato yā kliṣṭā prajñā // 63 //",
    "T06bsa065_u:177": " mithyādṛṣṭiḥ katamā /",
    "T06bsa065_u:178": " hetuṃ vā phalaṃ vā kriyāṃ vāpavadataḥ sadāvastu vināśayato yā kliṣṭā prajñā // 64 //",
    "T06bsa065_u:179": " dṛṣṭiparāmarśaḥ katamaḥ /",
    "T06bsa065_u:180": " tā eva tikhro dṛṣṭistadāśrayāṃśca skandhān śreṣṭhataḥ paramataśca paśyato yā kliṣṭā prajñā // 65 //",
    "T06bsa065_u:181": " śīlabrataparāmarśaḥ katamaḥ /",
    "T06bsa065_u:182": " śīlaṃ vrataṃ tadāśrayāṃśca skandhānaśuddhito muktito naiyāṇikataśca paśyato yā kliṣṭā prajñā // 66 //",
    "T06bsa065_u:183": " vicikitsā katamā /",
    "T06bsa065_u:184": " satyādiṣu yā dvikoṭikā matiḥ // 67 //",
    "T06bsa065_u:185": " eteṣāṃ kleśānāṃ paścimāstistro dṛṣṭayaḥ vīcikitsā ca parikalpitāḥ /",
    "T06bsa065_u:186": " śiṣṭāḥ sahajāḥ parikalpitāśca // 68 //",
    "T06bsa065_u:187": " krodhaḥ katamaḥ /",
    "T06bsa065_u:188": " vartaṃmānamapakāramāgamya yā cetasa āghātacetanā // 69 //",
    "T06bsa065_u:189": " upanāhaḥ katamaḥ /",
    "T06bsa065_u:190": " vairānubandha // 70 //",
    "T06bsa065_u:191": " mrakṣaḥ katamaḥ ātmano 'vaddhapracchādanā // 71 //",
    "T06bsa065_u:192": " pradāśaḥ katamaḥ caṇḍavacodāśitā // 72 //",
    "T06bsa065_u:193": " īrṣyā katamā parasaṃpattau cetaso vyāropa // 73 //",
    "T06bsa065_u:194": " mātsarya katamat /",
    "T06bsa065_u:195": " dānavirodhī cetasa āgrahaḥ // 74 //",
    "T06bsa065_u:196": " māyā katamā /",
    "T06bsa065_u:197": " paravaścanābhūtārthasadarśanatā // 75 //",
    "T06bsa065_u:198": " śāṭhyaṃ katamat /",
    "T06bsa065_u:199": " svadoṣapracchādanopāyasaṃgṛhitaṃ cetasaḥ kauṭilyam // 76 //",
    "T06bsa065_u:200": " madaḥ katamaḥ /",
    "T06bsa065_u:201": " svasaṃpattau raktasyoddharṣaḥ /",
    "T06bsa065_u:202": " cetasaḥ paryādānam // 77 //",
    "T06bsa065_u:203": " vihiṃsā katamā /",
    "T06bsa065_u:204": " satveṣu viheṭhanā // 78 //",
    "T06bsa065_u:205": " āhrīkyaṃ katamat /",
    "T06bsa065_u:206": " avadyena svayamalajjā // 79 //",
    "T06bsa065_u:207": " anapatrāp katamat /",
    "T06bsa065_u:208": " avadyena parato 'lajjā // 80",
    "T06bsa065_u:209": " satyānaṃ katamat /",
    "T06bsa065_u:210": " cittākarmaṇyatā /",
    "T06bsa065_u:211": " staimityam // 81 //",
    "T06bsa065_u:212": " auddhatyaṃ katamat /",
    "T06bsa065_u:213": " cittasyāvyupasamaḥ // 82 //",
    "T06bsa065_u:214": " āśraddhyaṃ katamat /",
    "T06bsa065_u:215": " karmaphalasatyaratneṣvanabhisaṃpratyayaḥ /",
    "T06bsa065_u:216": " śraddhāvipakṣaścetaso 'prasādaḥ // 83 //",
    "T06bsa065_u:217": " kauśiddyaṃ katamatṃḥ kuśale cetaso 'nabhyutsāho vīryavipakṣa // 84 //",
    "T06bsa065_u:218": " pramādaḥ katamaḥ /",
    "T06bsa065_u:219": " yā lobhadveṣamohekauśīddyaiścitasyānārakṣā kuśalasyābhāvanā // 85 //",
    "T06bsa065_u:220": " muṣitāsmṛtiḥ katamā /",
    "T06bsa065_u:221": " kliṣṭāsmṛtiḥ /",
    "T06bsa065_u:222": " kuśalāpratipatiḥ // 86 //",
    "T06bsa065_u:223": " vikṣepaḥ katamaḥ /",
    "T06bsa065_u:224": " rāgadveṣamohāṃśikaḥ pañcakāmaguṇeṣu cittasya visāraḥ // 87 //",
    "T06bsa065_u:225": " aprasajanyaṃ katamat kleśasaṃprayuktā prajñā /",
    "T06bsa065_u:226": " tayā kāyavākcittacaryā asaṃviditeva pravartate // 88 //",
    "T06bsa065_u:227": " kaukṛtyaṃ katamat /",
    "T06bsa065_u:228": " cetaso vipratisāraḥ // 89 //",
    "T06bsa065_u:229": " middhaṃ katamat /",
    "T06bsa065_u:230": " asvataṃtravṛtticetaso 'bhisaṃkṣepaḥ // 90 //",
    "T06bsa065_u:231": " vitarkaḥ katamaḥ paryeṣako manojalpaḥ cetanāprajñāviśeṣaḥ yā cittasyaudārikatā // 91 //",
    "T06bsa065_u:232": " vicāraḥ katamaḥ /",
    "T06bsa065_u:233": " paryeṣako manojalpaḥ tathaiva yā cittasya sūkṣmatā // 92 //",
    "T06bsa065_u:234": " // ittaviprayuktaparisaṃkhyānam //",
    "T06bsa065_u:235": " cittaviprayuttasaṃskārāḥ katame /",
    "T06bsa065_u:236": " ye rūpacitacaittadhikāre prajñaptāḥ,",
    "T06bsa065_u:237": " ta evānyatrāprajñaptāḥ // 93 //",
    "T06bsa065_u:238": " te katame / prāpti,",
    "T06bsa065_u:239": " asaṃjñisamāpattiḥ,",
    "T06bsa065_u:240": " nirodhasamāpattiḥ,",
    "T06bsa065_u:241": " āsaṃjñikam,",
    "T06bsa065_u:242": " jīvitendriyam,",
    "T06bsa065_u:243": " nikāyasabhāgatā,",
    "T06bsa065_u:244": " jāti,",
    "T06bsa065_u:245": " jarā,",
    "T06bsa065_u:246": " sthitiḥ,",
    "T06bsa065_u:247": " anityatā,",
    "T06bsa065_u:248": " nāmakāyaḥ,",
    "T06bsa065_u:249": " padakāyaḥ,",
    "T06bsa065_u:250": " vyañjanakāyaḥ,",
    "T06bsa065_u:251": " pṛthagjanatvamityevamādibhedasamādānāḥ // 94 //",
    "T06bsa065_u:252": " ittaviprayuktalakṣaṇāni //",
    "T06bsa065_u:253": " tatra prāptiḥ katamā /",
    "T06bsa065_u:254": " pratilambhaḥ samanvāgamaśca /",
    "T06bsa065_u:255": " bījam, vaśitā,",
    "T06bsa065_u:256": " abhimukhībhāvaśca yathāyogam // 95 //",
    "T06bsa065_u:257": " asaṃjñisamāpattiḥ katamṃ /",
    "T06bsa065_u:258": " śubhakṛtsnavītarāgasyoparyavītarāgasya niḥsaraṇasaṃjñāpūrvakeṇa manasikāreṇāvasthāvarāṇāṃ cittacaittadharmāṇāṃ yo nirodhaḥ // 96 //",
    "T06bsa065_u:259": " nirodhasamāpattiḥ katamā /",
    "T06bsa065_u:260": " ākiñcanyāyatanabītarāgasya bhavāgratāduccalitasya śāntavihāra - saṃjñāpūrvakeṇa manasikāreṇāsthāvarāṇāṃ cittacaittadharmāṇāṃ tadekatyānāṃ ca sthāvarāṇāṃ yo nirodhaḥ // 97 //",
    "T06bsa065_u:261": " āsaṃjñikaṃ katamat /",
    "T06bsa065_u:262": " asaṃjñisatvanikāyadeveṣupapannasyāsthāvarāṇāṃ cittacaittadharmāṇāṃ nirodhaḥ // 98 //",
    "T06bsa065_u:263": " jīvitendriyam katamat /",
    "T06bsa065_u:264": " nikāyasabhāgeṣu pūrvakarmānuviddhyeṣu yaḥ saṃskārāṇāṃ sthitikālaniyamaḥ // 99 //",
    "T06bsa065_u:265": " nikāyasabhāgaḥ katamaḥ /",
    "T06bsa065_u:266": " yā satvānāmātmabhāvasadṛśatā // 100 //",
    "T06bsa065_u:267": " jātiḥkatamā /",
    "T06bsa065_u:268": " nikāyasabhāge saṃskārāṇāmabhūtvā yo bhāṣaḥ // 101 //",
    "T06bsa065_u:269": " jarā katamā /",
    "T06bsa065_u:270": " tathā teṣāṃ prabandhānyathātvam // 102 //",
    "T06bsa065_u:271": " sthiti katamā /",
    "T06bsa065_u:272": " tathā teṣāṃ prabandhanvayaḥ // 103 //",
    "T06bsa065_u:273": " anityatā katamā /",
    "T06bsa065_u:274": " tathā teṣāṃ prabandhavināśaḥ // 104 //",
    "T06bsa065_u:275": " nāmakāyaḥ katamaḥ /",
    "T06bsa065_u:276": " dharmāṇāṃ svabhāvādhivacanam // 105 //",
    "T06bsa065_u:277": " pādakāyaḥ katamaḥ /",
    "T06bsa065_u:278": " dharmāṇāṃ viśeṣādhivacanam // 106 //",
    "T06bsa065_u:279": " vyañjanakāyaḥ katamaḥ /",
    "T06bsa065_u:280": " akṣarāṇi /",
    "T06bsa065_u:281": " tadubhayābhivyañjanatāmupādāya // 107 //",
    "T06bsa065_u:282": " vākyāni tāni /",
    "T06bsa065_u:283": " nāmapadāśreyeṇārthavacanatāmupādāya // 108 //",
    "T06bsa065_u:284": " akṣaraṃ punaḥ paryāyākṣaraṇatāmupādāya // 109 //",
    "T06bsa065_u:285": " pṛthagjanatvaṃ katamat /",
    "T06bsa065_u:286": " āryadharmāṇāmapratilambhaḥ // 110 //",
    "T06bsa065_u:287": " iti saṃskāraskandho nāma //",
    "T06bsa065_u:288": " saṃskāraskandhaḥ soddyeśanirdeśaḥ pariniṣṭhitaḥ // 111 //",
    "T06bsa065_u:289": " // vijñānalakṣaṇam //",
    "T06bsa065_u:290": " vijñānaṃ katamat /",
    "T06bsa065_u:291": " ālambanaṃbijñaptiḥ // 112 //",
    "T06bsa065_u:292": " cittamanasī vijñānaparyāyau sanirvacano taccitaṃ mano 'pi citrīkāratā manoniśrayatāṃ ca upādāya // 113 //",
    "T06bsa065_u:293": " // salakṣaṇacittanirdeśaḥ //",
    "T06bsa065_u:294": " mūlacittamālayavijñānaṃ tadyathedaṃ sarvasaṃskārāṇāṃ saṃcitaṃ bījam // 114 //",
    "T06bsa065_u:295": " tatpunarālambanamapyuparichinnamekasantānavarti /",
    "T06bsa065_u:296": " yathā nirodhasamāpatyasaṃjñisamāpatyā - saṃjñikebhyovyutthitasyāsmād viṣayavijñaptikaṃ nāma pravṛtti vijñānaṃ jāyate /",
    "T06bsa065_u:297": " ālambanapratyayamapekṣya bhinnākāreṣu vṛttitāmucchiddya punarjātatāṃ saṃsāre pravṛttinivṛtitāṃ copādāya tadālambanaṃ nāma vijñānam // 115 //",
    "T06bsa065_u:298": " ālayavijñānaṃ hi sarvabījālayatāmātmabhāvālayahetutāmātmabhāve sthititāṃ copādāya // 116 //",
    "T06bsa065_u:299": " ādānavijñānamapi tat /",
    "T06bsa065_u:300": " ātmabhāvādānatāmupādāya // 117 //",
    "T06bsa065_u:301": " // salakṣaṇamanonirdeśaḥ //",
    "T06bsa065_u:302": " mūlamana ālayavijñānamāśritya nityamātmamohātmadṛṣṭyātmamānātmasnehaiḥ saṃprayuktaṃ vijñānamekajātīyasantānavarti arhatvāryamārganirodhasamāpattikāle vyāvartate // 118 //",
    "T06bsa065_u:303": " vijñānaskandhaḥ soniddeśanirdeśaḥ pariniṣṭhitaḥ //",
    "T06bsa065_u:304": " // skandhanirvacanam //",
    "T06bsa065_u:305": " kimarthaḥ skandhaḥ rāśyarthaḥ /",
    "T06bsa065_u:306": " kālagotrākāragativiṣayabhinnānāṃ rūpādīnāmabhisaṃkṣepatāmupādāya // 119 //",
    "T06bsa065_u:307": " skandhoddyeśanirdeśakaḥ prathamādhikāraḥ pariniṣṭhitaḥ //",
    "T06bsa065_u:308": " dvitīyādhikāraḥ",
    "T06bsa065_u:309": " āyatanoddyeśanirdeśakaḥ",
    "T06bsa065_u:310": " dvādaśāyatanāni /",
    "T06bsa065_u:311": " cakṣurāyatanam,",
    "T06bsa065_u:312": " rūpāyatanam,",
    "T06bsa065_u:313": " śrotrāyatanam,",
    "T06bsa065_u:314": " śabdāyatanam,",
    "T06bsa065_u:315": " ghrāṇāyatanam,",
    "T06bsa065_u:316": " gandhāyatanam,",
    "T06bsa065_u:317": " jihvāyatanam,",
    "T06bsa065_u:318": " rasāyatanam,",
    "T06bsa065_u:319": " kāyāyatanam,",
    "T06bsa065_u:320": " spraṣṭavyāyatanam,",
    "T06bsa065_u:321": " mana āyatanam,",
    "T06bsa065_u:322": " dharmāyatanaṃ ca // 120 //",
    "T06bsa065_u:323": " / 'ayatanānāṃ salakṣaṇaprapañcaḥ //",
    "T06bsa065_u:324": " cakṣurādīni rūpaśabdagandharasāyatanāni yathoktapūrvāṇi // 121 //",
    "T06bsa065_u:325": " spraṣṭavyāyatanaṃ catvāri mahābhūtāni uktaśca yaḥ spraṣṭabyaikadeśaḥ // 122 //",
    "T06bsa065_u:326": " mana āyatanaṃ yo vijñānaskandhaḥ // 123 //",
    "T06bsa065_u:327": " dharmāyatanaṃ vedanā,",
    "T06bsa065_u:328": " saṃjñā,",
    "T06bsa065_u:329": " saṃskārāḥ,",
    "T06bsa065_u:330": " avijñaptiḥ,",
    "T06bsa065_u:331": " asaṃskṛtāśca // 124 //",
    "T06bsa065_u:332": " / 'saṃskṛtāḥ salakṣaṇaprabhedāḥ //",
    "T06bsa065_u:333": " asaṃskṛtā katame /",
    "T06bsa065_u:334": " ākāśam, apratisaṃkhyānirodhaḥ,",
    "T06bsa065_u:335": " pratisaṃkhyānirodhaḥ tathatā ca // 125 //",
    "T06bsa065_u:336": " tatrākāśaṃ katamat /",
    "T06bsa065_u:337": " rūpāvakāśaḥ // 126 //",
    "T06bsa065_u:338": " apratisaṃkhyānirodhaḥ katamaḥ /",
    "T06bsa065_u:339": " yo nirodho na visaṃyogaḥ // 127 //",
    "T06bsa065_u:340": " sakleśāpratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 128 //",
    "T06bsa065_u:341": " pratisaṃkhyānirodhaḥ katamaḥ /",
    "T06bsa065_u:342": " yo nirodho visaṃyogaḥ // 129 //",
    "T06bsa065_u:343": " sakleśapratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 130 //",
    "T06bsa065_u:344": " tathatā katamā /",
    "T06bsa065_u:345": " yā dharmāṇāṃ dharmatā /",
    "T06bsa065_u:346": " dharmanairātmyam // 131 //",
    "T06bsa065_u:347": " / 'ayatananirvacanam //",
    "T06bsa065_u:348": " kimupādāyāyatanamiti /",
    "T06bsa065_u:349": " vijñānotpādadvāratāmupādāya // 132 //",
    "T06bsa065_u:350": " āyatanoddyeśanirdeśako dvitīyādhikāraḥ pariniṣṭhitaḥ /",
    "T06bsa065_u:351": " tṛtīyādhikāraḥ",
    "T06bsa065_u:352": " dhātūddeśanirdeśakaḥ",
    "T06bsa065_u:353": " // dhātunāmāni //",
    "T06bsa065_u:354": " aṣṭadaśa dhātavaḥ /",
    "T06bsa065_u:355": " cakṣurdhātuḥ,",
    "T06bsa065_u:356": " rūpadhātuḥ,",
    "T06bsa065_u:357": " cakṣurvijñānadhātu,",
    "T06bsa065_u:358": " śrotradhātuḥ,",
    "T06bsa065_u:359": " śabdadhātuḥ,",
    "T06bsa065_u:360": " śrotravijñānadhātuḥ,",
    "T06bsa065_u:361": " ghrāṇadhātuḥ,",
    "T06bsa065_u:362": " gandhadhātuḥ,",
    "T06bsa065_u:363": " ghrāṇavijñānadhātuḥ,",
    "T06bsa065_u:364": " jihvādhātuḥ,",
    "T06bsa065_u:365": " rasadhātuḥ,",
    "T06bsa065_u:366": " jihvāvijñānadhātuḥ,",
    "T06bsa065_u:367": " kāyadhātuḥ,",
    "T06bsa065_u:368": " spraṣṭavyadhātuḥ,",
    "T06bsa065_u:369": " kāyabijñānadhātuḥ,",
    "T06bsa065_u:370": " manodhātuḥ,",
    "T06bsa065_u:371": " dharmadhātuḥ,",
    "T06bsa065_u:372": " manovijñānadhātuśca // 133 //",
    "T06bsa065_u:373": " // dhātuprapaścaḥ //",
    "T06bsa065_u:374": " cakṣurādayo dhātavo rūpādayo dhātavaśca yathāyatanāni // 134 //",
    "T06bsa065_u:375": " ṣaḍvijñānadhātavaścakṣurāddyāśrayā rūpādyālambanāvijñaptayaḥ // 135 //",
    "T06bsa065_u:376": " manodhātusteṣāmeva samanantaranirudhyaṃ ṣaḍvijñānadeśanāśrayatāmupādāya // 136 //",
    "T06bsa065_u:377": " // dhātuvyavasthanam //",
    "T06bsa065_u:378": " ebamaṣṭādaśadhātuvyavasthānam /",
    "T06bsa065_u:379": " yo rūpaskandhastāni daśāyatanadhātavaḥ // 137 //",
    "T06bsa065_u:380": " dharmāyatanaṃ dharmadhātvekadeśaḥ // 138 //",
    "T06bsa065_u:381": " yo vijñānaskandhastanmana āyatanam,",
    "T06bsa065_u:382": " saptama manodhātavaśca // 139 //",
    "T06bsa065_u:383": " anye ye trayaḥ skandhā rūpadhātvekadeśaśca,",
    "T06bsa065_u:384": " te 'saṃskṛtai sahitā dharmāyatanaṃ dharmadhātuśca // 140 //",
    "T06bsa065_u:385": " // dhātunirvacanam //",
    "T06bsa065_u:386": " kimupādāya dhātava iti /",
    "T06bsa065_u:387": " ākārakātmalakṣaṇadhāraṇatāmupādāya // 141 //",
    "T06bsa065_u:388": " // skandhāyatanadhātukrameṇa dharmadeśanāyāḥ prayojanam //",
    "T06bsa065_u:389": " kimartha skandhādikrameṇa deśanā /",
    "T06bsa065_u:390": " trividhātmagrāhapratipakṣeṇāyaṃ kramaḥ // 142 //",
    "T06bsa065_u:391": " ātmagrāhastrividhaḥ ekātmagrāhaḥ,",
    "T06bsa065_u:392": " vedakātmagrāhaḥ,",
    "T06bsa065_u:393": " kārakātmagrāhaśca // 143 //",
    "T06bsa065_u:394": " // 'ṣṭādaśadhātuvikalpāḥ //",
    "T06bsa065_u:395": " aṣṭādaśadhātuṣu kati rūpiṇaḥ /",
    "T06bsa065_u:396": " ye rūpaskandhasvabhāvāḥ // 144 //",
    "T06bsa065_u:397": " katyarūpiṇaḥ / śiṣṭāḥ // 145 //",
    "T06bsa065_u:398": " kati sanidarśanāḥ /",
    "T06bsa065_u:399": " rūpadhāturekaḥ sanidarśanaviṣayaḥ // 146 //",
    "T06bsa065_u:400": " katyanidarśanāḥ / śiṣṭāḥ // 147 //",
    "T06bsa065_u:401": " kati sapratidhāḥ /",
    "T06bsa065_u:402": " daśa rūpiṇaḥ /",
    "T06bsa065_u:403": " yatra yatpratidhātastatra tat sapratigham // 148 //",
    "T06bsa065_u:404": " katyapratighāḥ / śiṣṭāḥ // 149 //",
    "T06bsa065_u:405": " kati sāsravāḥ /",
    "T06bsa065_u:406": " pañcadaśa paścimānāṃ trayāṇāmekadeśaśca /",
    "T06bsa065_u:407": " te kleśotpādapratyakṣacaryāviṣayatāmupādāyaṃ // 150 //",
    "T06bsa065_u:408": " katyayanāstravāḥ /",
    "T06bsa065_u:409": " paścimānāṃ trayāṇāmekadeśaḥ // 151 //",
    "T06bsa065_u:410": " kati kāmapratisaṃyuktāḥ / sarva // 152 //",
    "T06bsa065_u:411": " kati rūpapratisaṃyuktāḥ /",
    "T06bsa065_u:412": " caturdaśa sthāpayitvā gandharasaghrāṇajihvāvijñānadhātūn // 153 //",
    "T06bsa065_u:413": " katyārūpyasaṃprayuktāḥ /",
    "T06bsa065_u:414": " paścimāśrayaḥ // 154 //",
    "T06bsa065_u:415": " katyapratisaṃyuktāḥ /",
    "T06bsa065_u:416": " trayāṇāmekadeśaḥ // 155 //",
    "T06bsa065_u:417": " kati skandhasaṃgrahītāḥ /",
    "T06bsa065_u:418": " asaṃskṛtān sthāpayitvā // 156 //",
    "T06bsa065_u:419": " katyupādānaskandhasaṃgṛhītāḥ ye sāsravāḥ // 157 //",
    "T06bsa065_u:420": " kati kuśalākuśalāvyākṛtāḥ /",
    "T06bsa065_u:421": " daśa trividhāḥ /",
    "T06bsa065_u:422": " sapta manodhātavaḥ,",
    "T06bsa065_u:423": " rūpaśabdadharmadhātavaśca /",
    "T06bsa065_u:424": " śiṣṭā avyākṛtāḥ // 158 //",
    "T06bsa065_u:425": " katyādhyātmikāḥ /",
    "T06bsa065_u:426": " dvādaśa /",
    "T06bsa065_u:427": " sthāpayitvā rūpaśabda gandharasaspraṣṭāvyadharmadhātūn // 159 //",
    "T06bsa065_u:428": " kati bāhyāḥ /",
    "T06bsa065_u:429": " ṣaṭ /",
    "T06bsa065_u:430": " ye sthāpitāḥ // 160 //",
    "T06bsa065_u:431": " kati sālambanāḥ /",
    "T06bsa065_u:432": " saptacittadhātavaḥ dharmadhātvekadeśo 'pi yaścaittaḥ // 161 //",
    "T06bsa065_u:433": " katyanālambanāḥ /",
    "T06bsa065_u:434": " śiṣṭā daśa dharmadhātvekadeśaśca // 162 //",
    "T06bsa065_u:435": " kati savitarkāḥ /",
    "T06bsa065_u:436": " manodhātuḥ manovijñānadhātuḥ dharmadhātvekadeśaśca // 163 //",
    "T06bsa065_u:437": " kati niṣpannāḥ /",
    "T06bsa065_u:438": " pañcādhyātmikāḥ /",
    "T06bsa065_u:439": " caturṇāmekadeśaḥ // 164 //",
    "T06bsa065_u:440": " katyaniṣpannāḥ śiṣṭā daśa,",
    "T06bsa065_u:441": " caturṇāmekadeśaśca // 165 //",
    "T06bsa065_u:442": " kati sabhāgāḥ /",
    "T06bsa065_u:443": " ādhyātmikāḥ pañca rūpiṇaḥ /",
    "T06bsa065_u:444": " svavijñānasahitaviṣayasadṛśatāmupādāya // 166 //",
    "T06bsa065_u:445": " kati tatsabhāgāḥ /",
    "T06bsa065_u:446": " ta eva svavijñānavirahitasvānvayasadṛśatāmupādāya // 167 //",
    "T06bsa065_u:447": " dhātūddyeśanirdeśakastṛtīyādhikaraḥ pariniṣṭitaḥ /",
    "T06bsa065_u:448": " ācāryavasubandhuviracit pañcaskandhaprakaraṇaṃ samāptam /",
    "T06bsa065_u:449": " pañcaskandhaprakaraṇaṃ śāstriṇā śāntibhikṣuṇā /",
    "T06bsa065_u:450": " bhoṭānuvādamāgamya saṃskṛte punaruddhṛtam",
    "T06bsa065_u:451": " yadatra sukṛtaṃ sarva tadācāryasya dhīmataḥ /",
    "T06bsa065_u:452": " yat kukṛtaṃ tadāgo me kṣantavyaṃ sādhusūribhiḥ //"
}