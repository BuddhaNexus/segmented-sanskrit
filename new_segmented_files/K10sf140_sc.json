{
  "K10sf140_sc:sand0_0": "Mahāsamājasūtra",
  "K10sf140_sc:sand1_0_0": "evaṁ mayā śrutam | ekasmiṁ samaye bhagavāṁ śākyeṣu viharati vane kāpilavastave sārdham",
  "K10sf140_sc:sand1_0_1": "pañcabhir bhikṣuśataiḥ sarvair arhadbhiḥ kṣīṇāsravaiḥ kṛtakṛyaiḥ kṛtakaraṇīyair avahṛtabhārair",
  "K10sf140_sc:sand1_0_2": "anuprāptasvakārthaiḥ parikṣīṇabhavasaṁyojanaiḥ samyagājñayā suvimuktacittaiḥ | tatra",
  "K10sf140_sc:sand1_0_3": "bhagavāṁ bhi kṣusaṁghasya nirvāṇapratisaṁyuktāṁ dharmikathām kathayati sma | api",
  "K10sf140_sc:sand1_0_4": "tatra daśamahālokadhātu mahāśākyamahāśākyā devatā sannipatitā bhagavantam upāsate bhikṣusaṁghañ ca |",
  "K10sf140_sc:sand2_0_0": "atha khalu catasṛṇāṁ brahmalokanivāsikānāṁ devatānāṁ brahmakāyikānām etad abhavat |",
  "K10sf140_sc:sand2_0_1": "adya khalu bhagavāṁ śākyeṣu viharati vane kāpilavastave sārdhaṁ pañcabhir bhikṣuśataiḥ",
  "K10sf140_sc:sand2_0_2": "sarvair arhadbhiḥ pūrvavad yāvad bhikṣusaṁghañ ca | yan nu vayaṁ gatvā pratyekapratīkṣā",
  "K10sf140_sc:sand2_0_3": "bhagavantaṁ bhikṣusaṁghañ ca gāthābhir abhiṣṭaveyāma |",
  "K10sf140_sc:sand3_0_0": "atha catasro brahmakāyikā devatās tadyathā balavāṁ puruṣaḥ saṁmiṁjitaṁ vā bāhuṁ prasārayet",
  "K10sf140_sc:sand3_0_1": "prasāritaṁ vā saṁmiṁjayed evam eva catasro brahmakāyikā devatā brahmaloke antarhitā",
  "K10sf140_sc:sand3_0_2": "bhagavataḥ purataḥ pratitasthuḥ | ekāntasthitā ekā brahmakāyikā devatā tasyāṁ velāyāṁ gāthāṁ babhāṣe ||",
  "K10sf140_sc:sand3_1": "mahāsamājaḥ pravaṇe",
  "K10sf140_sc:sand3_2": "devakāyāḥ samāgatāḥ |",
  "K10sf140_sc:sand3_3": "aparājitasaṁghaṁ hi",
  "K10sf140_sc:sand3_4": "draṣṭum abhyāgatā vayam ||",
  "K10sf140_sc:sand3_5": "dvitīyā brahmakāyikā devatā gāthāṁ babhāṣe ||",
  "K10sf140_sc:sand3_6": "ṛjubhūtena cittena",
  "K10sf140_sc:sand3_7": "pradadhaty atra bhikṣavaḥ |",
  "K10sf140_sc:sand3_8": "rathīva netrāṇy ādāya",
  "K10sf140_sc:sand3_9": "saṁrakṣaṁtīndriyāṇi hi ||",
  "K10sf140_sc:sand3_10": "tṛtīyā brahmakāyikā devatā gāthāṁ babhāṣe ||",
  "K10sf140_sc:sand3_11": "saṁpradālya khalu kīlam aneyā",
  "K10sf140_sc:sand3_12": "indrakīlaṁ parighaṁ parikhaṁ ca |",
  "K10sf140_sc:sand3_13": "te caranti śuddhā vimlāś ca",
  "K10sf140_sc:sand3_14": "nāyakena dāntāḥ śiśunāgāḥ ||",
  "K10sf140_sc:sand3_15": "caturthī brahmakāyikā devatā gāthāṁ babhāṣe ||",
  "K10sf140_sc:sand3_16": "ye buddhaṁ śaraṇaṁ yānti",
  "K10sf140_sc:sand3_17": "na te gacchanti durgatiṁ |",
  "K10sf140_sc:sand3_18": "prahāya mānuṣān kāyān",
  "K10sf140_sc:sand3_19": "divān kāyān upāsate ||",
  "K10sf140_sc:sand3_20": "ity uktvā catasro brahmakāyikā devatās tatraiva sthāne ’ntarhitāḥ ||",
  "K10sf140_sc:sand4_0_0": "atha bhagavāṁ sarvāṁ devapariṣadam avalokya bhikṣūn āmantrayati sma | sumahāṁ batāyaṁ",
  "K10sf140_sc:sand4_0_1": "bhikṣavo devatāsaṁnipātaḥ sumahāṁ batāyaṁ bhikṣavo devatāsaṁnipāto mama pariṣady",
  "K10sf140_sc:sand4_0_2": "etarhi | ye ’pi te atīte ’dhvani tathāgatā arhantaḥ samyaksaṁbuddhās teṣāṁ api buddhānāṁ",
  "K10sf140_sc:sand4_0_3": "bhagavatāṁ etāvad devatānāṁ saṁnipāto babhūva tadyathā mama pariṣady etarhi | ye",
  "K10sf140_sc:sand4_0_4": "pi te anāgāte ’dhvani tathāgatā arhantaḥ samyaksaṁbuddhās teṣām api buddhānāṁ bhagavatām",
  "K10sf140_sc:sand4_0_5": "etāvad devatānāṁ saṁnipāto bhaviṣyati tadyathā mama pariṣady etarhi | vakṣyāmi vo",
  "K10sf140_sc:sand4_0_6": "bhikṣavo devatānāṁ nāmāni kīrtayiṣyāmi devatānāṁ nāmāni | śṛṇuta mama bhāṣitam |",
  "K10sf140_sc:sand5_0": "…",
  "K10sf140_sc:sand5_1": "hitātmanaḥ samāhitāḥ |",
  "K10sf140_sc:sand5_2": "pṛthaksiṁhāiva …",
  "K10sf140_sc:sand5_3": "… pratāpinaḥ |",
  "K10sf140_sc:sand5_4": "avadātamanasaḥ śuddhā",
  "K10sf140_sc:sand5_5": "viprasannā anāvilāḥ |",
  "K10sf140_sc:sand5_6": "śritā devamanuṣyeṣu",
  "K10sf140_sc:sand5_7": "yāvad brahm … mṛtyuhāyinaḥ ||",
  "K10sf140_sc:sand5_8": "śaikṣā dvipañcaśatakaṁ",
  "K10sf140_sc:sand5_9": "śatañ ca daśa cāpare |",
  "K10sf140_sc:sand5_10": "anekāni sahasrāṇi",
  "K10sf140_sc:sand5_11": "daivatāni nyavedayan",
  "K10sf140_sc:sand5_12": "anantam api nāmaike",
  "K10sf140_sc:sand5_13": "sphuṭāḥ sarvadiśo ’bhavan ||",
  "K10sf140_sc:sand6_0": "tāṁś ca buddho hy abhijñāya",
  "K10sf140_sc:sand6_1": "vyavalokya tathāgataḥ |",
  "K10sf140_sc:sand6_2": "athāmantrayati śāstā",
  "K10sf140_sc:sand6_3": "śrāvakāñ chāsane ratān ||",
  "K10sf140_sc:sand6_4": "devakāyā abhikrāntā",
  "K10sf140_sc:sand6_5": "niśamayata bhikṣavaḥ |",
  "K10sf140_sc:sand6_6": "teṣāṁ nāmāni vakṣyāmi",
  "K10sf140_sc:sand6_7": "gāthābhir anupūrvaśaḥ |",
  "K10sf140_sc:sand6_8": "ekāgramanaso bhūtvā",
  "K10sf140_sc:sand6_9": "śṛṇuta mama bhāṣitam",
  "K10sf140_sc:sand7_0": "yakṣāṇāṁ saptasāhasrā",
  "K10sf140_sc:sand7_1": "vane kāpilavastave |",
  "K10sf140_sc:sand7_2": "avabhāsayitvā tiṣṭhanti",
  "K10sf140_sc:sand7_3": "venaṁ kāilavastavam ||",
  "K10sf140_sc:sand7_4": "te cāpi samitim āyātā",
  "K10sf140_sc:sand7_5": "ye caiṣāṁ sahagocarāḥ",
  "K10sf140_sc:sand7_6": "ṛddhimanto dyutimanto",
  "K10sf140_sc:sand7_7": "varṇavanto yaśasvinaḥ |",
  "K10sf140_sc:sand7_8": "vandamānā abhikrāntā",
  "K10sf140_sc:sand7_9": "bhikṣuṇāṁ samitiṁ vane ||",
  "K10sf140_sc:sand8_0": "sātāgiris trisāhasraḥ",
  "K10sf140_sc:sand8_1": "…",
  "K10sf140_sc:sand8_2": "[avabhāsayitvā tiṣṭhanti",
  "K10sf140_sc:sand8_3": "vanaṁ kāpilavastavam ||]",
  "K10sf140_sc:sand8_4": "sa cāpi samitim āyato",
  "K10sf140_sc:sand8_5": "ye cāsya sahagocarāḥ",
  "K10sf140_sc:sand8_6": "pūrvavat |",
  "K10sf140_sc:sand9_0": "ṣatṣāhasrā haimavatāḥ",
  "K10sf140_sc:sand9_1": "sarve nānātvavarṇinaḥ |",
  "K10sf140_sc:sand9_2": "avabhāsayitvā tiṣṭhanti",
  "K10sf140_sc:sand9_3": "venaṁ kāpilavastavaṁ ||",
  "K10sf140_sc:sand9_4": "te cāpi samitim āyātā",
  "K10sf140_sc:sand9_5": "ye caiṣāṁ sahagocarāḥ",
  "K10sf140_sc:sand9_6": "pūrvavat |",
  "K10sf140_sc:sand10_0": "ity ete ṣoḍaśa yakṣasahasrāḥ",
  "K10sf140_sc:sand10_1": "sarve nānātvavarṇinaḥ",
  "K10sf140_sc:sand10_2": "avabhāsayitvā tiṣṭhanti",
  "K10sf140_sc:sand10_3": "venaṁ kāpilavastavam ||",
  "K10sf140_sc:sand10_4": "te cāpi samitim āyātā",
  "K10sf140_sc:sand10_5": "ye caiṣāṁ sahagocarāḥ",
  "K10sf140_sc:sand10_6": "pūrvavat |",
  "K10sf140_sc:sand11_0": "viśamitraḥ pañcaśato",
  "K10sf140_sc:sand11_1": "viśvadevo maharddhikaḥ",
  "K10sf140_sc:sand11_2": "avabhāsayitvā tiṣṭhanti",
  "K10sf140_sc:sand11_3": "vanaṁ kāpilavastavam ||",
  "K10sf140_sc:sand11_4": "te cāpi samitim āyātā",
  "K10sf140_sc:sand11_5": "ye caiṣāṁ sahagocarāḥ",
  "K10sf140_sc:sand11_6": "pūrvavat |",
  "K10sf140_sc:sand12_0": "kumbhīro yakṣo rājagṛhe",
  "K10sf140_sc:sand12_1": "vipule ’smiṁ nivāsikaḥ",
  "K10sf140_sc:sand12_2": "bhūyaḥ śatasahasreṇa",
  "K10sf140_sc:sand12_3": "yakṣāṇāṁ paryupāsyate",
  "K10sf140_sc:sand12_4": "te cāpi samitim āyātā",
  "K10sf140_sc:sand12_5": "pūrvavat |",
  "K10sf140_sc:sand13_0": "pūrvān diśam abhipālayati",
  "K10sf140_sc:sand13_1": "rājā mahārājā |",
  "K10sf140_sc:sand13_2": "gandharvādhipatī rājā",
  "K10sf140_sc:sand13_3": "dhṛtirāṣṭra iti nāma sa",
  "K10sf140_sc:sand13_4": "ramate ’sau nṛttagītena",
  "K10sf140_sc:sand13_5": "jñātibhiḥ sahitaḥ sadā |",
  "K10sf140_sc:sand13_6": "sa cāpi samitim āyāto",
  "K10sf140_sc:sand13_7": "pūrvavat |",
  "K10sf140_sc:sand14_0": "[dakṣiṇān diśam abhipālayati",
  "K10sf140_sc:sand14_1": "rājā mahārājā",
  "K10sf140_sc:sand14_2": "kuṁbhāṇḍādhipatī rājā",
  "K10sf140_sc:sand14_3": "virūḍhaka iti nāma sa",
  "K10sf140_sc:sand14_4": "ramate ’sau nṛttagītena",
  "K10sf140_sc:sand14_5": "jñātibhiḥ sahitaḥ sadā |",
  "K10sf140_sc:sand14_6": "sa cāpi samitim āyāto",
  "K10sf140_sc:sand14_7": "pūrvavat |]",
  "K10sf140_sc:sand15_0": "paścimān diśam abhipālayati",
  "K10sf140_sc:sand15_1": "rājā mahārājā",
  "K10sf140_sc:sand15_2": "nāgānām adhipatī rājā",
  "K10sf140_sc:sand15_3": "virūpākṣa iti nāma sa",
  "K10sf140_sc:sand15_4": "ramate ’sau nṛttagītena",
  "K10sf140_sc:sand15_5": "jñātibhiḥ sahitaḥ sadā",
  "K10sf140_sc:sand15_6": "sa cāpi samitim āyāto",
  "K10sf140_sc:sand15_7": "purvavat |",
  "K10sf140_sc:sand16_0": "uttarān diśam abhipālayati",
  "K10sf140_sc:sand16_1": "rājā mahārājā",
  "K10sf140_sc:sand16_2": "yakṣāṇām adhipatī rājā",
  "K10sf140_sc:sand16_3": "kubera iti nāma sa",
  "K10sf140_sc:sand16_4": "ramte ’sau nṛttagītena",
  "K10sf140_sc:sand16_5": "jñātibhiḥ sahitaḥ sadā",
  "K10sf140_sc:sand16_6": "sa cāpi samitim āyāto",
  "K10sf140_sc:sand16_7": "pūrvavat |",
  "K10sf140_sc:sand17_0": "purastime ti dhṛtirāṣṭro",
  "K10sf140_sc:sand17_1": "dakṣiṇe ti virūḍhakaḥ |",
  "K10sf140_sc:sand17_2": "paścime ti virūpākṣaḥ",
  "K10sf140_sc:sand17_3": "kuberaś cottaran diśi ||",
  "K10sf140_sc:sand17_4": "catvāra ete mahārājā",
  "K10sf140_sc:sand17_5": "lokapālā yaśasvinaḥ",
  "K10sf140_sc:sand17_6": "diśaś catasraḥ pālayanti",
  "K10sf140_sc:sand17_7": "maharddhikā mahābalāḥ",
  "K10sf140_sc:sand17_8": "te cāpi samitim āyātā",
  "K10sf140_sc:sand17_9": "pūrvavat |",
  "K10sf140_sc:sand18_0": "teṣāṁ māyāvino dāsāḥ",
  "K10sf140_sc:sand18_1": "prāptā vañcakāḥ śaṭhāḥ",
  "K10sf140_sc:sand18_2": "māyā kiṭiś ca vikiṭiś ca",
  "K10sf140_sc:sand18_3": "bhṛguś ca bhṛkuṭinyā saha",
  "K10sf140_sc:sand18_4": "ṛddhiguṇāṁ darśayaṁtas",
  "K10sf140_sc:sand18_5": "tadā saṁbahucaitrike",
  "K10sf140_sc:sand18_6": "te cāpi samitim āyātā",
  "K10sf140_sc:sand18_7": "pūrvavat |",
  "K10sf140_sc:sand19_0_0": "ime py atra samabhikrāntā yakṣā mahāyakṣāḥ yakṣasenāpatayaḥ senāyā ye parināyakās",
  "K10sf140_sc:sand19_0_1": "tadyathā indraḥ somo varuṇaḥ prajāpatiḥ pūrvavat yathātānāṭike yāvat triphalī ca",
  "K10sf140_sc:sand19_0_2": "trikaṇṭhakī | te cāpi samitim āyātā pūrvavat |",
  "K10sf140_sc:sand20_0": "āgataḥ sahabhuṁ nāgo",
  "K10sf140_sc:sand20_1": "vaiśālaś cāpi takṣakaḥ",
  "K10sf140_sc:sand20_2": "kambalāśvataraḥ prāptaḥ",
  "K10sf140_sc:sand20_3": "prajāguś ca guṇā saha |",
  "K10sf140_sc:sand20_4": "saudāsako dhṛtirāṣṭro",
  "K10sf140_sc:sand20_5": "… pati kuñjarāḥ",
  "K10sf140_sc:sand20_6": "airāvaṇo mahābhīṣmaḥ",
  "K10sf140_sc:sand20_7": "prāpto nāgo yaśasvī mamā …",
  "K10sf140_sc:sand20_8": "te cāpi samitim āyātā",
  "K10sf140_sc:sand20_9": "pūrvavat |",
  "K10sf140_sc:sand21_0": "yo nāgarājaṁ harati prasahya",
  "K10sf140_sc:sand21_1": "balavāṁ dvijaḥ pakṣī viśuddhacakṣuḥ",
  "K10sf140_sc:sand21_2": "vaihāyasena vanam abhyupāgataś",
  "K10sf140_sc:sand21_3": "citraṁ suvaṇī iti tasya nāma |",
  "K10sf140_sc:sand21_4": "abhayaṁ tatra nāgarājñām",
  "K10sf140_sc:sand21_5": "adāt sa suparṇitaḥ",
  "K10sf140_sc:sand21_6": "kṣemam akārṣīc chāstā",
  "K10sf140_sc:sand21_7": "ślakṣṇāsu vācāsu muditāsu",
  "K10sf140_sc:sand21_8": "tatraiva buddhaṁ śaranaṁ upāgataḥ ||",
  "K10sf140_sc:sand21_9": "sa cāpi samitim āyāto",
  "K10sf140_sc:sand21_10": "pūrvavat |",
  "K10sf140_sc:sand22_0": "vijitā vajrahastena",
  "K10sf140_sc:sand22_1": "samudram asurāḥ śritāḥ |",
  "K10sf140_sc:sand22_2": "…",
  "K10sf140_sc:sand22_3": "… ||",
  "K10sf140_sc:sand22_4": "sucitro vemacitraś ca",
  "K10sf140_sc:sand22_5": "prahlādo mucilas tathā |",
  "K10sf140_sc:sand22_6": "śataṁ ca baḍiputrāṇāṁ",
  "K10sf140_sc:sand22_7": "sarve vairocanāmakāḥ ||",
  "K10sf140_sc:sand22_8": ".. hi baḍisenāṁ",
  "K10sf140_sc:sand22_9": "rāhur bhadram upāgataḥ |",
  "K10sf140_sc:sand22_10": "samayas te mahāvīra",
  "K10sf140_sc:sand22_11": "bhikṣūṇāṁ samitim vane ||",
  "K10sf140_sc:sand22_12": "te cāpi samitim āyātā",
  "K10sf140_sc:sand22_13": "pūrvavat |",
  "K10sf140_sc:sand23_0": "varuṇā vāruṇī cāpi",
  "K10sf140_sc:sand23_1": "kṣaumātho pṛṣṭhato gatā",
  "K10sf140_sc:sand23_2": "maitrī varuṇikā cāpi",
  "K10sf140_sc:sand23_3": "prāptā devī yaśasvinī |",
  "K10sf140_sc:sand23_4": "pṛthivī ca devā āpaś ca",
  "K10sf140_sc:sand23_5": "vāyur devaś ca pāvakaḥ ||",
  "K10sf140_sc:sand23_6": "daśaite daśakāḥ kāyāḥ",
  "K10sf140_sc:sand23_7": "sarve nānātvavarṇinaḥ |",
  "K10sf140_sc:sand23_8": "avabhāsayitvā tiṣṭhanti",
  "K10sf140_sc:sand23_9": "vanaṁ kāpilavastavam ||",
  "K10sf140_sc:sand23_10": "te cāpi samitim āyātā",
  "K10sf140_sc:sand23_11": "pūrvavat",
  "K10sf140_sc:sand24_0": "viṣṇuś ca devaḥ sahalī ca deva",
  "K10sf140_sc:sand24_1": "āgataś ca yama … |",
  "K10sf140_sc:sand24_2": "candropaniṣado devāś",
  "K10sf140_sc:sand24_3": "candram eva puraskṛtya ||",
  "K10sf140_sc:sand24_4": "sūryopaniṣado devāḥ",
  "K10sf140_sc:sand24_5": "sūryam eva puraskṛtya |",
  "K10sf140_sc:sand24_6": "nakṣatrāṇi puraskṛtya",
  "K10sf140_sc:sand24_7": "prāptaḥ mandavalāhakāḥ",
  "K10sf140_sc:sand24_8": "maghavāṁ sarva …",
  "K10sf140_sc:sand24_9": "prāptaḥ śakraḥ purandaraḥ |",
  "K10sf140_sc:sand24_10": "daśaite daśakāḥ kāyāḥ",
  "K10sf140_sc:sand24_11": "pūrvavat |",
  "K10sf140_sc:sand25_0": "āgatā sabhikā devī",
  "K10sf140_sc:sand25_1": "jvaladarciḥśikhopamā |",
  "K10sf140_sc:sand25_2": "ariṣṭikā ca romā ca",
  "K10sf140_sc:sand25_3": "umā puṣpanivāsinī ||",
  "K10sf140_sc:sand25_4": "carakā subaddhakakṣā",
  "K10sf140_sc:sand25_5": "acyutāś cāpi anekaśaḥ",
  "K10sf140_sc:sand25_6": "pradyumnaḥ svātikā prāptā",
  "K10sf140_sc:sand25_7": "yā diśo hy abhivarṣati ||",
  "K10sf140_sc:sand25_8": "daśaite daśakāḥ kāyāḥ",
  "K10sf140_sc:sand25_9": "pūrvavat |",
  "K10sf140_sc:sand26_0": "śuklā kaḍambā karuṇā",
  "K10sf140_sc:sand26_1": "prāptā nīlakavāsinī |",
  "K10sf140_sc:sand26_2": "avadātakeśā pramukhī",
  "K10sf140_sc:sand26_3": "prāptā pītakavāsinī |",
  "K10sf140_sc:sand26_4": "sadāmattā hāritakā",
  "K10sf140_sc:sand26_5": "miśrikāś cāpy anekaśaḥ |",
  "K10sf140_sc:sand26_6": "śuddhakā rucikā caiva",
  "K10sf140_sc:sand26_7": "prāptā devā yaśasvinaḥ ||",
  "K10sf140_sc:sand26_8": "daśaite daśakāḥ kāyāḥ",
  "K10sf140_sc:sand26_9": "pūrvavat |",
  "K10sf140_sc:sand28_0": "kṣemakās tuṣitā yāmāḥ",
  "K10sf140_sc:sand28_1": "kṛṣṇuktāś cāpy anekaśaḥ |",
  "K10sf140_sc:sand28_2": "lumbinī lumbinīśreṣṭhā",
  "K10sf140_sc:sand28_3": "jyotirnāmā ca svāśiṣā |",
  "K10sf140_sc:sand28_4": "nirmāṇaratayaḥ prāptā",
  "K10sf140_sc:sand28_5": "athāpi vaśavartinaḥ ||",
  "K10sf140_sc:sand28_6": "daśaite daśakāḥ kāyāḥ",
  "K10sf140_sc:sand28_7": "pūrvavat |",
  "K10sf140_sc:sand29_0": "daśa vai tv īśvarāḥ prāptāḥ",
  "K10sf140_sc:sand29_1": "pratyekaṁ vaśavartinaḥ |",
  "K10sf140_sc:sand29_2": "sahasrabrahmalokānāṁ",
  "K10sf140_sc:sand29_3": "mahābrahmā nirucyate |",
  "K10sf140_sc:sand29_4": "sanatkumāras tiṣyaś ca",
  "K10sf140_sc:sand29_5": "ṛddhimadbhiḥ sutaiḥ saha ||",
  "K10sf140_sc:sand29_6": "te cāpi samitim āyātā",
  "K10sf140_sc:sand29_7": "pūrvavat |",
  "K10sf140_sc:sand30_0": "teṣāñ ca pṛṣṭhataḥ prāptā",
  "K10sf140_sc:sand30_1": "hāritī vaśavartinī |",
  "K10sf140_sc:sand30_2": "uttamā varṇarūpeṇa",
  "K10sf140_sc:sand30_3": "putrebhiḥ parivāritā ||",
  "K10sf140_sc:sand30_4": "sā cāpi samitim āyātā",
  "K10sf140_sc:sand30_5": "pūrvavat |",
  "K10sf140_sc:sand30_6": "ime ca sarve ’bhikrāntāḥ",
  "K10sf140_sc:sand30_7": "sendrā devāḥ sa-īśānāḥ",
  "K10sf140_sc:sand30_8": "saprajāpatayaḥ |",
  "K10sf140_sc:sand31_0": "bhikṣavo ’py atrābhikrāntāḥ",
  "K10sf140_sc:sand31_1": "paśya kṛṣṇasya mantritam",
  "K10sf140_sc:sand31_2": "idaṁ gṛhṇīta bandhadhvaṁ",
  "K10sf140_sc:sand31_3": "rāgeṇa bhadram astu vaḥ |",
  "K10sf140_sc:sand31_4": "parivārya samantāc ca",
  "K10sf140_sc:sand31_5": "maitraṁ jīvantam utsṛjata ||",
  "K10sf140_sc:sand31_6": "iti tatra mahāsenaḥ",
  "K10sf140_sc:sand31_7": "kṛṣṇa senām acūcudat |",
  "K10sf140_sc:sand31_8": "svayaṁ cāpy abhyudāvṛttaḥ",
  "K10sf140_sc:sand31_9": "saṁkruddho ’py asvayaṁvaśaḥ ||",
  "K10sf140_sc:sand31_10": "pāninā pṛthivīṁ hatvā",
  "K10sf140_sc:sand31_11": "kṛtvā śabdaṁ subhairavam |",
  "K10sf140_sc:sand31_12": "atha prāvarṣayad devam",
  "K10sf140_sc:sand31_13": "abhramāliṁ savidyutam ||",
  "K10sf140_sc:sand32_0": "tāṁś ca buddho hy abhijñāya",
  "K10sf140_sc:sand32_1": "vyavalokya tathāgataḥ |",
  "K10sf140_sc:sand32_2": "athāmantrayati śāstā",
  "K10sf140_sc:sand32_3": "śrāvakāñ chāsane ratān ||",
  "K10sf140_sc:sand32_4": "mārasainyam abhikrāntaṁ",
  "K10sf140_sc:sand32_5": "niśamayata bhikṣavaḥ |",
  "K10sf140_sc:sand32_6": "ātaptakāriṇas te ’pi",
  "K10sf140_sc:sand32_7": "śrutvā buddhasya bhāṣitam |",
  "K10sf140_sc:sand32_8": "vītarāgaṁ hi taṁ matvā",
  "K10sf140_sc:sand32_9": "tasya romāṇi neñjaṁti ||",
  "K10sf140_sc:sand32_10": "te cāpi vijitasaṁgrāmā",
  "K10sf140_sc:sand32_11": "bhayābhītā yaśasvinaḥ |",
  "K10sf140_sc:sand32_12": "modante saha śiṣyebhiḥ",
  "K10sf140_sc:sand32_13": "śrāvakās te jinātmajāḥ ||",
  "K10sf140_sc:sand33_0": "idam avocad bhagavām āptamanasas te bhikṣavo bhagavato bhāṣitam abhyanandan ||",
  "K10sf140_sc:sand33_1": "a[nta]roddānam || varūṇā viṣṇu sabhikā śuklā śyāmā ca kṣemakāḥ ||",
  "K10sf140_sc:sand33_2": "mahāsamājasūtraṁ samāptaḥ samāptañ ca iti | sādhū ||"
}