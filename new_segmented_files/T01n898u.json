{
    "T01n898u_001:0": " vajrapāṇināmāṣṭottaraśatastotram",
    "T01n898u_001:1": " vajrasattva mahāsattva mahāyāna mahātmaka |",
    "T01n898u_001:2": " mahāprabha mahāśuddha mahānātha namo'stu te || 1 ||",
    "T01n898u_001:3": " vajrarāja mahāvajra vajra sarvatathāgata |",
    "T01n898u_001:4": " mahāsattva mahāvīrya mahopāya namo'stu te || 2 ||",
    "T01n898u_001:5": " vajrarāga mahāśuddha sarvasaukhya mahāsukha |",
    "T01n898u_001:6": " sukhāgryānādinidhana mahākāma namo'stu te || 3 ||",
    "T01n898u_001:7": " vajrasādhu mahātuṣṭi sādhukāra praharṣaka |",
    "T01n898u_001:8": " mahāharṣa mahāmoda suprāmodya namo'stu te || 4 ||",
    "T01n898u_001:9": " vajraratna mahārāja svābhiṣeka mahāmate |",
    "T01n898u_001:10": " sarvaratna mahāśobhavibhūṣaṇa namo'stu te || 5 ||",
    "T01n898u_001:11": " vajrateja mahāteja vajraprabha mahādyute |",
    "T01n898u_001:12": " jinaprabha mahājvāla buddhaprabha namo'stu te || 6 ||",
    "T01n898u_001:13": " vajraketu mahāketu mahādhvaja dhanaprada |",
    "T01n898u_001:14": " ākāśaketo mahāyaṣṭi tyāgadhvaja namo'stu te || 7 ||",
    "T01n898u_001:15": " vajrahāsa mahāhāsa mahāprītipramodana |",
    "T01n898u_001:16": " prītivega ratiprīte dharmaprīte namo'stu te || 8 ||",
    "T01n898u_001:17": " vajradharma mahādharma sarvadharmasuśodhaka |",
    "T01n898u_001:18": " buddhadharma sudharmāgrya rāgadharma namo'stu te || 9 ||",
    "T01n898u_001:19": " vajratīkṣṇa mahākośa prajñājñāna mahāmate |",
    "T01n898u_001:20": " pāpacchedamahākhaḍga buddhaśastra namo'stu te || 10 ||",
    "T01n898u_001:21": " vajrahetu mahācakra buddhacakra mahānidhe |",
    "T01n898u_001:22": " sarvamaṇḍaladharmāgra dharmacakra namo'stu te || 11 ||",
    "T01n898u_001:23": " vajrabhāṣa mahābhāṣa niṣprapañca mahākṣara |",
    "T01n898u_001:24": " anakṣara mahājāpa buddhavāca namo'stu te || 12 ||",
    "T01n898u_001:25": " vajrakarma sukarmāgrya mahākarma sukarmakṛt |",
    "T01n898u_001:26": " guhyapūja mahāpūja buddhapūja namo'stu te || 13 ||",
    "T01n898u_001:27": " vajrarakṣa mahāvarma kavacāgrya mahādṛḍha |",
    "T01n898u_001:28": " mahārakṣa mahāsāra buddhavīrya namo'stu te || 14 ||",
    "T01n898u_001:29": " vajrayakṣa mahākrodha sarvaduṣṭabhayānaka |",
    "T01n898u_001:30": " sarvabuddhamahopāya agrayakṣa namo'stu te || 15 ||",
    "T01n898u_001:31": " mahāsandhi mahāmudra mahāsamayabandhaka |",
    "T01n898u_001:32": " mahāmuṣṭe samudrāgrya vajramuṣṭe namo'stu te || 16 ||",
    "T01n898u_001:33": " vandyo mānyaśca pūjyaśca satkartavyastathāgataiḥ |",
    "T01n898u_001:34": " yasmādanādinidhanaṃ bodhicittaṃ tvamucyase || 17 ||",
    "T01n898u_001:35": " tvāmāsādya jināḥ sarve bodhisattvāśca śauriṇaḥ |",
    "T01n898u_001:36": " saṃbhūtāḥ saṃbhaviṣyanti buddhabodhyagrahetavaḥ || 18 ||",
    "T01n898u_001:37": " namaste vajrasattvāya vajraratnāya te namaḥ |",
    "T01n898u_001:38": " namaste vajradharmāya namaste vajrakarmaṇe || 19 ||",
    "T01n898u_001:39": " tvāmabhiṣṭutya nāmāgraiḥ praṇamya ca subhāvataḥ |",
    "T01n898u_001:40": " yatpuṇyaṃ tena sarvo hi buddhabodhimavāpnuyāt || 20 ||",
    "T01n898u_001:41": " idamuccārayetsamyag nāmāṣṭaśatamuttamam |",
    "T01n898u_001:42": " sakṛdvāraṃ subhaktisthaḥ sarvabuddhatvamāpnuyāt || 21 ||",
    "T01n898u_001:43": " śrīvajrapāṇināmāṣṭottaraśatastotraṃ samāptam |"
}