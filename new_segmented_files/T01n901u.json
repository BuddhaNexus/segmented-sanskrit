{
    "T01n901u_001:0": " māravijayastotram",
    "T01n901u_001:1": " jitamārakaliṃ jagadekaguruṃ",
    "T01n901u_001:2": " surayakṣamahoragadaityanatam |",
    "T01n901u_001:3": " catusatyasudeśitamārgamimaṃ",
    "T01n901u_001:4": " guṇameghamahaṃ praṇamāmi sadā || 1 ||",
    "T01n901u_001:5": " bhavabhīmamahodadhimadhyagataṃ",
    "T01n901u_001:6": " kṛpaṇārtaravaṃ samavekṣya janam |",
    "T01n901u_001:7": " parimocayituṃ ya iha praṇidhiṃ",
    "T01n901u_001:8": " pracakāra vibhuṃ tamahaṃ praṇataḥ || 2 ||",
    "T01n901u_001:9": " girirājanibhaṃ śaradindudhiyaṃ",
    "T01n901u_001:10": " sumukhaṃ subhujaṃ vararūpadharam |",
    "T01n901u_001:11": " sugataṃ gatamapratimaṃ sugataṃ",
    "T01n901u_001:12": " praṇato'smi sadā jagadarthakaram || 3 ||",
    "T01n901u_001:13": " kanakaprabhayā paripītatanuṃ",
    "T01n901u_001:14": " varadundubhitoyadavalgurutam |",
    "T01n901u_001:15": " gajahaṃsavilambitadhīragataṃ",
    "T01n901u_001:16": " śirasā'bhinato'smi guṇaikanidhim || 4 ||",
    "T01n901u_001:17": " taruṇārkasamairacalairnayanaiḥ",
    "T01n901u_001:18": " sphuradugramahāśanibhīmaravaiḥ |",
    "T01n901u_001:19": " namucipraharairasiśaktidharai-",
    "T01n901u_001:20": " ścalito'si vibho na hi tvaṃ vikṛtaiḥ || 5 ||",
    "T01n901u_001:21": " lavalīphalapāṇḍurakarṇapuṭāḥ",
    "T01n901u_001:22": " kucabhāravināmitagātralatāḥ |",
    "T01n901u_001:23": " nahi tvāmaśakan khalu nāgasutāḥ",
    "T01n901u_001:24": " śvasitaiḥ skhalitairapi kampayitum || 6 ||",
    "T01n901u_001:25": " taravaḥ kusumastabakābharaṇā",
    "T01n901u_001:26": " bahuratnasahasracitāśca nagāḥ |",
    "T01n901u_001:27": " na tathābhiratiṃ janayanti satāṃ",
    "T01n901u_001:28": " janayanti yathā tava vīraguṇāḥ || 7 ||",
    "T01n901u_001:29": " sphuṭacitrapadaṃ bahuyuktiyutaṃ",
    "T01n901u_001:30": " gamakaṃ vacanaṃ tava karṇasukham |",
    "T01n901u_001:31": " śubhamārgaphalaṃ prasamīkṣyajanā",
    "T01n901u_001:32": " na gṛṇanti punastvadṛte'nyavacaḥ || 8 ||",
    "T01n901u_001:33": " iti vaḥ śaraṇaṃ samavekṣya janā",
    "T01n901u_001:34": " na patantyapi kalpaśatairniraye |",
    "T01n901u_001:35": " vinihatya ca doṣaripūn bahulān",
    "T01n901u_001:36": " pariyanti śubhaṃ varamokṣapuram || 9 ||",
    "T01n901u_001:37": " tava saumyatayā'pyabhibhūtatamo",
    "T01n901u_001:38": " na virājati śītakaro gagane |",
    "T01n901u_001:39": " tava kāñcanakuṅkumasaprabhayā",
    "T01n901u_001:40": " prabhayā'bhihato na vibhāti raviḥ || 10 ||",
    "T01n901u_001:41": " tava nātha śubhe vadanāmburūhe",
    "T01n901u_001:42": " nayanabhramarā nipatanti nṛṇām |",
    "T01n901u_001:43": " pratibuddhadale kamale vimale",
    "T01n901u_001:44": " bhramarā iva puṣpaśatākulitāḥ || 11 ||",
    "T01n901u_001:45": " iti toṭakamantravarairatulaiḥ",
    "T01n901u_001:46": " parikīrtya mayā tava varṇalavān |",
    "T01n901u_001:47": " yadupārjitamadya śubhaṃ vipulaṃ",
    "T01n901u_001:48": " śivamastu tato bhuvi devanṛṇām || 12 ||",
    "T01n901u_001:49": " śrīmāravijayastotraṃ samāptam ||"
}