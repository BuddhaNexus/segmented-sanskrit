{
    "GK16vamkalsu:0": " pāṇḍitavaraśrīvāmanaviracitāni",
    "GK16vamkalsu:1": " kāvyālaṅkārasūtrāṇi",
    "GK16vamkalsu:2": " svavṛttisametāni @--------------------",
    "GK16vamkalsu:3": " śārīraṃ nāma prathamādhikaraṇam /",
    "GK16vamkalsu:4": " prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:5": " praṇamya paramaṃ jyotirvāmanena kavipriyā /",
    "GK16vamkalsu:6": " kāvyālaṅkārasūtrāṇāṃ teṣāṃ vṛttirvidhīyate //",
    "GK16vamkalsu:7": " kāvyaṃ grāhyamalaṅkārāt // VKal_1,1.1 //",
    "GK16vamkalsu:8": " saundaryamalaṅkāraḥ // VKal_1,1.2 //",
    "GK16vamkalsu:9": " sa doṣaguṇālaṅkārahānādānābhyām // VKal_1,1.3 //",
    "GK16vamkalsu:10": " śāstrataste // VKal_1,1.4 //",
    "GK16vamkalsu:11": " kāvyaṃ saddṛṣṭādṛṣṭārtham,",
    "GK16vamkalsu:12": " prītikīrtihetutvāt // VKal_1,1.5 //* @--------------------",
    "GK16vamkalsu:13": " dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:14": " arocakinaḥ satṛṇābhyavahāriṇaśca kavayaḥ // VKal_1,2.1 //",
    "GK16vamkalsu:15": " pūrve śiṣyāḥ,",
    "GK16vamkalsu:16": " vivekitvāt // VKal_1,2.2 //*",
    "GK16vamkalsu:17": " netare, tadviparyayāt // VKal_1,2.3 //*",
    "GK16vamkalsu:18": " na śāstramadravyeṣvarthavat // VKal_1,2.4 //",
    "GK16vamkalsu:19": " na katakaṃ paṅkaprasādanāya // VKal_1,2.5 //",
    "GK16vamkalsu:20": " rītirātmā kāvyasya // VKal_1,2.6 //",
    "GK16vamkalsu:21": " viśiṣṭā padaracanā rītiḥ // VKal_1,2.7 //",
    "GK16vamkalsu:22": " viśeṣo guṇātmā // VKal_1,2.8 //",
    "GK16vamkalsu:23": " sā tridhā --- vaidarbhī gauḍīyā pāñcālī ceti // VKal_1,2.9 //",
    "GK16vamkalsu:24": " vidarbhādiṣu dṛṣṭatvāttatsamākhyā // VKal_1,2.10 //",
    "GK16vamkalsu:25": " samagraguṇopetā vaidarbhī // VKal_1,2.11 //",
    "GK16vamkalsu:26": " ojaḥkāntimatī gauḍīyā // VKal_1,2.12 //",
    "GK16vamkalsu:27": " mādhuryasaukumāryopapannā pāñcālī // VKal_1,2.13 //",
    "GK16vamkalsu:28": " tāsāṃ pūrvā grāhyā,",
    "GK16vamkalsu:29": " guṇasākalyāt // VKal_1,2.14 //*",
    "GK16vamkalsu:30": " na punaritare,",
    "GK16vamkalsu:31": " stokaguṇatvāt // VKal_1,2.15 //*",
    "GK16vamkalsu:32": " tadārohaṇārthamitarābhyāsa ityeke // VKal_1,2.16 //",
    "GK16vamkalsu:33": " tattu na; atattvaśīlasya tattvāniṣpatteḥ // VKal_1,2.17 //*",
    "GK16vamkalsu:34": " na śaṇasūtravānābhyāse trasarasūtravānavaicitryalābhaḥ // VKal_1,2.18 //",
    "GK16vamkalsu:35": " sāpi samāsābhāve śuddhavaidarbhī // VKal_1,2.19 //",
    "GK16vamkalsu:36": " tasyāmarthaguṇasaṃpadāsvādyā // VKal_1,2.20 //",
    "GK16vamkalsu:37": " tadupārohādarthaguṇaleśo 'pi // VKal_1,2.21 //",
    "GK16vamkalsu:38": " sāpi vaidarbhī,",
    "GK16vamkalsu:39": " tātsthyāt // VKal_1,2.22 //* @--------------------",
    "GK16vamkalsu:40": " tṛtīyo 'dhyāyaḥ /",
    "GK16vamkalsu:41": " loko vidyā prakīrṇa ca kāvyāṅgāni // VKal_1,3.1 //",
    "GK16vamkalsu:42": " lokavṛttaṃ lokaḥ // VKal_1,3.2 //",
    "GK16vamkalsu:43": " śabdasmṛtyabhidhānakośacchandovicitikalākāmaśāstradaṇḍanītipūrvā vidyāḥ // VKal_1,3.3 //",
    "GK16vamkalsu:44": " śabdasmṛteḥ śabdaśuddhiḥ // VKal_1,3.4 //",
    "GK16vamkalsu:45": " abhidhānakośāt padārthaniścayaḥ // VKal_1,3.5 //",
    "GK16vamkalsu:46": " chandovicitervṛttasaṃśayacchedaḥ // VKal_1,3.6 //",
    "GK16vamkalsu:47": " kalāśāstrebhyaḥ kalātattvasya saṃvit // VKal_1,3.7 //",
    "GK16vamkalsu:48": " kāmaśāstrataḥ kāmopacārasya // VKal_1,3.8 //",
    "GK16vamkalsu:49": " daṇḍanīternayāpanayayoḥ // VKal_1,3.9 //",
    "GK16vamkalsu:50": " itivṛttakuṭilatvaṃ ca tataḥ // VKal_1,3.10 //",
    "GK16vamkalsu:51": " lakṣyajñatvamabhiyogo vṛddhasevāvekṣaṇaṃ pratibhānamavadhānaṃ ca prakīrṇam // VKal_1,3.11 //",
    "GK16vamkalsu:52": " tatra kāvyaparicayo lakṣyajñatvam // VKal_1,3.12 //",
    "GK16vamkalsu:53": " kāvyabandhodyamo 'bhiyogaḥ // VKal_1,3.13 //",
    "GK16vamkalsu:54": " kāvyopadeśaguruśuśrūṣaṇaṃ vṛddhasevā // VKal_1,3.14 //",
    "GK16vamkalsu:55": " padādhānoddharaṇamavekṣaṇam // VKal_1,3.15 //",
    "GK16vamkalsu:56": " kavitvabījaṃ pratibhānam // VKal_1,3.16 //",
    "GK16vamkalsu:57": " cittaikāgryamavadhānam // VKal_1,3.17 //",
    "GK16vamkalsu:58": " taddeśakālābhyām // VKal_1,3.18 //",
    "GK16vamkalsu:59": " vivikto deśaḥ // VKal_1,3.19 //",
    "GK16vamkalsu:60": " rātriyāmasturīyaḥ kālaḥ // VKal_1,3.20 //",
    "GK16vamkalsu:61": " kāvyaṃ gadyaṃ padyaṃ ca // VKal_1,3.21 //",
    "GK16vamkalsu:62": " gadyaṃ vṛttagandhi cūrṇamutkalikāprāyaṃ ca // VKal_1,3.22 //",
    "GK16vamkalsu:63": " padyabhāgavadvṛttagandhi // VKal_1,3.23 //",
    "GK16vamkalsu:64": " anāviddhalalitapadaṃ cūrṇam // VKal_1,3.24 //",
    "GK16vamkalsu:65": " viparītamutkalikāprāyam // VKal_1,3.25 //",
    "GK16vamkalsu:66": " padyamanekabhedam // VKal_1,3.26 //",
    "GK16vamkalsu:67": " tadanibaddhaṃ nibaddhaṃ ca // VKal_1,3.27 //",
    "GK16vamkalsu:68": " kramasiddhistayoḥ sraguttaṃsavat // VKal_1,3.28 //",
    "GK16vamkalsu:69": " nānibaddhaṃ cakāstyekatejaḥparamāṇuvat // VKal_1,3.29 //",
    "GK16vamkalsu:70": " saṃdarbheṣu daśarūpakaṃ śreyaḥ // VKal_1,3.30 //",
    "GK16vamkalsu:71": " taddhi citraṃ citrapaṭavadviśeṣasākalyāt // VKal_1,3.31 //",
    "GK16vamkalsu:72": " tato 'nyabhedakḷptiḥ // VKal_1,3.32 //",
    "GK16vamkalsu:73": " samāptaṃ cedaṃ śārīraṃ prathamamadhikaraṇam / @-------------------------------------------------------",
    "GK16vamkalsu:74": " doṣadarśanaṃ nāma dvitīyamadhikaraṇam /",
    "GK16vamkalsu:75": " prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:76": " guṇaviparyayātmāno doṣāḥ // VKal_2,1.1 //",
    "GK16vamkalsu:77": " arthatastadavagamaḥ // VKal_2,1.2 //",
    "GK16vamkalsu:78": " saukaryāya prapañcaḥ // VKal_2,1.3 //",
    "GK16vamkalsu:79": " duṣṭaṃ padamasādhu kaṣṭaṃ grāmyamapratītamanarthakaṃ ca // VKal_2,1.4 //",
    "GK16vamkalsu:80": " śabdasmṛtiviruddhamasādhu // VKal_2,1.5 //",
    "GK16vamkalsu:81": " śrutivirasaṃ kaṣṭam // VKal_2,1.6 //",
    "GK16vamkalsu:82": " lokamātraprayuktaṃ grāmyam // VKal_2,1.7 //",
    "GK16vamkalsu:83": " śāstramātraprayuktamapratītam // VKal_2,1.8 //",
    "GK16vamkalsu:84": " pūraṇārthamanarthakam // VKal_2,1.9 //",
    "GK16vamkalsu:85": " na vākyālaṅkārārtham // VKal_2,1.10 //",
    "GK16vamkalsu:86": " anyārthaneyagūḍhārthāślīlakliṣṭāni ca // VKal_2,1.11 //",
    "GK16vamkalsu:87": " rūḍhicyutamanyārtham // VKal_2,1.12 //",
    "GK16vamkalsu:88": " kalpitārthaṃ neyārtham // VKal_2,1.13 //",
    "GK16vamkalsu:89": " aprasiddhārthaprayuktaṃ gūḍhārtham // VKal_2,1.14 //",
    "GK16vamkalsu:90": " asamyārthāntaramasamyasmṛtihetuścāślīlam // VKal_2,1.15 //",
    "GK16vamkalsu:91": " na guptalakṣitasaṃvṛtāni // VKal_2,1.16 //",
    "GK16vamkalsu:92": " aprasiddhāsabhyaṃ guptam // VKal_2,1.17 //",
    "GK16vamkalsu:93": " lākṣaṇikāsabhyānvitaṃ lakṣitam // VKal_2,1.18 //",
    "GK16vamkalsu:94": " lokasaṃvītaṃ saṃvṛtam // VKal_2,1.19 //",
    "GK16vamkalsu:95": " tantraividhyam,",
    "GK16vamkalsu:96": " vrīḍājugupsāmaṅgalātaṅkadāyibhedāt // VKal_2,1.20 //*",
    "GK16vamkalsu:97": " vyavahitārthapratyayaṃ kliṣṭam // VKal_2,1.21 //",
    "GK16vamkalsu:98": " antyābhyāṃ vākyaṃ vyākhyātam // VKal_2,1.22 // @--------------------",
    "GK16vamkalsu:99": " dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:100": " bhinnavṛttayatibhraṣṭavisaṃdhīni vākyāni // VKal_2,2.1 //",
    "GK16vamkalsu:101": " svalakṣaṇacyutavṛttaṃ bhinnavṛttam // VKal_2,2.2 //",
    "GK16vamkalsu:102": " virasavirāmaṃ yatibhraṣṭam // VKal_2,2.3 //",
    "GK16vamkalsu:103": " taddhātunāmabhāgabhede svarasaṃdhyakṛte prāyeṇa // VKal_2,2.4 //",
    "GK16vamkalsu:104": " na vṛttadoṣātpṛthagyatidpṣaḥ,",
    "GK16vamkalsu:105": " vṛttasya yatyātmakatvāt // VKal_2,2.5 //*",
    "GK16vamkalsu:106": " na, lakṣaṇaḥ pṛthaktvāt // VKal_2,2.6 //*",
    "GK16vamkalsu:107": " virūpapadasaṃdhi visaṃdhiḥ // VKal_2,2.7 //",
    "GK16vamkalsu:108": " padasaṃdhervairūpyaṃ viśleṣo 'ślīlatvaṃ kaṣṭatvaṃ ca // VKal_2,2.8 //",
    "GK16vamkalsu:109": " vyarthaikārthasaṃdigdhāyuktāpakramalokavidyā viruddhāni ca // VKal_2,2.9 //",
    "GK16vamkalsu:110": " vyāhatapūrvottarārthaṃ vyartham // VKal_2,2.10 //",
    "GK16vamkalsu:111": " uktārthapadamekārtham // VKal_2,2.11 //",
    "GK16vamkalsu:112": " na, viśeṣaścedekārthaṃ duṣṭam // VKal_2,2.12 //*",
    "GK16vamkalsu:113": " dhanurjyādhvanau dhanuḥśrutirārūḍheḥ pratipattyai // VKal_2,2.13 //",
    "GK16vamkalsu:114": " karṇāvataṃsaśravaṇakuṇḍalaśiraḥśekhareṣu karṇādinirdeśaḥ saṃnidheḥ // VKal_2,2.14 //",
    "GK16vamkalsu:115": " muktāhāraśabde muktāśabdaḥ śuddheḥ // VKal_2,2.15 //",
    "GK16vamkalsu:116": " puṣpamālāśabde puṣpapadamutkarṣasya // VKal_2,2.16 //",
    "GK16vamkalsu:117": " karikalabhaśabde kariśabdastādrūpyasya // VKal_2,2.17 //",
    "GK16vamkalsu:118": " viśeṣaṇasya ca // VKal_2,2.18 //",
    "GK16vamkalsu:119": " tadidaṃ prayukteṣu // VKal_2,2.19 //",
    "GK16vamkalsu:120": " saṃśayakṛtsaṃdigdham // VKal_2,2.20 //",
    "GK16vamkalsu:121": " māyādivikalpitārthamprayuktam // VKal_2,2.21 //",
    "GK16vamkalsu:122": " kramahīnārthamapakramam // VKal_2,2.22 //",
    "GK16vamkalsu:123": " deśakālasvabhāvaviruddhārthāni lokaviruddhāni // VKal_2,2.23 //",
    "GK16vamkalsu:124": " kalācaturvargaśāstraviruddhārthāni vidyāviruddhāni // VKal_2,2.24 //",
    "GK16vamkalsu:125": " samāotaṃ cedaṃ doṣadarśanam dvitīyamadhikaraṇam / @-----------------------------------------------",
    "GK16vamkalsu:126": " guṇavivecanaṃ nāma tṛtīyamadhikaraṇam /",
    "GK16vamkalsu:127": " prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:128": " kāvyaśobhāyāḥ kartāro dharmā guṇāḥ // VKal_3,1.1 //",
    "GK16vamkalsu:129": " tadatiśayahetavastvalaṅkārāḥ // VKal_3,1.2 //",
    "GK16vamkalsu:130": " pūrve nityāḥ // VKal_3,1.3 //",
    "GK16vamkalsu:131": " ojaḥprasādaśleṣasamatāsamādhimādhuryasaukumāryodāratārthavyaktikāntayo bandhaguṇāḥ // VKal_3,1.4 //",
    "GK16vamkalsu:132": " gāḍhabandhatvamojaḥ // VKal_3,1.5 //",
    "GK16vamkalsu:133": " śaithilyaṃ prasādaḥ // VKal_3,1.6 //",
    "GK16vamkalsu:134": " guṇaḥ saṃplavāt // VKal_3,1.7 //",
    "GK16vamkalsu:135": " sa tvanubhavasiddhaḥ // VKal_3,1.8 //",
    "GK16vamkalsu:136": " sāmyotkarṣau ca // VKal_3,1.9 //",
    "GK16vamkalsu:137": " masṛṇatvaṃ śleṣaḥ // VKal_3,1.10 //",
    "GK16vamkalsu:138": " mārgāvhedaḥ samatā // VKal_3,1.11 //",
    "GK16vamkalsu:139": " ārohāvarohakramaḥ samādhiḥ // VKal_3,1.12 //",
    "GK16vamkalsu:140": " na pṛthak, ārohāvarohayorojaḥprasādarūpatvāt // VKal_3,1.13 //*",
    "GK16vamkalsu:141": " na, asaṃpṛktatvāt // VKal_3,1.14 //*",
    "GK16vamkalsu:142": " anaikāntyācca // VKal_3,1.15 //",
    "GK16vamkalsu:143": " ojaḥprasādayoḥ tīvrāvasthā,",
    "GK16vamkalsu:144": " tāviti cedabhyupagamaḥ // VKal_3,1.16 //*",
    "GK16vamkalsu:145": " viśeṣāpekṣitvāttayoḥ // VKal_3,1.17 //",
    "GK16vamkalsu:146": " ārohāvarohanimittaṃ samādhirākhyāyate // VKal_3,1.18 //",
    "GK16vamkalsu:147": " kramavidhānārthaṃ vā // VKal_3,1.19 //",
    "GK16vamkalsu:148": " pṛthakpadatvaṃ mādhuryam // VKal_3,1.20 //",
    "GK16vamkalsu:149": " ajaraṭhatvaṃ saukumāryam // VKal_3,1.21 //",
    "GK16vamkalsu:150": " vikaṭatvamudāratā // VKal_3,1.22 //",
    "GK16vamkalsu:151": " arthavyaktihetutvamarthavyaktiḥ // VKal_3,1.23 //",
    "GK16vamkalsu:152": " aujjvalyaṃ kāntiḥ // VKal_3,1.24 //",
    "GK16vamkalsu:153": " nāsantaḥ, sadvedyatvāt // VKal_3,1.25 //*",
    "GK16vamkalsu:154": " na bhrāntāḥ,",
    "GK16vamkalsu:155": " niṣkampatvāt // VKal_3,1.26 //*",
    "GK16vamkalsu:156": " na pāṭhadharmāḥ,",
    "GK16vamkalsu:157": " sarvatrādṛṣṭeḥ // VKal_3,1.27 //* @--------------------",
    "GK16vamkalsu:158": " dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:159": " ta evārthaguṇāḥ // VKal_3,2.1 //",
    "GK16vamkalsu:160": " arthasya prauḍhirojaḥ // VKal_3,2.2 //",
    "GK16vamkalsu:161": " arthavaimalyaṃ prasādaḥ // VKal_3,2.3 //",
    "GK16vamkalsu:162": " ghaṭanā śleṣaḥ // VKal_3,2.4 //",
    "GK16vamkalsu:163": " avaiṣamyaṃ samatā // VKal_3,2.5 //",
    "GK16vamkalsu:164": " arthadṛṣṭiḥ samādhiḥ // VKal_3,2.6 //",
    "GK16vamkalsu:165": " artho dvividhaḥ,",
    "GK16vamkalsu:166": " ayoniranyacchāyāyonirvā // VKal_3,2.7 //*",
    "GK16vamkalsu:167": " artho vyaktaḥ sūkṣmaśca // VKal_3,2.8 //",
    "GK16vamkalsu:168": " sūkṣmo bhāvyo vāsanīyaśca // VKal_3,2.9 //",
    "GK16vamkalsu:169": " uktivaicitryaṃ mādhuryam // VKal_3,2.10 //",
    "GK16vamkalsu:170": " apāruṣyaṃ saukumāryam // VKal_3,2.11 //",
    "GK16vamkalsu:171": " agrāmyatvamudāratā // VKal_3,2.12 //",
    "GK16vamkalsu:172": " vastusvabhāvasphuṭatvamarthavyaktiḥ // VKal_3,2.13 //",
    "GK16vamkalsu:173": " dīptarasatvaṃ kāntiḥ // VKal_3,2.14 //",
    "GK16vamkalsu:174": " samāotaṃ cedaṃ guṇavivecanaṃ tṛtīyamadhikaraṇam / @-----------------------------------------------",
    "GK16vamkalsu:175": " ālaṅkārikaṃ nāma caturthamadhikaraṇam /",
    "GK16vamkalsu:176": " prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:177": " padamanaikārthamakṣaraṃ cāvṛttaṃ sthānaniyame yamakam // VKal_4,1.1 //",
    "GK16vamkalsu:178": " pādāḥ pādasyaikasyānekasya cādimadhyāntabhāgāḥ sthānāni // VKal_4,1.2 //",
    "GK16vamkalsu:179": " bhaṅgādutkarṣaḥ // VKal_4,1.3 //",
    "GK16vamkalsu:180": " śṛṅkhalā parivartakaścūrṇamiti bhaṅgamārgaḥ // VKal_4,1.4 //",
    "GK16vamkalsu:181": " varṇavicchedacalanaṃ śṛṅkhalā // VKal_4,1.5 //",
    "GK16vamkalsu:182": " saṅgavinivṛttau svarūpāpattiḥ parivartakaḥ // VKal_4,1.6 //",
    "GK16vamkalsu:183": " piṇḍākṣarabhede svarūpalopaścūrṇam // VKal_4,1.7 //",
    "GK16vamkalsu:184": " śeṣaḥ sarūpo 'nuprāsaḥ // VKal_4,1.8 //",
    "GK16vamkalsu:185": " anulbaṇo varṇānuprāsaḥ śreyān // VKal_4,1.9 //",
    "GK16vamkalsu:186": " pādānuprāsaḥ pādayamakavat // VKal_4,1.10 // @--------------------",
    "GK16vamkalsu:187": " dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:188": " upamānenopameyasya guṇaleśataḥ sāmyamupamā // VKal_4,2.1 //",
    "GK16vamkalsu:189": " guṇabāhulyataśca kalpitā // VKal_4,2.2 //",
    "GK16vamkalsu:190": " taddvaividhyam,",
    "GK16vamkalsu:191": " padavākyārthavṛttibhedāt // VKal_4,2.3 //*",
    "GK16vamkalsu:192": " sā pūṇā luptā ca // VKal_4,2.4 //",
    "GK16vamkalsu:193": " guṇadyotakopamānopameyaśabdānāṃ sāmagrye pūrṇā // VKal_4,2.5 //",
    "GK16vamkalsu:194": " lope luptā // VKal_4,2.6 //",
    "GK16vamkalsu:195": " stutinindātattvākhyāneṣu // VKal_4,2.7 //",
    "GK16vamkalsu:196": " hīnatvādhikatvaliṅgavacanabhedāsādṛśyāsaṃbhavāstaddoṣāḥ // VKal_4,2.8 //",
    "GK16vamkalsu:197": " jātipramāṇadharmanyūnatopamānasya hīnatvam // VKal_4,2.9 //",
    "GK16vamkalsu:198": " dharmayorekanirdeśe 'nyasya saṃvit,",
    "GK16vamkalsu:199": " sāhacaryāt // VKal_4,2.10 //*",
    "GK16vamkalsu:200": " tenādhikarvaṃ vyākhyātam // VKal_4,2.11 //",
    "GK16vamkalsu:201": " upamānopameyayorliṅgavyatyāso liṅgabhedaḥ // VKal_4,2.12 //",
    "GK16vamkalsu:202": " iṣṭaḥ puṃpuṃsakayoḥ prāyeṇa // VKal_4,2.13 //",
    "GK16vamkalsu:203": " laukikyāṃ samāsābhihitāyāmupamāprapañce ca // VKal_4,2.14 //",
    "GK16vamkalsu:204": " tena vacanabhedo vyākhyātaḥ // VKal_4,2.15 //",
    "GK16vamkalsu:205": " apratītaguṇasādṛśyamasādṛśyam // VKal_4,2.16 //",
    "GK16vamkalsu:206": " asādṛśyahatā hyupamā,",
    "GK16vamkalsu:207": " tanniṣṭhāśca kavayaḥ // VKal_4,2.17 //*",
    "GK16vamkalsu:208": " upamānādhikyāttadapoha ityeke // VKal_4,2.18 //",
    "GK16vamkalsu:209": " na, apuṣṭārthatvāt // VKal_4,2.19 //*",
    "GK16vamkalsu:210": " anupapattirasaṃbhavaḥ // VKal_4,2.20 //",
    "GK16vamkalsu:211": " na viruddho 'tiśayaḥ // VKal_4,2.21 // @--------------------",
    "GK16vamkalsu:212": " tṛtīyo 'dhyāyaḥ /",
    "GK16vamkalsu:213": " prativastuprabhṛtirupamāprapañcaḥ // VKal_4,3.1 //",
    "GK16vamkalsu:214": " upameyasyoktau samānavastunyāsaḥ prativastu // VKal_4,3.2 //",
    "GK16vamkalsu:215": " anuktau samāsoktiḥ // VKal_4,3.3 //",
    "GK16vamkalsu:216": " kiñciduktāvaprastutapraśaṃsā // VKal_4,3.4 //",
    "GK16vamkalsu:217": " samena vastunānyāpalāpo 'pahnutiḥ // VKal_4,3.5 //",
    "GK16vamkalsu:218": " upamānenopameyasya guṇasāmyāttattvāropo rūpakam // VKal_4,3.6 //",
    "GK16vamkalsu:219": " sa dharmeṣu tantraprayoge śleṣaḥ // VKal_4,3.7 //",
    "GK16vamkalsu:220": " sādṛśyāllakṣaṇā vakroktiḥ // VKal_4,3.8 //",
    "GK16vamkalsu:221": " atadrūpasyānyathādhyavasānamatiśayārthamutprekṣā // VKal_4,3.9 //",
    "GK16vamkalsu:222": " saṃbhāvyadharmatadutkarṣakalpanātiśayoktiḥ // VKal_4,3.10 //",
    "GK16vamkalsu:223": " upamānopameyasaṃśayaḥ saṃdehaḥ // VKal_4,3.11 //",
    "GK16vamkalsu:224": " viruddhābhāsatvaṃ virodhaḥ // VKal_4,3.12 //",
    "GK16vamkalsu:225": " kriyāpratiṣedhe prasiddhatatphalavyaktirvibhāvanā // VKal_4,3.13 //",
    "GK16vamkalsu:226": " ekasyopameyatvopamānatve 'nanvayaḥ // VKal_4,3.14 //",
    "GK16vamkalsu:227": " krameṇopameyopamā // VKal_4,3.15 //",
    "GK16vamkalsu:228": " samavisadṛśābhyāṃ parivartanaṃ parivṛttiḥ // VKal_4,3.16 //",
    "GK16vamkalsu:229": " upameyopamānānāṃ kramasaṃbandhaḥ kramaḥ // VKal_4,3.17 //",
    "GK16vamkalsu:230": " upamānopameyavākyeṣvekā kriyā dīpakam // VKal_4,3.18 //",
    "GK16vamkalsu:231": " tantrividham /",
    "GK16vamkalsu:232": " ādimadhyāntavākyavṛttibhedāt // VKal_4,3.19 //*",
    "GK16vamkalsu:233": " kriyayaiva svatadarthānvayakhyāpanaṃ nidarśanam // VKal_4,3.20 //",
    "GK16vamkalsu:234": " uktisiddhyai vastuno 'rthāntarasyaiva nyasanamarthāntaranyāsaḥ // VKal_4,3.21 //",
    "GK16vamkalsu:235": " upameyasya guṇātirekatvaṃ vyatirekaḥ // VKal_4,3.22 //",
    "GK16vamkalsu:236": " ekaguṇahānikalpanāyāṃ sāmyadārḍhyaṃ viśeṣoktiḥ // VKal_4,3.23 //",
    "GK16vamkalsu:237": " saṃbhāvyaviśiṣṭakarmākaraṇānnindāstotrārthā vyājastutiḥ // VKal_4,3.24 //",
    "GK16vamkalsu:238": " vyājasya satyasārūpyaṃ vyājoktiḥ // VKal_4,3.25 //",
    "GK16vamkalsu:239": " viśiṣṭena sāmyārthamekakālakriyāyogastulyayogitā // VKal_4,3.26 //",
    "GK16vamkalsu:240": " upamānākṣepaścākṣepaḥ // VKal_4,3.27 //",
    "GK16vamkalsu:241": " vastudvayakriyayostulyakālayorekapadābhidhānaṃ sahoktiḥ // VKal_4,3.28 //",
    "GK16vamkalsu:242": " yatsādṛśyaṃ tatsaṃpattiḥ samāhitam // VKal_4,3.29 //",
    "GK16vamkalsu:243": " alaṅkārasyālaṅkārayonitvaṃ saṃsṛṣṭiḥ // VKal_4,3.30 //",
    "GK16vamkalsu:244": " tadbhedāvupamārūpakotprekṣāvayavau // VKal_4,3.31 //",
    "GK16vamkalsu:245": " upamājanyaṃ rūpakamupamārūpakam // VKal_4,3.32 //",
    "GK16vamkalsu:246": " utprekṣāheturutprekṣāvayavaḥ // VKal_4,3.33 //",
    "GK16vamkalsu:247": " samāotaṃ cedamālaṅkārikaṃ caturthamadhikaraṇam / @-----------------------------------------------",
    "GK16vamkalsu:248": " prāyogikaṃ nāma pañcamamadhikaraṇam /",
    "GK16vamkalsu:249": " prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:250": " naikaṃ padaṃ dviḥ prayojyaṃ prāyeṇa // VKal_5,1.1 //",
    "GK16vamkalsu:251": " nityā saṃhitaikapadavatpādeṣvardhāntavarjam // VKal_5,1.2 //",
    "GK16vamkalsu:252": " na pādāntalaghorgurutvaṃ ca sarvatra // VKal_5,1.3 //",
    "GK16vamkalsu:253": " na gādye samāptaprāyaṃ vṛttam,",
    "GK16vamkalsu:254": " anyatrodgatādibhyaḥ saṃvādāt // VKal_5,1.4 //*",
    "GK16vamkalsu:255": " na pādādau khalvādayaḥ // VKal_5,1.5 //",
    "GK16vamkalsu:256": " nārdhe kiñcitsamāptaṃ vākyam // VKal_5,1.6 //",
    "GK16vamkalsu:257": " na karmadhārayo bahuvrīhipratipattikaraḥ // VKal_5,1.7 //",
    "GK16vamkalsu:258": " tena viparyayo vyākhyātaḥ // VKal_5,1.8 //",
    "GK16vamkalsu:259": " saṃbhāvyaniṣedhanivartane dvau pratiṣedhau // VKal_5,1.9 //",
    "GK16vamkalsu:260": " viśeṣaṇamātraprayogo viśeṣyapratipattau // VKal_5,1.10 //",
    "GK16vamkalsu:261": " sarvanāmnānusaṃdhirvṛtticchannasya // VKal_5,1.11 //",
    "GK16vamkalsu:262": " saṃbandhasaṃbandhe 'pi ṣaṣṭhī kvacit // VKal_5,1.12 //",
    "GK16vamkalsu:263": " atiprayuktaṃ deśabhāṣāpadam // VKal_5,1.13 //",
    "GK16vamkalsu:264": " liṅgādhyāhārau // VKal_5,1.14 //",
    "GK16vamkalsu:265": " lakṣaṇāśabdāśca // VKal_5,1.15 //",
    "GK16vamkalsu:266": " na tadbāhulyamekatra // VKal_5,1.16 //",
    "GK16vamkalsu:267": " stanādīnāṃ dvitvāviṣṭā jātiḥ prāyeṇa // VKal_5,1.17 // @--------------------",
    "GK16vamkalsu:268": " dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:269": " rudrāvityekaśeṣo 'nveṣyaḥ // VKal_5,2.1 //",
    "GK16vamkalsu:270": " miliklabikṣapiprabhṛtīnāṃ dhātutvam,",
    "GK16vamkalsu:271": " dhātuguṇasyāsamāpteḥ // VKal_5,2.2 //*",
    "GK16vamkalsu:272": " valerātmanepadamanityam,",
    "GK16vamkalsu:273": " jñāpakāt // VKal_5,2.3 //*",
    "GK16vamkalsu:274": " kṣīyata iti karmakartari // VKal_5,2.4 //",
    "GK16vamkalsu:275": " khidyata iti ca // VKal_5,2.5 //",
    "GK16vamkalsu:276": " mārgerātmaepadamalakṣma // VKal_5,2.6 //",
    "GK16vamkalsu:277": " lolamānādayaścānaśi // VKal_5,2.7 //",
    "GK16vamkalsu:278": " labhergatyarthatvāṇṇicyaṇau kartuḥ karmatvākarmatve // VKal_5,2.8 //",
    "GK16vamkalsu:279": " te-me-śabdau nipāteṣu // VKal_5,2.9 //*",
    "GK16vamkalsu:280": " tiraskṛta iti paribhūte,",
    "GK16vamkalsu:281": " antardhyupacārāt // VKal_5,2.10 //*",
    "GK16vamkalsu:282": " naikaśabdaḥ,",
    "GK16vamkalsu:283": " supsupeti samāsāt // VKal_5,2.11 //*",
    "GK16vamkalsu:284": " madhupipāsuprabhṛtīnāṃ samāso gamigāmyādiṣu pāṭhāt // VKal_5,2.12 //",
    "GK16vamkalsu:285": " trivalīśabdaḥ siddhaḥ saṃjñā cet // VKal_5,2.13 //",
    "GK16vamkalsu:286": " bimbādhara iti vṛttau madhyamapadalopinyām // VKal_5,2.14 //",
    "GK16vamkalsu:287": " āmūlalolādiṣu vṛttirvispaṣṭapaṭuvat // VKal_5,2.15 //",
    "GK16vamkalsu:288": " na dhānyaṣaṣṭhādiṣu ṣaṣṭhīsamāsapratiṣedhaḥ,",
    "GK16vamkalsu:289": " pūraṇena taddhitāntatvāt // VKal_5,2.16 //*",
    "GK16vamkalsu:290": " patrapītimādiṣu guṇavacanena // VKal_5,2.17 //",
    "GK16vamkalsu:291": " avarjyo bahuvrīhirvyadhikaraṇo janmādyuttarapadaḥ // VKal_5,2.18 //",
    "GK16vamkalsu:292": " hastāgrāgrahastādayo guṇaguṇinorbhedābhedāt // VKal_5,2.19 //",
    "GK16vamkalsu:293": " pūrvanipāte 'pabhraṃśo rakṣyaḥ // VKal_5,2.20 //",
    "GK16vamkalsu:294": " nipātenāpyabhihite karmaṇi na karmavibhaktiḥ,",
    "GK16vamkalsu:295": " parigaṇanasya prāyikatvāt // VKal_5,2.21 //*",
    "GK16vamkalsu:296": " śakyamiti rūpaṃ karmābhidhāyāṃ liṅgavacanasyāpi sāmānyopakramāt // VKal_5,2.22 //",
    "GK16vamkalsu:297": " hānivadādhikyamapyaṅgānāṃ vikāraḥ // VKal_5,2.23 //",
    "GK16vamkalsu:298": " na kṛmikīṭānāmiti,",
    "GK16vamkalsu:299": " ekavadbhāvaprasaṅgāt // VKal_5,2.24 //*",
    "GK16vamkalsu:300": " na kharoṣṭrau,",
    "GK16vamkalsu:301": " uṣṭrakharamiti pāṭhāt // VKal_5,2.25 //*",
    "GK16vamkalsu:302": " āsetyasateḥ // VKal_5,2.26 //",
    "GK16vamkalsu:303": " yudhyediti yudhaḥ kyaci // VKal_5,2.27 //",
    "GK16vamkalsu:304": " viralāyamānādiṣu kyaṅ nirūpyaḥ // VKal_5,2.28 //",
    "GK16vamkalsu:305": " ahetau hanterṇic,",
    "GK16vamkalsu:306": " curādipāṭhāt // VKal_5,2.29 //*",
    "GK16vamkalsu:307": " anucarīti careṣṭittvāt // VKal_5,2.30 //",
    "GK16vamkalsu:308": " kesarālamityalateraṇi // VKal_5,2.31 //",
    "GK16vamkalsu:309": " patralamiti lāteḥ ke // VKal_5,2.32 //",
    "GK16vamkalsu:310": " mahīdhrādayo mūlavibhujādidarśanāt // VKal_5,2.33 //",
    "GK16vamkalsu:311": " brahmādiṣu hanterniyamādarihādyasiddhiḥ // VKal_5,2.34 //",
    "GK16vamkalsu:312": " brahmavidādayaḥ kṛdantavṛttyā // VKal_5,2.35 //",
    "GK16vamkalsu:313": " tairmahīdharādayo vyākhyātāḥ // VKal_5,2.36 //",
    "GK16vamkalsu:314": " bhidurādayaḥ karmakartari kartari ca // VKal_5,2.37 //",
    "GK16vamkalsu:315": " guṇavistarādayaścintyāḥ // VKal_5,2.38 //",
    "GK16vamkalsu:316": " avatarāpacāyaśabdayordīrghahrasvatvavyatyāso bālānām // VKal_5,2.39 //",
    "GK16vamkalsu:317": " śobheti nipātanāt // VKal_5,2.40 //",
    "GK16vamkalsu:318": " avidhau guroḥ striyāṃ bahulaṃ vivakṣā // VKal_5,2.41 //",
    "GK16vamkalsu:319": " vyavasitādiṣu ktaḥ kartari, cakārāt // VKal_5,2.42 //*",
    "GK16vamkalsu:320": " āheti bhūte 'nyaṇalantabhramād bruvo laṭi // VKal_5,2.43 //",
    "GK16vamkalsu:321": " śabalādibhyaḥ striyāṃ ṭāpo 'prāptiḥ // VKal_5,2.44 //",
    "GK16vamkalsu:322": " prāṇini nīleti cintyam // VKal_5,2.45 //",
    "GK16vamkalsu:323": " manuṣyajātervivakṣāvivakṣe // VKal_5,2.46 //",
    "GK16vamkalsu:324": " ūkārāntādapyūṅ,",
    "GK16vamkalsu:325": " pravṛtteḥ // VKal_5,2.47 //*",
    "GK16vamkalsu:326": " kārtikīya iti ṭhañ durdharaḥ // VKal_5,2.48 //",
    "GK16vamkalsu:327": " śārvaramiti ca // VKal_5,2.49 //",
    "GK16vamkalsu:328": " śāśvatamiti prayukteḥ // VKal_5,2.50 //",
    "GK16vamkalsu:329": " rājavaṃśyādayaḥ sādhvarthe ye bhavanti // VKal_5,2.51 //",
    "GK16vamkalsu:330": " dāravaśabdo duṣprayuktaḥ // VKal_5,2.52 //",
    "GK16vamkalsu:331": " mugdhimādiṣvivamanij mṛgyaḥ // VKal_5,2.53 //",
    "GK16vamkalsu:332": " aupamyādayaścāturvarṇyavat // VKal_5,2.54 //",
    "GK16vamkalsu:333": " ṣyañaḥ ṣitkaraṇādīkāro bahulam // VKal_5,2.55 //",
    "GK16vamkalsu:334": " dhanvīti vrīhyādiṣu pāṭhāt // VKal_5,2.56 //",
    "GK16vamkalsu:335": " caturasraśobhīti ṇinau // VKal_5,2.57 //",
    "GK16vamkalsu:336": " kañcukīyā iti kyaci // VKal_5,2.58 //",
    "GK16vamkalsu:337": " bauddhapratiyogyapekṣāyāmapyātiśāyanikāḥ // VKal_5,2.59 //",
    "GK16vamkalsu:338": " kauśilādaya ilaci varṇalopāt // VKal_5,2.60 //",
    "GK16vamkalsu:339": " mauktikamiti vinayādipāṭhāt // VKal_5,2.61 //",
    "GK16vamkalsu:340": " prātibhādayaḥ prajñādiṣu // VKal_5,2.62 //",
    "GK16vamkalsu:341": " na sarajasamityanavyayībhāve // VKal_5,2.63 //",
    "GK16vamkalsu:342": " dhṛtadhanuṣītyasaṃjñāyām // VKal_5,2.64 //",
    "GK16vamkalsu:343": " durgandhipada iddurlabhaḥ // VKal_5,2.65 //",
    "GK16vamkalsu:344": " sudatyādayaḥ pratividheyāḥ // VKal_5,2.66 //",
    "GK16vamkalsu:345": " kṣatadṛḍhorsa iti na kap,",
    "GK16vamkalsu:346": " tadantavidhipratiṣedhāt // VKal_5,2.67 //*",
    "GK16vamkalsu:347": " avaihīti vṛddhiravadyā // VKal_5,2.68 //",
    "GK16vamkalsu:348": " apāṅganetreti lugalabhyaḥ // VKal_5,2.69 //",
    "GK16vamkalsu:349": " neṣṭhāḥ śliṣṭapriyādayaḥ,",
    "GK16vamkalsu:350": " puṃvadbhāvapratiṣedhāt // VKal_5,2.70 //*",
    "GK16vamkalsu:351": " dṛḍhabhaktiriti sarvatreti // VKal_5,2.71 //",
    "GK16vamkalsu:352": " jambulatādayo hrasvavidheḥ // VKal_5,2.72 //",
    "GK16vamkalsu:353": " tilakādayo 'jirādiṣu // VKal_5,2.73 //",
    "GK16vamkalsu:354": " niśamya-niśamayyaśabdau prakṛtibhedāt // VKal_5,2.74 //",
    "GK16vamkalsu:355": " saṃyamyaniyamyaśabdau,",
    "GK16vamkalsu:356": " aṇijantatvāt // VKal_5,2.75 //*",
    "GK16vamkalsu:357": " prapīyeti pīṅaḥ // VKal_5,2.76 //",
    "GK16vamkalsu:358": " dūrayatīti bahulagrahaṇāt // VKal_5,2.77 //",
    "GK16vamkalsu:359": " gacchatīprabhṛtiṣvaniṣedhyo num // VKal_5,2.78 //",
    "GK16vamkalsu:360": " mitreṇa goptreti puṃvadbhāvāt // VKal_5,2.79 //",
    "GK16vamkalsu:361": " vetsyasīti padabhaṅgāt // VKal_5,2.80 //",
    "GK16vamkalsu:362": " kāmayānaśabdaḥ siddho 'nādiścet // VKal_5,2.81 //",
    "GK16vamkalsu:363": " sauhṛd-daurhṛdaśabdāvaṇi hṛdbhāvāt // VKal_5,2.82 //",
    "GK16vamkalsu:364": " virama iti nipātanāt // VKal_5,2.83 //",
    "GK16vamkalsu:365": " uparyādiṣu sāmīpye dvirukteṣu dvitīyā // VKal_5,2.84 //",
    "GK16vamkalsu:366": " mandaṃ mandamityaprakārārthatve // VKal_5,2.85 //",
    "GK16vamkalsu:367": " na nidrādrugiti,",
    "GK16vamkalsu:368": " bhaṣbhāvaprāpteḥ // VKal_5,2.86 //*",
    "GK16vamkalsu:369": " niṣpanda iti patvaṃ cintyam // VKal_5,2.87 //",
    "GK16vamkalsu:370": " nāṅgulisaṅga iti mūrdhanyavidheḥ // VKal_5,2.88 //",
    "GK16vamkalsu:371": " tenāvantisenādayaḥ pratyuktāḥ // VKal_5,2.89 //",
    "GK16vamkalsu:372": " nendravāhane ṇatvam;",
    "GK16vamkalsu:373": " āhitatvasyāvivakṣitatvāt // VKal_5,2.90 //*",
