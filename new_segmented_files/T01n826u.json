{
    "T01n826u_001:0": " rūpastavaḥ",
    "T01n826u_001:1": " sarvabhūtamaṇikampita tubhyaṃ",
    "T01n826u_001:2": " kṣāntimeva caritaṃ tava rūpam |",
    "T01n826u_001:3": " sarvarūpavaradivyasurūpaṃ",
    "T01n826u_001:4": " taṃ namāmi daśabalavararūpam || 1 ||",
    "T01n826u_001:5": " bhṛṅgarājaśikhimūrdhni sukeśaṃ",
    "T01n826u_001:6": " kokilābhaśikhidivyasukeśam |",
    "T01n826u_001:7": " snigdhanīlamṛdukuñcitakeśaṃ",
    "T01n826u_001:8": " taṃ namāmi daśabalavarakeśam || 2 ||",
    "T01n826u_001:9": " śaṅkhakundakumudaṃ vimaromaṃ",
    "T01n826u_001:10": " janmaduḥkhavigataṃ vimaromam |",
    "T01n826u_001:11": " rogaśokavimate vimaromaṃ",
    "T01n826u_001:12": " taṃ namāmi daśabalavararomam || 3 ||",
    "T01n826u_001:13": " pūrṇacandradyutiśobhitavaktraṃ",
    "T01n826u_001:14": " tuṅganāsamaṇibhāśubhavaktram |",
    "T01n826u_001:15": " buddhapaṅkajinabhāmaravaktraṃ",
    "T01n826u_001:16": " taṃ namāmi daśabalavaravaktram || 4 ||",
    "T01n826u_001:17": " raśmisahasravicitrasumūrdhni",
    "T01n826u_001:18": " puṣpavarṣaśatapūjitamūrdhni |",
    "T01n826u_001:19": " ratnavarṣaśatapūjitamūrdhni",
    "T01n826u_001:20": " taṃ namāmi daśabalavaramūrdhni || 5 ||",
    "T01n826u_001:21": " netrapatākavilambitaśīrṣaṃ",
    "T01n826u_001:22": " kāñcanachatravaropitaśīrṣam |",
    "T01n826u_001:23": " netravitānasuśobhitaśīrṣaṃ",
    "T01n826u_001:24": " taṃ namāmi daśabalavaraśīrṣam || 6 ||",
    "T01n826u_001:25": " divyāmbaramukuṭaṃ maṇimukuṭaṃ",
    "T01n826u_001:26": " candraprabhāmukuṭāmaṇimukuṭam |",
    "T01n826u_001:27": " āgataśīrṣamakhaṇḍitamukuṭaṃ",
    "T01n826u_001:28": " taṃ namāmi daśabalavaramukuṭam || 7 ||",
    "T01n826u_001:29": " cārudalāyatapaṅkajanetraṃ",
    "T01n826u_001:30": " divyajñānavipulāyatanetram |",
    "T01n826u_001:31": " dhyānamokṣaśubhasaṃskṛtanetraṃ",
    "T01n826u_001:32": " taṃ namāmi daśabalavaranetram || 8 ||",
    "T01n826u_001:33": " śuddhakarṇaśubhaśuddhasukarṇaṃ",
    "T01n826u_001:34": " śuddhaprajñavaraśobhitakarṇam |",
    "T01n826u_001:35": " śuddhajñānavaraśuddhasukarṇaṃ",
    "T01n826u_001:36": " taṃ namāmi daśabalavarakarṇam || 9 ||",
    "T01n826u_001:37": " śubhrakundaśaśipaṅkajadantaṃ",
    "T01n826u_001:38": " tīkṣṇasulakṣaṇanirmaladantam |",
    "T01n826u_001:39": " viśvapañcadaśapañcasudantaṃ",
    "T01n826u_001:40": " taṃ namāmi daśabalavaradantam || 10 ||",
    "T01n826u_001:41": " uccavākyasasurāsurajihvāṃ",
    "T01n826u_001:42": " dharmajñānakṛtapāpadajihvām |",
    "T01n826u_001:43": " gandhadhūpasasurāsurajihvāṃ",
    "T01n826u_001:44": " taṃ namāmi daśabalavarajihvām || 11 ||",
    "T01n826u_001:45": " meghasudundubhināditaghoṣaṃ",
    "T01n826u_001:46": " satyadharmanayasuviditaghoṣam |",
    "T01n826u_001:47": " divyavādiparavāditaghoṣaṃ",
    "T01n826u_001:48": " taṃ namāmi daśabalavaraghoṣam || 12 ||",
    "T01n826u_001:49": " grīvagrīvadaśagrīvasugrīvaṃ",
    "T01n826u_001:50": " kṣāntivīryaṃ tava grīvasugrīvam |",
    "T01n826u_001:51": " hemavarṇamaṇigrīvasugrīvaṃ",
    "T01n826u_001:52": " taṃ namāmi daśabalavaragrīvam || 13 ||",
    "T01n826u_001:53": " siṃhakāyahimakundasukāyaṃ",
    "T01n826u_001:54": " dhyānakāyaguṇasāgarakāyam |",
    "T01n826u_001:55": " aṣṭasulakṣaṇanirmalakāyaṃ",
    "T01n826u_001:56": " taṃ namāmi daśabalavarakāyam || 14 ||",
    "T01n826u_001:57": " śvetaraktaśubhaśobhitavastraṃ",
    "T01n826u_001:58": " nīlapītaharitāyitavastram |",
    "T01n826u_001:59": " hāradhavalaśubhaśobhitavastraṃ",
    "T01n826u_001:60": " taṃ namāmi daśabalavaravastram || 15 ||",
    "T01n826u_001:61": " hemanāgakarasaṃskṛtabāhū",
    "T01n826u_001:62": " puṣpadānakṛtaśobhitabāhū |",
    "T01n826u_001:63": " dhīravīryasumanoharabāhū",
    "T01n826u_001:64": " taṃ (tau) namāmi daśabalavarabāhū || 16 ||",
    "T01n826u_001:65": " cāmaracakrasuśobhitahastaṃ",
    "T01n826u_001:66": " vimalasukomalapaṅkajahastam |",
    "T01n826u_001:67": " bhūtapretaśaraṇāgatahastaṃ",
    "T01n826u_001:68": " taṃ namāmi daśabalavarahastam || 17 ||",
    "T01n826u_001:69": " rāgadveṣatanuvarjitacittaṃ",
    "T01n826u_001:70": " kalpakoṭisamabhāvitacittam |",
    "T01n826u_001:71": " śīlajñānasamabhāvitacittaṃ",
    "T01n826u_001:72": " taṃ namāmi daśabalavaracittam || 18 ||",
    "T01n826u_001:73": " padmanābhaśubhaśobhitanābhaṃ",
    "T01n826u_001:74": " padmayoniśubhaśobhitanābham |",
    "T01n826u_001:75": " brahmajyotisuśobhitanābhaṃ",
    "T01n826u_001:76": " taṃ namāmi daśabalavaranābham || 19 ||",
    "T01n826u_001:77": " cakrasulakṣaṇabhūṣitapādaṃ",
    "T01n826u_001:78": " aṅkuśaśaktivirājitapādam |",
    "T01n826u_001:79": " nāgayakṣagaṇavanditapādaṃ",
    "T01n826u_001:80": " taṃ namāmi daśabalavarapādam || 20 ||",
    "T01n826u_001:81": " bhāskaradyutirājitapādaṃ",
    "T01n826u_001:82": " devadānavagaṇavanditapādam |",
    "T01n826u_001:83": " sarvadevagaṇavanditapādaṃ",
    "T01n826u_001:84": " taṃ namāmi daśabalavarapādam || 21 ||",
    "T01n826u_001:85": " sarvadevagaṇapūjitaśīrṣaṃ",
    "T01n826u_001:86": " sūryakoṭisamabhāvitaśīrṣam |",
    "T01n826u_001:87": " divyajvālajvalitārpitaśīrṣaṃ",
    "T01n826u_001:88": " taṃ namāmi daśabalavaraśīrṣam || 22 ||",
    "T01n826u_001:89": " puṣpadhūpaśatapūjitaśīrṣaṃ",
    "T01n826u_001:90": " dīpagandhaśatapūjitaśīrṣam |",
    "T01n826u_001:91": " mālyavastraśatapūjitaśīrṣaṃ",
    "T01n826u_001:92": " taṃ namāmi daśabalavaraśīrṣam || 23 ||",
    "T01n826u_001:93": " tribhuvanadharmakalābhyuṣṇīṣa-",
    "T01n826u_001:94": " ratnachatramukuṭādipūjitam |",
    "T01n826u_001:95": " ye paṭhanti sugatastavamenaṃ",
    "T01n826u_001:96": " te labhanti śubhamokṣapathādhigamam || 24 ||",
    "T01n826u_001:97": " śrīmadāryāvalokiteśvarabhaṭṭārakasya rūpastavaṃ samāptam |"
}