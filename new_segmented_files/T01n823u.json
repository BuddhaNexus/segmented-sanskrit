{
    "T01n823u_001:0": " potalakāṣṭakam",
    "T01n823u_001:1": " jaya siddhasurāsuralokaguruṃ",
    "T01n823u_001:2": " kamalāsanasaṃsthitapādayugam |",
    "T01n823u_001:3": " daśapāramitādikapadmadharaṃ",
    "T01n823u_001:4": " praṇamāmi sadā varakāruṇikam || 1 ||",
    "T01n823u_001:5": " bahukoṭinarādhipalabdhavaraṃ",
    "T01n823u_001:6": " jaya susthitasusthitanāthamunim |",
    "T01n823u_001:7": " bhavasāgarapāragatādhikaraṃ",
    "T01n823u_001:8": " śaraṇāgatasattvahitārthakaram || 2 ||",
    "T01n823u_001:9": " kamalāsanacārusurupadharaṃ",
    "T01n823u_001:10": " amitābhatathāgatamaulidharam |",
    "T01n823u_001:11": " kamalojjvalabhūṣitagātravaraṃ",
    "T01n823u_001:12": " navabhāskaraśobhanadīptikaram || 3 ||",
    "T01n823u_001:13": " mṛdutāmranakhāṅguliśobhamukhaṃ",
    "T01n823u_001:14": " śaśibimbasamaprabhasaumyamukham |",
    "T01n823u_001:15": " ṣaḍabhijñaguṇākaravandyatanuṃ",
    "T01n823u_001:16": " varasusthitanūpurahārayutam || 4 ||",
    "T01n823u_001:17": " maṇikuṇḍalamaṇḍitagaṇḍayugaṃ",
    "T01n823u_001:18": " ghanagarjitatulyagabhīrarutam |",
    "T01n823u_001:19": " girigahvaradyotanivāsakṛtaṃ",
    "T01n823u_001:20": " vṛṣavāhanahaṃsamṛgāyuśatam || 5 ||",
    "T01n823u_001:21": " ṛṣidevagaṇādhipasaṃstutikaṃ",
    "T01n823u_001:22": " jinarūpadharātmakabodhiparam |",
    "T01n823u_001:23": " dvipadottamanāthasurādhipatiṃ",
    "T01n823u_001:24": " bahusattvavimocanamuktikaram || 6 ||",
    "T01n823u_001:25": " dhvajachatrasuśobhitavāmakaraṃ",
    "T01n823u_001:26": " malayāgiricandanadhūparatim |",
    "T01n823u_001:27": " kamalotpalapāṭalidāmadharaṃ",
    "T01n823u_001:28": " praṇamāmyatha pāṭalivṛddhikaram || 7 ||",
    "T01n823u_001:29": " jvararogavivarjitakuṣṭhaharaṃ",
    "T01n823u_001:30": " bahubodhimanāghanabhadrakaram |",
    "T01n823u_001:31": " navadurgatināśanamārgapradaṃ",
    "T01n823u_001:32": " karuṇā-tathatādvayaviśvapatim || 8 ||",
    "T01n823u_001:33": " śrīāryāvalokiteśvarasya potalakāṣṭakaṃ samāptam |"
}