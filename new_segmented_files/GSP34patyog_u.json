{
    "GSP34patyog_u:0": " atha yogānuśāsanam // YS_1.1 //",
    "GSP34patyog_u:1": " yogaś cittavṛttinirodhaḥ // YS_1.2 //",
    "GSP34patyog_u:2": " tadā draṣṭuḥ svarūpe 'vasthānam // YS_1.3 //",
    "GSP34patyog_u:3": " vṛttisārūpyam itaratra // YS_1.4 //",
    "GSP34patyog_u:4": " vṛttayaḥ pañcatayyaḥ kliṣṭākliṣṭāḥ // YS_1.5 //",
    "GSP34patyog_u:5": " pramāṇaviparyayavikalpanidrāsmṛtayaḥ // YS_1.6 //",
    "GSP34patyog_u:6": " pratyakṣānumānāgamāḥ pramāṇāni // YS_1.7 //",
    "GSP34patyog_u:7": " viparyayo mithyājñānam atadrūpapratiṣṭham // YS_1.8 //",
    "GSP34patyog_u:8": " śabdajñānānupātī vastuśūnyo vikalpaḥ // YS_1.9 //",
    "GSP34patyog_u:9": " abhāvapratyayālambanā vṛttir nidrā // YS_1.10 //",
    "GSP34patyog_u:10": " anubhūtaviṣayāsaṃpramoṣaḥ smṛtiḥ // YS_1.11 //",
    "GSP34patyog_u:11": " abhyāsavairāgyābhyāṃ tannirodhaḥ // YS_1.12 //",
    "GSP34patyog_u:12": " tatra sthitau yatno 'bhyāsaḥ // YS_1.13 //",
    "GSP34patyog_u:13": " sa tu dīrghakālanairantaryasatkārāsevito dṛḍhabhūmiḥ // YS_1.14 //",
    "GSP34patyog_u:14": " dṛṣṭānuśravikaviṣayavitṛṣṇasya vaśīkārasaṃjñā vairāgyam // YS_1.15 //",
    "GSP34patyog_u:15": " tat paraṃ puruṣakhyāter guṇavaitṛṣṇyam // YS_1.16 //",
    "GSP34patyog_u:16": " vitarkavicārānandāsmitārūpānugamāt saṃprajñātaḥ // YS_1.17 //",
    "GSP34patyog_u:17": " virāmapratyayābhyāsapūrvaḥ saṃskāraśeṣo 'nyaḥ // YS_1.18 //",
    "GSP34patyog_u:18": " bhavapratyayo videhaprakṛtilayānām // YS_1.19 //",
    "GSP34patyog_u:19": " śraddhāvīryasmṛtisamādhiprajñāpūrvaka itareṣām // YS_1.20 //",
    "GSP34patyog_u:20": " tīvrasaṃvegānām āsannaḥ // YS_1.21 //",
    "GSP34patyog_u:21": " mṛdumadhyādhimātratvāt tato 'pi viśeṣaḥ // YS_1.22 //",
    "GSP34patyog_u:22": " īśvarapraṇidhānād vā // YS_1.23 //",
    "GSP34patyog_u:23": " kleśakarmavipākāśayair aparāmṛṣṭaḥ puruṣaviśeṣa īśvaraḥ // YS_1.24 //",
    "GSP34patyog_u:24": " tatra niratiśayaṃ sarvajñabījam // YS_1.25 //",
    "GSP34patyog_u:25": " pūrveṣām api guruḥ kālenānavacchedāt // YS_1.26 //",
    "GSP34patyog_u:26": " tasya vācakaḥ praṇavaḥ // YS_1.27 //",
    "GSP34patyog_u:27": " tajjapas tadarthabhāvanam // YS_1.28 //",
    "GSP34patyog_u:28": " tataḥ pratyakcetanādhigamo 'py antarāyābhāvaś ca // YS_1.29 //",
    "GSP34patyog_u:29": " vyādhistyānasaṃśayapramādālasyāviratibhrāntidarśanālabdhabhūmikatvānavasthitatvāni cittavikṣepās te 'ntarāyāḥ // YS_1.30 //",
    "GSP34patyog_u:30": " duḥkhadaurmanasyāṅgamejayatvaśvāsapraśvāsā vikṣepasahabhuvaḥ // YS_1.31 //",
    "GSP34patyog_u:31": " tatpratiṣedhārtham ekatattvābhyāsaḥ // YS_1.32 //",
    "GSP34patyog_u:32": " maitrīkaruṇāmuditopekṣāṇāṃ sukhaduḥkhapuṇyāpuṇyaviṣayāṇāṃ bhāvanātaś cittaprasādanam // YS_1.33 //",
    "GSP34patyog_u:33": " pracchardanavidhāraṇābhyāṃ vā prāṇasya // YS_1.34 //",
    "GSP34patyog_u:34": " viṣayavatī vā pravṛttir utpannā manasaḥ sthitinibandhanī // YS_1.35 //",
    "GSP34patyog_u:35": " viśokā vā jyotiṣmatī // YS_1.36 //",
    "GSP34patyog_u:36": " vītarāgaviṣayaṃ vā cittam // YS_1.37 //",
    "GSP34patyog_u:37": " svapnanidrājñānālambanaṃ vā // YS_1.38 //",
    "GSP34patyog_u:38": " yathābhimatadhyānād vā // YS_1.39 //",
    "GSP34patyog_u:39": " paramāṇuparamamahattvānto 'sya vaśīkāraḥ // YS_1.40 //",
    "GSP34patyog_u:40": " kṣīṇavṛtter abhijātasyeva maṇer grahītṛgrahaṇagrāhyeṣu tatsthatadañjanatā samāpattiḥ // YS_1.41 //",
    "GSP34patyog_u:41": " tatra śabdārthajñānavikalpaiḥ saṃkīrṇā savitarkā samāpattiḥ // YS_1.42 //",
    "GSP34patyog_u:42": " smṛtipariśuddhau svarūpaśūnyevārthamātranirbhāsā nirvitarkā // YS_1.43 //",
    "GSP34patyog_u:43": " etayaiva savicārā nirvicārā ca sūkṣmaviṣayā vyākhyātā // YS_1.44 //",
    "GSP34patyog_u:44": " sūkṣmaviṣayatvaṃ cāliṅgaparyavasānam // YS_1.45 //",
    "GSP34patyog_u:45": " tā eva sabījaḥ samādhiḥ // YS_1.46 //",
    "GSP34patyog_u:46": " nirvicāravaiśāradye 'dhyātmaprasādaḥ // YS_1.47 //",
    "GSP34patyog_u:47": " ṛtaṃbharā tatra prajñā // YS_1.48 //",
    "GSP34patyog_u:48": " śrutānumānaprajñābhyām anyaviṣayā viśeṣārthatvāt // YS_1.49 //",
    "GSP34patyog_u:49": " tajjaḥ saṃskāro 'nyasaṃskārapratibandhī // YS_1.50 //",
    "GSP34patyog_u:50": " tasyāpi nirodhe sarvanirodhān nirbījaḥ samādhiḥ // YS_1.51 //",
    "GSP34patyog_u:51": " tapaḥsvādhyāyeśvarapraṇidhānāni kriyāyogaḥ // YS_2.1 //",
    "GSP34patyog_u:52": " samādhibhāvanārthaḥ kleśatanūkaraṇārthaś ca // YS_2.2 //",
    "GSP34patyog_u:53": " avidyāsmitārāgadveṣābhiniveśāḥ kleśāḥ // YS_2.3 //",
    "GSP34patyog_u:54": " avidyā kṣetram uttareṣāṃ prasuptatanuvicchinnodārāṇām // YS_2.4 //",
    "GSP34patyog_u:55": " anityāśuciduḥkhānātmasu nityaśucisukhātmakhyātir avidyā // YS_2.5 //",
    "GSP34patyog_u:56": " dṛgdarśanaśaktyor ekātmatevāsmitā // YS_2.6 //",
    "GSP34patyog_u:57": " sukhānuśayī rāgaḥ // YS_2.7 //",
    "GSP34patyog_u:58": " duḥkhānuśayī dveṣaḥ // YS_2.8 //",
    "GSP34patyog_u:59": " svarasavāhī viduṣo 'pi tathā rūḍho 'bhiniveśaḥ // YS_2.9 //",
    "GSP34patyog_u:60": " te pratiprasavaheyāḥ sūkṣmāḥ // YS_2.10 //",
    "GSP34patyog_u:61": " dhyānaheyās tadvṛttayaḥ // YS_2.11 //",
    "GSP34patyog_u:62": " kleśamūlaḥ karmāśayo dṛṣṭādṛṣṭajanmavedanīyaḥ // YS_2.12 //",
    "GSP34patyog_u:63": " sati mūle tadvipāko jātyāyurbhogāḥ // YS_2.13 //",
    "GSP34patyog_u:64": " te hlādaparitāpaphalāḥ puṇyāpuṇyahetutvāt // YS_2.14 //",
    "GSP34patyog_u:65": " pariṇāmatāpasaṃskāraduḥkhair guṇavṛttivirodhāc ca duḥkham eva sarvaṃ vivekinaḥ // YS_2.15 //",
    "GSP34patyog_u:66": " heyaṃ duḥkham anāgatam // YS_2.16 //",
    "GSP34patyog_u:67": " draṣṭṛdṛśyayoḥ saṃyogo heyahetuḥ // YS_2.17 //",
    "GSP34patyog_u:68": " prakāśakriyāsthitiśīlaṃ bhūtendriyātmakaṃ bhogāpavargārthaṃ dṛśyam // YS_2.18 //",
    "GSP34patyog_u:69": " viśeṣāviśeṣaliṅgamātrāliṅgāni guṇaparvāṇi // YS_2.19 //",
    "GSP34patyog_u:70": " draṣṭā dṛśimātraḥ śuddho 'pi pratyayānupaśyaḥ // YS_2.20 //",
    "GSP34patyog_u:71": " tadartha eva dṛśyasyātmā // YS_2.21 //",
    "GSP34patyog_u:72": " kṛtārthaṃ prati naṣṭam apy anaṣṭaṃ tadanyasādhāraṇatvāt // YS_2.22 //",
    "GSP34patyog_u:73": " svasvāmiśaktyoḥ svarūpopalabdhihetuḥ saṃyogaḥ // YS_2.23 //",
    "GSP34patyog_u:74": " tasya hetur avidyā // YS_2.24 //",
    "GSP34patyog_u:75": " tadabhāvāt saṃyogābhāvo hānaṃ taddṛśeḥ kaivalyam // YS_2.25 //",
    "GSP34patyog_u:76": " vivekakhyātir aviplavā hānopāyaḥ // YS_2.26 //",
    "GSP34patyog_u:77": " tasya saptadhā prāntabhūmiḥ prajñā // YS_2.27 //",
    "GSP34patyog_u:78": " yogāṅgānuṣṭhānād aśuddhikṣaye jñānadīptir ā vivekakhyāteḥ // YS_2.28 //",
    "GSP34patyog_u:79": " yamaniyamāsanaprāṇāyāmapratyāhāradhāraṇādhyānasamādhayo 'ṣṭāv aṅgāni // YS_2.29 //",
    "GSP34patyog_u:80": " ahiṃsāsatyāsteyabrahmacaryāparigrahā yamāḥ // YS_2.30 //",
    "GSP34patyog_u:81": " jātideśakālasamayānavacchinnāḥ sārvabhaumā mahāvratam // YS_2.31 //",
    "GSP34patyog_u:82": " śaucasaṃtoṣatapaḥsvādhyāyeśvarapraṇidhānāni niyamāḥ // YS_2.32 //",
    "GSP34patyog_u:83": " vitarkabādhane pratipakṣabhāvanam // YS_2.33 //",
    "GSP34patyog_u:84": " vitarkā hiṃsādayaḥ kṛtakāritānumoditā lobhakrodhamohapūrvakā mṛdumadhyādhimātrā duḥkhājñānānantaphalā iti pratipakṣabhāvanam // YS_2.34 //",
    "GSP34patyog_u:85": " ahiṃsāpratiṣṭhāyāṃ tatsaṃnidhau vairatyāgaḥ // YS_2.35 //",
    "GSP34patyog_u:86": " satyapratiṣṭhāyāṃ kriyāphalāśrayatvam // YS_2.36 //",
    "GSP34patyog_u:87": " asteyapratiṣṭhāyāṃ sarvaratnopasthānam // YS_2.37 //",
    "GSP34patyog_u:88": " brahmacaryapratiṣṭhāyāṃ vīryalābhaḥ // YS_2.38 //",
    "GSP34patyog_u:89": " aparigrahasthairye janmakathaṃtāsaṃbodhaḥ // YS_2.39 //",
    "GSP34patyog_u:90": " śaucāt svāṅgajugupsā parair asaṃsargaḥ // YS_2.40 //",
    "GSP34patyog_u:91": " sattvaśuddhisaumanasyaikāgryendriyajayātmadarśanayogyatvāni ca // YS_2.41 //",
    "GSP34patyog_u:92": " saṃtoṣād anuttamaḥ sukhalābhaḥ // YS_2.42 //",
    "GSP34patyog_u:93": " kāyendriyasiddhir aśuddhikṣayāt tapasaḥ // YS_2.43 //",
    "GSP34patyog_u:94": " svādhyāyād iṣṭadevatāsaṃprayogaḥ // YS_2.44 //",
    "GSP34patyog_u:95": " samādhisiddhir īśvarapraṇidhānāt // YS_2.45 //",
    "GSP34patyog_u:96": " sthirasukham āsanam // YS_2.46 //",
    "GSP34patyog_u:97": " prayatnaśaithilyānantasamāpattibhyām // YS_2.47 //",
    "GSP34patyog_u:98": " tato dvandvānabhighātaḥ // YS_2.48 //",
    "GSP34patyog_u:99": " tasmin sati śvāsapraśvāsayor gativicchedaḥ prāṇāyāmaḥ // YS_2.49 //",
    "GSP34patyog_u:100": " bāhyābhyantarastambhavṛttir deśakālasaṃkhyābhiḥ paridṛṣṭo dīrghasūkṣmaḥ // YS_2.50 //",
    "GSP34patyog_u:101": " bāhyābhyantaraviṣayākṣepī caturthaḥ // YS_2.51 //",
    "GSP34patyog_u:102": " tataḥ kṣīyate prakāśāvaraṇam // YS_2.52 //",
    "GSP34patyog_u:103": " dhāraṇāsu ca yogyatā manasaḥ // YS_2.53 //",
    "GSP34patyog_u:104": " svaviṣayāsaṃprayoge cittasvarūpānukāra ivendriyāṇāṃ pratyāhāraḥ // YS_2.54 //",
    "GSP34patyog_u:105": " tataḥ paramā vaśyatendriyāṇām // YS_2.55 //",
    "GSP34patyog_u:106": " deśabandhaś cittasya dhāraṇā // YS_3.1 //",
    "GSP34patyog_u:107": " tatra pratyayaikatānatā dhyānam // YS_3.2 //",
    "GSP34patyog_u:108": " tad evārthamātranirbhāsaṃ svarūpaśūnyam iva samādhiḥ // YS_3.3 //",
    "GSP34patyog_u:109": " trayam ekatra saṃyamaḥ // YS_3.4 //",
    "GSP34patyog_u:110": " tajjayāt prajñālokaḥ // YS_3.5 //",
    "GSP34patyog_u:111": " tasya bhūmiṣu viniyogaḥ // YS_3.6 //",
    "GSP34patyog_u:112": " trayam antaraṅgaṃ pūrvebhyaḥ // YS_3.7 //",
    "GSP34patyog_u:113": " tad api bahiraṅgaṃ nirbījasya // YS_3.8 //",
    "GSP34patyog_u:114": " vyutthānanirodhasaṃskārayor abhibhavaprādurbhāvau nirodhakṣaṇacittānvayo nirodhapariṇāmaḥ // YS_3.9 //",
    "GSP34patyog_u:115": " tasya praśāntavāhitā saṃskārāt // YS_3.10 //",
    "GSP34patyog_u:116": " sarvārthataikāgratayoḥ kṣayodayau cittasya samādhipariṇāmaḥ // YS_3.11 //",
    "GSP34patyog_u:117": " tataḥ punaḥ śāntoditau tulyapratyayau cittasyaikāgratāpariṇāmaḥ // YS_3.12 //",
    "GSP34patyog_u:118": " etena bhūtendriyeṣu dharmalakṣaṇāvasthāpariṇāmā vyākhyātāḥ // YS_3.13 //",
    "GSP34patyog_u:119": " śāntoditāvyapadeśyadharmānupātī dharmī // YS_3.14 //",
    "GSP34patyog_u:120": " kramānyatvaṃ pariṇāmānyatve hetuḥ // YS_3.15 //",
    "GSP34patyog_u:121": " pariṇāmatrayasaṃyamād atītānāgatajñānam // YS_3.16 //",
    "GSP34patyog_u:122": " śabdārthapratyayānām itaretarādhyāsāt saṃkaras tatpravibhāgasaṃyamāt sarvabhūtarutajñānam // YS_3.17 //",
    "GSP34patyog_u:123": " saṃskārasākṣātkaraṇāt pūrvajātijñānam // YS_3.18 //",
    "GSP34patyog_u:124": " pratyayasya paracittajñānam // YS_3.19 //",
    "GSP34patyog_u:125": " na ca tat sālambanaṃ tasyāviṣayībhūtatvāt // YS_3.20 //",
    "GSP34patyog_u:126": " kāyarūpasaṃyamāt tadgrāhyaśaktistambhe cakṣuḥprakāśāsaṃprayoge 'ntardhānam // YS_3.21 //",
    "GSP34patyog_u:127": " sopakramaṃ nirupakramaṃ ca karma tatsaṃyamād aparāntajñānam ariṣṭebhyo vā // YS_3.22 //",
    "GSP34patyog_u:128": " maitryādiṣu balāni // YS_3.23 //",
    "GSP34patyog_u:129": " baleṣu hastibalādīni // YS_3.24 //",
    "GSP34patyog_u:130": " pravṛttyālokanyāsāt sūkṣmavyavahitaviprakṛṣṭajñānam // YS_3.25 //",
    "GSP34patyog_u:131": " bhuvanajñānaṃ sūrye saṃyamāt // YS_3.26 //",
    "GSP34patyog_u:132": " candre tārāvyūhajñānam // YS_3.27 //",
    "GSP34patyog_u:133": " dhruve tadgatijñānam // YS_3.28 //",
    "GSP34patyog_u:134": " nābhicakre kāyavyūhajñānam // YS_3.29 //",
    "GSP34patyog_u:135": " kaṇṭhakūpe kṣutpipāsānivṛttiḥ // YS_3.30 //",
    "GSP34patyog_u:136": " kūrmanāḍyāṃ sthairyam // YS_3.31 //",
    "GSP34patyog_u:137": " mūrdhajyotiṣi siddhadarśanam // YS_3.32 //",
    "GSP34patyog_u:138": " prātibhād vā sarvam // YS_3.33 //",
    "GSP34patyog_u:139": " hṛdaye cittasaṃvit // YS_3.34 //",
    "GSP34patyog_u:140": " sattvapuruṣayor atyantāsaṃkīrṇayoḥ pratyayāviśeṣo bhogaḥ parārthāt svārthasaṃyamāt puruṣajñānam // YS_3.35 //",
    "GSP34patyog_u:141": " tataḥ prātibhaśrāvaṇavedanādarśāsvādavārtā jāyante // YS_3.36 //",
    "GSP34patyog_u:142": " te samādhāv upasargā vyutthāne siddhayaḥ // YS_3.37 //",
    "GSP34patyog_u:143": " bandhakāraṇaśaithilyāt pracārasaṃvedanāc ca cittasya paraśarīrāveśaḥ // YS_3.38 //",
    "GSP34patyog_u:144": " udānajayāj jalapaṅkakaṇṭakādiṣv asaṅga utkrāntiś ca // YS_3.39 //",
    "GSP34patyog_u:145": " samānajayāj jvalanam // YS_3.40 //",
    "GSP34patyog_u:146": " śrotrākāśayoḥ saṃbandhasaṃyamād divyaṃ śrotram // YS_3.41 //",
    "GSP34patyog_u:147": " kāyākāśayoḥ saṃbandhasaṃyamāl laghutūlasamāpatteś cākāśagamanam // YS_3.42 //",
    "GSP34patyog_u:148": " bahir akalpitā vṛttir mahāvidehā tataḥ prakāśāvaraṇakṣayaḥ // YS_3.43 //",
    "GSP34patyog_u:149": " sthūlasvarūpasūkṣmānvayārthavattvasaṃyamād bhūtajayaḥ // YS_3.44 //",
    "GSP34patyog_u:150": " tato 'ṇimādiprādurbhāvaḥ kāyasaṃpat taddharmānabhighātaś ca // YS_3.45 //",
    "GSP34patyog_u:151": " rūpalāvaṇyabalavajrasaṃhananatvāni kāyasaṃpat // YS_3.46 //",
    "GSP34patyog_u:152": " grahaṇasvarūpāsmitānvayārthavattvasaṃyamād indriyajayaḥ // YS_3.47 //",
    "GSP34patyog_u:153": " tato manojavitvaṃ vikaraṇabhāvaḥ pradhānajayaś ca // YS_3.48 //",
    "GSP34patyog_u:154": " sattvapuruṣānyatākhyātimātrasya sarvabhāvādhiṣṭhātṛtvaṃ sarvajñātṛtvaṃ ca // YS_3.49 //",
    "GSP34patyog_u:155": " tadvairāgyād api doṣabījakṣaye kaivalyam // YS_3.50 //",
    "GSP34patyog_u:156": " sthānyupanimantraṇe saṅgasmayākaraṇaṃ punar aniṣṭaprasaṅgāt // YS_3.51 //",
    "GSP34patyog_u:157": " kṣaṇatatkramayoḥ saṃyamād vivekajaṃ jñānam // YS_3.52 //",
    "GSP34patyog_u:158": " jātilakṣaṇadeśair anyatānavacchedāt tulyayos tataḥ pratipattiḥ // YS_3.53 //",
    "GSP34patyog_u:159": " tārakaṃ sarvaviṣayaṃ sarvathāviṣayam akramaṃ ceti vivekajaṃ jñānam // YS_3.54 //",
    "GSP34patyog_u:160": " sattvapuruṣayoḥ śuddhisāmye kaivalyam iti // YS_3.55 //",
    "GSP34patyog_u:161": " janmauṣadhimantratapaḥsamādhijāḥ siddhayaḥ // YS_4.1 //",
    "GSP34patyog_u:162": " jātyantarapariṇāmaḥ prakṛtyāpūrāt // YS_4.2 //",
    "GSP34patyog_u:163": " nimittam aprayojakaṃ prakṛtīnāṃ varaṇabhedas tu tataḥ kṣetrikavat // YS_4.3 //",
    "GSP34patyog_u:164": " nirmāṇacittāny asmitāmātrāt // YS_4.4 //",
    "GSP34patyog_u:165": " pravṛttibhede prayojakaṃ cittam ekam anekeṣām // YS_4.5 //",
    "GSP34patyog_u:166": " tatra dhyānajam anāśayam // YS_4.6 //",
    "GSP34patyog_u:167": " karmāśuklākṛṣṇaṃ yoginas trividham itareṣām // YS_4.7 //",
    "GSP34patyog_u:168": " tatas tadvipākānuguṇānām evābhivyaktir vāsanānām // YS_4.8 //",
    "GSP34patyog_u:169": " jātideśakālavyavahitānām apy ānantaryaṃ smṛtisaṃskārayor ekarūpatvāt // YS_4.9 //",
    "GSP34patyog_u:170": " tāsām anāditvaṃ cāśiṣo nityatvāt // YS_4.10 //",
    "GSP34patyog_u:171": " hetuphalāśrayālambanaiḥ saṃgṛhītatvād eṣām abhāve tadabhāvaḥ // YS_4.11 //",
    "GSP34patyog_u:172": " atītānāgataṃ svarūpato 'sty adhvabhedād dharmāṇām // YS_4.12 //",
    "GSP34patyog_u:173": " te vyaktasūkṣmā guṇātmānaḥ // YS_4.13 //",
    "GSP34patyog_u:174": " pariṇāmaikatvād vastutattvam // YS_4.14 //",
    "GSP34patyog_u:175": " vastusāmye cittabhedāt tayor vibhaktaḥ panthāḥ // YS_4.15 //",
    "GSP34patyog_u:176": " na caikacittatantraṃ vastu tadapramāṇakaṃ tadā kiṃ syāt // YS_4.16 //",
    "GSP34patyog_u:177": " taduparāgāpekṣitvāc cittasya vastu jñātājñātam // YS_4.17 //",
    "GSP34patyog_u:178": " sadā jñātāś cittavṛttayas tatprabhoḥ puruṣasyāpariṇāmitvāt // YS_4.18 //",
    "GSP34patyog_u:179": " na tat svābhāsaṃ dṛśyatvāt // YS_4.19 //",
    "GSP34patyog_u:180": " ekasamaye cobhayānavadhāraṇam // YS_4.20 //",
    "GSP34patyog_u:181": " cittāntaradṛśye buddhibuddher atiprasaṅgaḥ smṛtisaṃkaraś ca // YS_4.21 //",
    "GSP34patyog_u:182": " citer apratisaṃkramāyās tadākārāpattau svabuddhisaṃvedanam // YS_4.22 //",
    "GSP34patyog_u:183": " draṣṭṛdṛśyoparaktaṃ cittaṃ sarvārtham // YS_4.23 //",
    "GSP34patyog_u:184": " tad asaṃkhyeyavāsanābhiś citram api parārthaṃ saṃhatyakāritvāt // YS_4.24 //",
    "GSP34patyog_u:185": " viśeṣadarśina ātmabhāvabhāvanānivṛttiḥ // YS_4.25 //",
    "GSP34patyog_u:186": " tadā vivekanimnaṃ kaivalyaprāgbhāraṃ cittam // YS_4.26 //",
    "GSP34patyog_u:187": " tacchidreṣu pratyayāntarāṇi saṃskārebhyaḥ // YS_4.27 //",
    "GSP34patyog_u:188": " hānam eṣāṃ kleśavad uktam // YS_4.28 //",
    "GSP34patyog_u:189": " prasaṃkhyāne 'py akusīdasya sarvathā vivekakhyāter dharmameghaḥ samādhiḥ // YS_4.29 //",
    "GSP34patyog_u:190": " tataḥ kleśakarmanivṛttiḥ // YS_4.30 //",
    "GSP34patyog_u:191": " tadā sarvāvaraṇamalāpetasya jñānasyānantyāj jñeyam alpam // YS_4.31 //",
    "GSP34patyog_u:192": " tataḥ kṛtārthānāṃ pariṇāmakramasamāptir guṇānām // YS_4.32 //",
    "GSP34patyog_u:193": " kṣaṇapratiyogī pariṇāmāparāntanirgrāhyaḥ kramaḥ // YS_4.33 //",
    "GSP34patyog_u:194": " puruṣārthaśūnyānāṃ guṇānāṃ pratiprasavaḥ kaivalyaṃ svarūpapratiṣṭhā vā citiśaktir iti // YS_4.34 //"
}