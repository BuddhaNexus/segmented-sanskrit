{
    "GSP29antarkpu:0": " AnTs_1ab nidhāya hṛdi viśveśaṃ vidhāya guruvandanam /",
    "GSP29antarkpu:1": " AnTs_1cd bālānāṃ sukhabodhāya kriyate tarkasaṃgrahaḥ //",
    "GSP29antarkpu:2": " AnTs_2 dravyaguṇakarmasāmānyaviśeṣasamavāyābhāvāḥ sapta padārthāḥ //",
    "GSP29antarkpu:3": " AnTs_3 tatra dravyāṇi pṛthivyaptejovāyvākāśakāladigātmamanāṃsi navaiva //",
    "GSP29antarkpu:4": " AnTs_4 rūparasagandhasparśasaṃkhyāparimāṇapṛthaktvasaṃyogavibhāgaparatvāparatvagurutvadravatvasnehaśabdabuddhisukhaduḥkhecchādveṣaprayatnadharmādharmasaṃskārāś",
    "GSP29antarkpu:5": " caturviṃśati guṇāḥ //",
    "GSP29antarkpu:6": " AnTs_5 utkṣepaṇāvakṣepaṇākuñcanaprasāraṇagamanāni pañca karmāṇi //",
    "GSP29antarkpu:7": " AnTs_6 param aparaṃ ceti dvividhaṃ sāmānyam //",
    "GSP29antarkpu:8": " AnTs_7 nityadravyavṛttayo viśeṣās tv anantā eva //",
    "GSP29antarkpu:9": " AnTs_8 samavāyas tv eka eva //",
    "GSP29antarkpu:10": " AnTs_9 abhāvaś caturvidhaḥ /",
    "GSP29antarkpu:11": " prāgabhāvaḥ pradhvaṃsābhāvo 'tyantābhāvo 'nyonyābhāvaś ceti //",
    "GSP29antarkpu:12": " AnTs_10 gandhavatī pṛthivī /",
    "GSP29antarkpu:13": " sā dvividhā nityānityā ca /",
    "GSP29antarkpu:14": " nityā paramāṇurūpā /",
    "GSP29antarkpu:15": " anityā kāryarūpā /",
    "GSP29antarkpu:16": " punas trividhā śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:17": " śarīram asmadādīnām /",
    "GSP29antarkpu:18": " indriyaṃ gandhagrāhakaṃ ghrāṇaṃ nāsāgravarti /",
    "GSP29antarkpu:19": " viṣayo mṛtpāṣāṇādiḥ //",
    "GSP29antarkpu:20": " AnTs_11 śitasparśavatya āpaḥ /",
    "GSP29antarkpu:21": " tā dvividhāḥ nityā anityāś ca /",
    "GSP29antarkpu:22": " ntyāḥ paramāṇurūpāḥ /",
    "GSP29antarkpu:23": " anityāḥ kāryarūpāḥ. /",
    "GSP29antarkpu:24": " punas trividhāḥ. śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:25": " śarīraṃ varuṇaloke /",
    "GSP29antarkpu:26": " indriyaṃ rasagrāhakaṃ rasanaṃ jihvāgravarti /",
    "GSP29antarkpu:27": " viṣayaḥ saritsamudrādiḥ //",
    "GSP29antarkpu:28": " AnTs_12 uṣṇasparśavat tejaḥ /",
    "GSP29antarkpu:29": " tad dvividhaṃ nityam anityaṃ ca /",
    "GSP29antarkpu:30": " niyyaṃ paramāṇurūpam /",
    "GSP29antarkpu:31": " anityaṃ kāryarūpam /",
    "GSP29antarkpu:32": " punas trividhaṃ śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:33": " śarīram ādityaloke /",
    "GSP29antarkpu:34": " indriyaṃ rūpagrāhakaṃ cakṣuḥ kṛṣṇatārāgravarti /",
    "GSP29antarkpu:35": " viṣayaś caturvidhaḥ /",
    "GSP29antarkpu:36": " bhaumadivyaudaryākarajabhedāt /",
    "GSP29antarkpu:37": " bhaumaṃ vahnyādikam /",
    "GSP29antarkpu:38": " abindhanaṃ divyaṃ vidyudādi /",
    "GSP29antarkpu:39": " bhuktasya pariṇāmahetur audaryam. ākarajaṃ suvarṇādi //",
    "GSP29antarkpu:40": " AnTs_13 rūparahitasparśavān vāyuḥ /",
    "GSP29antarkpu:41": " sa dvividho nityo 'nityaś ca /",
    "GSP29antarkpu:42": " nityaḥ paramāṇurūpaḥ /",
    "GSP29antarkpu:43": " anityaḥ kāryarūpaḥ /",
    "GSP29antarkpu:44": " punas trividhaḥ śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:45": " śarīraṃ vāyuloke /",
    "GSP29antarkpu:46": " indriyaṃ sparśagrāhakaṃ tvaksarvaśarīravarti /",
    "GSP29antarkpu:47": " viṣayo vṛkṣādikampanahetuḥ //",
    "GSP29antarkpu:48": " śarīrāntaḥsaṃcārī vāyuḥ prāṇaḥ /",
    "GSP29antarkpu:49": " sa caiko 'py upādhibhedāt prāṇāpānādisaṃjñā labhate //",
    "GSP29antarkpu:50": " AnTs_14 śabdaguṇam ākāśam /",
    "GSP29antarkpu:51": " tac caikaṃ vibhu nityaṃ ca //",
    "GSP29antarkpu:52": " AnTs_15 atītādivyavahārahetuḥ kālaḥ /",
    "GSP29antarkpu:53": " sa caiko vibhur nityaś ca //",
    "GSP29antarkpu:54": " AnTs_16 prācyādivyavahārahetur dik /",
    "GSP29antarkpu:55": " sā caikā vibhvī nityā ca //",
    "GSP29antarkpu:56": " AnTs_17 jñānādhikaraṇam ātmā /",
    "GSP29antarkpu:57": " sa dvividhaḥ paramātmā jīvātmā ca /",
    "GSP29antarkpu:58": " tatreśvaraḥ sarvajñaḥ paramātmaika eva /",
    "GSP29antarkpu:59": " jīvātmā pratiśarīraṃ bhinno vibhur nityaś ca //",
    "GSP29antarkpu:60": " AnTs_18 sukhādyupalabdhisādhanam indriyaṃ manaḥ /",
    "GSP29antarkpu:61": " tac ca pratyātmaniyatatvād anantaṃ paramāṇurūpaṃ nityaṃ ca //",
    "GSP29antarkpu:62": " AnTs_19 cakṣurmātragrāhyo guṇo rūpam /",
    "GSP29antarkpu:63": " tac ca śuklanīlapītaraktaharitakapiśacitrabhedāt saptavidhaṃ pṛthivījalatejovṛtti /",
    "GSP29antarkpu:64": " tatra pṛthivyāṃ saptavidham /",
    "GSP29antarkpu:65": " abhāsvaraśuklaṃ jale /",
    "GSP29antarkpu:66": " bhāsvaraśuklaṃ tejasi //",
    "GSP29antarkpu:67": " AnTs_20 rasanagrāhyo guṇo rasaḥ /",
    "GSP29antarkpu:68": " sa ca madhurāmlalavaṇakaṭukaṣāyatiktabhedāt ṣaḍvidhaḥ /",
    "GSP29antarkpu:69": " pṛthivyāṃ ṣaḍvidhaḥ /",
    "GSP29antarkpu:70": " jale madhura eva //",
    "GSP29antarkpu:71": " AnTs_21 ghrāṇagrāhyo guṇo gandhaḥ /",
    "GSP29antarkpu:72": " sa ca dvividhaḥ surabhirasurabhiś ca /",
    "GSP29antarkpu:73": " pṛthivīmātravṛttiḥ /",
    "GSP29antarkpu:74": " AnTs_22 tvagindriyamātragrāhyo guṇo saparśaḥ /",
    "GSP29antarkpu:75": " sa ca trividhaḥ śitoṣṇānuṣṇāśītabhedāt /",
    "GSP29antarkpu:76": " pṛthivyaptejovāyuvṛttiḥ /",
    "GSP29antarkpu:77": " tatra śīto jale /",
    "GSP29antarkpu:78": " uṣṇas tejasi /",
    "GSP29antarkpu:79": " anuṣṇāśītaḥ pṛthivīvāyvoḥ //",
    "GSP29antarkpu:80": " AnTs_23 rūpādicatuṣṭayaṃ pṛthivyāṃ pākajam anityaṃ ca /",
    "GSP29antarkpu:81": " anyatrāpākajaṃ nityam antyaṃ ca /",
    "GSP29antarkpu:82": " nityagataṃ nityam /",
    "GSP29antarkpu:83": " anityagatam anityam //",
    "GSP29antarkpu:84": " AnTs_24 ekatvādivyavahārahetuḥ saṃkhyā /",
    "GSP29antarkpu:85": " navadravyavṛttir ekatvādiparārdhaparyantā /",
    "GSP29antarkpu:86": " ekatvaṃ nityam anityaṃ ca /",
    "GSP29antarkpu:87": " nityagataṃ nityam anityagatam anityam /",
    "GSP29antarkpu:88": " dvitvādikaṃ tu sarvatrānityam eva //",
    "GSP29antarkpu:89": " AnTs_25 mānavyavahārakāraṇaṃ parimāṇaṃ /",
    "GSP29antarkpu:90": " navadravyavṛtti /",
    "GSP29antarkpu:91": " tac caturvidham /",
    "GSP29antarkpu:92": " aṇu mahad dīrghaṃ hrasvaṃ ceti //",
    "GSP29antarkpu:93": " AnTs_26 pṛthagvyavahārakāraṇaṃ pṛthagtvaṃ /",
    "GSP29antarkpu:94": " sarvadravyavṛtti //",
    "GSP29antarkpu:95": " AnTs_27 saṃyuktavyavahārahetuḥ saṃyogaḥ /",
    "GSP29antarkpu:96": " sarvadravyavṛttiḥ //",
    "GSP29antarkpu:97": " AnTs_28 saṃyoganāśako guṇo vibhāgaḥ /",
    "GSP29antarkpu:98": " sarvadravyavṛttiḥ //",
    "GSP29antarkpu:99": " AnTs_29 parāparavyavahārāsādhāraṇakāraṇe paratvāparatve /",
    "GSP29antarkpu:100": " pṛthivyādicatuṣṭayamanovṛttīti /",
    "GSP29antarkpu:101": " te dvividhe dikkṛte kālakṛte ca /",
    "GSP29antarkpu:102": " dūrasthe dikkṛtaṃ paratvam /",
    "GSP29antarkpu:103": " samīpasthe dikkṛtam aparatvam /",
    "GSP29antarkpu:104": " jyeṣṭhe kālakṛthaṃ paratvam /",
    "GSP29antarkpu:105": " kaniṣṭhe kālakṛtam aparatvam //",
    "GSP29antarkpu:106": " AnTs_30 ādyapatanāsamavāyikāraṇaṃ gurutvaṃ /",
    "GSP29antarkpu:107": " pṛthivījalavṛtti //",
    "GSP29antarkpu:108": " AnTs_31 ādyasyandanāsamavāyikāraṇaṃ dravatvaṃ pṛthivyaptejovṛtti /",
    "GSP29antarkpu:109": " tad dvividhaṃ sāṃsiddhikaṃ naimittikaṃ ca /",
    "GSP29antarkpu:110": " sāṃsiddhikaṃ jale naimittikaṃ pṛthivītejasoḥ /",
    "GSP29antarkpu:111": " pṛthivyāṃ ghṛtādāvagnisaṃyogajanyaṃ daravatvam /",
    "GSP29antarkpu:112": " tejasi suvarṇādau //",
    "GSP29antarkpu:113": " AnTs_32 cūrṇādipiṇḍībhāvahetur guṇaḥ snehaḥ /",
    "GSP29antarkpu:114": " jalamātravṛttiḥ //",
    "GSP29antarkpu:115": " AnTs_33 śrotragrāhyo guṇaḥ śabdaḥ /",
    "GSP29antarkpu:116": " ākāśamātravṛttiḥ /",
    "GSP29antarkpu:117": " sa dvividho dvanyātmako varṇātmakaś ceti /",
    "GSP29antarkpu:118": " dvanyātmako bheryādau /",
    "GSP29antarkpu:119": " varṇātmakaḥ saṃskṛtabhāṣādirūpaḥ //",
    "GSP29antarkpu:120": " AnTs_34 sarvavyavahārahetur buddhir jñānam /",
    "GSP29antarkpu:121": " sā dvividhā smṛtiranubhavaś ca /",
    "GSP29antarkpu:122": " saṃskāramātrajanyaṃ jñānaṃ smṛtiḥ /",
    "GSP29antarkpu:123": " tadbhinnaṃ jñānam anubhavaḥ //",
    "GSP29antarkpu:124": " AnTs_35 sa dvividho yathārtho 'yathārthaś ca /",
    "GSP29antarkpu:125": " tadvati tatprakārako 'nubhavo yathārthaḥ /",
    "GSP29antarkpu:126": " yathā rajata idaṃ rajatam iti jñānam /",
    "GSP29antarkpu:127": " sa eva pramety ucyate /",
    "GSP29antarkpu:128": " tadabhāvavati tatprakārako 'nubhavo 'yathārthaḥ /",
    "GSP29antarkpu:129": " yathā śuktāv idaṃ rajatam iti jñānam //",
    "GSP29antarkpu:130": " AnTs_36 yathārthānubhavaś caturvidhaḥ pratyakṣānumityupamitiśabdabhedāt /",
    "GSP29antarkpu:131": " tatkaraṇam api caturvidhaṃ pratyakṣānumānopamānaśabdabhedāt //",
    "GSP29antarkpu:132": " AnTs_37 asādhāraṇam kāraṇaṃ karaṇam /",
    "GSP29antarkpu:133": " AnTs_38 kāryaniyatapūrvavṛtti kāraṇam /",
    "GSP29antarkpu:134": " AnTs_39 kāryaṃ prāgabhāvapratiyogi /",
    "GSP29antarkpu:135": " AnTs_40 kāraṇaṃ trividhaṃ samavāyyasamavāyinimittabhedāt /",
    "GSP29antarkpu:136": " yat samavetaṃ kāryam utpadyate tat samavāyikāraṇam /",
    "GSP29antarkpu:137": " yathā tantavaḥ paṭasya paṭaś ca khagatarūpādeḥ /",
    "GSP29antarkpu:138": " kāryeṇa kāraṇena vā sahaikasminn arthe samavetatve sati yat kāraṇaṃ tad asamavāyikāraṇam /",
    "GSP29antarkpu:139": " yathā tantusaṃyogaḥ paṭasya tanturūpaṃ paṭarūpasya /",
    "GSP29antarkpu:140": " tadubhayabhinnaṃ kāraṇaṃ nimittakāraṇam /",
    "GSP29antarkpu:141": " yathā turīvemādikaṃ paṭasya /",
    "GSP29antarkpu:142": " AnTs_41 tad etattrividhakāraṇamadhye yad asādhāraṇaṃ kāraṇaṃ tad eva karaṇam //",
    "GSP29antarkpu:143": " AnTs_42 tatra pratyakṣajñānakaraṇaṃ pratyakṣam /",
    "GSP29antarkpu:144": " indriyārthasannikarṣajanyaṃ jñānaṃ pratyakṣam /",
    "GSP29antarkpu:145": " tad dvividhaṃ nirvikalpakaṃ savikalpakaṃ ceti /",
    "GSP29antarkpu:146": " tatra nirvikalpakaṃ jñānaṃ nirvikalpakaṃ yathedaṃ kiṃcit /",
    "GSP29antarkpu:147": " saparakārakaṃ jñānaṃ savikalpakaṃ yathā ḍittho'yam brahmaṇo'yaṃ śyāmo'yam iti //",
    "GSP29antarkpu:148": " AnTs_43 pratyakṣajñānahetur indriyārthasannikarṣaḥ ṣaḍvidhaḥ /",
    "GSP29antarkpu:149": " saṃyogaḥ saṃyuktasamavāyaḥ saṃyuktasamavetasamavāyaḥ samavāyaḥ samavetasamavāyo viśeṣaṇaviśeṣyabhāvaś ceti /",
    "GSP29antarkpu:150": " cakṣuṣā ghaṭapratyakṣajanane saṃyogaḥ sannikarṣaḥ /",
    "GSP29antarkpu:151": " ghaṭarūpapratyakṣajanane saṃyuktasamavāyaḥ sannikarṣaḥ cakṣuḥsaṃyukte ghaṭe rūpasya samavāyāt /",
    "GSP29antarkpu:152": " rūpatvasāmānyapratyakṣe saṃyuktasamavetasamavāyaḥ sannikarṣaḥ cakṣuḥsaṃyukte ghaṭe rūpaṃ samavetaṃ tatra rūpatvasya samavāyāt /",
    "GSP29antarkpu:153": " śrotreṇa śabdasyākāśaguṇatvāt guṇaguṇinoś ca samavāyāt /",
    "GSP29antarkpu:154": " śabdatvasākṣātkāre samavetasamavāyaḥ sannikarṣaḥ śrotrasamavete śabde śabdatvasya samavāyāt /",
    "GSP29antarkpu:155": " abhāvaparatyakṣe viśeṣaṇaviśeṣyabhāvaḥ sannikarṣo ghaṭābhāvavad",
    "GSP29antarkpu:156": " bhṛtalam ity atra cakṣuḥsaṃyukte bhūtale ghaṭābhāvasya viśeṣaṇatvāt /",
    "GSP29antarkpu:157": " evaṃ snnikarṣaṣaṭkajanyaṃ jñānaṃ pratyakṣam /",
    "GSP29antarkpu:158": " tatkaraṇam indriyam /",
    "GSP29antarkpu:159": " tasmād indriyaṃ pratyakṣapramāṇam iti siddham //",
    "GSP29antarkpu:160": " AnTs_44 anumitikaraṇam anumānam /",
    "GSP29antarkpu:161": " parāmarśajanyaṃ jñānam anumitiḥ /",
    "GSP29antarkpu:162": " vyāptiviśiṣṭapakṣadharmatājñānaṃ parāmarśaḥ /",
    "GSP29antarkpu:163": " yathā vahnivyāpyadhūmavān ayaṃ parvata iti jñānaṃ parāmarśaḥ /",
    "GSP29antarkpu:164": " taj janyaṃ parvato vahnimān iti jñānam anumitiḥ /",
    "GSP29antarkpu:165": " yatra yatra dhūmas tatrāgnir iti sāhacaryaniyamo vyāptiḥ /",
    "GSP29antarkpu:166": " vyāpyasya parvatādivṛttitvaṃ pakṣadharmatā //",
    "GSP29antarkpu:167": " AnTs_45 anumānaṃ dvividhaṃ svārthaṃ parārthaṃ ca /",
    "GSP29antarkpu:168": " tatra svārthaṃ svānumitihetuḥ /",
    "GSP29antarkpu:169": " tathā hi svayam eva bhūyo darśanena yatra dhūmas tatra agnir iti mahānasādau vyāptiṃ",
    "GSP29antarkpu:170": " gṛhītvā parvatasamīpaṃ gatas tadgate cāgnau sandihānaḥ parvate dhūmaṃ paśyan vyāptiṃ",
    "GSP29antarkpu:171": " smarati yatra dhūmas tatrāgnir iti /",
    "GSP29antarkpu:172": " tadantaraṃ vahnivyāpyadhūmavān ayaṃ parvata iti jñānam utpadyate /",
    "GSP29antarkpu:173": " ayam eva liṅgaparāmarṣaṃ ity ucyate /",
    "GSP29antarkpu:174": " tasmāt parvato vahnimān iti jñānam anumitir utpadyate /",
    "GSP29antarkpu:175": " tad etat svārthānumānam /",
    "GSP29antarkpu:176": " yat tu svayaṃ dhūmād agnim anumāya parapratipattyarthaṃ pañcāvayavavākyaṃ prayukte tat parārthānumānam /",
    "GSP29antarkpu:177": " yathā parvato vahnimān dhūmavattvāt /",
    "GSP29antarkpu:178": " yo yo dhūmavān sa vahnimān yathā mahānasaḥ /",
    "GSP29antarkpu:179": " tathā cāyam /",
    "GSP29antarkpu:180": " tasmāt tatheti /",
    "GSP29antarkpu:181": " anena pratipāditāl lingāt paro'py agniṃ pratipadyate //",
    "GSP29antarkpu:182": " AnTs_46 pratijñāhetūdāharaṇopanayanigamāni pañcāvayavaḥ /",
    "GSP29antarkpu:183": " parvato vahnimān iti pratijñā /",
    "GSP29antarkpu:184": " dhūmavattvād iti hetuḥ /",
    "GSP29antarkpu:185": " yo yo dhūmavān sa so'gnimān yathā mahānasa ity udāharaṇam /",
    "GSP29antarkpu:186": " tathā cāyam iti upanayaḥ /",
    "GSP29antarkpu:187": " tasmāt tatheti nigamanam //",
    "GSP29antarkpu:188": " AnTs_47 svārthānumitiparārthānumityor liṅgaparāmarśa eva karaṇam /",
    "GSP29antarkpu:189": " tasmāl liṅgaparāmarśā'numānam //",
    "GSP29antarkpu:190": " AnTs_48 liṅgaṃ trividham /",
    "GSP29antarkpu:191": " anvayavyatireki kevalānvayi kevalavyatireki ceti /",
    "GSP29antarkpu:192": " anvayena vyatirekeṇa ca vyāptimad anvayavyatireki /",
    "GSP29antarkpu:193": " yathā vahnau sādhye dhūmavattvam /",
    "GSP29antarkpu:194": " yatra dhūmas tatrāgnir yathā mahānasa ity anvayavyāptiḥ /",
    "GSP29antarkpu:195": " yatra vahnir nāsti tatra dhūmo'pi nāsti yathā mahāhrada iti vyatirekavyāptiḥ /",
    "GSP29antarkpu:196": " anvayamātravyāptikaṃ kevalānvayi yathā ghaṭo'bhidheyaḥ prameyatvāt paṭavat /",
    "GSP29antarkpu:197": " atra prameyatvābhidhyatvayor vyatirekavyāptir nāsti sarvasyāpi prameyatvād abhidheyatvāc ca /",
    "GSP29antarkpu:198": " vyatirekamātravyāptikaṃ kevalavyatireki yathā pṛthivī tarebhyo bhidyate gandhavattvāt /",
    "GSP29antarkpu:199": " yad itarebhyo na bhidyate na tad gandhavat /",
    "GSP29antarkpu:200": " yathā jalam /",
    "GSP29antarkpu:201": " na ceyaṃ tathā /",
    "GSP29antarkpu:202": " tasmān na tatheti atra yad gandhavat tad itarabhinnam ity anvayadṛṣṭanto nāsti pṛthivīmātrasya pakṣatvāt //",
    "GSP29antarkpu:203": " AnTs_49 saṃdigdhasādhyavān pakṣaḥ /",
    "GSP29antarkpu:204": " yathā dhūmavattve hetau parvataḥ //",
    "GSP29antarkpu:205": " AnTs_50 niścitasādhyavān sapakṣaḥ /",
    "GSP29antarkpu:206": " yathā tatraiva mahānasaḥ //",
    "GSP29antarkpu:207": " AnTs_51 niścitasādhyābhāvavān vipakṣaḥ /",
    "GSP29antarkpu:208": " yathā tatraiva mahāhradaḥ //",
    "GSP29antarkpu:209": " AnTs_52 savyabhicāraviruddhasatpakṣāsiddhabādhitāḥ pañca hetvābhāsāḥ //",
    "GSP29antarkpu:210": " AnTs_53 savyabhicāro'naikāntikaḥ /",
    "GSP29antarkpu:211": " sa trividhaḥ /",
    "GSP29antarkpu:212": " sādhāraṇāsādhāraṇānupasaṃhāribhedāt /",
    "GSP29antarkpu:213": " tatra sādhyābhāvavadvṛttiḥ sādhāraṇo anaikāntikaḥ /",
    "GSP29antarkpu:214": " yathā parvato vahnimān prameyatvād iti prameyatvasya vahnyabhāvavati hrade vidyamānatvāt /",
    "GSP29antarkpu:215": " sarvasapakṣavipakṣavyāvṛtto'sādhāraṇaḥ /",
    "GSP29antarkpu:216": " yathā śabdo nityaḥ śabdatvād iti /",
    "GSP29antarkpu:217": " śabdatvaṃ sarvebhyo nityebhyo'ntyebhyaś ca vyāvṛttaṃ śabdamātravṛtti /",
    "GSP29antarkpu:218": " anvayavyatirekadṛṣṭāntarahito'nupasaṃhārī /",
    "GSP29antarkpu:219": " yathā sarvam anityaṃ prameyatvād iti /",
    "GSP29antarkpu:220": " atra sarvasyāpi pakṣatvād dṛṣṭānto nāsti //",
    "GSP29antarkpu:221": " AnTs_54 sādhyābhāvavyāpto hetur viruddhaḥ /",
    "GSP29antarkpu:222": " yatra śabdo nityaḥ kṛtakatvād iti /",
    "GSP29antarkpu:223": " kṛtakatvaṃ hi nityatvābhāvenānityatvena vyāptam //",
    "GSP29antarkpu:224": " AnTs_55 yasya sādhyabhāvasādhakaṃ hetvantaraṃ vidyate sa satpratipakṣaḥ /",
    "GSP29antarkpu:225": " yathā śabdo nityaḥ śrāvaṇatvāc chabdatvavad iti /",
    "GSP29antarkpu:226": " śabdo'nityaḥ kāryatvād ghaṭavad iti //",
    "GSP29antarkpu:227": " AnTs_56 asiddhas trividhaḥ /",
    "GSP29antarkpu:228": " āśrayāsiddhaḥ svarūpāsiddho vyāpyatvāsiddhaś ceti /",
    "GSP29antarkpu:229": " āśrayāsiddho yathā gaganāravindaṃ surabhy aravindatvāt sarojāravindavat /",
    "GSP29antarkpu:230": " atra gaganāravindam āśrayaḥ /",
    "GSP29antarkpu:231": " sa ca nāsty eva /",
    "GSP29antarkpu:232": " svarūpāsiddho yathā śabdo guṇaś cākṣuṣatvāt /",
    "GSP29antarkpu:233": " atra cākṣuṣatvaṃ śabde nāsti śabdasya śrāvaṇatvāt /",
    "GSP29antarkpu:234": " sopādhiko vyāpyatvāsiddhaḥ /",
    "GSP29antarkpu:235": " sādhyavyāpakatve sati sādhanāvyāpaka upādhiḥ /",
    "GSP29antarkpu:236": " sādhyasamānādhikaraṇātyantābhāvāpratiyogitvaṃ sādhyavyāpakatvam /",
    "GSP29antarkpu:237": " sādhanavann iṣṭātyantābhāvapratiyogitvaṃ sādhāraṇāvyapakatvam /",
    "GSP29antarkpu:238": " parvato dhūmavān vahnimattvād ity atrārdrendhasaṃyoga upādhiḥ /",
    "GSP29antarkpu:239": " tathā hi /",
    "GSP29antarkpu:240": " yatra dhūmas tatrārdrendhnasaṃyoga iti sādhyavyāpakatā /",
    "GSP29antarkpu:241": " yatra vahnis tatrārdrendhanasaṃyogābhāvād iti sādhanāvyāpakatā /",
    "GSP29antarkpu:242": " evaṃ sādhyavyāpakatve",
    "GSP29antarkpu:243": " sati sādhanāvyāpakatvād ardrendhanasaṃyoga upādhiḥ /",
    "GSP29antarkpu:244": " sopādhikatvād vahnimattvaṃ vyāpyatvāsiddham //",
    "GSP29antarkpu:245": " AnTs_57 yasya sādhyābhāvaḥ pramāṇāntareṇa niścitaḥ sa bādhitaḥ /",
    "GSP29antarkpu:246": " yathā vahnir anuṣṇo dravyatvād iti /",
    "GSP29antarkpu:247": " atrānuṣṇatvaṃ sādhyaṃ tadabhāva uṣṇatvaṃ spārśanapratyakṣeṇa gṛhyata iti bādhitatvam //",
    "GSP29antarkpu:248": " AnTs_58 upamitikaraṇam upamānam /",
    "GSP29antarkpu:249": " saṃjñāsaṃjñisaṃbandhajñānam upamitiḥ /",
    "GSP29antarkpu:250": " tatkaraṇaṃ sādṛśyajñānam /",
    "GSP29antarkpu:251": " atideśavākyārthasmaraṇam avāntaravyāpāraḥ /",
    "GSP29antarkpu:252": " tathā hi kaś cid gavayaśabdārtham ajānan kutaś cid āraṇyakapuruṣād gosadṛśo gavaya iti śrutvā vanaṃ gato vākyārthaṃ smaran gosadṛṣaṃ piṇḍaṃ paśyati /",
    "GSP29antarkpu:253": " tadanantaram asau gavayaśabdavācya ity upamitir utpadyte //",
    "GSP29antarkpu:254": " AnTs_59 āptavākyaṃ śabdaḥ /",
    "GSP29antarkpu:255": " āptas tu yathārthavaktā /",
    "GSP29antarkpu:256": " vākyaṃ padasamūhaḥ /",
    "GSP29antarkpu:257": " yathā gām ānayeti /",
    "GSP29antarkpu:258": " śaktaṃ padam /",
    "GSP29antarkpu:259": " asmāt padād ayam artho boddhavya itīśvarasaṃketaḥ śaktiḥ //",
    "GSP29antarkpu:260": " AnTs_60 ākāṅkṣā yogyatā saṃnidhiś ca vākyārthajñānahetuḥ /",
    "GSP29antarkpu:261": " padasya padāntaravyatirekaprayuktānvayānanubhāvakatvam ākāṅkṣā /",
    "GSP29antarkpu:262": " arthābādho yogyatā /",
    "GSP29antarkpu:263": " padānāmavilambenoccāraṇaṃ saṃnidhiḥ //",
    "GSP29antarkpu:264": " AnTs_61 ākāṅkṣādirahitaṃ vākyam apramāṇam /",
    "GSP29antarkpu:265": " yathā gaur aśvaḥ puruṣo hastīti na pramāṇam ākāṅkṣāvirahāt /",
    "GSP29antarkpu:266": " agninā siñced iti na pramāṇaṃ yogyatāvirahāt /",
    "GSP29antarkpu:267": " prahare prehare'sahoccāritāni gām ānayetyādipadāni na pramāṇaṃ saṃnidhyabhāvāt //",
    "GSP29antarkpu:268": " AnTs_62 vākyaṃ dvividham /",
    "GSP29antarkpu:269": " vaidikaṃ laukikaṃ ca /",
    "GSP29antarkpu:270": " vaidikam īśvaroktatvāt sarvam eva pramāṇam /",
    "GSP29antarkpu:271": " laukikaṃ tv āptoktaṃ pramāṇam /",
    "GSP29antarkpu:272": " anyad apramāṇam //",
    "GSP29antarkpu:273": " AnTs_63 vākyārthajñānaṃ śabdajñānam /",
    "GSP29antarkpu:274": " tatkaraṇaṃ śabdaḥ //",
    "GSP29antarkpu:275": " AnTs_64 ayathārthānubhavas trividhaḥ saṃśayaviparyayatarkabhedāt /",
    "GSP29antarkpu:276": " ekasmin dharmini viruddhanānādharmavaiśiṣṭyāvagāhi jñānaṃ saṃśayaḥ /",
    "GSP29antarkpu:277": " yathā sthāṇur vā purṣo veti /",
    "GSP29antarkpu:278": " mithyājñānaṃ viparyayaḥ /",
    "GSP29antarkpu:279": " yathā śuktāv idaṃ rajatam iti /",
    "GSP29antarkpu:280": " vyāpyāropeṇa vyāpakāropas tarkaḥ yathā yadi vahnir na syāt tarhi dhūmo'pi na syād iti //",
    "GSP29antarkpu:281": " AnTs_65 smṛtir api dvividhā /",
    "GSP29antarkpu:282": " yathārthāyathārthā ca pramājanyā yathārthā /",
    "GSP29antarkpu:283": " apramājanyāyathārthā //",
    "GSP29antarkpu:284": " AnTs_66 sarveśām anukūlatayā vedanīyaṃ sukham //",
    "GSP29antarkpu:285": " AnTs_67 sarveśāṃ pratikūlatayā vedanīyaṃ duḥkham //",
    "GSP29antarkpu:286": " AnTs_68 icchā kāmaḥ //",
    "GSP29antarkpu:287": " AnTs_69 krodho dveṣaḥ //",
    "GSP29antarkpu:288": " AnTs_70 kṛtiḥ prayatnaḥ //",
    "GSP29antarkpu:289": " AnTs_71 vihitakarmajanyo dharmaḥ //",
    "GSP29antarkpu:290": " AnTs_72 niṣiddhakarmajanyas tv adharmaḥ //",
    "GSP29antarkpu:291": " AnTs_73 buddhyādayo'ṣṭāv ātmamātraviśeṣaguṇāḥ //",
    "GSP29antarkpu:292": " AnTs_74 buddhīcchāprayatnā dvividhāḥ /",
    "GSP29antarkpu:293": " nityā anityāś ca /",
    "GSP29antarkpu:294": " nityā īśvarasya /",
    "GSP29antarkpu:295": " anityā jīvasya //",
    "GSP29antarkpu:296": " AnTs_75 saṃskāras trividhaḥ /",
    "GSP29antarkpu:297": " vego bhāvanā sthitisthāpakaś ceti /",
    "GSP29antarkpu:298": " vegaḥ pṛthivyādicatuṣṭayamanovṛttiḥ /",
    "GSP29antarkpu:299": " anubhavajanyā smṛtihetur bhāvanātmamātravṛttiḥ /",
    "GSP29antarkpu:300": " anyathā kṛtasya punas tadavasthāpādakaḥ sthitisthāpakaḥ kaṭādipṛthivīvṛttiḥ //",
    "GSP29antarkpu:301": " AnTs_76 calanātmakaṃ karma /",
    "GSP29antarkpu:302": " ūrdhvadeśasaṃyogahetur utkṣepaṇam /",
    "GSP29antarkpu:303": " adhodeśasaṃyogahetur apakṣepaṇam /",
    "GSP29antarkpu:304": " śarīrasaṃnikṛṣṭasaṃyogahetur ākuñcanam /",
    "GSP29antarkpu:305": " viprakṛṣṭasaṃyogahetuḥ prasāraṇam /",
    "GSP29antarkpu:306": " anyat sarvaṃ gamanam /",
    "GSP29antarkpu:307": " pṛthivyādicatuṣṭayamanomātravṛtti //",
    "GSP29antarkpu:308": " AnTs_77 nityam ekam anekānugataṃ sāmānyam /",
    "GSP29antarkpu:309": " dravyaguṇakarmavṛtti /",
    "GSP29antarkpu:310": " tad dvividhaṃ parāparabhedāt /",
    "GSP29antarkpu:311": " paraṃ sattā /",
    "GSP29antarkpu:312": " aparaṃ dravyatvādiḥ //",
    "GSP29antarkpu:313": " AnTs_78 nityadravyavṛttayo vyāvartakā viśeṣāḥ //",
    "GSP29antarkpu:314": " AnTs_79 nityasaṃbandhaḥ samavāyaḥ /",
    "GSP29antarkpu:315": " ayutasiddhavṛttiḥ /",
    "GSP29antarkpu:316": " yayor dvayor madhya ekam avinaśyadaparāśritam evāvatiṣṭate tāv ayutasiddau /",
    "GSP29antarkpu:317": " yathāvayavāvayavinau guṇaguṇinau kriyākriyāvantau jātivyaktī viśeṣanityadravye ceti //",
    "GSP29antarkpu:318": " AnTs_80 anādiḥ sāntaḥ prāgabhāvaḥ /",
    "GSP29antarkpu:319": " utpatteḥ pūrvaṃ kāryasya /",
    "GSP29antarkpu:320": " sādir anantaḥ pradhvaṃsaḥ /",
    "GSP29antarkpu:321": " utpattyanantaraṃ kāryasya /",
    "GSP29antarkpu:322": " traikālikasaṃsargāvacchinnapratiyogitāko'tyantābhāvaḥ /",
    "GSP29antarkpu:323": " yathā bhūtale ghaṭo nāstīti /",
    "GSP29antarkpu:324": " tādātmyasaṃbandhāvacchinnapratiyogitāko'nyonyābhāvaḥ yathā ghaṭaḥ paṭo na bhavatīti //",
    "GSP29antarkpu:325": " AnTs_81 sarveṣāṃ padārthānāṃ yathāyatham ukteṣv antarbhāvāt saptaiva padārthā iti siddham //",
    "GSP29antarkpu:326": " kāṇādanyāyamatayor bālāvyutpattisiddhaye /",
    "GSP29antarkpu:327": " annaṃbhaṭṭena viduṣā racitas tarkasaṃgrahaḥ //"
}