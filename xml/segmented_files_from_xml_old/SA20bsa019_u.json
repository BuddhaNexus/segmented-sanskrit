{
  "SA20bsa019_u:0": "// namo mañjuśriye kumārabhūtāya //",
  "SA20bsa019_u:1": "ācāryavasubandhupraṇītaḥ",
  "SA20bsa019_u:2": "Trisvabhāvanirdeśaḥ (Tsn)",
  "SA20bsa019_u:3": "Tsn_1 kalpitaḥ paratantraśca pariniṣpanna eva ca /",
  "SA20bsa019_u:4": "trayaḥ svabhāvā dhīrāṇāṃ gambhīrajñeyamiṣyate //",
  "SA20bsa019_u:5": "Tsn_2 yat khyāti paratantro 'sau yathā khyāti sa kalpitaḥ /",
  "SA20bsa019_u:6": "pratyayādhīnavṛttitvāt kalpanāmātrabhāvataḥ //",
  "SA20bsa019_u:7": "Tsn_3 tasya khyāturyathākhyānaṃ yā sadāvidyamānatā /",
  "SA20bsa019_u:8": "jñeyaḥ sa pariniṣpannasvabhāvo 'nanyathātvataḥ //",
  "SA20bsa019_u:9": "Tsn_4 tatra kiṃ khyātyasatkalpaḥ kathaṃ khyāti dvayātmanā /",
  "SA20bsa019_u:10": "tasya kā nāstitā tena yā tatrādvayadharmatā //",
  "SA20bsa019_u:11": "Tsn_5 asatkalpo 'tra kaścittaṃ yatastena hi kalpyate /",
  "SA20bsa019_u:12": "yathā ca kalpayatyartha tathātyantaṃ na vidyate //",
  "SA20bsa019_u:13": "Tsn_6 taddhetuphalabhāvena cittaṃ dvividhamiṣyate /",
  "SA20bsa019_u:14": "yadālayākhyaṃ vijñānaṃ pravṛttyākhyaṃ ca saptadhā //",
  "SA20bsa019_u:15": "Tsn_7 saṃkleśavāsanābījaiścitatvāccittamucyate /",
  "SA20bsa019_u:16": "cittamādyaṃ dvitīyaṃ tu citrākārapravṛttitaḥ //",
  "SA20bsa019_u:17": "Tsn_8 samāsato 'bhūtakalpaḥ sa caiṣa trividho mataḥ /",
  "SA20bsa019_u:18": "vaipākikastathā naimittiko 'nyaḥ prātibhāsikaḥ //",
  "SA20bsa019_u:19": "Tsn_9 prathamo mūlavijñānaṃ tadvipākātmakaṃ yataḥ /",
  "SA20bsa019_u:20": "anyaḥ pravṛttivijñānaṃ dṛśyadṛgvittivṛttitaḥ //",
  "SA20bsa019_u:21": "Tsn_10 sadasattvād dvayaikatvāt saṃkleśavyavadānayoḥ /",
  "SA20bsa019_u:22": "lakṣaṇābhedataśceṣṭā svabhāvānāṃ gaṃbhīratā //",
  "SA20bsa019_u:23": "Tsn_11 sattvena gṛhyate yasmādatyantābhāva eva ca /",
  "SA20bsa019_u:24": "svabhāvaḥ kalpitastena sadasallakṣaṇo mataḥ //",
  "SA20bsa019_u:25": "Tsn_12 vidyate bhrāntibhāvena yathākhyānaṃ na vidyate /",
  "SA20bsa019_u:26": "paratantro yatastena sadasallakṣaṇo mataḥ //",
  "SA20bsa019_u:27": "Tsn_13 advayatvena yaccāsti dvayasyābhāva eva ca /",
  "SA20bsa019_u:28": "svabhāvastena niṣpannaḥ sadasallakṣaṇo mataḥ //",
  "SA20bsa019_u:29": "Tsn_14 dvaividhyāt kalpitārthasya tadasattvaikabhāvataḥ /",
  "SA20bsa019_u:30": "svabhāvaḥ kalpito bālairdvayaikatvātmako mataḥ //",
  "SA20bsa019_u:31": "Tsn_15 prakhyānād dvayabhāvena bhrāntimātraikabhāvataḥ /",
  "SA20bsa019_u:32": "svabhāvaḥ paratantrākhyo dvayaikatvātmako mataḥ //",
  "SA20bsa019_u:33": "Tsn_16 dvayābhāvasvabhāvatvādadvayaikasvabhāvataḥ /",
  "SA20bsa019_u:34": "svabhāvaḥ pariniṣpanno dvayaikatvātmako mataḥ //",
  "SA20bsa019_u:35": "Tsn_17 kalpitaḥ paratantraśca jñeyaṃ saṃkleśalakṣaṇam /",
  "SA20bsa019_u:36": "pariniṣpanna iṣṭastu vyavadānasya lakṣaṇam //",
  "SA20bsa019_u:37": "Tsn_18 asaddvayasvabhāvatvāt tadabhāvasvabhāvataḥ /",
  "SA20bsa019_u:38": "svabhāvāt kalpitājjñeyo niṣpanno 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:39": "Tsn_19 advayatvasvabhāvatvād dvayābhāvasvabhāvataḥ /",
  "SA20bsa019_u:40": "niṣpannāt kalpitaścaiva vijñeyo 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:41": "Tsn_20 yathākhyānamasadbhāvāt tathāsattvasvabhāvataḥ /",
  "SA20bsa019_u:42": "svabhāvāt paratantrākhyānniṣpanno 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:43": "Tsn_21 asaddvayasvabhāvatvād yathākhyānāsvabhāvataḥ /",
  "SA20bsa019_u:44": "niṣpannāt paratantro 'pi vijñeyo 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:45": "Tsn_22 kramabhedaḥ svabhāvānāṃ vyavahārādhikārataḥ /",
  "SA20bsa019_u:46": "tatpraveśādhikārācca vyutpattyarthaṃ vidhīyate //",
  "SA20bsa019_u:47": "Tsn_23 kalpito vyavahārātmā vyavahartrātmako 'paraḥ /",
  "SA20bsa019_u:48": "vyavahārasamucchedasvabhāvaścānya iṣyate //",
  "SA20bsa019_u:49": "Tsn_24 dvayābhāvātmakaḥ pūrvaṃ paratantraḥ praviśyate /",
  "SA20bsa019_u:50": "tataḥ praviśyate tatra kalpamātramasaddvayam //",
  "SA20bsa019_u:51": "Tsn_25 tato dvayābhāvabhāvo niṣpanno 'tra praviśyate /",
  "SA20bsa019_u:52": "tathā hyasāveva tadā astināstīti cocyate //",
  "SA20bsa019_u:53": "Tsn_26 trayo 'pyete svabhāvā hi advayālambalakṣaṇāḥ /",
  "SA20bsa019_u:54": "abhāvādatathābhāvāt tadabhāvasvabhāvataḥ //",
  "SA20bsa019_u:55": "Tsn_27 māyākṛtaṃ mantravaśāt khyāti hastyātmanā yathā /",
  "SA20bsa019_u:56": "ākāramātraṃ tatrāsti hastī nāsti tu sarvathā //",
  "SA20bsa019_u:57": "Tsn_28 svabhāvaḥ kalpito hastī paratantrastadākṛtiḥ /",
  "SA20bsa019_u:58": "yastatra hastyabhāvo 'sau pariniṣpanna iṣyate //",
  "SA20bsa019_u:59": "Tsn_29 asatkalpastathā khyāti mūlacittād dvayātmanā /",
  "SA20bsa019_u:60": "dvayamatyantato nāsti tatrāstyākṛtimātrakam //",
  "SA20bsa019_u:61": "Tsn_30 mantravanmūlavijñānaṃ kāṣṭhavattathatā matā /",
  "SA20bsa019_u:62": "hastyākāravadeṣṭavyo vikalpo hastivad dvayam //",
  "SA20bsa019_u:63": "Tsn_31 arthatattvaprativedhe yugapallakṣaṇatrayam /",
  "SA20bsa019_u:64": "parijñā ca prahāṇaṃ ca prāptiśceṣṭā yathākramam //",
  "SA20bsa019_u:65": "Tsn_32 parijñānupalambho 'tra hānirakhyānamiṣyate /",
  "SA20bsa019_u:66": "upalambhanimittā tu prāptiḥ sākṣātkriyāpi sā //",
  "SA20bsa019_u:67": "Tsn_33 dvayasyānupalambhena dvayākāro vigacchati /",
  "SA20bsa019_u:68": "vigamāt tasya niṣpanno dvayābhāvo 'dhigamyate //",
  "SA20bsa019_u:69": "Tsn_34 hastino 'nupalambhaśca vigamaśca tadākṛteḥ /",
  "SA20bsa019_u:70": "upalambhaśca kāṣṭhasya māyāyāṃ yugapad yathā //",
  "SA20bsa019_u:71": "Tsn_35 viruddhadhīvāraṇatvād buddhyā vaiyarthyadarśanāt /",
  "SA20bsa019_u:72": "jñānatrayānuvṛtteśca mokṣāpattirayatnataḥ //",
  "SA20bsa019_u:73": "Tsn_36 cittamātropalambhena jñeyārthānupalambhatā /",
  "SA20bsa019_u:74": "jñeyārthānupalambhena syāccittānupalambhatā //",
  "SA20bsa019_u:75": "Tsn_37 dvayoranupalambhena dharmadhātūpalabhbhatā /",
  "SA20bsa019_u:76": "dharmadhātūpalambhena syād vibhutvopalambhatā //",
  "SA20bsa019_u:77": "Tsn_38 upalabdhavibhutvaśca svaparārthaprasiddhitaḥ /",
  "SA20bsa019_u:78": "prāpnotyanuttarāṃ bodhiṃ dhīmān kāyatrayātmikām //",
  "SA20bsa019_u:79": "iti trisvabhāvanirdeśaḥ samāptaḥ //",
  "SA20bsa019_u:80": "kṛtirācāryavasubandhupādānāmiti //"
}