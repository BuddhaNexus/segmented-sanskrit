{
  "S06bsa011_u:0": "Ācāryadignāgaviracitaḥ",
  "S06bsa011_u:1": "Prajñāpāramitāpiṇḍārthaḥ (Pppa)",
  "S06bsa011_u:2": "/ namaḥ prajñāpāramitāyai //",
  "S06bsa011_u:3": "Pppa_1 prajñāparamitā jñānamadvayaṃ sā tathāgataḥ /",
  "S06bsa011_u:4": "sādhyā tādarthyayogena tācchabdyaṃ granthamārgayoḥ //",
  "S06bsa011_u:5": "Pppa_2 āśrayaścādhikāraśca karma bhāvanayā saha /",
  "S06bsa011_u:6": "prabhedo liṅgamāpacca sānuśaṃsamudāhṛtam //",
  "S06bsa011_u:7": "Pppa_3 śradvāvatāṃ pravṛtyaṅgaṃ śāstā parṣacca sākṣiṇī /",
  "S06bsa011_u:8": "deśakālau ca nirdiṣṭau svaprāmāṇyaprasiddhaye //",
  "S06bsa011_u:9": "Pppa_4 saṅgītikartā loke hi deśakālopalakṣitam /",
  "S06bsa011_u:10": "sasākṣikaṃ vadan vaktā prāmāṇyamadhigacchati //",
  "S06bsa011_u:11": "Pppa_5 sarvaṃ caitannipātātmaśravaṇādeḥ prakīrtanam /",
  "S06bsa011_u:12": "prāsaṅgikaṃ tu evārthā mukhyā dvātriṃśadeva hi //",
  "S06bsa011_u:13": "Pppa_6 prabhedaḥ ṣoḍaśākāraḥ śūnyatāyā yathākramam /",
  "S06bsa011_u:14": "nirdiṣṭo 'ṣṭāsahastrayā sa vijñeyo 'nyāpadeśataḥ //",
  "S06bsa011_u:15": "Pppa_7 itthamaṣṭasahasrīyamanyūnārthairyathoditaiḥ /",
  "S06bsa011_u:16": "granthasaṅkṣepa iṣṭo 'tra ta evārthā yathoditāḥ //",
  "S06bsa011_u:17": "Pppa_8 bodhisattvaṃ na paśyāmītyuktavāṃstattvato muniḥ /",
  "S06bsa011_u:18": "bhoktādhyātmikavastūnāṃ kathitā tena śūnyatā //",
  "S06bsa011_u:19": "Pppa_9 rūpaṃ rūpasvabhāvena śūnyamityuktitaḥ punaḥ /",
  "S06bsa011_u:20": "bāhyānyāyatanānīha bhojyāni pratiṣiddhavān //",
  "S06bsa011_u:21": "Pppa_10 rūpādyabhāve taddehapratiṣṭhālakṣaṇakṣatiḥ /",
  "S06bsa011_u:22": "gatārthā yena taddṛṣṭaṃ tadādhyātmikamityasat //",
  "S06bsa011_u:23": "Pppa_11 ādhyātmikānāṃ śūnyatve prakṛterapi śūnyatā /",
  "S06bsa011_u:24": "vijñānarūpaṃ gotraṃ hi kṛpāprajñātmakaṃ matam //",
  "S06bsa011_u:25": "Pppa_12 notpanno na niruddho vā sattva ityādinā sphuṭam /",
  "S06bsa011_u:26": "sattvasaṃsārayoḥ kāmaṃ darśitā tena śūnyatā //",
  "S06bsa011_u:27": "Pppa_13 buddhadharmāṃstathā bodhisattvadharmānna paśyati /",
  "S06bsa011_u:28": "ityādinā vinirdiṣṭā śūnyā daśabalādayaḥ //",
  "S06bsa011_u:29": "Pppa_14 prati prati yato dharmāḥ kalpitā iti kīrtitam /",
  "S06bsa011_u:30": "tato na paramārtho 'sti dharmāṇāmiti coktavān //",
  "S06bsa011_u:31": "Pppa_15 ātmādidṛṣṭerucchedaṃ mahatyā prakaroti yat /",
  "S06bsa011_u:32": "tataḥ pudgalanairātmyaṃ bhagavān sarvathā jagau //",
  "S06bsa011_u:33": "Pppa_16 sarvadharmā anutpannā iti kīrtayatā tathā /",
  "S06bsa011_u:34": "kathitaṃ dharmanairātmyaṃ sarvathā tattvavedinā //",
  "S06bsa011_u:35": "Pppa_17 sāvadyaniravadyānāmavṛddhiparihāṇitaḥ /",
  "S06bsa011_u:36": "saṃskṛtāsaṃskṛtānāṃ ca kuśalānāṃ nirākṛtiḥ //",
  "S06bsa011_u:37": "Pppa_18 kuśalānāṃ ca śūnyatve tadgatā akṣatā tathā /",
  "S06bsa011_u:38": "kalpitaiveti bhedānāṃ śūnyatāyāḥ sa saṅgrahaḥ //",
  "S06bsa011_u:39": "Pppa_19 daśabhiścittavikṣepaiścittaṃ vikṣiptamanyataḥ /",
  "S06bsa011_u:40": "yogyaṃ bhavati bālānāṃ nādvayajñānasādhane //",
  "S06bsa011_u:41": "Pppa_20 tānapākartumanyonyaṃ vipakṣapratipakṣataḥ /",
  "S06bsa011_u:42": "prajñāpāramitāgranthaste ca sampiṇḍaya darśitāḥ //",
  "S06bsa011_u:43": "Pppa_21 yadāha bodhisattvaḥ sannityabhāvaprakalpanā /",
  "S06bsa011_u:44": "vikṣepaṃ vikṣipan śāstā sāṃvṛtaskandhadarśanāt //",
  "S06bsa011_u:45": "Pppa_22 etenāṣṭasahasryādāvādivākyātprabhṛtyapi /",
  "S06bsa011_u:46": "ā samāpterniṣeddhavyā vidhinābhāvakalpanā //",
  "S06bsa011_u:47": "Pppa_23 hetuvākyāni naitāni kṛtyamātraṃ tu sūcyate /",
  "S06bsa011_u:48": "brahmajālādisūtreṣu jñeyāḥ sarvatra yuktayaḥ //",
  "S06bsa011_u:49": "Pppa_24 bodhisattvaṃ na paśyāmi ahamityādi vistaraiḥ /",
  "S06bsa011_u:50": "nirākaroti bhagavān bhāvasaṅkalpavibhramam //",
  "S06bsa011_u:51": "Pppa_25 yanna paśyati nāmāpi gocaraṃ[na] kriyāṃ tathā /",
  "S06bsa011_u:52": "skandhāṃśca sarvatastena bodhisattvaṃ na paśyati //",
  "S06bsa011_u:53": "Pppa_26 kalpitasya niṣedho 'yamiti saṅgrahadarśanam /",
  "S06bsa011_u:54": "sarvo jñeyatayārūḍha ākāraḥ kalpito matau //",
  "S06bsa011_u:55": "Pppa_27 prajñāpāramitāyāṃ hi trīn samāśritya deśanā /",
  "S06bsa011_u:56": "kalpitaṃ paratantraṃ ca pariniṣpannameva ca //",
  "S06bsa011_u:57": "Pppa_28 nāstītyādipadaiḥ sarvaṃ kalpitaṃ vinivāryate /",
  "S06bsa011_u:58": "māyopamādidṛṣṭāntaiḥ paratantrasya deśanā //",
  "S06bsa011_u:59": "Pppa_29 caturdhā vyavadānena pariniṣpannakīrtanam /",
  "S06bsa011_u:60": "prajñāpāramitāyāṃ hi nānyā buddhasya deśanā //",
  "S06bsa011_u:61": "Pppa_30 daśasaṅkalpavikṣepavipakṣe deśanākrame /",
  "S06bsa011_u:62": "trayāṇāmiha boddhavyaṃ samastavyastakīrtanam //",
  "S06bsa011_u:63": "Pppa_31 yathādivākye niṣpannaparatantraparikalpitaiḥ /",
  "S06bsa011_u:64": "abhāvakalpanārūpavikṣepavinivāraṇam //",
  "S06bsa011_u:65": "Pppa_32 tena buddhaṃ tathā bodhiṃ na paśyāmīti vācakaiḥ /",
  "S06bsa011_u:66": "ā samāpteriha jñeyā kalpitānāṃ nirākṛtiḥ //",
  "S06bsa011_u:67": "Pppa_33 śūnye rūpe svabhāvena samāropaḥ kva kena vā /",
  "S06bsa011_u:68": "ityanyeṣvapi vākyeṣu boddhavyaṃ tannivāraṇam //",
  "S06bsa011_u:69": "Pppa_34 na hi śūnyatayā śūnyamiti vākyaṃ vinirdiśan /",
  "S06bsa011_u:70": "apavādavikalpānāṃ nirāsaṃ sarvathoktavān //",
  "S06bsa011_u:71": "Pppa_35 māyopamastathā buddhaḥ sa svapnopama ityapi /",
  "S06bsa011_u:72": "ayameva kramo jñeyo vijñairvākyāntareṣvapi //",
  "S06bsa011_u:73": "Pppa_36 sāmānādhikaraṇyena prokto māyopamo jinaḥ /",
  "S06bsa011_u:74": "māyopamādiśabdaiśca paratantro nigadyate //",
  "S06bsa011_u:75": "Pppa_37 pṛthagjanānāṃ yajjñānaṃ prakṛtivyavadānikam /",
  "S06bsa011_u:76": "uktaṃ tadbuddhaśabdena bodhisattvo yathā jinaḥ //",
  "S06bsa011_u:77": "Pppa_38 nijaṃ svarūpaṃ pracchādya tadavidyāvaśīkṛtam /",
  "S06bsa011_u:78": "māyāvadanyathā bhāti phalaṃ svapnam ivojhati //",
  "S06bsa011_u:79": "Pppa_39 advayasyānyathākhyātau phale vāpyapavādinām /",
  "S06bsa011_u:80": "apavādavikalpānāmapavādo 'yamucyate //",
  "S06bsa011_u:81": "Pppa_40 na rūpaṃ śūnyatā yuktā parasparavirodhataḥ /",
  "S06bsa011_u:82": "nīrūpā śūnyatā nāmarūpamākārasaṅgatam //",
  "S06bsa011_u:83": "Pppa_41 ityekatvavikalpasya bāddhā(dhyā?)",
  "S06bsa011_u:84": "nānātvakalpanam(nā?) /",
  "S06bsa011_u:85": "ruṇāddhi nānyattadrūpaṃ śūnyatāyāṃ kathañcana //",
  "S06bsa011_u:86": "Pppa_42 asadeva yataḥ khyāti tadavidyāvinirmitam /",
  "S06bsa011_u:87": "asatkhyāpanaśaktyaiva sāvidyeti nigadyate //",
  "S06bsa011_u:88": "Pppa_43 idamevocyate rūpaṃ prajñāpāramiteti ca /",
  "S06bsa011_u:89": "advayaṃ dvayamevaitadvikalpadvayabādhanam //",
  "S06bsa011_u:90": "Pppa_44 yuktiṃ cāha viśuddhatvāttathā cānupalambhataḥ /",
  "S06bsa011_u:91": "bhāvābhāvavirodhācca nānātvamapi paśyati //",
  "S06bsa011_u:92": "Pppa_45 nāmamātramidaṃ rūpaṃ tattvato hyasvabhāvakam /",
  "S06bsa011_u:93": "tatsvabhāvavikalpānāmavakāśaṃ nirasyati //",
  "S06bsa011_u:94": "Pppa_46 rūpaṃ rūpasvabhāvena śūnyaṃ yatprathamoditam /",
  "S06bsa011_u:95": "tatsvabhāvasamāropasaṅkalpapratiṣedhanam //",
  "S06bsa011_u:96": "Pppa_47 notpādaṃ na nirodhaṃ ca dharmāṇāṃ paśyatīti yat /",
  "S06bsa011_u:97": "bhagavānāha,",
  "S06bsa011_u:98": "tadvayastā tadviśeṣasya kalpanā //",
  "S06bsa011_u:99": "Pppa_48 kṛtrimaṃ nāma vācyāśca dharmāste kalpitā yataḥ /",
  "S06bsa011_u:100": "śabdārthayorna sambandhastena svābhāviko mataḥ //",
  "S06bsa011_u:101": "Pppa_49 bāhyārthābhiniveśastu bhrāntyā bālasya jṛmbhate /",
  "S06bsa011_u:102": "tathaiva vyavahāro 'yaṃ tvatrārtho 'sti kaścana //",
  "S06bsa011_u:103": "Pppa_50 atra tena yathā nāma kalpyate na tathāsti tat /",
  "S06bsa011_u:104": "vācyaṃ vastu tato niṣṭhā yathānāmārthakalpanā //",
  "S06bsa011_u:105": "Pppa_51 prajñāpāramitā buddho bodhisattvo 'pi vā tathā /",
  "S06bsa011_u:106": "nāmamātramiti prāha vyasan satyārthakalpanam //",
  "S06bsa011_u:107": "Pppa_52 śabdārthapratiṣedho 'yaṃ na vastu vinivāryate /",
  "S06bsa011_u:108": "evamanyeṣvapi jñeyo vākyeṣvarthaviniścayaḥ //",
  "S06bsa011_u:109": "Pppa_53 naivopalabhate samyak sarvanāmāni tattvavit /",
  "S06bsa011_u:110": "yathārthatvena tenedaṃ na dhvanervinivāraṇam //",
  "S06bsa011_u:111": "Pppa_54 subhūtistu dvayaṃ vyasan śabdaṃ śabdārthameva ca /",
  "S06bsa011_u:112": "bodhisattvasya no nāma paśyāmīti sa uktavān //",
  "S06bsa011_u:113": "Pppa_55 prajñāpāramitāvākyaṃ nāsti yanneyatā gatam /",
  "S06bsa011_u:114": "ūhyāstu kevalaṃ te 'rthā[stadevaṃ]sūkṣmayā dhiyā //",
  "S06bsa011_u:115": "Pppa_56 prakrāntārthatiraskāro yā cārthāntarakalpanā /",
  "S06bsa011_u:116": "prajñāpāramitāyāṃ hi proktā sā prativarṇikā //",
  "S06bsa011_u:117": "Pppa_57 etāvānarthasaṅkṣepaḥ prajñāpāramitāśrayaḥ /",
  "S06bsa011_u:118": "āvartya(rta?)",
  "S06bsa011_u:119": "te sa evārthaḥ punararthāntarāśritaḥ //",
  "S06bsa011_u:120": "Pppa_58 prajñāpāramitāṃ samyak saṅgṛhyāṣṭasahasrikām /",
  "S06bsa011_u:121": "yatpuṇyamāptaṃ tenāstu prajñāpāramito janaḥ //",
  "S06bsa011_u:122": "prajñāpāramitāpiṇḍārthasaṅgrahaḥ samāptaḥ /",
  "S06bsa011_u:123": "kṛtirācāryadignāgapādānām //"
}