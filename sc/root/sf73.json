{
    "sf73:0": "",
    "sf73:1": "",
    "sf73:2": "Pravāraṇasūtra",
    "sf73:3": "",
    "sf73:4": "sugata pratibhātu te vāgīśa bhagavān avocat athāyuṣmāṁ vāgīśas tasyāṁ velāyāṁ gāthāṁ babhāṣe ||",
    "sf73:5": "",
    "sf73:6": "iha paṁcadaśī viśuddhikā samitā |",
    "sf73:7": "paṁcaśatāś ca bhikṣavaḥ saṁyojana bandhana |",
    "sf73:8": "cchidaḥ sarve kṣīṇabhavā maharṣayaḥ ||",
    "sf73:9": "śuddhā upāsate śuddhaṁ vipramuktapunarbhavā |",
    "sf73:10": "prahīṇa-jāti-maranāḥ kṛtakṛtyā nirāsravāḥ ||",
    "sf73:11": "audhatyavicikitsecchā mānagranthabhavacchidaḥ",
    "sf73:12": "tṛṣṇā-śalyasya hartāro ’cita-tṛṣṇā-punarbhavāḥ ||",
    "sf73:13": "siṁho ’si nirupādānaṁ prahīṇa-bhaya-bhairava |",
    "sf73:14": "upadhiṁ samatikrāntaḥ āsravā nihatas tvayā ||",
    "sf73:15": "cakravarti yathā rājā sacivaiḥ parivāritaḥ |",
    "sf73:16": "samaṁtād anuśāstīmāṁ sāgarāṁtāṁ vasundharām ||",
    "sf73:17": "tathā vijīta saṁgrāmaṁ sārthavāham anuttaram |",
    "sf73:18": "upāsate śravakāstvā traividyā-mṛtyu-hāyina ||",
    "sf73:19": "putrās te sarva evaite plāvī hy atra na vidyate |",
    "sf73:20": "hartāraṁ sarva śalyānāṁ vande tvāditya-bāndhavam ||",
    "sf73:21": "",
    "sf73:22": "idam avocad bhagavān āptamanasas te bhikṣavo bhagavato bhāṣitam abhyanandaṁ ||",
    "sf73:23": "pravāraṇa sūtraṁ ||"
}