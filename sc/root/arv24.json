    "arv24:3": "Arthaviniścayasūtram",
    "arv24:4": "",
    "arv24:5": "24. Catasraḥ Pratisaṁvidaḥ",
    "arv24:6": "",
    "arv24:7": "Tatra katamās-Tathāgatasya catasraḥ pratisaṁvidaḥ?",
    "arv24:8": "Tad-yathā:",
    "arv24:9": "",
    "arv24:10": "Artha-pratisaṁvit |",
    "arv24:11": "dharma-pratisaṁvit |",
    "arv24:12": "nirukti-pratisaṁvit |",
    "arv24:13": "pratibhāna-pratisaṁvic-ca ||",
    "arv24:14": "",
    "arv24:15": "",
    "arv24:16": "",
    "arv24:17": "Artha-pratisaṁvit katamā?",
    "arv24:18": "Yad-uta, paramārthe yad-avaivartya-jñānam ||",
    "arv24:19": "",
    "arv24:20": "",
    "arv24:21": "Dharma-pratisaṁvit katamā?",
    "arv24:22": "Anāsraveṣu dharmeṣu yad-avaivartya-jñānam ||",
    "arv24:23": "",
    "arv24:24": "",
    "arv24:25": "Nirukti-pratisaṁvit katamā?",
    "arv24:26": "Abhivyāhāre yad-avaivartya-jñānam ||",
    "arv24:27": "",
    "arv24:28": "",
    "arv24:29": "Pratibhāna-pratisaṁvic katamā?",
    "arv24:30": "Yukta-muktam-abhilāpitāyāṁ:",
    "arv24:31": "samādhivaśo saṁprakhyāneṣu yad-avaivartya-jñānam ||",
    "arv24:32": "",
    "arv24:33": "",
    "arv24:34": "Imāś-catasraḥ pratisaṁvidaḥ ||"
}