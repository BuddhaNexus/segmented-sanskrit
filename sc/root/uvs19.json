    "uvs19:3": "Udānavarga de Subaši",
    "uvs19:4": "",
    "uvs19:5": "19. Aśvavarga",
    "uvs19:6": "",
    "uvs19:7": "",
    "uvs19:8": "aśvo yathā bhadra kaśābhispṛṣṭaḥ",
    "uvs19:9": "ātapinaḥ . . . . . . .",
    "uvs19:10": "śraddhāya śīelna ca vīryyavāṁs ta-",
    "uvs19:11": "thā",
    "uvs19:12": "samādhinā dharmma-viniścayena |",
    "uvs19:13": "sampanna-vidyā-caraṇām pratismṛtām",
    "uvs19:14": "prahāsyate sarvva-bhavāni tādṛśāḥ",
    "uvs19:15": ". . . . . . . . . . .",
    "uvs19:16": "ātāpinaḥ saṁvijitaś carantaḥ |",
    "uvs19:17": "śraddhāya sīlena ca vīryyavaṁs tathā",
    "uvs19:18": "samādhinā dharmma-viniścayena ca |",
    "uvs19:19": "te kṣānti-sauratya-samāhitendriyāḥ",
    "uvs19:20": "śrutasya . . . . . . . . .",
    "uvs19:21": ". . . . . samatāṇ gatāni",
    "uvs19:22": "aśvo yathā sārathinā sudāntaḥ |",
    "uvs19:23": "prahīṇa-krodhasya-m-anāsravasya",
    "uvs19:24": "devāpi tasya smṛhayanti tādṛṇaḥ",
    "uvs19:25": ". . . . &#160;&#160; . . . . |",
    "uvs19:26": "appramattaḥ pramatteṣu",
    "uvs19:27": "abalāśvan va bhadraśva",
    "uvs19:28": "hitvā yānti sumedhasaḥ",
    "uvs19:29": "hirī-niṣevī puruṣaḥ",
    "uvs19:30": "prajñavāṁ susamāhitaḥ |",
    "uvs19:31": "sarvva-pāpan apohati",
    "uvs19:32": "bhadraḥ aśva kaśām iva",
    "uvs19:33": "dānto . . &#160;&#160; . . . .",
    "uvs19:34": ". . . . &#160;&#160; . . . . |",
    "uvs19:35": "dantaḥ śreṣṭo manuṣyāṇāṁ",
    "uvs19:36": "yo ’tivākyaṁ titīkṣati",
    "uvs19:37": "yo aśvataraṁ damayed",
    "uvs19:38": "ajāneyan va saindhavaṁ |",
    "uvs19:39": "kuñjaraṁ vā mahā-nāgaṁ",
    "uvs19:40": "atma-dantas tato vara",
    "uvs19:41": "na hi . . &#160;&#160; . . . .",
    "uvs19:42": ". . . . &#160;&#160; . . . . |",
    "uvs19:43": "yathātmanā sudāntena",
    "uvs19:44": "dānto dāntena gacchati",
    "uvs19:45": "atmānam eva damayed",
    "uvs19:46": "āśva bhadram va sārathiḥ |",
    "uvs19:47": "atmā hi ātmanaṁ dāntạh",
    "uvs19:48": "smṛtimaṁ duḥkha-pāragaḥ",
    "uvs19:49": ". . . . &#160;&#160; . . . .",
    "uvs19:50": ". . . . &#160;&#160; . . gatiḥ |",
    "uvs19:51": "tasmāt sanyamayātmānaṁ",
    "uvs19:52": "aśva bhadraṁ va vānija",
    "uvs19:53": "aśva-varga ekūna-vīśati | | * | |",
    "uvs19:54": ""
}