{
    "san-lo-bi-vb-as7:0": "sapta adhikaraṇa-samathā dharmā",
    "san-lo-bi-vb-as7:1": "Seven ways of settling disputes (7)",
    "san-lo-bi-vb-as7:2": "saptādhikaraṇa-samathā dharmā yathā bhikṣūṇāṁ |",
    "san-lo-bi-vb-as7:3": "tṛṇaprastārako 7 ca saptamaḥ ||",
    "san-lo-bi-vb-as7:4": "dharmā anudharmā",
    "san-lo-bi-vb-as7:5": "dharmaś cānudharmaś ca yathā bhikṣūṇāṁ |",
    "san-lo-bi-vb-as7:6": "samāpto bhikṣuṇīnāṁ prātimokṣa-vibhaṅgaḥ ||"
}