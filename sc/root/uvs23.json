    "uvs23:3": "Udānavarga de Subaši",
    "uvs23:4": "",
    "uvs23:5": "23. Ātmavarga",
    "uvs23:6": "",
    "uvs23:7": "",
    "uvs23:8": "subhāṣitasya śikṣeya",
    "uvs23:9": "śramaṇā ’pāsanasya ca |",
    "uvs23:10": "ekāsanasya ca rahaṁś",
    "uvs23:11": "citta-vyupaśmasya ca",
    "uvs23:12": "ekāsanaṁ eka-śayyā",
    "uvs23:13": "eka-cāryyam atandritaḥ",
    "uvs23:14": ". . . . &#160;&#160; . . . .",
    "uvs23:15": ". . . . &#160;&#160; . . vaset",
    "uvs23:16": "yaḥ sahasra sahasrāṇaṁ",
    "uvs23:17": "saṁgrāme mānuṣaṁ jayet |",
    "uvs23:18": "ekañ ca jayam ātmānaṁ",
    "uvs23:19": "sa vai saṁgrāma-jin nataḥ",
    "uvs23:20": "ātma hy asya jita śreyaṁ",
    "uvs23:21": ". . . . &#160;&#160; . . . .",
    "uvs23:22": ". . . . &#160;&#160; . . . .",
    "uvs23:23": "nityaṁ sanyama-cāriṇaḥ",
    "uvs23:24": "na devā nāpi gandharvvā",
    "uvs23:25": "na māraḥ saha brāhmuṇā |",
    "uvs23:26": "jitam avajitaṁ kūryyāt",
    "uvs23:27": "tathā prājñasya bhikṣuṇā",
    "uvs23:28": "ātmānam eva prathamaṁ . . . . &#160;&#160; . . . .",
    "uvs23:29": ". . . . &#160;&#160; . . . .",
    "uvs23:30": "evaṁ bhavatha yathā ahaṁ",
    "uvs23:31": "atmānam eva prathamaṁ",
    "uvs23:32": "pratirūpe niveśayet |",
    "uvs23:33": "tato ’nnyam anuśāsanto",
    "uvs23:34": "na kiliśyeya paṇḍitaḥ",
    "uvs23:35": "atmanañ . &#160;&#160; . . . .",
    "uvs23:36": ". . . . &#160;&#160; . . . .",
    "uvs23:37": ". . . . &#160;&#160; . . . .",
    "uvs23:38": ". . . . &#160;&#160; . . . .",
    "uvs23:39": "atmānañ cet tathā kūryyād",
    "uvs23:40": "yathānnyam anuśāsati",
    "uvs23:41": ". . . . &#160;&#160; . . . .",
    "uvs23:42": ". . . . &#160;&#160; . . . .",
    "uvs23:43": ". . . . &#160;&#160; . . . .",
    "uvs23:44": ". . . . &#160;&#160; . hāpayet |",
    "uvs23:45": "atmārtthaṁ paramaṁ jñātvā",
    "uvs23:46": "svakārttha-paramo bhavet",
    "uvs23:47": "atmā hi ātmano nāthaḥ",
    "uvs23:48": "ko hi nāthaḥ . . . .",
    "uvs23:49": ". . . . &#160;&#160; . . . .",
    "uvs23:50": ". . . . &#160;&#160; . . . .",
    "uvs23:51": ". . labhati paṇḍitaḥ",
    "uvs23:52": ". . labhati paṇḍitaḥ",
    "uvs23:53": "śīlaṁ labhati paṇḍitaḥ",
    "uvs23:54": "sukhaṁ labhati paṇḍitaḥ",
    "uvs23:55": "jñāti-maddhye virocati",
    "uvs23:56": "śoka-maddhye virocati",
    "uvs23:57": "atmā hi ātmano nāthaḥ",
    "uvs23:58": ". . . . &#160;&#160; . . . . |",
    "uvs23:59": "ātmanā hi sudāntenā",
    "uvs23:60": "sarvva-duḥkhāt pramucyate iti",
    "uvs23:61": "ātma-vargga traivīśati samāptaḥ | | * | |",
