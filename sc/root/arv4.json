    "arv4:3": "Arthaviniścayasūtram",
    "arv4:4": "",
    "arv4:5": "4. Dvā-Daśāyatanāni",
    "arv4:6": "",
    "arv4:7": "Tatra bhikṣavaḥ katamāni dvā-daśāyatanāni?",
    "arv4:8": "Tad-yathā:",
    "arv4:9": "",
    "arv4:10": "Cakṣur-ādhyātmikam-āyatanam |",
    "arv4:11": "rūpaṁ bāhyam-āyatanam |",
    "arv4:12": "śrotram-ādhyātmikam-āyatanam |",
    "arv4:13": "śabdo bāhyam-āyatanam |",
    "arv4:14": "ghrāṇām-ādhyātmikam-āyatanam |",
    "arv4:15": "gandho bāhyam-āyatanam |",
    "arv4:16": "jihvā ādhyātmikam-āyatanam |",
    "arv4:17": "raso bāhyam-āyatanam |",
    "arv4:18": "kāya ādhyātmikam-āyatanam |",
    "arv4:19": "sparśo bāhyam-āyatanam |",
    "arv4:20": "mana ādhyātmikam-āyatanam |",
    "arv4:21": "dharmo bāhyam-āyatanam ||",
    "arv4:22": "",
    "arv4:23": "Imāni bhikṣava ucyante dvā-daśāyatanāni ||"
}