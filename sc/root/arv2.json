{
    "arv2:0": "Arthaviniścayasūtram",
    "arv2:1": "2. Pañcopādānaskandhāḥ",
    "arv2:2": "Tatra bhikṣavaḥ katame pañcopādānaskandhāḥ?",
    "arv2:3": "Tad-yathā:",
    "arv2:4": "Rūpopādānaskandhaḥ |",
    "arv2:5": "vedanopādānaskandhaḥ |",
    "arv2:6": "saṁjñopādānaskandhaḥ |",
    "arv2:7": "saṁskāropādānaskandhaḥ |",
    "arv2:8": "vijñānopādānaskandhaḥ ||",
    "arv2:9": "Ime bhikṣavaḥ pañcopādānaskandhāḥ ||"
}