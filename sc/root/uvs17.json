    "uvs17:5": "17. Udakavarga",
    "uvs17:6": "",
    "uvs17:7": "",
    "uvs17:8": ". . . . smṛtīmanto",
    "uvs17:9": "na nikete ramanti te |",
    "uvs17:10": "haṁsa va palvalaṁ hetvā",
    "uvs17:11": "ogham oghaṁ jahaṁti te",
    "uvs17:12": "hansā vāditya-pathe",
    "uvs17:13": "ākāśe. &#160;&#160; . . . .",
    "uvs17:14": ". . . . &#160;&#160; . . . .",
    "uvs17:15": ". . . . &#160;&#160; . . . .",
    "uvs17:16": ". . . . &#160;&#160; . . . .",
    "uvs17:17": "acaritvā brāhma-cāryyaṁm",
    "uvs17:18": "alabdhvā yauvane dhanam |",
    "uvs17:19": "jīrṇṇa-krauṁjaṁ va dhyāyante",
    "uvs17:20": "alpa-matsya va palvare",
    "uvs17:21": "śenti cāpāvakīṇṇā vā",
    "uvs17:22": "paurāṇāni-m-anusmaran",
    "uvs17:23": ". . . . &#160;&#160; . . . .",
    "uvs17:24": ". . . āgamiṣyati |",
    "uvs17:25": "uda-bindu-nipātena",
    "uvs17:26": "mahā-kumbhā va pūryyati |",
    "uvs17:27": "pūryyanti bāla pāpena",
    "uvs17:28": "stoka-stokaṁ pi ācinam",
    "uvs17:29": "nālpa-mannyeta puṇyasya",
    "uvs17:30": "na me taṁ āgamiṣyati |",
    "uvs17:31": "uda-bindu-nipātena",
    "uvs17:32": ". . . . &#160;&#160; . pūryyati |",
    "uvs17:33": "pūryyanti dhīrāḥ puṇyena",
    "uvs17:34": ". . . . &#160;&#160; . ācinām",
    "uvs17:35": "ye taranti ārṇṇavaṁ saraḥ",
    "uvs17:36": ". . kṛtvā visṛjya palvalāni |",
    "uvs17:37": "kolaṁ hi janaḥ prabandhati",
    "uvs17:38": "tīrṇṇā medhāvino . .",
    "uvs17:39": ". . . . &#160;&#160; . . . .",
    "uvs17:40": ". . . . &#160;&#160; . . . .",
    "uvs17:41": ". . . . &#160;&#160; . . . .",
    "uvs17:42": ". . bandhanti śrāvakaḥ",
    "uvs17:43": "kiṁ kūryyād uda-pānena",
    "uvs17:44": "āpaś ca sarvvato bhavet |",
    "uvs17:45": "tṛṣṇya mūlaṁ cchetveha",
    "uvs17:46": "kasya . . &#160;&#160; . . . .",
    "uvs17:47": ". . . . . . . . . .",
    "uvs17:48": ". . . . . . . . tejasā |",
    "uvs17:49": "dārun namayanti tacchakā",
    "uvs17:50": "atmānaṁ namayanti paṇḍitāḥ",
    "uvs17:51": "hrado yathā . . . .",
    "uvs17:52": ". . . . &#160;&#160; . . . .",
    "uvs17:53": ". . . . &#160;&#160; . . . .",
    "uvs17:54": ". . . . &#160;&#160; . . . .",
    "uvs17:55": ". . . . . . lipyate",
    "uvs17:56": "indrakīlopama tādṛ sūratā |",
    "uvs17:57": "hrada iva apeta-karddamāḥ",
    "uvs17:58": "saṁsārā . . . . . . .",
