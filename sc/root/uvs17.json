    "uvs17:9": ". . . . &#160;&#160; . . . .",
    "uvs17:10": ". . . . &#160;&#160; . . . .",
    "uvs17:11": "acaritvā brāhma-cāryyaṁm",
    "uvs17:12": "alabdhvā yauvane dhanam |",
    "uvs17:13": "jīrṇṇa-krauṁjaṁ va dhyāyante",
    "uvs17:14": "alpa-matsya va palvare",
    "uvs17:15": "śenti cāpāvakīṇṇā vā",
    "uvs17:16": "paurāṇāni-m-anusmaran",
    "uvs17:17": ". . . . &#160;&#160; . . . .",
    "uvs17:18": ". . . āgamiṣyati |",
    "uvs17:19": "uda-bindu-nipātena",
    "uvs17:20": "mahā-kumbhā va pūryyati |",
    "uvs17:21": "pūryyanti bāla pāpena",
    "uvs17:22": "stoka-stokaṁ pi ācinam",
    "uvs17:23": "nālpa-mannyeta puṇyasya",
    "uvs17:24": "na me taṁ āgamiṣyati |",
    "uvs17:25": "uda-bindu-nipātena",
    "uvs17:26": ". . . . &#160;&#160; . pūryyati |",
    "uvs17:27": "pūryyanti dhīrāḥ puṇyena",
    "uvs17:28": ". . . . &#160;&#160; . ācinām",
    "uvs17:29": "ye taranti ārṇṇavaṁ saraḥ",
    "uvs17:30": ". . kṛtvā visṛjya palvalāni |",
    "uvs17:31": "kolaṁ hi janaḥ prabandhati",
    "uvs17:32": "tīrṇṇā medhāvino . .",
    "uvs17:33": ". . . . &#160;&#160; . . . .",
    "uvs17:34": ". . . . &#160;&#160; . . . .",
    "uvs17:35": ". . . . &#160;&#160; . . . .",
    "uvs17:36": ". . bandhanti śrāvakaḥ",
    "uvs17:37": "kiṁ kūryyād uda-pānena",
    "uvs17:38": "āpaś ca sarvvato bhavet |",
    "uvs17:39": "tṛṣṇya mūlaṁ cchetveha",
    "uvs17:40": "kasya . . &#160;&#160; . . . .",
    "uvs17:41": ". . . . . . . . . .",
    "uvs17:42": ". . . . . . . . tejasā |",
    "uvs17:43": "dārun namayanti tacchakā",
    "uvs17:44": "atmānaṁ namayanti paṇḍitāḥ",
    "uvs17:45": "hrado yathā . . . .",
    "uvs17:46": ". . . . &#160;&#160; . . . .",
    "uvs17:47": ". . . . &#160;&#160; . . . .",
    "uvs17:48": ". . . . &#160;&#160; . . . .",
    "uvs17:49": ". . . . . . lipyate",
    "uvs17:50": "indrakīlopama tādṛ sūratā |",
    "uvs17:51": "hrada iva apeta-karddamāḥ",
