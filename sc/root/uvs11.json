{
    "uvs11:0": "Udānavarga de Subaši",
    "uvs11:1": "11. Śramaṇavarga",
    "uvs11:2": "cchindhi srotaḥ parākkrāṁmya",
    "uvs11:3": "kāmāṁ praṇuda brāhmaṇa ; |",
    "uvs11:4": "na ’pprahāya muniḥ kāmān",
    "uvs11:5": "ekatvam adhigacchati",
    "uvs11:6": "kareyā nāṁ kareyā nāṁ",
    "uvs11:7": "dṛḍham eva parakkramet |",
    "uvs11:8": "śithilo hi parivrājo",
    "uvs11:9": ". . . . &#160;&#160; . . . .",
    "uvs11:10": ". kiñ cic chitilāṁ karmmaṁ",
    "uvs11:11": "saṁkiliṣṭā va yas tapaḥ |",
    "uvs11:12": "apariśuddhaṁ brāhma-caryyaṁ",
    "uvs11:13": "na tad bhavati sukhāvaha",
    "uvs11:14": "śaro yathā durgṛhīto",
    "uvs11:15": "hastam . . &#160;&#160; . . . .",
    "uvs11:16": ". . . duṣparāmṛṣṭaṁ",
    "uvs11:17": "narakām upakarṣati",
    "uvs11:18": "śaro yathā sugṛhīto",
    "uvs11:19": "na hastam evavakarttati |",
    "uvs11:20": "śrāmaṁṇyam suparāmṛṣṭaṁ",
    "uvs11:21": "nirvvāṇasyaiva santike",
    "uvs11:22": "duṣkaraṁ dustīkṣaṁ ca",
    "uvs11:23": ". . . . &#160;&#160; . . śrāmaṇyam |",
    "uvs11:24": "bahavas tatra saṁbādhā",
    "uvs11:25": "yatra mando viṣīdati",
    "uvs11:26": "kathañ careya śrāmaṇye",
    "uvs11:27": "cittañ ca na nivārayet |",
    "uvs11:28": "pade pade viṣīdantaḥ",
    "uvs11:29": "saṁkalpānaṁ vaśaṁ gatāḥ",
    "uvs11:30": "duṣpravrajyaṁ durabhiramaṁ",
    "uvs11:31": "duradhyāvasanā gharāḥ |",
    "uvs11:32": "duḥkhaṁ samāna-saṁvāso",
    "uvs11:33": "duḥkhānupatitā bhavaḥ",
    "uvs11:34": "kāṣāya-kaṇṭha bahavaḥ",
    "uvs11:35": "pāpa-dharmma asaṁyataḥ |",
    "uvs11:36": "pāpaḥ pāpehi karmmehi",
    "uvs11:37": "ito gacchati durggatim",
    "uvs11:38": ". . atyanta-duḥśīlaḥ",
    "uvs11:39": "sālaṁ vā mālutātataṁ |",
    "uvs11:40": "karoti sa tathātmānaṁ",
    "uvs11:41": "yathainaṁ dviṣa-d-icchati",
    "uvs11:42": "sthero na tāvatā bhavati",
    "uvs11:43": "yāvatā palitaṁ śiraḥ |",
    "uvs11:44": "paripakvaṁ vayas . .",
    "uvs11:45": ". . . . &#160;&#160;ti ucyate",
    "uvs11:46": "yas tu puṇyañ ca pāpañ ca",
    "uvs11:47": "vāhetvā brāhma-cāryyavān |",
    "uvs11:48": "viṣeṇi-kṛtvā caratī",
    "uvs11:49": "sa vai sthero ti ucyate",
    "uvs11:50": "na munda- . &#160;&#160; . . . .",
    "uvs11:51": ". . . . &#160;&#160; . . . .",
    "uvs11:52": ". . . . &#160;&#160; . . . .",
    "uvs11:53": ". . . . &#160;&#160; . . . .",
    "uvs11:54": "[Stance 14 perdue]",
    "uvs11:55": ". . . . &#160;&#160; . . . .",
    "uvs11:56": ". . -cārī śramaṇo nirucyate |",
    "uvs11:57": "pravāhiya ātmano malaṁ",
