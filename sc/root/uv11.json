{
    "uv11:0": "Udānavarga",
    "uv11:1": "11. Śramaṇavarga",
    "uv11:2": "− − − − । ⏑ − − − ।। − − ⏑ ⏑ । ⏑ − ⏑ −",
    "uv11:3": "chindhi srotaḥ parākramya kāmāṁ praṇuda sarvaśaḥ ।",
    "uv11:4": "− ⏑ − ⏑ । ⏑ − − − ।। − − ⏑ ⏑ । ⏑ − ⏑ −",
    "uv11:5": "nāprahāya muniḥ kāmān ekatvam adhigacchati ।।",
    "uv11:6": "− − − ⏑ । ⏑ − − − ।। ⏑ ⏑ − ⏑ । ⏑ − ⏑ −",
    "uv11:7": "kurvāṇo hi sadā prājño dṛḍham eva parākramet ।",
    "uv11:8": "⏑ ⏑ − ⏑ । ⏑ − − − ।। − ⏑ − ⏑ । ⏑ − ⏑ −",
    "uv11:9": "śithilā khalu pravrajyā hy ādadāti puno rajaḥ ।।",
    "uv11:10": "− − − ⏑ । ⏑ − − − ।। − − − − । ⏑ − ⏑ −",
    "uv11:11": "yat kiṁ cic chitilaṁ karma saṁkliṣṭaṁ vāpi yat tapaḥ ।",
    "uv11:12": "⏑ ⏑ ⏑ − − । − ⏑ − − ।। ⏑ − − ⏑ । ⏑ − ⏑ − ravipulā",
    "uv11:13": "apariśuddhaṁ brahmacaryaṁ na tad bhavati mahāphalam ।।",
    "uv11:14": "⏑ − ⏑ − । − ⏑ − − ।। − ⏑ − − । ⏑ − ⏑ − ravipulā",
    "uv11:15": "śaro yathā durgṛhīto hastam evāpakṛntati ।",
    "uv11:16": "− − − − । ⏑ − − − ।। ⏑ ⏑ − ⏑ । ⏑ − ⏑ −",
    "uv11:17": "śrāmaṇyaṁ duṣparāmṛṣṭaṁ narakān upakarṣati ।।",
    "uv11:18": "⏑ − ⏑ − । ⏑ ⏑ − − ।। ⏑ − ⏑ ⏑ । ⏑ − ⏑ − savipulā",
    "uv11:19": "śaro yathā sugṛhīto na hastam apakṛntati ।",
    "uv11:20": "− − − ⏑ । ⏑ − − − ।। − − − − । ⏑ − ⏑ −",
    "uv11:21": "śrāmaṇyaṁ suparāmṛṣṭaṁ nirvāṇasyaiva so ’ntike ।।",
    "uv11:22": "− ⏑ − − । ⏑ − − − ।। − − − − । ⏑ − ⏑ −",
    "uv11:23": "duṣkaraṁ dustitīkṣaṁ ca śrāmaṇyaṁ mandabuddhinā ।",
    "uv11:24": "⏑ ⏑ − − । ⏑ − − − ।। − ⏑ − − । ⏑ − ⏑ −",
    "uv11:25": "bahavas tatra saṁbādhā yatra mando viḍīdati ।।",
    "uv11:26": "− − − ⏑ । ⏑ − − − ।। ⏑ − ⏑ ⏑ । ⏑ − ⏑ −",
    "uv11:27": "śrāmaṇye carate yas tu svacittam anivārayet ।",
    "uv11:28": "⏑ − ⏑ − । ⏑ − − − ।। − − − − । ⏑ − ⏑ −",
    "uv11:29": "punaḥ punar viṣīdet ca saṁkalpānāṁ vaśaṁ gataḥ ।।",
    "uv11:30": "− − − − । ⏑ ⏑ ⏑ ⏑ − ।। ⏑ − − ⏑ । ⏑ − ⏑ − bhavipulā",
    "uv11:31": "duṣpravrajyaṁ durabhiramaṁ duradhyāvasitā gṛhāḥ ।",
    "uv11:32": "− − ⏑ − । ⏑ − − − ।। − − − ⏑ । ⏑ − ⏑ −",
    "uv11:33": "duḥkhāsamānasaṁvāsā duḥkāś copacitā bhavāḥ ।।",
    "uv11:34": "− − ⏑ − । − ⏑ ⏑ − ।। − ⏑ − − । ⏑ − ⏑ − bhavipulā",
    "uv11:35": "kāṣāyakaṇṭhā bahavaḥ pāpadharmā hy asaṁyatāḥ ।",
    "uv11:36": "− − ⏑ − । ⏑ − − − ।। ⏑ − − − । ⏑ − ⏑ −",
    "uv11:37": "pāpā hi karmabhiḥ pāpair ito gacchanti durgatim ।।",
    "uv11:38": "− − − − । ⏑ − − − ।। − ⏑ − − । ⏑ − ⏑ −",
    "uv11:39": "yo ’sāv atyantaduḥśīlaḥ sālavāṁ mālutā yathā ।",
    "uv11:40": "⏑ − ⏑ − । ⏑ − − − ।। ⏑ − − ⏑ । ⏑ − ⏑ −",
    "uv11:41": "karoty asau tathāṭmānaṁ yathainaṁ dviṣa-d-icchati ।।",
    "uv11:42": "⏑ ⏑ − ⏑ − । ⏑ − − − ।। − ⏑ − ⏑ । ⏑ − ⏑ − pathyā",
    "uv11:43": "sthaviro na tāvatā bhavati yāvatā palitaṁ śiraḥ ।",
    "uv11:44": "⏑ ⏑ − − । ⏑ − − − ।। − ⏑ − − । ⏑ − ⏑ −",
    "uv11:45": "paripakvaṁ vayas tasya mohajīrṇaḥ sa ucyate ।।",
    "uv11:46": "− ⏑ − − । ⏑ − − − ।। ⏑ − ⏑ ⏑ । ⏑ − ⏑ −",
    "uv11:47": "yas tu puṇyaṁ ca pāpaṁ ca prahāya brahmacaryavān ।",
    "uv11:48": "⏑ − ⏑ − । − ⏑ ⏑ − ।। ⏑ − ⏑ ⏑ । ⏑ − ⏑ − bhavipulā",
    "uv11:49": "viśreṇayitvā carati sa vai sthavira ucyate ।।",
    "uv11:50": "⏑ − ⏑ − । − ⏑ ⏑ − ।। ⏑ ⏑ − ⏑ । ⏑ − ⏑ − bhavipulā",
    "uv11:51": "na muṇḍabhāvāc chramaṇo hy avṛtas tv anṛtaṁ vadan ।",
    "uv11:52": "− − − ⏑ । ⏑ − − − ।। ⏑ ⏑ − − । ⏑ − ⏑ −",
    "uv11:53": "icchālobhasamāpannaḥ śramaṇaḥ kiṁ bhaviṣyati ।।",
    "uv11:54": "⏑ − ⏑ − । − ⏑ ⏑ − ।। ⏑ ⏑ − ⏑ । ⏑ − ⏑ − bhavipulā",
    "uv11:55": "na muṇḍabhāvāc chramaṇo hy avṛtas tv anṛtaṁ vadan ।",
    "uv11:56": "⏑ ⏑ − − । ⏑ − − − ।। ⏑ − − − । ⏑ − ⏑ −",
    "uv11:57": "śamitaṁ yena pāpaṁ syād aṇusthūlaṁ hi sarvaśaḥ ।",
    "uv11:58": "⏑ ⏑ − − । ⏑ − − − ।। ⏑ ⏑ − ⏑ । ⏑ − ⏑ −",
    "uv11:59": "śamitatvāt tu pāpānāṁ śramaṇo hi nirucyate ।।",
    "uv11:60": "− ⏑ − − । ⏑ − − − ।। ⏑ ⏑ − ⏑ । ⏑ − ⏑ −",
    "uv11:61": "brāhmaṇo vāhitaiḥ pāpaiḥ śramaṇaḥ śamitāśubhaḥ ।",
    "uv11:62": "− − ⏑ − । − ⏑ ⏑ − ।। − − − ⏑ । ⏑ − ⏑ − bhavipulā",
    "uv11:63": "pravrājayitvā tu malān uktaḥ pravrajitas tv iha ।।",
    "uv11:64": "।। śramaṇavargaḥ 11 ।। ।।"
}