{
    "SA14knavslou:0_0": "Navaślokī",
    "SA14knavslou:1_0": "āryāṣṭasāhaśrikāyāḥ prajñāpāramitāyāḥ piṇḍārthaḥ",
    "SA14knavslou:ns_1_0": "prajñāpāramitāmbodhau śubharatnākare svayam /",
    "SA14knavslou:ns_1_1": "sarvā pāramitās tatra tādātmyena vyavasthitāḥ //",
    "SA14knavslou:ns_2_0": "niṣprapañcā nirābhāsā nirvikalpā nirālayā /",
    "SA14knavslou:ns_2_1": "niḥsvabhāvā parā sūkṣmā bindunādavivarjitā //",
    "SA14knavslou:ns_3_0": "prajñāpāramitā mātā sarvabuddhodayā parā /",
    "SA14knavslou:ns_3_1": "trayānupalabdhirūpā sarvajñajñānagocarā //",
    "SA14knavslou:ns_4_0": "prajñāpāramitāṃ saṃyag yo bhāvayitum icchati /",
    "SA14knavslou:ns_4_1": "tenārthato navaślokāś cintanīyāḥ samāsataḥ //",
    "SA14knavslou:ns_5_[1]_0": "karmaprabhāvasaṃbhūtaṃ ṣaḍāyatanalakṣaṇam /",
    "SA14knavslou:ns_5_[1]_1": "punarbhavam iti khyātaṃ pratibhāsopamaṃ hi tat //",
    "SA14knavslou:ns_6_[2]_0": "nirmitaṃ nagaraṃ yadvad vilokayati nirmitaḥ /",
    "SA14knavslou:ns_6_[2]_1": "tadvat paśyati rūpāṇi karmabhir nirmitaṃ jagat //",
    "SA14knavslou:ns_7_[3]_0": "dharmaṃ deśayataḥ śabdā ye kecit śrutigocarāḥ /",
    "SA14knavslou:ns_7_[3]_1": "pratiśrutkopamāḥ sarve prodbhūtaśrutinaḥ śrutāḥ //",
    "SA14knavslou:ns_8_[4]_0": "āghrataṃ svāditaṃ tathā spraṣṭaṃ viṣayalālāsaiḥ /",
    "SA14knavslou:ns_8_[4]_1": "svapnatulyam idaṃ sarvam upalabdhaṃ na vidyate //",
    "SA14knavslou:ns_9_[5]_0": "māyāyantro naro yadvad bhinnāṃ ceṣṭāṃ karoti vai /",
    "SA14knavslou:ns_9_[5]_1": "tadvad ceṣṭāṃ karoty eva dehayantro nirātmakaḥ //",
    "SA14knavslou:ns_10_[6]_0": "nānopalabdhayo yāś ca pratikṣaṇasamudbhavāḥ /",
    "SA14knavslou:ns_10_[6]_1": "marīcisadṛśāś caite dṛṣṭanaṣṭāḥ vilakṣaṇāḥ //",
    "SA14knavslou:ns_11_[7]_0": "pratibimbanibhaṃ grāhyam anādicittasaṃbhavam /",
    "SA14knavslou:ns_11_[7]_1": "tadākāraṃ ca vijñānam anyonyapratibimbavat //",
    "SA14knavslou:ns_12_[8]_0": "dhyāyināṃ svacchasaṃtāne yaj jnanendusamudbhavam /",
    "SA14knavslou:ns_12_[8]_1": "udakacandropamaṃ tad dhi pratyakṣaṃ na vidyate //",
    "SA14knavslou:ns_13_[9]_0": "yoginām api yaj jñānaṃ tad apy ākāśalakṣaṇam /",
    "SA14knavslou:ns_13_[9]_1": "tasmāj jñānaṃ ca jñeyaṃ ca sarvam ākāśalakṣaṇam //",
    "SA14knavslou:ns_14_0": "iti cintayataḥ tattvaṃ sarvabhāveṣv anāśritam /",
    "SA14knavslou:ns_14_1": "bodhipraṇidhicittena jñānaṃ agraṃ bhaviṣyati //",
    "SA14knavslou:2_0": "āryāṣṭasāhasrikāyāḥ prajñāpāramitāyāḥ piṇḍārthaḥ samāptaḥ",
    "SA14knavslou:2_1": "kṛtir iyaṃ śrīkambalāmbarapādanām"
}