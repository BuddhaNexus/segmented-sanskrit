{
    "SA14knavslou:0_0": "Navaślokī",
    "SA14knavslou:1_0": "āryāṣṭasāhaśrikāyāḥ prajñāpāramitāyāḥ piṇḍārthaḥ",
    "SA14knavslou:ns_1_0": "prajñāpāramitāmbodhau śubharatnākare svayam /",
    "SA14knavslou:ns_1_1": "sarvā pāramitās tatra tādātmyena vyavasthitāḥ //",
    "SA14knavslou:ns_2_0": "niṣprapañcā nirābhāsā nirvikalpā nirālayā /",
    "SA14knavslou:ns_2_1": "niḥsvabhāvā parā sūkṣmā bindunādavivarjitā //",
    "SA14knavslou:ns_3_0": "prajñāpāramitā mātā sarvabuddhodayā parā /",
    "SA14knavslou:ns_3_1": "trayānupalabdhirūpā sarvajñajñānagocarā //",
    "SA14knavslou:ns_4_0": "prajñāpāramitāṃ saṃyag yo bhāvayitum icchati /",
    "SA14knavslou:ns_4_1": "tenārthato navaślokāś cintanīyāḥ samāsataḥ //",
    "SA14knavslou:ns_5_1_0": "karmaprabhāvasaṃbhūtaṃ ṣaḍāyatanalakṣaṇam /",
    "SA14knavslou:ns_5_1_1": "punarbhavam iti khyātaṃ pratibhāsopamaṃ hi tat //",
    "SA14knavslou:ns_6_2_0": "nirmitaṃ nagaraṃ yadvad vilokayati nirmitaḥ /",
    "SA14knavslou:ns_6_2_1": "tadvat paśyati rūpāṇi karmabhir nirmitaṃ jagat //",
    "SA14knavslou:ns_7_3_0": "dharmaṃ deśayataḥ śabdā ye kecit śrutigocarāḥ /",
    "SA14knavslou:ns_7_3_1": "pratiśrutkopamāḥ sarve prodbhūtaśrutinaḥ śrutāḥ //",
    "SA14knavslou:ns_8_4_0": "āghrataṃ svāditaṃ tathā spraṣṭaṃ viṣayalālāsaiḥ /",
    "SA14knavslou:ns_8_4_1": "svapnatulyam idaṃ sarvam upalabdhaṃ na vidyate //",
    "SA14knavslou:ns_9_5_0": "māyāyantro naro yadvad bhinnāṃ ceṣṭāṃ karoti vai /",
    "SA14knavslou:ns_9_5_1": "tadvad ceṣṭāṃ karoty eva dehayantro nirātmakaḥ //",
    "SA14knavslou:ns_10_6_0": "nānopalabdhayo yāś ca pratikṣaṇasamudbhavāḥ /",
    "SA14knavslou:ns_10_6_1": "marīcisadṛśāś caite dṛṣṭanaṣṭāḥ vilakṣaṇāḥ //",
    "SA14knavslou:ns_11_7_0": "pratibimbanibhaṃ grāhyam anādicittasaṃbhavam /",
    "SA14knavslou:ns_11_7_1": "tadākāraṃ ca vijñānam anyonyapratibimbavat //",
    "SA14knavslou:ns_12_8_0": "dhyāyināṃ svacchasaṃtāne yaj jnanendusamudbhavam /",
    "SA14knavslou:ns_12_8_1": "udakacandropamaṃ tad dhi pratyakṣaṃ na vidyate //",
    "SA14knavslou:ns_13_9_0": "yoginām api yaj jñānaṃ tad apy ākāśalakṣaṇam /",
    "SA14knavslou:ns_13_9_1": "tasmāj jñānaṃ ca jñeyaṃ ca sarvam ākāśalakṣaṇam //",
    "SA14knavslou:ns_14_0": "iti cintayataḥ tattvaṃ sarvabhāveṣv anāśritam /",
    "SA14knavslou:ns_14_1": "bodhipraṇidhicittena jñānaṃ agraṃ bhaviṣyati //",
    "SA14knavslou:2_0": "āryāṣṭasāhasrikāyāḥ prajñāpāramitāyāḥ piṇḍārthaḥ samāptaḥ",
    "SA14knavslou:2_1": "kṛtir iyaṃ śrīkambalāmbarapādanām"
}