{
  "SA20bsa019_u:0_0": "// namo mañjuśriye kumārabhūtāya //",
  "SA20bsa019_u:1_0": "ācāryavasubandhupraṇītaḥ",
  "SA20bsa019_u:2_0": "Trisvabhāvanirdeśaḥ (Tsn)",
  "SA20bsa019_u:tsn_1_0": "kalpitaḥ paratantraśca pariniṣpanna eva ca /",
  "SA20bsa019_u:tsn_1_1": "trayaḥ svabhāvā dhīrāṇāṃ gambhīrajñeyamiṣyate //",
  "SA20bsa019_u:tsn_2_0": "yat khyāti paratantro 'sau yathā khyāti sa kalpitaḥ /",
  "SA20bsa019_u:tsn_2_1": "pratyayādhīnavṛttitvāt kalpanāmātrabhāvataḥ //",
  "SA20bsa019_u:tsn_3_0": "tasya khyāturyathākhyānaṃ yā sadāvidyamānatā /",
  "SA20bsa019_u:tsn_3_1": "jñeyaḥ sa pariniṣpannasvabhāvo 'nanyathātvataḥ //",
  "SA20bsa019_u:tsn_4_0": "tatra kiṃ khyātyasatkalpaḥ kathaṃ khyāti dvayātmanā /",
  "SA20bsa019_u:tsn_4_1": "tasya kā nāstitā tena yā tatrādvayadharmatā //",
  "SA20bsa019_u:tsn_5_0": "asatkalpo 'tra kaścittaṃ yatastena hi kalpyate /",
  "SA20bsa019_u:tsn_5_1": "yathā ca kalpayatyartha tathātyantaṃ na vidyate //",
  "SA20bsa019_u:tsn_6_0": "taddhetuphalabhāvena cittaṃ dvividhamiṣyate /",
  "SA20bsa019_u:tsn_6_1": "yadālayākhyaṃ vijñānaṃ pravṛttyākhyaṃ ca saptadhā //",
  "SA20bsa019_u:tsn_7_0": "saṃkleśavāsanābījaiścitatvāccittamucyate /",
  "SA20bsa019_u:tsn_7_1": "cittamādyaṃ dvitīyaṃ tu citrākārapravṛttitaḥ //",
  "SA20bsa019_u:tsn_8_0": "samāsato 'bhūtakalpaḥ sa caiṣa trividho mataḥ /",
  "SA20bsa019_u:tsn_8_1": "vaipākikastathā naimittiko 'nyaḥ prātibhāsikaḥ //",
  "SA20bsa019_u:tsn_9_0": "prathamo mūlavijñānaṃ tadvipākātmakaṃ yataḥ /",
  "SA20bsa019_u:tsn_9_1": "anyaḥ pravṛttivijñānaṃ dṛśyadṛgvittivṛttitaḥ //",
  "SA20bsa019_u:tsn_10_0": "sadasattvād dvayaikatvāt saṃkleśavyavadānayoḥ /",
  "SA20bsa019_u:tsn_10_1": "lakṣaṇābhedataśceṣṭā svabhāvānāṃ gaṃbhīratā //",
  "SA20bsa019_u:tsn_11_0": "sattvena gṛhyate yasmādatyantābhāva eva ca /",
  "SA20bsa019_u:tsn_11_1": "svabhāvaḥ kalpitastena sadasallakṣaṇo mataḥ //",
  "SA20bsa019_u:tsn_12_0": "vidyate bhrāntibhāvena yathākhyānaṃ na vidyate /",
  "SA20bsa019_u:tsn_12_1": "paratantro yatastena sadasallakṣaṇo mataḥ //",
  "SA20bsa019_u:tsn_13_0": "advayatvena yaccāsti dvayasyābhāva eva ca /",
  "SA20bsa019_u:tsn_13_1": "svabhāvastena niṣpannaḥ sadasallakṣaṇo mataḥ //",
  "SA20bsa019_u:tsn_14_0": "dvaividhyāt kalpitārthasya tadasattvaikabhāvataḥ /",
  "SA20bsa019_u:tsn_14_1": "svabhāvaḥ kalpito bālairdvayaikatvātmako mataḥ //",
  "SA20bsa019_u:tsn_15_0": "prakhyānād dvayabhāvena bhrāntimātraikabhāvataḥ /",
  "SA20bsa019_u:tsn_15_1": "svabhāvaḥ paratantrākhyo dvayaikatvātmako mataḥ //",
  "SA20bsa019_u:tsn_16_0": "dvayābhāvasvabhāvatvādadvayaikasvabhāvataḥ /",
  "SA20bsa019_u:tsn_16_1": "svabhāvaḥ pariniṣpanno dvayaikatvātmako mataḥ //",
  "SA20bsa019_u:tsn_17_0": "kalpitaḥ paratantraśca jñeyaṃ saṃkleśalakṣaṇam /",
  "SA20bsa019_u:tsn_17_1": "pariniṣpanna iṣṭastu vyavadānasya lakṣaṇam //",
  "SA20bsa019_u:tsn_18_0": "asaddvayasvabhāvatvāt tadabhāvasvabhāvataḥ /",
  "SA20bsa019_u:tsn_18_1": "svabhāvāt kalpitājjñeyo niṣpanno 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:tsn_19_0": "advayatvasvabhāvatvād dvayābhāvasvabhāvataḥ /",
  "SA20bsa019_u:tsn_19_1": "niṣpannāt kalpitaścaiva vijñeyo 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:tsn_20_0": "yathākhyānamasadbhāvāt tathāsattvasvabhāvataḥ /",
  "SA20bsa019_u:tsn_20_1": "svabhāvāt paratantrākhyānniṣpanno 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:tsn_21_0": "asaddvayasvabhāvatvād yathākhyānāsvabhāvataḥ /",
  "SA20bsa019_u:tsn_21_1": "niṣpannāt paratantro 'pi vijñeyo 'bhinnalakṣaṇaḥ //",
  "SA20bsa019_u:tsn_22_0": "kramabhedaḥ svabhāvānāṃ vyavahārādhikārataḥ /",
  "SA20bsa019_u:tsn_22_1": "tatpraveśādhikārācca vyutpattyarthaṃ vidhīyate //",
  "SA20bsa019_u:tsn_23_0": "kalpito vyavahārātmā vyavahartrātmako 'paraḥ /",
  "SA20bsa019_u:tsn_23_1": "vyavahārasamucchedasvabhāvaścānya iṣyate //",
  "SA20bsa019_u:tsn_24_0": "dvayābhāvātmakaḥ pūrvaṃ paratantraḥ praviśyate /",
  "SA20bsa019_u:tsn_24_1": "tataḥ praviśyate tatra kalpamātramasaddvayam //",
  "SA20bsa019_u:tsn_25_0": "tato dvayābhāvabhāvo niṣpanno 'tra praviśyate /",
  "SA20bsa019_u:tsn_25_1": "tathā hyasāveva tadā astināstīti cocyate //",
  "SA20bsa019_u:tsn_26_0": "trayo 'pyete svabhāvā hi advayālambalakṣaṇāḥ /",
  "SA20bsa019_u:tsn_26_1": "abhāvādatathābhāvāt tadabhāvasvabhāvataḥ //",
  "SA20bsa019_u:tsn_27_0": "māyākṛtaṃ mantravaśāt khyāti hastyātmanā yathā /",
  "SA20bsa019_u:tsn_27_1": "ākāramātraṃ tatrāsti hastī nāsti tu sarvathā //",
  "SA20bsa019_u:tsn_28_0": "svabhāvaḥ kalpito hastī paratantrastadākṛtiḥ /",
  "SA20bsa019_u:tsn_28_1": "yastatra hastyabhāvo 'sau pariniṣpanna iṣyate //",
  "SA20bsa019_u:tsn_29_0": "asatkalpastathā khyāti mūlacittād dvayātmanā /",
  "SA20bsa019_u:tsn_29_1": "dvayamatyantato nāsti tatrāstyākṛtimātrakam //",
  "SA20bsa019_u:tsn_30_0": "mantravanmūlavijñānaṃ kāṣṭhavattathatā matā /",
  "SA20bsa019_u:tsn_30_1": "hastyākāravadeṣṭavyo vikalpo hastivad dvayam //",
  "SA20bsa019_u:tsn_31_0": "arthatattvaprativedhe yugapallakṣaṇatrayam /",
  "SA20bsa019_u:tsn_31_1": "parijñā ca prahāṇaṃ ca prāptiśceṣṭā yathākramam //",
  "SA20bsa019_u:tsn_32_0": "parijñānupalambho 'tra hānirakhyānamiṣyate /",
  "SA20bsa019_u:tsn_32_1": "upalambhanimittā tu prāptiḥ sākṣātkriyāpi sā //",
  "SA20bsa019_u:tsn_33_0": "dvayasyānupalambhena dvayākāro vigacchati /",
  "SA20bsa019_u:tsn_33_1": "vigamāt tasya niṣpanno dvayābhāvo 'dhigamyate //",
  "SA20bsa019_u:tsn_34_0": "hastino 'nupalambhaśca vigamaśca tadākṛteḥ /",
  "SA20bsa019_u:tsn_34_1": "upalambhaśca kāṣṭhasya māyāyāṃ yugapad yathā //",
  "SA20bsa019_u:tsn_35_0": "viruddhadhīvāraṇatvād buddhyā vaiyarthyadarśanāt /",
  "SA20bsa019_u:tsn_35_1": "jñānatrayānuvṛtteśca mokṣāpattirayatnataḥ //",
  "SA20bsa019_u:tsn_36_0": "cittamātropalambhena jñeyārthānupalambhatā /",
  "SA20bsa019_u:tsn_36_1": "jñeyārthānupalambhena syāccittānupalambhatā //",
  "SA20bsa019_u:tsn_37_0": "dvayoranupalambhena dharmadhātūpalabhbhatā /",
  "SA20bsa019_u:tsn_37_1": "dharmadhātūpalambhena syād vibhutvopalambhatā //",
  "SA20bsa019_u:tsn_38_0": "upalabdhavibhutvaśca svaparārthaprasiddhitaḥ /",
  "SA20bsa019_u:tsn_38_1": "prāpnotyanuttarāṃ bodhiṃ dhīmān kāyatrayātmikām //",
  "SA20bsa019_u:3_0": "iti trisvabhāvanirdeśaḥ samāptaḥ //",
  "SA20bsa019_u:3_1": "kṛtirācāryavasubandhupādānāmiti //"
}