{
  "SA20bsa019_u:0": "// namo mañjuśriye kumārabhūtāya //",
  "SA20bsa019_u:1": "ācāryavasubandhupraṇītaḥ",
  "SA20bsa019_u:2": "Trisvabhāvanirdeśaḥ (Tsn)",
  "SA20bsa019_u:3": "kalpitaḥ paratantraśca pariniṣpanna eva ca /",
  "SA20bsa019_u:4": "trayaḥ svabhāvā dhīrāṇāṃ gambhīrajñeyamiṣyate // Tsn_1 //",
  "SA20bsa019_u:5": "yat khyāti paratantro 'sau yathā khyāti sa kalpitaḥ /",
  "SA20bsa019_u:6": "pratyayādhīnavṛttitvāt kalpanāmātrabhāvataḥ // Tsn_2 //",
  "SA20bsa019_u:7": "tasya khyāturyathākhyānaṃ yā sadāvidyamānatā /",
  "SA20bsa019_u:8": "jñeyaḥ sa pariniṣpannasvabhāvo 'nanyathātvataḥ // Tsn_3 //",
  "SA20bsa019_u:9": "tatra kiṃ khyātyasatkalpaḥ kathaṃ khyāti dvayātmanā /",
  "SA20bsa019_u:10": "tasya kā nāstitā tena yā tatrādvayadharmatā // Tsn_4 //",
  "SA20bsa019_u:11": "asatkalpo 'tra kaścittaṃ yatastena hi kalpyate /",
  "SA20bsa019_u:12": "yathā ca kalpayatyartha tathātyantaṃ na vidyate // Tsn_5 //",
  "SA20bsa019_u:13": "taddhetuphalabhāvena cittaṃ dvividhamiṣyate /",
  "SA20bsa019_u:14": "yadālayākhyaṃ vijñānaṃ pravṛttyākhyaṃ ca saptadhā // Tsn_6 //",
  "SA20bsa019_u:15": "saṃkleśavāsanābījaiścitatvāccittamucyate /",
  "SA20bsa019_u:16": "cittamādyaṃ dvitīyaṃ tu citrākārapravṛttitaḥ // Tsn_7 //",
  "SA20bsa019_u:17": "samāsato 'bhūtakalpaḥ sa caiṣa trividho mataḥ /",
  "SA20bsa019_u:18": "vaipākikastathā naimittiko 'nyaḥ prātibhāsikaḥ // Tsn_8 //",
  "SA20bsa019_u:19": "prathamo mūlavijñānaṃ tadvipākātmakaṃ yataḥ /",
  "SA20bsa019_u:20": "anyaḥ pravṛttivijñānaṃ dṛśyadṛgvittivṛttitaḥ // Tsn_9 //",
  "SA20bsa019_u:21": "sadasattvād dvayaikatvāt saṃkleśavyavadānayoḥ /",
  "SA20bsa019_u:22": "lakṣaṇābhedataśceṣṭā svabhāvānāṃ gaṃbhīratā // Tsn_10 //",
  "SA20bsa019_u:23": "sattvena gṛhyate yasmādatyantābhāva eva ca /",
  "SA20bsa019_u:24": "svabhāvaḥ kalpitastena sadasallakṣaṇo mataḥ // Tsn_11 //",
  "SA20bsa019_u:25": "vidyate bhrāntibhāvena yathākhyānaṃ na vidyate /",
  "SA20bsa019_u:26": "paratantro yatastena sadasallakṣaṇo mataḥ // Tsn_12 //",
  "SA20bsa019_u:27": "advayatvena yaccāsti dvayasyābhāva eva ca /",
  "SA20bsa019_u:28": "svabhāvastena niṣpannaḥ sadasallakṣaṇo mataḥ // Tsn_13 //",
  "SA20bsa019_u:29": "dvaividhyāt kalpitārthasya tadasattvaikabhāvataḥ /",
  "SA20bsa019_u:30": "svabhāvaḥ kalpito bālairdvayaikatvātmako mataḥ // Tsn_14 //",
  "SA20bsa019_u:31": "prakhyānād dvayabhāvena bhrāntimātraikabhāvataḥ /",
  "SA20bsa019_u:32": "svabhāvaḥ paratantrākhyo dvayaikatvātmako mataḥ // Tsn_15 //",
  "SA20bsa019_u:33": "dvayābhāvasvabhāvatvādadvayaikasvabhāvataḥ /",
  "SA20bsa019_u:34": "svabhāvaḥ pariniṣpanno dvayaikatvātmako mataḥ // Tsn_16 //",
  "SA20bsa019_u:35": "kalpitaḥ paratantraśca jñeyaṃ saṃkleśalakṣaṇam /",
  "SA20bsa019_u:36": "pariniṣpanna iṣṭastu vyavadānasya lakṣaṇam // Tsn_17 //",
  "SA20bsa019_u:37": "asaddvayasvabhāvatvāt tadabhāvasvabhāvataḥ /",
  "SA20bsa019_u:38": "svabhāvāt kalpitājjñeyo niṣpanno 'bhinnalakṣaṇaḥ // Tsn_18 //",
  "SA20bsa019_u:39": "advayatvasvabhāvatvād dvayābhāvasvabhāvataḥ /",
  "SA20bsa019_u:40": "niṣpannāt kalpitaścaiva vijñeyo 'bhinnalakṣaṇaḥ // Tsn_19 //",
  "SA20bsa019_u:41": "yathākhyānamasadbhāvāt tathāsattvasvabhāvataḥ /",
  "SA20bsa019_u:42": "svabhāvāt paratantrākhyānniṣpanno 'bhinnalakṣaṇaḥ // Tsn_20 //",
  "SA20bsa019_u:43": "asaddvayasvabhāvatvād yathākhyānāsvabhāvataḥ /",
  "SA20bsa019_u:44": "niṣpannāt paratantro 'pi vijñeyo 'bhinnalakṣaṇaḥ // Tsn_21 //",
  "SA20bsa019_u:45": "kramabhedaḥ svabhāvānāṃ vyavahārādhikārataḥ /",
  "SA20bsa019_u:46": "tatpraveśādhikārācca vyutpattyarthaṃ vidhīyate // Tsn_22 //",
  "SA20bsa019_u:47": "kalpito vyavahārātmā vyavahartrātmako 'paraḥ /",
  "SA20bsa019_u:48": "vyavahārasamucchedasvabhāvaścānya iṣyate // Tsn_23 //",
  "SA20bsa019_u:49": "dvayābhāvātmakaḥ pūrvaṃ paratantraḥ praviśyate /",
  "SA20bsa019_u:50": "tataḥ praviśyate tatra kalpamātramasaddvayam // Tsn_24 //",
  "SA20bsa019_u:51": "tato dvayābhāvabhāvo niṣpanno 'tra praviśyate /",
  "SA20bsa019_u:52": "tathā hyasāveva tadā astināstīti cocyate // Tsn_25 //",
  "SA20bsa019_u:53": "trayo 'pyete svabhāvā hi advayālambalakṣaṇāḥ /",
  "SA20bsa019_u:54": "abhāvādatathābhāvāt tadabhāvasvabhāvataḥ // Tsn_26 //",
  "SA20bsa019_u:55": "māyākṛtaṃ mantravaśāt khyāti hastyātmanā yathā /",
  "SA20bsa019_u:56": "ākāramātraṃ tatrāsti hastī nāsti tu sarvathā // Tsn_27 //",
  "SA20bsa019_u:57": "svabhāvaḥ kalpito hastī paratantrastadākṛtiḥ /",
  "SA20bsa019_u:58": "yastatra hastyabhāvo 'sau pariniṣpanna iṣyate // Tsn_28 //",
  "SA20bsa019_u:59": "asatkalpastathā khyāti mūlacittād dvayātmanā /",
  "SA20bsa019_u:60": "dvayamatyantato nāsti tatrāstyākṛtimātrakam // Tsn_29 //",
  "SA20bsa019_u:61": "mantravanmūlavijñānaṃ kāṣṭhavattathatā matā /",
  "SA20bsa019_u:62": "hastyākāravadeṣṭavyo vikalpo hastivad dvayam // Tsn_30 //",
  "SA20bsa019_u:63": "arthatattvaprativedhe yugapallakṣaṇatrayam /",
  "SA20bsa019_u:64": "parijñā ca prahāṇaṃ ca prāptiśceṣṭā yathākramam // Tsn_31 //",
  "SA20bsa019_u:65": "parijñānupalambho 'tra hānirakhyānamiṣyate /",
  "SA20bsa019_u:66": "upalambhanimittā tu prāptiḥ sākṣātkriyāpi sā // Tsn_32 //",
  "SA20bsa019_u:67": "dvayasyānupalambhena dvayākāro vigacchati /",
  "SA20bsa019_u:68": "vigamāt tasya niṣpanno dvayābhāvo 'dhigamyate // Tsn_33 //",
  "SA20bsa019_u:69": "hastino 'nupalambhaśca vigamaśca tadākṛteḥ /",
  "SA20bsa019_u:70": "upalambhaśca kāṣṭhasya māyāyāṃ yugapad yathā // Tsn_34 //",
  "SA20bsa019_u:71": "viruddhadhīvāraṇatvād buddhyā vaiyarthyadarśanāt /",
  "SA20bsa019_u:72": "jñānatrayānuvṛtteśca mokṣāpattirayatnataḥ // Tsn_35 //",
  "SA20bsa019_u:73": "cittamātropalambhena jñeyārthānupalambhatā /",
  "SA20bsa019_u:74": "jñeyārthānupalambhena syāccittānupalambhatā // Tsn_36 //",
  "SA20bsa019_u:75": "dvayoranupalambhena dharmadhātūpalabhbhatā /",
  "SA20bsa019_u:76": "dharmadhātūpalambhena syād vibhutvopalambhatā // Tsn_37 //",
  "SA20bsa019_u:77": "upalabdhavibhutvaśca svaparārthaprasiddhitaḥ /",
  "SA20bsa019_u:78": "prāpnotyanuttarāṃ bodhiṃ dhīmān kāyatrayātmikām // Tsn_38 //",
  "SA20bsa019_u:79": "iti trisvabhāvanirdeśaḥ samāptaḥ //",
  "SA20bsa019_u:80": "kṛtirācāryavasubandhupādānāmiti //"
}