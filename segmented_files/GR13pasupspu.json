{
  "GR13pasupspu:0": "atha prathamo 'dhyāyaḥ",
  "GR13pasupspu:1": "athātaḥ paśupateḥ pāśupataṃ yoga-vidhiṃ vyākhyāsyāmaḥ // PS_1.1 //",
  "GR13pasupspu:2": "bhasmanā triṣavaṇaṃ snāyīta // PS_1.2 //",
  "GR13pasupspu:3": "bhasmani śayīta // PS_1.3 //",
  "GR13pasupspu:4": "anusnānaṃ // PS_1.4 //",
  "GR13pasupspu:5": "nirmālyaṃ // PS_1.5 //",
  "GR13pasupspu:6": "liṅga-dhārī // PS_1.6 //",
  "GR13pasupspu:7": "āyatana-vāsī // PS_1.7 //",
  "GR13pasupspu:8": "hasita-gīta-nṛtta-duṃduṃ-kāra-namaskāra-japyo1pahāreṇo7patiṣṭhet // PS_1.8 //",
  "GR13pasupspu:9": "mahā-devasya dakṣiṇā-mūrteḥ // PS_1.9 //",
  "GR13pasupspu:10": "eka-vāsāḥ // PS_1.10 //",
  "GR13pasupspu:11": "avāsā vā // PS_1.11 //",
  "GR13pasupspu:12": "mūtra-purīṣaṃ nā7vekṣet // PS_1.12 //",
  "GR13pasupspu:13": "strī-śūdraṃ nā7bhibhāṣet // PS_1.13 //",
  "GR13pasupspu:14": "yady avekṣed yady abhibhāṣet // PS_1.14 //",
  "GR13pasupspu:15": "upaspṛśya // PS_1.15 //",
  "GR13pasupspu:16": "prāṇā3yāmaṃ kṛtvā // PS_1.16 //",
  "GR13pasupspu:17": "raudrīṃ gāyatrīṃ bahu-rūpīṃ vā japet // PS_1.17 //",
  "GR13pasupspu:18": "akaluṣa-mateḥ // PS_1.18 //",
  "GR13pasupspu:19": "carataḥ // PS_1.19 //",
  "GR13pasupspu:20": "tato 'sya yogaḥ pravartate // PS_1.20 //",
  "GR13pasupspu:21": "dūra-darśana-śravaṇa-manana-vijñānāni cā3sya pravartante // PS_1.21 //",
  "GR13pasupspu:22": "sarva-jñātā // PS_1.22 //",
  "GR13pasupspu:23": "mano-javitvam // PS_1.23 //",
  "GR13pasupspu:24": "kāma-rūpitvam // PS_1.24 //",
  "GR13pasupspu:25": "vikaraṇaḥ // PS_1.25 //",
  "GR13pasupspu:26": "dharmitvaṃ ca // PS_1.26 //",
  "GR13pasupspu:27": "sarve cā7sya vaśyā bhavanti // PS_1.27 //",
  "GR13pasupspu:28": "sarveṣāṃ cā7vaśyo bhavati // PS_1.28 //",
  "GR13pasupspu:29": "sarvāṃś cā8viśati // PS_1.29 //",
  "GR13pasupspu:30": "sarveṣāṃ cā7nāveśyo bhavati // PS_1.30 //",
  "GR13pasupspu:31": "sarve cā7sya vadhyā bhavanti // PS_1.31 //",
  "GR13pasupspu:32": "sarveṣāṃ cā7vadhyo bhavati // PS_1.32 //",
  "GR13pasupspu:33": "abhītaḥ // PS_1.33 //",
  "GR13pasupspu:34": "akṣayaḥ // PS_1.34 //",
  "GR13pasupspu:35": "ajaraḥ // PS_1.35 //",
  "GR13pasupspu:36": "amaraḥ // PS_1.36 //",
  "GR13pasupspu:37": "sarvatra cā7pratihata-gatir bhavati // PS_1.37 //",
  "GR13pasupspu:38": "ity etair guṇa-yukto [guṇair yukto] bhagavato mahā-devasya mahā-gaṇa-patir bhavati // PS_1.38 //",
  "GR13pasupspu:39": "atre7daṃ brahma japet // PS_1.39 //",
  "GR13pasupspu:40": "sadyo-jātaṃ prapadyāmi // PS_1.40 //",
  "GR13pasupspu:41": "sadyo-jātāya vai namaḥ // PS_1.41 //",
  "GR13pasupspu:42": "bhave bhave nā7tibhave // PS_1.42 //",
  "GR13pasupspu:43": "bhajasva mām // PS_1.43 //",
  "GR13pasupspu:44": "bhavodbhavaḥ // PS_1.44 //",
  "GR13pasupspu:45": "atha dvitīyo 'dhyāyaḥ",
  "GR13pasupspu:46": "vāmaḥ // PS_2.1 //",
  "GR13pasupspu:47": "devasya // PS_2.2 //",
  "GR13pasupspu:48": "jyeṣṭhasya // PS_2.3 //",
  "GR13pasupspu:49": "rudrasya // PS_2.4 //",
  "GR13pasupspu:50": "kalitā3sanam // PS_2.5 //",
  "GR13pasupspu:51": "sārva-kāmika ity ācakṣate // PS_2.6 //",
  "GR13pasupspu:52": "amaṅgalaṃ cā7tra maṅgalaṃ bhavati // PS_2.7 //",
  "GR13pasupspu:53": "apasavyaṃ ca pradakṣiṇam // PS_2.8 //",
  "GR13pasupspu:54": "tasmād ubhayathā yaṣṭavyaḥ // PS_2.9 //",
  "GR13pasupspu:55": "devavat pitṛvac ca // PS_2.10 //",
  "GR13pasupspu:56": "ubhayaṃ tu rudre devāḥ pitaraś ca // PS_2.11 //",
  "GR13pasupspu:57": "harṣā1pramādī // PS_2.12 //",
  "GR13pasupspu:58": "caryāyāṃ caryāyām // PS_2.13 //",
  "GR13pasupspu:59": "māhātmyam avāpnoti // PS_2.14 //",
  "GR13pasupspu:60": "atidattam atīṣṭam // PS_2.15 //",
  "GR13pasupspu:61": "atitaptaṃ tapas tathā // PS_2.16 //",
  "GR13pasupspu:62": "atyāgatiṃ gamayate // PS_2.17 //",
  "GR13pasupspu:63": "tasmāt // PS_2.18 //",
  "GR13pasupspu:64": "bhūyas tapaś caret // PS_2.19 //",
  "GR13pasupspu:65": "nā7nya-bhaktis tu śaṃkare // PS_2.20 //",
  "GR13pasupspu:66": "atre7daṃ brahma japet // PS_2.21 //",
  "GR13pasupspu:67": "vāma-devāya namo jyeṣṭhāya namo rudrāya namaḥ // PS_2.22 //",
  "GR13pasupspu:68": "kālāya namaḥ // PS_2.23 //",
  "GR13pasupspu:69": "kala-vikaraṇāya namaḥ // PS_2.24 //",
  "GR13pasupspu:70": "bala-prathamanāya namaḥ // PS_2.25 //",
  "GR13pasupspu:71": "sarva-bhūta-damanāya namaḥ // PS_2.26 //",
  "GR13pasupspu:72": "mano-'manāya namaḥ // PS_2.27 //",
  "GR13pasupspu:73": "atha tṛtīyo 'dhyāyaḥ",
  "GR13pasupspu:74": "avyakta-liṅgī // PS_3.1 //",
  "GR13pasupspu:75": "vyaktā3cāraḥ // PS_3.2 //",
  "GR13pasupspu:76": "avamataḥ // PS_3.3 //",
  "GR13pasupspu:77": "sarva-bhūteṣu // PS_3.4 //",
  "GR13pasupspu:78": "paribhūyamānaś caret // PS_3.5 //",
  "GR13pasupspu:79": "apahata-pāpmā // PS_3.6 //",
  "GR13pasupspu:80": "pareṣāṃ parivādāt // PS_3.7 //",
  "GR13pasupspu:81": "pāpaṃ ca tebhyo dadāti // PS_3.8 //",
  "GR13pasupspu:82": "sukṛtaṃ ca teṣām ādatte // PS_3.9 //",
  "GR13pasupspu:83": "tasmāt // PS_3.10 //",
  "GR13pasupspu:84": "pretavac caret // PS_3.11 //",
  "GR13pasupspu:85": "krātheta vā // PS_3.12 //",
  "GR13pasupspu:86": "spandeta vā // PS_3.13 //",
  "GR13pasupspu:87": "maṇṭeta vā // PS_3.14 //",
  "GR13pasupspu:88": "śṛṅgāreta vā // PS_3.15 //",
  "GR13pasupspu:89": "api tat kuryāt // PS_3.16 //",
  "GR13pasupspu:90": "api tad bhāṣet // PS_3.17 //",
  "GR13pasupspu:91": "yena paribhavaṃ gacchet // PS_3.18 //",
  "GR13pasupspu:92": "paribhūyamāno hi vidvān kṛtsna-tapā bhavati // PS_3.19 //",
  "GR13pasupspu:93": "atre7daṃ brahma japet // PS_3.20 //",
  "GR13pasupspu:94": "aghorebhyaḥ // PS_3.21 //",
  "GR13pasupspu:95": "atha ghorebhyaḥ // PS_3.22 //",
  "GR13pasupspu:96": "ghora-ghoratarebhyaś ca // PS_3.23 //",
  "GR13pasupspu:97": "sarvebhyaḥ // PS_3.24 //",
  "GR13pasupspu:98": "śarva-sarvebhyaḥ // PS_3.25 //",
  "GR13pasupspu:99": "namas te astu rudra-rūpebhyaḥ // PS_3.26 //",
  "GR13pasupspu:100": "atha caturtho 'dhyāyaḥ",
  "GR13pasupspu:101": "gūḍha-vidyā tapa ānantyāya prakāśate // PS_4.1 //",
  "GR13pasupspu:102": "gūḍha-vrataḥ // PS_4.2 //",
  "GR13pasupspu:103": "gūḍha-pavitra-vāṇiḥ // PS_4.3 //",
  "GR13pasupspu:104": "sarvāṇi dvārāṇi pidhāya // PS_4.4 //",
  "GR13pasupspu:105": "buddhyā // PS_4.5 //",
  "GR13pasupspu:106": "unmatavad eko vicareta loke // PS_4.6 //",
  "GR13pasupspu:107": "kṛtā1nnam utsṛṣṭam upādadīta // PS_4.7 //",
  "GR13pasupspu:108": "unmatto mūḍha ity evaṃ manyante itare janāḥ // PS_4.8 //",
  "GR13pasupspu:109": "asan-māno hi yantrāṇāṃ sarveṣām uttamaḥ smṛtaḥ // PS_4.9 //",
  "GR13pasupspu:110": "indro vā agre asureṣu pāśupatam acarat // PS_4.10 //",
  "GR13pasupspu:111": "sa teṣām iṣṭā3pūrtam ādatta // PS_4.11 //",
  "GR13pasupspu:112": "māyayā sukṛtayā samavindata // PS_4.12 //",
  "GR13pasupspu:113": "nindā hy eṣā9nindā tasmāt // PS_4.13 //",
  "GR13pasupspu:114": "nindyamānaś caret // PS_4.14 //",
  "GR13pasupspu:115": "anindita-karmā // PS_4.15 //",
  "GR13pasupspu:116": "sarva-viśiṣṭo 'yaṃ panthāḥ // PS_4.16 //",
  "GR13pasupspu:117": "sat-pathaḥ // PS_4.17 //",
  "GR13pasupspu:118": "kupathās tv anye // PS_4.18 //",
  "GR13pasupspu:119": "anena vidhinā rudra-samīpaṃ gatvā // PS_4.19 //",
  "GR13pasupspu:120": "na kaścid brāhmaṇaḥ punar āvartate // PS_4.20 //",
