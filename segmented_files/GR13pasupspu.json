{
    "GR13pasupspu:0": " atha prathamo 'dhyāyaḥ",
    "GR13pasupspu:1": " athātaḥ paśupateḥ pāśupataṃ yoga-vidhiṃ vyākhyāsyāmaḥ // PS_1.1 //",
    "GR13pasupspu:2": " bhasmanā triṣavaṇaṃ snāyīta // PS_1.2 //",
    "GR13pasupspu:3": " bhasmani śayīta // PS_1.3 //",
    "GR13pasupspu:4": " anusnānaṃ // PS_1.4 //",
    "GR13pasupspu:5": " nirmālyaṃ // PS_1.5 //",
    "GR13pasupspu:6": " liṅga-dhārī // PS_1.6 //",
    "GR13pasupspu:7": " āyatana-vāsī // PS_1.7 //",
    "GR13pasupspu:8": " mahā-devasya dakṣiṇā-mūrteḥ // PS_1.9 //",
    "GR13pasupspu:9": " eka-vāsāḥ // PS_1.10 //",
    "GR13pasupspu:10": " avāsā vā // PS_1.11 //",
    "GR13pasupspu:11": " mūtra-purīṣaṃ nā7vekṣet // PS_1.12 //",
    "GR13pasupspu:12": " strī-śūdraṃ nā7bhibhāṣet // PS_1.13 //",
    "GR13pasupspu:13": " yady avekṣed yady abhibhāṣet // PS_1.14 //",
    "GR13pasupspu:14": " upaspṛśya // PS_1.15 //",
    "GR13pasupspu:15": " prāṇā3yāmaṃ kṛtvā // PS_1.16 //",
    "GR13pasupspu:16": " raudrīṃ gāyatrīṃ bahu-rūpīṃ vā japet // PS_1.17 //",
    "GR13pasupspu:17": " akaluṣa-mateḥ // PS_1.18 //",
    "GR13pasupspu:18": " carataḥ // PS_1.19 //",
    "GR13pasupspu:19": " tato 'sya yogaḥ pravartate // PS_1.20 //",
    "GR13pasupspu:20": " dūra-darśana-śravaṇa-manana-vijñānāni cā3sya pravartante // PS_1.21 //",
    "GR13pasupspu:21": " sarva-jñātā // PS_1.22 //",
    "GR13pasupspu:22": " mano-javitvam // PS_1.23 //",
    "GR13pasupspu:23": " kāma-rūpitvam // PS_1.24 //",
    "GR13pasupspu:24": " vikaraṇaḥ // PS_1.25 //",
    "GR13pasupspu:25": " dharmitvaṃ ca // PS_1.26 //",
    "GR13pasupspu:26": " sarve cā7sya vaśyā bhavanti // PS_1.27 //",
    "GR13pasupspu:27": " sarveṣāṃ cā7vaśyo bhavati // PS_1.28 //",
    "GR13pasupspu:28": " sarvāṃś cā8viśati // PS_1.29 //",
    "GR13pasupspu:29": " sarveṣāṃ cā7nāveśyo bhavati // PS_1.30 //",
    "GR13pasupspu:30": " sarve cā7sya vadhyā bhavanti // PS_1.31 //",
    "GR13pasupspu:31": " sarveṣāṃ cā7vadhyo bhavati // PS_1.32 //",
    "GR13pasupspu:32": " abhītaḥ // PS_1.33 //",
    "GR13pasupspu:33": " akṣayaḥ // PS_1.34 //",
    "GR13pasupspu:34": " ajaraḥ // PS_1.35 //",
    "GR13pasupspu:35": " amaraḥ // PS_1.36 //",
    "GR13pasupspu:36": " sarvatra cā7pratihata-gatir bhavati // PS_1.37 //",
    "GR13pasupspu:37": " ity etair guṇa-yukto [guṇair yukto] bhagavato mahā-devasya mahā-gaṇa-patir bhavati // PS_1.38 //",
    "GR13pasupspu:38": " atre7daṃ brahma japet // PS_1.39 //",
    "GR13pasupspu:39": " sadyo-jātaṃ prapadyāmi // PS_1.40 //",
    "GR13pasupspu:40": " sadyo-jātāya vai namaḥ // PS_1.41 //",
    "GR13pasupspu:41": " bhave bhave nā7tibhave // PS_1.42 //",
    "GR13pasupspu:42": " bhajasva mām // PS_1.43 //",
    "GR13pasupspu:43": " bhavodbhavaḥ // PS_1.44 //",
    "GR13pasupspu:44": " atha dvitīyo 'dhyāyaḥ",
    "GR13pasupspu:45": " vāmaḥ // PS_2.1 //",
    "GR13pasupspu:46": " devasya // PS_2.2 //",
    "GR13pasupspu:47": " jyeṣṭhasya // PS_2.3 //",
    "GR13pasupspu:48": " rudrasya // PS_2.4 //",
    "GR13pasupspu:49": " sārva-kāmika ity ācakṣate // PS_2.6 //",
    "GR13pasupspu:50": " amaṅgalaṃ cā7tra maṅgalaṃ bhavati // PS_2.7 //",
    "GR13pasupspu:51": " apasavyaṃ ca pradakṣiṇam // PS_2.8 //",
    "GR13pasupspu:52": " tasmād ubhayathā yaṣṭavyaḥ // PS_2.9 //",
    "GR13pasupspu:53": " devavat pitṛvac ca // PS_2.10 //",
    "GR13pasupspu:54": " ubhayaṃ tu rudre devāḥ pitaraś ca // PS_2.11 //",
    "GR13pasupspu:55": " caryāyāṃ caryāyām // PS_2.13 //",
    "GR13pasupspu:56": " māhātmyam avāpnoti // PS_2.14 //",
    "GR13pasupspu:57": " atidattam atīṣṭam // PS_2.15 //",
    "GR13pasupspu:58": " atitaptaṃ tapas tathā // PS_2.16 //",
    "GR13pasupspu:59": " atyāgatiṃ gamayate // PS_2.17 //",
    "GR13pasupspu:60": " tasmāt // PS_2.18 //",
    "GR13pasupspu:61": " bhūyas tapaś caret // PS_2.19 //",
    "GR13pasupspu:62": " nā7nya-bhaktis tu śaṃkare // PS_2.20 //",
    "GR13pasupspu:63": " atre7daṃ brahma japet // PS_2.21 //",
    "GR13pasupspu:64": " vāma-devāya namo jyeṣṭhāya namo rudrāya namaḥ // PS_2.22 //",
    "GR13pasupspu:65": " kālāya namaḥ // PS_2.23 //",
    "GR13pasupspu:66": " kala-vikaraṇāya namaḥ // PS_2.24 //",
    "GR13pasupspu:67": " bala-prathamanāya namaḥ // PS_2.25 //",
    "GR13pasupspu:68": " sarva-bhūta-damanāya namaḥ // PS_2.26 //",
    "GR13pasupspu:69": " mano-'manāya namaḥ // PS_2.27 //",
    "GR13pasupspu:70": " atha tṛtīyo 'dhyāyaḥ",
    "GR13pasupspu:71": " avyakta-liṅgī // PS_3.1 //",
    "GR13pasupspu:72": " avamataḥ // PS_3.3 //",
    "GR13pasupspu:73": " sarva-bhūteṣu // PS_3.4 //",
    "GR13pasupspu:74": " paribhūyamānaś caret // PS_3.5 //",
    "GR13pasupspu:75": " apahata-pāpmā // PS_3.6 //",
    "GR13pasupspu:76": " pareṣāṃ parivādāt // PS_3.7 //",
    "GR13pasupspu:77": " pāpaṃ ca tebhyo dadāti // PS_3.8 //",
    "GR13pasupspu:78": " sukṛtaṃ ca teṣām ādatte // PS_3.9 //",
    "GR13pasupspu:79": " tasmāt // PS_3.10 //",
    "GR13pasupspu:80": " pretavac caret // PS_3.11 //",
    "GR13pasupspu:81": " krātheta vā // PS_3.12 //",
    "GR13pasupspu:82": " spandeta vā // PS_3.13 //",
    "GR13pasupspu:83": " maṇṭeta vā // PS_3.14 //",
    "GR13pasupspu:84": " śṛṅgāreta vā // PS_3.15 //",
    "GR13pasupspu:85": " api tat kuryāt // PS_3.16 //",
    "GR13pasupspu:86": " api tad bhāṣet // PS_3.17 //",
    "GR13pasupspu:87": " yena paribhavaṃ gacchet // PS_3.18 //",
    "GR13pasupspu:88": " paribhūyamāno hi vidvān kṛtsna-tapā bhavati // PS_3.19 //",
    "GR13pasupspu:89": " atre7daṃ brahma japet // PS_3.20 //",
    "GR13pasupspu:90": " aghorebhyaḥ // PS_3.21 //",
    "GR13pasupspu:91": " atha ghorebhyaḥ // PS_3.22 //",
    "GR13pasupspu:92": " ghora-ghoratarebhyaś ca // PS_3.23 //",
    "GR13pasupspu:93": " sarvebhyaḥ // PS_3.24 //",
    "GR13pasupspu:94": " śarva-sarvebhyaḥ // PS_3.25 //",
    "GR13pasupspu:95": " namas te astu rudra-rūpebhyaḥ // PS_3.26 //",
    "GR13pasupspu:96": " atha caturtho 'dhyāyaḥ",
    "GR13pasupspu:97": " gūḍha-vidyā tapa ānantyāya prakāśate // PS_4.1 //",
    "GR13pasupspu:98": " gūḍha-vrataḥ // PS_4.2 //",
    "GR13pasupspu:99": " gūḍha-pavitra-vāṇiḥ // PS_4.3 //",
    "GR13pasupspu:100": " sarvāṇi dvārāṇi pidhāya // PS_4.4 //",
    "GR13pasupspu:101": " buddhyā // PS_4.5 //",
    "GR13pasupspu:102": " unmatavad eko vicareta loke // PS_4.6 //",
    "GR13pasupspu:103": " kṛtā1nnam utsṛṣṭam upādadīta // PS_4.7 //",
    "GR13pasupspu:104": " unmatto mūḍha ity evaṃ manyante itare janāḥ // PS_4.8 //",
    "GR13pasupspu:105": " asan-māno hi yantrāṇāṃ sarveṣām uttamaḥ smṛtaḥ // PS_4.9 //",
    "GR13pasupspu:106": " indro vā agre asureṣu pāśupatam acarat // PS_4.10 //",
    "GR13pasupspu:107": " sa teṣām iṣṭā3pūrtam ādatta // PS_4.11 //",
    "GR13pasupspu:108": " māyayā sukṛtayā samavindata // PS_4.12 //",
    "GR13pasupspu:109": " nindā hy eṣā9nindā tasmāt // PS_4.13 //",
    "GR13pasupspu:110": " nindyamānaś caret // PS_4.14 //",
    "GR13pasupspu:111": " anindita-karmā // PS_4.15 //",
    "GR13pasupspu:112": " sarva-viśiṣṭo 'yaṃ panthāḥ // PS_4.16 //",
    "GR13pasupspu:113": " sat-pathaḥ // PS_4.17 //",
    "GR13pasupspu:114": " kupathās tv anye // PS_4.18 //",
    "GR13pasupspu:115": " anena vidhinā rudra-samīpaṃ gatvā // PS_4.19 //",
    "GR13pasupspu:116": " na kaścid brāhmaṇaḥ punar āvartate // PS_4.20 //",
    "GR13pasupspu:117": " atre7daṃ brahma japet // PS_4.21 //",
    "GR13pasupspu:118": " tat-puruṣāya vidmahe // PS_4.22 //",
    "GR13pasupspu:119": " mahā-devāya dhīmahi // PS_4.23 //",
    "GR13pasupspu:120": " tan no rudraḥ pracodayāt // PS_4.24 //",
    "GR13pasupspu:121": " atha pañcamo 'dhyāyaḥ",
    "GR13pasupspu:122": " asaṅgaḥ // PS_5.1 //",
    "GR13pasupspu:123": " yogī // PS_5.2 //",
    "GR13pasupspu:124": " ajaḥ // PS_5.4 //",
    "GR13pasupspu:125": " maitraḥ // PS_5.5 //",
    "GR13pasupspu:126": " abhijāyate // PS_5.6 //",
    "GR13pasupspu:127": " indriyāṇām abhijayāt // PS_5.7 //",
    "GR13pasupspu:128": " rudraḥ provāca tāvat // PS_5.8 //",
    "GR13pasupspu:129": " deva-nityaḥ // PS_5.10 //",
    "GR13pasupspu:130": " ṣaṇ-māsān nitya-yuktasya // PS_5.12 //",
    "GR13pasupspu:131": " bhūyiṣṭhaṃ saṃpravartate // PS_5.13 //",
    "GR13pasupspu:132": " bhaikṣyam // PS_5.14 //",
    "GR13pasupspu:133": " māṃsam aduṣyaṃ lavaṇena vā // PS_5.16 //",
    "GR13pasupspu:134": " āpo vā9pi yathā-kālam aśnīyād anupūrvaśaḥ // PS_5.17 //",
    "GR13pasupspu:135": " go-dharmā mṛga-dharmā vā // PS_5.18 //",
    "GR13pasupspu:136": " adbhir eva śucir bhavet // PS_5.19 //",
    "GR13pasupspu:137": " siddha-yogī na lipyate karmaṇā pātakena vā // PS_5.20 //",
    "GR13pasupspu:138": " ṛcam iṣṭām adhīyīta gāyatrīm ātma-yantritaḥ // PS_5.21 //",
    "GR13pasupspu:139": " raudrīṃ vā bahu-rūpīṃ vā // PS_5.22 //",
    "GR13pasupspu:140": " ato yoga pravartate // PS_5.23 //",
    "GR13pasupspu:141": " oṃ-kāram abhidhyāyīta // PS_5.24 //",
    "GR13pasupspu:142": " hṛdi kurvīta dhāraṇām // PS_5.25 //",
    "GR13pasupspu:143": " ṛṣir vipro mahān eṣaḥ // PS_5.26 //",
    "GR13pasupspu:144": " vāg-viśuddhaḥ // PS_5.27 //",
    "GR13pasupspu:145": " maheśvaraḥ // PS_5.28 // ***",
    "GR13pasupspu:146": " śmaśāna-vāsī // PS_5.30 //",
    "GR13pasupspu:147": " labhate rudra-sāyujyam // PS_5.33 //",
    "GR13pasupspu:148": " sadā rudram anusmaret // PS_5.34 //",
    "GR13pasupspu:149": " chittvā doṣāṇāṃ hetu-jālasya mūlam // PS_5.35 //",
    "GR13pasupspu:150": " buddhyā // PS_5.36 //",
    "GR13pasupspu:151": " saṃcittam // PS_5.37 //",
    "GR13pasupspu:152": " sthāpayitvā ca rudre // PS_5.38 //",
    "GR13pasupspu:153": " ekaḥ kṣemī san vīta-śokaḥ // PS_5.39 //",
    "GR13pasupspu:154": " apramādī gacched duḥkhānām antam īśa-prasādāt // PS_5.40 //",
    "GR13pasupspu:155": " atre7daṃ brahma japet // PS_5.41 //",
    "GR13pasupspu:156": " īśānaḥ sarva-vidyānām // PS_5.42 //",
    "GR13pasupspu:157": " īśvaraḥ sarva-bhūtānām // PS_5.43 //",
    "GR13pasupspu:158": " brahmaṇo 'dhipatir brahmā // PS_5.44 //",
    "GR13pasupspu:159": " śivo me astu // PS_5.45 //",
    "GR13pasupspu:160": " sadā // PS_5.46 //",
    "GR13pasupspu:161": " śivaḥ // PS_5.47 //"
}