{
    "T01n830u_001:0": " śāradāṣṭakastotram",
    "T01n830u_001:1": " śaṅkhendukundahimasannibhacārudehāṃ",
    "T01n830u_001:2": " haṃsasthitāṃ kamalapatrasurocanīyām |",
    "T01n830u_001:3": " divyāmbarābharaṇabhūṣitasaumyarūpāṃ",
    "T01n830u_001:4": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 1 ||",
    "T01n830u_001:5": " saṃsārasāgaramahodadhimagnasattva-",
    "T01n830u_001:6": " santāriṇīṃ suranarārcitapādapadmām |",
    "T01n830u_001:7": " hārārddhahāramaṇikuṇḍalamaṇḍitāṅgīṃ",
    "T01n830u_001:8": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 2 ||",
    "T01n830u_001:9": " yā bhāratīti kathitā jananī ca loke",
    "T01n830u_001:10": " mohāndhakārabharabhagnakṛtāṃ janānām |",
    "T01n830u_001:11": " saṃkīrtitā munibhirastasamastadoṣaiḥ",
    "T01n830u_001:12": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 3 ||",
    "T01n830u_001:13": " vīṇānuvādanaratāṃ sphaṭikākṣamālā-",
    "T01n830u_001:14": " saṃdhāriṇīṃ kanakapustakadhāriṇīṃ ca |",
    "T01n830u_001:15": " ratnaiḥ śubhaiḥ surucirāṃ kṛtahastapadmāṃ",
    "T01n830u_001:16": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 4 ||",
    "T01n830u_001:17": " pūjyā sadaiva jananī parivandanīyā",
    "T01n830u_001:18": " devyā gaṇā manasi saṃparimodanīyāḥ |",
    "T01n830u_001:19": " jīvārthinaḥ phalabhṛto guṇavarṇanā ca",
    "T01n830u_001:20": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 5 ||",
    "T01n830u_001:21": " natvā bhajejjagati yo jananīṃ prasiddhāṃ",
    "T01n830u_001:22": " kuryātsadā bhagavatī kilapañcasaukhyam |",
    "T01n830u_001:23": " saundaryarūpaguṇavittasubhogabhājaṃ",
    "T01n830u_001:24": " śrī śāradāṃ bhagavatīṃ satataṃ namāmi || 6 ||",
    "T01n830u_001:25": " pūjopahāravalibhiḥ paripūjanīyā",
    "T01n830u_001:26": " devyā gaṇādikamanojñasuvaktrapadmāḥ |",
    "T01n830u_001:27": " yaiḥ pūjitā bhagavatī kila jāḍyahantrī",
    "T01n830u_001:28": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 7 ||",
    "T01n830u_001:29": " yasyāḥ prasādamavagamya sukhapradāyā-",
    "T01n830u_001:30": " strailokyanāthauditārkasamaprabhaḥ syāt |",
    "T01n830u_001:31": " dharmārthakāmaphaladāmatha mokṣadāṃ ca",
    "T01n830u_001:32": " śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 8 ||",
    "T01n830u_001:33": " śrīśāradāṣṭakastotraṃ samāptam |"
}