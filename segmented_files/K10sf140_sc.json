  "K10sf140_sc:255": "hāritī vaśavartinī /",
  "K10sf140_sc:256": "uttamā varṇarūpeṇa",
  "K10sf140_sc:257": "putrebhiḥ parivāritā //",
  "K10sf140_sc:258": "sā cāpi samitim āyātā",
  "K10sf140_sc:259": "pūrvavat /",
  "K10sf140_sc:260": "ime ca sarve ’bhikrāntāḥ",
  "K10sf140_sc:261": "sendrā devāḥ sa-īśānāḥ",
  "K10sf140_sc:262": "saprajāpatayaḥ /",
  "K10sf140_sc:263": "Sand_31 Sand_31 bhikṣavo ’py atrābhikrāntāḥ",
  "K10sf140_sc:264": "paśya kṛṣṇasya mantritam",
  "K10sf140_sc:265": "idaṁ gṛhṇīta bandhadhvaṁ",
  "K10sf140_sc:266": "rāgeṇa bhadram astu vaḥ /",
  "K10sf140_sc:267": "parivārya samantāc ca",
  "K10sf140_sc:268": "maitraṁ jīvantam utsṛjata //",
  "K10sf140_sc:269": "iti tatra mahāsenaḥ",
  "K10sf140_sc:270": "kṛṣṇa senām acūcudat /",
  "K10sf140_sc:271": "svayaṁ cāpy abhyudāvṛttaḥ",
  "K10sf140_sc:272": "saṁkruddho ’py asvayaṁvaśaḥ //",
  "K10sf140_sc:273": "pāninā pṛthivīṁ hatvā",
  "K10sf140_sc:274": "kṛtvā śabdaṁ subhairavam /",
  "K10sf140_sc:275": "atha prāvarṣayad devam",
  "K10sf140_sc:276": "abhramāliṁ savidyutam //",
  "K10sf140_sc:277": "Sand_32 Sand_32 tāṁś ca buddho hy abhijñāya",
  "K10sf140_sc:278": "vyavalokya tathāgataḥ /",
  "K10sf140_sc:279": "athāmantrayati śāstā",
  "K10sf140_sc:280": "śrāvakāñ chāsane ratān //",
  "K10sf140_sc:281": "mārasainyam abhikrāntaṁ",
  "K10sf140_sc:282": "niśamayata bhikṣavaḥ /",
  "K10sf140_sc:283": "ātaptakāriṇas te ’pi",
  "K10sf140_sc:284": "śrutvā buddhasya bhāṣitam /",
  "K10sf140_sc:285": "vītarāgaṁ hi taṁ matvā",
  "K10sf140_sc:286": "tasya romāṇi neñjaṁti //",
  "K10sf140_sc:287": "te cāpi vijitasaṁgrāmā",
  "K10sf140_sc:288": "bhayābhītā yaśasvinaḥ /",
  "K10sf140_sc:289": "modante saha śiṣyebhiḥ",
  "K10sf140_sc:290": "śrāvakās te jinātmajāḥ //",
  "K10sf140_sc:291": "Sand_33 Sand_33 idam avocad bhagavām āptamanasas te bhikṣavo bhagavato bhāṣitam abhyanandan //",
  "K10sf140_sc:292": "a[nta]roddānam // varūṇā viṣṇu sabhikā śuklā śyāmā ca kṣemakāḥ //",
  "K10sf140_sc:293": "mahāsamājasūtraṁ samāptaḥ samāptañ ca iti / sādhū //"
}