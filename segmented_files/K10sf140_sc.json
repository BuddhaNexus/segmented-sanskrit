{
  "K10sf140_sc:0": "Mahāsamājasūtra",
  "K10sf140_sc:1": "Sand_1 evaṁ mayā śrutam / ekasmiṁ samaye bhagavāṁ śākyeṣu viharati vane kāpilavastave sārdham",
  "K10sf140_sc:2": "pañcabhir bhikṣuśataiḥ sarvair arhadbhiḥ kṣīṇāsravaiḥ kṛtakṛyaiḥ kṛtakaraṇīyair avahṛtabhārair",
  "K10sf140_sc:3": "anuprāptasvakārthaiḥ parikṣīṇabhavasaṁyojanaiḥ samyagājñayā suvimuktacittaiḥ / tatra",
  "K10sf140_sc:4": "bhagavāṁ bhi kṣusaṁghasya nirvāṇapratisaṁyuktāṁ dharmikathām kathayati sma / api",
  "K10sf140_sc:5": "tatra daśamahālokadhātu mahāśākyamahāśākyā devatā sannipatitā bhagavantam upāsate bhikṣusaṁghañ ca /",
  "K10sf140_sc:6": "Sand_2 atha khalu catasṛṇāṁ brahmalokanivāsikānāṁ devatānāṁ brahmakāyikānām etad abhavat /",
  "K10sf140_sc:7": "adya khalu bhagavāṁ śākyeṣu viharati vane kāpilavastave sārdhaṁ pañcabhir bhikṣuśataiḥ",
  "K10sf140_sc:8": "sarvair arhadbhiḥ pūrvavad yāvad bhikṣusaṁghañ ca / yan nu vayaṁ gatvā pratyekapratīkṣā",
  "K10sf140_sc:9": "bhagavantaṁ bhikṣusaṁghañ ca gāthābhir abhiṣṭaveyāma /",
  "K10sf140_sc:10": "Sand_3 atha catasro brahmakāyikā devatās tadyathā balavāṁ puruṣaḥ saṁmiṁjitaṁ vā bāhuṁ prasārayet",
  "K10sf140_sc:11": "prasāritaṁ vā saṁmiṁjayed evam eva catasro brahmakāyikā devatā brahmaloke antarhitā",
  "K10sf140_sc:12": "bhagavataḥ purataḥ pratitasthuḥ / ekāntasthitā ekā brahmakāyikā devatā tasyāṁ velāyāṁ gāthāṁ babhāṣe //",
  "K10sf140_sc:13": "mahāsamājaḥ pravaṇe",
  "K10sf140_sc:14": "devakāyāḥ samāgatāḥ /",
  "K10sf140_sc:15": "aparājitasaṁghaṁ hi",
  "K10sf140_sc:16": "draṣṭum abhyāgatā vayam //",
  "K10sf140_sc:17": "dvitīyā brahmakāyikā devatā gāthāṁ babhāṣe //",
  "K10sf140_sc:18": "ṛjubhūtena cittena",
  "K10sf140_sc:19": "pradadhaty atra bhikṣavaḥ /",
  "K10sf140_sc:20": "rathīva netrāṇy ādāya",
  "K10sf140_sc:21": "saṁrakṣaṁtīndriyāṇi hi //",
  "K10sf140_sc:22": "tṛtīyā brahmakāyikā devatā gāthāṁ babhāṣe //",
  "K10sf140_sc:23": "saṁpradālya khalu kīlam aneyā",
  "K10sf140_sc:24": "indrakīlaṁ parighaṁ parikhaṁ ca /",
  "K10sf140_sc:25": "te caranti śuddhā vimlāś ca",
  "K10sf140_sc:26": "nāyakena dāntāḥ śiśunāgāḥ //",
  "K10sf140_sc:27": "caturthī brahmakāyikā devatā gāthāṁ babhāṣe //",
  "K10sf140_sc:28": "ye buddhaṁ śaraṇaṁ yānti",
  "K10sf140_sc:29": "na te gacchanti durgatiṁ /",
  "K10sf140_sc:30": "prahāya mānuṣān kāyān",
  "K10sf140_sc:31": "divān kāyān upāsate //",
  "K10sf140_sc:32": "ity uktvā catasro brahmakāyikā devatās tatraiva sthāne ’ntarhitāḥ //",
  "K10sf140_sc:33": "Sand_4 atha bhagavāṁ sarvāṁ devapariṣadam avalokya bhikṣūn āmantrayati sma / sumahāṁ batāyaṁ",
  "K10sf140_sc:34": "bhikṣavo devatāsaṁnipātaḥ sumahāṁ batāyaṁ bhikṣavo devatāsaṁnipāto mama pariṣady",
  "K10sf140_sc:35": "etarhi / ye ’pi te atīte ’dhvani tathāgatā arhantaḥ samyaksaṁbuddhās teṣāṁ api buddhānāṁ",
  "K10sf140_sc:36": "bhagavatāṁ etāvad devatānāṁ saṁnipāto babhūva tadyathā mama pariṣady etarhi / ye",
  "K10sf140_sc:37": "pi te anāgāte ’dhvani tathāgatā arhantaḥ samyaksaṁbuddhās teṣām api buddhānāṁ bhagavatām",
  "K10sf140_sc:38": "etāvad devatānāṁ saṁnipāto bhaviṣyati tadyathā mama pariṣady etarhi / vakṣyāmi vo",
  "K10sf140_sc:39": "bhikṣavo devatānāṁ nāmāni kīrtayiṣyāmi devatānāṁ nāmāni / śṛṇuta mama bhāṣitam /",
  "K10sf140_sc:40": "Sand_5 Sand_5 …",
  "K10sf140_sc:41": "hitātmanaḥ samāhitāḥ /",
  "K10sf140_sc:42": "pṛthaksiṁhāiva …",
  "K10sf140_sc:43": "… pratāpinaḥ /",
  "K10sf140_sc:44": "avadātamanasaḥ śuddhā",
  "K10sf140_sc:45": "viprasannā anāvilāḥ /",
  "K10sf140_sc:46": "śritā devamanuṣyeṣu",
  "K10sf140_sc:47": "yāvad brahm … mṛtyuhāyinaḥ //",
  "K10sf140_sc:48": "śaikṣā dvipañcaśatakaṁ",
