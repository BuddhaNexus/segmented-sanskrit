{
  "SA14bsa026_u:0": "Abhisamayālaṅkāra (Asa)",
  "SA14bsa026_u:1": "namo mañjuśriye kumārabhūtāya",
  "SA14bsa026_u:2": "āryamaitreyanāthaviracitam abhisamayālaṅkāro nāma prajñāpāramitopadeśaśāstram",
  "SA14bsa026_u:3": "sarvākārajñātādhikāraḥ prathamaḥ",
  "SA14bsa026_u:4": "maṅgālācaraṇam",
  "SA14bsa026_u:5": "yā sarvajñatayā nayatyupaśamaṃ śāntaiṣiṇaḥ śrāvakān",
  "SA14bsa026_u:6": "yā mārgajñatayā jagaddhitakṛtāṃ lokārthasampādikā /",
  "SA14bsa026_u:7": "sarvākāramidaṃ vadanti munayo viśvaṃ yayā saṃgatās",
  "SA14bsa026_u:8": "tasyai śrāvakabodhisattvagaṇino buddhasya mātre namaḥ //",
  "SA14bsa026_u:9": "Asa_1.2 grānthārambhaprayojanam sarvākārajñatāmārgaḥ śāsitrā yo 'tra deśitaḥ /",
  "SA14bsa026_u:10": "dhīmanto vīkṣiṣiraṃstamanālīḍhaṃ parairiti //",
  "SA14bsa026_u:11": "Asa_1.3 smṛtau cādhāya sūtrārthaṃ dharmacaryāṃ daśātmikām /",
  "SA14bsa026_u:12": "sukhena pratipatsīrannityārambhaprayojanam //",
  "SA14bsa026_u:13": "Asa_1.4 prajñāpāramitāṣṭābhiḥ padārthaiḥ samudīritā /",
  "SA14bsa026_u:14": "sarvākārajñatā mārgajñatā sarvajñatā tataḥ //",
  "SA14bsa026_u:15": "Asa_1.5 sarvākārābhisambodho mūrdhaprāpto 'nupūrvikaḥ /",
  "SA14bsa026_u:16": "ekakṣaṇābhisambodho dharmakāyaśca te 'ṣṭadhā //",
  "SA14bsa026_u:17": "Asa_1.6 cittotpādo 'vavādaśca nirvedhāṅgaṃ caturvidham /",
  "SA14bsa026_u:18": "ādhāraḥ pratipatteśca dharmadhātusvabhāvakaḥ //",
  "SA14bsa026_u:19": "Asa_1.7 ālambanaṃ samuddeśaḥ sannāhaprasthitikriye /",
  "SA14bsa026_u:20": "sambhārāśca saniryāṇāḥ sarvākārajñatā muneḥ //",
  "SA14bsa026_u:21": "Asa_1.8 dhyāmīkaraṇatādīni śiṣyakhaḍgapathau ca yau /",
  "SA14bsa026_u:22": "mahānuśaṃso dṛṅmārga ehikāmutrikairguṇaiḥ //",
  "SA14bsa026_u:23": "Asa_1.9 kāritramadhimuktiśca stutastobhitaśaṃsitāḥ /",
  "SA14bsa026_u:24": "pariṇāme 'numode ca manaskārāvanuttamau //",
  "SA14bsa026_u:25": "Asa_1.10 nirhāraḥ śuddhiratyantamityayaṃ bhāvanāpathaḥ /",
  "SA14bsa026_u:26": "vijñānāṃ bodhisattvānāmiti mārgajñatoditā //",
  "SA14bsa026_u:27": "Asa_1.11 prajñayā na bhave sthānaṃ kṛpayā na śame sthitiḥ /",
  "SA14bsa026_u:28": "anupāyena dūratvam upāyenāvidūratā //",
  "SA14bsa026_u:29": "Asa_1.12 vipakṣapratipakṣau ca prayogaḥ samatāsya ca /",
  "SA14bsa026_u:30": "dṛṅmārgaḥ śrāvakādīnāmiti sarvajñateṣyate //",
  "SA14bsa026_u:31": "Asa_1.13 ākārāḥ saprayogāśca guṇā doṣāḥ salakṣaṇāḥ /",
  "SA14bsa026_u:32": "mokṣanirvedhabhāgīye śaikṣo 'vaivartiko gaṇaḥ //",
  "SA14bsa026_u:33": "Asa_1.14 samatā bhavaśāntyośca kṣetraśuddhiranuttarā /",
  "SA14bsa026_u:34": "sarvākārābhisambodha eṣa sopāyakauśalaḥ //",
  "SA14bsa026_u:35": "Asa_1.15 liṅgaṃ tasya vivṛddhiśca nirūḍhiścittasaṃsthitiḥ /",
  "SA14bsa026_u:36": "caturdhā ca vikalpasya pratipakṣaścaturvidhaḥ //",
  "SA14bsa026_u:37": "Asa_1.16 pratyekaṃ darśanākhye ca bhāvanākhye ca vartmani /",
  "SA14bsa026_u:38": "ānantaryasamādhiśca saha vipratipattibhiḥ //",
  "SA14bsa026_u:39": "Asa_1.17 mūrdhābhisamayastredhā daśadhā cānupūrvikaḥ /",
  "SA14bsa026_u:40": "ekakṣaṇābhisambodho lakṣaṇena caturvidhaḥ //",
  "SA14bsa026_u:41": "Asa_1.18 svābhāvikaḥ sasāmbhogo nairmāṇiko 'parastathā /",
  "SA14bsa026_u:42": "dharmakāyaḥ sakāritraścaturdhā samudīritaḥ //",
  "SA14bsa026_u:43": "sarvākārajñatā",
  "SA14bsa026_u:44": "1 - cittotpādaḥ",
  "SA14bsa026_u:45": "Asa_1.19 cittotpādaḥ parārthāya samyaksambodhikāmatā /",
  "SA14bsa026_u:46": "samāsavyāsataḥ sā ca yathāsūtraṃ sa cocyate //",
  "SA14bsa026_u:47": "Asa_1.20 bhūhemacandrajvalanairnidhiratnākarārṇavaiḥ /",
  "SA14bsa026_u:48": "vajrācalauṣadhīmitraiścintāmaṇyarkagītibhiḥ //",
  "SA14bsa026_u:49": "Asa_1.21 nṛpagañjamahāmārgayānaprasravaṇodakaiḥ /",
  "SA14bsa026_u:50": "ānandoktinadīmevairdvāviṃśatividhaḥ sa ca //",
  "SA14bsa026_u:51": "2 - avavādaḥ",
  "SA14bsa026_u:52": "Asa_1.22 pratipattau ca satyeṣu buddharatnādiṣu triṣu /",
  "SA14bsa026_u:53": "asaktāvapariśrānto pratipatsamparigrahe //",
  "SA14bsa026_u:54": "Asa_1.23 cakṣuḥṣu pañcasu jñeyaḥ ṣaṭsvabhijñāguṇeṣu ca /",
  "SA14bsa026_u:55": "dṛṅmārge bhāvanākhye cetyavavādo daśātmakaḥ //",
  "SA14bsa026_u:56": "Asa_1.24 mṛdutīkṣṇendriyau śraddhādṛṣṭiprāptau kulaṅkalau /",
  "SA14bsa026_u:57": "ekavīcyantarotpadya kārākārākaniṣṭhagāḥ //",
  "SA14bsa026_u:58": "Asa_1.25 plutāstrayo bhavasyāgraparamo rūparāgahā /",
  "SA14bsa026_u:59": "dṛṣṭadharmaśamaḥ kāyasākṣī khaḍgaśca viṃśatiḥ //",
  "SA14bsa026_u:60": "3 - nirvedhāṅgam",
  "SA14bsa026_u:61": "Asa_1.26 ālambanata ākārāddhetutvāt samparigrahāt /",
  "SA14bsa026_u:62": "caturvikalpasaṃyogaṃ yathāsvaṃ bhajatāṃ satām //",
  "SA14bsa026_u:63": "Asa_1.27 śrāvakebhyaḥ sakhaḍgebhyo bodhisattvasya tāyinaḥ /",
  "SA14bsa026_u:64": "mṛdumadhyādhimātrāṇāmūṣmādīnāṃ viśiṣṭatā //",
  "SA14bsa026_u:65": "Asa_1.28 ālambanamanityādi satyādhāraṃ tadākṛtiḥ /",
  "SA14bsa026_u:66": "niṣedho 'bhiniveśāderheturyānatrayāptaye //",
  "SA14bsa026_u:67": "Asa_1.29 rūpādyāyavyayau viṣṭhāsthiti prajñaptyavācyate /",
  "SA14bsa026_u:68": "rūpāvādavasthitisteṣāṃ tadbhāvenāsvabhāvatā //",
  "SA14bsa026_u:69": "Asa_1.30 tayormithaḥ svabhāvatvaṃ tadanityādyasaṃsthitiḥ /",
  "SA14bsa026_u:70": "tāsāṃ tadbhāvaśūnyatvaṃ mithaḥ svābhāvyametayoḥ //",
  "SA14bsa026_u:71": "Asa_1.31 anudgraho yo dharmāṇāṃ tannimittāsamīkṣaṇam /",
  "SA14bsa026_u:72": "parīkṣaṇaṃ ca prajñāyāḥ sarvasyānupalambhataḥ //",
  "SA14bsa026_u:73": "Asa_1.32 rūpāderasvabhāvatvaṃ tadabhāvasvabhāvatā /",
  "SA14bsa026_u:74": "tadajātiraniryāṇaṃ śuddhistadanimittatā //",
  "SA14bsa026_u:75": "Asa_1.33 tannimittānadhiṣṭhānānadhimuktirasaṃjñatā /",
  "SA14bsa026_u:76": "samādhistasya kāritraṃ vyākṛtirmananākṣayaḥ //",
  "SA14bsa026_u:77": "Asa_1.34 mithastrikasya svābhāvyaṃ samādheravikalpanā /",
  "SA14bsa026_u:78": "iti nirvedhabhāgīyaṃ mṛdumadhyādhimātrataḥ //",
  "SA14bsa026_u:79": "Asa_1.35 dvaividhyaṃ grāhyakalpasya vastutatpratipakṣataḥ /",
  "SA14bsa026_u:80": "moharāśyādibhedena pratyekaṃ navadhā tu saḥ //",
  "SA14bsa026_u:81": "Asa_1.36 dravyaprajñaptyadhiṣṭhāno dvividho grāhako mataḥ /",
  "SA14bsa026_u:82": "svatantrātmādirūpeṇa skandhādyāśrayatastathā //",
  "SA14bsa026_u:83": "Asa_1.37 cittānavalīnatvādinaiḥsvābhāvyādideśakaḥ /",
  "SA14bsa026_u:84": "tadvipakṣaparityāgaḥ sarvathā samparigrahaḥ //",
  "SA14bsa026_u:85": "4 - pratipatterādhāraḥ",
  "SA14bsa026_u:86": "Asa_1.38 ṣoḍhādhigamadharmasya pratipakṣaprahāṇayoḥ /",
  "SA14bsa026_u:87": "tayoḥ paryupayogasya prajñāyāḥ kṛpayā saha //",
  "SA14bsa026_u:88": "Asa_1.39 śiṣyāsādhāraṇatvasya parārthānukramasya ca /",
  "SA14bsa026_u:89": "jñānasyāyatnavṛtteśca pratiṣṭhā gotramucyate //",
  "SA14bsa026_u:90": "Asa_1.40 dharmadhātorasambhedād gotrabhedo na yujyate /",
  "SA14bsa026_u:91": "ādheyadharmabhedāttu tadbhedaḥ parigīyate //",
  "SA14bsa026_u:92": "5 - ālambanam",
  "SA14bsa026_u:93": "Asa_1.41 ālambanaṃ sarvadharmāste punaḥ kuśalādayaḥ /",
  "SA14bsa026_u:94": "laukikādhigamākhyāśca ye ca lokottarā matāḥ //",
  "SA14bsa026_u:95": "Asa_1.42 sāsravānāsravā dharmāḥ saṃskṛtāsaṃskṛtāśca ye /",
  "SA14bsa026_u:96": "śiṣyasādhāraṇā dharmā ye cāsādhāraṇā muneḥ //",
  "SA14bsa026_u:97": "6 - samuddeśaḥ",
  "SA14bsa026_u:98": "Asa_1.43 sarvasattvāgratācittaprahāṇādhigamatraye /",
  "SA14bsa026_u:99": "tribhirmahattvairuddeśo vijñeyo 'yaṃ svayambhuvām //",
  "SA14bsa026_u:100": "7 - sannāhapratipattiḥ",
  "SA14bsa026_u:101": "Asa_1.44 dānādau ṣaḍvidhe teṣāṃ pratyekaṃ saṃgraheṇa yā /",
  "SA14bsa026_u:102": "sannāhapratipattiḥ sā ṣaḍbhiḥ ṣaṭkairyathoditā //",
  "SA14bsa026_u:103": "8 - prasthānapratipattiḥ",
  "SA14bsa026_u:104": "Asa_1.45 dhyānārūpyeṣu dānādau mārge maitryādikeṣu ca /",
  "SA14bsa026_u:105": "gatopalambhayoge ca trimaṇḍalaviśuddhiṣu //",
  "SA14bsa026_u:106": "Asa_1.46 uddeśe ṣaṭsvabhijñāsu sarvākārajñatānaye /",
  "SA14bsa026_u:107": "prasthānapratipajjñeyā mahāyānādhirohiṇī //",
  "SA14bsa026_u:108": "9 - sambhārapratipattiḥ",
  "SA14bsa026_u:109": "Asa_1.47 dayā dānādikaṃ ṣaṭkaṃ śamathaḥ savidarśanaḥ /",
  "SA14bsa026_u:110": "yuganaddhaśca yo mārga upāye yacca kauśalam //",
  "SA14bsa026_u:111": "Asa_1.48 jñānaṃ puṇyaṃ ca mārgaśca dhāraṇī bhūmayo daśa /",
  "SA14bsa026_u:112": "pratipakṣaśca vijñeyaḥ sambhārapratipatkramaḥ //",
  "SA14bsa026_u:113": "1.",
  "SA14bsa026_u:114": "pramuditā bhūmiḥ",
  "SA14bsa026_u:115": "Asa_1.49 labhyate prathamā bhūmirdaśadhā parikarmaṇā",
  "SA14bsa026_u:116": "āśayo hitavastutvaṃ sattveṣu samacittatā //",
  "SA14bsa026_u:117": "Asa_1.50 tyāgaḥ sevā ca mitrāṇāṃ saddharmālambanaiṣaṇā /",
  "SA14bsa026_u:118": "sadā naiṣkramyacittatvaṃ buddhakāyagatā spṛhā //",
  "SA14bsa026_u:119": "Asa_1.51 dharmasya deśanā satyaṃ daśamaṃ vākyamiṣyate /",
  "SA14bsa026_u:120": "jñeyaṃ ca parikarmaiṣāṃ svabhāvānupalambhataḥ //",
  "SA14bsa026_u:121": "2.",
  "SA14bsa026_u:122": "vimalā bhūmiḥ",
  "SA14bsa026_u:123": "Asa_1.52 śīlaṃ kṛtajñatā kṣāntiḥ prāmodyaṃ mahatī kṛpā /",
  "SA14bsa026_u:124": "gauravaṃ guruśuśrūṣā vīryaṃ dānādike 'ṣṭamam //",
  "SA14bsa026_u:125": "3.",
  "SA14bsa026_u:126": "prabhākarī bhūmiḥ",
  "SA14bsa026_u:127": "Asa_1.53 atṛptatā śrute dānaṃ dharmasya ca nirāmiṣam /",
  "SA14bsa026_u:128": "buddhakṣetrasya saṃśuddhiḥ saṃsārāparikheditā //",
  "SA14bsa026_u:129": "hrīrapatrāpyamityetat pañcadhā mananātmakam /",
  "SA14bsa026_u:130": "4.",
  "SA14bsa026_u:131": "arciṣmatī bhūmiḥ",
  "SA14bsa026_u:132": "Asa_1.54 vanāśālpecchatā tuṣṭirdhūtasaṃlekhasevanam //",
  "SA14bsa026_u:133": "Asa_1.55 śikṣāyā aparityāgaḥ kāmānāṃ vijugupsanam /",
  "SA14bsa026_u:134": "nirvitsarvāstisantyāgo 'navalīnānapekṣate //",
  "SA14bsa026_u:135": "5.",
  "SA14bsa026_u:136": "sudurjayā bhūmiḥ",
  "SA14bsa026_u:137": "Asa_1.56 saṃstavaṃ kulamātsaryaṃ sthānaṃ saṅgaṇikāvaham / ātmotkarṣaparāvajñe",
  "SA14bsa026_u:138": "karmamārgān daśāśubhān //",
  "SA14bsa026_u:139": "Asa_1.57 mānaṃ stambhaṃ viparyāsaṃ vimatiṃ kleśamarṣaṇam /",
  "SA14bsa026_u:140": "vivarjayan samāpnoti daśaitān pañcamī bhuvam //",
  "SA14bsa026_u:141": "6.",
  "SA14bsa026_u:142": "abhimukhī bhūmiḥ",
  "SA14bsa026_u:143": "Asa_1.58 dānaśīlakṣamāvīryadhyānaprajñāprapūraṇāt /",
  "SA14bsa026_u:144": "śiṣyakhaḍgaspṛhātrāsacetasāṃ parivarjakaḥ //",
  "SA14bsa026_u:145": "Asa_1.59 yācito 'navalīnaśca sarvatyāge 'pyadurmanāḥ /",
  "SA14bsa026_u:146": "kṛśo 'pi nārthināṃ kṣeptā ṣaṣṭhīṃ bhūmiṃ samaśnute //",
  "SA14bsa026_u:147": "7.",
  "SA14bsa026_u:148": "dūraṅgamā bhūmiḥ",
  "SA14bsa026_u:149": "Asa_1.60 ātmasattvagraho jīvapudgalocchedaśāśvataḥ /",
  "SA14bsa026_u:150": "nimittahetvoḥ skandheṣu dhātuṣvāyataneṣu ca //",
  "SA14bsa026_u:151": "Asa_1.61 traidhātuke pratiṣṭhānaṃ saktirālīnacittatā /",
  "SA14bsa026_u:152": "ratnatritayaśīleṣu taddṛṣṭayabhiniveśitā //",
  "SA14bsa026_u:153": "Asa_1.62 śūnyatāyāṃ vivādaśca tadvirodhaśca viṃśatiḥ /",
  "SA14bsa026_u:154": "kalaṅkā yasya vicchinnāḥ saptamīmetyasau bhuvam //",
  "SA14bsa026_u:155": "Asa_1.63 trivimokṣamukhajñānaṃ trimaṇḍalaviśuddhatā /",
  "SA14bsa026_u:156": "karuṇāmananā dharmasamataikanayajñatā //",
  "SA14bsa026_u:157": "Asa_1.64 anutpādakṣamājñānaṃ dharmāṇāmekadheraṇā /",
  "SA14bsa026_u:158": "kalpanāyāḥ samudghātaḥ saṃjñādṛkkleśavarjanam //",
  "SA14bsa026_u:159": "Asa_1.65 śamathasya ca nidhyaptiḥ kauśalaṃ ca vidarśane /",
  "SA14bsa026_u:160": "cittasya dāntatā jñānaṃ sarvatrapratighāti ca //",
  "SA14bsa026_u:161": "Asa_1.66 sakterabhūmiryatrecchaṃ kṣetrāntaragatiḥ samam /",
  "SA14bsa026_u:162": "sarvatra svātmabhāvasya darśanaṃ ceti viṃśatiḥ //",
  "SA14bsa026_u:163": "8.",
  "SA14bsa026_u:164": "acalā bhūmiḥ",
  "SA14bsa026_u:165": "Asa_1.67 sarvasattvamanojñānamabhijñākrīḍanaṃ śubhā /",
  "SA14bsa026_u:166": "buddhakṣetrasya niṣpattirbuddhasevāparīkṣaṇe //",
  "SA14bsa026_u:167": "Asa_1.68 akṣajñānaṃ jinakṣetraśuddhirmāyopamā sthitiḥ /",
  "SA14bsa026_u:168": "saṃcintya ca bhavādānamidaṃ karmāṣṭadhoditam //",
  "SA14bsa026_u:169": "9.",
  "SA14bsa026_u:170": "sādhumato bhūmiḥ",
  "SA14bsa026_u:171": "Asa_1.69 praṇidhānānyanantāni devādīnāṃ rūtajñatā /",
  "SA14bsa026_u:172": "nadīva pratibhānānāṃ garbhāvakrāntiruttamā //",
  "SA14bsa026_u:173": "Asa_1.70 kulajātyośca gotraśca parivārasya janmanaḥ /",
  "SA14bsa026_u:174": "naiṣkramyabodhivṛkṣāṇāṃ guṇapūre svasampadaḥ //",
  "SA14bsa026_u:175": "10.",
  "SA14bsa026_u:176": "dharmamedhā bhūmiḥ",
  "SA14bsa026_u:177": "Asa_1.71 navabhūmīratikramya buddhabhūmau pratiṣṭhate /",
  "SA14bsa026_u:178": "yena jñānena sā jñeyā daśamī bodhisattvabhūḥ //",
  "SA14bsa026_u:179": "Asa_1.72 [pratipakṣasambhāraḥ] pratipakṣo 'ṣṭadhā jñeyo darśanābhyāsamārgayoḥ /",
  "SA14bsa026_u:180": "grāhyagrāhakavikalpānāmaṣṭānāmupaśāntaye //",
  "SA14bsa026_u:181": "20 - niryāṇapratipattiḥ",
  "SA14bsa026_u:182": "Asa_1.73 uddeśe samatāyāṃ ca sattvārthe yatnavarjane /",
  "SA14bsa026_u:183": "atyantāya ca niryāṇaṃ niryāṇaṃ prāptilakṣaṇam //",
  "SA14bsa026_u:184": "Asa_1.74 sarvākārajñatāyāṃ ca niryāṇaṃ mārgagocaram /",
  "SA14bsa026_u:185": "niryāṇapratipajjñeyā seyamaṣṭavidhātmikā //",
  "SA14bsa026_u:186": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:187": "prajñāpāramitopadeśaśāstre prathamādhikāraḥ /",
  "SA14bsa026_u:188": "2 Mārgajñatādhikāraḥ dvitīyaḥ",
  "SA14bsa026_u:189": "1 - dhyāmīkaraṇatādīni",
  "SA14bsa026_u:190": "Asa_2.1 dhyāmīkaraṇatā bhābhirdevānāṃ yogyatāṃ prati /",
  "SA14bsa026_u:191": "viṣayo niyato vyāptiḥ svabhāvastasya karma ca //",
  "SA14bsa026_u:192": "2 - śrāvakamārgaḥ",
  "SA14bsa026_u:193": "Asa_2.2 caturṇāmāryasatyānāmākārānupalambhataḥ /",
  "SA14bsa026_u:194": "śrāvakāṇāmayaṃ mārgo jñeyo mārgajñatānaye //",
  "SA14bsa026_u:195": "Asa_2.3 rūpādiskandhaśūnyatvācchūnyatānāmabhedataḥ /",
  "SA14bsa026_u:196": "ūsmāṇau 'nupalambhena teṣāṃ mūrdhagataṃ matam //",
  "SA14bsa026_u:197": "Asa_2.4 kṣāntayasteṣu nityādiyogasthānaniṣedhataḥ /",
  "SA14bsa026_u:198": "daśa bhūmīḥ samārabhya vistarāsthānadeśanāt //",
  "SA14bsa026_u:199": "Asa_2.5 agradharmagataṃ proktamāryaśrāvakartmani /",
  "SA14bsa026_u:200": "tatkasya hetorbuddhena buddhvā dharmāsamīkṣaṇāt //",
  "SA14bsa026_u:201": "3 - pratyekabuddhamārgaḥ",
  "SA14bsa026_u:202": "Asa_2.6 paropadeśavaiyarthyaṃ svayambodhāt svayambhuvām /",
  "SA14bsa026_u:203": "gambhīratā ca jñānasya khaḍgānāmabhidhīyate //",
  "SA14bsa026_u:204": "Asa_2.7 śuśrūṣā yasyā yasyārthe yatra yatra yathā yathā /",
  "SA14bsa026_u:205": "sa so 'rthaḥ khyātyaśabdo 'pi tasya tasya tathā tathā //",
  "SA14bsa026_u:206": "Asa_2.8 grāhyārthakalpanāhānād grāhakasyāprahāṇataḥ /",
  "SA14bsa026_u:207": "ādhārataśca vijñeyaḥ khaḍgamārgasya saṃgrahaḥ //",
  "SA14bsa026_u:208": "Asa_2.9 prajñapteravirodhena dharmatāsūcanākṛtiḥ /",
  "SA14bsa026_u:209": "ūṣmagaṃ mūrdhagaṃ rūpādyahānādiprabhāvitam //",
  "SA14bsa026_u:210": "Asa_2.10 adhyātmaśūnyatādyābhī rūpāderaparigrāhāt /",
  "SA14bsa026_u:211": "kṣāntī rūpādyanutpādādyākārairagradharmatā //",
  "SA14bsa026_u:212": "4 - bodhisattvamārgaḥ",
  "SA14bsa026_u:213": "Asa_2.11 kṣāntijñānakṣaṇaiḥ satyaṃ satyaṃ prati caturvidhaiḥ /",
  "SA14bsa026_u:214": "mārgajñatāyāṃ dṛṅmārgaḥ sānuśaṃso 'yamucyate //",
  "SA14bsa026_u:215": "Asa_2.12 ādhārādheyatābhāvāttathatābuddhayormithaḥ /",
  "SA14bsa026_u:216": "paryāyeṇānanujñānaṃ mahattā sāpramāṇatā //",
  "SA14bsa026_u:217": "Asa_2.13 parimāṇāntatābhāvo rūpāderavadhāraṇam /",
  "SA14bsa026_u:218": "tasyāṃ sthitasya buddhatve 'nudgrahātyāgatādayaḥ //",
  "SA14bsa026_u:219": "Asa_2.14 maitryādi śūnyatā prāptirbuddhatvasya parigrahaḥ /",
  "SA14bsa026_u:220": "sarvasya vyavadānasya sarvādhivyādhiśātanam //",
  "SA14bsa026_u:221": "Asa_2.15 nirvāṇagrāhaśāntatvaṃ buddhebhyo rakṣaṇādikam /",
  "SA14bsa026_u:222": "aprāṇivadhamārabhya sarvākārajñatānaye //",
  "SA14bsa026_u:223": "Asa_2.16 svayaṃ sthitasya sattvānāṃ sthāpanaṃ pariṇāmanam /",
  "SA14bsa026_u:224": "dānādīnāṃ ca sambodhāviti mārgajñatākṣaṇāḥ //",
  "SA14bsa026_u:225": "5 - bhāvanāmārgakāritram",
  "SA14bsa026_u:226": "Asa_2.17 sarvato damanaṃ nāma sarvataḥ kleśanirjayaḥ /",
  "SA14bsa026_u:227": "upakramāviṣahyatvaṃ bodhirādhārapūjyatā //",
  "SA14bsa026_u:228": "sāsravo bhāvanāmārgaḥ",
  "SA14bsa026_u:229": "6 - bhāvanāmārgādhimuktiḥ",
  "SA14bsa026_u:230": "Asa_2.18 adhimuktistridhā jñeyā svārthā ca svaparārthikā /",
  "SA14bsa026_u:231": "parārthikaivetyeṣā ca pratyekaṃ trividheṣyate //",
  "SA14bsa026_u:232": "Asa_2.19 mṛdvī madhyādhimātrā ca mṛdumṛdvādibhedataḥ /",
  "SA14bsa026_u:233": "sā punastribidhetyevaṃ saptaviṃśatidhā matā //",
  "SA14bsa026_u:234": "7 - bhāvanāmārgādhimuktasya stutiḥ stobhaḥ praśaṃsā ca",
  "SA14bsa026_u:235": "Asa_2.20 stutiḥ stobhaḥ praśaṃsā ca prajñāpāramitāṃ prati /",
  "SA14bsa026_u:236": "adhimokṣasya mātrāṇāṃ navakaistribhiriṣyate //",
  "SA14bsa026_u:237": "8 - pariṇāmanā",
  "SA14bsa026_u:238": "Asa_2.21 viśeṣaḥ pariṇāmastu tasya kāritramuttamam /",
  "SA14bsa026_u:239": "nopalambhākṛtiścāsāvaviparyāsalakṣaṇaḥ //",
  "SA14bsa026_u:240": "Asa_2.22 vivikto buddhapuṇyaughasvabhāvasmṛtigocaraḥ /",
  "SA14bsa026_u:241": "sopāyaścānimittaśca buddhairabhyanumoditaḥ //",
  "SA14bsa026_u:242": "Asa_2.23 traidhātukāprapannaśca pariṇāmo 'parastridhā /",
  "SA14bsa026_u:243": "mṛdurmadhyo 'dhimātraśca mahāpuṇyodayātmakaḥ //",
  "SA14bsa026_u:244": "9 - anumodanā",
  "SA14bsa026_u:245": "Asa_2.24 upāyānupalambhābhyāṃ śubhamūlānumodanā /",
  "SA14bsa026_u:246": "anumode manaskārabhāvaneha vidhīyate //",
  "SA14bsa026_u:247": "anāsravo bhāvanāmārgaḥ",
  "SA14bsa026_u:248": "10 - abhinirhāraḥ",
  "SA14bsa026_u:249": "Asa_2.25 svabhāvaḥ śreṣṭhatā tasya sarvasyānabhisaṃskṛtiḥ /",
  "SA14bsa026_u:250": "nopalambhena dharmāṇāmarpaṇā ca mahārthatā //",
  "SA14bsa026_u:251": "11 - atyantaviśuddhiḥ",
  "SA14bsa026_u:252": "Asa_2.26 buddhasevā ca dānādirupāye yacca kauśalam /",
  "SA14bsa026_u:253": "hetavo 'trādhimokṣasya dharmavyasanahetavaḥ //",
  "SA14bsa026_u:254": "Asa_2.27 mārādhiṣṭhānagambhīradharmatānadhimuktate /",
  "SA14bsa026_u:255": "skandhādyabhiniveśaśca pāpamitraparigrahaḥ //",
  "SA14bsa026_u:256": "Asa_2.28 phalaśuddhiśca rūpādiśuddhireva tayordvayoḥ /",
  "SA14bsa026_u:257": "abhinnācchinnatā yasmāditi śuddhirudīritā //",
  "SA14bsa026_u:258": "Asa_2.29 kleśajñeyatrimārgasya śiṣyakhaḍgajinaurasām /",
  "SA14bsa026_u:259": "hānād viśuddhirātyantikī tu buddhasya sarvathā //",
  "SA14bsa026_u:260": "Asa_2.30 mṛdumṛdvādiko mārgaḥ śuddhirnavasu bhūmiṣu /",
  "SA14bsa026_u:261": "adhimātrādhimātrādermalasya pratipakṣataḥ //",
  "SA14bsa026_u:262": "Asa_2.31 tridhātupratipakṣatvaṃ samatā mānameyayoḥ /",
  "SA14bsa026_u:263": "mārgasya ceṣyate tasya codyasya parihārataḥ //",
  "SA14bsa026_u:264": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:265": "prajñāpāramitopadeśaśāstre dvitīyādhikāraḥ /",
  "SA14bsa026_u:266": "3 Sarvajñatādhikāraḥ tṛtīyaḥ",
  "SA14bsa026_u:267": "1 - prajñayā na bhave sthānam",
  "SA14bsa026_u:268": "2 - kṛpayā na śame sthitiḥ",
  "SA14bsa026_u:269": "Asa_3.1 nāpare na pare tīre nāntarāle tayoḥ sthitā /",
  "SA14bsa026_u:270": "adhvanāṃ samatājñānāt prajñāpāramitā matā //",
  "SA14bsa026_u:271": "3 - anupāyena dūratvam",
  "SA14bsa026_u:272": "4 - upāyenāvidūratā",
  "SA14bsa026_u:273": "Asa_3.2 anupāyena dūraṃ sā sanimittopalambhataḥ /",
  "SA14bsa026_u:274": "upāyakauśalenāsyāḥ samyagāsannatoditā //",
  "SA14bsa026_u:275": "5 - vipakṣaḥ",
  "SA14bsa026_u:276": "Asa_3.3 rupādiskandhaśūnyatve dharmeṣu tryadhvageṣu ca /",
  "SA14bsa026_u:277": "dānādau bodhipakṣeṣu caryāsaṃjñā vipakṣatā //",
  "SA14bsa026_u:278": "6 - pratipakṣaḥ",
  "SA14bsa026_u:279": "Asa_3.4 dānādiṣvanahaṃkāraḥ pareṣāṃ tanniyojanam /",
  "SA14bsa026_u:280": "saṅgakoṭiniṣedho 'yaṃ sūkṣmaḥ saṅgo jinādiṣu //",
  "SA14bsa026_u:281": "Asa_3.5 tadgāmbhīrya prakṛtyaiva vivekād dharmapaddhateḥ /",
  "SA14bsa026_u:282": "ekaprakṛtikaṃ jñānaṃ dharmāṇāṃ saṅgavarjanam //",
  "SA14bsa026_u:283": "Asa_3.6 dṛṣṭādipratiṣedhena tasyā durbodhatoditā /",
  "SA14bsa026_u:284": "rūpādibhiravijñānāt tadacintyatvamiṣyate //",
  "SA14bsa026_u:285": "Asa_3.7 evaṃ kṛtvā yathokto vai jñeyaḥ sarvajñatānaye /",
  "SA14bsa026_u:286": "ayaṃ vibhāgo niḥśeṣo vipakṣapratipakṣayoḥ //",
  "SA14bsa026_u:287": "7 - prayogaḥ",
  "SA14bsa026_u:288": "Asa_3.8 rupādau tadanityādau tadapūriprapūrayoḥ /",
  "SA14bsa026_u:289": "tadasaṅgatve caryāyāḥ prayogaḥ pratiṣedhataḥ //",
  "SA14bsa026_u:290": "Asa_3.9 avikāro na karttā ca prayogo duṣkarastridhā /",
  "SA14bsa026_u:291": "yathābhavya phalaprāpteravandhyo 'bhimataśca saḥ //",
  "SA14bsa026_u:292": "aparapratyayo yaśca saptadhā khyātivedakaḥ /",
  "SA14bsa026_u:293": "8 - samatā",
  "SA14bsa026_u:294": "Asa_3.10 caturdhāmananā tasya rūpādau samatā matā //",
  "SA14bsa026_u:295": "9 - darśanamārgaḥ",
  "SA14bsa026_u:296": "Asa_3.11 dharmajñānānvayajñānakṣāntijñānakṣaṇātmakaḥ /",
  "SA14bsa026_u:297": "duḥkhādisatye dṛṅmārga eṣa sarvajñatānaya //",
  "SA14bsa026_u:298": "Asa_3.12 rūpaṃ na nityaṃ nānityamatītāntaṃ viśuddhakam /",
  "SA14bsa026_u:299": "anutpannāniruddhādi vyomābhaṃ lepavarjitam //",
  "SA14bsa026_u:300": "Asa_3.13 parigraheṇa nirmuktamavyāhāraṃ svabhāvataḥ /",
  "SA14bsa026_u:301": "pravyāhāreṇa nāsyārthaḥ pareṣu prāpyate yataḥ //",
  "SA14bsa026_u:302": "Asa_3.14 nopalambhakṛdatyantaviśuddhirvyādhyasambhavaḥ /",
  "SA14bsa026_u:303": "apāyocchittyakalpatve phalasākṣātkriyāṃ prati //",
  "SA14bsa026_u:304": "Asa_3.15 asaṃsargo nimittaiśca vastuni vyañjane dvaye /",
  "SA14bsa026_u:305": "jñānasya yā cānutpattiriti sarvajñatākṣaṇāḥ //",
  "SA14bsa026_u:306": "Asa_3.16 iti seyaṃ punaḥ seyaṃ seyaṃ khalu punastridhā /",
  "SA14bsa026_u:307": "adhikāratrayasyaiṣā samāptiḥ paridīpitā //",
  "SA14bsa026_u:308": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre tṛtīyādhikāraḥ /",
  "SA14bsa026_u:309": "4 Sarvākārābhisambodhādhikāraḥ caturthaḥ",
  "SA14bsa026_u:310": "1 - ākārāḥ",
  "SA14bsa026_u:311": "Asa_4.1 vastujñānaprakārāṇāmākārā iti lakṣaṇam /",
  "SA14bsa026_u:312": "sarvajñatānāṃ traividhyāt trividhā eva te matāḥ //",
  "SA14bsa026_u:313": "Asa_4.2 asadākāramārabhya yāvanniścalatākṛtiḥ /",
  "SA14bsa026_u:314": "catvāraḥ pratisatyaṃ te mārge pañcadaśa smṛtāḥ //",
  "SA14bsa026_u:315": "Asa_4.3 hetau mārge ca duḥkhe ca nirodhe ca yathākramam /",
  "SA14bsa026_u:316": "aṣṭau te sapta pañceti ṣoḍaśeti ca kīrtitāḥ //",
  "SA14bsa026_u:317": "Asa_4.4 smṛtyupasthānamārabhya buddhatvākārapaścimāḥ /",
  "SA14bsa026_u:318": "śiṣyāṇāṃ bodhisattvānāṃ buddhānāṃ ca yathākramam //",
  "SA14bsa026_u:319": "Asa_4.5 saptatriṃśaccatustriṃśattriṃśannava ca te matāḥ /",
  "SA14bsa026_u:320": "trisarvajñatvabhedena mārgasatyānurodhataḥ //",
  "SA14bsa026_u:321": "Asa_4.6 kṛtādhikārā buddheṣu teṣūptaśubhamūlakāḥ /",
  "SA14bsa026_u:322": "mitraiḥ sanāthāḥ kalyāṇairasyāḥ śravaṇabhājanam //",
  "SA14bsa026_u:323": "Asa_4.7 buddhopāsanasampraśnadānaśīlādicaryayā /",
  "SA14bsa026_u:324": "udgrahadhāraṇādīnāṃ bhājanatvaṃ satāṃ matam //",
  "SA14bsa026_u:325": "2 - prayogāḥ",
  "SA14bsa026_u:326": "Asa_4.8 rūpādiṣvanavasthānāt teṣu yoganiṣedhataḥ /",
  "SA14bsa026_u:327": "tattathatāgambhīratvāt teṣāṃ duravagāhataḥ //",
  "SA14bsa026_u:328": "Asa_4.9 tadaprāmāṇyataḥ kṛcchrāccireṇa pratibodhataḥ /",
  "SA14bsa026_u:329": "vyākṛtāvavivartyatve niryāṇe sanirantare //",
  "SA14bsa026_u:330": "Asa_4.10 āsannabodhe kṣipraṃ ca parārthe 'vṛddhayahānitaḥ /",
  "SA14bsa026_u:331": "dharmādharmādyadṛṣṭau ca rūpācintyādyadarśane //",
  "SA14bsa026_u:332": "Asa_4.11 rūpādestannimittasya tadbhāvasyāvikalpakaḥ /",
  "SA14bsa026_u:333": "phalaratnapradātā ca śuddhakaḥ sāvadhiśca saḥ //",
  "SA14bsa026_u:334": "3 - guṇāḥ",
  "SA14bsa026_u:335": "mārāṇāṃ śaktihānyādiścaturdaśavidho guṇaḥ /",
  "SA14bsa026_u:336": "4 - doṣāḥ",
  "SA14bsa026_u:337": "Asa_4.12 doṣāśca ṣaḍ viboddhavyāścaturbhirdaśakaiḥ saha //",
  "SA14bsa026_u:338": "5 - lakṣaṇāni",
  "SA14bsa026_u:339": "Asa_4.13 lakṣyate yena tajjñeyaṃ lakṣaṇaṃ trividhaṃ ca tat /",
  "SA14bsa026_u:340": "jñānaṃ viśeṣaḥ kāritraṃ svabhāvo yaśca lakṣyate //",
  "SA14bsa026_u:341": "Asa_4.14 tathāgatasya nirvṛttau loke cālujyanātmake /",
  "SA14bsa026_u:342": "sattvānāṃ cittacaryāsu tatsaṃkṣepe bahirgatau //",
  "SA14bsa026_u:343": "Asa_4.15 akṣayākāratāyāṃ ca sarāgādau pravistṛte /",
  "SA14bsa026_u:344": "mahadgate 'pramāṇe ca vijñāne cānidarśane //",
  "SA14bsa026_u:345": "Asa_4.16 adṛśyacittajñāne ca tadunmiñjādisaṃjñakam /",
  "SA14bsa026_u:346": "punastathatākāreṇa teṣāṃ jñānamataḥ param //",
  "SA14bsa026_u:347": "Asa_4.17 tathatāyāṃ munerbodhatatparākhyānamityayam /",
  "SA14bsa026_u:348": "sarvajñatādhikāreṇa jñānalakṣaṇasaṃgrahaḥ //",
  "SA14bsa026_u:349": "Asa_4.18 śūnyatve sānimitte ca praṇīdhānavivarjite /",
  "SA14bsa026_u:350": "anutpādānirodhādau dharmatāyā akopane //",
  "SA14bsa026_u:351": "Asa_4.19 asaṃskāre 'vikalpe ca prabhedālakṣaṇatvayoḥ /",
  "SA14bsa026_u:352": "mārgajñatādhikāreṇa jñānalakṣaṇamiṣyate //",
  "SA14bsa026_u:353": "Asa_4.20 svadharmamupaniśritya vihāre tasya satkṛtau /",
  "SA14bsa026_u:354": "gurutve mānanāyāṃ ca tatpūjākṛtakatvayoḥ //",
  "SA14bsa026_u:355": "Asa_4.21 sarvatra vṛttimajjñānamadṛṣṭasya ca darśakam /",
  "SA14bsa026_u:356": "lokasya śūnyatākāraśūcakajñāpakākṣagam //",
  "SA14bsa026_u:357": "Asa_4.22 acintyaśāntatādarśi lokasaṃjñanirodhi ca /",
  "SA14bsa026_u:358": "jñānalakṣaṇamityuktaṃ sarvākārajñatānaye //",
  "SA14bsa026_u:359": "Asa_4.23 acintyādiviśeṣeṇa viśiṣṭaiḥ satyagocaraiḥ /",
  "SA14bsa026_u:360": "viśeṣalakṣaṇaṃ ṣaḍbhirdaśabhiścoditaṃ kṣaṇaiḥ //",
  "SA14bsa026_u:361": "Asa_4.24 acintyātulyate meyasaṃkhyayoḥ samatikramau /",
  "SA14bsa026_u:362": "sarvāryasaṃgraho vijñavedyāsādhāraṇajñate //",
  "SA14bsa026_u:363": "Asa_4.25 kṣiprajñānyūnapūrṇatve pratipatsamudāgamau /",
  "SA14bsa026_u:364": "ālambanaṃ ca sādhāraṃ sākalyaṃ samparigrahaḥ //",
  "SA14bsa026_u:365": "Asa_4.26 anāsvādaśca vijñeyo viśeṣaḥ ṣoḍaśātmakaḥ /",
  "SA14bsa026_u:366": "viśeṣamārgo mārgebhyo yenānyebhyo viśiṣyate //",
  "SA14bsa026_u:367": "Asa_4.27 hitaṃ sukhaṃ ca trāṇaṃ ca śaraṇaṃ layanaṃ nṛṇām /",
  "SA14bsa026_u:368": "parāyaṇaṃ ca dvīpaṃ ca pariṇāyakasaṃjñakam //",
  "SA14bsa026_u:369": "Asa_4.28 anābhogaṃ tribhiryānaiḥ phalāsākṣātkriyātmakam /",
  "SA14bsa026_u:370": "paścimaṃ gatikāritramidaṃ kāritralakṣaṇam //",
  "SA14bsa026_u:371": "Asa_4.29 kleśaliṅganimittānāṃ vipakṣapratipakṣayoḥ /",
  "SA14bsa026_u:372": "viveko duṣkaraikāntāvuddeśo 'nupalambhakaḥ //",
  "SA14bsa026_u:373": "Asa_4.30 niṣiddhābhiniveśaśca yaścālambanasaṃjñakaḥ /",
  "SA14bsa026_u:374": "vipratyayo 'vighātī ca so 'padāgatyajātikaḥ //",
  "SA14bsa026_u:375": "Asa_4.31 tathatānupalambhaśca svabhāvaḥ ṣoḍaśātmakaḥ /",
  "SA14bsa026_u:376": "lakṣīva lakṣyate ceti caturthaṃ lakṣaṇaṃ matam //",
  "SA14bsa026_u:377": "6 - mokṣabhāgīyam",
  "SA14bsa026_u:378": "Asa_4.32 animittapradānādisamudāgamakauśalam /",
  "SA14bsa026_u:379": "sarvākārāvabodhe 'smin mokṣabhāgīyamiṣyate //",
  "SA14bsa026_u:380": "Asa_4.33 buddhādyālambanā śraddhā vīryaṃ dānādigocaram /",
  "SA14bsa026_u:381": "smṛtirāśayasampattiḥ samādhiravikalpanā //",
  "SA14bsa026_u:382": "Asa_4.34 dharmeṣu sarvairākārairjñānaṃ prajñeti pañcadhā /",
  "SA14bsa026_u:383": "tīkṣṇaiḥ subodhā sambodhirdurbodhā mṛdubhirmatā //",
  "SA14bsa026_u:384": "7 - virvedhabhāgīyam",
  "SA14bsa026_u:385": "Asa_4.35 ālambanaṃ sarvasattvā ūṣmaṇāmiha śasyate /",
  "SA14bsa026_u:386": "samacittādirākārasteṣveva daśadhoditaḥ //",
  "SA14bsa026_u:387": "Asa_4.36 svayaṃ pāpānnivṛttasya dānādyeṣu sthitasya ca /",
  "SA14bsa026_u:388": "tayorniyojanānyeṣāṃ varṇavādānukūlate //",
  "SA14bsa026_u:389": "Asa_4.37 mūrdhagaṃ svaparādhāraṃ satyajñānaṃ tathā kṣamā /",
  "SA14bsa026_u:390": "tathāgradharmā vijñeyāḥ sattvānāṃ pācanādibhiḥ //",
  "SA14bsa026_u:391": "8 - avaivartiko gaṇaḥ",
  "SA14bsa026_u:392": "Asa_4.38 nirvedhāṅgānyupādāya darśanābhyāsamārgayoḥ /",
  "SA14bsa026_u:393": "ye bodhisattvā vartante so 'trāvaivartiko gaṇaḥ //",
  "SA14bsa026_u:394": "Asa_4.39 rūpādibhyo nivṛttyādyairliṅgairviśatidheritaiḥ /",
  "SA14bsa026_u:395": "nirvedhāṅgaśitasyedamakvairtikalakṣaṇam //",
  "SA14bsa026_u:396": "Asa_4.40 rūpādibhyo nivṛttiśca vicikitsākṣaṇakṣayo /",
  "SA14bsa026_u:397": "ātmanaḥ kuśalasthasya pareṣāṃ tanniyojanam //",
  "SA14bsa026_u:398": "Asa_4.41 parādhāraṃ ca dānādi gambhīre 'rthe 'pyakāṃkṣaṇam /",
  "SA14bsa026_u:399": "maitraṃ kāyādyasaṃvāsaḥ paṃcadhāvaraṇena ca //",
  "SA14bsa026_u:400": "Asa_4.42 sarvānuśayahānaṃ ca smṛtisamprajñatā śuci /",
  "SA14bsa026_u:401": "cīvarādi śarīre ca kṛmīṇāmasamudbhavaḥ //",
  "SA14bsa026_u:402": "Asa_4.43 cittākauṭilyamādānaṃ dhūtasyāmatsarāditā /",
  "SA14bsa026_u:403": "dharmatāyuktagāmitvaṃ lokārthaṃ narakaiṣaṇā //",
  "SA14bsa026_u:404": "Asa_4.44 parairaneyatā mārasyānyamārgopadeśinaḥ /",
  "SA14bsa026_u:405": "māra ityeva bodhaśca caryā buddhānumoditā //",
  "SA14bsa026_u:406": "Asa_4.45 ūṣmamūrdhasu sakṣāntiṣvagradharmeṣvavasthitaḥ /",
  "SA14bsa026_u:407": "liṅgairamībhirviśatyā sambodherna nivartatte //",
  "SA14bsa026_u:408": "Asa_4.46 kṣāntijñānakṣaṇāḥ ṣaṭ ca pañca pañca ca dṛkpathe /",
  "SA14bsa026_u:409": "bodhisattvasya vijñeyamavaivartikalakṣaṇam //",
  "SA14bsa026_u:410": "Asa_4.47 rūpādisaṃjñāvyāvṛttirdāḍharyaṃ cittasya hīnayoḥ /",
  "SA14bsa026_u:411": "yānayorvinivṛttiśca dhyānādyaṅgaparikṣayaḥ //",
  "SA14bsa026_u:412": "Asa_4.48 kāyacetolaghutvaṃ ca kāmasevābhyupāyikī /",
  "SA14bsa026_u:413": "sadaiva brahmacāritvamājīvasya viśuddhatā //",
  "SA14bsa026_u:414": "Asa_4.49 skandhādāvantarāyeṣu sambhāre sendriyādike /",
  "SA14bsa026_u:415": "samare matsarādau ca neti yogānuyogayoḥ //",
  "SA14bsa026_u:416": "Asa_4.50 vihārapratiṣedhaśca dharmasyāṇoralabdhatā /",
  "SA14bsa026_u:417": "niścitattvaṃ svabhūmau ca bhūmitritayasaṃsthitiḥ //",
  "SA14bsa026_u:418": "Asa_4.51 dharmārthaḥ jīvitatyāga ityamī ṣoḍaśa kṣaṇāḥ /",
  "SA14bsa026_u:419": "avaivartikaliṅgāni dṛṅmārgasthasya dhīmataḥ //",
  "SA14bsa026_u:420": "Asa_4.52 gambhīro bhāvanāmārgo gāmbhīryaṃ śūnyatādikam /",
  "SA14bsa026_u:421": "samāropāpavādāntamuktatā sā gabhīratā //",
  "SA14bsa026_u:422": "Asa_4.53 cintātulananidhyānānyabhīkṣṇaṃ bhāvanāpathaḥ /",
  "SA14bsa026_u:423": "nirvedhāṅgeṣu dṛṅmārge bhāvanāmārga eva ca //",
  "SA14bsa026_u:424": "Asa_4.54 prābandhikatvādiṣṭo 'sau navadhā ca prakārataḥ /",
  "SA14bsa026_u:425": "mṛdumadhyādhimātrāṇāṃ punarmṛddhādibhedataḥ //",
  "SA14bsa026_u:426": "Asa_4.55 asaṃkhyeyādinirdeśāḥ paramārthena na kṣamāḥ /",
  "SA14bsa026_u:427": "kṛpāniṣyandabhūtāste saṃvṛtyābhimatā muneḥ //",
  "SA14bsa026_u:428": "Asa_4.56 hānivṛddhī na yujyete nirālāpasya vastunaḥ /",
  "SA14bsa026_u:429": "bhāvanākhyena kiṃ hīnaṃ vartmanā kimupadāgatam //",
  "SA14bsa026_u:430": "Asa_4.57 yathā bodhistathaivāsāviṣṭasyārthasya sādhakaḥ /",
  "SA14bsa026_u:431": "tathatālakṣaṇā bodhiḥ so 'pi tallakṣaṇo mataḥ //",
  "SA14bsa026_u:432": "Asa_4.58 pūrveṇa bodhirno yuktā manasā paścimena vā /",
  "SA14bsa026_u:433": "dīpadṛṣṭāntayogena gambhīrā dharmatāṣṭadhā //",
  "SA14bsa026_u:434": "Asa_4.59 utpāde ca nirodhe ca tathatāyāṃ gabhīratā /",
  "SA14bsa026_u:435": "jñeye jñāne ca caryāyāmadvayopāyakauśale //",
  "SA14bsa026_u:436": "9 - bhavaśāntyoḥ samatā",
  "SA14bsa026_u:437": "Asa_4.60 svapnopamatvād dharmāṇāṃ bhavaśāntyorakalpanā /",
  "SA14bsa026_u:438": "karmābhāvādicodyānāṃ parihārā yathoditāḥ //",
  "SA14bsa026_u:439": "10 - anuttarā kṣetraśuddhiḥ",
  "SA14bsa026_u:440": "Asa_4.61 sattvalokasya yāśuddhistasyāḥ śuddhyupahārataḥ /",
  "SA14bsa026_u:441": "tathā bhājanalokasya buddhakṣetrasya śuddhatā //",
  "SA14bsa026_u:442": "11 - upāyakauśalam",
  "SA14bsa026_u:443": "Asa_4.62 viṣayo 'sya prayogasya śātravāṇāmatikramaḥ /",
  "SA14bsa026_u:444": "apratiṣṭho yathāvedhamasādhāraṇalakṣaṇaḥ //",
  "SA14bsa026_u:445": "Asa_4.63 asakto 'nupalambhaśca nimittapranidhikṣataḥ /",
  "SA14bsa026_u:446": "talliṅgaṃ cāpramāṇaṃ ca daśadhopāyakauśalam //",
  "SA14bsa026_u:447": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre caturthādhikāraḥ //",
  "SA14bsa026_u:448": "Mūrdhābhisamayādhikāraḥ pañcamaḥ",
  "SA14bsa026_u:449": "1 - liṅgama(ūṣmā murdhaprayogaḥ)",
  "SA14bsa026_u:450": "Asa_5.1 svapnāntare 'pi svapnābhasarvadharmekṣaṇādikam /",
  "SA14bsa026_u:451": "mūrdhaprāptasya yogasya liṅgaṃ dvādaśadhā matam //",
  "SA14bsa026_u:452": "2 - vivṛddhiḥ(mūrdhā mūrdhaprayogaḥ)",
  "SA14bsa026_u:453": "Asa_5.2 jambudvipajaneyattābuddhapūjāśubhādikām /",
  "SA14bsa026_u:454": "upamāṃ bahudhā kṛtvā vivṛddhiḥ ṣoḍaśātmikā //",
  "SA14bsa026_u:455": "3 - nirudhiḥ(kṣāntiḥ mūrdhaprayogaḥ)",
  "SA14bsa026_u:456": "Asa_5.3 trisarvajñatvadharmāṇāṃ paripuriranuttarā /",
  "SA14bsa026_u:457": "aparityaktasattvārthā nirūḍhirabhidhīyate //",
  "SA14bsa026_u:458": "4 - cittasaṃsthitiḥ(agradharmākhyaḥ mūrdhaprayogaḥ)",
  "SA14bsa026_u:459": "Asa_5.4 caturdvīpakasāhasradvitrisāhasrakopamaḥ /",
  "SA14bsa026_u:460": "kṛtvā puṇyabahutvena samādhiḥ parikīrtitaḥ //",
  "SA14bsa026_u:461": "5 - darśanamārgaḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:462": "Asa_5.5 pravṛttau ca nivṛttau ca pratyekaṃ tau navātmakau /",
  "SA14bsa026_u:463": "grāhyau vikalpau vijñeyāvayathāviṣayātmakau //",
  "SA14bsa026_u:464": "Asa_5.6 dravyaprajñaptisatsattvavikalpau grāhakau matau /",
  "SA14bsa026_u:465": "pṛthagjanāryabhedena pratyekaṃ tau navātmakau //",
  "SA14bsa026_u:466": "Asa_5.7 grāhyau cenna tathā sto 'rthau kasya tau grāhakau matau /",
  "SA14bsa026_u:467": "iti grāhakabhāvena śūnyatālakṣaṇaṃ tayoḥ //",
  "SA14bsa026_u:468": "Asa_5.8 eṣa svabhāve gotre ca pratipatsamudāgame /",
  "SA14bsa026_u:469": "jñānasyālambanābhrāntau pratipakṣavipakṣayoḥ //",
  "SA14bsa026_u:470": "Asa_5.9 svasminnadhigame kartṛtatkāritrakriyāphale /",
  "SA14bsa026_u:471": "pravṛttipakṣādhiṣṭhāno vikalpo navadhā mataḥ //",
  "SA14bsa026_u:472": "Asa_5.10 bhavaśāntiprapātitvānnyūnatve 'dhigamasya ca /",
  "SA14bsa026_u:473": "parigrahasyābhāve ca vaikalye pratipadgate //",
  "SA14bsa026_u:474": "Asa_5.11 parapratyagāmitve samuddeśanivartane /",
  "SA14bsa026_u:475": "prādeśikatve nānātve sthānaprasthānamohayoḥ //",
  "SA14bsa026_u:476": "Asa_5.12 pṛṣṭhato gamane ceti vikalpo 'yaṃ navātmakaḥ /",
  "SA14bsa026_u:477": "nivṛttipakṣādhiṣṭhānaḥ śrāvakādimanobhavaḥ //",
  "SA14bsa026_u:478": "Asa_5.13 grāhakaḥ prathamo jñeyo grahaṇapratimokṣaṇe /",
  "SA14bsa026_u:479": "manaskriyāyāṃ dhātūnāmupaśleṣe trayasya ca //",
  "SA14bsa026_u:480": "Asa_5.14 sthāne cābhiniveśe ca prajñaptau dharmavastunaḥ /",
  "SA14bsa026_u:481": "saktau ca pratipakṣe ca yathecchaṃ ca gatikṣatau //",
  "SA14bsa026_u:482": "Asa_5.15 yathoddeśamaniryāṇe mārgāmārgāvadhāraṇe /",
  "SA14bsa026_u:483": "sanirodhe samutpāde vastuyogaviyogayoḥ //",
  "SA14bsa026_u:484": "Asa_5.16 sthāne gotrasya nāśe ca prārthanāhetvabhāvayoḥ /",
  "SA14bsa026_u:485": "pratyarthikopalambhe ca vikalpo grāhako 'paraḥ //",
  "SA14bsa026_u:486": "Asa_5.17 bodhau sandarśanānyeṣāṃ taddhetośca parīndanā /",
  "SA14bsa026_u:487": "tatprāptyanantaro hetuḥ puṇyabāhulyalakṣaṇaḥ //",
  "SA14bsa026_u:488": "Asa_5.18 kṣayānutpādayorjñāne malānāṃ bodhirucyate /",
  "SA14bsa026_u:489": "kṣayābhāvādanutpādātte hi jñeye yathākramam //",
  "SA14bsa026_u:490": "Asa_5.19 prakṛtāvaniruddhāyāṃ darśanākhyena vartmanā /",
  "SA14bsa026_u:491": "vikalpajātaṃ ki kṣīṇaṃ kiṃ vānatpattimāgatam //",
  "SA14bsa026_u:492": "Asa_5.20 sattā ca nāma dharmāṇāṃ jñeye cāvaraṇakṣayaḥ /",
  "SA14bsa026_u:493": "kathyate yatparaiḥ śāsturatra vismīyate mayā //",
  "SA14bsa026_u:494": "Asa_5.21 nāpaneyamataḥ kiñcit prakṣeptavyaṃ na kiṃcana /",
  "SA14bsa026_u:495": "draṣṭavyaṃ bhūtato bhūtaṃ bhūtadarśī vimucyate //",
  "SA14bsa026_u:496": "Asa_5.22 ekaikasyaiva dānādau teṣāṃ yaḥ saṃgraho mithaḥ /",
  "SA14bsa026_u:497": "sa ekakṣaṇikaḥ kṣāntisaṃgṛhīto 'tra dṛkpathaḥ //",
  "SA14bsa026_u:498": "Asa_5.23 sa samādhiṃ samāpadya tataḥ siṃhavijṛmbhitam /",
  "SA14bsa026_u:499": "anulomaṃ vilomaṃ ca pratītyotpādamīkṣate //",
  "SA14bsa026_u:500": "6 - bhāvanāmārgaḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:501": "Asa_5.24 kāmāptamabadhīkṛtya vijñānamasamāhitam /",
  "SA14bsa026_u:502": "sanirodhāḥ samapattīrgattvāgamya nava dvidhā //",
  "SA14bsa026_u:503": "Asa_5.25 ekadvitricatuḥpañcaṣaṭsaptāṣṭavyatikramāt /",
  "SA14bsa026_u:504": "avaskandasamāpattiranirodhamatulyatā //",
  "SA14bsa026_u:505": "Asa_5.26 saṃkṣepe vistare buddhaiḥ sānāthyenāparigrahe /",
  "SA14bsa026_u:506": "traikālike guṇābhāve śreyasastrividhe pathi //",
  "SA14bsa026_u:507": "Asa_5.27 eko grāhyavikalpo 'yaṃ prayogākāragocaraḥ /",
  "SA14bsa026_u:508": "dvitīyaścittacaittānāṃ pravṛttiviṣayo mataḥ //",
  "SA14bsa026_u:509": "Asa_5.28 anutpādastu cittasya bodhimaṇḍāmanaskriyā /",
  "SA14bsa026_u:510": "hīnayānamanaskārau sambodheramanaskṛtiḥ //",
  "SA14bsa026_u:511": "Asa_5.29 bhāvane 'bhāvane caiva tadviparyaya eva ca /",
  "SA14bsa026_u:512": "ayathārthaśca vijñeyo vikalpo bhāvanāpathe //",
  "SA14bsa026_u:513": "Asa_5.30 grāhakaḥ prathamo jñeyaḥ sattvaprajñaptigocaraḥ /",
  "SA14bsa026_u:514": "dharmapratyaśūnyatvasaktipravicayātmakaḥ //",
  "SA14bsa026_u:515": "Asa_5.31 kṛte ca vastuno yānatritaye ca sa kīrttitaḥ /",
  "SA14bsa026_u:516": "dakṣiṇāyā aśuddhau vā caryāyāśca vikopane //",
  "SA14bsa026_u:517": "Asa_5.32 sattvaprajñaptitaddhetuviṣayo navadhāparaḥ /",
  "SA14bsa026_u:518": "bhāvanāmārgasambaddho vipakṣastad vighātataḥ //",
  "SA14bsa026_u:519": "Asa_5.33 sarvajñatānāṃ tisṛṇāṃ yathāsvaṃ trividhāvṛttau /",
  "SA14bsa026_u:520": "śāntimārgatathatādisamprayogaviyogayoḥ //",
  "SA14bsa026_u:521": "Asa_5.34 asamatve ca duḥkhādau kleśānāṃ prakṛtāvapi /",
  "SA14bsa026_u:522": "dvayābhāve ca saṃmohe vikalpaḥ paścimo mataḥ //",
  "SA14bsa026_u:523": "Asa_5.35 āsāṃ kṣaye satītīnāṃ cirāyocchvasitā iva /",
  "SA14bsa026_u:524": "sarvākārajagatsaukhyasādhanā guṇasampadaḥ //",
  "SA14bsa026_u:525": "Asa_5.36 sarvāḥ sarvābhisāreṇa nikāmaphalaśālinam /",
  "SA14bsa026_u:526": "bhajante taṃ mahāsattvaṃ mahodadhimivāpagāḥ //",
  "SA14bsa026_u:527": "7 - ānantaryasamādhiḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:528": "Asa_5.37 trisāhasrajanaṃ śiṣyakhaḍgādhigamasampadi /",
  "SA14bsa026_u:529": "bodhisattvasya ca nyāme pratiṣṭhāpya śubhopamāḥ //",
  "SA14bsa026_u:530": "Asa_5.38 kṛtvā puṇyabahutvena buddhatvāpteranantaraḥ /",
  "SA14bsa026_u:531": "ānantaryasamādhiḥ sa sarvākārajñatā ca tat //",
  "SA14bsa026_u:532": "Asa_5.39 ālambanamabhāvo 'sya smṛtiścādhipatirmataḥ /",
  "SA14bsa026_u:533": "ākāraḥ śāntatā cātra jalpājalpipravādinām //",
  "SA14bsa026_u:534": "Asa_5.40 ālambanopapattau ca tatsvabhāvāvadhāraṇe /",
  "SA14bsa026_u:535": "sarvākarajñatājñāne paramārthe sasaṃvṛttau //",
  "SA14bsa026_u:536": "Asa_5.41 prayoge triṣu ratneṣu sopāye samaye muneḥ /",
  "SA14bsa026_u:537": "viparyāse samārge ca pratipakṣavipakṣayoḥ //",
  "SA14bsa026_u:538": "Asa_5.42 lakṣaṇe bhāvanāyāṃ ca matā vipratipattayaḥ /",
  "SA14bsa026_u:539": "sarvākārajñatādhārā ṣoḍhā daśa ca vādinām //",
  "SA14bsa026_u:540": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre paṃcamādhikāraḥ /",
  "SA14bsa026_u:541": "Ānupūrvikādhikāraḥ ṣaṣṭhaḥ",
  "SA14bsa026_u:542": "Asa_6.1 dānena prajñayā yāvad buddhādau smṛtibhiśca sā /",
  "SA14bsa026_u:543": "dharmābhāvasvabhāvenetyanupūrvakriyā matā //",
  "SA14bsa026_u:544": "ityabhisamāyālaṅkāre nāma prajñāpāramitopadeśaśāstre ṣaṣṭhādhikāraḥ /",
  "SA14bsa026_u:545": "Ekakṣaṇābhisambodhādhikāraḥ saptamaḥ",
  "SA14bsa026_u:546": "1 - avipākalalakṣaṇaḥ",
  "SA14bsa026_u:547": "Asa_7.1 anāsravāṇāṃ sarveṣāmekaikenāpi saṃgrahāt /",
  "SA14bsa026_u:548": "ekakṣaṇāvabodho 'yaṃ jñeyo dānādinā muneḥ //",
  "SA14bsa026_u:549": "Asa_7.2 araghaṭṭaṃ yathaikāpi padikā puruṣeritā /",
  "SA14bsa026_u:550": "sakṛt sarvaṃ cālayati jñānamekakṣaṇe tathā //",
  "SA14bsa026_u:551": "2 - vipākalakṣaṇaḥ",
  "SA14bsa026_u:552": "Asa_7.3 vipākadharmatāvasthā sarvaśuklamayī yadā /",
  "SA14bsa026_u:553": "prajñāpāramitā jātā jñānamekakṣaṇe tadā //",
  "SA14bsa026_u:554": "3 - alakṣaṇalakṣaṇaḥ",
  "SA14bsa026_u:555": "Asa_7.4 svapnopameṣu dharmeṣu sthitvā dānādicaryayā /",
  "SA14bsa026_u:556": "alakṣaṇatvaṃ dharmāṇāṃ kṣaṇenaikena vindati //",
  "SA14bsa026_u:557": "4 - advayalakṣaṇaḥ",
  "SA14bsa026_u:558": "Asa_7.5 svapnaṃ taddarśinaṃ caiva dvayayogena nekṣate /",
  "SA14bsa026_u:559": "dharmāṇāmadvayaṃ tattvaṃ kṣaṇenaikena paśyati //",
  "SA14bsa026_u:560": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:561": "prajñāpāramitopadeśaśāstre saptamādhikāraḥ /",
  "SA14bsa026_u:562": "Dharmakāyādhikāraḥ aṣṭamaḥ",
  "SA14bsa026_u:563": "1 - svabhāvakāyaḥ",
  "SA14bsa026_u:564": "Asa_8.1 sarvākārāṃ viśuddhiṃ ye dharmāḥ prāptā nirāsravāḥ /",
  "SA14bsa026_u:565": "svābhāviko muneḥ kāyasteṣāṃ prakṛtilakṣaṇaḥ //",
  "SA14bsa026_u:566": "2 - jñānadharmakāyaḥ",
  "SA14bsa026_u:567": "Asa_8.2 bodhipakṣāpramāṇāni vimokṣā anupūrvaśaḥ /",
  "SA14bsa026_u:568": "navātmikā samāpattiḥ kṛtsnaṃ daśavidhātmakam //",
  "SA14bsa026_u:569": "Asa_8.3 abhibhvāyatanānyaṣṭaprakārāṇi prabhedataḥ /",
  "SA14bsa026_u:570": "araṇā praṇidhijñānamabhijñāḥ pratisaṃvidaḥ //",
  "SA14bsa026_u:571": "Asa_8.4 sarvākārāścatastro 'tha śuddhayo vaśitā daśa /",
  "SA14bsa026_u:572": "balāni daśa catvāri vaiśāradyānyarakṣaṇam //",
  "SA14bsa026_u:573": "Asa_8.5 trividhaṃ smutyupasthānaṃ tridhāsaṃmoṣadharmatā /",
  "SA14bsa026_u:574": "vāsanāyāḥ samudghāto mahatī karuṇā jane //",
  "SA14bsa026_u:575": "Asa_8.6 āveṇikā munereva dharmā ye 'ṣṭādaśeritāḥ /",
  "SA14bsa026_u:576": "sarvākārajñatā ceti dharmakāyo 'bhidhīyate //",
  "SA14bsa026_u:577": "Asa_8.7 śrāvakasyāraṇādṛṣṭernṛkleśaparihāritā /",
  "SA14bsa026_u:578": "tatkleśastrotaucchittyai grāmādiṣu jināraṇā //",
  "SA14bsa026_u:579": "Asa_8.8 anābhogamanāsaṅgamavyāghātaṃ sadā sthitam /",
  "SA14bsa026_u:580": "sarvapraśnāpanud bauddhaṃ praṇidhijñānamiṣyate //",
  "SA14bsa026_u:581": "Asa_8.9 paripākaṃ gate hetau yasya yasya yadā yadā /",
  "SA14bsa026_u:582": "hitaṃ bhavati karttavyaṃ prathate tasya tasya saḥ //",
  "SA14bsa026_u:583": "Asa_8.10 varṣatyapi hi parjanye naiva bījaṃ prarohati /",
  "SA14bsa026_u:584": "samutpāde 'pi buddhānāṃ nābhavyo bhadramaśnute //",
  "SA14bsa026_u:585": "Asa_8.11 iti kāritravaipulyād buddho vyāpī nirucyate /",
  "SA14bsa026_u:586": "akṣayatvācca tasyaiva nitya ityapi kathyate //",
  "SA14bsa026_u:587": "3 - sambhogakāyaḥ",
  "SA14bsa026_u:588": "Asa_8.12 dvātriṃśallakṣaṇāśītivyañjanātmā munerayam /",
  "SA14bsa026_u:589": "sāmbhogiko mataḥ kāyo mahāyānopabhogataḥ //",
  "SA14bsa026_u:590": "Asa_8.13 cakrāṅkahastaḥ kramakūrmapādo jālāvanaddhāṅgulipāṇipādaḥ /",
  "SA14bsa026_u:591": "karau sapādau taruṇau mṛdū ca samutsadai saptabhirāśrayo 'sya //",
  "SA14bsa026_u:592": "Asa_8.14 dīrghāṅgulirvyāyatapārṣṇigātraṃ prājyaṃ tvṛjūcchaṅkhapadordhvaromā /",
  "SA14bsa026_u:593": "eṇeyajaṅghaśca paṭūrubāhuḥ kośāvanaddhottamabastiguhyaḥ //",
  "SA14bsa026_u:594": "Asa_8.15 suvarṇavarṇaḥ pratanūcchaviśca pradakṣiṇaikaikasujātaromā /",
  "SA14bsa026_u:595": "ūrṇāṅkitāsyo haripūrvakāyaḥ skandhau vṛtāvasya citāntarāṃsaḥ //",
  "SA14bsa026_u:596": "Asa_8.16 hīno rasaḥ khyāti rasottamo 'sya nyagrodhavanmaṇḍalatulyamūrtiḥ /",
  "SA14bsa026_u:597": "uṣṇīṣamūrdhā pṛthucārujivho brahmasvaraḥ siṃhahanuḥ suśuklāḥ //",
  "SA14bsa026_u:598": "Asa_8.17 tulyāḥ pramāṇe 'viralāśca dantā anyūnasaṃkhyā daśikāścatasraḥ /",
  "SA14bsa026_u:599": "nīlekṣaṇo govṛṣapakṣmanetro dvātriṃśadetāni hi lakṣaṇāni //",
  "SA14bsa026_u:600": "Asa_8.18 yasya yasyātra yo heturlakṣaṇasya prasādhakaḥ /",
  "SA14bsa026_u:601": "tasya tasya prapūryāyaṃ samudāgamalakṣaṇaḥ //",
  "SA14bsa026_u:602": "Asa_8.19 gurūṇāmanuyānādirdṛḍhatā saṃvaraṃ prati /",
  "SA14bsa026_u:603": "saṃgrahāsevanaṃ dānaṃ praṇītasya ca vastunaḥ //",
  "SA14bsa026_u:604": "Asa_8.20 vadhyamokṣasamādānaṃ vivṛddhiḥ kuśalasya ca /",
  "SA14bsa026_u:605": "ityādiko yathāsūtraṃ heturlakṣaṇasādhakaḥ //",
  "SA14bsa026_u:606": "Asa_8.21 tāmrāḥ snigdhāśca tuṅgāśca nakhā aṅgulayo muneḥ /",
  "SA14bsa026_u:607": "vṛttāścitānupūrvāśca gūḍhā nirgranthayaḥ śirāḥ //",
  "SA14bsa026_u:608": "Asa_8.22 gūḍhau gulphau samau pādau siṃhebhadvijagopateḥ /",
  "SA14bsa026_u:609": "vikrāntaṃ dakṣiṇaṃ cārugamanamṛjuvṛttatā //",
  "SA14bsa026_u:610": "Asa_8.23 muṣṭānupūrvate medhyamṛdutve śuddhagātratā /",
  "SA14bsa026_u:611": "pūrvavyañjanatā cārupṛthumaṇḍalagātratā //",
  "SA14bsa026_u:612": "Asa_8.24 samakramatvaṃ śuddhatvaṃ netrayoḥ sukumāratā /",
  "SA14bsa026_u:613": "adīnotsadagātratve susaṃhatanagātratā //",
  "SA14bsa026_u:614": "Asa_8.25 suvibhaktāṅgatā dhvāntapradhvastālokaśuddhatā /",
  "SA14bsa026_u:615": "vṛttamṛṣṭākṣatākṣāmakukṣitāśca gabhīratā //",
  "SA14bsa026_u:616": "Asa_8.26 dakṣiṇāvartatā nābheḥ samantād darśanīyatā /",
  "SA14bsa026_u:617": "samācāraḥ śuciḥ kālatilakāpagatā tanuḥ //",
  "SA14bsa026_u:618": "Asa_8.27 karau tulamṛdū snigdhagambhīrāyatalekhatā /",
  "SA14bsa026_u:619": "nātyāyataṃ vaco bimbapratibimbaupamauṣṭhatā //",
  "SA14bsa026_u:620": "Asa_8.28 mṛdvī tanvī ca raktā ca jivhā jīmūtaghoṣatā /",
  "SA14bsa026_u:621": "cārūmañjusvaro daṃṣṭrā vṛttāstīkṣṇāḥ sitāḥ samāḥ //",
  "SA14bsa026_u:622": "Asa_8.29 anupūrvī gatāstuṅgā nāsikā paramaṃ śuciḥ /",
  "SA14bsa026_u:623": "viśāle nayane pakṣmacitaṃ padmadalākṣitā //",
  "SA14bsa026_u:624": "Asa_8.30 āyataślakṣṇasusnigdhasamaromnau bhruvau bhujau /",
  "SA14bsa026_u:625": "pīnāyatau samau karṇāvupaghātavivarjitau //",
  "SA14bsa026_u:626": "Asa_8.31 lalāṭamaparimlānaṃ pṛthupūrṇottamāṅgatā /",
  "SA14bsa026_u:627": "bhrabharābhāścitāḥ ślakṣṇā asaṃlulitamurtayaḥ //",
  "SA14bsa026_u:628": "Asa_8.32 keśā aparuṣāḥ pusāṃ saurabhyādapahāriṇaḥ /",
  "SA14bsa026_u:629": "śrīvatsaḥ svastikaṃ ceti buddhānuvyañjanaṃ matam //",
  "SA14bsa026_u:630": "4 - nairmāṇikakāyaḥ",
  "SA14bsa026_u:631": "Asa_8.33 karoti yena citrāṇi hitāni jagataḥ samam /",
  "SA14bsa026_u:632": "ābhavāt so 'nupacchinnaḥ kāyo nairmāṇiko muneḥ //",
  "SA14bsa026_u:633": "Asa_8.34 buddhakāritrāṇi tathā karmāpyanucchinnamasyāsaṃsāramiṣyate /",
  "SA14bsa026_u:634": "gatīnāṃ śamanaṃ karma saṃgrahe ca caturvidhe //",
  "SA14bsa026_u:635": "Asa_8.35 niveśanaṃ sasaṃkleśe vyavadānāvabodhane /",
  "SA14bsa026_u:636": "sattvānāmarthayāthātmye ṣaṭsu pāramitāsu ca //",
  "SA14bsa026_u:637": "Asa_8.36 buddhamārge prakṛtyaiva śūnyatāyāṃ dvayakṣaye /",
  "SA14bsa026_u:638": "saṃkete 'nupalambhe ca paripāke ca dehinām //",
  "SA14bsa026_u:639": "Asa_8.37 bodhisattvasya mārge 'bhiniveśasya nivāraṇe /",
  "SA14bsa026_u:640": "bodhiprāptau jinakṣetraviśuddhau niyatiṃ prati //",
  "SA14bsa026_u:641": "Asa_8.38 aprameye ca sattvārthe buddhasevādike guṇe /",
  "SA14bsa026_u:642": "bodheraṅgeṣvanāśe ca karmaṇāṃ satyadarśane //",
  "SA14bsa026_u:643": "Asa_8.39 viparyāsaprahāṇe ca tadavastukatānaye /",
  "SA14bsa026_u:644": "vyavadāne sasambhāre saṃskṛtāsaṃskṛte prati //",
  "SA14bsa026_u:645": "Asa_8.40 vyatibhedāparijñāne nirvāṇe ca niveśanam /",
  "SA14bsa026_u:646": "dharmakāyasya karmedaṃ saptaviṃśatidhā matam //",
  "SA14bsa026_u:647": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre aṣṭamādhikāraḥ /"
}