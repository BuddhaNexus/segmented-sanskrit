{
  "SA14bsa026_u:0_0": "Abhisamayālaṅkāra (Asa)",
  "SA14bsa026_u:1_0": "namo mañjuśriye kumārabhūtāya",
  "SA14bsa026_u:2_0": "āryamaitreyanāthaviracitam abhisamayālaṅkāro nāma prajñāpāramitopadeśaśāstram",
  "SA14bsa026_u:3_0": "sarvākārajñātādhikāraḥ prathamaḥ",
  "SA14bsa026_u:4_0": "maṅgālācaraṇam",
  "SA14bsa026_u:4_1": "yā sarvajñatayā nayatyupaśamaṃ śāntaiṣiṇaḥ śrāvakān",
  "SA14bsa026_u:4_2": "yā mārgajñatayā jagaddhitakṛtāṃ lokārthasampādikā /",
  "SA14bsa026_u:4_3": "sarvākāramidaṃ vadanti munayo viśvaṃ yayā saṃgatās",
  "SA14bsa026_u:4_4": "tasyai śrāvakabodhisattvagaṇino buddhasya mātre namaḥ //",
  "SA14bsa026_u:asa_1.2_0": "grānthārambhaprayojanam sarvākārajñatāmārgaḥ śāsitrā yo 'tra deśitaḥ /",
  "SA14bsa026_u:asa_1.2_1": "dhīmanto vīkṣiṣiraṃstamanālīḍhaṃ parairiti //",
  "SA14bsa026_u:asa_1.3_0": "smṛtau cādhāya sūtrārthaṃ dharmacaryāṃ daśātmikām /",
  "SA14bsa026_u:asa_1.3_1": "sukhena pratipatsīrannityārambhaprayojanam //",
  "SA14bsa026_u:asa_1.4_0": "prajñāpāramitāṣṭābhiḥ padārthaiḥ samudīritā /",
  "SA14bsa026_u:asa_1.4_1": "sarvākārajñatā mārgajñatā sarvajñatā tataḥ //",
  "SA14bsa026_u:asa_1.5_0": "sarvākārābhisambodho mūrdhaprāpto 'nupūrvikaḥ /",
  "SA14bsa026_u:asa_1.5_1": "ekakṣaṇābhisambodho dharmakāyaśca te 'ṣṭadhā //",
  "SA14bsa026_u:asa_1.6_0": "cittotpādo 'vavādaśca nirvedhāṅgaṃ caturvidham /",
  "SA14bsa026_u:asa_1.6_1": "ādhāraḥ pratipatteśca dharmadhātusvabhāvakaḥ //",
  "SA14bsa026_u:asa_1.7_0": "ālambanaṃ samuddeśaḥ sannāhaprasthitikriye /",
  "SA14bsa026_u:asa_1.7_1": "sambhārāśca saniryāṇāḥ sarvākārajñatā muneḥ //",
  "SA14bsa026_u:asa_1.8_0": "dhyāmīkaraṇatādīni śiṣyakhaḍgapathau ca yau /",
  "SA14bsa026_u:asa_1.8_1": "mahānuśaṃso dṛṅmārga ehikāmutrikairguṇaiḥ //",
  "SA14bsa026_u:asa_1.9_0": "kāritramadhimuktiśca stutastobhitaśaṃsitāḥ /",
  "SA14bsa026_u:asa_1.9_1": "pariṇāme 'numode ca manaskārāvanuttamau //",
  "SA14bsa026_u:asa_1.10_0": "nirhāraḥ śuddhiratyantamityayaṃ bhāvanāpathaḥ /",
  "SA14bsa026_u:asa_1.10_1": "vijñānāṃ bodhisattvānāmiti mārgajñatoditā //",
  "SA14bsa026_u:asa_1.11_0": "prajñayā na bhave sthānaṃ kṛpayā na śame sthitiḥ /",
  "SA14bsa026_u:asa_1.11_1": "anupāyena dūratvam upāyenāvidūratā //",
  "SA14bsa026_u:asa_1.12_0": "vipakṣapratipakṣau ca prayogaḥ samatāsya ca /",
  "SA14bsa026_u:asa_1.12_1": "dṛṅmārgaḥ śrāvakādīnāmiti sarvajñateṣyate //",
  "SA14bsa026_u:asa_1.13_0": "ākārāḥ saprayogāśca guṇā doṣāḥ salakṣaṇāḥ /",
  "SA14bsa026_u:asa_1.13_1": "mokṣanirvedhabhāgīye śaikṣo 'vaivartiko gaṇaḥ //",
  "SA14bsa026_u:asa_1.14_0": "samatā bhavaśāntyośca kṣetraśuddhiranuttarā /",
  "SA14bsa026_u:asa_1.14_1": "sarvākārābhisambodha eṣa sopāyakauśalaḥ //",
  "SA14bsa026_u:asa_1.15_0": "liṅgaṃ tasya vivṛddhiśca nirūḍhiścittasaṃsthitiḥ /",
  "SA14bsa026_u:asa_1.15_1": "caturdhā ca vikalpasya pratipakṣaścaturvidhaḥ //",
  "SA14bsa026_u:asa_1.16_0": "pratyekaṃ darśanākhye ca bhāvanākhye ca vartmani /",
  "SA14bsa026_u:asa_1.16_1": "ānantaryasamādhiśca saha vipratipattibhiḥ //",
  "SA14bsa026_u:asa_1.17_0": "mūrdhābhisamayastredhā daśadhā cānupūrvikaḥ /",
  "SA14bsa026_u:asa_1.17_1": "ekakṣaṇābhisambodho lakṣaṇena caturvidhaḥ //",
  "SA14bsa026_u:asa_1.18_0": "svābhāvikaḥ sasāmbhogo nairmāṇiko 'parastathā /",
  "SA14bsa026_u:asa_1.18_1": "dharmakāyaḥ sakāritraścaturdhā samudīritaḥ //",
  "SA14bsa026_u:5_0": "sarvākārajñatā",
  "SA14bsa026_u:6_0": "1 - cittotpādaḥ",
  "SA14bsa026_u:asa_1.19_0": "cittotpādaḥ parārthāya samyaksambodhikāmatā /",
  "SA14bsa026_u:asa_1.19_1": "samāsavyāsataḥ sā ca yathāsūtraṃ sa cocyate //",
  "SA14bsa026_u:asa_1.20_0": "bhūhemacandrajvalanairnidhiratnākarārṇavaiḥ /",
  "SA14bsa026_u:asa_1.20_1": "vajrācalauṣadhīmitraiścintāmaṇyarkagītibhiḥ //",
  "SA14bsa026_u:asa_1.21_0": "nṛpagañjamahāmārgayānaprasravaṇodakaiḥ /",
  "SA14bsa026_u:asa_1.21_1": "ānandoktinadīmevairdvāviṃśatividhaḥ sa ca //",
  "SA14bsa026_u:7_0": "2 - avavādaḥ",
  "SA14bsa026_u:asa_1.22_0": "pratipattau ca satyeṣu buddharatnādiṣu triṣu /",
  "SA14bsa026_u:asa_1.22_1": "asaktāvapariśrānto pratipatsamparigrahe //",
  "SA14bsa026_u:asa_1.23_0": "cakṣuḥṣu pañcasu jñeyaḥ ṣaṭsvabhijñāguṇeṣu ca /",
  "SA14bsa026_u:asa_1.23_1": "dṛṅmārge bhāvanākhye cetyavavādo daśātmakaḥ //",
  "SA14bsa026_u:asa_1.24_0": "mṛdutīkṣṇendriyau śraddhādṛṣṭiprāptau kulaṅkalau /",
  "SA14bsa026_u:asa_1.24_1": "ekavīcyantarotpadya kārākārākaniṣṭhagāḥ //",
  "SA14bsa026_u:asa_1.25_0": "plutāstrayo bhavasyāgraparamo rūparāgahā /",
  "SA14bsa026_u:asa_1.25_1": "dṛṣṭadharmaśamaḥ kāyasākṣī khaḍgaśca viṃśatiḥ //",
  "SA14bsa026_u:8_0": "3 - nirvedhāṅgam",
  "SA14bsa026_u:asa_1.26_0": "ālambanata ākārāddhetutvāt samparigrahāt /",
  "SA14bsa026_u:asa_1.26_1": "caturvikalpasaṃyogaṃ yathāsvaṃ bhajatāṃ satām //",
  "SA14bsa026_u:asa_1.27_0": "śrāvakebhyaḥ sakhaḍgebhyo bodhisattvasya tāyinaḥ /",
  "SA14bsa026_u:asa_1.27_1": "mṛdumadhyādhimātrāṇāmūṣmādīnāṃ viśiṣṭatā //",
  "SA14bsa026_u:asa_1.28_0": "ālambanamanityādi satyādhāraṃ tadākṛtiḥ /",
  "SA14bsa026_u:asa_1.28_1": "niṣedho 'bhiniveśāderheturyānatrayāptaye //",
  "SA14bsa026_u:asa_1.29_0": "rūpādyāyavyayau viṣṭhāsthiti prajñaptyavācyate /",
  "SA14bsa026_u:asa_1.29_1": "rūpāvādavasthitisteṣāṃ tadbhāvenāsvabhāvatā //",
  "SA14bsa026_u:asa_1.30_0": "tayormithaḥ svabhāvatvaṃ tadanityādyasaṃsthitiḥ /",
  "SA14bsa026_u:asa_1.30_1": "tāsāṃ tadbhāvaśūnyatvaṃ mithaḥ svābhāvyametayoḥ //",
  "SA14bsa026_u:asa_1.31_0": "anudgraho yo dharmāṇāṃ tannimittāsamīkṣaṇam /",
  "SA14bsa026_u:asa_1.31_1": "parīkṣaṇaṃ ca prajñāyāḥ sarvasyānupalambhataḥ //",
  "SA14bsa026_u:asa_1.32_0": "rūpāderasvabhāvatvaṃ tadabhāvasvabhāvatā /",
  "SA14bsa026_u:asa_1.32_1": "tadajātiraniryāṇaṃ śuddhistadanimittatā //",
  "SA14bsa026_u:asa_1.33_0": "tannimittānadhiṣṭhānānadhimuktirasaṃjñatā /",
  "SA14bsa026_u:asa_1.33_1": "samādhistasya kāritraṃ vyākṛtirmananākṣayaḥ //",
  "SA14bsa026_u:asa_1.34_0": "mithastrikasya svābhāvyaṃ samādheravikalpanā /",
  "SA14bsa026_u:asa_1.34_1": "iti nirvedhabhāgīyaṃ mṛdumadhyādhimātrataḥ //",
  "SA14bsa026_u:asa_1.35_0": "dvaividhyaṃ grāhyakalpasya vastutatpratipakṣataḥ /",
  "SA14bsa026_u:asa_1.35_1": "moharāśyādibhedena pratyekaṃ navadhā tu saḥ //",
  "SA14bsa026_u:asa_1.36_0": "dravyaprajñaptyadhiṣṭhāno dvividho grāhako mataḥ /",
  "SA14bsa026_u:asa_1.36_1": "svatantrātmādirūpeṇa skandhādyāśrayatastathā //",
  "SA14bsa026_u:asa_1.37_0": "cittānavalīnatvādinaiḥsvābhāvyādideśakaḥ /",
  "SA14bsa026_u:asa_1.37_1": "tadvipakṣaparityāgaḥ sarvathā samparigrahaḥ //",
  "SA14bsa026_u:9_0": "4 - pratipatterādhāraḥ",
  "SA14bsa026_u:asa_1.38_0": "ṣoḍhādhigamadharmasya pratipakṣaprahāṇayoḥ /",
  "SA14bsa026_u:asa_1.38_1": "tayoḥ paryupayogasya prajñāyāḥ kṛpayā saha //",
  "SA14bsa026_u:asa_1.39_0": "śiṣyāsādhāraṇatvasya parārthānukramasya ca /",
  "SA14bsa026_u:asa_1.39_1": "jñānasyāyatnavṛtteśca pratiṣṭhā gotramucyate //",
  "SA14bsa026_u:asa_1.40_0": "dharmadhātorasambhedād gotrabhedo na yujyate /",
  "SA14bsa026_u:asa_1.40_1": "ādheyadharmabhedāttu tadbhedaḥ parigīyate //",
  "SA14bsa026_u:10_0": "5 - ālambanam",
  "SA14bsa026_u:asa_1.41_0": "ālambanaṃ sarvadharmāste punaḥ kuśalādayaḥ /",
  "SA14bsa026_u:asa_1.41_1": "laukikādhigamākhyāśca ye ca lokottarā matāḥ //",
  "SA14bsa026_u:asa_1.42_0": "sāsravānāsravā dharmāḥ saṃskṛtāsaṃskṛtāśca ye /",
  "SA14bsa026_u:asa_1.42_1": "śiṣyasādhāraṇā dharmā ye cāsādhāraṇā muneḥ //",
  "SA14bsa026_u:11_0": "6 - samuddeśaḥ",
  "SA14bsa026_u:asa_1.43_0": "sarvasattvāgratācittaprahāṇādhigamatraye /",
  "SA14bsa026_u:asa_1.43_1": "tribhirmahattvairuddeśo vijñeyo 'yaṃ svayambhuvām //",
  "SA14bsa026_u:12_0": "7 - sannāhapratipattiḥ",
  "SA14bsa026_u:asa_1.44_0": "dānādau ṣaḍvidhe teṣāṃ pratyekaṃ saṃgraheṇa yā /",
  "SA14bsa026_u:asa_1.44_1": "sannāhapratipattiḥ sā ṣaḍbhiḥ ṣaṭkairyathoditā //",
  "SA14bsa026_u:13_0": "8 - prasthānapratipattiḥ",
  "SA14bsa026_u:asa_1.45_0": "dhyānārūpyeṣu dānādau mārge maitryādikeṣu ca /",
  "SA14bsa026_u:asa_1.45_1": "gatopalambhayoge ca trimaṇḍalaviśuddhiṣu //",
  "SA14bsa026_u:asa_1.46_0": "uddeśe ṣaṭsvabhijñāsu sarvākārajñatānaye /",
  "SA14bsa026_u:asa_1.46_1": "prasthānapratipajjñeyā mahāyānādhirohiṇī //",
  "SA14bsa026_u:14_0": "9 - sambhārapratipattiḥ",
  "SA14bsa026_u:asa_1.47_0": "dayā dānādikaṃ ṣaṭkaṃ śamathaḥ savidarśanaḥ /",
  "SA14bsa026_u:asa_1.47_1": "yuganaddhaśca yo mārga upāye yacca kauśalam //",
  "SA14bsa026_u:asa_1.48_0": "jñānaṃ puṇyaṃ ca mārgaśca dhāraṇī bhūmayo daśa /",
  "SA14bsa026_u:asa_1.48_1": "pratipakṣaśca vijñeyaḥ sambhārapratipatkramaḥ //",
  "SA14bsa026_u:15_0": "1.",
  "SA14bsa026_u:15_1": "pramuditā bhūmiḥ",
  "SA14bsa026_u:asa_1.49_0": "labhyate prathamā bhūmirdaśadhā parikarmaṇā",
  "SA14bsa026_u:asa_1.49_1": "āśayo hitavastutvaṃ sattveṣu samacittatā //",
  "SA14bsa026_u:asa_1.50_0": "tyāgaḥ sevā ca mitrāṇāṃ saddharmālambanaiṣaṇā /",
  "SA14bsa026_u:asa_1.50_1": "sadā naiṣkramyacittatvaṃ buddhakāyagatā spṛhā //",
  "SA14bsa026_u:asa_1.51_0": "dharmasya deśanā satyaṃ daśamaṃ vākyamiṣyate /",
  "SA14bsa026_u:asa_1.51_1": "jñeyaṃ ca parikarmaiṣāṃ svabhāvānupalambhataḥ //",
  "SA14bsa026_u:16_0": "2.",
  "SA14bsa026_u:16_1": "vimalā bhūmiḥ",
  "SA14bsa026_u:asa_1.52_0": "śīlaṃ kṛtajñatā kṣāntiḥ prāmodyaṃ mahatī kṛpā /",
  "SA14bsa026_u:asa_1.52_1": "gauravaṃ guruśuśrūṣā vīryaṃ dānādike 'ṣṭamam //",
  "SA14bsa026_u:17_0": "3.",
  "SA14bsa026_u:17_1": "prabhākarī bhūmiḥ",
  "SA14bsa026_u:asa_1.53_0": "atṛptatā śrute dānaṃ dharmasya ca nirāmiṣam /",
  "SA14bsa026_u:asa_1.53_1": "buddhakṣetrasya saṃśuddhiḥ saṃsārāparikheditā //",
  "SA14bsa026_u:18_0": "hrīrapatrāpyamityetat pañcadhā mananātmakam /",
  "SA14bsa026_u:19_0": "4.",
  "SA14bsa026_u:19_1": "arciṣmatī bhūmiḥ",
  "SA14bsa026_u:asa_1.54_0": "vanāśālpecchatā tuṣṭirdhūtasaṃlekhasevanam //",
  "SA14bsa026_u:asa_1.55_0": "śikṣāyā aparityāgaḥ kāmānāṃ vijugupsanam /",
  "SA14bsa026_u:asa_1.55_1": "nirvitsarvāstisantyāgo 'navalīnānapekṣate //",
  "SA14bsa026_u:20_0": "5.",
  "SA14bsa026_u:20_1": "sudurjayā bhūmiḥ",
  "SA14bsa026_u:asa_1.56_0": "saṃstavaṃ kulamātsaryaṃ sthānaṃ saṅgaṇikāvaham / ātmotkarṣaparāvajñe",
  "SA14bsa026_u:asa_1.56_1": "karmamārgān daśāśubhān //",
  "SA14bsa026_u:asa_1.57_0": "mānaṃ stambhaṃ viparyāsaṃ vimatiṃ kleśamarṣaṇam /",
  "SA14bsa026_u:asa_1.57_1": "vivarjayan samāpnoti daśaitān pañcamī bhuvam //",
  "SA14bsa026_u:21_0": "6.",
  "SA14bsa026_u:21_1": "abhimukhī bhūmiḥ",
  "SA14bsa026_u:asa_1.58_0": "dānaśīlakṣamāvīryadhyānaprajñāprapūraṇāt /",
  "SA14bsa026_u:asa_1.58_1": "śiṣyakhaḍgaspṛhātrāsacetasāṃ parivarjakaḥ //",
  "SA14bsa026_u:asa_1.59_0": "yācito 'navalīnaśca sarvatyāge 'pyadurmanāḥ /",
  "SA14bsa026_u:asa_1.59_1": "kṛśo 'pi nārthināṃ kṣeptā ṣaṣṭhīṃ bhūmiṃ samaśnute //",
  "SA14bsa026_u:22_0": "7.",
  "SA14bsa026_u:22_1": "dūraṅgamā bhūmiḥ",
  "SA14bsa026_u:asa_1.60_0": "ātmasattvagraho jīvapudgalocchedaśāśvataḥ /",
  "SA14bsa026_u:asa_1.60_1": "nimittahetvoḥ skandheṣu dhātuṣvāyataneṣu ca //",
  "SA14bsa026_u:asa_1.61_0": "traidhātuke pratiṣṭhānaṃ saktirālīnacittatā /",
  "SA14bsa026_u:asa_1.61_1": "ratnatritayaśīleṣu taddṛṣṭayabhiniveśitā //",
  "SA14bsa026_u:asa_1.62_0": "śūnyatāyāṃ vivādaśca tadvirodhaśca viṃśatiḥ /",
  "SA14bsa026_u:asa_1.62_1": "kalaṅkā yasya vicchinnāḥ saptamīmetyasau bhuvam //",
  "SA14bsa026_u:asa_1.63_0": "trivimokṣamukhajñānaṃ trimaṇḍalaviśuddhatā /",
  "SA14bsa026_u:asa_1.63_1": "karuṇāmananā dharmasamataikanayajñatā //",
  "SA14bsa026_u:asa_1.64_0": "anutpādakṣamājñānaṃ dharmāṇāmekadheraṇā /",
  "SA14bsa026_u:asa_1.64_1": "kalpanāyāḥ samudghātaḥ saṃjñādṛkkleśavarjanam //",
  "SA14bsa026_u:asa_1.65_0": "śamathasya ca nidhyaptiḥ kauśalaṃ ca vidarśane /",
  "SA14bsa026_u:asa_1.65_1": "cittasya dāntatā jñānaṃ sarvatrapratighāti ca //",
  "SA14bsa026_u:asa_1.66_0": "sakterabhūmiryatrecchaṃ kṣetrāntaragatiḥ samam /",
  "SA14bsa026_u:asa_1.66_1": "sarvatra svātmabhāvasya darśanaṃ ceti viṃśatiḥ //",
  "SA14bsa026_u:23_0": "8.",
  "SA14bsa026_u:23_1": "acalā bhūmiḥ",
  "SA14bsa026_u:asa_1.67_0": "sarvasattvamanojñānamabhijñākrīḍanaṃ śubhā /",
  "SA14bsa026_u:asa_1.67_1": "buddhakṣetrasya niṣpattirbuddhasevāparīkṣaṇe //",
  "SA14bsa026_u:asa_1.68_0": "akṣajñānaṃ jinakṣetraśuddhirmāyopamā sthitiḥ /",
  "SA14bsa026_u:asa_1.68_1": "saṃcintya ca bhavādānamidaṃ karmāṣṭadhoditam //",
  "SA14bsa026_u:24_0": "9.",
  "SA14bsa026_u:24_1": "sādhumato bhūmiḥ",
  "SA14bsa026_u:asa_1.69_0": "praṇidhānānyanantāni devādīnāṃ rūtajñatā /",
  "SA14bsa026_u:asa_1.69_1": "nadīva pratibhānānāṃ garbhāvakrāntiruttamā //",
  "SA14bsa026_u:asa_1.70_0": "kulajātyośca gotraśca parivārasya janmanaḥ /",
  "SA14bsa026_u:asa_1.70_1": "naiṣkramyabodhivṛkṣāṇāṃ guṇapūre svasampadaḥ //",
  "SA14bsa026_u:25_0": "10.",
  "SA14bsa026_u:25_1": "dharmamedhā bhūmiḥ",
  "SA14bsa026_u:asa_1.71_0": "navabhūmīratikramya buddhabhūmau pratiṣṭhate /",
  "SA14bsa026_u:asa_1.71_1": "yena jñānena sā jñeyā daśamī bodhisattvabhūḥ //",
  "SA14bsa026_u:asa_1.72_0": "[pratipakṣasambhāraḥ] pratipakṣo 'ṣṭadhā jñeyo darśanābhyāsamārgayoḥ /",
  "SA14bsa026_u:asa_1.72_1": "grāhyagrāhakavikalpānāmaṣṭānāmupaśāntaye //",
  "SA14bsa026_u:26_0": "20 - niryāṇapratipattiḥ",
  "SA14bsa026_u:asa_1.73_0": "uddeśe samatāyāṃ ca sattvārthe yatnavarjane /",
  "SA14bsa026_u:asa_1.73_1": "atyantāya ca niryāṇaṃ niryāṇaṃ prāptilakṣaṇam //",
  "SA14bsa026_u:asa_1.74_0": "sarvākārajñatāyāṃ ca niryāṇaṃ mārgagocaram /",
  "SA14bsa026_u:asa_1.74_1": "niryāṇapratipajjñeyā seyamaṣṭavidhātmikā //",
  "SA14bsa026_u:27_0": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:28_0": "prajñāpāramitopadeśaśāstre prathamādhikāraḥ /",
  "SA14bsa026_u:29_0": "2 Mārgajñatādhikāraḥ dvitīyaḥ",
  "SA14bsa026_u:30_0": "1 - dhyāmīkaraṇatādīni",
  "SA14bsa026_u:asa_2.1_0": "dhyāmīkaraṇatā bhābhirdevānāṃ yogyatāṃ prati /",
  "SA14bsa026_u:asa_2.1_1": "viṣayo niyato vyāptiḥ svabhāvastasya karma ca //",
  "SA14bsa026_u:31_0": "2 - śrāvakamārgaḥ",
  "SA14bsa026_u:asa_2.2_0": "caturṇāmāryasatyānāmākārānupalambhataḥ /",
  "SA14bsa026_u:asa_2.2_1": "śrāvakāṇāmayaṃ mārgo jñeyo mārgajñatānaye //",
  "SA14bsa026_u:asa_2.3_0": "rūpādiskandhaśūnyatvācchūnyatānāmabhedataḥ /",
  "SA14bsa026_u:asa_2.3_1": "ūsmāṇau 'nupalambhena teṣāṃ mūrdhagataṃ matam //",
  "SA14bsa026_u:asa_2.4_0": "kṣāntayasteṣu nityādiyogasthānaniṣedhataḥ /",
  "SA14bsa026_u:asa_2.4_1": "daśa bhūmīḥ samārabhya vistarāsthānadeśanāt //",
  "SA14bsa026_u:asa_2.5_0": "agradharmagataṃ proktamāryaśrāvakartmani /",
  "SA14bsa026_u:asa_2.5_1": "tatkasya hetorbuddhena buddhvā dharmāsamīkṣaṇāt //",
  "SA14bsa026_u:32_0": "3 - pratyekabuddhamārgaḥ",
  "SA14bsa026_u:asa_2.6_0": "paropadeśavaiyarthyaṃ svayambodhāt svayambhuvām /",
  "SA14bsa026_u:asa_2.6_1": "gambhīratā ca jñānasya khaḍgānāmabhidhīyate //",
  "SA14bsa026_u:asa_2.7_0": "śuśrūṣā yasyā yasyārthe yatra yatra yathā yathā /",
  "SA14bsa026_u:asa_2.7_1": "sa so 'rthaḥ khyātyaśabdo 'pi tasya tasya tathā tathā //",
  "SA14bsa026_u:asa_2.8_0": "grāhyārthakalpanāhānād grāhakasyāprahāṇataḥ /",
  "SA14bsa026_u:asa_2.8_1": "ādhārataśca vijñeyaḥ khaḍgamārgasya saṃgrahaḥ //",
  "SA14bsa026_u:asa_2.9_0": "prajñapteravirodhena dharmatāsūcanākṛtiḥ /",
  "SA14bsa026_u:asa_2.9_1": "ūṣmagaṃ mūrdhagaṃ rūpādyahānādiprabhāvitam //",
  "SA14bsa026_u:asa_2.10_0": "adhyātmaśūnyatādyābhī rūpāderaparigrāhāt /",
  "SA14bsa026_u:asa_2.10_1": "kṣāntī rūpādyanutpādādyākārairagradharmatā //",
  "SA14bsa026_u:33_0": "4 - bodhisattvamārgaḥ",
  "SA14bsa026_u:asa_2.11_0": "kṣāntijñānakṣaṇaiḥ satyaṃ satyaṃ prati caturvidhaiḥ /",
  "SA14bsa026_u:asa_2.11_1": "mārgajñatāyāṃ dṛṅmārgaḥ sānuśaṃso 'yamucyate //",
  "SA14bsa026_u:asa_2.12_0": "ādhārādheyatābhāvāttathatābuddhayormithaḥ /",
  "SA14bsa026_u:asa_2.12_1": "paryāyeṇānanujñānaṃ mahattā sāpramāṇatā //",
  "SA14bsa026_u:asa_2.13_0": "parimāṇāntatābhāvo rūpāderavadhāraṇam /",
  "SA14bsa026_u:asa_2.13_1": "tasyāṃ sthitasya buddhatve 'nudgrahātyāgatādayaḥ //",
  "SA14bsa026_u:asa_2.14_0": "maitryādi śūnyatā prāptirbuddhatvasya parigrahaḥ /",
  "SA14bsa026_u:asa_2.14_1": "sarvasya vyavadānasya sarvādhivyādhiśātanam //",
  "SA14bsa026_u:asa_2.15_0": "nirvāṇagrāhaśāntatvaṃ buddhebhyo rakṣaṇādikam /",
  "SA14bsa026_u:asa_2.15_1": "aprāṇivadhamārabhya sarvākārajñatānaye //",
  "SA14bsa026_u:asa_2.16_0": "svayaṃ sthitasya sattvānāṃ sthāpanaṃ pariṇāmanam /",
  "SA14bsa026_u:asa_2.16_1": "dānādīnāṃ ca sambodhāviti mārgajñatākṣaṇāḥ //",
  "SA14bsa026_u:34_0": "5 - bhāvanāmārgakāritram",
  "SA14bsa026_u:asa_2.17_0": "sarvato damanaṃ nāma sarvataḥ kleśanirjayaḥ /",
  "SA14bsa026_u:asa_2.17_1": "upakramāviṣahyatvaṃ bodhirādhārapūjyatā //",
  "SA14bsa026_u:35_0": "sāsravo bhāvanāmārgaḥ",
  "SA14bsa026_u:36_0": "6 - bhāvanāmārgādhimuktiḥ",
  "SA14bsa026_u:asa_2.18_0": "adhimuktistridhā jñeyā svārthā ca svaparārthikā /",
  "SA14bsa026_u:asa_2.18_1": "parārthikaivetyeṣā ca pratyekaṃ trividheṣyate //",
  "SA14bsa026_u:asa_2.19_0": "mṛdvī madhyādhimātrā ca mṛdumṛdvādibhedataḥ /",
  "SA14bsa026_u:asa_2.19_1": "sā punastribidhetyevaṃ saptaviṃśatidhā matā //",
  "SA14bsa026_u:37_0": "7 - bhāvanāmārgādhimuktasya stutiḥ stobhaḥ praśaṃsā ca",
  "SA14bsa026_u:asa_2.20_0": "stutiḥ stobhaḥ praśaṃsā ca prajñāpāramitāṃ prati /",
  "SA14bsa026_u:asa_2.20_1": "adhimokṣasya mātrāṇāṃ navakaistribhiriṣyate //",
  "SA14bsa026_u:38_0": "8 - pariṇāmanā",
  "SA14bsa026_u:asa_2.21_0": "viśeṣaḥ pariṇāmastu tasya kāritramuttamam /",
  "SA14bsa026_u:asa_2.21_1": "nopalambhākṛtiścāsāvaviparyāsalakṣaṇaḥ //",
  "SA14bsa026_u:asa_2.22_0": "vivikto buddhapuṇyaughasvabhāvasmṛtigocaraḥ /",
  "SA14bsa026_u:asa_2.22_1": "sopāyaścānimittaśca buddhairabhyanumoditaḥ //",
  "SA14bsa026_u:asa_2.23_0": "traidhātukāprapannaśca pariṇāmo 'parastridhā /",
  "SA14bsa026_u:asa_2.23_1": "mṛdurmadhyo 'dhimātraśca mahāpuṇyodayātmakaḥ //",
  "SA14bsa026_u:39_0": "9 - anumodanā",
  "SA14bsa026_u:asa_2.24_0": "upāyānupalambhābhyāṃ śubhamūlānumodanā /",
  "SA14bsa026_u:asa_2.24_1": "anumode manaskārabhāvaneha vidhīyate //",
  "SA14bsa026_u:40_0": "anāsravo bhāvanāmārgaḥ",
  "SA14bsa026_u:41_0": "10 - abhinirhāraḥ",
  "SA14bsa026_u:asa_2.25_0": "svabhāvaḥ śreṣṭhatā tasya sarvasyānabhisaṃskṛtiḥ /",
  "SA14bsa026_u:asa_2.25_1": "nopalambhena dharmāṇāmarpaṇā ca mahārthatā //",
  "SA14bsa026_u:42_0": "11 - atyantaviśuddhiḥ",
  "SA14bsa026_u:asa_2.26_0": "buddhasevā ca dānādirupāye yacca kauśalam /",
  "SA14bsa026_u:asa_2.26_1": "hetavo 'trādhimokṣasya dharmavyasanahetavaḥ //",
  "SA14bsa026_u:asa_2.27_0": "mārādhiṣṭhānagambhīradharmatānadhimuktate /",
  "SA14bsa026_u:asa_2.27_1": "skandhādyabhiniveśaśca pāpamitraparigrahaḥ //",
  "SA14bsa026_u:asa_2.28_0": "phalaśuddhiśca rūpādiśuddhireva tayordvayoḥ /",
  "SA14bsa026_u:asa_2.28_1": "abhinnācchinnatā yasmāditi śuddhirudīritā //",
  "SA14bsa026_u:asa_2.29_0": "kleśajñeyatrimārgasya śiṣyakhaḍgajinaurasām /",
  "SA14bsa026_u:asa_2.29_1": "hānād viśuddhirātyantikī tu buddhasya sarvathā //",
  "SA14bsa026_u:asa_2.30_0": "mṛdumṛdvādiko mārgaḥ śuddhirnavasu bhūmiṣu /",
  "SA14bsa026_u:asa_2.30_1": "adhimātrādhimātrādermalasya pratipakṣataḥ //",
  "SA14bsa026_u:asa_2.31_0": "tridhātupratipakṣatvaṃ samatā mānameyayoḥ /",
  "SA14bsa026_u:asa_2.31_1": "mārgasya ceṣyate tasya codyasya parihārataḥ //",
  "SA14bsa026_u:43_0": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:43_1": "prajñāpāramitopadeśaśāstre dvitīyādhikāraḥ /",
  "SA14bsa026_u:44_0": "3 Sarvajñatādhikāraḥ tṛtīyaḥ",
  "SA14bsa026_u:45_0": "1 - prajñayā na bhave sthānam",
  "SA14bsa026_u:46_0": "2 - kṛpayā na śame sthitiḥ",
  "SA14bsa026_u:asa_3.1_0": "nāpare na pare tīre nāntarāle tayoḥ sthitā /",
  "SA14bsa026_u:asa_3.1_1": "adhvanāṃ samatājñānāt prajñāpāramitā matā //",
  "SA14bsa026_u:47_0": "3 - anupāyena dūratvam",
  "SA14bsa026_u:48_0": "4 - upāyenāvidūratā",
  "SA14bsa026_u:asa_3.2_0": "anupāyena dūraṃ sā sanimittopalambhataḥ /",
  "SA14bsa026_u:asa_3.2_1": "upāyakauśalenāsyāḥ samyagāsannatoditā //",
  "SA14bsa026_u:49_0": "5 - vipakṣaḥ",
  "SA14bsa026_u:asa_3.3_0": "rupādiskandhaśūnyatve dharmeṣu tryadhvageṣu ca /",
  "SA14bsa026_u:asa_3.3_1": "dānādau bodhipakṣeṣu caryāsaṃjñā vipakṣatā //",
  "SA14bsa026_u:50_0": "6 - pratipakṣaḥ",
  "SA14bsa026_u:asa_3.4_0": "dānādiṣvanahaṃkāraḥ pareṣāṃ tanniyojanam /",
  "SA14bsa026_u:asa_3.4_1": "saṅgakoṭiniṣedho 'yaṃ sūkṣmaḥ saṅgo jinādiṣu //",
  "SA14bsa026_u:asa_3.5_0": "tadgāmbhīrya prakṛtyaiva vivekād dharmapaddhateḥ /",
  "SA14bsa026_u:asa_3.5_1": "ekaprakṛtikaṃ jñānaṃ dharmāṇāṃ saṅgavarjanam //",
  "SA14bsa026_u:asa_3.6_0": "dṛṣṭādipratiṣedhena tasyā durbodhatoditā /",
  "SA14bsa026_u:asa_3.6_1": "rūpādibhiravijñānāt tadacintyatvamiṣyate //",
  "SA14bsa026_u:asa_3.7_0": "evaṃ kṛtvā yathokto vai jñeyaḥ sarvajñatānaye /",
  "SA14bsa026_u:asa_3.7_1": "ayaṃ vibhāgo niḥśeṣo vipakṣapratipakṣayoḥ //",
  "SA14bsa026_u:51_0": "7 - prayogaḥ",
  "SA14bsa026_u:asa_3.8_0": "rupādau tadanityādau tadapūriprapūrayoḥ /",
  "SA14bsa026_u:asa_3.8_1": "tadasaṅgatve caryāyāḥ prayogaḥ pratiṣedhataḥ //",
  "SA14bsa026_u:asa_3.9_0": "avikāro na karttā ca prayogo duṣkarastridhā /",
  "SA14bsa026_u:asa_3.9_1": "yathābhavya phalaprāpteravandhyo 'bhimataśca saḥ //",
  "SA14bsa026_u:52_0": "aparapratyayo yaśca saptadhā khyātivedakaḥ /",
  "SA14bsa026_u:53_0": "8 - samatā",
  "SA14bsa026_u:asa_3.10_0": "caturdhāmananā tasya rūpādau samatā matā //",
  "SA14bsa026_u:54_0": "9 - darśanamārgaḥ",
  "SA14bsa026_u:asa_3.11_0": "dharmajñānānvayajñānakṣāntijñānakṣaṇātmakaḥ /",
  "SA14bsa026_u:asa_3.11_1": "duḥkhādisatye dṛṅmārga eṣa sarvajñatānaya //",
  "SA14bsa026_u:asa_3.12_0": "rūpaṃ na nityaṃ nānityamatītāntaṃ viśuddhakam /",
  "SA14bsa026_u:asa_3.12_1": "anutpannāniruddhādi vyomābhaṃ lepavarjitam //",
  "SA14bsa026_u:asa_3.13_0": "parigraheṇa nirmuktamavyāhāraṃ svabhāvataḥ /",
  "SA14bsa026_u:asa_3.13_1": "pravyāhāreṇa nāsyārthaḥ pareṣu prāpyate yataḥ //",
  "SA14bsa026_u:asa_3.14_0": "nopalambhakṛdatyantaviśuddhirvyādhyasambhavaḥ /",
  "SA14bsa026_u:asa_3.14_1": "apāyocchittyakalpatve phalasākṣātkriyāṃ prati //",
  "SA14bsa026_u:asa_3.15_0": "asaṃsargo nimittaiśca vastuni vyañjane dvaye /",
  "SA14bsa026_u:asa_3.15_1": "jñānasya yā cānutpattiriti sarvajñatākṣaṇāḥ //",
  "SA14bsa026_u:asa_3.16_0": "iti seyaṃ punaḥ seyaṃ seyaṃ khalu punastridhā /",
  "SA14bsa026_u:asa_3.16_1": "adhikāratrayasyaiṣā samāptiḥ paridīpitā //",
  "SA14bsa026_u:55_0": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre tṛtīyādhikāraḥ /",
  "SA14bsa026_u:56_0": "4 Sarvākārābhisambodhādhikāraḥ caturthaḥ",
  "SA14bsa026_u:57_0": "1 - ākārāḥ",
  "SA14bsa026_u:asa_4.1_0": "vastujñānaprakārāṇāmākārā iti lakṣaṇam /",
  "SA14bsa026_u:asa_4.1_1": "sarvajñatānāṃ traividhyāt trividhā eva te matāḥ //",
  "SA14bsa026_u:asa_4.2_0": "asadākāramārabhya yāvanniścalatākṛtiḥ /",
  "SA14bsa026_u:asa_4.2_1": "catvāraḥ pratisatyaṃ te mārge pañcadaśa smṛtāḥ //",
  "SA14bsa026_u:asa_4.3_0": "hetau mārge ca duḥkhe ca nirodhe ca yathākramam /",
  "SA14bsa026_u:asa_4.3_1": "aṣṭau te sapta pañceti ṣoḍaśeti ca kīrtitāḥ //",
  "SA14bsa026_u:asa_4.4_0": "smṛtyupasthānamārabhya buddhatvākārapaścimāḥ /",
  "SA14bsa026_u:asa_4.4_1": "śiṣyāṇāṃ bodhisattvānāṃ buddhānāṃ ca yathākramam //",
  "SA14bsa026_u:asa_4.5_0": "saptatriṃśaccatustriṃśattriṃśannava ca te matāḥ /",
  "SA14bsa026_u:asa_4.5_1": "trisarvajñatvabhedena mārgasatyānurodhataḥ //",
  "SA14bsa026_u:asa_4.6_0": "kṛtādhikārā buddheṣu teṣūptaśubhamūlakāḥ /",
  "SA14bsa026_u:asa_4.6_1": "mitraiḥ sanāthāḥ kalyāṇairasyāḥ śravaṇabhājanam //",
  "SA14bsa026_u:asa_4.7_0": "buddhopāsanasampraśnadānaśīlādicaryayā /",
  "SA14bsa026_u:asa_4.7_1": "udgrahadhāraṇādīnāṃ bhājanatvaṃ satāṃ matam //",
  "SA14bsa026_u:58_0": "2 - prayogāḥ",
  "SA14bsa026_u:asa_4.8_0": "rūpādiṣvanavasthānāt teṣu yoganiṣedhataḥ /",
  "SA14bsa026_u:asa_4.8_1": "tattathatāgambhīratvāt teṣāṃ duravagāhataḥ //",
  "SA14bsa026_u:asa_4.9_0": "tadaprāmāṇyataḥ kṛcchrāccireṇa pratibodhataḥ /",
  "SA14bsa026_u:asa_4.9_1": "vyākṛtāvavivartyatve niryāṇe sanirantare //",
  "SA14bsa026_u:asa_4.10_0": "āsannabodhe kṣipraṃ ca parārthe 'vṛddhayahānitaḥ /",
  "SA14bsa026_u:asa_4.10_1": "dharmādharmādyadṛṣṭau ca rūpācintyādyadarśane //",
  "SA14bsa026_u:asa_4.11_0": "rūpādestannimittasya tadbhāvasyāvikalpakaḥ /",
  "SA14bsa026_u:asa_4.11_1": "phalaratnapradātā ca śuddhakaḥ sāvadhiśca saḥ //",
  "SA14bsa026_u:59_0": "3 - guṇāḥ",
  "SA14bsa026_u:60_0": "mārāṇāṃ śaktihānyādiścaturdaśavidho guṇaḥ /",
  "SA14bsa026_u:61_0": "4 - doṣāḥ",
  "SA14bsa026_u:asa_4.12_0": "doṣāśca ṣaḍ viboddhavyāścaturbhirdaśakaiḥ saha //",
  "SA14bsa026_u:62_0": "5 - lakṣaṇāni",
  "SA14bsa026_u:asa_4.13_0": "lakṣyate yena tajjñeyaṃ lakṣaṇaṃ trividhaṃ ca tat /",
  "SA14bsa026_u:asa_4.13_1": "jñānaṃ viśeṣaḥ kāritraṃ svabhāvo yaśca lakṣyate //",
  "SA14bsa026_u:asa_4.14_0": "tathāgatasya nirvṛttau loke cālujyanātmake /",
  "SA14bsa026_u:asa_4.14_1": "sattvānāṃ cittacaryāsu tatsaṃkṣepe bahirgatau //",
  "SA14bsa026_u:asa_4.15_0": "akṣayākāratāyāṃ ca sarāgādau pravistṛte /",
  "SA14bsa026_u:asa_4.15_1": "mahadgate 'pramāṇe ca vijñāne cānidarśane //",
  "SA14bsa026_u:asa_4.16_0": "adṛśyacittajñāne ca tadunmiñjādisaṃjñakam /",
  "SA14bsa026_u:asa_4.16_1": "punastathatākāreṇa teṣāṃ jñānamataḥ param //",
  "SA14bsa026_u:asa_4.17_0": "tathatāyāṃ munerbodhatatparākhyānamityayam /",
  "SA14bsa026_u:asa_4.17_1": "sarvajñatādhikāreṇa jñānalakṣaṇasaṃgrahaḥ //",
  "SA14bsa026_u:asa_4.18_0": "śūnyatve sānimitte ca praṇīdhānavivarjite /",
  "SA14bsa026_u:asa_4.18_1": "anutpādānirodhādau dharmatāyā akopane //",
  "SA14bsa026_u:asa_4.19_0": "asaṃskāre 'vikalpe ca prabhedālakṣaṇatvayoḥ /",
  "SA14bsa026_u:asa_4.19_1": "mārgajñatādhikāreṇa jñānalakṣaṇamiṣyate //",
  "SA14bsa026_u:asa_4.20_0": "svadharmamupaniśritya vihāre tasya satkṛtau /",
  "SA14bsa026_u:asa_4.20_1": "gurutve mānanāyāṃ ca tatpūjākṛtakatvayoḥ //",
  "SA14bsa026_u:asa_4.21_0": "sarvatra vṛttimajjñānamadṛṣṭasya ca darśakam /",
  "SA14bsa026_u:asa_4.21_1": "lokasya śūnyatākāraśūcakajñāpakākṣagam //",
  "SA14bsa026_u:asa_4.22_0": "acintyaśāntatādarśi lokasaṃjñanirodhi ca /",
  "SA14bsa026_u:asa_4.22_1": "jñānalakṣaṇamityuktaṃ sarvākārajñatānaye //",
  "SA14bsa026_u:asa_4.23_0": "acintyādiviśeṣeṇa viśiṣṭaiḥ satyagocaraiḥ /",
  "SA14bsa026_u:asa_4.23_1": "viśeṣalakṣaṇaṃ ṣaḍbhirdaśabhiścoditaṃ kṣaṇaiḥ //",
  "SA14bsa026_u:asa_4.24_0": "acintyātulyate meyasaṃkhyayoḥ samatikramau /",
  "SA14bsa026_u:asa_4.24_1": "sarvāryasaṃgraho vijñavedyāsādhāraṇajñate //",
  "SA14bsa026_u:asa_4.25_0": "kṣiprajñānyūnapūrṇatve pratipatsamudāgamau /",
  "SA14bsa026_u:asa_4.25_1": "ālambanaṃ ca sādhāraṃ sākalyaṃ samparigrahaḥ //",
  "SA14bsa026_u:asa_4.26_0": "anāsvādaśca vijñeyo viśeṣaḥ ṣoḍaśātmakaḥ /",
  "SA14bsa026_u:asa_4.26_1": "viśeṣamārgo mārgebhyo yenānyebhyo viśiṣyate //",
  "SA14bsa026_u:asa_4.27_0": "hitaṃ sukhaṃ ca trāṇaṃ ca śaraṇaṃ layanaṃ nṛṇām /",
  "SA14bsa026_u:asa_4.27_1": "parāyaṇaṃ ca dvīpaṃ ca pariṇāyakasaṃjñakam //",
  "SA14bsa026_u:asa_4.28_0": "anābhogaṃ tribhiryānaiḥ phalāsākṣātkriyātmakam /",
  "SA14bsa026_u:asa_4.28_1": "paścimaṃ gatikāritramidaṃ kāritralakṣaṇam //",
  "SA14bsa026_u:asa_4.29_0": "kleśaliṅganimittānāṃ vipakṣapratipakṣayoḥ /",
  "SA14bsa026_u:asa_4.29_1": "viveko duṣkaraikāntāvuddeśo 'nupalambhakaḥ //",
  "SA14bsa026_u:asa_4.30_0": "niṣiddhābhiniveśaśca yaścālambanasaṃjñakaḥ /",
  "SA14bsa026_u:asa_4.30_1": "vipratyayo 'vighātī ca so 'padāgatyajātikaḥ //",
  "SA14bsa026_u:asa_4.31_0": "tathatānupalambhaśca svabhāvaḥ ṣoḍaśātmakaḥ /",
  "SA14bsa026_u:asa_4.31_1": "lakṣīva lakṣyate ceti caturthaṃ lakṣaṇaṃ matam //",
  "SA14bsa026_u:63_0": "6 - mokṣabhāgīyam",
  "SA14bsa026_u:asa_4.32_0": "animittapradānādisamudāgamakauśalam /",
  "SA14bsa026_u:asa_4.32_1": "sarvākārāvabodhe 'smin mokṣabhāgīyamiṣyate //",
  "SA14bsa026_u:asa_4.33_0": "buddhādyālambanā śraddhā vīryaṃ dānādigocaram /",
  "SA14bsa026_u:asa_4.33_1": "smṛtirāśayasampattiḥ samādhiravikalpanā //",
  "SA14bsa026_u:asa_4.34_0": "dharmeṣu sarvairākārairjñānaṃ prajñeti pañcadhā /",
  "SA14bsa026_u:asa_4.34_1": "tīkṣṇaiḥ subodhā sambodhirdurbodhā mṛdubhirmatā //",
  "SA14bsa026_u:64_0": "7 - virvedhabhāgīyam",
  "SA14bsa026_u:asa_4.35_0": "ālambanaṃ sarvasattvā ūṣmaṇāmiha śasyate /",
  "SA14bsa026_u:asa_4.35_1": "samacittādirākārasteṣveva daśadhoditaḥ //",
  "SA14bsa026_u:asa_4.36_0": "svayaṃ pāpānnivṛttasya dānādyeṣu sthitasya ca /",
  "SA14bsa026_u:asa_4.36_1": "tayorniyojanānyeṣāṃ varṇavādānukūlate //",
  "SA14bsa026_u:asa_4.37_0": "mūrdhagaṃ svaparādhāraṃ satyajñānaṃ tathā kṣamā /",
  "SA14bsa026_u:asa_4.37_1": "tathāgradharmā vijñeyāḥ sattvānāṃ pācanādibhiḥ //",
  "SA14bsa026_u:65_0": "8 - avaivartiko gaṇaḥ",
  "SA14bsa026_u:asa_4.38_0": "nirvedhāṅgānyupādāya darśanābhyāsamārgayoḥ /",
  "SA14bsa026_u:asa_4.38_1": "ye bodhisattvā vartante so 'trāvaivartiko gaṇaḥ //",
  "SA14bsa026_u:asa_4.39_0": "rūpādibhyo nivṛttyādyairliṅgairviśatidheritaiḥ /",
  "SA14bsa026_u:asa_4.39_1": "nirvedhāṅgaśitasyedamakvairtikalakṣaṇam //",
  "SA14bsa026_u:asa_4.40_0": "rūpādibhyo nivṛttiśca vicikitsākṣaṇakṣayo /",
  "SA14bsa026_u:asa_4.40_1": "ātmanaḥ kuśalasthasya pareṣāṃ tanniyojanam //",
  "SA14bsa026_u:asa_4.41_0": "parādhāraṃ ca dānādi gambhīre 'rthe 'pyakāṃkṣaṇam /",
  "SA14bsa026_u:asa_4.41_1": "maitraṃ kāyādyasaṃvāsaḥ paṃcadhāvaraṇena ca //",
  "SA14bsa026_u:asa_4.42_0": "sarvānuśayahānaṃ ca smṛtisamprajñatā śuci /",
  "SA14bsa026_u:asa_4.42_1": "cīvarādi śarīre ca kṛmīṇāmasamudbhavaḥ //",
  "SA14bsa026_u:asa_4.43_0": "cittākauṭilyamādānaṃ dhūtasyāmatsarāditā /",
  "SA14bsa026_u:asa_4.43_1": "dharmatāyuktagāmitvaṃ lokārthaṃ narakaiṣaṇā //",
  "SA14bsa026_u:asa_4.44_0": "parairaneyatā mārasyānyamārgopadeśinaḥ /",
  "SA14bsa026_u:asa_4.44_1": "māra ityeva bodhaśca caryā buddhānumoditā //",
  "SA14bsa026_u:asa_4.45_0": "ūṣmamūrdhasu sakṣāntiṣvagradharmeṣvavasthitaḥ /",
  "SA14bsa026_u:asa_4.45_1": "liṅgairamībhirviśatyā sambodherna nivartatte //",
  "SA14bsa026_u:asa_4.46_0": "kṣāntijñānakṣaṇāḥ ṣaṭ ca pañca pañca ca dṛkpathe /",
  "SA14bsa026_u:asa_4.46_1": "bodhisattvasya vijñeyamavaivartikalakṣaṇam //",
  "SA14bsa026_u:asa_4.47_0": "rūpādisaṃjñāvyāvṛttirdāḍharyaṃ cittasya hīnayoḥ /",
  "SA14bsa026_u:asa_4.47_1": "yānayorvinivṛttiśca dhyānādyaṅgaparikṣayaḥ //",
  "SA14bsa026_u:asa_4.48_0": "kāyacetolaghutvaṃ ca kāmasevābhyupāyikī /",
  "SA14bsa026_u:asa_4.48_1": "sadaiva brahmacāritvamājīvasya viśuddhatā //",
  "SA14bsa026_u:asa_4.49_0": "skandhādāvantarāyeṣu sambhāre sendriyādike /",
  "SA14bsa026_u:asa_4.49_1": "samare matsarādau ca neti yogānuyogayoḥ //",
  "SA14bsa026_u:asa_4.50_0": "vihārapratiṣedhaśca dharmasyāṇoralabdhatā /",
  "SA14bsa026_u:asa_4.50_1": "niścitattvaṃ svabhūmau ca bhūmitritayasaṃsthitiḥ //",
  "SA14bsa026_u:asa_4.51_0": "dharmārthaḥ jīvitatyāga ityamī ṣoḍaśa kṣaṇāḥ /",
  "SA14bsa026_u:asa_4.51_1": "avaivartikaliṅgāni dṛṅmārgasthasya dhīmataḥ //",
  "SA14bsa026_u:asa_4.52_0": "gambhīro bhāvanāmārgo gāmbhīryaṃ śūnyatādikam /",
  "SA14bsa026_u:asa_4.52_1": "samāropāpavādāntamuktatā sā gabhīratā //",
  "SA14bsa026_u:asa_4.53_0": "cintātulananidhyānānyabhīkṣṇaṃ bhāvanāpathaḥ /",
  "SA14bsa026_u:asa_4.53_1": "nirvedhāṅgeṣu dṛṅmārge bhāvanāmārga eva ca //",
  "SA14bsa026_u:asa_4.54_0": "prābandhikatvādiṣṭo 'sau navadhā ca prakārataḥ /",
  "SA14bsa026_u:asa_4.54_1": "mṛdumadhyādhimātrāṇāṃ punarmṛddhādibhedataḥ //",
  "SA14bsa026_u:asa_4.55_0": "asaṃkhyeyādinirdeśāḥ paramārthena na kṣamāḥ /",
  "SA14bsa026_u:asa_4.55_1": "kṛpāniṣyandabhūtāste saṃvṛtyābhimatā muneḥ //",
  "SA14bsa026_u:asa_4.56_0": "hānivṛddhī na yujyete nirālāpasya vastunaḥ /",
  "SA14bsa026_u:asa_4.56_1": "bhāvanākhyena kiṃ hīnaṃ vartmanā kimupadāgatam //",
  "SA14bsa026_u:asa_4.57_0": "yathā bodhistathaivāsāviṣṭasyārthasya sādhakaḥ /",
  "SA14bsa026_u:asa_4.57_1": "tathatālakṣaṇā bodhiḥ so 'pi tallakṣaṇo mataḥ //",
  "SA14bsa026_u:asa_4.58_0": "pūrveṇa bodhirno yuktā manasā paścimena vā /",
  "SA14bsa026_u:asa_4.58_1": "dīpadṛṣṭāntayogena gambhīrā dharmatāṣṭadhā //",
  "SA14bsa026_u:asa_4.59_0": "utpāde ca nirodhe ca tathatāyāṃ gabhīratā /",
  "SA14bsa026_u:asa_4.59_1": "jñeye jñāne ca caryāyāmadvayopāyakauśale //",
  "SA14bsa026_u:66_0": "9 - bhavaśāntyoḥ samatā",
  "SA14bsa026_u:asa_4.60_0": "svapnopamatvād dharmāṇāṃ bhavaśāntyorakalpanā /",
  "SA14bsa026_u:asa_4.60_1": "karmābhāvādicodyānāṃ parihārā yathoditāḥ //",
  "SA14bsa026_u:67_0": "10 - anuttarā kṣetraśuddhiḥ",
  "SA14bsa026_u:asa_4.61_0": "sattvalokasya yāśuddhistasyāḥ śuddhyupahārataḥ /",
  "SA14bsa026_u:asa_4.61_1": "tathā bhājanalokasya buddhakṣetrasya śuddhatā //",
  "SA14bsa026_u:68_0": "11 - upāyakauśalam",
  "SA14bsa026_u:asa_4.62_0": "viṣayo 'sya prayogasya śātravāṇāmatikramaḥ /",
  "SA14bsa026_u:asa_4.62_1": "apratiṣṭho yathāvedhamasādhāraṇalakṣaṇaḥ //",
  "SA14bsa026_u:asa_4.63_0": "asakto 'nupalambhaśca nimittapranidhikṣataḥ /",
  "SA14bsa026_u:asa_4.63_1": "talliṅgaṃ cāpramāṇaṃ ca daśadhopāyakauśalam //",
  "SA14bsa026_u:69_0": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre caturthādhikāraḥ //",
  "SA14bsa026_u:70_0": "Mūrdhābhisamayādhikāraḥ pañcamaḥ",
  "SA14bsa026_u:71_0": "1 - liṅgama(ūṣmā murdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.1_0": "svapnāntare 'pi svapnābhasarvadharmekṣaṇādikam /",
  "SA14bsa026_u:asa_5.1_1": "mūrdhaprāptasya yogasya liṅgaṃ dvādaśadhā matam //",
  "SA14bsa026_u:72_0": "2 - vivṛddhiḥ(mūrdhā mūrdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.2_0": "jambudvipajaneyattābuddhapūjāśubhādikām /",
  "SA14bsa026_u:asa_5.2_1": "upamāṃ bahudhā kṛtvā vivṛddhiḥ ṣoḍaśātmikā //",
  "SA14bsa026_u:73_0": "3 - nirudhiḥ(kṣāntiḥ mūrdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.3_0": "trisarvajñatvadharmāṇāṃ paripuriranuttarā /",
  "SA14bsa026_u:asa_5.3_1": "aparityaktasattvārthā nirūḍhirabhidhīyate //",
  "SA14bsa026_u:74_0": "4 - cittasaṃsthitiḥ(agradharmākhyaḥ mūrdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.4_0": "caturdvīpakasāhasradvitrisāhasrakopamaḥ /",
  "SA14bsa026_u:asa_5.4_1": "kṛtvā puṇyabahutvena samādhiḥ parikīrtitaḥ //",
  "SA14bsa026_u:75_0": "5 - darśanamārgaḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.5_0": "pravṛttau ca nivṛttau ca pratyekaṃ tau navātmakau /",
  "SA14bsa026_u:asa_5.5_1": "grāhyau vikalpau vijñeyāvayathāviṣayātmakau //",
  "SA14bsa026_u:asa_5.6_0": "dravyaprajñaptisatsattvavikalpau grāhakau matau /",
  "SA14bsa026_u:asa_5.6_1": "pṛthagjanāryabhedena pratyekaṃ tau navātmakau //",
  "SA14bsa026_u:asa_5.7_0": "grāhyau cenna tathā sto 'rthau kasya tau grāhakau matau /",
  "SA14bsa026_u:asa_5.7_1": "iti grāhakabhāvena śūnyatālakṣaṇaṃ tayoḥ //",
  "SA14bsa026_u:asa_5.8_0": "eṣa svabhāve gotre ca pratipatsamudāgame /",
  "SA14bsa026_u:asa_5.8_1": "jñānasyālambanābhrāntau pratipakṣavipakṣayoḥ //",
  "SA14bsa026_u:asa_5.9_0": "svasminnadhigame kartṛtatkāritrakriyāphale /",
  "SA14bsa026_u:asa_5.9_1": "pravṛttipakṣādhiṣṭhāno vikalpo navadhā mataḥ //",
  "SA14bsa026_u:asa_5.10_0": "bhavaśāntiprapātitvānnyūnatve 'dhigamasya ca /",
  "SA14bsa026_u:asa_5.10_1": "parigrahasyābhāve ca vaikalye pratipadgate //",
  "SA14bsa026_u:asa_5.11_0": "parapratyagāmitve samuddeśanivartane /",
  "SA14bsa026_u:asa_5.11_1": "prādeśikatve nānātve sthānaprasthānamohayoḥ //",
  "SA14bsa026_u:asa_5.12_0": "pṛṣṭhato gamane ceti vikalpo 'yaṃ navātmakaḥ /",
  "SA14bsa026_u:asa_5.12_1": "nivṛttipakṣādhiṣṭhānaḥ śrāvakādimanobhavaḥ //",
  "SA14bsa026_u:asa_5.13_0": "grāhakaḥ prathamo jñeyo grahaṇapratimokṣaṇe /",
  "SA14bsa026_u:asa_5.13_1": "manaskriyāyāṃ dhātūnāmupaśleṣe trayasya ca //",
  "SA14bsa026_u:asa_5.14_0": "sthāne cābhiniveśe ca prajñaptau dharmavastunaḥ /",
  "SA14bsa026_u:asa_5.14_1": "saktau ca pratipakṣe ca yathecchaṃ ca gatikṣatau //",
  "SA14bsa026_u:asa_5.15_0": "yathoddeśamaniryāṇe mārgāmārgāvadhāraṇe /",
  "SA14bsa026_u:asa_5.15_1": "sanirodhe samutpāde vastuyogaviyogayoḥ //",
  "SA14bsa026_u:asa_5.16_0": "sthāne gotrasya nāśe ca prārthanāhetvabhāvayoḥ /",
  "SA14bsa026_u:asa_5.16_1": "pratyarthikopalambhe ca vikalpo grāhako 'paraḥ //",
  "SA14bsa026_u:asa_5.17_0": "bodhau sandarśanānyeṣāṃ taddhetośca parīndanā /",
  "SA14bsa026_u:asa_5.17_1": "tatprāptyanantaro hetuḥ puṇyabāhulyalakṣaṇaḥ //",
  "SA14bsa026_u:asa_5.18_0": "kṣayānutpādayorjñāne malānāṃ bodhirucyate /",
  "SA14bsa026_u:asa_5.18_1": "kṣayābhāvādanutpādātte hi jñeye yathākramam //",
  "SA14bsa026_u:asa_5.19_0": "prakṛtāvaniruddhāyāṃ darśanākhyena vartmanā /",
  "SA14bsa026_u:asa_5.19_1": "vikalpajātaṃ ki kṣīṇaṃ kiṃ vānatpattimāgatam //",
  "SA14bsa026_u:asa_5.20_0": "sattā ca nāma dharmāṇāṃ jñeye cāvaraṇakṣayaḥ /",
  "SA14bsa026_u:asa_5.20_1": "kathyate yatparaiḥ śāsturatra vismīyate mayā //",
  "SA14bsa026_u:asa_5.21_0": "nāpaneyamataḥ kiñcit prakṣeptavyaṃ na kiṃcana /",
  "SA14bsa026_u:asa_5.21_1": "draṣṭavyaṃ bhūtato bhūtaṃ bhūtadarśī vimucyate //",
  "SA14bsa026_u:asa_5.22_0": "ekaikasyaiva dānādau teṣāṃ yaḥ saṃgraho mithaḥ /",
  "SA14bsa026_u:asa_5.22_1": "sa ekakṣaṇikaḥ kṣāntisaṃgṛhīto 'tra dṛkpathaḥ //",
  "SA14bsa026_u:asa_5.23_0": "sa samādhiṃ samāpadya tataḥ siṃhavijṛmbhitam /",
  "SA14bsa026_u:asa_5.23_1": "anulomaṃ vilomaṃ ca pratītyotpādamīkṣate //",
  "SA14bsa026_u:76_0": "6 - bhāvanāmārgaḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.24_0": "kāmāptamabadhīkṛtya vijñānamasamāhitam /",
  "SA14bsa026_u:asa_5.24_1": "sanirodhāḥ samapattīrgattvāgamya nava dvidhā //",
  "SA14bsa026_u:asa_5.25_0": "ekadvitricatuḥpañcaṣaṭsaptāṣṭavyatikramāt /",
  "SA14bsa026_u:asa_5.25_1": "avaskandasamāpattiranirodhamatulyatā //",
  "SA14bsa026_u:asa_5.26_0": "saṃkṣepe vistare buddhaiḥ sānāthyenāparigrahe /",
  "SA14bsa026_u:asa_5.26_1": "traikālike guṇābhāve śreyasastrividhe pathi //",
  "SA14bsa026_u:asa_5.27_0": "eko grāhyavikalpo 'yaṃ prayogākāragocaraḥ /",
  "SA14bsa026_u:asa_5.27_1": "dvitīyaścittacaittānāṃ pravṛttiviṣayo mataḥ //",
  "SA14bsa026_u:asa_5.28_0": "anutpādastu cittasya bodhimaṇḍāmanaskriyā /",
  "SA14bsa026_u:asa_5.28_1": "hīnayānamanaskārau sambodheramanaskṛtiḥ //",
  "SA14bsa026_u:asa_5.29_0": "bhāvane 'bhāvane caiva tadviparyaya eva ca /",
  "SA14bsa026_u:asa_5.29_1": "ayathārthaśca vijñeyo vikalpo bhāvanāpathe //",
  "SA14bsa026_u:asa_5.30_0": "grāhakaḥ prathamo jñeyaḥ sattvaprajñaptigocaraḥ /",
  "SA14bsa026_u:asa_5.30_1": "dharmapratyaśūnyatvasaktipravicayātmakaḥ //",
  "SA14bsa026_u:asa_5.31_0": "kṛte ca vastuno yānatritaye ca sa kīrttitaḥ /",
  "SA14bsa026_u:asa_5.31_1": "dakṣiṇāyā aśuddhau vā caryāyāśca vikopane //",
  "SA14bsa026_u:asa_5.32_0": "sattvaprajñaptitaddhetuviṣayo navadhāparaḥ /",
  "SA14bsa026_u:asa_5.32_1": "bhāvanāmārgasambaddho vipakṣastad vighātataḥ //",
  "SA14bsa026_u:asa_5.33_0": "sarvajñatānāṃ tisṛṇāṃ yathāsvaṃ trividhāvṛttau /",
  "SA14bsa026_u:asa_5.33_1": "śāntimārgatathatādisamprayogaviyogayoḥ //",
  "SA14bsa026_u:asa_5.34_0": "asamatve ca duḥkhādau kleśānāṃ prakṛtāvapi /",
  "SA14bsa026_u:asa_5.34_1": "dvayābhāve ca saṃmohe vikalpaḥ paścimo mataḥ //",
  "SA14bsa026_u:asa_5.35_0": "āsāṃ kṣaye satītīnāṃ cirāyocchvasitā iva /",
  "SA14bsa026_u:asa_5.35_1": "sarvākārajagatsaukhyasādhanā guṇasampadaḥ //",
  "SA14bsa026_u:asa_5.36_0": "sarvāḥ sarvābhisāreṇa nikāmaphalaśālinam /",
  "SA14bsa026_u:asa_5.36_1": "bhajante taṃ mahāsattvaṃ mahodadhimivāpagāḥ //",
  "SA14bsa026_u:77_0": "7 - ānantaryasamādhiḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:asa_5.37_0": "trisāhasrajanaṃ śiṣyakhaḍgādhigamasampadi /",
  "SA14bsa026_u:asa_5.37_1": "bodhisattvasya ca nyāme pratiṣṭhāpya śubhopamāḥ //",
  "SA14bsa026_u:asa_5.38_0": "kṛtvā puṇyabahutvena buddhatvāpteranantaraḥ /",
  "SA14bsa026_u:asa_5.38_1": "ānantaryasamādhiḥ sa sarvākārajñatā ca tat //",
  "SA14bsa026_u:asa_5.39_0": "ālambanamabhāvo 'sya smṛtiścādhipatirmataḥ /",
  "SA14bsa026_u:asa_5.39_1": "ākāraḥ śāntatā cātra jalpājalpipravādinām //",
  "SA14bsa026_u:asa_5.40_0": "ālambanopapattau ca tatsvabhāvāvadhāraṇe /",
  "SA14bsa026_u:asa_5.40_1": "sarvākarajñatājñāne paramārthe sasaṃvṛttau //",
  "SA14bsa026_u:asa_5.41_0": "prayoge triṣu ratneṣu sopāye samaye muneḥ /",
  "SA14bsa026_u:asa_5.41_1": "viparyāse samārge ca pratipakṣavipakṣayoḥ //",
  "SA14bsa026_u:asa_5.42_0": "lakṣaṇe bhāvanāyāṃ ca matā vipratipattayaḥ /",
  "SA14bsa026_u:asa_5.42_1": "sarvākārajñatādhārā ṣoḍhā daśa ca vādinām //",
  "SA14bsa026_u:78_0": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre paṃcamādhikāraḥ /",
  "SA14bsa026_u:79_0": "Ānupūrvikādhikāraḥ ṣaṣṭhaḥ",
  "SA14bsa026_u:asa_6.1_0": "dānena prajñayā yāvad buddhādau smṛtibhiśca sā /",
  "SA14bsa026_u:asa_6.1_1": "dharmābhāvasvabhāvenetyanupūrvakriyā matā //",
  "SA14bsa026_u:80_0": "ityabhisamāyālaṅkāre nāma prajñāpāramitopadeśaśāstre ṣaṣṭhādhikāraḥ /",
  "SA14bsa026_u:81_0": "Ekakṣaṇābhisambodhādhikāraḥ saptamaḥ",
  "SA14bsa026_u:82_0": "1 - avipākalalakṣaṇaḥ",
  "SA14bsa026_u:asa_7.1_0": "anāsravāṇāṃ sarveṣāmekaikenāpi saṃgrahāt /",
  "SA14bsa026_u:asa_7.1_1": "ekakṣaṇāvabodho 'yaṃ jñeyo dānādinā muneḥ //",
  "SA14bsa026_u:asa_7.2_0": "araghaṭṭaṃ yathaikāpi padikā puruṣeritā /",
  "SA14bsa026_u:asa_7.2_1": "sakṛt sarvaṃ cālayati jñānamekakṣaṇe tathā //",
  "SA14bsa026_u:83_0": "2 - vipākalakṣaṇaḥ",
  "SA14bsa026_u:asa_7.3_0": "vipākadharmatāvasthā sarvaśuklamayī yadā /",
  "SA14bsa026_u:asa_7.3_1": "prajñāpāramitā jātā jñānamekakṣaṇe tadā //",
  "SA14bsa026_u:84_0": "3 - alakṣaṇalakṣaṇaḥ",
  "SA14bsa026_u:asa_7.4_0": "svapnopameṣu dharmeṣu sthitvā dānādicaryayā /",
  "SA14bsa026_u:asa_7.4_1": "alakṣaṇatvaṃ dharmāṇāṃ kṣaṇenaikena vindati //",
  "SA14bsa026_u:85_0": "4 - advayalakṣaṇaḥ",
  "SA14bsa026_u:asa_7.5_0": "svapnaṃ taddarśinaṃ caiva dvayayogena nekṣate /",
  "SA14bsa026_u:asa_7.5_1": "dharmāṇāmadvayaṃ tattvaṃ kṣaṇenaikena paśyati //",
  "SA14bsa026_u:86_0": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:86_1": "prajñāpāramitopadeśaśāstre saptamādhikāraḥ /",
  "SA14bsa026_u:87_0": "Dharmakāyādhikāraḥ aṣṭamaḥ",
  "SA14bsa026_u:88_0": "1 - svabhāvakāyaḥ",
  "SA14bsa026_u:asa_8.1_0": "sarvākārāṃ viśuddhiṃ ye dharmāḥ prāptā nirāsravāḥ /",
  "SA14bsa026_u:asa_8.1_1": "svābhāviko muneḥ kāyasteṣāṃ prakṛtilakṣaṇaḥ //",
  "SA14bsa026_u:89_0": "2 - jñānadharmakāyaḥ",
  "SA14bsa026_u:asa_8.2_0": "bodhipakṣāpramāṇāni vimokṣā anupūrvaśaḥ /",
  "SA14bsa026_u:asa_8.2_1": "navātmikā samāpattiḥ kṛtsnaṃ daśavidhātmakam //",
  "SA14bsa026_u:asa_8.3_0": "abhibhvāyatanānyaṣṭaprakārāṇi prabhedataḥ /",
  "SA14bsa026_u:asa_8.3_1": "araṇā praṇidhijñānamabhijñāḥ pratisaṃvidaḥ //",
  "SA14bsa026_u:asa_8.4_0": "sarvākārāścatastro 'tha śuddhayo vaśitā daśa /",
  "SA14bsa026_u:asa_8.4_1": "balāni daśa catvāri vaiśāradyānyarakṣaṇam //",
  "SA14bsa026_u:asa_8.5_0": "trividhaṃ smutyupasthānaṃ tridhāsaṃmoṣadharmatā /",
  "SA14bsa026_u:asa_8.5_1": "vāsanāyāḥ samudghāto mahatī karuṇā jane //",
  "SA14bsa026_u:asa_8.6_0": "āveṇikā munereva dharmā ye 'ṣṭādaśeritāḥ /",
  "SA14bsa026_u:asa_8.6_1": "sarvākārajñatā ceti dharmakāyo 'bhidhīyate //",
  "SA14bsa026_u:asa_8.7_0": "śrāvakasyāraṇādṛṣṭernṛkleśaparihāritā /",
  "SA14bsa026_u:asa_8.7_1": "tatkleśastrotaucchittyai grāmādiṣu jināraṇā //",
  "SA14bsa026_u:asa_8.8_0": "anābhogamanāsaṅgamavyāghātaṃ sadā sthitam /",
  "SA14bsa026_u:asa_8.8_1": "sarvapraśnāpanud bauddhaṃ praṇidhijñānamiṣyate //",
  "SA14bsa026_u:asa_8.9_0": "paripākaṃ gate hetau yasya yasya yadā yadā /",
  "SA14bsa026_u:asa_8.9_1": "hitaṃ bhavati karttavyaṃ prathate tasya tasya saḥ //",
  "SA14bsa026_u:asa_8.10_0": "varṣatyapi hi parjanye naiva bījaṃ prarohati /",
  "SA14bsa026_u:asa_8.10_1": "samutpāde 'pi buddhānāṃ nābhavyo bhadramaśnute //",
  "SA14bsa026_u:asa_8.11_0": "iti kāritravaipulyād buddho vyāpī nirucyate /",
  "SA14bsa026_u:asa_8.11_1": "akṣayatvācca tasyaiva nitya ityapi kathyate //",
  "SA14bsa026_u:90_0": "3 - sambhogakāyaḥ",
  "SA14bsa026_u:asa_8.12_0": "dvātriṃśallakṣaṇāśītivyañjanātmā munerayam /",
  "SA14bsa026_u:asa_8.12_1": "sāmbhogiko mataḥ kāyo mahāyānopabhogataḥ //",
  "SA14bsa026_u:asa_8.13_0": "cakrāṅkahastaḥ kramakūrmapādo jālāvanaddhāṅgulipāṇipādaḥ /",
  "SA14bsa026_u:asa_8.13_1": "karau sapādau taruṇau mṛdū ca samutsadai saptabhirāśrayo 'sya //",
  "SA14bsa026_u:asa_8.14_0": "dīrghāṅgulirvyāyatapārṣṇigātraṃ prājyaṃ tvṛjūcchaṅkhapadordhvaromā /",
  "SA14bsa026_u:asa_8.14_1": "eṇeyajaṅghaśca paṭūrubāhuḥ kośāvanaddhottamabastiguhyaḥ //",
  "SA14bsa026_u:asa_8.15_0": "suvarṇavarṇaḥ pratanūcchaviśca pradakṣiṇaikaikasujātaromā /",
  "SA14bsa026_u:asa_8.15_1": "ūrṇāṅkitāsyo haripūrvakāyaḥ skandhau vṛtāvasya citāntarāṃsaḥ //",
  "SA14bsa026_u:asa_8.16_0": "hīno rasaḥ khyāti rasottamo 'sya nyagrodhavanmaṇḍalatulyamūrtiḥ /",
  "SA14bsa026_u:asa_8.16_1": "uṣṇīṣamūrdhā pṛthucārujivho brahmasvaraḥ siṃhahanuḥ suśuklāḥ //",
  "SA14bsa026_u:asa_8.17_0": "tulyāḥ pramāṇe 'viralāśca dantā anyūnasaṃkhyā daśikāścatasraḥ /",
  "SA14bsa026_u:asa_8.17_1": "nīlekṣaṇo govṛṣapakṣmanetro dvātriṃśadetāni hi lakṣaṇāni //",
  "SA14bsa026_u:asa_8.18_0": "yasya yasyātra yo heturlakṣaṇasya prasādhakaḥ /",
  "SA14bsa026_u:asa_8.18_1": "tasya tasya prapūryāyaṃ samudāgamalakṣaṇaḥ //",
  "SA14bsa026_u:asa_8.19_0": "gurūṇāmanuyānādirdṛḍhatā saṃvaraṃ prati /",
  "SA14bsa026_u:asa_8.19_1": "saṃgrahāsevanaṃ dānaṃ praṇītasya ca vastunaḥ //",
  "SA14bsa026_u:asa_8.20_0": "vadhyamokṣasamādānaṃ vivṛddhiḥ kuśalasya ca /",
  "SA14bsa026_u:asa_8.20_1": "ityādiko yathāsūtraṃ heturlakṣaṇasādhakaḥ //",
  "SA14bsa026_u:asa_8.21_0": "tāmrāḥ snigdhāśca tuṅgāśca nakhā aṅgulayo muneḥ /",
  "SA14bsa026_u:asa_8.21_1": "vṛttāścitānupūrvāśca gūḍhā nirgranthayaḥ śirāḥ //",
  "SA14bsa026_u:asa_8.22_0": "gūḍhau gulphau samau pādau siṃhebhadvijagopateḥ /",
  "SA14bsa026_u:asa_8.22_1": "vikrāntaṃ dakṣiṇaṃ cārugamanamṛjuvṛttatā //",
  "SA14bsa026_u:asa_8.23_0": "muṣṭānupūrvate medhyamṛdutve śuddhagātratā /",
  "SA14bsa026_u:asa_8.23_1": "pūrvavyañjanatā cārupṛthumaṇḍalagātratā //",
  "SA14bsa026_u:asa_8.24_0": "samakramatvaṃ śuddhatvaṃ netrayoḥ sukumāratā /",
  "SA14bsa026_u:asa_8.24_1": "adīnotsadagātratve susaṃhatanagātratā //",
  "SA14bsa026_u:asa_8.25_0": "suvibhaktāṅgatā dhvāntapradhvastālokaśuddhatā /",
  "SA14bsa026_u:asa_8.25_1": "vṛttamṛṣṭākṣatākṣāmakukṣitāśca gabhīratā //",
  "SA14bsa026_u:asa_8.26_0": "dakṣiṇāvartatā nābheḥ samantād darśanīyatā /",
  "SA14bsa026_u:asa_8.26_1": "samācāraḥ śuciḥ kālatilakāpagatā tanuḥ //",
  "SA14bsa026_u:asa_8.27_0": "karau tulamṛdū snigdhagambhīrāyatalekhatā /",
  "SA14bsa026_u:asa_8.27_1": "nātyāyataṃ vaco bimbapratibimbaupamauṣṭhatā //",
  "SA14bsa026_u:asa_8.28_0": "mṛdvī tanvī ca raktā ca jivhā jīmūtaghoṣatā /",
  "SA14bsa026_u:asa_8.28_1": "cārūmañjusvaro daṃṣṭrā vṛttāstīkṣṇāḥ sitāḥ samāḥ //",
  "SA14bsa026_u:asa_8.29_0": "anupūrvī gatāstuṅgā nāsikā paramaṃ śuciḥ /",
  "SA14bsa026_u:asa_8.29_1": "viśāle nayane pakṣmacitaṃ padmadalākṣitā //",
  "SA14bsa026_u:asa_8.30_0": "āyataślakṣṇasusnigdhasamaromnau bhruvau bhujau /",
  "SA14bsa026_u:asa_8.30_1": "pīnāyatau samau karṇāvupaghātavivarjitau //",
  "SA14bsa026_u:asa_8.31_0": "lalāṭamaparimlānaṃ pṛthupūrṇottamāṅgatā /",
  "SA14bsa026_u:asa_8.31_1": "bhrabharābhāścitāḥ ślakṣṇā asaṃlulitamurtayaḥ //",
  "SA14bsa026_u:asa_8.32_0": "keśā aparuṣāḥ pusāṃ saurabhyādapahāriṇaḥ /",
  "SA14bsa026_u:asa_8.32_1": "śrīvatsaḥ svastikaṃ ceti buddhānuvyañjanaṃ matam //",
  "SA14bsa026_u:91_0": "4 - nairmāṇikakāyaḥ",
  "SA14bsa026_u:asa_8.33_0": "karoti yena citrāṇi hitāni jagataḥ samam /",
  "SA14bsa026_u:asa_8.33_1": "ābhavāt so 'nupacchinnaḥ kāyo nairmāṇiko muneḥ //",
  "SA14bsa026_u:asa_8.34_0": "buddhakāritrāṇi tathā karmāpyanucchinnamasyāsaṃsāramiṣyate /",
  "SA14bsa026_u:asa_8.34_1": "gatīnāṃ śamanaṃ karma saṃgrahe ca caturvidhe //",
  "SA14bsa026_u:asa_8.35_0": "niveśanaṃ sasaṃkleśe vyavadānāvabodhane /",
  "SA14bsa026_u:asa_8.35_1": "sattvānāmarthayāthātmye ṣaṭsu pāramitāsu ca //",
  "SA14bsa026_u:asa_8.36_0": "buddhamārge prakṛtyaiva śūnyatāyāṃ dvayakṣaye /",
  "SA14bsa026_u:asa_8.36_1": "saṃkete 'nupalambhe ca paripāke ca dehinām //",
  "SA14bsa026_u:asa_8.37_0": "bodhisattvasya mārge 'bhiniveśasya nivāraṇe /",
  "SA14bsa026_u:asa_8.37_1": "bodhiprāptau jinakṣetraviśuddhau niyatiṃ prati //",
  "SA14bsa026_u:asa_8.38_0": "aprameye ca sattvārthe buddhasevādike guṇe /",
  "SA14bsa026_u:asa_8.38_1": "bodheraṅgeṣvanāśe ca karmaṇāṃ satyadarśane //",
  "SA14bsa026_u:asa_8.39_0": "viparyāsaprahāṇe ca tadavastukatānaye /",
  "SA14bsa026_u:asa_8.39_1": "vyavadāne sasambhāre saṃskṛtāsaṃskṛte prati //",
  "SA14bsa026_u:asa_8.40_0": "vyatibhedāparijñāne nirvāṇe ca niveśanam /",
  "SA14bsa026_u:asa_8.40_1": "dharmakāyasya karmedaṃ saptaviṃśatidhā matam //",
  "SA14bsa026_u:92_0": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre aṣṭamādhikāraḥ /"
}