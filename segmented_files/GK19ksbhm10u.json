{
  "GK19ksbhm10u:0": "//gadāparva//",
  "GK19ksbhm10u:1": "nārāyaṇaṃ namaskṛtya naraṃ caiva narottamam /",
  "GK19ksbhm10u:2": "devīṃ sarasvatīṃ vyāsaṃ tato jayamudīrayet // Bhmj_10.1 //",
  "GK19ksbhm10u:3": "tato drauṇimukhā vīrāḥ samabhyetya rathāstrayaḥ /",
  "GK19ksbhm10u:4": "hradaṃ praviṣṭaṃ rājānamūcurmānadhanaṃ śaraiḥ // Bhmj_10.2 //",
  "GK19ksbhm10u:5": "rājannuttiṣṭha saṃgrāme jahi śatruṃ dhanurdhara /",
  "GK19ksbhm10u:6": "sahāsmābhirayaṃ kālo vikramasya yaśobhuvaḥ // Bhmj_10.3 //",
  "GK19ksbhm10u:7": "iti śrutvā kurupatiḥ praśaṃsaṃstatparākramam /",
  "GK19ksbhm10u:8": "ūce prātaḥ samāśvāsya mama kṣatrocitā gatiḥ // Bhmj_10.4 //",
  "GK19ksbhm10u:9": "iti teṣāṃ samālāpaṃ śrutvā jñātvā suyodhanam /",
  "GK19ksbhm10u:10": "śaśaṃsurbhīmasenāya lubdhakā dhanalubdhakāḥ // Bhmj_10.5 //",
  "GK19ksbhm10u:11": "bhīmasenagirā rājā sasainyo 'tha yudhiṣṭhiraḥ /",
  "GK19ksbhm10u:12": "kṛṣṇapradhānaiḥ sahitastaṃ deśaṃ tūrṇamāyayau // Bhmj_10.6 //",
  "GK19ksbhm10u:13": "garjadgajarathānīkaiḥ pārthānvīkṣya samāgatān /",
  "GK19ksbhm10u:14": "narendrasūcanabhayātprayayurgautamādayaḥ // Bhmj_10.7 //",
  "GK19ksbhm10u:15": "tasya kṛṣṇasya vacasā dharmasūnuḥ suyodhanam /",
  "GK19ksbhm10u:16": "tataḥ sa salilāntaḥsthamuvāca kṣmābhṛtāṃ varaḥ // Bhmj_10.8 //",
  "GK19ksbhm10u:17": "rājanrajatakarpūrarajanīpatisundaram /",
  "GK19ksbhm10u:18": "aho vīra yaśaḥ śubhraṃ bhavatā malinīkṛtam // Bhmj_10.9 //",
  "GK19ksbhm10u:19": "ghātayitvā narapatīṃllakṣmīkulagṛhānraṇe /",
  "GK19ksbhm10u:20": "prāṇarakṣāparo mānī kathaṃ jīvanna lajjase // Bhmj_10.10 //",
  "GK19ksbhm10u:21": "uttiṣṭha yuddhe svāṃ lakṣmīṃ tridivaṃ vā samāpnuhi /",
  "GK19ksbhm10u:22": "lākṣaḥ samo hi vīrāṇāṃ vijayo nidhanaṃ nu vā // Bhmj_10.11 //",
  "GK19ksbhm10u:23": "iti yaudhiṣṭhiraṃ śrutvā vaco manyuviṣākulaḥ /",
  "GK19ksbhm10u:24": "prāha dūryodhanastoye stambheram iva śvasan // Bhmj_10.12 //",
  "GK19ksbhm10u:25": "evametadyathāttha tvaṃ śriyaṃ nidhanameva me /",
  "GK19ksbhm10u:26": "bandhusvajanahīnā tu lakṣmīḥ kasya manaḥ spṛśet // Bhmj_10.13 //",
  "GK19ksbhm10u:27": "dhvajinī vīrahīnena goṣṭhīva budhavarjitā /",
  "GK19ksbhm10u:28": "svajanena vinā lakṣmīḥ kasya prīṇāti mānasam // Bhmj_10.14 //",
  "GK19ksbhm10u:29": "sa karṇaḥ suhṛdāmagryaḥ sa guruḥ sa pitāmahaḥ /",
  "GK19ksbhm10u:30": "vinā tapovanamaho sumuneḥ kasya vismaret // Bhmj_10.15 //",
  "GK19ksbhm10u:31": "bandhumittraviyuktānāṃ bhūtayo hi viḍambanāḥ /",
  "GK19ksbhm10u:32": "andhānāmiva saundaryavicitrāścitramālikāḥ // Bhmj_10.16 //",
  "GK19ksbhm10u:33": "bhuṅkṣva bhuktāṃ mayā pṛthvīṃ vīraratnavivarjitām /",
  "GK19ksbhm10u:34": "vairāgyamuktābharaṇāṃ jaratīmiva yoṣitam // Bhmj_10.17 //",
  "GK19ksbhm10u:35": "ityākarṇya pṛthāsūnurdhārtarāṣṭramabhāṣata /",
  "GK19ksbhm10u:36": "diṣṭyā tapovanaruciḥ śānto 'si vasudhāpate // Bhmj_10.18 //",
  "GK19ksbhm10u:37": "sūcyagramapi na tyājyaṃ pāṇḍavānprati te bhuvaḥ /",
  "GK19ksbhm10u:38": "samagrāṃ pṛthivīmadya dātumicchasi kautukam // Bhmj_10.19 //",
  "GK19ksbhm10u:39": "uttiṣṭha yuddhamekena tavāstu vijitaḥ sa cet /",
  "GK19ksbhm10u:40": "jitāḥ sarve tvayā rājanmā pralāpaiḥ kṛśo bhava // Bhmj_10.20 //",
  "GK19ksbhm10u:41": "udatiṣṭhanniśamyaitatpārthavākyaṃ nṛpaḥ krudhā /",
  "GK19ksbhm10u:42": "sāvalepaṃ vacaḥ śrutvā na sahante hi māninaḥ // Bhmj_10.21 //",
  "GK19ksbhm10u:43": "hemāṅgadagadāpāṇiḥ sumeruriva śṛṅgavān /",
  "GK19ksbhm10u:44": "utthitaḥ sa babhau rājā daṇḍadhāra ivāparaḥ // Bhmj_10.22 //",
  "GK19ksbhm10u:45": "kṛṣṇavākyātsa saṃnaddho gadāpāṇirvṛkodaraḥ /",
  "GK19ksbhm10u:46": "tasya tasthau puraḥ kṛṣṇānikāragaṇanāparaḥ // Bhmj_10.23 //",
  "GK19ksbhm10u:47": "***** duryodhanāvirbhāvaḥ // 1 // *****",
  "GK19ksbhm10u:48": "atrāntare haladharaḥ kurupāṇḍavasaṃkṣaye /",
  "GK19ksbhm10u:49": "bandhunāśabhayodvignastīrthayātrāgataḥ śanaiḥ // Bhmj_10.24 //",
  "GK19ksbhm10u:50": "tadābhyetya kurukṣetraṃ tāvapaśyadgadāyudhau /",
  "GK19ksbhm10u:51": "śiṣyayoḥ samaraṃ paśyañśṛṇvanniva giro mithaḥ // Bhmj_10.25 //",
  "GK19ksbhm10u:52": "sa pūjitaḥ kṛṣṇamukhyaiḥ pārthaiḥ śaśiśatadyutiḥ /",
  "GK19ksbhm10u:53": "upāviśannīlavāsā himavāniva sāmbudaḥ // Bhmj_10.26 //",
  "GK19ksbhm10u:54": "ityākarṇya kathāpadhye papraccha janamejayaḥ /",
  "GK19ksbhm10u:55": "tīrthayātrāṃ halabhṛto vaiśampāyanamādarāt // Bhmj_10.27 //",
  "GK19ksbhm10u:56": "so 'bravītprastute yuddhe kurupāṇḍavabhūbhujām /",
  "GK19ksbhm10u:57": "āmantrya kṛṣṇaṃ prayayau kṣayāśaṅkī pralambahā // Bhmj_10.28 //",
  "GK19ksbhm10u:58": "puṣpeṇa nirgato rāmo dvijebhyaḥ sarvakāmadaḥ /",
  "GK19ksbhm10u:59": "sarasvatīmavāpyātha prabhāsaṃ śayanena saḥ // Bhmj_10.29 //",
  "GK19ksbhm10u:60": "yatra tārāpatiṃ tārā rohiṇīsaktamānasam /",
  "GK19ksbhm10u:61": "khinnā nyavedayatpitre tacchāpātso 'bhavatkṣayī // Bhmj_10.30 //",
  "GK19ksbhm10u:62": "kṣaṇena śāpātprakṣīṇe śaśalakṣmaṇi yakṣmaṇā /",
  "GK19ksbhm10u:63": "sarvakṣayabhayodvignairdaivairdiṣṭaṃ tadā yayau // Bhmj_10.31 //",
  "GK19ksbhm10u:64": "tatprabhāsaṃ hasaṃstūrṇaṃ tatrāpi prāpa pūrṇatām /",
  "GK19ksbhm10u:65": "tatra bharādvajaḥ snātvā tritakūpaṃ samāyayau // Bhmj_10.32 //",
  "GK19ksbhm10u:66": "upekṣitaḥ purā yatra bhrātṛbhyāṃ nṛpatistritaḥ /",
  "GK19ksbhm10u:67": "sa cakre mānasaṃ yajñaṃ tato devāḥ samabhyayuḥ // Bhmj_10.33 //",
  "GK19ksbhm10u:68": "tato varātsakūpo 'bhūtsomapānaphalapradaḥ /",
  "GK19ksbhm10u:69": "tasminsarasvatītīre sasne rāmeṇa sādaram // Bhmj_10.34 //",
  "GK19ksbhm10u:70": "tato vinaśanaṃ puṇyaṃ prāpa yatra sarasvatī /",
  "GK19ksbhm10u:71": "śūdrabhīrukavidviṣṭā vinaṣṭā duṣṭanāśinī // Bhmj_10.35 //",
  "GK19ksbhm10u:72": "sa bhūmikaṃ tataḥ prāyādbalī yatrāpsaraḥsakhā /",
  "GK19ksbhm10u:73": "krīḍanti suragandharvā nṛtyagītavinodanaiḥ // Bhmj_10.36 //",
  "GK19ksbhm10u:74": "durgasrotaḥ śaṅkhatīrthaṃ puṇyaṃ dvaitavanaṃ tathā /",
  "GK19ksbhm10u:75": "avāpya nagadhanvānaṃ vasatiryatra vāsukeḥ // Bhmj_10.37 //",
  "GK19ksbhm10u:76": "viveśa naimiṣa rāmo yatra prācī sarasvatī /",
  "GK19ksbhm10u:77": "munīnāṃ darśanaṃ yatra naimiṣāraṇyavāsinām // Bhmj_10.38 //",
  "GK19ksbhm10u:78": "sapta sārasvataṃ prāyādyatra caṅkaṇako muniḥ /",
  "GK19ksbhm10u:79": "nanarta gātrādālokya nijācchākarasaṃ srutam // Bhmj_10.39 //",
  "GK19ksbhm10u:80": "nṛtyatastasya sāvegaṃ sakhelaṃ varakānanam /",
  "GK19ksbhm10u:81": "munestapaḥprabhāvena jagatsarvaṃ nanarta ca // Bhmj_10.40 //",
  "GK19ksbhm10u:82": "devairathārthito 'bhyetya tasya śambaradarśanāt /",
  "GK19ksbhm10u:83": "svakarānniḥsṛtaṃ bhasma tenābhūnnirmado muniḥ // Bhmj_10.41 //",
  "GK19ksbhm10u:84": "tasmai stutikṛte tuṣṭastapovṛddhiṃ dadau vibhuḥ /",
  "GK19ksbhm10u:85": "saptasārasvate tīrthe saṃnidhānaṃ ca śaṃkaraḥ // Bhmj_10.42 //",
  "GK19ksbhm10u:86": "kapālamocanaṃ prāpa tīrthamauśanasaṃ balaḥ /",
  "GK19ksbhm10u:87": "yatra jaṅghāgrahānmukto munirghorānmahodaraḥ // Bhmj_10.43 //",
  "GK19ksbhm10u:88": "rāghaveṣunikṛttasya rākṣasasya śiraḥ purā /",
  "GK19ksbhm10u:89": "mahodarasya jaṅghāyāṃ lagnaṃ kila visarpadam // Bhmj_10.44 //",
  "GK19ksbhm10u:90": "sarvatīrthanimagno 'pi tatraiva svāsthyamāpa saḥ /",
  "GK19ksbhm10u:91": "uttaṅkāśramamāsādya pṛthūdakamavāpa ca // Bhmj_10.45 //",
  "GK19ksbhm10u:92": "vasiṣṭhena jitaḥ pūrvaṃ dhenuhṛdbrahmatejasā /",
  "GK19ksbhm10u:93": "rājanyavaṃśyo yatrāpa brahmatāṃ kauśiko muniḥ // Bhmj_10.46 //",
  "GK19ksbhm10u:94": "tatra rāmo dadau bhūri dhanaṃ vārṣṇeyanandanaḥ /",
  "GK19ksbhm10u:95": "tataḥ sārasvate kuñje yatrendro namucervadhāt // Bhmj_10.47 //",
  "GK19ksbhm10u:96": "brahmahatyākulo muktastaṃ prāpa dāruṇaṃ śanaiḥ /",
  "GK19ksbhm10u:97": "somatīrthaṃ tataḥ prāpa yatreṣṭamamṛtatviṣā // Bhmj_10.48 //",
  "GK19ksbhm10u:98": "tārakakhyo hato yatra ṣaṇmukhena mahāsuraḥ /",
  "GK19ksbhm10u:99": "skandābhiṣekātsnātvātha vahnitīrthaṃ samāyayau // Bhmj_10.49 //",
  "GK19ksbhm10u:100": "yatrānalo bhṛgoḥ śāpānnaṣṭo 'bhūnnirvṛtaḥ punaḥ /",
  "GK19ksbhm10u:101": "kauberamindratīrthaṃ ca yāmunaṃ cācyutāgrajaḥ // Bhmj_10.50 //",
  "GK19ksbhm10u:102": "devalasyāśramaṃ prāpya jaijīṣavyaḥ suyogavit /",
  "GK19ksbhm10u:103": "yatra sarvagataṃ brahma devalāya jagau muniḥ // Bhmj_10.51 //",
  "GK19ksbhm10u:104": "tataḥ sārasvataṃ tīrthamāsasāda halāyudhaḥ /",
  "GK19ksbhm10u:105": "apasarodarśanātpūrvaṃ śukraṃ nipatitaṃ jale // Bhmj_10.52 //",
  "GK19ksbhm10u:106": "dadhīcasya munestena putraṃ lebhe sarasvatī /",
  "GK19ksbhm10u:107": "dadhīcasyāsthinicaye mahendrāyudhatāṃ gate // Bhmj_10.53 //",
  "GK19ksbhm10u:108": "anāvṛṣṭyāṃ sa tanayastayā matsyāśanairvṛtaḥ /",
  "GK19ksbhm10u:109": "sārasvataḥ suto vedānsa sasmāra mahāmatiḥ // Bhmj_10.54 //",
  "GK19ksbhm10u:110": "durbhikṣakarṣitā nāma vismṛte 'dhyayane tataḥ /",
  "GK19ksbhm10u:111": "sarveṣāṃ munimukhyānāṃ babhūva gururarthitaḥ // Bhmj_10.55 //",
  "GK19ksbhm10u:112": "tasmiṃstīrthe balaḥ snātvā yayau yatra purā vapuḥ /",
  "GK19ksbhm10u:113": "sanatkumārī tapasā svargāya tyaktumudyatā // Bhmj_10.56 //",
  "GK19ksbhm10u:114": "nāradaḥ prāha nodvāhaṃ vinā svargo bhavediti /",
  "GK19ksbhm10u:115": "sā gālavasutāyaiva dinabhogyāṃ tanuṃ dadau // Bhmj_10.57 //",
  "GK19ksbhm10u:116": "kṛtvā lalitamākāraṃ tridivaṃ ca yayau tataḥ /",
  "GK19ksbhm10u:117": "tatra snātvā ca datvā ca plakṣaprasravaṇe tathā // Bhmj_10.58 //",
  "GK19ksbhm10u:118": "avāpa kārapavanaṃ tīrthānyālokayañśanaiḥ /",
  "GK19ksbhm10u:119": "nāradāttatra śuśrāva kurukṣetre nṛpakṣayam // Bhmj_10.59 //",
  "GK19ksbhm10u:120": "***** rāmatīrthayātrā // 2 // *****",
  "GK19ksbhm10u:121": "pṛṣṭo 'tha dhṛtarāṣṭreṇa saṃjayo yuddhamabravīt /",
  "GK19ksbhm10u:122": "bhīmasenasya virathaṃ kauravasya ca māninaḥ // Bhmj_10.60 //",
  "GK19ksbhm10u:123": "samantapañcakaṃ gatvā padbhyāmeva suyodhanaḥ /",
  "GK19ksbhm10u:124": "tasthāvakampo mahatāmadhikaṃ dhairyamāpadi // Bhmj_10.61 //",
  "GK19ksbhm10u:125": "tato duryodhano 'vādīddharmaputraṃ sahānugāḥ /",
  "GK19ksbhm10u:126": "upaviśya nirīkṣantāṃ bhavanto yudhyamānayoḥ // Bhmj_10.62 //",
  "GK19ksbhm10u:127": "ajātaśatruḥ śrutvaitatsaha pāñcālasainikaiḥ /",
  "GK19ksbhm10u:128": "upāviśatkṛṣṇasakhaḥ parivārya halāyudham // Bhmj_10.63 //",
  "GK19ksbhm10u:129": "atha bhīmaṃ gadāpāṇiṃ baddhakakṣastarasvinam /",
  "GK19ksbhm10u:130": "ājuhāva vacoyuddhe pravṛtte kurunandanaḥ // Bhmj_10.64 //",
  "GK19ksbhm10u:131": "tatastayorgadāghātajātā vahnikaṇāvalī /",
  "GK19ksbhm10u:132": "vidadhe saṃbhramāvṛttahemābharaṇavibhramam // Bhmj_10.65 //",
  "GK19ksbhm10u:133": "surairāpūrite vyomni yuddhadarśanalālasaiḥ /",
  "GK19ksbhm10u:134": "babhūva nākakāntānāṃ ko 'pi kautukavibhramaḥ // Bhmj_10.66 //",
  "GK19ksbhm10u:135": "ākṣepe varjane mokṣe rājavṛttau nivartake /",
  "GK19ksbhm10u:136": "dhāvane samavasthāne viplute samaviplute // Bhmj_10.67 //",
  "GK19ksbhm10u:137": "tau pragalbhatayā vīrau citramaṇḍalacāriṇau /",
  "GK19ksbhm10u:138": "sanirghoṣaṃ mahāvegaḥ pārśvadeśamatāḍayat // Bhmj_10.68 //",
  "GK19ksbhm10u:139": "tato bhīmaḥ samādhūya gadāmaśaniniḥsvanām /",
  "GK19ksbhm10u:140": "bhrāmayāmāsa yenābhūddhoro gharagharāravaḥ // Bhmj_10.69 //",
  "GK19ksbhm10u:141": "tatastāṃ prahiṇodbhīmo girigurvīṃ gadāṃ javāt/",
  "GK19ksbhm10u:142": "akampitastāḍito 'pi punarbhīmo 'sṛjadgadām /",
  "GK19ksbhm10u:143": "punarmoghīkṛtā rājñā sāpatatkampitāvaniḥ // Bhmj_10.70 //",
  "GK19ksbhm10u:144": "atha vakṣastaṭe bhīmaḥ kauraveṇa samāhataḥ /",
  "GK19ksbhm10u:145": "na viveda kṣaṇaṃ kiṃcinmūrchāvyākulitāśayaḥ // Bhmj_10.71 //",
  "GK19ksbhm10u:146": "pāñcālasṛñjayānīke saṃdehākulite tataḥ /",
  "GK19ksbhm10u:147": "śanairbhīmaḥ samāśvasya pārśve nṛpamatāḍayat // Bhmj_10.72 //",
  "GK19ksbhm10u:148": "gāḍhaprahārābhihataḥ patitaḥ kauraveśvaraḥ /",
  "GK19ksbhm10u:149": "utthāya tūrṇamāhatya bhuvi bhīmamapātayat // Bhmj_10.73 //",
  "GK19ksbhm10u:150": "athāpatatsurotsṛṣṭa puṣpavṛṣṭiryaśaḥsitā /",
  "GK19ksbhm10u:151": "sādhuvādāvalī mūrdhi sūnave kurubhūpateḥ // Bhmj_10.74 //",
  "GK19ksbhm10u:152": "athotthite bhīmasene parimṛjyāśu śoṇitam /",
  "GK19ksbhm10u:153": "ko 'nayoradhiko yoddhā papracchetyarjuno harim // Bhmj_10.75 //",
  "GK19ksbhm10u:154": "so 'bravīdbalavānbhīmo bhīmavīryaḥ kimucyate /",
  "GK19ksbhm10u:155": "upadeśe gadāyuddhe kiṃtu yuddhaparākrame // Bhmj_10.76 //",
  "GK19ksbhm10u:156": "sa prayatnaḥ kurupatirbhīmasenādvilokyate /",
  "GK19ksbhm10u:157": "tasmānna dharmayuddhena bhīmaḥ śakto nipātane // Bhmj_10.77 //",
  "GK19ksbhm10u:158": "māyayā nihatā daityā māyāvadhyā hi māyinaḥ /",
  "GK19ksbhm10u:159": "ūrubhaṅgamato bhīmo vidadhātvasya māyayā // Bhmj_10.78 //",
  "GK19ksbhm10u:160": "anyathā durjayo rājā trayodaśasamāvyadhāt /",
  "GK19ksbhm10u:161": "eṣa lokabhayaṃ bhīmo(me)",
  "GK19ksbhm10u:162": "vidhāya gadayā śramam // Bhmj_10.79 //",
  "GK19ksbhm10u:163": "vyāyāmacaturo vegādūrdhvacārī suyodhanaḥ /",
  "GK19ksbhm10u:164": "mahāśaileyabhaṅgo 'sminsukaraśchidradarśanāt // Bhmj_10.80 //",
  "GK19ksbhm10u:165": "ukte janārdaneneti bhīmaścikṣepa bhīṣaṇām /",
  "GK19ksbhm10u:166": "gadāṃ tāṃ kururājo 'pi vilaṅghya tamatāḍayat // Bhmj_10.81 //",
  "GK19ksbhm10u:167": "vyathāṃ saṃstambhya bhīmo 'tha ghūrṇamānatanuḥ kṣaṇam /",
  "GK19ksbhm10u:168": "gadāṃ randhraṃ vicintyāsmai prāhiṇotkanakāṅgadām // Bhmj_10.82 //",
  "GK19ksbhm10u:169": "puṣpāyudhapurodārahemābharaṇavibhrame /",
  "GK19ksbhm10u:170": "ūruyugme bhujaṅgīva sā papāta mahīpateḥ // Bhmj_10.83 //",
  "GK19ksbhm10u:171": "nirghātaghoraghanaghoṣagadānipātaniṣpeṣajarjarataratruṭitorusaṃdhiḥ /",
  "GK19ksbhm10u:172": "duryodhano 'tha nipapāta divaṃ vilokya vajrāvapātahatapakṣa ivādrirājaḥ // Bhmj_10.84 //",
  "GK19ksbhm10u:173": "akṣauhiṇīpatinareśvaramauliratnaraśmicchaṭāpaṭalapāṭalapādapīṭhe /",
  "GK19ksbhm10u:174": "asminmahīparivṛḍhe patite ca bhūmau kairninditaṃ na bhavabhaṅgurabhaṅgi janma // Bhmj_10.85 //",
  "GK19ksbhm10u:175": "***** gadāyuddham // 3 // *****",
  "GK19ksbhm10u:176": "tasminnekādaśacamūnāthe bhuvi nipātite /",
  "GK19ksbhm10u:177": "cakampe lolataṭinīraśanā vasudhāvadhūḥ // Bhmj_10.86 //",
  "GK19ksbhm10u:178": "babhūva bhayadairvyāptaṃ durnimittaśatairjagat /",
  "GK19ksbhm10u:179": "praśaṃsantaśca tadyuddhaṃ prayayurvyomacāriṇaḥ // Bhmj_10.87 //",
  "GK19ksbhm10u:180": "bhīmo nikāragaṇanāṃ kurvankurupateḥ śiraḥ /",
  "GK19ksbhm10u:181": "caraṇenāspṛśallakṣmīvilāsasadanaṃ tataḥ // Bhmj_10.88 //",
  "GK19ksbhm10u:182": "taṃ vilokya rurodeva patitaṃ cakravartinam /",
  "GK19ksbhm10u:183": "tvaṅgadgadāṅgadacchinnahāramuktāśrubhirmahī // Bhmj_10.89 //",
  "GK19ksbhm10u:184": "bhīmaṃ nirasya garjantaṃ dharmarājaḥ suyodhanam /",
  "GK19ksbhm10u:185": "ūce rājaṃstavaivāyamanayātsuhṛdāṃ kṣayaḥ // Bhmj_10.90 //",
  "GK19ksbhm10u:186": "dṛṣṭvā śiraḥ padā spṛṣṭaṃ bhīmena jagatībhujaḥ /",
  "GK19ksbhm10u:187": "prāhottālahalaḥ kopakarālastālalāñchanaḥ // Bhmj_10.91 //",
  "GK19ksbhm10u:188": "pratāpatāpitaricoryaśaḥ śatruhṛtaśriyaḥ /",
  "GK19ksbhm10u:189": "śiro vyājahatasyāsya kaḥ spṛśedapaśuḥ padā // Bhmj_10.92 //",
  "GK19ksbhm10u:190": "ityuktvā krodhaśikhinā taṃ bhīmaṃ dagdhumudyatam /",
  "GK19ksbhm10u:191": "śamayāmāsa kaṃsāristaistairvinayabhāṣitaiḥ // Bhmj_10.93 //",
  "GK19ksbhm10u:192": "viṣadyutāgnidoṣāṇāṃ prabhāvo 'sīti vādinam /",
  "GK19ksbhm10u:193": "kṛṣṇaṃ duryodhano 'vādītsaṃstambhya rujamutkaṭām // Bhmj_10.94 //",
  "GK19ksbhm10u:194": "gopālabāla mūrkhāṇāṃ saṃjñāṃ kṛtvāsmi vañcitaḥ /",
  "GK19ksbhm10u:195": "tvayā bhīṣmamukhā vīrāste vyājaireva pātitāḥ // Bhmj_10.95 //",
  "GK19ksbhm10u:196": "bhuktaṃ suhṛdbhirvihṛtaṃ kāntābhirhutamagryajaiḥ /",
  "GK19ksbhm10u:197": "vadhaḥ kṣatrocitaḥ prāptaḥ ko 'nyo 'sti sadṛśo mayā // Bhmj_10.96 //",
  "GK19ksbhm10u:198": "bhrūbhaṅgena nivāsitā vanabhuvaṃ śuṣyanmukhāḥ śatravo dṛṣṭā śrīḥ svayamarpitāgryasuhṛdāmuṣṇīṣahārasmitā /",
  "GK19ksbhm10u:199": "saṃgrāme 'pyaparāṅmukhasya nidhanaṃ dikṣu prarūḍhaṃ yaśaḥ kartavya spṛhaṇīyamanyadicitaṃ yuktaṃ kimastyāyuṣaḥ // Bhmj_10.97 //",
  "GK19ksbhm10u:200": "ityākarṇya vacastasya lajjitā iva māninaḥ /",
  "GK19ksbhm10u:201": "nṛpaiḥ saha yayuḥ pārthā nādayanto rathairjayam // Bhmj_10.98 //",
  "GK19ksbhm10u:202": "athāvaropya prathamaṃ phalguṇaṃ madhusūdanaḥ /",
  "GK19ksbhm10u:203": "rathādavātaratpascājjajvāla sa rathastataḥ // Bhmj_10.99 //",
  "GK19ksbhm10u:204": "droṇakarṇāstradagdho 'yamatyantaṃ jvalito rathaḥ /",
  "GK19ksbhm10u:205": "iti pṛṣṭo 'vadatpārthaṃ vismitaṃ kāliyāntakaḥ // Bhmj_10.100 //",
  "GK19ksbhm10u:206": "śrutveti saṃjayādrājā dāruṇaṃ mohavihvalaḥ /",
  "GK19ksbhm10u:207": "śuśoca putrānvākyāni vidurasya smaranmuhuḥ // Bhmj_10.101 //",
  "GK19ksbhm10u:208": "tataḥ kṛṣṇamunau prāpte kuruveśma harirnṛpam /",
  "GK19ksbhm10u:209": "gāndhārīṃ cāvadatprāpya visṛṣṭo 'jātaśatruṇā // Bhmj_10.102 //",
  "GK19ksbhm10u:210": "yācito 'piśamaṃ naicchatkauravo nijadurnayāt /",
  "GK19ksbhm10u:211": "na ca dharmāccyutāḥ pārthāstena tena padaṃ ruṣaḥ // Bhmj_10.103 //",
  "GK19ksbhm10u:212": "putrāste yuvayoradya pāṇḍuputrāḥ suratviṣaḥ /",
  "GK19ksbhm10u:213": "yuvābhyāmeva coktaṃ prāgyato dharmastato jayaḥ // Bhmj_10.104 //",
  "GK19ksbhm10u:214": "iti prasādya tau yāte vṛṣṇivīre 'mbikāsutaḥ /",
  "GK19ksbhm10u:215": "munāvāśvāsya yāte ca śeṣaṃ papraccha saṃjayam // Bhmj_10.105 //",
  "GK19ksbhm10u:216": "so 'bravīdatha putraste veṣya(pa)",
  "GK19ksbhm10u:217": "māno mahītale /",
  "GK19ksbhm10u:218": "māmūce paśya sūdena māyayāhaṃ nipātitaḥ // Bhmj_10.106 //",
  "GK19ksbhm10u:219": "atrāntare samabhyetya drauṇihārdikyagautamāḥ /",
  "GK19ksbhm10u:220": "dadṛśuḥ kauravapatiṃ nītaṃ kālena tāṃ daśām // Bhmj_10.107 //",
  "GK19ksbhm10u:221": "tato gurusutaḥ kopādviniṣpiṣya kare karam /",
  "GK19ksbhm10u:222": "niḥśeṣaśatrunidhane pratijñāṃ niḥśvasanvyadhāt // Bhmj_10.108 //",
  "GK19ksbhm10u:223": "atha vīraṃ tamāliṅgya cakre senāpatiṃ nṛpaḥ /",
  "GK19ksbhm10u:224": "na hyahaṃkāraśaithilyaṃ paryante 'pyabhimāninām // Bhmj_10.109 //",
  "GK19ksbhm10u:225": "pralayāmbudanirghoṣaṃ vinadya droṇanandanaḥ /",
  "GK19ksbhm10u:226": "niśāyāṃ tāṃ rujā rājā viṣamāmagamaddaśām // Bhmj_10.110 //",
  "GK19ksbhm10u:227": "tato gautamahārdikyaguruputrā mahāvanam /",
  "GK19ksbhm10u:228": "viviśuḥ kalayanto 'ntaḥ śriyaḥ kallolalolatām // Bhmj_10.111 //",
  "GK19ksbhm10u:229": "iti kṣemendraviracitāyāṃ bhāratamañjaryāṃ gadāparva"
}