{
  "SA20bsa018_u:0": "Atha Triṃśikāvijñaptikārikāḥ (Tvk)",
  "SA20bsa018_u:1": "kṛtiriyamācāryavasubandhoḥ /",
  "SA20bsa018_u:2": "ātmadharmopacāro hi vividho yaḥ pravartate /",
  "SA20bsa018_u:3": "vijñānapariṇāme 'sau pariṇāmaḥ sa ca tridhā // Tvk_1 //",
  "SA20bsa018_u:4": "vipāko mananākhyaśca vijñaptirviṣayasya ca /",
  "SA20bsa018_u:5": "tatrālayākhyaṃ vijñānaṃ vipākaḥ sarvabījakam // Tvk_2 //",
  "SA20bsa018_u:6": "asaṃviditakopādisthānavijñaptikaṃ ca tat /",
  "SA20bsa018_u:7": "sadā sparśamanaskāravitsañjñācetanānvitam // Tvk_3 //",
  "SA20bsa018_u:8": "upekṣā vedanā tatrānivṛtāvyākṛtaṃ ca tat /",
  "SA20bsa018_u:9": "tathā sparśādayastacca vartate srotasaughavat // Tvk_4 //",
  "SA20bsa018_u:10": "tasya vyāvṛtirarhatve tadāśritya pravartate /",
  "SA20bsa018_u:11": "tadālambaṃ manonāma vijñānaṃ mananātmakam // Tvk_5 //",
  "SA20bsa018_u:12": "kleśaiścaturbhiḥ sahitaṃ nivṛtāvyākṛtaiḥ sadā /",
  "SA20bsa018_u:13": "ātmaduṣṭyātmamohātmamānātmasnehasañjñitaiḥ // Tvk_6 //",
  "SA20bsa018_u:14": "yatrajastanmayairanyaiḥ sparśādyaiścārhato na tat /",
  "SA20bsa018_u:15": "na nirodhasamāpattau mārge lokottare na ca // Tvk_7 //",
  "SA20bsa018_u:16": "dvitīyaḥ pariṇāmo 'yaṃ tṛtīyaḥ ṣaḍvidhasya yā /",
  "SA20bsa018_u:17": "viṣayasyopalabdhiḥ sā kuśalākuśalādvayā // Tvk_8 //",
  "SA20bsa018_u:18": "sarvatragairviniyataiḥ kuśalaiścaitasairasau /",
  "SA20bsa018_u:19": "samprayuktā tathā kleśairūpakleśaistrivedanā // Tvk_9 //",
  "SA20bsa018_u:20": "ādyāḥ sparśādayaśchandādhimokṣasmṛtayaḥ saha /",
  "SA20bsa018_u:21": "samādhidhībhyāṃ niyatāḥ śraddhātha hrirapatrapā // Tvk_10 //",
  "SA20bsa018_u:22": "alobhādi trayaṃ vīryaṃ praśrabdhiḥ sāpramādikā /",
  "SA20bsa018_u:23": "ahiṃsā kuśalāḥ kleśā rāgapratighamūḍhayaḥ // Tvk_11 //",
  "SA20bsa018_u:24": "mānadṛgvicikitsāśca krodhopanahane punaḥ /",
  "SA20bsa018_u:25": "mrakṣaḥ pradāśa īrṣyārtha mātsaryaṃ saha māyayā // Tvk_12 //",
  "SA20bsa018_u:26": "śāṭhyaṃ mado 'vihiṃsāhrīratrapā styānamuddhavaḥ /",
  "SA20bsa018_u:27": "āśraddhamatha kausīdyaṃ pramādo muṣitā smṛtiḥ // Tvk_13 //",
  "SA20bsa018_u:28": "vikṣepo 'samprajanyaṃ ca kaukṛtyaṃ middhameva ca /",
  "SA20bsa018_u:29": "vitarkaśca vicāraścetyupakleśā dvaye dvidhā // Tvk_14 //",
  "SA20bsa018_u:30": "pañcānāṃ mūlavijñāne yathāpratyayamudbhavaḥ /",
  "SA20bsa018_u:31": "vijñānānāṃ saha na vā taraṅgāṇāṃ yathā jale // Tvk_15 //",
  "SA20bsa018_u:32": "manovijñānasambhūtiḥ sarvadāsañjñikādṛte /",
  "SA20bsa018_u:33": "samāpattidvayānmiddhānmūrchanādapyacittakāt // Tvk_16 //",
  "SA20bsa018_u:34": "vijñānapariṇāmo 'yaṃ vikalpo yadvikalpyate /",
  "SA20bsa018_u:35": "tena tannāsti tenedaṃ sarvaṃ vijñaptimātrakam // Tvk_17 //",
  "SA20bsa018_u:36": "sarvabījaṃ hi vijñānaṃ pariṇāmastathā tathā /",
  "SA20bsa018_u:37": "yātyanyonyavaśād yena vikalpaḥ sa sa jāyate // Tvk_18 //",
  "SA20bsa018_u:38": "karmaṇo vāsanā grāhadvayavāsanayā saha /",
  "SA20bsa018_u:39": "kṣīṇe pūrvavipāke 'nyadvipākaṃ janayanti tat // Tvk_19 //",
  "SA20bsa018_u:40": "yena yena vikalpena yadyad vastu vikalpyate /",
  "SA20bsa018_u:41": "parikalpita evāsau svabhāvo na sa vidyate // Tvk_20 //",
  "SA20bsa018_u:42": "paratantrasvabhāvastu vikalpaḥ pratyayodbhavaḥ /",
  "SA20bsa018_u:43": "niṣpannastasya pūrveṇa sadā rahitatā tu yā // Tvk_21 //",
  "SA20bsa018_u:44": "ata eva sa naivānyo nānanyaḥ paratantrataḥ /",
  "SA20bsa018_u:45": "anityatādivad vācyo nādṛṣṭe 'smin sa dṛśyate // Tvk_22 //",
  "SA20bsa018_u:46": "trividhasya svabhāvasya trividhāṃ niḥsvabhāvatām /",
  "SA20bsa018_u:47": "sandhāya sarvadharmāṇāṃ deśitā niḥsvabhāvatā // Tvk_23 //",
  "SA20bsa018_u:48": "prathamo lakṣaṇenaiva niḥsvabhāvo 'paraḥ punaḥ /",
  "SA20bsa018_u:49": "na svayambhāva etasyetyaparā niḥsvabhāvatā // Tvk_24 //",
  "SA20bsa018_u:50": "dharmāṇāṃ paramārthaśca sa yatastathatāpi saḥ /",
  "SA20bsa018_u:51": "sarvakālaṃ tathābhāvāt saiva vijñaptimātratā // Tvk_25 //",
  "SA20bsa018_u:52": "yāvadvijñaptimātratve vijñānaṃ nāvatiṣṭhati /",
  "SA20bsa018_u:53": "grāhadvayasyānuśayastāvanna vinivartate // Tvk_26 //",
  "SA20bsa018_u:54": "vijñaptimātramevedamityapi hayupalambhataḥ /",
  "SA20bsa018_u:55": "sthāpayannagrataḥ kiñcit tanmātre nāvatiṣṭhate // Tvk_27 //",
  "SA20bsa018_u:56": "yadālambanaṃ vijñānaṃ naivopalabhate tadā /",
  "SA20bsa018_u:57": "sthitaṃ vijñānamātratve grāhyābhāve tadagrahāt // Tvk_28 //",
  "SA20bsa018_u:58": "acitto 'nupalambho 'sau jñānaṃ lokottaraṃ ca tat /",
  "SA20bsa018_u:59": "āśrayasya parāvṛttirdvidhā dauṣṭhulyahānitaḥ // Tvk_29 //",
  "SA20bsa018_u:60": "sa evānasravo dhāturacintyaḥ kuśalo dhruvaḥ /",
  "SA20bsa018_u:61": "sukho vimuktikāyo 'sau dharmākhyo 'yaṃ mahāmuneḥ // Tvk_30 //",
  "SA20bsa018_u:62": "triṃśikāvijñaptikārikāḥ samāptāḥ /",
  "SA20bsa018_u:63": "kṛtiriyamācāryavasubandhoḥ /"
}