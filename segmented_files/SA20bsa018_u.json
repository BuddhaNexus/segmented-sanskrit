{
  "SA20bsa018_u:0": "Atha Triṃśikāvijñaptikārikāḥ (Tvk)",
  "SA20bsa018_u:1": "kṛtiriyamācāryavasubandhoḥ /",
  "SA20bsa018_u:2": "Tvk_1 ātmadharmopacāro hi vividho yaḥ pravartate /",
  "SA20bsa018_u:3": "vijñānapariṇāme 'sau pariṇāmaḥ sa ca tridhā //",
  "SA20bsa018_u:4": "Tvk_2 vipāko mananākhyaśca vijñaptirviṣayasya ca /",
  "SA20bsa018_u:5": "tatrālayākhyaṃ vijñānaṃ vipākaḥ sarvabījakam //",
  "SA20bsa018_u:6": "Tvk_3 asaṃviditakopādisthānavijñaptikaṃ ca tat /",
  "SA20bsa018_u:7": "sadā sparśamanaskāravitsañjñācetanānvitam //",
  "SA20bsa018_u:8": "Tvk_4 upekṣā vedanā tatrānivṛtāvyākṛtaṃ ca tat /",
  "SA20bsa018_u:9": "tathā sparśādayastacca vartate srotasaughavat //",
  "SA20bsa018_u:10": "Tvk_5 tasya vyāvṛtirarhatve tadāśritya pravartate /",
  "SA20bsa018_u:11": "tadālambaṃ manonāma vijñānaṃ mananātmakam //",
  "SA20bsa018_u:12": "Tvk_6 kleśaiścaturbhiḥ sahitaṃ nivṛtāvyākṛtaiḥ sadā /",
  "SA20bsa018_u:13": "ātmaduṣṭyātmamohātmamānātmasnehasañjñitaiḥ //",
  "SA20bsa018_u:14": "Tvk_7 yatrajastanmayairanyaiḥ sparśādyaiścārhato na tat /",
  "SA20bsa018_u:15": "na nirodhasamāpattau mārge lokottare na ca //",
  "SA20bsa018_u:16": "Tvk_8 dvitīyaḥ pariṇāmo 'yaṃ tṛtīyaḥ ṣaḍvidhasya yā /",
  "SA20bsa018_u:17": "viṣayasyopalabdhiḥ sā kuśalākuśalādvayā //",
  "SA20bsa018_u:18": "Tvk_9 sarvatragairviniyataiḥ kuśalaiścaitasairasau /",
  "SA20bsa018_u:19": "samprayuktā tathā kleśairūpakleśaistrivedanā //",
  "SA20bsa018_u:20": "Tvk_10 ādyāḥ sparśādayaśchandādhimokṣasmṛtayaḥ saha /",
  "SA20bsa018_u:21": "samādhidhībhyāṃ niyatāḥ śraddhātha hrirapatrapā //",
  "SA20bsa018_u:22": "Tvk_11 alobhādi trayaṃ vīryaṃ praśrabdhiḥ sāpramādikā /",
  "SA20bsa018_u:23": "ahiṃsā kuśalāḥ kleśā rāgapratighamūḍhayaḥ //",
  "SA20bsa018_u:24": "Tvk_12 mānadṛgvicikitsāśca krodhopanahane punaḥ /",
  "SA20bsa018_u:25": "mrakṣaḥ pradāśa īrṣyārtha mātsaryaṃ saha māyayā //",
  "SA20bsa018_u:26": "Tvk_13 śāṭhyaṃ mado 'vihiṃsāhrīratrapā styānamuddhavaḥ /",
  "SA20bsa018_u:27": "āśraddhamatha kausīdyaṃ pramādo muṣitā smṛtiḥ //",
  "SA20bsa018_u:28": "Tvk_14 vikṣepo 'samprajanyaṃ ca kaukṛtyaṃ middhameva ca /",
  "SA20bsa018_u:29": "vitarkaśca vicāraścetyupakleśā dvaye dvidhā //",
  "SA20bsa018_u:30": "Tvk_15 pañcānāṃ mūlavijñāne yathāpratyayamudbhavaḥ /",
  "SA20bsa018_u:31": "vijñānānāṃ saha na vā taraṅgāṇāṃ yathā jale //",
  "SA20bsa018_u:32": "Tvk_16 manovijñānasambhūtiḥ sarvadāsañjñikādṛte /",
  "SA20bsa018_u:33": "samāpattidvayānmiddhānmūrchanādapyacittakāt //",
  "SA20bsa018_u:34": "Tvk_17 vijñānapariṇāmo 'yaṃ vikalpo yadvikalpyate /",
  "SA20bsa018_u:35": "tena tannāsti tenedaṃ sarvaṃ vijñaptimātrakam //",
  "SA20bsa018_u:36": "Tvk_18 sarvabījaṃ hi vijñānaṃ pariṇāmastathā tathā /",
  "SA20bsa018_u:37": "yātyanyonyavaśād yena vikalpaḥ sa sa jāyate //",
  "SA20bsa018_u:38": "Tvk_19 karmaṇo vāsanā grāhadvayavāsanayā saha /",
  "SA20bsa018_u:39": "kṣīṇe pūrvavipāke 'nyadvipākaṃ janayanti tat //",
  "SA20bsa018_u:40": "Tvk_20 yena yena vikalpena yadyad vastu vikalpyate /",
  "SA20bsa018_u:41": "parikalpita evāsau svabhāvo na sa vidyate //",
  "SA20bsa018_u:42": "Tvk_21 paratantrasvabhāvastu vikalpaḥ pratyayodbhavaḥ /",
  "SA20bsa018_u:43": "niṣpannastasya pūrveṇa sadā rahitatā tu yā //",
  "SA20bsa018_u:44": "Tvk_22 ata eva sa naivānyo nānanyaḥ paratantrataḥ /",
  "SA20bsa018_u:45": "anityatādivad vācyo nādṛṣṭe 'smin sa dṛśyate //",
  "SA20bsa018_u:46": "Tvk_23 trividhasya svabhāvasya trividhāṃ niḥsvabhāvatām /",
  "SA20bsa018_u:47": "sandhāya sarvadharmāṇāṃ deśitā niḥsvabhāvatā //",
  "SA20bsa018_u:48": "Tvk_24 prathamo lakṣaṇenaiva niḥsvabhāvo 'paraḥ punaḥ /",
  "SA20bsa018_u:49": "na svayambhāva etasyetyaparā niḥsvabhāvatā //",
  "SA20bsa018_u:50": "Tvk_25 dharmāṇāṃ paramārthaśca sa yatastathatāpi saḥ /",
  "SA20bsa018_u:51": "sarvakālaṃ tathābhāvāt saiva vijñaptimātratā //",
  "SA20bsa018_u:52": "Tvk_26 yāvadvijñaptimātratve vijñānaṃ nāvatiṣṭhati /",
  "SA20bsa018_u:53": "grāhadvayasyānuśayastāvanna vinivartate //",
  "SA20bsa018_u:54": "Tvk_27 vijñaptimātramevedamityapi hayupalambhataḥ /",
  "SA20bsa018_u:55": "sthāpayannagrataḥ kiñcit tanmātre nāvatiṣṭhate //",
  "SA20bsa018_u:56": "Tvk_28 yadālambanaṃ vijñānaṃ naivopalabhate tadā /",
  "SA20bsa018_u:57": "sthitaṃ vijñānamātratve grāhyābhāve tadagrahāt //",
  "SA20bsa018_u:58": "Tvk_29 acitto 'nupalambho 'sau jñānaṃ lokottaraṃ ca tat /",
  "SA20bsa018_u:59": "āśrayasya parāvṛttirdvidhā dauṣṭhulyahānitaḥ //",
  "SA20bsa018_u:60": "Tvk_30 sa evānasravo dhāturacintyaḥ kuśalo dhruvaḥ /",
  "SA20bsa018_u:61": "sukho vimuktikāyo 'sau dharmākhyo 'yaṃ mahāmuneḥ //",
  "SA20bsa018_u:62": "triṃśikāvijñaptikārikāḥ samāptāḥ /",
  "SA20bsa018_u:63": "kṛtiriyamācāryavasubandhoḥ /"
}