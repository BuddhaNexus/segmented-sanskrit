{
    "SA20bsa018_u:0_0": "Atha Triṃśikāvijñaptikārikāḥ (Tvk)",
    "SA20bsa018_u:0_1": "kṛtiriyamācāryavasubandhoḥ /",
    "SA20bsa018_u:tvk_1_0": "ātmadharmopacāro hi vividho yaḥ pravartate /",
    "SA20bsa018_u:tvk_1_1": "vijñānapariṇāme 'sau pariṇāmaḥ sa ca tridhā //",
    "SA20bsa018_u:tvk_2_0": "vipāko mananākhyaśca vijñaptirviṣayasya ca /",
    "SA20bsa018_u:tvk_2_1": "tatrālayākhyaṃ vijñānaṃ vipākaḥ sarvabījakam //",
    "SA20bsa018_u:tvk_3_0": "asaṃviditakopādisthānavijñaptikaṃ ca tat /",
    "SA20bsa018_u:tvk_3_1": "sadā sparśamanaskāravitsañjñācetanānvitam //",
    "SA20bsa018_u:tvk_4_0": "upekṣā vedanā tatrānivṛtāvyākṛtaṃ ca tat /",
    "SA20bsa018_u:tvk_4_1": "tathā sparśādayastacca vartate srotasaughavat //",
    "SA20bsa018_u:tvk_5_0": "tasya vyāvṛtirarhatve tadāśritya pravartate /",
    "SA20bsa018_u:tvk_5_1": "tadālambaṃ manonāma vijñānaṃ mananātmakam //",
    "SA20bsa018_u:tvk_6_0": "kleśaiścaturbhiḥ sahitaṃ nivṛtāvyākṛtaiḥ sadā /",
    "SA20bsa018_u:tvk_6_1": "ātmaduṣṭyātmamohātmamānātmasnehasañjñitaiḥ //",
    "SA20bsa018_u:tvk_7_0": "yatrajastanmayairanyaiḥ sparśādyaiścārhato na tat /",
    "SA20bsa018_u:tvk_7_1": "na nirodhasamāpattau mārge lokottare na ca //",
    "SA20bsa018_u:tvk_8_0": "dvitīyaḥ pariṇāmo 'yaṃ tṛtīyaḥ ṣaḍvidhasya yā /",
    "SA20bsa018_u:tvk_8_1": "viṣayasyopalabdhiḥ sā kuśalākuśalādvayā //",
    "SA20bsa018_u:tvk_9_0": "sarvatragairviniyataiḥ kuśalaiścaitasairasau /",
    "SA20bsa018_u:tvk_9_1": "samprayuktā tathā kleśairūpakleśaistrivedanā //",
    "SA20bsa018_u:tvk_10_0": "ādyāḥ sparśādayaśchandādhimokṣasmṛtayaḥ saha /",
    "SA20bsa018_u:tvk_10_1": "samādhidhībhyāṃ niyatāḥ śraddhātha hrirapatrapā //",
    "SA20bsa018_u:tvk_11_0": "alobhādi trayaṃ vīryaṃ praśrabdhiḥ sāpramādikā /",
    "SA20bsa018_u:tvk_11_1": "ahiṃsā kuśalāḥ kleśā rāgapratighamūḍhayaḥ //",
    "SA20bsa018_u:tvk_12_0": "mānadṛgvicikitsāśca krodhopanahane punaḥ /",
    "SA20bsa018_u:tvk_12_1": "mrakṣaḥ pradāśa īrṣyārtha mātsaryaṃ saha māyayā //",
    "SA20bsa018_u:tvk_13_0": "śāṭhyaṃ mado 'vihiṃsāhrīratrapā styānamuddhavaḥ /",
    "SA20bsa018_u:tvk_13_1": "āśraddhamatha kausīdyaṃ pramādo muṣitā smṛtiḥ //",
    "SA20bsa018_u:tvk_14_0": "vikṣepo 'samprajanyaṃ ca kaukṛtyaṃ middhameva ca /",
    "SA20bsa018_u:tvk_14_1": "vitarkaśca vicāraścetyupakleśā dvaye dvidhā //",
    "SA20bsa018_u:tvk_15_0": "pañcānāṃ mūlavijñāne yathāpratyayamudbhavaḥ /",
    "SA20bsa018_u:tvk_15_1": "vijñānānāṃ saha na vā taraṅgāṇāṃ yathā jale //",
    "SA20bsa018_u:tvk_16_0": "manovijñānasambhūtiḥ sarvadāsañjñikādṛte /",
    "SA20bsa018_u:tvk_16_1": "samāpattidvayānmiddhānmūrchanādapyacittakāt //",
    "SA20bsa018_u:tvk_17_0": "vijñānapariṇāmo 'yaṃ vikalpo yadvikalpyate /",
    "SA20bsa018_u:tvk_17_1": "tena tannāsti tenedaṃ sarvaṃ vijñaptimātrakam //",
    "SA20bsa018_u:tvk_18_0": "sarvabījaṃ hi vijñānaṃ pariṇāmastathā tathā /",
    "SA20bsa018_u:tvk_18_1": "yātyanyonyavaśād yena vikalpaḥ sa sa jāyate //",
    "SA20bsa018_u:tvk_19_0": "karmaṇo vāsanā grāhadvayavāsanayā saha /",
    "SA20bsa018_u:tvk_19_1": "kṣīṇe pūrvavipāke 'nyadvipākaṃ janayanti tat //",
    "SA20bsa018_u:tvk_20_0": "yena yena vikalpena yadyad vastu vikalpyate /",
    "SA20bsa018_u:tvk_20_1": "parikalpita evāsau svabhāvo na sa vidyate //",
    "SA20bsa018_u:tvk_21_0": "paratantrasvabhāvastu vikalpaḥ pratyayodbhavaḥ /",
    "SA20bsa018_u:tvk_21_1": "niṣpannastasya pūrveṇa sadā rahitatā tu yā //",
    "SA20bsa018_u:tvk_22_0": "ata eva sa naivānyo nānanyaḥ paratantrataḥ /",
    "SA20bsa018_u:tvk_22_1": "anityatādivad vācyo nādṛṣṭe 'smin sa dṛśyate //",
    "SA20bsa018_u:tvk_23_0": "trividhasya svabhāvasya trividhāṃ niḥsvabhāvatām /",
    "SA20bsa018_u:tvk_23_1": "sandhāya sarvadharmāṇāṃ deśitā niḥsvabhāvatā //",
    "SA20bsa018_u:tvk_24_0": "prathamo lakṣaṇenaiva niḥsvabhāvo 'paraḥ punaḥ /",
    "SA20bsa018_u:tvk_24_1": "na svayambhāva etasyetyaparā niḥsvabhāvatā //",
    "SA20bsa018_u:tvk_25_0": "dharmāṇāṃ paramārthaśca sa yatastathatāpi saḥ /",
    "SA20bsa018_u:tvk_25_1": "sarvakālaṃ tathābhāvāt saiva vijñaptimātratā //",
    "SA20bsa018_u:tvk_26_0": "yāvadvijñaptimātratve vijñānaṃ nāvatiṣṭhati /",
    "SA20bsa018_u:tvk_26_1": "grāhadvayasyānuśayastāvanna vinivartate //",
    "SA20bsa018_u:tvk_27_0": "vijñaptimātramevedamityapi hayupalambhataḥ /",
    "SA20bsa018_u:tvk_27_1": "sthāpayannagrataḥ kiñcit tanmātre nāvatiṣṭhate //",
    "SA20bsa018_u:tvk_28_0": "yadālambanaṃ vijñānaṃ naivopalabhate tadā /",
    "SA20bsa018_u:tvk_28_1": "sthitaṃ vijñānamātratve grāhyābhāve tadagrahāt //",
    "SA20bsa018_u:tvk_29_0": "acitto 'nupalambho 'sau jñānaṃ lokottaraṃ ca tat /",
    "SA20bsa018_u:tvk_29_1": "āśrayasya parāvṛttirdvidhā dauṣṭhulyahānitaḥ //",
    "SA20bsa018_u:tvk_30_0": "sa evānasravo dhāturacintyaḥ kuśalo dhruvaḥ /",
    "SA20bsa018_u:tvk_30_1": "sukho vimuktikāyo 'sau dharmākhyo 'yaṃ mahāmuneḥ //",
    "SA20bsa018_u:1_0": "triṃśikāvijñaptikārikāḥ samāptāḥ /",
    "SA20bsa018_u:2_0": "kṛtiriyamācāryavasubandhoḥ /"
}