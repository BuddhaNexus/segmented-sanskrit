{
  "GK19ksbhm11u:0": "//sauptikaparva//",
  "GK19ksbhm11u:1": "nārāyaṇaṃ namaskṛtya naraṃ caiva narottamam /",
  "GK19ksbhm11u:2": "devīṃ sarasvatīṃ vyāsaṃ tato jayamudīrayet // Bhmj_11.1 //",
  "GK19ksbhm11u:3": "pratāpadhāmni yāte 'staṃ ravau kamalinīpriye /",
  "GK19ksbhm11u:4": "śokāvakīrṇakeśena tamobhirabhavanmahī // Bhmj_11.2 //",
  "GK19ksbhm11u:5": "kṣayaṃ kālena nīte 'hni śānte saṃdhyācitānale /",
  "GK19ksbhm11u:6": "asthiśeṣairivāpūrṇaṃ tārakābhirabhūnnabhaḥ // Bhmj_11.3 //",
  "GK19ksbhm11u:7": "śyāmā pratyagraduḥkhena prasarattimirāñjanā /",
  "GK19ksbhm11u:8": "babhāra tārataralāṃ nakṣatrāśrukaṇāvalīm // Bhmj_11.4 //",
  "GK19ksbhm11u:9": "ghore tasminniśāvaktre ghorasattvaghane vane /",
  "GK19ksbhm11u:10": "śramānnidrāmupagate kṛpe sa kṛtavarmaṇi // Bhmj_11.5 //",
  "GK19ksbhm11u:11": "dhyāyanduryodhanavadhaṃ manyuhālāhalākulaḥ /",
  "GK19ksbhm11u:12": "pratijñāṃ śatrunidhane drauṇireko vyacintayat // Bhmj_11.6 //",
  "GK19ksbhm11u:13": "atrāntare dadarśoccanyagrodhaśikharāśrayam /",
  "GK19ksbhm11u:14": "vadhyamānamulūkena vāyasānāṃ ca maṇḍalam // Bhmj_11.7 //",
  "GK19ksbhm11u:15": "kākānkālabalenaitya dṛṣṭvā ghūkena pātitān /",
  "GK19ksbhm11u:16": "tāmeva pāṇḍavavadhe nītiṃ drauṇiracintayat // Bhmj_11.8 //",
  "GK19ksbhm11u:17": "krudhā krūrāṃ dhiyaṃ kṛtvā kṛpabhojau vibodhya saḥ /",
  "GK19ksbhm11u:18": "uvāca niḥśvasandīrghaṃ dahyamāna iva krudhā // Bhmj_11.9 //",
  "GK19ksbhm11u:19": "rājā rājasahasrāṇāmekākī viṣamasthitiḥ /",
  "GK19ksbhm11u:20": "vyājādasmāsu jīvatsu bhīmasenena pātitaḥ // Bhmj_11.10 //",
  "GK19ksbhm11u:21": "śastraṃ vahadbhirmithyaiva dṛṣṭo 'smābhiḥ sa bhūtale /",
  "GK19ksbhm11u:22": "aśrupātapratīkārākṣamaiḥ klībatarairiva // Bhmj_11.11 //",
  "GK19ksbhm11u:23": "deśakālau samāśritya vadhyaḥ sarvātmanā ripuḥ /",
  "GK19ksbhm11u:24": "tasmātprasuptānadyaiva sānugānhanmi pāṇḍavān // Bhmj_11.12 //",
  "GK19ksbhm11u:25": "śrutvaitatkūṇitamanāḥ prasuptavadhapātakam /",
  "GK19ksbhm11u:26": "kṛpaḥ kṛpākulo nindannabhāṣata sutaṃ svasuḥ // Bhmj_11.13 //",
  "GK19ksbhm11u:27": "dhigetatkutsitaṃ yuddhaṃ niśceṣṭo yatra hanyate /",
  "GK19ksbhm11u:28": "bhagnadhairye 'pi no vīrāḥ praharanti yaśasvinaḥ // Bhmj_11.14 //",
  "GK19ksbhm11u:29": "bhavānahaṃ ca bhojaśca kartāraḥ prātareva tat /",
  "GK19ksbhm11u:30": "punaryena na yāsyāmo duryodhanavadhavyathām // Bhmj_11.15 //",
  "GK19ksbhm11u:31": "surāṇāmapi paryāptāḥ saṃhatāḥ samare vayam /",
  "GK19ksbhm11u:32": "pravekṣyāmo vairivanaṃ jvalitā iva pāvakāḥ // Bhmj_11.16 //",
  "GK19ksbhm11u:33": "gautamenetyabhihite saṃmate kṛtavarmaṇaḥ /",
  "GK19ksbhm11u:34": "niḥśvasansāśrunyanaḥ punardraiṇirabhāṣata // Bhmj_11.17 //",
  "GK19ksbhm11u:35": "māyopamā vidagdheṣu nikṛtyāvicarastu ye /",
  "GK19ksbhm11u:36": "spṛśantyadharmamaryādāṃ na te tatphalabhāginaḥ // Bhmj_11.18 //",
  "GK19ksbhm11u:37": "arjunena hataḥ karṇaḥ sa ca śāntanavo yathā /",
  "GK19ksbhm11u:38": "tatkiṃ na viditaṃ loke śeṣaṃ vaktuṃ na pāryate // Bhmj_11.19 //",
  "GK19ksbhm11u:39": "adhunā svasti dharmāya racito 'smai mayāñjaliḥ /",
  "GK19ksbhm11u:40": "parābhavamimaṃ tāvanna sahe marmadāraṇam // Bhmj_11.20 //",
  "GK19ksbhm11u:41": "drauṇirdāruṇasaṃkalpamityuktvā kṛtaniścayaḥ /",
  "GK19ksbhm11u:42": "rathena tamasi prāyācchibiraṃ kauravadviṣam // Bhmj_11.21 //",
  "GK19ksbhm11u:43": "kṛṣṇasya śāsanātpūrvaṃ yāteṣu śibirāntaram /",
  "GK19ksbhm11u:44": "saha sātyakinā yodhaiḥ pāṇḍaveṣu vinānugaiḥ // Bhmj_11.22 //",
  "GK19ksbhm11u:45": "kṛpabhojāvatikramya javāddrauṇirupāgataḥ /",
  "GK19ksbhm11u:46": "śibiradvāramāsādya mahadbhūtaṃ vyalokayat // Bhmj_11.23 //",
  "GK19ksbhm11u:47": "hariśmaśrujaṭānetraṃ jvālānāmiva saṃcayam /",
  "GK19ksbhm11u:48": "tamasāmiva saṃghātamañjanācalasaṃnibham // Bhmj_11.24 //",
  "GK19ksbhm11u:49": "sahasraśīrṣanayanaṃ sahasracaraṇādbhutam/",
  "GK19ksbhm11u:50": "sarpayajñopavītāṅgaṃ sarpakeyūrakaṅkaṇam /",
  "GK19ksbhm11u:51": "bhayaṃkaraṃ bhayasyāpi kṛtāntasyeva daivatam // Bhmj_11.25 //",
  "GK19ksbhm11u:52": "taddṛṣṭvā ghorasaṃrambho drauṇirdurjayavikramaḥ /",
  "GK19ksbhm11u:53": "sarvāyudhāvalī tasmai prāhiṇauddhairyabhūdharaḥ // Bhmj_11.26 //",
  "GK19ksbhm11u:54": "tānyāyudhāni sahasā jvalitānyeva tejasā /",
  "GK19ksbhm11u:55": "bṛmbhamāṇaṃ mahadbhūtaṃ tadāsādya kṣayaṃ yayuḥ // Bhmj_11.27 //",
  "GK19ksbhm11u:56": "drauṇistatastadākāramarīcibhyo vyalokayat /",
  "GK19ksbhm11u:57": "cakrāyudhasahasraṇi niḥsṛtāni sa vismayaḥ // Bhmj_11.28 //",
  "GK19ksbhm11u:58": "kṣaṇādgrastāyudhastena gāḍhānuśayatāpitaḥ /",
  "GK19ksbhm11u:59": "prastutaṃ malinaṃ karma nininda svayamākulaḥ // Bhmj_11.29 //",
  "GK19ksbhm11u:60": "sa rathādavatīryātha prayatastripurāntakam /",
  "GK19ksbhm11u:61": "śūlinaṃ śaraṇaṃ prāyāttatstotramukharānanaḥ // Bhmj_11.30 //",
  "GK19ksbhm11u:62": "ātmopahāraṃ rudrāya tasmindātuṃ samudyate /",
  "GK19ksbhm11u:63": "jvālākulā hemavedī purastātpratyadṛśyata // Bhmj_11.31 //",
  "GK19ksbhm11u:64": "tadudbhūtairgaṇairghorairnānāprāṇimukhodaraiḥ /",
  "GK19ksbhm11u:65": "nānāpraharaṇairdīptairvyoma kṣipramapūrayat // Bhmj_11.32 //",
  "GK19ksbhm11u:66": "tato vedyāṃ mahāvahnau jvalite droṇanandanaḥ /",
  "GK19ksbhm11u:67": "bhaktyātmānaṃ trinetrāya nivedyābhyapatatsvayam // Bhmj_11.33 //",
  "GK19ksbhm11u:68": "tuṣṭo 'tha bhagavānetya svayamandhakasūdanaḥ /",
  "GK19ksbhm11u:69": "tamuvāca svamevāṃśamuttiṣṭhāṅgirasāṃ vara // Bhmj_11.34 //",
  "GK19ksbhm11u:70": "saṃsthānāya mayaitāvatkṛtaṃ kaiṭabhavidviṣaḥ /",
  "GK19ksbhm11u:71": "ātgānaṃ viddhi me kṛṣṇaṃ sa me bahumataḥ sadā // Bhmj_11.35 //",
  "GK19ksbhm11u:72": "kālapakvāstu pāñcālā naiṣāṃ rakṣāsti kutracit /",
  "GK19ksbhm11u:73": "gṛhāṇa khaḍgametena dārayaitānaśaṅkitaḥ // Bhmj_11.36 //",
  "GK19ksbhm11u:74": "ityuktvā bhairave dīptaṃ khaḍgaṃ datvā tirohite /",
  "GK19ksbhm11u:75": "tadā vikārādduṣprekṣyo drauṇiḥ śibiramāviśat // Bhmj_11.37 //",
  "GK19ksbhm11u:76": "dvāri bhojakṛpau dhṛtvā vidrutānāṃ vadhāya saḥ /",
  "GK19ksbhm11u:77": "praviśya śayane śubhre dhṛṣṭadyumnaṃ vyalokayat // Bhmj_11.38 //",
  "GK19ksbhm11u:78": "tārahārāṃśunakharaṃ dīptakesarisaṃnibham /",
  "GK19ksbhm11u:79": "ghūrṇamānaśikhairdīpairvismayādiva vīkṣitam // Bhmj_11.39 //",
  "GK19ksbhm11u:80": "anekaraṇasaṃmardaśrāntairnidrāvimohitaiḥ /",
  "GK19ksbhm11u:81": "adhiṣṭhitaṃ bahubhaṭairmuktaparyāṇavāhanaiḥ // Bhmj_11.40 //",
  "GK19ksbhm11u:82": "taṃ dṛṣṭvā kopaśikhinā dahyamānaḥ smaranpituḥ /",
  "GK19ksbhm11u:83": "uttiṣṭha re gurughneti drauṇiruktvā padāspṛśat // Bhmj_11.41 //",
  "GK19ksbhm11u:84": "bodhitaḥ sahasā tena saṃbhramasvīkṛtāyudhaḥ /",
  "GK19ksbhm11u:85": "droṇaputraṃ parijñāya cakampe drupadātmajaḥ // Bhmj_11.42 //",
  "GK19ksbhm11u:86": "drauṇistamutthitaṃ vegānmālyavadbhiḥ śiroruhaiḥ /",
  "GK19ksbhm11u:87": "ākṛṣyāpātayatkaṇṭhe gāḍhaṃ niṣpīḍya vihvalam // Bhmj_11.43 //",
  "GK19ksbhm11u:88": "niṣpiṣyamāṇaḥ saṃrambhādraktodgārākulānanaḥ /",
  "GK19ksbhm11u:89": "kupitvā pādayordraiṇiṃ nakhairdantaiśca visphuran // Bhmj_11.44 //",
  "GK19ksbhm11u:90": "śastreṇa chindhi māṃ tūrṇaṃ mā padeneti gadgadam /",
  "GK19ksbhm11u:91": "kaṇṭharuddhasvaraḥ śvāsādasphuṭākṣaramabhyadhāt // Bhmj_11.45 //",
  "GK19ksbhm11u:92": "tamabravīddroṇasutaḥ pāpa padbhyāṃ nihanyase /",
  "GK19ksbhm11u:93": "lokānāṃ śastrapūtānāṃ na pāpaṃ gurughātinām // Bhmj_11.46 //",
  "GK19ksbhm11u:94": "ityuktvā caraṇāghātairjarjaraṃ tamapothayat /",
  "GK19ksbhm11u:95": "tasminhate tadvimardaśabdena bubudhe janaḥ // Bhmj_11.47 //",
  "GK19ksbhm11u:96": "***** dhṛṣṭadyumnavadhaḥ || 1 || *****",
  "GK19ksbhm11u:97": "tato drauṇiṃ samālekya ripuraktacchaṭāṅkitam /",
  "GK19ksbhm11u:98": "pāñcālā draupadeyāśca kālo 'yamiti menire // Bhmj_11.48 //",
  "GK19ksbhm11u:99": "nidrāśeṣāruṇadṛśāṃ śastrāṇyānayatāṃ mithaḥ /",
  "GK19ksbhm11u:100": "krośatāṃ varma varmeti teṣāmāsīnmahāsvanaḥ // Bhmj_11.49 //",
  "GK19ksbhm11u:101": "kva gajaḥ kva rathaḥ kvāśvaḥ kva cāpaṃ kva śilīmukhāḥ /",
  "GK19ksbhm11u:102": "iti bruvāṇānkhaṅgena drauṇiściccheda tānkrudhā // Bhmj_11.50 //",
  "GK19ksbhm11u:103": "hatvottamaujasaṃ padbhyāṃ kaṇṭhe kaṭhinavikramaḥ /",
  "GK19ksbhm11u:104": "gadāhastaṃ yudhāmanyuṃ niṣpipeṣa virāviṇam // Bhmj_11.51 //",
  "GK19ksbhm11u:105": "tataḥ khaṅgena śūrāṇāṃ śirāṃsi bhuvi pātayan /",
  "GK19ksbhm11u:106": "kālarātryāḥ sa vidadhe vinodaṃ kandukairiva // Bhmj_11.52 //",
  "GK19ksbhm11u:107": "kṛṣṇāsutānāpatato draupadeyānasaṃbhramāt /",
  "GK19ksbhm11u:108": "cakāra karavālena sa kālakavalocitān // Bhmj_11.53 //",
  "GK19ksbhm11u:109": "prativindhye hate vīre sutasome nipātite /",
  "GK19ksbhm11u:110": "śrutakīrtai śatānīke chinne ca śrutavarmaṇi // Bhmj_11.54 //",
  "GK19ksbhm11u:111": "śikhaṇḍī kṛṣṭakodaṇḍaḥ śarairdraiṇimapūrayat /",
  "GK19ksbhm11u:112": "droṇaputro 'pi khaḍgena kruddhastaṃ vidadhe dvidhā // Bhmj_11.55 //",
  "GK19ksbhm11u:113": "***** draupadeyaśikhaṇḍivadhaḥ || 2 || *****",
  "GK19ksbhm11u:114": "kopādācāryaputreṇa vadhyamānāḥ prabhadrakāḥ /",
  "GK19ksbhm11u:115": "matsyasṛñjayapāñcālāḥ kālaṃ dadṛśurantike // Bhmj_11.56 //",
  "GK19ksbhm11u:116": "asitaṃ raktavasanaṃ raktamālyānulepanam /",
  "GK19ksbhm11u:117": "hasantaṃ vīkṣya taṃ vīrāḥ saṃhāraṃ menire sphṛṭam // Bhmj_11.57 //",
  "GK19ksbhm11u:118": "ghanāndhakāre vīrāṇāṃ rathakuñjaravājinām /",
  "GK19ksbhm11u:119": "yodhānāṃ ca kṣayaṃ dvāri cakraturbhojagautamau // Bhmj_11.58 //",
  "GK19ksbhm11u:120": "evamakṣauhiṇīṃ hatvā vasāraktānulepanaḥ /",
  "GK19ksbhm11u:121": "niryayau raudracarito drauṇirbhairavavigrahaḥ // Bhmj_11.59 //",
  "GK19ksbhm11u:122": "sa gatvā sahitastūrṇaṃ hārdikyena kṛpeṇa ca /",
  "GK19ksbhm11u:123": "dadarśa bhuvi rājānaṃ bhagnaśaktiṃ suyodhanam // Bhmj_11.60 //",
  "GK19ksbhm11u:124": "vārayantaṃ gadāgreṇa muhuḥ kravyādamaṇḍalam /",
  "GK19ksbhm11u:125": "vyathayā marmadinyā bhakṣayantaṃ vasuṃdharām // Bhmj_11.61 //",
  "GK19ksbhm11u:126": "śoṇitaṃ vadanodvāntaṃ parimṛjyāśu tasya te /",
  "GK19ksbhm11u:127": "sarvapāñcalasaṃhārakathayā vavṛṣuḥ sudhām // Bhmj_11.62 //",
  "GK19ksbhm11u:128": "sa kiṃciccheṣajīvo 'pi tacchrutvā vṛttakandharaḥ /",
  "GK19ksbhm11u:129": "paryantagrasitāvyaktabhāṣitaiḥ praśaśaṃsa tān // Bhmj_11.63 //",
  "GK19ksbhm11u:130": "sa pariṣvajya yatnena drauṇiṃ sāśruvilocanam /",
  "GK19ksbhm11u:131": "punarnaḥ saṃgamaḥ svarge bhūyādityavadacchanaiḥ // Bhmj_11.64 //",
  "GK19ksbhm11u:132": "aṅge vidhāya dayitāṃ sa gadāṃ sadaiva bhuktāṃ ciraṃ vasumatīmavagūhya dorbhyām /",
  "GK19ksbhm11u:133": "karṇāvalambitabalodgatajīvacāro vīraḥ śanaiḥ suravadhūpraṇayī babhūva // Bhmj_11.65 //",
  "GK19ksbhm11u:134": "***** duryodhanasvargagamanam || 3 || *****",
  "GK19ksbhm11u:135": "atrāntare prayātāyāṃ kṣapāyāṃ kṣapitārayaḥ /",
  "GK19ksbhm11u:136": "jajñire pāṇḍavāḥ karma dāruṇaṃ drauṇinā kṛtam // Bhmj_11.66 //",
  "GK19ksbhm11u:137": "muktānmahābhayāttasmātkṛṣṇā śrutvā hatānsutān /",
  "GK19ksbhm11u:138": "anujāṃśca sapāñcālānpapāta bhuvi mūrcchitā // Bhmj_11.67 //",
  "GK19ksbhm11u:139": "śanakairlabdhasaṃjñā sā vilapantī sulocanā /",
  "GK19ksbhm11u:140": "pārthānāṃ śokavidhurā hṛdayāni vyadārayat // Bhmj_11.68 //",
  "GK19ksbhm11u:141": "yāvanna viśikhaiḥ kṛttamuttamāṅgaṃ vikarmaṇaḥ /",
  "GK19ksbhm11u:142": "drauṇerdṛṣṭaṃ mayā tāvajjīvitasyāyamañjaliḥ // Bhmj_11.69 //",
  "GK19ksbhm11u:143": "ityuktvā draupadī duḥkhādvīkṣamāṇā vṛkodaram /",
  "GK19ksbhm11u:144": "novāca kiṃcitsaṃtāpānmaraṇe kṛtaniścayā // Bhmj_11.70 //",
  "GK19ksbhm11u:145": "tato bhīmaḥ priyāśokaśūlenābhyāhato hṛdi /",
  "GK19ksbhm11u:146": "prayayau rathamāruhya droṇaputrajighāṃsayā // Bhmj_11.71 //",
  "GK19ksbhm11u:147": "vrajatastasya kālagneriva lokāndidhakṣataḥ /",
  "GK19ksbhm11u:148": "dhūmamālābhramaṃ cakre lalāṭe bhrukuṭiḥ kṣaṇam // Bhmj_11.72 //",
  "GK19ksbhm11u:149": "raudro rudrāśrayo drauṇiḥ kruddhaśca pavanātmajaḥ /",
  "GK19ksbhm11u:150": "gacchāmaḥ pṛṣṭhatastasmādityūce pāṇḍavānhariḥ // Bhmj_11.73 //",
  "GK19ksbhm11u:151": "tataḥ kirīṭipramukhāḥ saha sātyakinā yayuḥ /",
  "GK19ksbhm11u:152": "rathinaḥ seṣavaḥ sarve pāṇḍuputrā yuyutsavaḥ // Bhmj_11.74 //",
  "GK19ksbhm11u:153": "tataste drauṇimāsādya prasuptavadhapātakāt /",
  "GK19ksbhm11u:154": "dhvastacchāyaṃ kriyāhīnaṃ kopenākulatāṃ yayuḥ // Bhmj_11.75 //",
  "GK19ksbhm11u:155": "abhidrute bhīmasene drauṇiṃ nakulasārathau/",
  "GK19ksbhm11u:156": "pādacārī ghṛtābhyaktaḥ sarasvatyāstaṭe kṣaṇam /",
  "GK19ksbhm11u:157": "apāṇḍavaṃ jagatkartuṃ brahmaśīrṣāstramādade // Bhmj_11.76 //",
  "GK19ksbhm11u:158": "tadvināśāya pārtho 'pi brahmāstramasṛjatkṛtī /",
  "GK19ksbhm11u:159": "yenābhavandiśo vyāptāḥ kampātaṅkataraṅgitāḥ // Bhmj_11.77 //",
  "GK19ksbhm11u:160": "atrāntare samabhyāyānnāradena saha svayam /",
  "GK19ksbhm11u:161": "muniḥ satyavatīsūnurbhagavānbhūtabhāvanaḥ // Bhmj_11.78 //",
  "GK19ksbhm11u:162": "so 'bravīddroṇatanayaṃ jiṣṇuṃ ca kṣayaśaṅkitaḥ /",
  "GK19ksbhm11u:163": "ayaṃ divyāstrasaṃmarde hatavīraṃ jagaddahet // Bhmj_11.79 //",
  "GK19ksbhm11u:164": "pārtha saṃhara divyāstraṃ tvaṃ ca drauṇe girā mama /",
  "GK19ksbhm11u:165": "akāṇḍe mā bhavantvete lokāḥ pralayabhāginaḥ // Bhmj_11.80 //",
  "GK19ksbhm11u:166": "ityukte muninā kṣipraṃ saṃjahārāstramarjunaḥ /",
  "GK19ksbhm11u:167": "drauṇiḥ prayogakuśalo neśo 'bhūdastrasaṃhṛtau // Bhmj_11.81 //",
  "GK19ksbhm11u:168": "tataḥ prajvalite loke vyathite suramaṇḍale /",
  "GK19ksbhm11u:169": "abhimanyuvadhūgarbhe drauṇirastramapātayat // Bhmj_11.82 //",
  "GK19ksbhm11u:170": "bhūyastatpratiṣedhāya putrasaṃtānavāñchayā /",
  "GK19ksbhm11u:171": "nindandroṇasutaṃ pārthastadastraṃ kupito 'sṛjat // Bhmj_11.83 //",
  "GK19ksbhm11u:172": "tataḥ kṛṣṇo 'vadajjiṣṇuṃ viṣaṇṇaṃ saṃtatikṣayāt /",
  "GK19ksbhm11u:173": "mā śuco dhairyajaladhe na te vaṃśo vinaṅkṣyati // Bhmj_11.84 //",
  "GK19ksbhm11u:174": "jātvenaṃ jīvayiṣyāmi dagdhamapyastratejasā /",
  "GK19ksbhm11u:175": "uttarāyāḥ sthitaṃ garbhe saubhadratanayaṃ śiśum // Bhmj_11.85 //",
  "GK19ksbhm11u:176": "iti bruvāṇe govinde pārthāstre jvalite bhuvi /",
  "GK19ksbhm11u:177": "uvāca droṇatanayaṃ vyāso devarṣiṇā saha // Bhmj_11.86 //",
  "GK19ksbhm11u:178": "tyaja cūḍāmaṇiṃ rakṣa jīvaratnaṃ mahāstrataḥ /",
  "GK19ksbhm11u:179": "aśaktaśca pramādī ca jānīṣe na yathocitam // Bhmj_11.87 //",
  "GK19ksbhm11u:180": "vatsarāṇāṃ sahasra tvaṃ visrarogī samāhitaḥ /",
  "GK19ksbhm11u:181": "durbhikṣavyañjako duḥkhī vijane vicariṣyasi // Bhmj_11.88 //",
  "GK19ksbhm11u:182": "iti śapto gurusutaḥ kopādvyāsamabhāṣata /",
  "GK19ksbhm11u:183": "vyasane patitaḥ kasmātkṣipto 'haṃ saṃkaṭe tvayā // Bhmj_11.89 //",
  "GK19ksbhm11u:184": "yatra yatra bhaviṣyāmi tatra tatra mamānugaḥ /",
  "GK19ksbhm11u:185": "tvamapi pratiśāpena mune nityaṃ bhaviṣyasi // Bhmj_11.90 //",
  "GK19ksbhm11u:186": "ityuktvāstropahārāya cūḍāratnaṃ vitīrya saḥ /",
  "GK19ksbhm11u:187": "prayayau droṇatanayo muniśca muninā saha // Bhmj_11.91 //",
  "GK19ksbhm11u:188": "tataḥ praśānte brahmāstre pāṇḍavāḥ saha śauriṇā /",
  "GK19ksbhm11u:189": "cūḍāratnaṃ samādāya yājñasenyai nyavedayan // Bhmj_11.92 //",
  "GK19ksbhm11u:190": "***** aiṣīkamastram || 4 || *****",
  "GK19ksbhm11u:191": "evaṃ prasādya pāñcālīṃ śokārtāḥ pāṇḍunandanāḥ /",
  "GK19ksbhm11u:192": "pāñcālakadanaṃ ghoraṃ dhyāyanto na yayurvṛtim // Bhmj_11.93 //",
  "GK19ksbhm11u:193": "saṃjayeneti kathite śrutvā rājāmbikāsutaḥ /",
  "GK19ksbhm11u:194": "papāta mūrcchito mohāllīnaśokānalaḥ kṣaṇam // Bhmj_11.94 //",
  "GK19ksbhm11u:195": "taṃ śītasalilāpūrairāśvāsyovāca saṃjayaḥ /",
  "GK19ksbhm11u:196": "mantriṇaśca priyānputrāñśocantaṃ vyathitendriyam // Bhmj_11.95 //",
  "GK19ksbhm11u:197": "svapnamāyāvilasitaiḥ paryante śokamohadaiḥ /",
  "GK19ksbhm11u:198": "saṃsāravibhramairdhīrāḥ spṛśyante na manīṣiṇaḥ // Bhmj_11.96 //",
  "GK19ksbhm11u:199": "devaindrajālikakṛtaiśchāyākrīḍanakairiva /",
  "GK19ksbhm11u:200": "putrairdhanaiśca dāraiśca na muhyanti madāśayāḥ // Bhmj_11.97 //",
  "GK19ksbhm11u:201": "upekṣitastvayaivāyaṃ durnayādbāndhavakṣayaḥ /",
  "GK19ksbhm11u:202": "vṛtte tasminnamithyaiva śokaṃ kartumihārhasi // Bhmj_11.98 //",
  "GK19ksbhm11u:203": "prabhāvaprabhavairbhāvairmāyāvibhavabhāvitaiḥ /",
  "GK19ksbhm11u:204": "abhāvaniṣṭhāparyantaiḥ satāṃ na kriyate bhramaḥ // Bhmj_11.99 //",
  "GK19ksbhm11u:205": "svakarmamudrite loke niyatau pralayodadhau /",
  "GK19ksbhm11u:206": "kalayāmi na kālasya kaśca tāvati vartate // Bhmj_11.100 //",
  "GK19ksbhm11u:207": "ityādi saṃjayamukhā nṛpamugramohabhagnaṃ svayaṃ vihitadurnayadṛṣṭapāram /",
  "GK19ksbhm11u:208": "helāvalatkalabhacñcalakarṇatālalolāṃ bhavasthitikathāmasakṛttamūcuḥ // Bhmj_11.101 //",
  "GK19ksbhm11u:209": "iti kṣemendraviracitāyāṃ bhāratamañjaryāṃ sauptikaparva"
}