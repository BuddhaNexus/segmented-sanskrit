{
  "GK19ksbhm14u:0": "//āśvamedhikaṃ parva//",
  "GK19ksbhm14u:1": "nārāyaṇaṃ namaskṛtya naraṃ caiva narottamam /",
  "GK19ksbhm14u:2": "devīṃ sarasvatīṃ vyāsaṃ tato jayamudīrayet // Bhmj_14.1 //",
  "GK19ksbhm14u:3": "yaśaḥ śaśāṅkośeṣe 'staṃ prayāte bhīṣmabhāsvati /",
  "GK19ksbhm14u:4": "papāta dharmatanayaḥ śokamlānamukhāmbujaḥ // Bhmj_14.2 //",
  "GK19ksbhm14u:5": "mohamūrcchākule tasminviṣaṇṇe saha bāndhavaiḥ /",
  "GK19ksbhm14u:6": "śucaṃ saṃstambhya śanakairdhṛtarāṣṭrastamabravīt // Bhmj_14.3 //",
  "GK19ksbhm14u:7": "uttiṣṭha pṛthivīpāla na dhairyaṃ hātumarhasi /",
  "GK19ksbhm14u:8": "saṃsārāsāracaritaṃ munibhyaḥ śrutavānasi // Bhmj_14.4 //",
  "GK19ksbhm14u:9": "śokasyāvasaro nāyaṃ svadharmādacyutasya te /",
  "GK19ksbhm14u:10": "pāhi sattvasudhāsindho prajāpatiriva prajāḥ // Bhmj_14.5 //",
  "GK19ksbhm14u:11": "śocitavyo mamaivāyaṃ kālaḥ samucito 'dhunā /",
  "GK19ksbhm14u:12": "vidurasya giro yena na śrutāḥ sunayojjvalāḥ // Bhmj_14.6 //",
  "GK19ksbhm14u:13": "ityukte dhṛtarāṣṭreṇa kṛṣṇaḥ kṣmāpālamabhyadhāt /",
  "GK19ksbhm14u:14": "mā śucaḥ kṣatradharmeṇa labdhāṃ bhuṅkṣva mahīmimām // Bhmj_14.7 //",
  "GK19ksbhm14u:15": "duḥkhibhyo hi palāyante dhairyāyattā vibhūtayaḥ /",
  "GK19ksbhm14u:16": "yajasva vijayī rājanpāpaśaṅkāpanuttaye // Bhmj_14.8 //",
  "GK19ksbhm14u:17": "aśvamedhena vidhinā yātastadyogyatāmasi /",
  "GK19ksbhm14u:18": "etanmunivacaḥ śrutvā yajñe baddhamanorathaḥ // Bhmj_14.9 //",
  "GK19ksbhm14u:19": "dhanamūlāṃ kriyaṃ matvā rājyakośamacintayat /",
  "GK19ksbhm14u:20": "so 'tha sarvānsamāhūya kośādhyakṣānmakhotsukaḥ // Bhmj_14.10 //",
  "GK19ksbhm14u:21": "tebhyaḥ śuśrāva sarvasvaṃ kṣayitaṃ yudhi kauravaiḥ /",
  "GK19ksbhm14u:22": "aśvamedhe nirāśaṃ taṃ vijñāya draviṇaṃ vinā // Bhmj_14.11 //",
  "GK19ksbhm14u:23": "uvāca karuṇāsindhurvyāso nirvāpayanniva /",
  "GK19ksbhm14u:24": "maruttasya kṣitipateryajñaśeṣaṃ himācale // Bhmj_14.12 //",
  "GK19ksbhm14u:25": "kāñcanaṃ vidyate bhūri yajñārthaṃ tadavāpsyasi /",
  "GK19ksbhm14u:26": "śrutvaitatpāṇḍuputreṇa maruttasya mahīpateḥ // Bhmj_14.13 //",
  "GK19ksbhm14u:27": "mahāyajñakathāṃ pṛṣṭaḥ provāca munipuṃgavaḥ /",
  "GK19ksbhm14u:28": "mahīyasi manorvaṃśe mahatāṃ yaśasāṃ nidhiḥ // Bhmj_14.14 //",
  "GK19ksbhm14u:29": "karaṃdhamakule śrīmānmarutto 'bhūnmahīpatiḥ /",
  "GK19ksbhm14u:30": "tamandraḥ spardhate nityaṃ na sa śaknoti ceṣṭitaiḥ // Bhmj_14.15 //",
  "GK19ksbhm14u:31": "yadā tadā manyutapto rahaḥ prāha bṛhaspatim /",
  "GK19ksbhm14u:32": "karaṃdhamo 'bhavadyājyastvatpituḥ pṛthivīpatiḥ // Bhmj_14.16 //",
  "GK19ksbhm14u:33": "kramāttavāpi tatsūnurmaruttastadguṇādhikaḥ /",
  "GK19ksbhm14u:34": "kiṃtu taddarpasaṃgharṣādbhavantamahamarthaye // Bhmj_14.17 //",
  "GK19ksbhm14u:35": "na yājakapadaṃ tasya gantavyaṃ bhavatādhunā /",
  "GK19ksbhm14u:36": "ityarthito maghavatā tathetyūce bṛhaspatiḥ // Bhmj_14.18 //",
  "GK19ksbhm14u:37": "adyaprabhṛti martyo me na yājya iti saṃvidā /",
  "GK19ksbhm14u:38": "atrāntare narapatirmarutto vipulaṃ kratum // Bhmj_14.19 //",
  "GK19ksbhm14u:39": "āhartuṃ kṛtasaṃkalpo bṛhaspatimupāyayau /",
  "GK19ksbhm14u:40": "sa tenābhyarthito yatnādyājako 'stu bhavāniti // Bhmj_14.20 //",
  "GK19ksbhm14u:41": "na cakārābhyupagamaṃ śakrapraṇayayantritaḥ /",
  "GK19ksbhm14u:42": "pratyākhyātaḥ sa guruṇā rājā lajjānatānanaḥ // Bhmj_14.21 //",
  "GK19ksbhm14u:43": "vrajanvinaṣṭasaṃkalpo dadarśa pathi nāradam /",
  "GK19ksbhm14u:44": "nārado 'pi tamālokya pṛṣṭvā śrutvā ca tatkathām // Bhmj_14.22 //",
  "GK19ksbhm14u:45": "uvāca tyaja saṃtāpaṃ mā rājanvimanā bhava /",
  "GK19ksbhm14u:46": "saṃvartākhyo nidhirdhāmnāmanujo 'sti bṛhaspateḥ // Bhmj_14.23 //",
  "GK19ksbhm14u:47": "svacchandacārī yogīndro vārāṇasyāṃ sa vartate /",
  "GK19ksbhm14u:48": "saṃvartaḥ sa parijñeyo vrajethāḥ śaraṇaṃ tu tam // Bhmj_14.24 //",
  "GK19ksbhm14u:49": "nivedito 'haṃ keneti pṛṣṭastena tvamānataḥ /",
  "GK19ksbhm14u:50": "bravīthā nārado mahyaṃ tvāmihasthaṃ nyavedayat // Bhmj_14.25 //",
  "GK19ksbhm14u:51": "kathayitvā prahṛṣṭaśca jvalitaṃ jātavedasam /",
  "GK19ksbhm14u:52": "ityuktvā prārthayethāstaṃ yājakaṃ yajvanāṃ varaḥ // Bhmj_14.26 //",
  "GK19ksbhm14u:53": "nāradenetyabhihite bhūpālo harṣanirbharaḥ /",
  "GK19ksbhm14u:54": "taduktaṃ vidadhe sarvaṃ gatvā vāraṇasīṃ svayam // Bhmj_14.27 //",
  "GK19ksbhm14u:55": "saṃtyaktonmattarūpaṃ sa prāpya taṃ yājakaṃ nṛpaḥ /",
  "GK19ksbhm14u:56": "cakāra tadanujñātastapo himagirestaṭe // Bhmj_14.28 //",
  "GK19ksbhm14u:57": "tapasā yajñavittārthī devamārādhya śaṃkaram /",
  "GK19ksbhm14u:58": "anekameruvipulaṃ lebhe hema sa pārthivaḥ // Bhmj_14.29 //",
  "GK19ksbhm14u:59": "haimeṣu sarvabhāṇḍeṣu raciteṣvatha śilpibhiḥ /",
  "GK19ksbhm14u:60": "saṃpūrṇo yajñasaṃbhāraḥ prāvartata mahībhujaḥ // Bhmj_14.30 //",
  "GK19ksbhm14u:61": "samṛddhimatulāṃ tasya yajñe jñātvā bṛhaspatiḥ /",
  "GK19ksbhm14u:62": "saṃvartakaṃ yājakaṃ ca saṃtaptastanutāṃ yayau // Bhmj_14.31 //",
  "GK19ksbhm14u:63": "sa śokakāraṇaṃ pṛṣṭaḥ śakreṇa mlānamānasaḥ /",
  "GK19ksbhm14u:64": "maruttayajñavibhavaṃ bhrātṛspardhākulo 'vadat // Bhmj_14.32 //",
  "GK19ksbhm14u:65": "tataḥ śatakratustasya prītye guruvatsalaḥ /",
  "GK19ksbhm14u:66": "uvāca vahnimāhūya maruttaṃ vraja pārthivam // Bhmj_14.33 //",
  "GK19ksbhm14u:67": "gatvā madvacasā vācyaḥ sa ca rājā tvayā kratau /",
  "GK19ksbhm14u:68": "yathā saṃvartakaṃ tyaktvā bhavedyājyo bṛhaspateḥ // Bhmj_14.34 //",
  "GK19ksbhm14u:69": "ityuktaḥ surarājena gatvā vahnirnarādhipam /",
  "GK19ksbhm14u:70": "śakrasaṃdeśamavadanna ca rājābhyamanyata // Bhmj_14.35 //",
  "GK19ksbhm14u:71": "tataḥ pratinivṛtto 'tha punargaccheti vajriṇā /",
  "GK19ksbhm14u:72": "preritaḥ pāvako 'vādītsaṃvartabhayakampitaḥ // Bhmj_14.36 //",
  "GK19ksbhm14u:73": "sa tatra brahmacārī māmūce saṃvartakaḥ krudhā /",
  "GK19ksbhm14u:74": "tejorāśiḥ śapeyaṃ tvāṃ sameṣyasi punaryadi // Bhmj_14.37 //",
  "GK19ksbhm14u:75": "śrutvaitatkātaro 'sīti taṃ jagāda puraṃdaraḥ /",
  "GK19ksbhm14u:76": "jvalanaḥ pratyuvācātha taṃ kopahaviṣā jvalan // Bhmj_14.38 //",
  "GK19ksbhm14u:77": "aho nu brahmakopāgnirahalyāvallabhasya te /",
  "GK19ksbhm14u:78": "pramādādatha kopādvā mohādvā śakra vismṛtaḥ // Bhmj_14.39 //",
  "GK19ksbhm14u:79": "sa yadā cyavanaḥ kopānmahādaṃṣṭraṃ mahāsuram /",
  "GK19ksbhm14u:80": "asṛjattvadvadhāyograṃ tadā dhairyaṃ kva te gatam // Bhmj_14.40 //",
  "GK19ksbhm14u:81": "duḥsahā brāhmaṇaruṣo nirdahanti jagatkṣaṇāt /",
  "GK19ksbhm14u:82": "ukte hutāśaneneti na śaktaḥ kiṃcidabravīt // Bhmj_14.41 //",
  "GK19ksbhm14u:83": "visṛṣṭaḥ punarindreṇa gandharvādhipatirnṛpam /",
  "GK19ksbhm14u:84": "ūce guruṃ bhajasveti na ca rājā tamagrahīt // Bhmj_14.42 //",
  "GK19ksbhm14u:85": "tataḥ kruddhaḥ svayaṃ vajrī garjangambhīraniḥsvanaḥ /",
  "GK19ksbhm14u:86": "yajñāṅganaṃ narapatervyomnā meghairvṛto yayau // Bhmj_14.43 //",
  "GK19ksbhm14u:87": "prabhāvenātha mahatā saṃvartasyogratejasaḥ /",
  "GK19ksbhm14u:88": "śatamanyusamutsṛṣṭaṃ vajraṃ meghānsamāviśat // Bhmj_14.44 //",
  "GK19ksbhm14u:89": "tanmantraśaktyā vivaśaḥ samāhūto 'tha vṛtrahā /",
  "GK19ksbhm14u:90": "āyayau saha bhāgārhaistridaśaiḥ somapīthibhiḥ // Bhmj_14.45 //",
  "GK19ksbhm14u:91": "pūjito 'tha maruttena śāntamanyuḥ śatakratuḥ /",
  "GK19ksbhm14u:92": "yajñakriyāparikare paricaryāparo 'bhavat // Bhmj_14.46 //",
  "GK19ksbhm14u:93": "tadājñayā surāḥ sarve yajñasaṃbhārakāriṇaḥ /",
  "GK19ksbhm14u:94": "babhūvurbhūmipālasya nideśavaśagāściram // Bhmj_14.47 //",
  "GK19ksbhm14u:95": "evaṃ tasyendrajayino rājñaḥ saṃvartatejasā /",
  "GK19ksbhm14u:96": "nirvighnaḥ pūrṇatāṃ prāpa yajñaḥ kanakavarṣiṇaḥ // Bhmj_14.48 //",
  "GK19ksbhm14u:97": "taccheṣadviṇena tvaṃ hayamedhamahāmakham /",
  "GK19ksbhm14u:98": "prayataḥ prāpnuhītyuktāva virarāma munīśvaraḥ // Bhmj_14.49 //",
  "GK19ksbhm14u:99": "***** saṃvartamaruttīyam // 1 // *****",
  "GK19ksbhm14u:100": "kṛṣṇenāśvārasyamāno 'tha munibhirbhrātṛbhistathā /",
  "GK19ksbhm14u:101": "aśvamedhadhṛtodyogo dhṛtiṃ lebhe yudhiṣṭhiraḥ // Bhmj_14.50 //",
  "GK19ksbhm14u:102": "draṣṭuṃ tataḥ prajākāryaṃ pravṛtte dharmanandane /",
  "GK19ksbhm14u:103": "antardadhe tamāmantrya sarvaṃ tanmunimaṇḍalam // Bhmj_14.51 //",
  "GK19ksbhm14u:104": "hemaratnalatākānte vikāsikanakāmbuje /",
  "GK19ksbhm14u:105": "atha kṛṣṇau sabhodyāne svairaṃ prītyā vijahvatuḥ // Bhmj_14.52 //",
  "GK19ksbhm14u:106": "kathānte tatra kaṃsāriṃ subhadrāvallabho 'vadat /",
  "GK19ksbhm14u:107": "yuddhārambhe tvayoktaṃ yatsarvaṃ tanmama vismṛtam // Bhmj_14.53 //",
  "GK19ksbhm14u:108": "visṛṣṭaṃ paramārthajña jñānaṃ tatpunarucyatām /",
  "GK19ksbhm14u:109": "ityuktaḥ pāṇḍuputreṇa viṣaṇṇaḥ keśavo 'bravīt // Bhmj_14.54 //",
  "GK19ksbhm14u:110": "aho tvayāvadhānena na śrutaṃ tadabuddhinā /",
  "GK19ksbhm14u:111": "na śakyate punarvaktuṃ tathāpi śrūyatāmidam // Bhmj_14.55 //",
  "GK19ksbhm14u:112": "brāhmaṇena purā kaścijjīvanmuktadaśāṃ śritaḥ /",
  "GK19ksbhm14u:113": "siddhaḥ pṛṣṭo 'vadatsarvaṃ vijñāya bhavavibhramam // Bhmj_14.56 //",
  "GK19ksbhm14u:114": "asminnasatyasaṃghāte bhūtānāṃ pāñcabhautike /",
  "GK19ksbhm14u:115": "guṇairnibaddhaḥ pakṣīva jīvastiṣṭhati pañjare // Bhmj_14.57 //",
  "GK19ksbhm14u:116": "ahaṃkāraikasārāṇāṃ dehināṃ vismṛtātmanām /",
  "GK19ksbhm14u:117": "cakravatparivartante sukhaduḥkhakṣayodayāḥ // Bhmj_14.58 //",
  "GK19ksbhm14u:118": "dehānte karmasaṃsargājjantavaḥ śubhaduṣkṛtaiḥ /",
  "GK19ksbhm14u:119": "tārārūpā vimānāni bhajante narakāṇi vā // Bhmj_14.59 //",
  "GK19ksbhm14u:120": "tato bhogakṣaye garbhaṃ jarāyupariveṣṭitam /",
  "GK19ksbhm14u:121": "śukrarūpo viśatyātmā yonyāṃ śoṇitasaṃplutaḥ // Bhmj_14.60 //",
  "GK19ksbhm14u:122": "niḥśvāsāgrairyathā vāri līyate darpaṇodare /",
  "GK19ksbhm14u:123": "nirdhūtasya yathā vahnerūṣmā vyomni prasarpati // Bhmj_14.61 //",
  "GK19ksbhm14u:124": "yathā vā kusumāmodaḥ praviśatyantare 'nile /",
  "GK19ksbhm14u:125": "rāgaḥ saṃkrāntimāyāti sarandhre sphaṭike yathā // Bhmj_14.62 //",
  "GK19ksbhm14u:126": "tathā durlakṣyasaṃcāro garbhamātmā pradhāvati /",
  "GK19ksbhm14u:127": "bibhrāṇo vāsanāmantaḥ śītasparśamivānilaḥ // Bhmj_14.63 //",
  "GK19ksbhm14u:128": "jātasya jāyate tasya vardhamānasya vardhate /",
  "GK19ksbhm14u:129": "tṛṣṇātanturbhi(rvi)",
  "GK19ksbhm14u:130": "sasyeva śuṣyato na tu śuṣyati // Bhmj_14.64 //",
  "GK19ksbhm14u:131": "vivekādduḥkhasaṃyogādvairāgyaṃ gāḍhamāśritaḥ /",
  "GK19ksbhm14u:132": "nāhamasasmīti mantreṇa mucyate brahmadīkṣitaḥ // Bhmj_14.65 //",
  "GK19ksbhm14u:133": "pūrvaṃ bhāveṣu nirvāṇastataḥ kāraṇavṛttiṣu /",
  "GK19ksbhm14u:134": "nirālambadaśāmetya yogī brahmaṇi līyate // Bhmj_14.66 //",
  "GK19ksbhm14u:135": "sarvataḥ pāṇicaraṇaṃ sarvavyāpinamavyayam /",
  "GK19ksbhm14u:136": "ātmānaṃ sa parijñāya satyāmetāṃ bhavasthitim // Bhmj_14.67 //",
  "GK19ksbhm14u:137": "ityuktvā brāhmaṇaṃ siddhaḥ sahasāntaradhīyata /",
  "GK19ksbhm14u:138": "idamanyacca kaunteya śreyase prayataḥ śṛṇu // Bhmj_14.68 //",
  "GK19ksbhm14u:139": "uvāca brāhmaṇī kācidbhartāraṃ vijane purā /",
  "GK19ksbhm14u:140": "gamiṣyāmi gatiṃ sādhvī kāmahaṃ tvatparāyaṇā // Bhmj_14.69 //",
  "GK19ksbhm14u:141": "iti pṛṣṭastayā viprastāmuvāca smitānanaḥ /",
  "GK19ksbhm14u:142": "agnirvaiśvānaro nāma sthito 'ntaḥ kila dehinām // Bhmj_14.70 //",
  "GK19ksbhm14u:143": "indriyāṇi manobuddhistasya saptārciṣa smṛtāḥ /",
  "GK19ksbhm14u:144": "viṣayāścāsya samidho bhoktāraḥ sapta cartvijaḥ /",
  "GK19ksbhm14u:145": "tasminhate sarvamidaṃ jāyate ca punaḥ punaḥ // Bhmj_14.71 //",
  "GK19ksbhm14u:146": "indriyāṇīva manaso manasteṣāṃ ca sarvadā /",
  "GK19ksbhm14u:147": "parasparopakāreṇa prīyate dehasaṃgame // Bhmj_14.72 //",
  "GK19ksbhm14u:148": "manobuddhiprabhṛtayaḥ saptendriyamṛgāḥ purā /",
  "GK19ksbhm14u:149": "atarkeṇaiva nihatā rājñā yogamayaiḥ śaraiḥ // Bhmj_14.73 //",
  "GK19ksbhm14u:150": "jitendriyasya no kiṃcididaṃ tasyaiva cākhilam /",
  "GK19ksbhm14u:151": "viṣayā viṣayasyeti janako vipramabhyadāt // Bhmj_14.74 //",
  "GK19ksbhm14u:152": "tasmādasaktamanasā dṛṣṭvā sarvamidaṃ mayā /",
  "GK19ksbhm14u:153": "tyaktaṃ parāvarajñena bhavāvirbhāvaśāntaye // Bhmj_14.75 //",
  "GK19ksbhm14u:154": "gatistavāpi kalyāṇi madbhāvasadṛśī sadā /",
  "GK19ksbhm14u:155": "brahmāraṇīsamudbhūtaṃ jānīhi jñānapāvakam // Bhmj_14.76 //",
  "GK19ksbhm14u:156": "etatpatyurvacaḥ śrutvā brāhmaṇī tattvadarśinī /",
  "GK19ksbhm14u:157": "kṣetrakṣetrajñavijñānaṃ paraṃ prāvīṇyamāyayau // Bhmj_14.77 //",
  "GK19ksbhm14u:158": "śrutvaitadarjunaḥ kṛṣṇaṃ babhāṣe vismayākulaḥ /",
  "GK19ksbhm14u:159": "yogīndro brāhmaṇaḥ ko 'sau kvāste vā sā ca tadvadhūḥ // Bhmj_14.78 //",
  "GK19ksbhm14u:160": "ityuktaḥ kaiṭabhārātiḥ punarūce dhanaṃjayam /",
  "GK19ksbhm14u:161": "śiṣyeṇa pṛṣṭaḥ sarvajñaḥ purā gururabhāṣata // Bhmj_14.79 //",
  "GK19ksbhm14u:162": "bṛhaspatiprabhṛtibhirmunīndraiḥ sarvadarśibhiḥ /",
  "GK19ksbhm14u:163": "śruteyaṃ bhavavicchedakathā proktā svayaṃbhuvā // Bhmj_14.80 //",
  "GK19ksbhm14u:164": "guṇatrayanibaddhānāṃ karmabhedavikāriṇām /",
  "GK19ksbhm14u:165": "bhāvo 'yaṃ prāṇināṃ śaśvadalpo 'pyāyātyanalpatām // Bhmj_14.81 //",
  "GK19ksbhm14u:166": "vicitraḥ kila sargo 'yamahaṃkārādvinirgataḥ /",
  "GK19ksbhm14u:167": "tasminneva layaṃ yāti śvabhre puṣpacayo yathā // Bhmj_14.82 //",
  "GK19ksbhm14u:168": "janmāntaraśatābhyāsādantaraṅgatvamāgataḥ /",
  "GK19ksbhm14u:169": "ahaṃkāraḥ sa manaso līlayā kena bhedyate // Bhmj_14.83 //",
  "GK19ksbhm14u:170": "avidyā tānavenaiva śanaiḥ saṃnyastakarmaṇaḥ /",
  "GK19ksbhm14u:171": "vikalpadvaṃdvaviratau jñānālokaḥ prajāyate // Bhmj_14.84 //",
  "GK19ksbhm14u:172": "jñānāgninā pravṛddhena dagdhvā bhavaviṣadrumam /",
  "GK19ksbhm14u:173": "sarvagranthivinirmuktaḥ paramāmṛtamaśnute // Bhmj_14.85 //",
  "GK19ksbhm14u:174": "iti brahmoditaṃ jñānaṃ śiṣyāya guruṇā purā /",
  "GK19ksbhm14u:175": "nānānidarśanopetaṃ kathitaṃ bhavaśāntaye // Bhmj_14.86 //",
  "GK19ksbhm14u:176": "ahameva guruḥ pārtha śiṣyaśca tvaṃ samo mama /",
  "GK19ksbhm14u:177": "ityuktvā jñānasarvasvaṃ virarāma janārdanaḥ // Bhmj_14.87 //",
  "GK19ksbhm14u:178": "***** anugītāḥ // 2 // *****",
  "GK19ksbhm14u:179": "tato vihṛtya suciraṃ prītyā kṛṣṇaḥ kirīṭinā /",
  "GK19ksbhm14u:180": "dvārakāgamane cakre matiṃ bandhujanotsukaḥ // Bhmj_14.88 //",
  "GK19ksbhm14u:181": "sa gatvā nṛpamāmantrya dhṛtarāṣṭraṃ ca sānugam /",
  "GK19ksbhm14u:182": "āśvāsya kṛṣṇāṃ kuntīṃ ca praṇamyādāya cānujām // Bhmj_14.89 //",
  "GK19ksbhm14u:183": "aśvamedhe sameṣyāmi punaḥ kṛtveti saṃvidam /",
  "GK19ksbhm14u:184": "dārukapreritarathaḥ pratasthe garuḍadhvajaḥ // Bhmj_14.90 //",
  "GK19ksbhm14u:185": "sa vrajansātyakisakhastejorañjitadiṅmukhaḥ /",
  "GK19ksbhm14u:186": "uttaṅkaṃ tapasāṃ rāśimāluloke muniḥ pathi // Bhmj_14.91 //",
  "GK19ksbhm14u:187": "taṃ dṛṣṭvā sādaraṃ śauriḥ praṇamya racitāñjaliḥ /",
  "GK19ksbhm14u:188": "kuśalaṃ vītavairāṇāṃ papracchāśramavāsinām // Bhmj_14.92 //",
  "GK19ksbhm14u:189": "uttaṅko 'pi hariṃ dṛṣṭvā prītipūrvamanāmayam /",
  "GK19ksbhm14u:190": "ūce kaccittvayā sāmnā rakṣitaṃ bhārataṃ kulam // Bhmj_14.93 //",
  "GK19ksbhm14u:191": "kaccidduryodhano rājā dharmaputraśca sānugau /",
  "GK19ksbhm14u:192": "sthitau vigatasaṃgharṣau tvadbuddhyā paitrike pade // Bhmj_14.94 //",
  "GK19ksbhm14u:193": "ityuttaṅkena govindaḥ pṛṣṭastaṃ pratyabhāṣata /",
  "GK19ksbhm14u:194": "bhāratānāṃ mune yatnaḥ paraḥ saṃdhau kṛto mayā // Bhmj_14.95 //",
  "GK19ksbhm14u:195": "hitamukto 'pi bahuśaḥ kauravo vidurādibhiḥ /",
  "GK19ksbhm14u:196": "na jagrāha kṛtāntena daivena ca vimohitaḥ // Bhmj_14.96 //",
  "GK19ksbhm14u:197": "nirbandhātkururājasya rādheyamunivartinaḥ /",
  "GK19ksbhm14u:198": "nihatāḥ pārthivāḥ sarve śeṣāḥ pañcaiva pāṇḍavāḥ // Bhmj_14.97 //",
  "GK19ksbhm14u:199": "etcchrutvā muniḥ kopājjvalitānalasaṃnibhaḥ /",
  "GK19ksbhm14u:200": "tamūce vakramanasā te tvayaiva nipātitāḥ // Bhmj_14.98 //",
  "GK19ksbhm14u:201": "śaktenāpi tvayā kṛṣṇa yasmātpārthahitaiṣiṇā /",
  "GK19ksbhm14u:202": "upekṣitaḥ kṣayo ghorastasmācchāpaṃ dadāni te // Bhmj_14.99 //",
  "GK19ksbhm14u:203": "uttaṅkenetyabhihite babhāṣe kaiṭabhāntakaḥ /",
  "GK19ksbhm14u:204": "jānāmi tejasāṃ rāśiṃ tvāmuttaṅka taponidhim // Bhmj_14.100 //",
  "GK19ksbhm14u:205": "kiṃtu nārhasi kopena tīvraṃ kartuṃ tapovyayam /",
  "GK19ksbhm14u:206": "tamo hi duḥsahaṃ ghoraṃ krodho mṛtyuḥ śarīriṇām // Bhmj_14.101 //",
  "GK19ksbhm14u:207": "nādyāpi naṣṭamoho 'si nanu nādyāpi paśyasi /",
  "GK19ksbhm14u:208": "kiṃ na jānāsi māṃ sarvaṃ sarvavyāpinamavyayam // Bhmj_14.102 //",
  "GK19ksbhm14u:209": "sarvadevamayaḥ kartā viśvātmā jagatāmaham /",
  "GK19ksbhm14u:210": "ityukte puṣkarākṣeṇa śāntakopo 'bravīnmuniḥ // Bhmj_14.103 //",
  "GK19ksbhm14u:211": "jāne tvāṃ kiṃ tu taddivyaṃ draṣṭumicchāmi te vapuḥ /",
  "GK19ksbhm14u:212": "ityarthito munīndreṇa viśvāviṣkāralīlayā // Bhmj_14.104 //",
  "GK19ksbhm14u:213": "śaktiprakāśaparipūritasaṃvidagre rekhāsphuraddruhiṇarudramahendracandram /",
  "GK19ksbhm14u:214": "dhāmatrayottaramanaśvaramaiśvaraṃ ta ddṛṣṭvā vapurmunivaro 'bhavadastamohaḥ // Bhmj_14.105 //",
  "GK19ksbhm14u:215": "tataḥ stotrairuttaṅkena viṣṇurbhaktyā namaskṛtaḥ /",
  "GK19ksbhm14u:216": "tadgirā svavapuḥ saumyaṃ tadeva punarādade // Bhmj_14.106 //",
  "GK19ksbhm14u:217": "saṃnidhi te vidhāsyāmi sarvataḥ smaraṇānmune /",
  "GK19ksbhm14u:218": "ityuktvā taṃ samāmantrya prāyādgaruḍaketanaḥ // Bhmj_14.107 //",
  "GK19ksbhm14u:219": "tataḥ kadācitsa munirjalārthī marudhanvasu /",
  "GK19ksbhm14u:220": "carandadarśa caṇḍālaṃ dhanuṣpāṇiṃ jalapradam // Bhmj_14.108 //",
  "GK19ksbhm14u:221": "gṛhāṇetyasakṛttena prārthito 'pi muniryadā /",
  "GK19ksbhm14u:222": "na jagrāhāśuci jalamuttaṅko 'ntardadhe tadā // Bhmj_14.109 //",
  "GK19ksbhm14u:223": "tataḥ sametya bhagavānsvayaṃ viṣṇustamabravīt /",
  "GK19ksbhm14u:224": "mune madvacasā śakraḥ sudhāṃ tvāṃ dātumāyayau // Bhmj_14.110 //",
  "GK19ksbhm14u:225": "jalado na sa cāṇḍālo mithyā śaṅkā tavābhavat /",
  "GK19ksbhm14u:226": "pratyākhyātaḥ sa yanmohānna tadyuktaṃ tvayā kṛtam // Bhmj_14.111 //",
  "GK19ksbhm14u:227": "adhunā madvarātte 'stu sāmṛtairjalanirjharaiḥ /",
  "GK19ksbhm14u:228": "pūriteyaṃ marumahī nigadyeti yayau hariḥ // Bhmj_14.112 //",
  "GK19ksbhm14u:229": "evaṃ mahāprabhāvo 'sāvuttaṅko yaśasāṃ nidhiḥ /",
  "GK19ksbhm14u:230": "durlabhaṃ darśanaṃ viṣṇoḥ sānugrahamavāptavān // Bhmj_14.113 //",
  "GK19ksbhm14u:231": "āsādya paramaṃ jñānaṃ yaḥ purā gurusevayā /",
  "GK19ksbhm14u:232": "taddakṣiṇārthī prayayau rājñaḥ pauṣasya mandiram // Bhmj_14.114 //",
  "GK19ksbhm14u:233": "kuṇḍale prāpya rucire vitīrṇe pauṣabhāryayā /",
  "GK19ksbhm14u:234": "hṛte ca dṛṣṭvā nāśena jīviteśarasātalam // Bhmj_14.115 //",
  "GK19ksbhm14u:235": "jitvā nāgānsamāsādya te purā ratnakuṇḍale /",
  "GK19ksbhm14u:236": "yo dadau gurubhāryāyai kṛtakṛtyo babhūva ca // Bhmj_14.116 //",
  "GK19ksbhm14u:237": "***** uttaṅkaviśvarūpadarśanam // 3 // *****",
  "GK19ksbhm14u:238": "yāte dvāravatīṃ kṛṣṇe yādavānandadāyini /",
  "GK19ksbhm14u:239": "yudhiṣṭhiraḥ sahāmātyo yajñakāryamacintayat // Bhmj_14.117 //",
  "GK19ksbhm14u:240": "sa gatvā sānujaḥ śailaṃ śrīkaṇṭhadayitaṃ śanaiḥ /",
  "GK19ksbhm14u:241": "ārādhya tapasā rudraṃ devadevaṃ pinākinam // Bhmj_14.118 //",
  "GK19ksbhm14u:242": "upahāreṇa vidhinā muhūrte śubhaśaṃsini /",
  "GK19ksbhm14u:243": "abhyarcya guhyakādhīśaṃ maṇibhadraṃ ca sānugam // Bhmj_14.119 //",
  "GK19ksbhm14u:244": "munibhistarpite vahnau dhaumyena ca purodhasā /",
  "GK19ksbhm14u:245": "lebhe pāṇaḍusutaḥ śrīmānmaruttanihitaṃ nidhim // Bhmj_14.120 //",
  "GK19ksbhm14u:246": "pṛthupramāṇarūpāṇi bhāsvanti vividhāni ca /",
  "GK19ksbhm14u:247": "hemabhāṇḍasahasrāṇi ratnabhāraśatāni ca // Bhmj_14.121 //",
  "GK19ksbhm14u:248": "uṣṭrāṇāṃ śakaṭānāṃ ca hayānāṃ kariṇāṃ tathā /",
  "GK19ksbhm14u:249": "daśalakṣāṇi tadvittamanayaddhastināpuram // Bhmj_14.122 //",
  "GK19ksbhm14u:250": "mahiṣāṇāṃ svarāṇāṃ ca puruṣāṇāṃ gavāṃ tathā /",
  "GK19ksbhm14u:251": "na babhūva tadā saṃkhyā vahatāṃ bhūri kāñcanam // Bhmj_14.123 //",
  "GK19ksbhm14u:252": "akṣayyaṃ kośamādāya prāpte 'tha svapuraṃ nṛpe /",
  "GK19ksbhm14u:253": "sahito vṛṣṇibhiḥ sarvairāyayau madhusūdanaḥ // Bhmj_14.124 //",
  "GK19ksbhm14u:254": "etasminneva samaye saubhadramahiṣī sutam /",
  "GK19ksbhm14u:255": "droṇaputrāstranirdagdhamasūta gatajīvitam // Bhmj_14.125 //",
  "GK19ksbhm14u:256": "uttarāyāḥ suto jātaḥ strīṇāmityutsavasvanaḥ /",
  "GK19ksbhm14u:257": "vyasurvyasuścetyabhavatsa eva rodanadhvaniḥ // Bhmj_14.126 //",
  "GK19ksbhm14u:258": "bālapravālakalikākomalāvayave śiśau /",
  "GK19ksbhm14u:259": "jāte nirjīvite śokānmumoha jananījanaḥ // Bhmj_14.127 //",
  "GK19ksbhm14u:260": "tataḥ kuntī subhadrā ca sametya karuṇānidhim /",
  "GK19ksbhm14u:261": "bāṣpasaṃdigdhayā vācā rukmiṇīpatimūcatuḥ // Bhmj_14.128 //",
  "GK19ksbhm14u:262": "svasreyasya yaśomūrterabhimanyoḥ priyasya te /",
  "GK19ksbhm14u:263": "bālasya bālastanayo jāto 'dya gatajīvanaḥ // Bhmj_14.129 //",
  "GK19ksbhm14u:264": "karuṇārdre niśamyaitadbhagavānbhūtabhāvanaḥ /",
  "GK19ksbhm14u:265": "viveśa sūtikāveśma śauriḥ kalaśabhūṣitam // Bhmj_14.130 //",
  "GK19ksbhm14u:266": "ratnadīptāṃśukapiśaṃ dīptauṣadhiśatācitam /",
  "GK19ksbhm14u:267": "mantrairbhiṣagbhiḥ śikhinā śastrairastraiśca rakṣitam // Bhmj_14.131 //",
  "GK19ksbhm14u:268": "athottarā prāpya saṃjñāṃ śītavāribhirukṣitā /",
  "GK19ksbhm14u:269": "tamaṅke śiśumādāya vilalāpa hareḥ puraḥ // Bhmj_14.132 //",
  "GK19ksbhm14u:270": "tato jaladagambhīraghoṣaḥ śaurirabhāṣata /",
  "GK19ksbhm14u:271": "dṛśā pīyūṣavarṣiṇyā vilokya śanakaiḥ śiśum // Bhmj_14.133 //",
  "GK19ksbhm14u:272": "satyāddvijebhyo dharmācca yathā nānyatpriyaṃ mama /",
  "GK19ksbhm14u:273": "tena satyena bālo 'yamastramukto 'dya jīvatu // Bhmj_14.134 //",
  "GK19ksbhm14u:274": "mādhavenetyabhihite labdhajīvaḥ sa bālakaḥ /",
  "GK19ksbhm14u:275": "sūryāṃśubhiriva spṛṣṭo babhūvāmbujakorakaḥ // Bhmj_14.135 //",
  "GK19ksbhm14u:276": "tataḥ pravṛtte vipule sahasā nagarotsave /",
  "GK19ksbhm14u:277": "nabhaścarāṇāmapyāsītyasādhu sādhviti niḥsvanaḥ // Bhmj_14.136 //",
  "GK19ksbhm14u:278": "parikṣīṇe kurulaye yasmājjāto 'yamarbhakaḥ /",
  "GK19ksbhm14u:279": "tasmātparīkṣinnāmnāstu jagādetyatha keśavaḥ // Bhmj_14.137 //",
  "GK19ksbhm14u:280": "***** pirakṣijjanma // 4 // *****",
  "GK19ksbhm14u:281": "vyāsājñayā tato rājño yajñayogyo mahādhanaḥ /",
  "GK19ksbhm14u:282": "prāvartatoruratnāḍhyaḥ saṃbhāro bahukautukaḥ // Bhmj_14.138 //",
  "GK19ksbhm14u:283": "athotsṛjya vidhānena kṛṣṇaśāraṃ turaṅgamam /",
  "GK19ksbhm14u:284": "rakṣitaṃ pāṇḍuputreṇa svayaṃ gāṇḍīvadhanvanā // Bhmj_14.139 //",
  "GK19ksbhm14u:285": "babhūva caitraśuklānte dīkṣito dharmanandanaḥ /",
  "GK19ksbhm14u:286": "hemamālādharo daṇḍī muṇḍaḥ kṛṣṇājināvṛtaḥ // Bhmj_14.140 //",
  "GK19ksbhm14u:287": "tatastrigartaviṣayaṃ prāpte yajñaturaṅgame /",
  "GK19ksbhm14u:288": "rājaputrairabhūdyuddhaṃ tumulaṃ savyasācinaḥ // Bhmj_14.141 //",
  "GK19ksbhm14u:289": "yudhiṣṭhirājñā tasyābhūnna hantavyā nṛpā iti /",
  "GK19ksbhm14u:290": "tāṃ smarannāvadhīdbhūpānsa vijigye tu kevalam // Bhmj_14.142 //",
  "GK19ksbhm14u:291": "prāgjyotiṣeśvaraṃ vīraṃ bhagadattātmajaṃ yudhi /",
  "GK19ksbhm14u:292": "tribhirdinairvajradattaṃ jitvā kuñjarayodhinm // Bhmj_14.143 //",
  "GK19ksbhm14u:293": "saindhavairakarodyuddhaṃ suciraṃ krūrayodhibhiḥ /",
  "GK19ksbhm14u:294": "pādacārī raṇe pārtho rathakuñjaravartibhiḥ // Bhmj_14.144 //",
  "GK19ksbhm14u:295": "pautraṃ jayadrathasyātha śiśumādāya duḥśalā /",
  "GK19ksbhm14u:296": "savyasācinamāsādya babhāṣe sāśrulocanā // Bhmj_14.145 //",
  "GK19ksbhm14u:297": "bhrātastvayi samāyāte tvannāmaiva niśamya me /",
  "GK19ksbhm14u:298": "putro jāyadrathiryātaḥ pañcatāṃ sphaṭitāśayaḥ // Bhmj_14.146 //",
  "GK19ksbhm14u:299": "tatsuto mama pautro 'yaṃ svasrayatanayastava /",
  "GK19ksbhm14u:300": "rakṣyaḥ sarvātmanetyuktvā ruroda dhṛtarāṣṭrajā // Bhmj_14.147 //",
  "GK19ksbhm14u:301": "viṣaṇṇāstāṃ samāśvāsya pārtho bāṣpārdralocanaḥ /",
  "GK19ksbhm14u:302": "nindannijāṃ kṣattrajātiṃ jagāma turagānugaḥ // Bhmj_14.148 //",
  "GK19ksbhm14u:303": "śvetāśvamaśvagoptāraṃ maṇipūrapuraṃ tataḥ /",
  "GK19ksbhm14u:304": "prāptaṃ pratyudyayau rājā tatputro babhruvāhanaḥ // Bhmj_14.149 //",
  "GK19ksbhm14u:305": "pūjāmādāya tanayaṃ namraṃ dṛṣṭvā puraḥ sthitam /",
  "GK19ksbhm14u:306": "yuddhārthī śakratanayo nābhyanandatkṛtāñjalim // Bhmj_14.150 //",
  "GK19ksbhm14u:307": "dṛṣṭvā māṃ sāyudhaṃ prāptaṃ sāmnā pratyudyato 'si kim /",
  "GK19ksbhm14u:308": "dhiktvāmakṣattriyaṃ bhīrumityūce taṃ dhanaṃjayaḥ // Bhmj_14.151 //",
  "GK19ksbhm14u:309": "atrāntare mahīṃ bhittvā samutthāyoragāṅganā /",
  "GK19ksbhm14u:310": "bhartuḥ pāṇḍusutasyāgre babhāṣe babhruvāhanam // Bhmj_14.152 //",
  "GK19ksbhm14u:311": "yuddhena tuṣyati pitā tavāyaṃ subhaṭapriyaḥ /",
  "GK19ksbhm14u:312": "vīrasaṃtānasaphalā śauryaśrīrabhimāninām // Bhmj_14.153 //",
  "GK19ksbhm14u:313": "ityuktaḥ sa tayā dhanvī rathaṃ hemaharidhvajam /",
  "GK19ksbhm14u:314": "āruhyāmuktakavacaḥ pitaraṃ yoddhumāyayau // Bhmj_14.154 //",
  "GK19ksbhm14u:315": "tataḥ śaraśatairviddhastena śrānto dhanaṃjayaḥ /",
  "GK19ksbhm14u:316": "ānandanirbharastasya praśaśaṃsa parākramam // Bhmj_14.155 //",
  "GK19ksbhm14u:317": "gāṇḍīvadhanvanā muktānnārācānvajragauravān /",
  "GK19ksbhm14u:318": "aprāptāneva ciccheda śaraiścitrāṅgadāsutaḥ // Bhmj_14.156 //",
  "GK19ksbhm14u:319": "athonmamātha putrasya syandanaṃ śakranandanaḥ /",
  "GK19ksbhm14u:320": "hemapattralatācitramudyānamiva mārutaḥ // Bhmj_14.157 //",
  "GK19ksbhm14u:321": "vilokya virathaṃ putraṃ yudhyamānamasaṃbhramam /",
  "GK19ksbhm14u:322": "mandaprayatnaśithilānprāhiṇodvijayaḥ śarān // Bhmj_14.158 //",
  "GK19ksbhm14u:323": "atha tīkṣṇena hṛdaye pattriṇā babhruvāhanaḥ /",
  "GK19ksbhm14u:324": "vajreṇevācalabhidā jaghāna śvetavāhanam // Bhmj_14.159 //",
  "GK19ksbhm14u:325": "avadhye sarvabhūtānāṃ putreṇa nihater'june /",
  "GK19ksbhm14u:326": "nādo babhūva gagane hā heti tridivaukasām // Bhmj_14.160 //",
  "GK19ksbhm14u:327": "pitaraṃ patitaṃ dṛṣṭvā śakraketumiva kṣitau /",
  "GK19ksbhm14u:328": "mumoha nindannātmānaṃ tyaktacāpor'junātmajaḥ // Bhmj_14.161 //",
  "GK19ksbhm14u:329": "atha citrāṅgadābhyetya putreṇa nihataṃ patim /",
  "GK19ksbhm14u:330": "vilokya śokasaṃpannā vilalāpa sumadhyamā // Bhmj_14.162 //",
  "GK19ksbhm14u:331": "aho bata cirādetya jāyāyā mama mandiram /",
  "GK19ksbhm14u:332": "āryaputra tvayātithyaṃ prāptaṃ putrādbalīyasaḥ // Bhmj_14.163 //",
  "GK19ksbhm14u:333": "aho nu hatabhāgyāhaṃ ciraviproṣitaṃ patim /",
  "GK19ksbhm14u:334": "tvāṃ prāptaṃ nayanānandaṃ paśyāmi nihataṃ kṣitau // Bhmj_14.164 //",
  "GK19ksbhm14u:335": "ityuktvā gāḍhamāliṅgya sā patiṃ bāṣpagadgadā /",
  "GK19ksbhm14u:336": "ulūpī nāgatanayā bhuvi citrāṅgadāpatat // Bhmj_14.165 //",
  "GK19ksbhm14u:337": "labdhasaṃjño 'tha śokārtaḥ pralapanbabhruvāhanaḥ /",
  "GK19ksbhm14u:338": "śarīratyāganiyamaṃ cakre kilbiṣakūṇitaḥ // Bhmj_14.166 //",
  "GK19ksbhm14u:339": "tataḥ pradadhyau manasā nāgī saṃjīvanaṃ maṇim /",
  "GK19ksbhm14u:340": "sa ca dhyātastayā satyā nāgalokātsamāyayau // Bhmj_14.167 //",
  "GK19ksbhm14u:341": "spṛṣṭo 'tha hṛdaye tena jīvitaṃ prāpya phalguṇaḥ /",
  "GK19ksbhm14u:342": "citrāṅgadā ca sahasā śokaṃ tyaktvā hriyaṃ yayau // Bhmj_14.168 //",
  "GK19ksbhm14u:343": "tato nivedya vṛttāntaṃ jagāda bhujagātmajā /",
  "GK19ksbhm14u:344": "ulūpī dhairyaladhiṃ suptotthicamivārjunam // Bhmj_14.169 //",
  "GK19ksbhm14u:345": "śāpo bhīṣmavadhādeṣa dattaste vasubhiḥ purā /",
  "GK19ksbhm14u:346": "ko 'nyathā tvāṃ raṇe śakto jetuṃ nirjitadhūrjaṭim // Bhmj_14.170 //",
  "GK19ksbhm14u:347": "matpitā mayi vātsalyācchāpitā vasavaśca te /",
  "GK19ksbhm14u:348": "ūcustvacchāpanirvāṇaṃ ratnametanmahāprabham // Bhmj_14.171 //",
  "GK19ksbhm14u:349": "śrutvaitadvismitaḥ pārtho nāgīṃ citrāṅgadāṃ tathā /",
  "GK19ksbhm14u:350": "parisāntvya śanaiḥ pūjāṃ jagrāha tanayārpitām // Bhmj_14.172 //",
  "GK19ksbhm14u:351": "tatastaṃ dayitaṃ vīraḥ putramāmantrya pāṇḍavaḥ /",
  "GK19ksbhm14u:352": "hayānusārī babhrāma vasudhāmabdhimekhalām // Bhmj_14.173 //",
  "GK19ksbhm14u:353": "sa jitvā magadhādhīśaṃ jarāsaṃdhātmajātmajam /",
  "GK19ksbhm14u:354": "vaṅgānpuṇḍrānkirātāṃśca dākṣiṇātyānsamāgadhān // Bhmj_14.174 //",
  "GK19ksbhm14u:355": "gāndhārānsaubalasutānrājaputrānprahāriṇaḥ /",
  "GK19ksbhm14u:356": "āgantavyaṃ makhe rājñaḥ sarvairityādideśa tān // Bhmj_14.175 //",
  "GK19ksbhm14u:357": "tataḥ pratinivṛttena turagena sahārjunaḥ /",
  "GK19ksbhm14u:358": "viveśa pūjitaḥ pauraurvijayī hastināpuram // Bhmj_14.176 //",
  "GK19ksbhm14u:359": "***** hayotsargaḥ // 5 // *****",
  "GK19ksbhm14u:360": "tataḥ pravṛtte vidhivadyajñe kanakavarṣiṇaḥ /",
  "GK19ksbhm14u:361": "rājño munijanākīrṇe prāpteṣvakhilarājasu // Bhmj_14.177 //",
  "GK19ksbhm14u:362": "jananyā saha bhūpāle saṃprāpte babhruvāhane /",
  "GK19ksbhm14u:363": "sādaraṃ phalguṇaprītyā pūjite pāṇḍunandanaiḥ // Bhmj_14.178 //",
  "GK19ksbhm14u:364": "niḥśeṣakalmaṣaploṣadivyadīkṣākṛtakṣaṇaḥ /",
  "GK19ksbhm14u:365": "kriyāmahīnāṃ vidadhe svayaṃ satyavatīsutaḥ // Bhmj_14.179 //",
  "GK19ksbhm14u:366": "haimaṃ tatrābhavatsarvamiṣṭakācayanādikam /",
  "GK19ksbhm14u:367": "yūpabhāṇḍaghanasthālīparyaṅkagṛhatoraṇam // Bhmj_14.180 //",
  "GK19ksbhm14u:368": "devarṣisiddhagandharvakiṃnarā maṅgalaṃ jaguḥ /",
  "GK19ksbhm14u:369": "karmāntareṣu yajñasya nanṛtuścāpsarogaṇāḥ // Bhmj_14.181 //",
  "GK19ksbhm14u:370": "śamite paśusaṅghe ca turagaṃ brāhmaṇottamāḥ /",
  "GK19ksbhm14u:371": "ālabhanta yathāśāstramupasaṃrodhya pārṣatīm // Bhmj_14.182 //",
  "GK19ksbhm14u:372": "tataste yājakāstatra vapāmuddhṛtya vājinaḥ /",
  "GK19ksbhm14u:373": "śrapayitvā śubhaṃ dhūmaṃ pāṇḍavebhyo nyavedayan // Bhmj_14.183 //",
  "GK19ksbhm14u:374": "tataḥ pūrṇe na vidhinā vyāsāya pṛthivīpatiḥ /",
  "GK19ksbhm14u:375": "catuḥsamudrarasanāṃ pṛthivīṃ dakṣiṇāṃ dadhau // Bhmj_14.184 //",
  "GK19ksbhm14u:376": "dattvātha hemakoṭīnāṃ koṭiṃ tadvacasā nṛpaḥ /",
  "GK19ksbhm14u:377": "brāhmaṇebhyaḥ punaḥ kṣoṇīṃ jagrāha muninārpitām // Bhmj_14.185 //",
  "GK19ksbhm14u:378": "pūjayitvā narendreṇa mānārheṣvatha rājasu /",
  "GK19ksbhm14u:379": "visṛṣṭeṣvabhavatko 'pi brahmasaṅghamahotsavaḥ // Bhmj_14.186 //",
  "GK19ksbhm14u:380": "babhūvustatra vāhinyo madhunaḥ kṣīrasarpiṣām /",
  "GK19ksbhm14u:381": "babhuśca kulaśailābhā bhakṣyā modakarāśayaḥ // Bhmj_14.187 //",
  "GK19ksbhm14u:382": "dvijebhyo dīyamāneṣu hemaratneṣu bhūbhujā /",
  "GK19ksbhm14u:383": "śakrāyudhairiva vyāptā diśo daśa cakāśire // Bhmj_14.188 //",
  "GK19ksbhm14u:384": "dhṛtarāṣṭraḥ saviduraḥ sañjayaḥ subalātmajāḥ /",
  "GK19ksbhm14u:385": "tasthurdaivatavattatra pūjyamānā mahībhujā // Bhmj_14.189 //",
  "GK19ksbhm14u:386": "***** yajñaḥ // 6 // *****",
  "GK19ksbhm14u:387": "atrāntare hemacitrapārśvo bilamukhodgataḥ /",
  "GK19ksbhm14u:388": "nakulo laghusaṃcāro yajñabhūmimupāyayau // Bhmj_14.190 //",
  "GK19ksbhm14u:389": "sa manuṣyagirā prāha janayañjanakautukam /",
  "GK19ksbhm14u:390": "aho nu saktṛprasthena na tulyo 'yaṃ mahāmakhaḥ // Bhmj_14.191 //",
  "GK19ksbhm14u:391": "nūnaṃ dānakaṇaḥ śuddhaḥ kṛśo 'pi prathate nṛṇām /",
  "GK19ksbhm14u:392": "nūnaṃ bahudraviṇatā mithyaivotsavaḍambaraḥ // Bhmj_14.192 //",
  "GK19ksbhm14u:393": "iti bruvāṇaḥ pṛṣṭo 'sau viprairvismayanirbharaiḥ /",
  "GK19ksbhm14u:394": "uvāca sraktuprasthasya māhātmyaṃ śrūyatāṃ dvijāḥ // Bhmj_14.193 //",
  "GK19ksbhm14u:395": "śilocchavṛttirabhavatkurukṣetre purā dvijaḥ /",
  "GK19ksbhm14u:396": "viśuddhasattvadraviṇaḥ kṛśo bahukuṭumbakaḥ // Bhmj_14.194 //",
  "GK19ksbhm14u:397": "ghore kadāciddurbhikṣe kṣīṇavṛttiścireṇa saḥ /",
  "GK19ksbhm14u:398": "kṛcchrātkṣetraśataṃ bhrāntvā saktuprasthamavāptavān // Bhmj_14.195 //",
  "GK19ksbhm14u:399": "baiśvadevena vidhinā sa kṛtvāvaśyakaṃ gṛhe /",
  "GK19ksbhm14u:400": "kalatrasahito bhoktuṃ prasthito 'paśyadarthinam // Bhmj_14.196 //",
  "GK19ksbhm14u:401": "kṣutparītaḥ kṣudhākrāntaṃ sa taṃ dṛṣṭvātithiṃ dvijaḥ /",
  "GK19ksbhm14u:402": "dadau svamaśanaṃ tasmai saṃtoṣaviṣadāśayaḥ // Bhmj_14.197 //",
  "GK19ksbhm14u:403": "tadbhuktvā nābhavattasya kṣunnavṛttiryadārthinaḥ /",
  "GK19ksbhm14u:404": "tadbhāryā tatsutaścāsmai tatsnuṣā ca dadau tadā // Bhmj_14.198 //",
  "GK19ksbhm14u:405": "sarveṣāṃ bhojanenātha tṛptaḥ so 'tithirabravīt /",
  "GK19ksbhm14u:406": "dharmastuṣṭo 'smi sattvena mahatī siddhirastu vaḥ // Bhmj_14.199 //",
  "GK19ksbhm14u:407": "ityuktvāntarhite tasminvimānaistaraṇiprabhaiḥ /",
  "GK19ksbhm14u:408": "saśarīrā divaṃ prāpuḥ sarve te ślāghyavṛttayaḥ // Bhmj_14.200 //",
  "GK19ksbhm14u:409": "athāhaṃ saktugandhena samāhūto bilāśrayaḥ /",
  "GK19ksbhm14u:410": "tāmucchiṣṭabhuvaṃ prāpto bhuktavānyatra so 'tithiḥ // Bhmj_14.201 //",
  "GK19ksbhm14u:411": "tatpātrasalilaspṛṣṭamekaṃ pārśvamidaṃ mama /",
  "GK19ksbhm14u:412": "luṭhitaṃ hemaruciraṃ jātaṃ puṇyairivāplutam // Bhmj_14.202 //",
  "GK19ksbhm14u:413": "dvitīyapārśvacintā me kathaṃ haimaṃ bhavediti /",
  "GK19ksbhm14u:414": "babhūva suciraṃ tasmātprāpto 'haṃ vasudhādhipam // Bhmj_14.203 //",
  "GK19ksbhm14u:415": "dvijalakṣasahasrāṇāmiha bhojanabhūmiṣu /",
  "GK19ksbhm14u:416": "bhrāntasyāpi na saṃjātaḥ ko 'pi kānto lavastanau // Bhmj_14.204 //",
  "GK19ksbhm14u:417": "tasmānna saktuprasthasya samatāmarhati kratuḥ /",
  "GK19ksbhm14u:418": "ityuktvā nakulaḥ prāyāccitrakāntiradarśanam // Bhmj_14.205 //",
  "GK19ksbhm14u:419": "evaṃ yajñasahasrebhyaḥ śreyasī bhāvaśuddhatā /",
  "GK19ksbhm14u:420": "manasaḥ kila vaimalyaṃ paraṃ brahma pracakṣate // Bhmj_14.206 //",
  "GK19ksbhm14u:421": "agastyasya purā satre viprā dvādaśavārṣike /",
  "GK19ksbhm14u:422": "śaśaṃsurbhāvinīṃ ghorāmanāvṛṣṭiṃ parasparam // Bhmj_14.207 //",
  "GK19ksbhm14u:423": "tato 'gastyaḥ parityajya dravyayajñaṃ mahāmatiḥ /",
  "GK19ksbhm14u:424": "cakāra stabdhamanasā dhyānayajñaṃ taponidhiḥ // Bhmj_14.208 //",
  "GK19ksbhm14u:425": "sa eva vasusaṃpannaḥ sarvavitkṛtasaṃnidhiḥ /",
  "GK19ksbhm14u:426": "kraturbabhūva yenendro vavarṣānandanirbharaḥ // Bhmj_14.209 //",
  "GK19ksbhm14u:427": "śreyānsattvaviśuddhasya pāṇḍavasyābhavatkratuḥ /",
  "GK19ksbhm14u:428": "kiṃ tu svakāryānnakulastatra cakre vimānanām // Bhmj_14.210 //",
  "GK19ksbhm14u:429": "jijñāsurjamadagniṃ prāksvayaṃ krodhaḥ śvavigrahaḥ /",
  "GK19ksbhm14u:430": "pasparśa jihvayā śrāddhe dhṛtaṃ hi piṭhire payaḥ // Bhmj_14.211 //",
  "GK19ksbhm14u:431": "krodho 'yamiti vijñāya munistaṃ jñānalocanaḥ /",
  "GK19ksbhm14u:432": "na śaśāpa tato bhītaḥ krodhaḥ kampākulo 'bhavat // Bhmj_14.212 //",
  "GK19ksbhm14u:433": "tataste pitaraḥ kruddhā yeṣāṃ śrāddhe dhṛtaṃ payaḥ /",
  "GK19ksbhm14u:434": "aśapansuciraṃ yena krodho nakulatāṃ yayau // Bhmj_14.213 //",
  "GK19ksbhm14u:435": "avamānakathāṃ kṛtvā svayaṃ yaudhiṣṭhire kratau /",
  "GK19ksbhm14u:436": "śāpakṣayaṃ prāpsyasīti tairevāsya kṛto 'vadhiḥ // Bhmj_14.214 //",
  "GK19ksbhm14u:437": "***** nakulopākhyānam // 7 // *****",
  "GK19ksbhm14u:438": "iti sakalanarendrairvandyamānasya rājño dvijajanaparipuṣṭaḥ sattvapuṣpaprakāraḥ /",
  "GK19ksbhm14u:439": "śubhaphalanicayādyo yajñasaṃbhāramūlaścaritatarurudāraḥ satyaśākho rarāja // Bhmj_14.215 //",
  "GK19ksbhm14u:440": "iti śrīkṣemendraviracitāyāṃ bhāratamañjaryāmāśvamedhikaṃ parva"
}