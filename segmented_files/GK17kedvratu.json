{
  "GK17kedvratu:0": "[śrīḥ]",
  "GK17kedvratu:1": "[vṛttaratnākaraḥ]",
  "GK17kedvratu:2": "prathamo 'dhyāyaḥ",
  "GK17kedvratu:3": "sukhasantānasiddhyarthaṃ natvā brahmā 'cyutā 'rcitam /",
  "GK17kedvratu:4": "gaurīvināyakopetaṃ śaṅkaraṃ lokaśaṅkaram // KedV_1.1 //",
  "GK17kedvratu:5": "vedārthaśaivaśāstrajñaḥ pavyeko 'bhūd lokaśaṅkaram /",
  "GK17kedvratu:6": "tasya putro 'sti kedāraḥ śivapādārcane rataḥ // KedV_1.2 //",
  "GK17kedvratu:7": "tenedaṃ kriyate chando lakṣyalakṣaṇasaṃyutam /",
  "GK17kedvratu:8": "vṛttaratnākaraṃ nāma bālānāṃ sukhasiddhye // KedV_1.3 //",
  "GK17kedvratu:9": "piṅgalādibhrācāryairyaduktaṃ laukikaṃ dvidhā /",
  "GK17kedvratu:10": "mātrāvarṇavibhedena cchandastadiha kathyate // KedV_1.4 //",
  "GK17kedvratu:11": "ṣaḍadhyāyanibaddhasya cchandaso 'sya parisphuṭam /",
  "GK17kedvratu:12": "pramāṇamapi vijñeyaṃ ṣaḍtriṃśadadhikaṃ śatam // KedV_1.5 //",
  "GK17kedvratu:13": "myarastajabhnagairlāntairebhirdaśabhirakṣaraiḥ /",
  "GK17kedvratu:14": "samastaṃ vāṅmayaṃ vyāptaṃ trailokyamiva viṣṇunā // KedV_1.6 //",
  "GK17kedvratu:15": "sarvagurmo mukhāntarlau yarāvantagalau satau /",
  "GK17kedvratu:16": "gmadhyādyau jmau trilo no 'ṣṭau bhavantyatra gaṇāstrikāḥ // KedV_1.7 //",
  "GK17kedvratu:17": "jñeyāḥ sarvāntamadhyādiguravo 'tra catuṣkalāḥ /",
  "GK17kedvratu:18": "gaṇāścaturlaghūpetāḥ pañcāryādiṣu saṃsthitāḥ // KedV_1.8 //",
  "GK17kedvratu:19": "sānusvāro visargānto dīrgho yuktaparaśca yaḥ /",
  "GK17kedvratu:20": "vā pādānte tvasau gvakro jñeyo 'nyo mātriko lṛjuḥ // KedV_1.9 //",
  "GK17kedvratu:21": "pādādāviha varṇasya saṃyogaḥ kramasaṃjñakaḥ /",
  "GK17kedvratu:22": "puraḥsthitena tena syāllaghutā 'pi kvacid guroḥ // KedV_1.10 //",
  "GK17kedvratu:23": "taruṇaṃ sarṣapaśākaṃ navaudanaṃ picchilāni ca dadhīni /",
  "GK17kedvratu:24": "alpavyayena sundari (/",
  "GK17kedvratu:25": ") grāmyajano miṣṭamiśnāti // KedV_1.11 //",
  "GK17kedvratu:26": "abdhibhūtarasādīnāṃ jñeyāḥ saṃjñāstu lokataḥ /",
  "GK17kedvratu:27": "jñeyaḥ pādaścaturthāśo yatirvicchedasaṃjñitaḥ // KedV_1.12 //",
  "GK17kedvratu:28": "yuksamaṃ viṣamaṃ cālūksthānaṃ sadbhirnigadyate /",
  "GK17kedvratu:29": "samamardhasamaṃ vṛttaṃ viṣamaṃ ca tathāparam // KedV_1.13 //",
  "GK17kedvratu:30": "aṅghrayo yasya catvārastulyalakṣaṇalakṣitāḥ /",
  "GK17kedvratu:31": "tacchandaḥśāstratattvajñāḥ samaṃ vṛttaṃ pracakṣate // KedV_1.14 //",
  "GK17kedvratu:32": "prathamāṅghrisamo yasya tṛtīyaścaraṇo bhavet /",
  "GK17kedvratu:33": "dvitīyasturyavadvṛttaṃ tadardhasamamucyate // KedV_1.15 //",
  "GK17kedvratu:34": "yasya pādacatuṣke 'pi lakṣma bhinnaṃ parasparam /",
  "GK17kedvratu:35": "tadāhurviṣamaṃ vṛttaṃ chandaḥśāstraviśāradāḥ // KedV_1.16 //",
  "GK17kedvratu:36": "ārabhyaikākṣārātpādādekaikakṣaravaddhitaiḥ /",
  "GK17kedvratu:37": "pṛthakchando bhavetpādairyāvatṣaḍviṃśatiṃ gatam // KedV_1.17 //",
  "GK17kedvratu:38": "tadūrdhvaṃ caṇḍavṛṣṭyādidaṇḍakāḥ parikīrtitāḥ /",
  "GK17kedvratu:39": "śeṣaṃ gāthāstribhiḥ ṣaḍbhiścaraṇaiścopalakṣitāḥ // KedV_1.18 //",
  "GK17kedvratu:40": "uktā 'tyuktā tathā madhyā pratiṣṭhā 'nyā supūrvikā /",
  "GK17kedvratu:41": "gāyatryuṣṇiganuṣṭup ca bṛhatī paṅktireva ca // KedV_1.19 //",
  "GK17kedvratu:42": "triṣṭup ca jagatī caiva tathā 'tijagatī matā /",
  "GK17kedvratu:43": "śakvarī sā 'tipūrvā syādaṣṭyatyaṣṭī tataḥ smṛte // KedV_1.20 //",
  "GK17kedvratu:44": "dhṛtiścā 'tidhṛtiścaiva krṭiḥ prakṛtirākṛtiḥ /",
  "GK17kedvratu:45": "vikṛtiḥ saṅkṛtiścaiva tathā 'tikṛtirutkṛtiḥ // KedV_1.21 //",
  "GK17kedvratu:46": "ityuktāśchandasāṃ saṃjñāḥ kramato vacmi sāmpratam /",
  "GK17kedvratu:47": "lakṣaṇaṃ sarvavṛttānāṃ mātrāvṛttā 'nupūrvakam // KedV_1.22 //",
  "GK17kedvratu:48": "iti śrīkedārabhaṭṭaviracite vṛttaratnākare saṃjñābhidhāno nāma prathamo 'dhyāyaḥ /",
  "GK17kedvratu:49": "---------------------",
  "GK17kedvratu:50": "dvitīyo 'dhyāyaḥ",
  "GK17kedvratu:51": "[ āryā-prakaraṇam (1-7) ]",
  "GK17kedvratu:52": "lakṣmaitatsapta gaṇā gopetā bhavati neha viṣame jaḥ /",
  "GK17kedvratu:53": "ṣaṣṭho 'yaṃ nalaghū vā prathame 'rdhe niyatamāryāyāḥ // KedV_2.1 //",
  "GK17kedvratu:54": "ṣaṣthe dvitīyalātparake nle mukhalācca sayatipadaniyamaḥ /",
  "GK17kedvratu:55": "carame 'rdhe pañcake tasmādiha bhavati ṣaṣṭho laḥ // KedV_2.2 //",
  "GK17kedvratu:56": "triṣvaṅkeṣu pādo dalayorādyeṣu dṛśyate yasyāḥ /",
  "GK17kedvratu:57": "pathyeti nāma tasyāḥ prakīrtitaṃ nāgarājena // KedV_2.3 //",
  "GK17kedvratu:58": "saṃlaṅghya gaṇatrayamādimaṃ śakalayordvayorbhavati pādaḥ /",
  "GK17kedvratu:59": "[saṃ(anunāsika)-laṅghya]",
  "GK17kedvratu:60": "yasyāstāṃ piṅgalanāgo vipulāmiti samākhyāti // KedV_2.4 //",
  "GK17kedvratu:61": "ubhayārdhayorjakārau dvitīyaturyau gamadhyagau tasyāḥ /",
  "GK17kedvratu:62": "capaleti nāma tasyā prakīrtitaṃ nāgarājena // KedV_2.5 //",
  "GK17kedvratu:63": "ādyaṃ dalaṃ samastaṃ bhajeya lakṣma capalāgataṃ yasyāḥ /",
  "GK17kedvratu:64": "śeṣe pūrvajalakṣmā mukhacapalāsoditā muninā // KedV_2.6 //",
  "GK17kedvratu:65": "prākpratipāditamardhe prathame prathametare ca capalāyāḥ /",
  "GK17kedvratu:66": "lakṣmāśrayate soktā viśuddhadhībhirjaghanacapalā // KedV_2.7 //",
  "GK17kedvratu:67": "[ gīti-prakaraṇam (8-11) ]",
  "GK17kedvratu:68": "āryāorathamadaloktaṃ yadi kathamapi lakṣaṇaṃ bhavedubhayoḥ /",
  "GK17kedvratu:69": "dalayoḥ krṭayatiśobhāṃ tāṃ gītiṃ gītavānbhujaṅgeśaḥ // KedV_2.8 //",
  "GK17kedvratu:70": "āryādvitīyake 'rdhe yadgaditaṃ lakṣaṇaṃ tatsyāt /",
  "GK17kedvratu:71": "yadyubhayorapi dalayorupagītiṃ tā munirbrūte // KedV_2.9 //",
  "GK17kedvratu:72": "āryāśakaladvitayaṃ vyatyayaracitaṃ bhavedyasyāḥ /",
  "GK17kedvratu:73": "sodgītiḥ kila gaditā tadvadyatyaṃśabhedasaṃyuktā // KedV_2.10 //",
  "GK17kedvratu:74": "āryāpūrvārdhaṃ yadi guruṇaikenādhikena nidhane yuktam /",
  "GK17kedvratu:75": "itarattadvannikhilaṃ bhavati yadīyamarddhamuditāryāgītiḥ // KedV_2.11 //",
  "GK17kedvratu:76": "[ vaitālīya-prakaraṇam (12-20) ]",
  "GK17kedvratu:77": "ṣaḍviṣame 'ṣṭau same kalāstāśca same syurno nirantarāḥ /",
  "GK17kedvratu:78": "na samātra parāśritā kalā vaitālīye 'nte ralā guruḥ // KedV_2.12 //",
  "GK17kedvratu:79": "paryante ryau tathaiva śeṣamaupacchandasikaṃ sudhībhiruktam // KedV_2.13 //",
  "GK17kedvratu:80": "āpātalikā kathiteyaṃ bhādgurukāvatha pūrvavadanyat // KedV_2.14 //",
  "GK17kedvratu:81": "tṛtīyayugdakṣiṇāntikā samastapādeṣu dvitīyalaḥ // KedV_2.15 //",
  "GK17kedvratu:82": "udīcyavṛttirdvitīyalaḥ sakto 'greṇa bhavedayugmayoḥ // KedV_2.16 //",
  "GK17kedvratu:83": "pūrveṇa yuto 'tha pañcamaḥ prācyavṛttiruditeti yugmayoḥ // KedV_2.17 //",
  "GK17kedvratu:84": "yadā samāvojayugmakau pūrvayorbhavati tatpravṛttakam // KedV_2.18 //",
  "GK17kedvratu:85": "asya yugmaracitā 'parāntikā // KedV_2.19 //",
  "GK17kedvratu:86": "ayugbhavā cāruhāsinī // KedV_2.20 //",
  "GK17kedvratu:87": "[ vaktra-prakaraṇam (21-30) ]",
  "GK17kedvratu:88": "vaktraṃ nādyānnasau syātāmabdheryo 'nuṣṭubhi khyātam // KedV_2.21 //",
  "GK17kedvratu:89": "yujorjena saridbhartuḥ pathyāvaktraṃ prakīrtitam // KedV_2.22 //",
  "GK17kedvratu:90": "ojayorjena vāridhestadeva viparītādi // KedV_2.23 //",
  "GK17kedvratu:91": "capalāvaktramayujornakāraśvetpayorāśeḥ // KedV_2.24 //",
  "GK17kedvratu:92": "yasyā laḥ saptamo yugme sā yugmavipulā matā // KedV_2.25 //",
  "GK17kedvratu:93": "sautavasyā 'khileṣvapi // KedV_2.26 //",
  "GK17kedvratu:94": "bhenā 'bdhito bhādvipulā // KedV_2.27 //",
  "GK17kedvratu:95": "itthamanyā raścaturthāt // KedV_2.28 //",
  "GK17kedvratu:96": "no 'mbudheścennavipulā // KedV_2.29 //",
  "GK17kedvratu:97": "to 'bdhestatpūrvānyā bhavet // KedV_2.30 //",
  "GK17kedvratu:98": "[ mātrāsamaka-prakaraṇam (31-38) ]",
  "GK17kedvratu:99": "dviguṇitavasuladhuracaladhṛtiriti // KedV_2.31 //",
  "GK17kedvratu:100": "mātrāsamakaṃ navamo lgāntam // KedV_2.32 //",
  "GK17kedvratu:101": "jo nlāvathāmbudherviślokaḥ // KedV_2.33 //",
  "GK17kedvratu:102": "tadyugalādvānavāsikā syāt // KedV_2.34 //",
  "GK17kedvratu:103": "bāṇāṣṭanavasu yadi laścitā // KedV_2.35 //",
  "GK17kedvratu:104": "upacitrā navame parayukte // KedV_2.36 //",
  "GK17kedvratu:105": "yadatītakṛtavividhalakṣmayutairmātrāsamādipādaiḥ kalitam /",
  "GK17kedvratu:106": "aniyatavṛttaparimāṇayuktaṃ prathitaṃ jagatsu pādākulakam // KedV_2.37 //",
  "GK17kedvratu:107": "vṛttasya lā vinā varṇairgā varṇā gurubhistathā /",
  "GK17kedvratu:108": "guruvo lairdale nityaṃ pramāṇamiti niścitam // KedV_2.38 //",
  "GK17kedvratu:109": "śikhiguṇitadaśalaghuracitamapagatalaghuyugalamaparamidamakhilam /",
  "GK17kedvratu:110": "saguru śakalayugalakamapi suparighaṭitalalitapadavitati bhavati śikhā // KedV_2.39 //",
  "GK17kedvratu:111": "vinimayavinihitaśakalayugalakalitapadavitativiracitaguṇanicayā /",
  "GK17kedvratu:112": "śrutisukhakṛdiyamapi jagati ñi jaśira upagatavati sati bhavati khajā // KedV_2.40 //",
  "GK17kedvratu:113": "aṣṭāvardhe gā dvayabhyastā yasyāḥ sā 'naṅgakrīḍoktā /",
  "GK17kedvratu:114": "dalamaparamapi vasuguṇitasalilanidhilaghu kaviracitaṃ padavitati bhavati // KedV_2.41 //",
  "GK17kedvratu:115": "triguṇanavalaghuraṃvasitigururitidalayugakṛtatanuratirucirā // KedV_2.42 //",
  "GK17kedvratu:116": "iti śrīmadbhaṭṭarāmeśvarasūnunārāyaṇabhaṭṭaviracitāyāṃ vṛttaratnākaravyākhyāyāṃ",
  "GK17kedvratu:117": "mātrāvṛttādhikāro nāma dvitīyo 'dhyāyaḥ //",
  "GK17kedvratu:118": "----------------------------",
  "GK17kedvratu:119": "tṛtīyo 'dhyāyaḥ",
  "GK17kedvratu:120": "guḥ śtrīḥ // KedV_3.1 //",
  "GK17kedvratu:121": "gau strī // KedV_3.2 //",
  "GK17kedvratu:122": "mo nārī // KedV_3.3 //",
  "GK17kedvratu:123": "ro mṛgī // KedV_3.4 //",
  "GK17kedvratu:124": "mgau cetkanyā // KedV_3.5 //",
  "GK17kedvratu:125": "mgau giti paṅktiḥ // KedV_3.6 //",
  "GK17kedvratu:126": "tyau stastanumadhyā // KedV_3.7 //",
  "GK17kedvratu:127": "śaśivadanā nyau // KedV_3.8 //",
  "GK17kedvratu:128": "vidyullekhā mo maḥ // KedV_3.9 //",
  "GK17kedvratu:129": "tsā cedvasumatī // KedV_3.10 //",
  "GK17kedvratu:130": "msau gaḥ syānmadalekhāḥ // KedV_3.11 //",
  "GK17kedvratu:131": "bhau giti citrapadā gaḥ // KedV_3.12 //",
  "GK17kedvratu:132": "mo mo go go vidyunmālā // KedV_3.13 //",
  "GK17kedvratu:133": "māṇavakaṃ bhāttalagā // KedV_3.14 //",
  "GK17kedvratu:134": "mnau gau haṃsarutametat // KedV_3.15 //",
  "GK17kedvratu:135": "rjau samānikā galau ca // KedV_3.16 //",
  "GK17kedvratu:136": "pramāṇikā jarau lagau // KedV_3.17 //",
  "GK17kedvratu:137": "vitānamābhyāṃ yadanyat // KedV_3.18 //",
  "GK17kedvratu:138": "rāmnasāviha halamukhī // KedV_3.19 //",
  "GK17kedvratu:139": "bhujagaśiśubhṛtāṃ nau maḥ // KedV_3.20 //",
  "GK17kedvratu:140": "msā jgau śuddhavirāḍidaṃ matam // KedV_3.21 //",
  "GK17kedvratu:141": "mnau jgau ceti paṇavanāmakam // KedV_3.22 //",
  "GK17kedvratu:142": "rjau ragau mayūrasāriṇī syāt // KedV_3.23 //",
  "GK17kedvratu:143": "bhmau sagayuktau rukmavatīyam // KedV_3.24 //",
  "GK17kedvratu:144": "mattā jñeyā mabhasagayuktā // KedV_3.25 //",
  "GK17kedvratu:145": "narajagaurbhavenmanoramā // KedV_3.26 //",
  "GK17kedvratu:146": "tjau jo guruṇeyamupasthitā // KedV_3.27 //",
  "GK17kedvratu:147": "syādindravajrā yadi tau jagau gaḥ // KedV_3.28 //",
  "GK17kedvratu:148": "upendravajrā jatajāstato gau // KedV_3.29 //",
  "GK17kedvratu:149": "pādau yadīyāvupajātayastāḥ // KedV_3.30 //",
  "GK17kedvratu:150": "smaranti jātiṣvidameva nāma me // KedV_3.31 //",
  "GK17kedvratu:151": "najajalagairgaditā sumukhī // KedV_3.32 //",
  "GK17kedvratu:152": "dodhakavṛttamidaṃ bhabhabhādgau // KedV_3.33 //",
  "GK17kedvratu:153": "śālinyuktā mtau tagau go 'bdhilokaiḥ // KedV_3.34 //",
  "GK17kedvratu:154": "vātormīyaṃ kathitā mbhau tagau gaḥ // KedV_3.35 //",
  "GK17kedvratu:155": "vāṇarasaiḥ syādbhatanagagaiḥ śrīḥ // KedV_3.36 //",
  "GK17kedvratu:156": "mbhau nlau gaḥ syād bhramaravilasitam // KedV_3.37 //",
  "GK17kedvratu:157": "rānnarāviha rathoddhatā lagau // KedV_3.38 //",
  "GK17kedvratu:158": "svāgateti ranabhādguruyugmam // KedV_3.39 //",
  "GK17kedvratu:159": "nanaralagururacitā vṛntā // KedV_3.40 //",
  "GK17kedvratu:160": "nanaramagurubhiśca bhadrikā // KedV_3.41 //",
  "GK17kedvratu:161": "śyenikā rajau ralau gururpadā // KedV_3.42 //",
  "GK17kedvratu:162": "mauktikamālā yadi bhatanādgau // KedV_3.43 //",
  "GK17kedvratu:163": "upasthitamidaṃ jsau tādgakārau // KedV_3.44 //",
  "GK17kedvratu:164": "candravartma nigadanti ranabhasaiḥ // KedV_3.45 //",
  "GK17kedvratu:165": "jatau tu vaṃśasthamudīritaṃ jarau // KedV_3.46 //",
  "GK17kedvratu:166": "syādindravaṃśā tatajai rasaṃyutaiḥ // KedV_3.47 //",
  "GK17kedvratu:167": "iha toṭakamambudhisaiḥ prathitam // KedV_3.48 //",
  "GK17kedvratu:168": "drutavilambitamāha nabhau bharau // KedV_3.49 //",
  "GK17kedvratu:169": "minuśaraviratirnau myau puṭo 'yam // KedV_3.50 //",
  "GK17kedvratu:170": "pramuditavadanā bhavennau ca rau // KedV_3.51 //",
  "GK17kedvratu:171": "nayasahitau nyau kusumavacitrā // KedV_3.52 //",
  "GK17kedvratu:172": "rasairjasajasā jaloddhatagatiḥ // KedV_3.53 //",
  "GK17kedvratu:173": "caturjagaṇaṃ vada mauktikadāma // KedV_3.54 //",
  "GK17kedvratu:174": "bhujaṅgaprayātaṃ bhavedyaiścatubhiḥ // KedV_3.55 //",
  "GK17kedvratu:175": "raiścaturbhiryutā sragviṇī sammatā // KedV_3.56 //",
  "GK17kedvratu:176": "bhuvi bhavennabhajaraiḥ priyaṃvadā // KedV_3.57 //",
  "GK17kedvratu:177": "tyau tyau maṇimālā cchinnā guhavaktraiḥ // KedV_3.58 //",
  "GK17kedvratu:178": "dhīrairabhāṇi lalitā tabhaujarau // KedV_3.59 //",
  "GK17kedvratu:179": "pramitākṣarā sajasasairuditā // KedV_3.60 //",
  "GK17kedvratu:180": "nanabharasahitā mahitojjvalā // KedV_3.61 //",
  "GK17kedvratu:181": "pañcāśvaiśchinnā vaiśyadevī mamau yau // KedV_3.62 //",
  "GK17kedvratu:182": "abdhyaṣṭābhirjaladharamālā mbhau smau // KedV_3.63 //",
  "GK17kedvratu:183": "iha navamālikā najabhayaiḥ syāt // KedV_3.64 //",
  "GK17kedvratu:184": "svaraśaraviratirnanau rau prabhā // KedV_3.65 //",
  "GK17kedvratu:185": "bhavati najāvatha mālatī jarau // KedV_3.66 //",
  "GK17kedvratu:186": "jabhau jarau vadati pañcāmaram // KedV_3.67 //",
  "GK17kedvratu:187": "abhinavatāmarasaṃ najajādyaḥ // KedV_3.68 //",
  "GK17kedvratu:188": "turagarasayatirnau tatau gaḥ kṣamā // KedV_3.69 //",
  "GK17kedvratu:189": "mnau jrau gastridaśayatiḥ praharṣiṇīam // KedV_3.70 //",
  "GK17kedvratu:190": "caturgrahairatirucirā jabhasjagāḥ // KedV_3.71 //",
  "GK17kedvratu:191": "vedai randhrairmtau yasagā mattamayūram // KedV_3.72 //",
  "GK17kedvratu:192": "( upasthitamidaṃ jsau tsau sagurukaṃ cet ) // KedV_3.73 //",
  "GK17kedvratu:193": "sajasā jagau bhavati mañjubhāṣiṇī // KedV_3.74 //",
  "GK17kedvratu:194": "nanatatagurubhiścandrikāśvartubhiḥ // KedV_3.75 //",
  "GK17kedvratu:195": "mtau nsau gāvakṣagrahaviratisambādhā // KedV_3.76 //",
  "GK17kedvratu:196": "nanarasalaghugaiḥ svarairaparājitā // KedV_3.77 //",
  "GK17kedvratu:197": "nanabhanalaghugaiḥ praharaṇakalitā // KedV_3.78 //",
  "GK17kedvratu:198": "uktā vasantatilakā tabhajā jagau gaḥ // KedV_3.79 //",
  "GK17kedvratu:199": "siṃhonnateyamuditā munikāśyapena // KedV_3.80 //",
  "GK17kedvratu:200": "uddharṣiṇīyamuditā munisaitavena // KedV_3.81 //",
  "GK17kedvratu:201": "induvadanā bhajasanaiḥ saguruyugmaiḥ // KedV_3.82 //",
  "GK17kedvratu:202": "dviḥsaptacchidalolā msau mbhau gau caraṇe cet // KedV_3.83 //",
  "GK17kedvratu:203": "dvihatahayalaghuratha giti śaśikalā // KedV_3.84 //",
  "GK17kedvratu:204": "sragiti bhavati rasanavakayatiriyam // KedV_3.85 //",
  "GK17kedvratu:205": "vasuhayayatiriha maṇiguṇanikaraḥ // KedV_3.86 //",
  "GK17kedvratu:206": "nanamayayayuteyaṃ mālinī bhogilokaiḥ // KedV_3.87 //",
  "GK17kedvratu:207": "bhavati najau bhajau rasahitau prabhadrakam // KedV_3.88 //",
  "GK17kedvratu:208": "sajanā nayau śaradaśayatiriyamelā // KedV_3.89 //",
  "GK17kedvratu:209": "mrau myau yāntau bhavetāṃ saptāṣṭabhiścandralekhā // KedV_3.90 //",
  "GK17kedvratu:210": "bhratrinagaiḥ svarātkhamṛṣabhagajavilasitam // KedV_3.91 //",
  "GK17kedvratu:211": "najabhajaraiḥ sadā bhavati vāṇinī gayuktaiḥ // KedV_3.92 //",
  "GK17kedvratu:212": "rasai rudraiśchinnā yamanasabhalā gaḥ śikhariṇī // KedV_3.93 //",
  "GK17kedvratu:213": "jasau jasayalā vasugrahayatiśca pṛthvī guruḥ // KedV_3.94 //",
  "GK17kedvratu:214": "diṅmuni vaṃśapatrapatitaṃ bharanabhanalagaiḥ // KedV_3.95 //",
  "GK17kedvratu:215": "rasayugahayairnsau mrau slau gau yadā hariṇī tadā // KedV_3.96 //",
  "GK17kedvratu:216": "mandākrāntā jaladhiṣaḍagairmbhaunatau tād guru cet // KedV_3.97 //",
  "GK17kedvratu:217": "hayadaśabhirnajau bhajajalā guru narkuṭakam // KedV_3.98 //",
  "GK17kedvratu:218": "muniguhakārṃavaiḥ kṛtayati vada kokilakam // KedV_3.99 //",
  "GK17kedvratu:219": "syād bhūtartvaśvaiḥ kusumitalatāvellitā mtau nayau yau // KedV_3.100 //",
  "GK17kedvratu:220": "sūryāśvairmasajastatāḥ saguravaḥ śārdūlavikrīḍitam // KedV_3.101 //",
  "GK17kedvratu:221": "jñeyā saptāśvaṣaḍbhrmarabhanayayutā bhlo gaḥ suvadanā // KedV_3.102 //",
  "GK17kedvratu:222": "trī rajau galau bhavedihedṛśena lakṣaṇena vṛttanāma // KedV_3.103 //",
  "GK17kedvratu:223": "mrabhnairyānāṃ trayeṇa trimuniyatiyutā sragdharā kīrtiteyam // KedV_3.104 //",
  "GK17kedvratu:224": "bhrau naranā ranāvatha gururdigarkaviramaṃ hi bhadrakamiti // KedV_3.105 //",
  "GK17kedvratu:225": "yadiha najau bhajau bhjabhalagāstadaśvalalitaṃ harārkayatimam // KedV_3.106 //",
  "GK17kedvratu:226": "matākrīḍā mau tnau nau nalgiti bhavati vasuśaradaśayatiyutā // KedV_3.107 //",
  "GK17kedvratu:227": "bhūtamuninairyatiriha bhatanāḥ sbhau bhanayāśca yadi bhavati tanvī // KedV_3.108 //",
  "GK17kedvratu:228": "krauñjapadā bhamau smau nananā ngāviṣaśaravasumuniviratiriha bhavet // KedV_3.109 //",
  "GK17kedvratu:229": "vasvīśāśvacchedopetaṃ mamatanayuganarasalagairbhujaṅgavijṛmbhitam // KedV_3.110 //",
  "GK17kedvratu:230": "mo nāḥ ṣaṭ sagagiti yadi navarasarasaśarayatiyutamapavāhākhyam // KedV_3.111 //",
  "GK17kedvratu:231": "yadi ha nayugalaṃ tataḥ saptarephā stadā caṇḍavṛṣṭiprapāto bhaveddaṇḍakaḥ // KedV_3.112 //",
  "GK17kedvratu:232": "praticaraṇavivṛddharephāḥ syurṇārṇavavyālajīmūtalīlākaroddāmaśaṅkhādayaḥ // KedV_3.113 //",
  "GK17kedvratu:233": "pracitakasamabhidho dhīradhībhiḥ smṛtodaṇḍako nadvayāduttaraiḥ saptabhiryaiḥ // KedV_3.114 //",
  "GK17kedvratu:234": "iti śrīmadbhaṭṭarāmeśvarasūnunārāyaṇabhaṭṭaviracitāyāṃ",
  "GK17kedvratu:235": "vṛttaratnākaravyākhyāyāṃ samavṛttādhyāyastṛtīyaḥ //",
  "GK17kedvratu:236": "-----------------------",
  "GK17kedvratu:237": "caturtho 'dhyāyaḥ",
  "GK17kedvratu:238": "viṣame yadi sau salagā dale bhau yuji bhād gurukāvupacitram // KedV_4.1 //",
  "GK17kedvratu:239": "bhatrayamojagataṃ guruṇī cedyuji ca najau jyayutā drutamadhyā // KedV_4.2 //",
  "GK17kedvratu:240": "sayugātsagurū viṣame cedbhāviha vegavatī yuji bhādgau // KedV_4.3 //",
  "GK17kedvratu:241": "oje taparau jarau guruścenmasau jgaugbhadravirāṅ bhavedanoje // KedV_4.4 //",
  "GK17kedvratu:242": "asame sajo saguruyuktau ketumatī same bharanagādgaḥ // KedV_4.5 //",
  "GK17kedvratu:243": "ākhyānakīṃ tau jagurū ga oje jatāvanoje jagurū guruścet // KedV_4.6 //",
  "GK17kedvratu:244": "jatau jagau go viṣame same cettau jgau ga eṣā viparītapūrvā // KedV_4.7 //",
  "GK17kedvratu:245": "sayugātsalaghū viṣame gururyuji nabhau bharakau hariṇaplutā // KedV_4.8 //",
  "GK17kedvratu:246": "ayuji nanaralā guruḥ samenjamaparavaktramidaṃ tato jarau // KedV_4.9 //",
  "GK17kedvratu:247": "ayuji nayugarephato yakāro yuji ca najau jaragāśca puṣpitāgrā // KedV_4.10 //",
  "GK17kedvratu:248": "vadantyaparavaktrākhyaṃ vaitālīyaṃ vipaścitaḥ /",
  "GK17kedvratu:249": "puṣpatāgrābhidhaṃ kecidaupacchandasikaṃ tathā // KedV_4.11 //",
  "GK17kedvratu:250": "syāayugmake rajau rayau same cejjarau jarau gururyavātparā matīyam // KedV_4.12 //",
  "GK17kedvratu:251": "iti śrīmadbhaṭṭarāmeśvarasūnunārāyaṇabhaṭṭaviracitāyāṃ",
  "GK17kedvratu:252": "vṛttaratnākaravyākhyāyāmardhasamādhyāyaścaturthaḥ //",
  "GK17kedvratu:253": "------------------------",
  "GK17kedvratu:254": "pañcamo 'dhyāyaḥ",
  "GK17kedvratu:255": "[padacaturūrdhva-prakaraṇam (1-5)]",
  "GK17kedvratu:256": "mukhavādo 'ṣṭabhirvarṇaiḥ pare syurmakarālayaiḥ kramād vṛddhaiḥ //",
  "GK17kedvratu:257": "satataṃ yasya vicitraiḥ pādaiḥ sampannasaundaryaṃ",
  "GK17kedvratu:258": "taduditamamalamatibhiḥ padacaturūrdhvābhidhaṃ vṛttam // KedV_5.1 //",
  "GK17kedvratu:259": "prathamamuditavṛtte viracitaviṣamacaraṇabhāji //",
  "GK17kedvratu:260": "gurukayugalanidhana iha sahita āṅā",
  "GK17kedvratu:261": "laghuviratapadavitatiyatiriti bhavati pīḍaḥ // KedV_5.2 //",
  "GK17kedvratu:262": "prathamamitaracaraṇasamutthaṃ śrayati sa yadi lakṣma /",
  "GK17kedvratu:263": "itaraditaragatitamapi yadi ca turyaṃ",
  "GK17kedvratu:264": "caraṇayugalakamiti kalikā sā // KedV_5.3 //",
  "GK17kedvratu:265": "dviguruyutasamalacaraṇāntā mukhacaraṇagatamanubhavati ca tṛtīyaḥ /",
  "GK17kedvratu:266": "aparamiha lakṣma prakṛtamakhilamapi yadidamanubhavati lavalī sā // KedV_5.4 //",
  "GK17kedvratu:267": "prathamadhivasati yadi turyaṃ,",
  "GK17kedvratu:268": "caramacaraṇapadamavasitaguruyugmam /",
  "GK17kedvratu:269": "nikhilamaparamuparigatamiti lalitapadayuktā,",
  "GK17kedvratu:270": "tadidamamṛtadhārā // KedV_5.5 //",
  "GK17kedvratu:271": "[udgatā-prakaraṇam (6-8)]",
  "GK17kedvratu:272": "sajādime salaghukau ca nasajagurukairathodgatā /",
  "GK17kedvratu:273": "tryaṅghrigatabhanajalā gayutāḥ sajasā agau caraṇamekataḥ paṭhet // KedV_5.6 //",
  "GK17kedvratu:274": "caraṇatrayaṃ vrajati lakṣma yadi sakalamudgatāgatam /",
  "GK17kedvratu:275": "nā bhagau bhavati saurabhakaṃ caraṇe yadīha bhavatastṛtīyake // KedV_5.7 //",
  "GK17kedvratu:276": "nayugaṃ sakārayugalaṃ ca bhavati caraṇe tṛtīyake /",
  "GK17kedvratu:277": "taduditamurumatibhirlalitaṃ yadi śeṣamasya khalu pūrvatulyakam // KedV_5.8 //",
  "GK17kedvratu:278": "[upasthitapracupita-prakaraṇam (9-11)]",
  "GK17kedvratu:279": "msau jbhau gau prathamāṅghrirekataḥ pṛthaganyattritayaṃ sanajaragāstato nanau saḥ /",
  "GK17kedvratu:280": "trinaparikalitajayau pracupitamidamuditamupasthitapūrvam // KedV_5.9 //",
  "GK17kedvratu:281": "nau pāde 'tha tṛtīyake sanau nasayuktau prathamāṅghrikṛtayatistu vardhamānam /",
  "GK17kedvratu:282": "tritayamaparamapi pūrvasadṛśamiha bhavati pratatamatibhiriti gaditaṃ laghu vṛttam // KedV_5.10 //",
  "GK17kedvratu:283": "prathame ca viratirārṣabhaṃ bruvanti //",
  "GK17kedvratu:284": "tacchuddhavirāṭ puraḥ sthitaṃ tritayamaparamapi yadi pūrvasamaṃ syāt // KedV_5.11 //",
  "GK17kedvratu:285": "viṣamākṣarapādaṃ vā pādairasamaṃ daśadhrmavat /",
  "GK17kedvratu:286": "yacchando noktamatra gātheti tatsūribhiḥ proktam // KedV_5.12 //",
  "GK17kedvratu:287": "iti śrīmadbhaṭṭarāmeśvarasūnunārāyaṇabhaṭṭaviracitāyāṃ vṛttaratnākaravyākhyāyāṃ",
  "GK17kedvratu:288": "pañcamo 'dhyāyaḥ //",
  "GK17kedvratu:289": "---------------------------------",
  "GK17kedvratu:290": "ṣaṣṭho 'dhyāyaḥ",
  "GK17kedvratu:291": "[prastāraḥ]",
  "GK17kedvratu:292": "prastāro naṣṭamuddiṣṭamekadvayādilagakriyā /",
  "GK17kedvratu:293": "saṅkhyānamadhvayogaśca ṣaḍete pratyayāḥ smṛtāḥ // KedV_6.1 //",
  "GK17kedvratu:294": "pāde sarvagurāvādyāllaghuṃ nyasya guroradhaḥ /",
  "GK17kedvratu:295": "yathopari tathā śeṣaṃ bhūyaḥ kuryādamuṃ vidhim // KedV_6.2 //",
  "GK17kedvratu:296": "ūne dadyād gurūneva yāvatsarvalaghurbhavet /",
  "GK17kedvratu:297": "prastāro 'yaṃ samākhyātaśchandovicitivedibhiḥ // KedV_6.3 //",
  "GK17kedvratu:298": "[naṣṭam]",
  "GK17kedvratu:299": "naṣṭasya yo bhavedaṅkastasyārdhe 'rdhe same ca laḥ //",
  "GK17kedvratu:300": "viṣame caikamādhāya syādardhe 'rdhe gurubhavet // KedV_6.4 //",
  "GK17kedvratu:301": "[uddiṣṭam]",
  "GK17kedvratu:302": "uddiṣṭaṃ dviguṇānādyāduparyaṅkānsamālikhet /",
  "GK17kedvratu:303": "laghusthā ye ca tatrāṅkāstaiḥ saikairmiśritairbhavet // KedV_6.5 //",
  "GK17kedvratu:304": "[ekadvayādilagakriyā]",
  "GK17kedvratu:305": "varṇānvṛttabhavānsaikānauttarādharyataḥ sthitān /",
  "GK17kedvratu:306": "ekādikramataścaitānuparyupari nikṣipet // KedV_6.6 //",
  "GK17kedvratu:307": "upāntyato nivarteta tyajannekaikamūrdhvataḥ /",
  "GK17kedvratu:308": "uparyādyād gurorekamekadvyādilagakriyā // KedV_6.7 //",
  "GK17kedvratu:309": "[saṅkhyānam]",
  "GK17kedvratu:310": "lagakriyāṅkasandohe bhavetsaṅkhyā vimiśrite /",
  "GK17kedvratu:311": "uddiṣṭāṅkasamāhāraḥ saikā vā janayedimām // KedV_6.8 //",
  "GK17kedvratu:312": "[adhvayogaḥ]",
  "GK17kedvratu:313": "saṅkhyaiva dviguṇaikonā sadbhiradhvā prakīrtitaḥ //",
  "GK17kedvratu:314": "vṛttasyāṅgulikā vyāptiradhaḥ kuryāttathāṅgulim // KedV_6.9 //",
  "GK17kedvratu:315": "vaṃśe 'bhūtkaśyapasya prakaṭaguṇagaṇaḥ śaivasiddhāntavettā",
  "GK17kedvratu:316": "vipraḥ pavyekanāmā vimalataramatirvedatattvārthabodhe /",
  "GK17kedvratu:317": "kedārastasya sūnuḥ śivacaraṇayugārādhanaikāgracittaśchandastenābhirāmaṃ",
  "GK17kedvratu:318": "vṛttaratnākarākhyam // KedV_6.10 //",
  "GK17kedvratu:319": "iti śrīmadbhaṭṭarāmeśvarasūnunārāyaṇabhaṭṭaviracitāyāṃ vṛttaratnākaravyākhyāyāṃ",
  "GK17kedvratu:320": "prastārādhyāyaḥ ṣaṣṭhaḥ samāptaḥ //",
  "GK17kedvratu:321": "----------------------------------------------"
}