{
    "T01n862u_001:0": " pratisarāstotram",
    "T01n862u_001:1": " om namaḥ śrīpratisarāyai",
    "T01n862u_001:2": " pratisarai ! amaraughaiḥ pūjitāṃ tvāṃ nato'smi",
    "T01n862u_001:3": " pratipadajanarakṣākāriṇīṃ sarvakālam |",
    "T01n862u_001:4": " bhṛgudahanasamudrāpātaduḥkhe surakṣāṃ",
    "T01n862u_001:5": " ripuviṣabhayahatrīṃ rājyabhogādikartrīm || 1 ||",
    "T01n862u_001:6": " parivṛtanijavargān bhaktadattāpavargān",
    "T01n862u_001:7": " kalitaduritavargān mokṣalābhaikamārgān |",
    "T01n862u_001:8": " kṛtanarasurasargān kāritānaṅgabhaṅgān",
    "T01n862u_001:9": " stutikṛtamunigargān sarvarakṣaikasaṅgān || 2 ||",
    "T01n862u_001:10": " prahṛtasakalavighne sarvalokaikamānye",
    "T01n862u_001:11": " daśabalakṛtadhanye pañcadevīvareṇye |",
    "T01n862u_001:12": " viṣamapadaśaraṇye sarvadā tvāṃ namasye",
    "T01n862u_001:13": " vasita iva araṇye dāvadagdhe virasye || 3 ||",
    "T01n862u_001:14": " sakalajanasuvāñchāpūraṇe kāmadhenu-",
    "T01n862u_001:15": " mabhilaṣitaphalāptyai kalpavṛkṣāgravallīm |",
    "T01n862u_001:16": " asurasuranarādyairvanditāṅghryabjayugmā-",
    "T01n862u_001:17": " murasi lalitānānāratnamālādiyuktām || 4 ||",
    "T01n862u_001:18": " uḍupatiśatadīptāṃ śaṅkhakundāvadātāṃ",
    "T01n862u_001:19": " grahabhayapariśāntāṃ vajrasattvātmikāṃ tām |",
    "T01n862u_001:20": " śravaṇalalitalole kuṇḍale saṃvahantīṃ",
    "T01n862u_001:21": " mukuṭamaṇiśikhābhiḥ kāśitāṃ tāṃ namāmi || 5 ||",
    "T01n862u_001:22": " janani sakaladuḥkhāttāraṇī tvaṃ prasīda",
    "T01n862u_001:23": " sapadi vigatarakṣāṃ rakṣa me mātaraṃ ca |",
    "T01n862u_001:24": " sakaruṇaruditaṃ māṃ tvaddayāsvasthabhūtaṃ",
    "T01n862u_001:25": " kṛtasarasinipātāṃ tvaṃ gatistvaṃ gatirnau || 6 ||",
    "T01n862u_001:26": " paṭhati pratisarāyāḥ stotrametatsadā yo",
    "T01n862u_001:27": " jvalanajalaviṣāṇāṃ coraśārdūlakānām |",
    "T01n862u_001:28": " sagadanidhanakānāṃ bhītayā nāśayantī",
    "T01n862u_001:29": " pratipadamalabhagnaṃ kāmunā dhairyalābham || 7 ||",
    "T01n862u_001:30": " nijajanapariyuktā dharmakāmārthavṛddhā",
    "T01n862u_001:31": " ripugaṇaparimuktā saukhyameva prabhuktā |",
    "T01n862u_001:32": " dadati vipulabhogyaṃ kāmadā sarvadā'sau",
    "T01n862u_001:33": " nihatasakalapāpaṃ mokṣamāyānti cānte || 8 ||",
    "T01n862u_001:34": " vigalitanayanāmbuḥ sāñjaliḥ saṃvilāpī",
    "T01n862u_001:35": " stutimiti pracakāra traibhavānāṃ jananyāḥ |",
    "T01n862u_001:36": " bhavatu mama tu saṃpat sarvabhāvā kṛśāno",
    "T01n862u_001:37": " tanu kuśalasurakṣā itthamevaṃ sucittaḥ || 9 ||",
    "T01n862u_001:38": " vipacatu mayi sarvaṃ mādṛśaṃ karma bhogyaṃ",
    "T01n862u_001:39": " sasuranaracayānāṃ bhrātṛmātṛdruhāṇām |",
    "T01n862u_001:40": " kuruta kuruta dhairyaṃ yena mokṣaṃ sulabhyaṃ",
    "T01n862u_001:41": " jaṭharanilayavāsaṃ māstu pāpaṃ kadācit || 10 ||",
    "T01n862u_001:42": " udaranarakavāsaṃ ye tadā sāvatāraṃ",
    "T01n862u_001:43": " satataśubhasucetāḥ sādhayiṣyāmi bodhim |",
    "T01n862u_001:44": " kṛtajagati surakṣāṃ yena kaivalyaprāptiḥ",
    "T01n862u_001:45": " iti muditasucetāḥ karmaṇā digdhacittaḥ || 11 ||",
    "T01n862u_001:46": " pratisara avatāryāṃ sāmbikāṃ mā vilamba",
    "T01n862u_001:47": " aniśamitivadan so nindayan vai svakarma |",
    "T01n862u_001:48": " pratibhayaviṣadigdhastasthivānarbhakaḥ san",
    "T01n862u_001:49": " muhuriha hi śaraṇyāṃ tāṃ smaran sarvarakṣām || 12 ||",
    "T01n862u_001:50": " aho sudhanyā jagadekamātā prakrāntadaivā ripuvargabhītā |",
    "T01n862u_001:51": " yasyāḥ prabhāvādvihitātyapāpāt prayāti vahnirjalaśītalatvam || 13 ||",
    "T01n862u_001:52": " prabhāvatāho punareva tasyāḥ",
    "T01n862u_001:53": " vahniṃ jalaṃ cāpi giriṃ sthalaṃ vai |",
    "T01n862u_001:54": " yasyāḥ smṛtenaiva savismatīyān",
    "T01n862u_001:55": " vaiśvānaraḥ prāpa dvidhākṛtiṃ tām || 14 ||",
    "T01n862u_001:56": " gopāśarīraṃ hradapātaśītaṃ śaitaṃ tvanāśārthamivādhikoṣṇām |",
    "T01n862u_001:57": " daivādidattābhijane samantānnitāntadūrādatipāpadattām || 15 ||",
    "T01n862u_001:58": " śrībhadrakalpāvadānoddhṛtaṃ yaśodharāgarbhasthabālakṛtaṃ",
    "T01n862u_001:59": " pratisarāstotraṃ samāptam |"
}