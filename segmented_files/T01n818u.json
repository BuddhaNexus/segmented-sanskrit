{
    "T01n818u_001:0": " avalokiteśvarāṣṭottaraśatanāmastotram",
    "T01n818u_001:1": " om namo'valokiteśvarāya",
    "T01n818u_001:2": " padmasattva mahāpadma lokeśvara maheśvara |",
    "T01n818u_001:3": " avalokiteśa dhīrāgrya vajradharma namo'stu te || 1 ||",
    "T01n818u_001:4": " dharmarāja mahāśuddha sattvarāja mahāmate |",
    "T01n818u_001:5": " padmātmaka mahāpadma padmanātha namo'stu te || 2 ||",
    "T01n818u_001:6": " padmodbhava supadmābha padmaśuddha suśodhaka |",
    "T01n818u_001:7": " vajrapadma supadmāṅka padmapadma namo'stu te || 3 ||",
    "T01n818u_001:8": " mahāviśva mahāloka mahākāya mahopama |",
    "T01n818u_001:9": " mahādhīra mahāvīra mahāśaure namo'stu te || 4 ||",
    "T01n818u_001:10": " sattvāśaya mahāyāna mahāyoga pitāmaha |",
    "T01n818u_001:11": " śambhu śaṅkara śuddhārtha buddhapadma namo'stu te || 5 ||",
    "T01n818u_001:12": " dharmatattvārtha saddharma śuddhaddharmaṃ sudharmakṛt |",
    "T01n818u_001:13": " mahādharma sudharmāgrya dharmacakra namo'stu te || 6 ||",
    "T01n818u_001:14": " buddhasattva susattvāgrya dharmasattva susattvadhṛk |",
    "T01n818u_001:15": " sattvottama susattvajña sattvasattva namo'stu te || 7 ||",
    "T01n818u_001:16": " avalokiteśa nāthāgrya mahānātha vilokita |",
    "T01n818u_001:17": " ālokaloka lokārtha lokanātha namo'stu te || 8 ||",
    "T01n818u_001:18": " lokākṣarākṣara mahā akṣarāgryākṣaropama |",
    "T01n818u_001:19": " akṣarākṣara sarvākṣa cakrākṣara namo'stu te || 9 ||",
    "T01n818u_001:20": " padmahasta mahāhasta samāśvāsaka dāyaka |",
    "T01n818u_001:21": " buddhadharma mahābuddha buddhātmaka namo'stu te || 10 ||",
    "T01n818u_001:22": " buddharūpa mahārūpa vajrarūpa surupavit |",
    "T01n818u_001:23": " dharmāloka sutejāgrya lokāloka namo'stu te || 11 ||",
    "T01n818u_001:24": " padmaśrīnātha nāthāgrya dharmaśrīnātha nāthavān |",
    "T01n818u_001:25": " brahyanātha mahābrahma brahmaputra namo'stu te || 12 ||",
    "T01n818u_001:26": " dīpa dīpāgrya dīpogra dīpāloka sudīpaka |",
    "T01n818u_001:27": " dīpanātha mahādipa buddhadīpa namo'stu te || 13 ||",
    "T01n818u_001:28": " buddhābhiṣikta buddhāgrya buddhaputra mahābudha |",
    "T01n818u_001:29": " buddhābhiṣekamurddhāgrya buddhabuddha namo'stu te || 14 ||",
    "T01n818u_001:30": " buddhacakṣo mahācakṣo dharmacakṣo mahekṣaṇa |",
    "T01n818u_001:31": " samādhijñānasarvasva vajranetra namo'stu te || 15 ||",
    "T01n818u_001:32": " evaṃ sarvātmanā gauṇaṃ nāmnāmaṣṭaśataṃ tava |",
    "T01n818u_001:33": " bhāvayet stunuyādvāpi lokaiśvaryamavāpnuyāt || 16 ||",
    "T01n818u_001:34": " āryāvalokiteśvaranāmāṣṭottaraśatādhyeṣaṇāstotraṃ samāptam |"
}