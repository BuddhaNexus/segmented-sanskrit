{
    "GSP29antarkau:0": " ANALYTIC VERSION according to BHELA conventions:",
    "GSP29antarkau:1": " Sandhi markers:",
    "GSP29antarkau:2": " (. =short vowel, - =long vowel)",
    "GSP29antarkau:3": " in word-sandhi:",
    "GSP29antarkau:4": " 1: . . e.g.: veda+anta = vedā1nta",
    "GSP29antarkau:5": " in sentence-sandhi:",
    "GSP29antarkau:6": " 7: . . e.g.: ca+api = cā7pi",
    "GSP29antarkau:7": " AnTs_1ab nidhāya hṛdi viśveśaṃ vidhāya guru-vandanam /",
    "GSP29antarkau:8": " AnTs_1cd bālānāṃ sukha-bodhāya kriyate tarka-saṃgrahaḥ //",
    "GSP29antarkau:9": " AnTs_2 dravya-guṇa-karma-sāmānya-viśeṣa-samavāyā1bhāvāḥ sapta padā1rthāḥ",
    "GSP29antarkau:10": " AnTs_3 tatra dravyāṇi pṛthivy-ap-tejo-vāyv-ākāśa-kāla-dig-ātma-manāṃsi",
    "GSP29antarkau:11": " AnTs_4",
    "GSP29antarkau:12": " caturviṃśati guṇāḥ //",
    "GSP29antarkau:13": " AnTs_5 utkṣepaṇā1vakṣepaṇā3kuñcana-prasāraṇa-gamanāni pañca karmāṇi //",
    "GSP29antarkau:14": " AnTs_6 param aparaṃ ce7ti dvividhaṃ sāmānyam //",
    "GSP29antarkau:15": " AnTs_7 nitya-dravya-vṛttayo viśeṣās tv an-antā eva //",
    "GSP29antarkau:16": " AnTs_8 samavāyas tv eka eva //",
    "GSP29antarkau:17": " AnTs_9 abhāvaś caturvidhaḥ /",
    "GSP29antarkau:18": " prāg-abhāvaḥ pradhvaṃsā1bhāvo 'tyantā1bhāvo",
    "GSP29antarkau:19": " AnTs_10 gandhavatī pṛthivī /",
    "GSP29antarkau:20": " sā dvividhā nityā1nityā ca /",
    "GSP29antarkau:21": " nityā",
    "GSP29antarkau:22": " paramāṇu-rūpā /",
    "GSP29antarkau:23": " anityā kārya-rūpā /",
    "GSP29antarkau:24": " punas trividhā śarīre1ndriya-viṣaya-bhedāt /",
    "GSP29antarkau:25": " śarīram asmad-ādīnām /",
    "GSP29antarkau:26": " indriyaṃ",
    "GSP29antarkau:27": " gandha-grāhakaṃ ghrāṇaṃ nāsā1gravarti /",
    "GSP29antarkau:28": " viṣayo mṛt-pāṣāṇā3diḥ //",
    "GSP29antarkau:29": " AnTs_11 śita-sparśavatya āpaḥ /",
    "GSP29antarkau:30": " tā dvividhāḥ nityā anityāś ca /",
    "GSP29antarkau:31": " ntyāḥ",
    "GSP29antarkau:32": " paramāṇu-rūpāḥ /",
    "GSP29antarkau:33": " anityāḥ kārya-rūpāḥ. /",
    "GSP29antarkau:34": " punas trividhāḥ.",
    "GSP29antarkau:35": " śarīraṃ varuṇa-loke /",
    "GSP29antarkau:36": " indriyaṃ rasa-grāhakaṃ",
    "GSP29antarkau:37": " rasanaṃ jihvā1gravarti /",
    "GSP29antarkau:38": " viṣayaḥ sarit-samudrā3diḥ //",
    "GSP29antarkau:39": " AnTs_12 uṣṇa-sparśavat tejaḥ /",
    "GSP29antarkau:40": " tad dvividhaṃ nityam anityaṃ ca /",
    "GSP29antarkau:41": " niyyaṃ",
    "GSP29antarkau:42": " paramāṇu-rūpam /",
    "GSP29antarkau:43": " anityaṃ kārya-rūpam /",
    "GSP29antarkau:44": " punas trividhaṃ śarīre1ndriya-viṣaya-bhedāt /",
    "GSP29antarkau:45": " śarīram ādityaloke /",
    "GSP29antarkau:46": " indriyaṃ rūpa-grāhakaṃ",
    "GSP29antarkau:47": " cakṣuḥ kṛṣṇatārā1gravarti /",
    "GSP29antarkau:48": " viṣayaś caturvidhaḥ /",
    "GSP29antarkau:49": " bhaumaṃ vahny-ādikam /",
    "GSP29antarkau:50": " abindhanaṃ divyaṃ vidyud-ādi /",
    "GSP29antarkau:51": " bhuktasya pariṇāma-hetur audaryam. ākarajaṃ suvarṇā3di",
    "GSP29antarkau:52": " AnTs_13 rūpa-rahita-sparśavān vāyuḥ /",
    "GSP29antarkau:53": " sa dvividho nityo 'nityaś ca /",
    "GSP29antarkau:54": " nityaḥ paramāṇu-rūpaḥ /",
    "GSP29antarkau:55": " anityaḥ kārya-rūpaḥ /",
    "GSP29antarkau:56": " punas trividhaḥ śarīre1ndriya-viṣaya-bhedāt /",
    "GSP29antarkau:57": " śarīraṃ vāyu-loke /",
    "GSP29antarkau:58": " indriyaṃ sparśa-grāhakaṃ",
    "GSP29antarkau:59": " tvak-sarva-śarīravarti /",
    "GSP29antarkau:60": " viṣayo vṛkṣā3di-kampana-hetuḥ //",
    "GSP29antarkau:61": " śarīrā1ntaḥsaṃcārī vāyuḥ prāṇaḥ /",
    "GSP29antarkau:62": " sa cai7ko 'py upādhi-bhedāt",
    "GSP29antarkau:63": " prāṇā1pānā3di-saṃjñā labhate //",
    "GSP29antarkau:64": " AnTs_14 śabda-guṇam ākāśam /",
    "GSP29antarkau:65": " tac cai7kaṃ vibhu nityaṃ ca //",
    "GSP29antarkau:66": " AnTs_15 atītā3di-vyavahāra-hetuḥ kālaḥ /",
    "GSP29antarkau:67": " sa cai7ko vibhur nityaś ca //",
    "GSP29antarkau:68": " AnTs_16 prācyā3di-vyavahāra-hetur dik /",
    "GSP29antarkau:69": " sā cai7kā vibhvī nityā ca //",
    "GSP29antarkau:70": " AnTs_17 jñānā1dhikaraṇam ātmā /",
    "GSP29antarkau:71": " sa dvividhaḥ param-ātmā jīvā3tmā ca /",
    "GSP29antarkau:72": " tatre8śvaraḥ sarvajñaḥ param-ātmai2ka eva /",
    "GSP29antarkau:73": " jīvā3tmā pratiśarīraṃ bhinno vibhur nityaś ca //",
    "GSP29antarkau:74": " AnTs_18 sukhā3dy-upalabdhi-sādhanam indriyaṃ manaḥ /",
    "GSP29antarkau:75": " tac ca pratyātma-niyatatvād anantaṃ paramāṇu-rūpaṃ nityaṃ ca //",
    "GSP29antarkau:76": " AnTs_19 cakṣur-mātra-grāhyo guṇo rūpam /",
    "GSP29antarkau:77": " tac ca śukla-nīla-pīta-rakta-harita-kapiśa-citra-bhedāt saptavidhaṃ",
    "GSP29antarkau:78": " pṛthivī-jala-tejo-vṛtti /",
    "GSP29antarkau:79": " tatra pṛthivyāṃ saptavidham /",
    "GSP29antarkau:80": " abhāsvara-śuklaṃ",
    "GSP29antarkau:81": " jale /",
    "GSP29antarkau:82": " bhāsvara-śuklaṃ tejasi //",
    "GSP29antarkau:83": " AnTs_20 rasana-grāhyo guṇo rasaḥ /",
    "GSP29antarkau:84": " sa ca madhu-rāmla-lavaṇa-kaṭu-kaṣāya-tikta-bhedāt ṣaḍvidhaḥ /",
    "GSP29antarkau:85": " pṛthivyāṃ ṣaḍvidhaḥ /",
    "GSP29antarkau:86": " jale madhura eva //",
    "GSP29antarkau:87": " AnTs_21 ghrāṇa-grāhyo guṇo gandhaḥ /",
    "GSP29antarkau:88": " sa ca dvividhaḥ surabhir-asurabhiś ca",
    "GSP29antarkau:89": " / pṛthivī-mātra-vṛttiḥ /",
    "GSP29antarkau:90": " AnTs_22 tvag-indriya-mātra-grāhyo guṇo saparśaḥ /",
    "GSP29antarkau:91": " sa ca trividhaḥ",
    "GSP29antarkau:92": " pṛthivy-ap-tejo-vāyu-vṛttiḥ /",
    "GSP29antarkau:93": " tatra śīto jale",
    "GSP29antarkau:94": " / uṣṇas tejasi /",
    "GSP29antarkau:95": " anuṣṇā1śītaḥ pṛthivī-vāyvoḥ //",
    "GSP29antarkau:96": " AnTs_23 rūpā3di-catuṣṭayaṃ pṛthivyāṃ pākajam anityaṃ ca /",
    "GSP29antarkau:97": " nityam antyaṃ ca /",
    "GSP29antarkau:98": " nityagataṃ nityam /",
    "GSP29antarkau:99": " anityagatam anityam //",
    "GSP29antarkau:100": " AnTs_24 ekatvā3di-vyavahāra-hetuḥ saṃkhyā /",
    "GSP29antarkau:101": " nava-dravya-vṛttir",
    "GSP29antarkau:102": " ekatvaṃ nityam anityaṃ ca /",
    "GSP29antarkau:103": " nityagataṃ nityam anityagatam anityam /",
    "GSP29antarkau:104": " dvitvā3dikaṃ tu sarvatrā7-nityam eva //",
    "GSP29antarkau:105": " AnTs_25 māna-vyavahāra-kāraṇaṃ parimāṇaṃ /",
    "GSP29antarkau:106": " nava-dravya-vṛtti /",
    "GSP29antarkau:107": " tac caturvidham /",
    "GSP29antarkau:108": " aṇu mahad dīrghaṃ hrasvaṃ ce7ti //",
    "GSP29antarkau:109": " AnTs_26 pṛthag-vyavahāra-kāraṇaṃ pṛthagtvaṃ /",
    "GSP29antarkau:110": " sarvadravyavṛtti //",
    "GSP29antarkau:111": " AnTs_27 saṃyukta-vyavahāra-hetuḥ saṃyogaḥ /",
    "GSP29antarkau:112": " sarvadravyavṛttiḥ //",
    "GSP29antarkau:113": " AnTs_28 saṃyoga-nāśako guṇo vibhāgaḥ /",
    "GSP29antarkau:114": " sarvadravyavṛttiḥ //",
    "GSP29antarkau:115": " te dvividhe dik-kṛte kāla-kṛte ca /",
    "GSP29antarkau:116": " dūra-sthe dik-kṛtaṃ paratvam /",
    "GSP29antarkau:117": " samīpasthe dik-kṛtam aparatvam /",
    "GSP29antarkau:118": " jyeṣṭhe",
    "GSP29antarkau:119": " kāla-kṛthaṃ paratvam /",
    "GSP29antarkau:120": " kaniṣṭhe kāla-kṛtam aparatvam //",
    "GSP29antarkau:121": " AnTs_30 ādya-patanā1samavāyi-kāraṇaṃ gurutvaṃ /",
    "GSP29antarkau:122": " pṛthivī-jala-vṛtti //",
    "GSP29antarkau:123": " AnTs_31 ādya-syandanā1samavāyi-kāraṇaṃ dravatvaṃ pṛthivy-ap-tejo-vṛtti /",
    "GSP29antarkau:124": " tad dvividhaṃ sāṃsiddhikaṃ naimittikaṃ ca /",
    "GSP29antarkau:125": " sāṃsiddhikaṃ jale naimittikaṃ",
    "GSP29antarkau:126": " pṛthivī-tejasoḥ /",
    "GSP29antarkau:127": " pṛthivyāṃ ghṛtā3dāv-agni-saṃyogajanyaṃ daravatvam /",
    "GSP29antarkau:128": " tejasi suvarṇādau //",
    "GSP29antarkau:129": " AnTs_32 cūrṇā3di-piṇḍī-bhāva-hetur guṇaḥ snehaḥ /",
    "GSP29antarkau:130": " jala-mātra-vṛttiḥ //",
    "GSP29antarkau:131": " AnTs_33 śrotra-grāhyo guṇaḥ śabdaḥ /",
    "GSP29antarkau:132": " ākāśa-mātra-vṛttiḥ /",
    "GSP29antarkau:133": " sa dvividho dvany-ātmako varṇā3tmakaś ce7ti /",
    "GSP29antarkau:134": " dvany-ātmako bhery-ādau /",
    "GSP29antarkau:135": " AnTs_34 sarva-vyavahāra-hetur buddhir jñānam /",
    "GSP29antarkau:136": " sā dvividhā smṛtir-anubhavaś ca /",
    "GSP29antarkau:137": " saṃskāra-mātra-janyaṃ jñānaṃ smṛtiḥ /",
    "GSP29antarkau:138": " tad-bhinnaṃ",
    "GSP29antarkau:139": " jñānam anubhavaḥ //",
    "GSP29antarkau:140": " AnTs_35 sa dvividho yathārtho 'yathārthaś ca /",
    "GSP29antarkau:141": " tad-vati tat-prakārako",
    "GSP29antarkau:142": " 'nubhavo yathārthaḥ /",
    "GSP29antarkau:143": " yathā rajata idaṃ rajatam iti jñānam /",
    "GSP29antarkau:144": " sa eva prame9ty ucyate /",
    "GSP29antarkau:145": " tad-abhāva-vati tat-prakārako 'nubhavo 'yathārthaḥ /",
    "GSP29antarkau:146": " yathā śuktāv idaṃ rajatam iti jñānam //",
    "GSP29antarkau:147": " AnTs_36 yathārthā1nubhavaś caturvidhaḥ",
    "GSP29antarkau:148": " tat-karaṇam api caturvidhaṃ pratyakṣā1numāno1pamāna-śabda-bhedāt //",
    "GSP29antarkau:149": " AnTs_37 asādhāraṇam kāraṇaṃ karaṇam /",
    "GSP29antarkau:150": " AnTs_38 kārya-niyata-pūrva-vṛtti kāraṇam /",
    "GSP29antarkau:151": " AnTs_39 kāryaṃ prāg-abhāva-pratiyogi /",
    "GSP29antarkau:152": " AnTs_40 kāraṇaṃ trividhaṃ samavāyy-asamavāyi-nimitta-bhedāt /",
    "GSP29antarkau:153": " yat samavetaṃ kāryam utpadyate tat samavāyikāraṇam /",
    "GSP29antarkau:154": " yathā tantavaḥ paṭasya paṭaś ca khagata-rūpādeḥ /",
    "GSP29antarkau:155": " kāryeṇa kāraṇena vā sahai7kasminn arthe samavetatve sati yat kāraṇaṃ tad asamavāyikāraṇam",
    "GSP29antarkau:156": " yathā tantusaṃyogaḥ paṭasya tanturūpaṃ paṭarūpasya /",
    "GSP29antarkau:157": " tad-ubhaya-bhinnaṃ kāraṇaṃ nimittakāraṇam /",
    "GSP29antarkau:158": " yathā turī-vemā3dikaṃ paṭasya /",
    "GSP29antarkau:159": " AnTs_41 tad etat-trividha-kāraṇa-madhye yad asādhāraṇaṃ kāraṇaṃ tad eva karaṇam //",
    "GSP29antarkau:160": " AnTs_42 tatra pratyakṣa-jñāna-karaṇaṃ pratyakṣam /",
    "GSP29antarkau:161": " indriyā1rtha-sannikarṣa-janyaṃ jñānaṃ pratyakṣam /tad dvividhaṃ nirvikalpakaṃ savikalpakaṃ tatra nirvikalpakaṃ jñānaṃ nirvikalpakaṃ yathe9daṃ kiṃcit /",
    "GSP29antarkau:162": " saparakārakaṃ jñānaṃ savikalpakaṃ yathā",
    "GSP29antarkau:163": " ḍittho'yam brahmaṇo'yaṃ śyāmo'yam iti //",
    "GSP29antarkau:164": " AnTs_43 pratyakṣa-jñāna-hetur indriyā1rtha-sannikarṣaḥ ṣaḍvidhaḥ /",
    "GSP29antarkau:165": " saṃyogaḥ saṃyukta-samavāyaḥ saṃyukta-samaveta-samavāyaḥ samavāyaḥ",
    "GSP29antarkau:166": " samaveta-samavāyo viśeṣaṇa-viśeṣya-bhāvaś ce7ti /",
    "GSP29antarkau:167": " cakṣuṣā",
    "GSP29antarkau:168": " ghaṭa-pratyakṣa-janane saṃyogaḥ sannikarṣaḥ /",
    "GSP29antarkau:169": " ghaṭa-rūpa-pratyakṣa-janane",
    "GSP29antarkau:170": " saṃyukta-samavāyaḥ sannikarṣaḥ cakṣuḥsaṃyukte ghaṭe rūpasya samavāyāt /",
    "GSP29antarkau:171": " rūpatva-sāmānyapratyakṣe saṃyukta-samaveta-samavāyaḥ sannikarṣaḥ",
    "GSP29antarkau:172": " cakṣuḥ-saṃyukte ghaṭe rūpaṃ samavetaṃ tatra rūpatvasya samavāyāt /",
    "GSP29antarkau:173": " śrotreṇa śabdasyā8kāśa-guṇatvāt guṇa-guṇinoś ca samavāyāt /",
    "GSP29antarkau:174": " śabdatva-sākṣātkāre samaveta-samavāyaḥ sannikarṣaḥ śrotra-samavete śabde śabdatvasya samavāyāt",
    "GSP29antarkau:175": " abhāvaparatyakṣe viśeṣaṇa-viśeṣya-bhāvaḥ",
    "GSP29antarkau:176": " sannikarṣo ghaṭā1bhāvavad bhṛtalam ity atra cakṣuḥ-saṃyukte bhūtale",
    "GSP29antarkau:177": " ghaṭā1bhāvasya viśeṣaṇatvāt /",
    "GSP29antarkau:178": " evaṃ snnikarṣa-ṣaṭka-janyaṃ jñānaṃ pratyakṣam /",
    "GSP29antarkau:179": " tatkaraṇam indriyam /",
    "GSP29antarkau:180": " tasmād indriyaṃ pratyakṣa-pramāṇam iti siddham //",
    "GSP29antarkau:181": " AnTs_44 anumiti-karaṇam anumānam /",
    "GSP29antarkau:182": " parāmarśajanyaṃ jñānam anumitiḥ /",
    "GSP29antarkau:183": " vyāpti-viśiṣṭa-pakṣa-dharmatā-jñānaṃ parāmarśaḥ /",
    "GSP29antarkau:184": " yathā",
    "GSP29antarkau:185": " vahni-vyāpya-dhūmavān ayaṃ parvata iti jñānaṃ parāmarśaḥ /",
    "GSP29antarkau:186": " taj janyaṃ parvato vahnimān iti jñānam anumitiḥ /",
    "GSP29antarkau:187": " yatra yatra dhūmas tatrā7gnir iti",
    "GSP29antarkau:188": " sāhacarya-niyamo vyāptiḥ /",
    "GSP29antarkau:189": " vyāpyasya parvatā3di-vṛttitvaṃ pakṣadharmatā //",
    "GSP29antarkau:190": " AnTs_45 anumānaṃ dvividhaṃ svā1rthaṃ parā1rthaṃ ca /",
    "GSP29antarkau:191": " tatra svā1rthaṃ",
    "GSP29antarkau:192": " tathā hi svayam eva bhūyo darśanena yatra dhūmas tatra agnir iti mahānasādau vyāptiṃ gṛhītvā parvatasamīpaṃ gatas tad-gate",
    "GSP29antarkau:193": " cā7gnau sandihānaḥ parvate dhūmaṃ paśyan vyāptiṃ smarati yatra dhūmas tatrā7gnir iti /",
    "GSP29antarkau:194": " tad-antaraṃ vahni-vyāpya-dhūmavān ayaṃ parvata iti jñānam utpadyate /",
    "GSP29antarkau:195": " ayam eva liṅgaparāmarṣaṃ ity ucyate /",
    "GSP29antarkau:196": " tasmāt parvato vahnimān iti jñānam anumitir utpadyate /",
    "GSP29antarkau:197": " tad etat svā1rthā1numānam /",
    "GSP29antarkau:198": " yat tu svayaṃ dhūmād agnim anumāya para-pratipatty-arthaṃ pañcā1vayava-vākyaṃ prayukte tat yathā parvato vahnimān dhūmavattvāt /",
    "GSP29antarkau:199": " yo yo dhūmavān sa vahnimān yathā mahānasaḥ /",
    "GSP29antarkau:201": " tasmāt tathe9ti /",
    "GSP29antarkau:202": " anena pratipāditāl lingāt paro'py agniṃ pratipadyate //",
    "GSP29antarkau:203": " parvato vahnimān iti pratijñā /",
    "GSP29antarkau:204": " dhūmavattvād iti hetuḥ /",
    "GSP29antarkau:205": " yo yo dhūmavān sa",
    "GSP29antarkau:206": " so'gnimān yathā mahānasa ity udāharaṇam /",
    "GSP29antarkau:207": " tathā cā7yam iti upanayaḥ /",
    "GSP29antarkau:208": " tasmāt tathe9ti nigamanam //",
    "GSP29antarkau:209": " AnTs_47 svā1rthā1numiti-parā1rthā1numityor liṅga-parāmarśa eva karaṇam /",
    "GSP29antarkau:210": " tasmāl liṅga-parāmarśā'numānam //",
    "GSP29antarkau:211": " AnTs_48 liṅgaṃ trividham /",
    "GSP29antarkau:212": " anvaya-vyatireki kevalā1nvayi kevala-vyatireki",
    "GSP29antarkau:213": " anvayena vyatirekeṇa ca vyāptimad anvaya-vyatireki /",
    "GSP29antarkau:214": " yathā vahnau sādhye dhūmavattvam /",
    "GSP29antarkau:215": " yatra dhūmas tatrā7gnir yathā mahānasa ity",
    "GSP29antarkau:216": " anvaya-vyāptiḥ /",
    "GSP29antarkau:217": " yatra vahnir nā7sti tatra dhūmo'pi nā7sti yathā mahāhrada iti vyatireka-vyāptiḥ /",
    "GSP29antarkau:218": " anvaya-mātra-vyāptikaṃ kevalā1nvayi yathā",
    "GSP29antarkau:219": " ghaṭo'bhidheyaḥ prameyatvāt paṭavat /",
    "GSP29antarkau:220": " atra prameyatvā1bhidhyatvayor",
    "GSP29antarkau:221": " vyatirekavyāptir nā7sti sarvasyā7pi prameyatvād abhidheyatvāc ca /",
    "GSP29antarkau:222": " vyatirekamātravyāptikaṃ kevala-vyatireki yathā pṛthivī tarebhyo bhidyate gandhavattvāt /",
    "GSP29antarkau:223": " yad itarebhyo na bhidyate na tad gandhavat /",
    "GSP29antarkau:224": " yathā jalam /",
    "GSP29antarkau:225": " na ce7yaṃ tathā /",
    "GSP29antarkau:226": " tasmān na tathe9ti atra yad gandhavat tad itara-bhinnam",
    "GSP29antarkau:227": " ity anvayadṛṣṭanto nā7sti pṛthivī-mātrasya pakṣatvāt //",
    "GSP29antarkau:228": " AnTs_49 saṃdigdha-sādhyavān pakṣaḥ /",
    "GSP29antarkau:229": " yathā dhūmavattve hetau parvataḥ //",
    "GSP29antarkau:230": " AnTs_50 niścita-sādhyavān sapakṣaḥ /",
    "GSP29antarkau:231": " yathā tatrai7va mahānasaḥ //",
    "GSP29antarkau:232": " AnTs_51 niścita-sādhyā1bhāvavān vipakṣaḥ /",
    "GSP29antarkau:233": " yathā tatrai7va mahāhradaḥ //",
    "GSP29antarkau:234": " AnTs_52 savyabhicāra-viruddha-satpakṣā1siddha-bādhitāḥ pañca hetvābhāsāḥ //",
    "GSP29antarkau:235": " AnTs_53 savyabhicāro'naikāntikaḥ /",
    "GSP29antarkau:236": " sa trividhaḥ /",
    "GSP29antarkau:237": " tatra sādhyā1bhāvavad-vṛttiḥ",
    "GSP29antarkau:238": " sādhāraṇo anaikāntikaḥ /",
    "GSP29antarkau:239": " yathā parvato vahnimān prameyatvād iti prameyatvasya vahny-abhāvavati hrade vidyamānatvāt",
    "GSP29antarkau:240": " sarva-sapakṣa-vipakṣa-vyāvṛtto'sādhāraṇaḥ /",
    "GSP29antarkau:241": " yathā śabdo nityaḥ śabdatvād iti /",
    "GSP29antarkau:242": " śabdatvaṃ sarvebhyo nityebhyo'ntyebhyaś ca vyāvṛttaṃ śabda-mātra-vṛtti /",
    "GSP29antarkau:243": " anvaya-vyatireka-dṛṣṭānta-rahito'nupasaṃhārī /",
    "GSP29antarkau:244": " yathā sarvam anityaṃ prameyatvād iti /",
    "GSP29antarkau:245": " atra sarvasyā7pi pakṣatvād dṛṣṭānto nā7sti //",
    "GSP29antarkau:246": " AnTs_54 sādhyā1bhāva-vyāpto hetur viruddhaḥ /",
    "GSP29antarkau:247": " yatra śabdo nityaḥ kṛtakatvād iti /",
    "GSP29antarkau:248": " kṛtakatvaṃ hi nityatvā1bhāvenā7-nityatvena vyāptam //",
    "GSP29antarkau:249": " AnTs_55 yasya sādhya-bhāva-sādhakaṃ hetv-antaraṃ vidyate sa satpratipakṣaḥ",
    "GSP29antarkau:250": " / yathā śabdo nityaḥ śrāvaṇatvāc chabdatvavad iti /",
    "GSP29antarkau:251": " śabdo'nityaḥ kāryatvād ghaṭavad iti //",
    "GSP29antarkau:252": " AnTs_56 asiddhas trividhaḥ /",
    "GSP29antarkau:253": " āśrayā1siddho yathā gaganā1ravindaṃ surabhy aravindatvāt saro-jā1ravindavat /",
    "GSP29antarkau:254": " atra gaganā1ravindam āśrayaḥ /",
    "GSP29antarkau:255": " sa ca nā7sty eva /",
    "GSP29antarkau:256": " svarūpā1siddho yathā śabdo guṇaś cākṣuṣatvāt /",
    "GSP29antarkau:257": " atra cākṣuṣatvaṃ śabde nā7sti śabdasya śrāvaṇatvāt /",
    "GSP29antarkau:258": " sādhya-vyāpakatve sati sādhanā1vyāpaka upādhiḥ /",
    "GSP29antarkau:259": " sādhya-samānā1dhikaraṇā1tyantā1bhāvā1pratiyogitvaṃ sādhya-vyāpakatvam /",
    "GSP29antarkau:260": " sādhanavann iṣṭā1tyantā1bhāva-pratiyogitvaṃ sādhāraṇā1vyapakatvam /",
    "GSP29antarkau:261": " parvato dhūmavān vahnimattvād ity atrā8rdre1ndhasaṃyoga upādhiḥ /",
    "GSP29antarkau:262": " tathā hi /",
    "GSP29antarkau:263": " yatra dhūmas tatrā8rdre1ndhna-saṃyoga iti sādhya-vyāpakatā /",
    "GSP29antarkau:264": " yatra vahnis tatrā8rdre1ndhana-saṃyogā1bhāvād iti sādhanā1vyāpakatā /",
    "GSP29antarkau:265": " evaṃ",
    "GSP29antarkau:266": " sādhya-vyāpakatve sati sādhanā1vyāpakatvād ardre1ndhana-saṃyoga upādhiḥ /",
    "GSP29antarkau:267": " so1pādhikatvād vahnimattvaṃ vyāpyatvā1siddham //",
    "GSP29antarkau:268": " AnTs_57 yasya sādhyā1bhāvaḥ pramāṇā1ntareṇa niścitaḥ sa bādhitaḥ /",
    "GSP29antarkau:269": " yathā vahnir anuṣṇo dravyatvād iti /",
    "GSP29antarkau:270": " atrā7nuṣṇatvaṃ sādhyaṃ tad-abhāva uṣṇatvaṃ",
    "GSP29antarkau:271": " spārśana-pratyakṣeṇa gṛhyata iti bādhitatvam //",
    "GSP29antarkau:272": " AnTs_58 upamiti-karaṇam upamānam /",
    "GSP29antarkau:273": " saṃjñā-saṃjñi-saṃbandha-jñānam upamitiḥ",
    "GSP29antarkau:274": " / tat-karaṇaṃ sādṛśya-jñānam /",
    "GSP29antarkau:275": " avāntara-vyāpāraḥ /",
    "GSP29antarkau:276": " tathā hi kaś cid gavaya-śabdā1rtham ajānan kutaś cid",
    "GSP29antarkau:277": " āraṇyaka-puruṣād go-sadṛśo gavaya iti śrutvā vanaṃ gato vākyā1rthaṃ smaran",
    "GSP29antarkau:278": " go-sadṛṣaṃ piṇḍaṃ paśyati /",
    "GSP29antarkau:279": " tad-anantaram asau gavaya-śabda-vācya ity upamitir utpadyte //",
    "GSP29antarkau:280": " AnTs_59 āpta-vākyaṃ śabdaḥ /",
    "GSP29antarkau:281": " āptas tu yathā9rtha-vaktā /",
    "GSP29antarkau:282": " vākyaṃ",
    "GSP29antarkau:283": " pada-samūhaḥ /",
    "GSP29antarkau:284": " yathā gām ānaye7ti /",
    "GSP29antarkau:285": " śaktaṃ padam /",
    "GSP29antarkau:286": " asmāt padād ayam artho boddhavya itī8śvara-saṃketaḥ śaktiḥ //",
    "GSP29antarkau:287": " AnTs_60 ākāṅkṣā yogyatā saṃnidhiś ca vākyā1rtha-jñāna-hetuḥ /",
    "GSP29antarkau:288": " padasya",
    "GSP29antarkau:289": " padā1ntara-vyatireka-prayuktā1nvayā1nanubhāvakatvam ākāṅkṣā /",
    "GSP29antarkau:290": " yogyatā /",
    "GSP29antarkau:291": " padānām-avilambeno7ccāraṇaṃ saṃnidhiḥ //",
    "GSP29antarkau:292": " AnTs_61 ākāṅkṣā4di-rahitaṃ vākyam apramāṇam /",
    "GSP29antarkau:293": " yathā gaur aśvaḥ puruṣo",
    "GSP29antarkau:294": " hastī9ti na pramāṇam ākāṅkṣā-virahāt /",
    "GSP29antarkau:295": " agninā siñced iti na pramāṇaṃ yogyatā-virahāt /",
    "GSP29antarkau:296": " prahare prehare-'saho7ccāritāni gām ānaye7ty-ādi-padāni",
    "GSP29antarkau:297": " na pramāṇaṃ saṃnidhy-abhāvāt //",
    "GSP29antarkau:298": " AnTs_62 vākyaṃ dvividham /",
    "GSP29antarkau:299": " vaidikaṃ laukikaṃ ca /",
    "GSP29antarkau:300": " vaidikam īśvaro1ktatvāt",
    "GSP29antarkau:301": " sarvam eva pramāṇam /",
    "GSP29antarkau:302": " laukikaṃ tv āpto1ktaṃ pramāṇam /",
    "GSP29antarkau:303": " anyad apramāṇam //",
    "GSP29antarkau:304": " AnTs_63 vākyā1rtha-jñānaṃ śabda-jñānam /",
    "GSP29antarkau:305": " tat-karaṇaṃ śabdaḥ //",
    "GSP29antarkau:306": " AnTs_64 ayathā2rthā1nubhavas trividhaḥ saṃśaya-viparyaya-tarka-bhedāt /",
    "GSP29antarkau:307": " ekasmin dharmini viruddha-nānā-dharma-vaiśiṣṭyā1vagāhi jñānaṃ saṃśayaḥ /",
    "GSP29antarkau:308": " yathā sthāṇur vā purṣo ve9ti /",
    "GSP29antarkau:309": " mithyā-jñānaṃ viparyayaḥ /",
    "GSP29antarkau:310": " yathā śuktāv idaṃ rajatam iti /",
    "GSP29antarkau:311": " vyāpyā8ropeṇa vyāpakā3ropas tarkaḥ yathā yadi vahnir na syāt tarhi dhūmo'pi na syād iti",
    "GSP29antarkau:312": " AnTs_65 smṛtir api dvividhā /",
    "GSP29antarkau:313": " yathā2rthā1yathā2rthā ca pramā-janyā",
    "GSP29antarkau:314": " AnTs_66 sarveśām anukūlatayā vedanīyaṃ sukham //",
    "GSP29antarkau:315": " AnTs_67 sarveśāṃ pratikūlatayā vedanīyaṃ duḥkham //",
    "GSP29antarkau:316": " AnTs_68 icchā kāmaḥ //",
    "GSP29antarkau:317": " AnTs_69 krodho dveṣaḥ //",
    "GSP29antarkau:318": " AnTs_70 kṛtiḥ prayatnaḥ //",
    "GSP29antarkau:319": " AnTs_71 vihita-karma-janyo dharmaḥ //",
    "GSP29antarkau:320": " AnTs_72 niṣiddha-karma-janyas tv adharmaḥ //",
    "GSP29antarkau:321": " AnTs_73 buddhy-ādayo'ṣṭāv ātma-mātraviśeṣa-guṇāḥ //",
    "GSP29antarkau:322": " AnTs_74 buddhī1cchā-prayatnā dvividhāḥ /",
    "GSP29antarkau:323": " nityā anityāś ca /",
    "GSP29antarkau:324": " nityā īśvarasya /",
    "GSP29antarkau:325": " anityā jīvasya //",
    "GSP29antarkau:326": " AnTs_75 saṃskāras trividhaḥ /",
    "GSP29antarkau:327": " vego bhāvanā sthiti-sthāpakaś ce7ti /",
    "GSP29antarkau:328": " vegaḥ",
    "GSP29antarkau:329": " pṛthivy-ādi-catuṣṭaya-mano-vṛttiḥ /",
    "GSP29antarkau:330": " anubhava-janyā smṛti-hetur",
    "GSP29antarkau:331": " anyathā kṛtasya punas tad-avasthā-pādakaḥ",
    "GSP29antarkau:332": " sthiti-sthāpakaḥ kaṭā3di-pṛthivī-vṛttiḥ //",
    "GSP29antarkau:333": " AnTs_76 calanā3tmakaṃ karma /",
    "GSP29antarkau:334": " ūrdhva-deśa-saṃyoga-hetur utkṣepaṇam /",
    "GSP29antarkau:335": " adho-deśa-saṃyoga-hetur apakṣepaṇam /",
    "GSP29antarkau:336": " śarīra-saṃnikṛṣṭa-saṃyoga-hetur",
    "GSP29antarkau:337": " ākuñcanam /",
    "GSP29antarkau:338": " viprakṛṣṭa-saṃyoga-hetuḥ prasāraṇam /",
    "GSP29antarkau:339": " anyat sarvaṃ gamanam /",
    "GSP29antarkau:340": " pṛthivy-ādi-catuṣṭaya-mano-mātra-vṛtti //",
    "GSP29antarkau:341": " AnTs_77 nityam ekam anekā1nugataṃ sāmānyam /",
    "GSP29antarkau:342": " dravya-guṇa-karma-vṛtti /",
    "GSP29antarkau:343": " tad dvividhaṃ parā1para-bhedāt /",
    "GSP29antarkau:344": " paraṃ sattā /",
    "GSP29antarkau:345": " aparaṃ dravyatvā3diḥ //",
    "GSP29antarkau:346": " AnTs_78 nitya-dravya-vṛttayo vyāvartakā viśeṣāḥ //",
    "GSP29antarkau:347": " AnTs_79 nitya-saṃbandhaḥ samavāyaḥ /",
    "GSP29antarkau:348": " ayuta-siddha-vṛttiḥ /",
    "GSP29antarkau:349": " yayor dvayor madhya ekam avinaśyad-aparā3śritam evā7vatiṣṭate tāv ayutasiddau /",
    "GSP29antarkau:350": " yathā9vayavā1vayavinau guṇa-guṇinau kriyā-kriyāvantau jāti-vyaktī",
    "GSP29antarkau:351": " viśeṣa-nitya-dravye ce7ti //",
    "GSP29antarkau:352": " AnTs_80 an-ādiḥ sā1ntaḥ prāg-abhāvaḥ /",
    "GSP29antarkau:353": " utpatteḥ pūrvaṃ kāryasya /",
    "GSP29antarkau:354": " an-antaḥ pradhvaṃsaḥ /",
    "GSP29antarkau:355": " utpatty-an-antaraṃ kāryasya /",
    "GSP29antarkau:356": " yathā bhūtale ghaṭo nā7stī7ti /",
    "GSP29antarkau:357": " yathā ghaṭaḥ paṭo na bhavatī7ti //",
    "GSP29antarkau:358": " AnTs_81 sarveṣāṃ padā1rthānāṃ yathā-yatham ukteṣv antarbhāvāt saptai7va",
    "GSP29antarkau:359": " padā1rthā iti siddham //",
    "GSP29antarkau:360": " kāṇāda-nyāya-matayor bālā-vyutpatti-siddhaye /",
    "GSP29antarkau:361": " annaṃbhaṭṭena viduṣā racitas tarkasaṃgrahaḥ //"
}