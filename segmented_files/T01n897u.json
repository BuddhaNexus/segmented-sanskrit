{
    "T01n897u_001:0": " vāgīśvaravarṇanāstotram",
    "T01n897u_001:1": " brahmādidevamanujāsurapūjitāṅgaṃ",
    "T01n897u_001:2": " jñānādisarvanilayaṃ guṇarājavantam |",
    "T01n897u_001:3": " jñānādhipaṃ guṇamayaṃ guṇavṛddhimantaṃ",
    "T01n897u_001:4": " vāgīśvaraṃ suraguruṃ satataṃ namāmi || 1 ||",
    "T01n897u_001:5": " vedādiśāstraracanākhilamantravijñaṃ",
    "T01n897u_001:6": " stotraiḥ kathādibhiraśeṣasugītaśabdam |",
    "T01n897u_001:7": " yaṃ kīryayanti munayo naravāṇyabhijñaṃ",
    "T01n897u_001:8": " vāgīśvaraṃ suraguruṃ satataṃ namāmi || 2 ||",
    "T01n897u_001:9": " jñānapradhānakaśarīramacintyabuddhiṃ",
    "T01n897u_001:10": " vācaspatiṃ divi bhuvi stutakīrtidaṃ tam |",
    "T01n897u_001:11": " jñāneśvaraṃ tribhuvaneśacarācaraṃ taṃ",
    "T01n897u_001:12": " vāgīśvaraṃ suraguruṃ satataṃ namāmi || 3 ||",
    "T01n897u_001:13": " ratnādiśilpaparikalpanapūrṇajñeyaṃ",
    "T01n897u_001:14": " citraṃ vicitritaguṇaṃ janamānanīyam |",
    "T01n897u_001:15": " sarvajñakārakadharād hṛdi brahmapuñjaṃ",
    "T01n897u_001:16": " vāgīśvaraṃ suraguruṃ satataṃ namāmi || 4 ||",
    "T01n897u_001:17": " caryāvidhau ratipatiṃ sthiradhairyabodhiṃ",
    "T01n897u_001:18": " dhyānādhipaṃ vividhinā sukharañjanīyam |",
    "T01n897u_001:19": " vidyāpratiṣṭhitaguruṃ bhuvanetipūrṇaṃ",
    "T01n897u_001:20": " vāgīśvaraṃ suraguruṃ satataṃ namāmi || 5 ||",
    "T01n897u_001:21": " vāṇīpatiṃ bhujacatuṣṭayabāṇacāpaṃ",
    "T01n897u_001:22": " pustaṃ kṛpāṇamaniśaṃ kramaśo dadhānam |",
    "T01n897u_001:23": " sarveṣṭadaṃ jagati yaccaṇāravindaṃ",
    "T01n897u_001:24": " vāgīśvaraṃ suraguruṃ satataṃ namāmi || 6 ||",
    "T01n897u_001:25": " śrīvāgīśvaravarṇanāstotraṃ samāptam |"
}