{
    "T01n829u_001:0": " saptavidhānuttarastotram",
    "T01n829u_001:1": " namaḥ śrīlokanāthāya",
    "T01n829u_001:2": " amitaruciśiraḥsthaṃ padmapatrāyatākṣaṃ",
    "T01n829u_001:3": " duritakṛtavināśaṃ mohamāyā'hitākṣam |",
    "T01n829u_001:4": " karadhṛtakanakābjaṃ lokapālaikadakṣaṃ",
    "T01n829u_001:5": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 1 ||",
    "T01n829u_001:6": " aruṇavaruṇaśobhābodhivijñānahetuṃ",
    "T01n829u_001:7": " maṇimayamukuṭaṃ saṃsārakāmbhodhisetum |",
    "T01n829u_001:8": " kṛtamanasijanāśaṃ bodhisattvādiketuṃ",
    "T01n829u_001:9": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 2 ||",
    "T01n829u_001:10": " sarasijakṛtavāsaṃ dūrakandarpapāśaṃ",
    "T01n829u_001:11": " suranaradanujāśaṃ pūrṇacandraprakāśam |",
    "T01n829u_001:12": " sukṛtajanavilāsaṃ koṭisūryāvabhāsaṃ",
    "T01n829u_001:13": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 3 ||",
    "T01n829u_001:14": " urasi vihitanāgaṃ mocitāśeṣarāgaṃ",
    "T01n829u_001:15": " bhavajaladhivibhāgaṃ mokṣasaukhyaikamārgam |",
    "T01n829u_001:16": " pratapitakanakāṅgaṃ bodhibodhaikarāgaṃ",
    "T01n829u_001:17": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 4 ||",
    "T01n829u_001:18": " jinavarasutarājaṃ muktimālāvirājaṃ",
    "T01n829u_001:19": " sulalitaphaṇirājaṃ kīrṇitācābhirājam |",
    "T01n829u_001:20": " pramuditavalirājaṃ pālitāmartyarājaṃ",
    "T01n829u_001:21": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 5 ||",
    "T01n829u_001:22": " dhṛtamadhukararūpaṃ kṣuptipāsāntakūpaṃ",
    "T01n829u_001:23": " sucaritahayarūpaṃ siṃhalādantabhūpam |",
    "T01n829u_001:24": " agurusurabhidhūpaṃ dhāritāśeṣarūpaṃ",
    "T01n829u_001:25": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 6 ||",
    "T01n829u_001:26": " trividhavidhṛtapāpaṃ me harantaṃ tritāpaṃ",
    "T01n829u_001:27": " pratidinamitijāpaṃ devacakre vilāpam |",
    "T01n829u_001:28": " nirayabhayakalāpaṃ bhedakājñānacāpaṃ",
    "T01n829u_001:29": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 7 ||",
    "T01n829u_001:30": " vihitaśubhajanānāṃ muktitāpodyatānāṃ",
    "T01n829u_001:31": " vigalitakaluṣāṇāṃ bhaktipūjāratānām |",
    "T01n829u_001:32": " varakuśalasamūhaṃ saṃprapede svabhaktyā",
    "T01n829u_001:33": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 8 ||",
    "T01n829u_001:34": " daśabalakṛtasiddhiṃ saṃvadhe buddhabodhiṃ",
    "T01n829u_001:35": " jananamaraṇabhītaṃ mārapāśābhinītam |",
    "T01n829u_001:36": " manasi malinavṛtte pūritājñānavitte",
    "T01n829u_001:37": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 9 ||",
    "T01n829u_001:38": " pravarasugataratnaṃ dharmakāyaṃ saṃsaṃghaṃ",
    "T01n829u_001:39": " kṣaraṇamiha prayāmi kāritānaṅgabhaṅgam |",
    "T01n829u_001:40": " pramuditamanasāptaṃ bodhilābhaikayatnaṃ",
    "T01n829u_001:41": " praṇamitaśirasā'haṃ naumi taṃ lokanātham || 10 ||",
    "T01n829u_001:42": " śrīlokanāthacaraṇāmbujabhaktipadma-",
    "T01n829u_001:43": " pīyūṣanandaracitānativistarāḍhyam |",
    "T01n829u_001:44": " yatsaptadhārcanayutaṃ sarasātihṛdyaṃ",
    "T01n829u_001:45": " saukhyāvatīgamanamārgakaraṃ tu sadyaḥ || 11 ||",
    "T01n829u_001:46": " stutvā lokaguruṃ munīśvaramimaṃ sarvārthasiddhipradaṃ",
    "T01n829u_001:47": " yatpuṇyaṃ samupārjitaṃ vrajatu tatpuṇyena saukhyāvatīm |",
    "T01n829u_001:48": " śrīlokeśvarapādapaṅkajarasollāsaikaharṣoditaṃ",
    "T01n829u_001:49": " lokakleśaharaṃ sudurjayavaraṃ jitvā pramuktāmalam || 12 ||",
    "T01n829u_001:50": " śrīmadāryāvalokiteśvarasya saptavidhānuttararastotraṃ samāptam |"
}