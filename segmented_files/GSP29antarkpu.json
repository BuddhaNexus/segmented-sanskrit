{
    "GSP29antarkpu:0": "AnTs_1ab nidhāya hṛdi viśveśaṃ vidhāya guruvandanam /",
    "GSP29antarkpu:1": "AnTs_1cd bālānāṃ sukhabodhāya kriyate tarkasaṃgrahaḥ //",
    "GSP29antarkpu:2": "AnTs_2 dravyaguṇakarmasāmānyaviśeṣasamavāyābhāvāḥ sapta padārthāḥ //",
    "GSP29antarkpu:3": "AnTs_3 tatra dravyāṇi pṛthivyaptejovāyvākāśakāladigātmamanāṃsi navaiva //",
    "GSP29antarkpu:4": "AnTs_4",
    "GSP29antarkpu:5": "rūparasagandhasparśasaṃkhyāparimāṇapṛthaktvasaṃyogavibhāgaparatvāparatvagurutvadravatvasnehaśabdabuddhisukhaduḥkhecchādveṣaprayatnadharmādharmasaṃskārāś caturviṃśati guṇāḥ //",
    "GSP29antarkpu:6": "AnTs_5 utkṣepaṇāvakṣepaṇākuñcanaprasāraṇagamanāni pañca karmāṇi //",
    "GSP29antarkpu:7": "AnTs_6 param aparaṃ ceti dvividhaṃ sāmānyam //",
    "GSP29antarkpu:8": "AnTs_7 nityadravyavṛttayo viśeṣās tv anantā eva //",
    "GSP29antarkpu:9": "AnTs_8 samavāyas tv eka eva //",
    "GSP29antarkpu:10": "AnTs_9 abhāvaś caturvidhaḥ /",
    "GSP29antarkpu:11": "prāgabhāvaḥ pradhvaṃsābhāvo 'tyantābhāvo",
    "GSP29antarkpu:12": "'nyonyābhāvaś ceti //",
    "GSP29antarkpu:13": "AnTs_10 gandhavatī pṛthivī /",
    "GSP29antarkpu:14": "sā dvividhā nityānityā ca /",
    "GSP29antarkpu:15": "nityā paramāṇurūpā /",
    "GSP29antarkpu:16": "anityā kāryarūpā /",
    "GSP29antarkpu:17": "punas trividhā śarīrendriyaviṣayabhedāt",
    "GSP29antarkpu:18": "/ śarīram asmadādīnām /",
    "GSP29antarkpu:19": "indriyaṃ gandhagrāhakaṃ ghrāṇaṃ nāsāgravarti /",
    "GSP29antarkpu:20": "viṣayo mṛtpāṣāṇādiḥ //",
    "GSP29antarkpu:21": "AnTs_11 śitasparśavatya āpaḥ /",
    "GSP29antarkpu:22": "tā dvividhāḥ nityā anityāś ca /",
    "GSP29antarkpu:23": "ntyāḥ paramāṇurūpāḥ /",
    "GSP29antarkpu:24": "anityāḥ kāryarūpāḥ. /",
    "GSP29antarkpu:25": "punas trividhāḥ.",
    "GSP29antarkpu:26": "śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:27": "śarīraṃ varuṇaloke /",
    "GSP29antarkpu:28": "indriyaṃ rasagrāhakaṃ rasanaṃ jihvāgravarti /",
    "GSP29antarkpu:29": "viṣayaḥ saritsamudrādiḥ //",
    "GSP29antarkpu:30": "AnTs_12 uṣṇasparśavat tejaḥ /",
    "GSP29antarkpu:31": "tad dvividhaṃ nityam anityaṃ ca /",
    "GSP29antarkpu:32": "niyyaṃ paramāṇurūpam /",
    "GSP29antarkpu:33": "anityaṃ kāryarūpam /",
    "GSP29antarkpu:34": "punas trividhaṃ śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:35": "śarīram ādityaloke /",
    "GSP29antarkpu:36": "indriyaṃ rūpagrāhakaṃ cakṣuḥ kṛṣṇatārāgravarti /",
    "GSP29antarkpu:37": "viṣayaś caturvidhaḥ /",
    "GSP29antarkpu:38": "bhaumadivyaudaryākarajabhedāt /",
    "GSP29antarkpu:39": "bhaumaṃ vahnyādikam /",
    "GSP29antarkpu:40": "abindhanaṃ divyaṃ vidyudādi /",
    "GSP29antarkpu:41": "bhuktasya pariṇāmahetur audaryam. ākarajaṃ suvarṇādi //",
    "GSP29antarkpu:42": "AnTs_13 rūparahitasparśavān vāyuḥ /",
    "GSP29antarkpu:43": "sa dvividho nityo 'nityaś ca /",
    "GSP29antarkpu:44": "nityaḥ paramāṇurūpaḥ /",
    "GSP29antarkpu:45": "anityaḥ kāryarūpaḥ /",
    "GSP29antarkpu:46": "punas trividhaḥ śarīrendriyaviṣayabhedāt /",
    "GSP29antarkpu:47": "śarīraṃ vāyuloke /",
    "GSP29antarkpu:48": "indriyaṃ sparśagrāhakaṃ tvaksarvaśarīravarti /",
    "GSP29antarkpu:49": "viṣayo vṛkṣādikampanahetuḥ // śarīrāntaḥsaṃcārī vāyuḥ prāṇaḥ /",
    "GSP29antarkpu:50": "sa caiko 'py upādhibhedāt prāṇāpānādisaṃjñā labhate //",
    "GSP29antarkpu:51": "AnTs_14 śabdaguṇam ākāśam /",
    "GSP29antarkpu:52": "tac caikaṃ vibhu nityaṃ ca //",
    "GSP29antarkpu:53": "AnTs_15 atītādivyavahārahetuḥ kālaḥ /",
    "GSP29antarkpu:54": "sa caiko vibhur nityaś ca //",
    "GSP29antarkpu:55": "AnTs_16 prācyādivyavahārahetur dik /",
    "GSP29antarkpu:56": "sā caikā vibhvī nityā ca //",
    "GSP29antarkpu:57": "AnTs_17 jñānādhikaraṇam ātmā /",
    "GSP29antarkpu:58": "sa dvividhaḥ paramātmā jīvātmā ca /",
    "GSP29antarkpu:59": "tatreśvaraḥ sarvajñaḥ paramātmaika eva /",
    "GSP29antarkpu:60": "jīvātmā pratiśarīraṃ bhinno vibhur nityaś ca //",
    "GSP29antarkpu:61": "AnTs_18 sukhādyupalabdhisādhanam indriyaṃ manaḥ /",
    "GSP29antarkpu:62": "tac ca pratyātmaniyatatvād anantaṃ paramāṇurūpaṃ nityaṃ ca //",
    "GSP29antarkpu:63": "AnTs_19 cakṣurmātragrāhyo guṇo rūpam /",
    "GSP29antarkpu:64": "tac ca śuklanīlapītaraktaharitakapiśacitrabhedāt saptavidhaṃ pṛthivījalatejovṛtti",
    "GSP29antarkpu:65": "/ tatra pṛthivyāṃ saptavidham /",
    "GSP29antarkpu:66": "abhāsvaraśuklaṃ jale /",
    "GSP29antarkpu:67": "bhāsvaraśuklaṃ tejasi //",
    "GSP29antarkpu:68": "AnTs_20 rasanagrāhyo guṇo rasaḥ /",
    "GSP29antarkpu:69": "sa ca madhurāmlalavaṇakaṭukaṣāyatiktabhedāt ṣaḍvidhaḥ /",
    "GSP29antarkpu:70": "pṛthivyāṃ ṣaḍvidhaḥ /",
    "GSP29antarkpu:71": "jale madhura eva //",
    "GSP29antarkpu:72": "AnTs_21 ghrāṇagrāhyo guṇo gandhaḥ /",
    "GSP29antarkpu:73": "sa ca dvividhaḥ surabhirasurabhiś ca /",
    "GSP29antarkpu:74": "pṛthivīmātravṛttiḥ /",
    "GSP29antarkpu:75": "AnTs_22 tvagindriyamātragrāhyo guṇo saparśaḥ /",
    "GSP29antarkpu:76": "sa ca trividhaḥ śitoṣṇānuṣṇāśītabhedāt /",
    "GSP29antarkpu:77": "pṛthivyaptejovāyuvṛttiḥ /",
    "GSP29antarkpu:78": "tatra śīto jale /",
    "GSP29antarkpu:79": "uṣṇas tejasi /",
    "GSP29antarkpu:80": "anuṣṇāśītaḥ pṛthivīvāyvoḥ //",
    "GSP29antarkpu:81": "AnTs_23 rūpādicatuṣṭayaṃ pṛthivyāṃ pākajam anityaṃ ca /",
    "GSP29antarkpu:82": "anyatrāpākajaṃ nityam antyaṃ ca /",
    "GSP29antarkpu:83": "nityagataṃ nityam /",
    "GSP29antarkpu:84": "anityagatam anityam //",
    "GSP29antarkpu:85": "AnTs_24 ekatvādivyavahārahetuḥ saṃkhyā /",
    "GSP29antarkpu:86": "navadravyavṛttir ekatvādiparārdhaparyantā /",
    "GSP29antarkpu:87": "ekatvaṃ nityam anityaṃ ca /",
    "GSP29antarkpu:88": "nityagataṃ nityam anityagatam anityam /",
    "GSP29antarkpu:89": "dvitvādikaṃ tu sarvatrānityam eva //",
    "GSP29antarkpu:90": "AnTs_25 mānavyavahārakāraṇaṃ parimāṇaṃ /",
    "GSP29antarkpu:91": "navadravyavṛtti /",
    "GSP29antarkpu:92": "tac caturvidham /",
    "GSP29antarkpu:93": "aṇu mahad dīrghaṃ hrasvaṃ ceti //",
    "GSP29antarkpu:94": "AnTs_26 pṛthagvyavahārakāraṇaṃ pṛthagtvaṃ /",
    "GSP29antarkpu:95": "sarvadravyavṛtti //",
    "GSP29antarkpu:96": "AnTs_27 saṃyuktavyavahārahetuḥ saṃyogaḥ /",
    "GSP29antarkpu:97": "sarvadravyavṛttiḥ //",
    "GSP29antarkpu:98": "AnTs_28 saṃyoganāśako guṇo vibhāgaḥ /",
    "GSP29antarkpu:99": "sarvadravyavṛttiḥ //",
    "GSP29antarkpu:100": "AnTs_29 parāparavyavahārāsādhāraṇakāraṇe paratvāparatve /",
    "GSP29antarkpu:101": "pṛthivyādicatuṣṭayamanovṛttīti /",
    "GSP29antarkpu:102": "te dvividhe dikkṛte kālakṛte ca /",
    "GSP29antarkpu:103": "dūrasthe dikkṛtaṃ paratvam /",
    "GSP29antarkpu:104": "samīpasthe dikkṛtam aparatvam /",
    "GSP29antarkpu:105": "jyeṣṭhe kālakṛthaṃ paratvam /",
    "GSP29antarkpu:106": "kaniṣṭhe kālakṛtam aparatvam //",
    "GSP29antarkpu:107": "AnTs_30 ādyapatanāsamavāyikāraṇaṃ gurutvaṃ /",
    "GSP29antarkpu:108": "pṛthivījalavṛtti //",
    "GSP29antarkpu:109": "AnTs_31 ādyasyandanāsamavāyikāraṇaṃ dravatvaṃ pṛthivyaptejovṛtti /",
    "GSP29antarkpu:110": "tad dvividhaṃ sāṃsiddhikaṃ naimittikaṃ ca /",
    "GSP29antarkpu:111": "sāṃsiddhikaṃ jale naimittikaṃ pṛthivītejasoḥ /",
    "GSP29antarkpu:112": "pṛthivyāṃ ghṛtādāvagnisaṃyogajanyaṃ daravatvam /",
    "GSP29antarkpu:113": "tejasi suvarṇādau //",
    "GSP29antarkpu:114": "AnTs_32 cūrṇādipiṇḍībhāvahetur guṇaḥ snehaḥ /",
    "GSP29antarkpu:115": "jalamātravṛttiḥ //",
    "GSP29antarkpu:116": "AnTs_33 śrotragrāhyo guṇaḥ śabdaḥ /",
    "GSP29antarkpu:117": "ākāśamātravṛttiḥ /",
    "GSP29antarkpu:118": "sa dvividho dvanyātmako varṇātmakaś ceti /",
    "GSP29antarkpu:119": "dvanyātmako bheryādau /",
    "GSP29antarkpu:120": "varṇātmakaḥ saṃskṛtabhāṣādirūpaḥ //",
    "GSP29antarkpu:121": "AnTs_34 sarvavyavahārahetur buddhir jñānam /",
    "GSP29antarkpu:122": "sā dvividhā smṛtiranubhavaś ca /",
    "GSP29antarkpu:123": "saṃskāramātrajanyaṃ jñānaṃ smṛtiḥ /",
    "GSP29antarkpu:124": "tadbhinnaṃ jñānam anubhavaḥ //",
    "GSP29antarkpu:125": "AnTs_35 sa dvividho yathārtho 'yathārthaś ca /",
    "GSP29antarkpu:126": "tadvati tatprakārako 'nubhavo yathārthaḥ /",
    "GSP29antarkpu:127": "yathā rajata idaṃ rajatam iti jñānam /",
    "GSP29antarkpu:128": "sa eva pramety ucyate /",
    "GSP29antarkpu:129": "tadabhāvavati tatprakārako 'nubhavo 'yathārthaḥ /",
    "GSP29antarkpu:130": "yathā śuktāv idaṃ rajatam iti jñānam //",
    "GSP29antarkpu:131": "AnTs_36 yathārthānubhavaś caturvidhaḥ pratyakṣānumityupamitiśabdabhedāt /",
    "GSP29antarkpu:132": "tatkaraṇam api caturvidhaṃ pratyakṣānumānopamānaśabdabhedāt //",
    "GSP29antarkpu:133": "AnTs_37 asādhāraṇam kāraṇaṃ karaṇam /",
    "GSP29antarkpu:134": "AnTs_38 kāryaniyatapūrvavṛtti kāraṇam /",
    "GSP29antarkpu:135": "AnTs_39 kāryaṃ prāgabhāvapratiyogi /",
    "GSP29antarkpu:136": "AnTs_40 kāraṇaṃ trividhaṃ samavāyyasamavāyinimittabhedāt /",
    "GSP29antarkpu:137": "yat samavetaṃ kāryam utpadyate tat samavāyikāraṇam /",
    "GSP29antarkpu:138": "yathā tantavaḥ paṭasya paṭaś ca khagatarūpādeḥ /",
    "GSP29antarkpu:139": "kāryeṇa kāraṇena vā sahaikasminn arthe samavetatve sati yat kāraṇaṃ tad asamavāyikāraṇam /",
    "GSP29antarkpu:140": "yathā tantusaṃyogaḥ paṭasya tanturūpaṃ paṭarūpasya /",
    "GSP29antarkpu:141": "tadubhayabhinnaṃ kāraṇaṃ nimittakāraṇam /",
    "GSP29antarkpu:142": "yathā turīvemādikaṃ paṭasya /",
    "GSP29antarkpu:143": "AnTs_41 tad etattrividhakāraṇamadhye yad asādhāraṇaṃ kāraṇaṃ tad eva karaṇam //",
    "GSP29antarkpu:144": "AnTs_42 tatra pratyakṣajñānakaraṇaṃ pratyakṣam /",
    "GSP29antarkpu:145": "indriyārthasannikarṣajanyaṃ jñānaṃ pratyakṣam /tad dvividhaṃ nirvikalpakaṃ savikalpakaṃ ceti /",
    "GSP29antarkpu:146": "tatra nirvikalpakaṃ jñānaṃ nirvikalpakaṃ yathedaṃ kiṃcit /",
    "GSP29antarkpu:147": "saparakārakaṃ jñānaṃ savikalpakaṃ yathā ḍittho'yam brahmaṇo'yaṃ",
    "GSP29antarkpu:148": "śyāmo'yam iti //",
    "GSP29antarkpu:149": "AnTs_43 pratyakṣajñānahetur indriyārthasannikarṣaḥ ṣaḍvidhaḥ /",
    "GSP29antarkpu:150": "saṃyogaḥ saṃyuktasamavāyaḥ saṃyuktasamavetasamavāyaḥ samavāyaḥ samavetasamavāyo viśeṣaṇaviśeṣyabhāvaś ceti /",
    "GSP29antarkpu:151": "cakṣuṣā ghaṭapratyakṣajanane saṃyogaḥ sannikarṣaḥ /",
    "GSP29antarkpu:152": "ghaṭarūpapratyakṣajanane saṃyuktasamavāyaḥ sannikarṣaḥ cakṣuḥsaṃyukte ghaṭe rūpasya samavāyāt",
    "GSP29antarkpu:153": "rūpatvasāmānyapratyakṣe saṃyuktasamavetasamavāyaḥ sannikarṣaḥ cakṣuḥsaṃyukte ghaṭe rūpaṃ samavetaṃ tatra rūpatvasya samavāyāt /",
    "GSP29antarkpu:154": "śrotreṇa śabdasyākāśaguṇatvāt guṇaguṇinoś ca samavāyāt /",
    "GSP29antarkpu:155": "śabdatvasākṣātkāre samavetasamavāyaḥ sannikarṣaḥ śrotrasamavete śabde śabdatvasya samavāyāt",
    "GSP29antarkpu:156": "abhāvaparatyakṣe viśeṣaṇaviśeṣyabhāvaḥ sannikarṣo ghaṭābhāvavad bhṛtalam ity atra cakṣuḥsaṃyukte bhūtale ghaṭābhāvasya viśeṣaṇatvāt /",
    "GSP29antarkpu:157": "evaṃ snnikarṣaṣaṭkajanyaṃ jñānaṃ pratyakṣam /",
    "GSP29antarkpu:158": "tatkaraṇam indriyam /",
    "GSP29antarkpu:159": "tasmād indriyaṃ pratyakṣapramāṇam iti siddham //",
    "GSP29antarkpu:160": "AnTs_44 anumitikaraṇam anumānam /",
    "GSP29antarkpu:161": "parāmarśajanyaṃ jñānam anumitiḥ /",
    "GSP29antarkpu:162": "vyāptiviśiṣṭapakṣadharmatājñānaṃ parāmarśaḥ /",
    "GSP29antarkpu:163": "yathā vahnivyāpyadhūmavān ayaṃ parvata iti jñānaṃ parāmarśaḥ /",
    "GSP29antarkpu:164": "taj janyaṃ parvato vahnimān iti jñānam anumitiḥ /",
    "GSP29antarkpu:165": "yatra yatra dhūmas tatrāgnir iti sāhacaryaniyamo vyāptiḥ",
    "GSP29antarkpu:166": "/ vyāpyasya parvatādivṛttitvaṃ pakṣadharmatā //",
    "GSP29antarkpu:167": "AnTs_45 anumānaṃ dvividhaṃ svārthaṃ parārthaṃ ca /",
    "GSP29antarkpu:168": "tatra svārthaṃ svānumitihetuḥ /",
    "GSP29antarkpu:169": "tathā hi svayam eva bhūyo darśanena yatra dhūmas tatra agnir iti mahānasādau vyāptiṃ gṛhītvā parvatasamīpaṃ gatas tadgate cāgnau sandihānaḥ parvate dhūmaṃ paśyan vyāptiṃ smarati yatra dhūmas tatrāgnir iti /",
    "GSP29antarkpu:170": "tadantaraṃ vahnivyāpyadhūmavān ayaṃ parvata iti jñānam utpadyate /",
    "GSP29antarkpu:171": "ayam eva liṅgaparāmarṣaṃ ity ucyate /",
    "GSP29antarkpu:172": "tasmāt parvato vahnimān iti jñānam anumitir utpadyate /",
    "GSP29antarkpu:173": "tad etat svārthānumānam /",
    "GSP29antarkpu:174": "yat tu svayaṃ dhūmād agnim anumāya parapratipattyarthaṃ pañcāvayavavākyaṃ prayukte tat parārthānumānam /",
    "GSP29antarkpu:175": "yathā parvato vahnimān dhūmavattvāt /",
    "GSP29antarkpu:176": "yo yo dhūmavān sa vahnimān yathā mahānasaḥ /",
    "GSP29antarkpu:178": "tasmāt tatheti /",
    "GSP29antarkpu:179": "anena pratipāditāl lingāt paro'py agniṃ pratipadyate //",
    "GSP29antarkpu:180": "AnTs_46 pratijñāhetūdāharaṇopanayanigamāni pañcāvayavaḥ /",
    "GSP29antarkpu:181": "parvato vahnimān iti pratijñā /",
    "GSP29antarkpu:182": "dhūmavattvād iti hetuḥ /",
    "GSP29antarkpu:183": "yo yo dhūmavān sa so'gnimān yathā mahānasa ity udāharaṇam /",
    "GSP29antarkpu:184": "tathā cāyam iti upanayaḥ /",
    "GSP29antarkpu:185": "tasmāt tatheti nigamanam //",
    "GSP29antarkpu:186": "AnTs_47 svārthānumitiparārthānumityor liṅgaparāmarśa eva karaṇam /",
    "GSP29antarkpu:187": "tasmāl",
    "GSP29antarkpu:188": "liṅgaparāmarśā'numānam //",
    "GSP29antarkpu:189": "AnTs_48 liṅgaṃ trividham /",
    "GSP29antarkpu:190": "anvayavyatireki kevalānvayi kevalavyatireki ceti /",
    "GSP29antarkpu:191": "anvayena vyatirekeṇa ca vyāptimad anvayavyatireki /",
    "GSP29antarkpu:192": "yathā vahnau sādhye dhūmavattvam /",
    "GSP29antarkpu:193": "yatra dhūmas tatrāgnir yathā mahānasa ity anvayavyāptiḥ /",
    "GSP29antarkpu:194": "yatra vahnir nāsti tatra dhūmo'pi nāsti yathā mahāhrada",
    "GSP29antarkpu:195": "iti vyatirekavyāptiḥ /",
    "GSP29antarkpu:196": "anvayamātravyāptikaṃ kevalānvayi yathā",
    "GSP29antarkpu:197": "ghaṭo'bhidheyaḥ prameyatvāt paṭavat /",
    "GSP29antarkpu:198": "atra prameyatvābhidhyatvayor vyatirekavyāptir nāsti sarvasyāpi prameyatvād abhidheyatvāc ca",
    "GSP29antarkpu:199": "vyatirekamātravyāptikaṃ kevalavyatireki yathā pṛthivī tarebhyo bhidyate gandhavattvāt /",
    "GSP29antarkpu:200": "yad itarebhyo na bhidyate na tad gandhavat /",
    "GSP29antarkpu:201": "yathā jalam /",
    "GSP29antarkpu:202": "na ceyaṃ tathā /",
    "GSP29antarkpu:203": "tasmān na tatheti atra yad gandhavat tad itarabhinnam ity anvayadṛṣṭanto nāsti pṛthivīmātrasya pakṣatvāt //",
    "GSP29antarkpu:204": "AnTs_49 saṃdigdhasādhyavān pakṣaḥ /",
    "GSP29antarkpu:205": "yathā dhūmavattve hetau parvataḥ //",
    "GSP29antarkpu:206": "AnTs_50 niścitasādhyavān sapakṣaḥ /",
    "GSP29antarkpu:207": "yathā tatraiva mahānasaḥ //",
    "GSP29antarkpu:208": "AnTs_51 niścitasādhyābhāvavān vipakṣaḥ /",
    "GSP29antarkpu:209": "yathā tatraiva mahāhradaḥ //",
    "GSP29antarkpu:210": "AnTs_52 savyabhicāraviruddhasatpakṣāsiddhabādhitāḥ pañca hetvābhāsāḥ //",
    "GSP29antarkpu:211": "AnTs_53 savyabhicāro'naikāntikaḥ /",
    "GSP29antarkpu:212": "sa trividhaḥ /",
    "GSP29antarkpu:213": "sādhāraṇāsādhāraṇānupasaṃhāribhedāt /",
    "GSP29antarkpu:214": "tatra sādhyābhāvavadvṛttiḥ sādhāraṇo anaikāntikaḥ /",
    "GSP29antarkpu:215": "yathā parvato vahnimān prameyatvād iti prameyatvasya vahnyabhāvavati hrade vidyamānatvāt /",
    "GSP29antarkpu:216": "sarvasapakṣavipakṣavyāvṛtto'sādhāraṇaḥ /",
    "GSP29antarkpu:217": "yathā śabdo nityaḥ śabdatvād iti /",
    "GSP29antarkpu:218": "śabdatvaṃ sarvebhyo nityebhyo'ntyebhyaś ca vyāvṛttaṃ śabdamātravṛtti /",
    "GSP29antarkpu:219": "anvayavyatirekadṛṣṭāntarahito'nupasaṃhārī /",
    "GSP29antarkpu:220": "yathā sarvam anityaṃ prameyatvād iti /",
    "GSP29antarkpu:221": "atra sarvasyāpi pakṣatvād dṛṣṭānto nāsti //",
    "GSP29antarkpu:222": "AnTs_54 sādhyābhāvavyāpto hetur viruddhaḥ /",
    "GSP29antarkpu:223": "yatra śabdo nityaḥ kṛtakatvād iti /",
    "GSP29antarkpu:224": "kṛtakatvaṃ hi nityatvābhāvenānityatvena vyāptam //",
    "GSP29antarkpu:225": "AnTs_55 yasya sādhyabhāvasādhakaṃ hetvantaraṃ vidyate sa satpratipakṣaḥ /",
    "GSP29antarkpu:226": "yathā śabdo nityaḥ śrāvaṇatvāc chabdatvavad iti /",
    "GSP29antarkpu:227": "śabdo'nityaḥ kāryatvād ghaṭavad iti //",
    "GSP29antarkpu:228": "AnTs_56 asiddhas trividhaḥ /",
    "GSP29antarkpu:229": "āśrayāsiddhaḥ svarūpāsiddho vyāpyatvāsiddhaś ceti /",
    "GSP29antarkpu:230": "āśrayāsiddho yathā gaganāravindaṃ surabhy aravindatvāt sarojāravindavat /",
    "GSP29antarkpu:231": "atra gaganāravindam āśrayaḥ /",
    "GSP29antarkpu:232": "sa ca nāsty eva /",
    "GSP29antarkpu:233": "svarūpāsiddho yathā śabdo guṇaś cākṣuṣatvāt /",
    "GSP29antarkpu:234": "atra cākṣuṣatvaṃ śabde nāsti śabdasya śrāvaṇatvāt /",
    "GSP29antarkpu:235": "sopādhiko vyāpyatvāsiddhaḥ /",
    "GSP29antarkpu:236": "sādhyavyāpakatve sati sādhanāvyāpaka upādhiḥ /",
    "GSP29antarkpu:237": "sādhyasamānādhikaraṇātyantābhāvāpratiyogitvaṃ sādhyavyāpakatvam /",
    "GSP29antarkpu:238": "sādhanavann iṣṭātyantābhāvapratiyogitvaṃ sādhāraṇāvyapakatvam /",
    "GSP29antarkpu:239": "parvato dhūmavān vahnimattvād ity atrārdrendhasaṃyoga upādhiḥ /",
    "GSP29antarkpu:240": "tathā hi /",
    "GSP29antarkpu:241": "yatra dhūmas tatrārdrendhnasaṃyoga iti sādhyavyāpakatā /",
    "GSP29antarkpu:242": "yatra vahnis tatrārdrendhanasaṃyogābhāvād iti sādhanāvyāpakatā /",
    "GSP29antarkpu:243": "evaṃ sādhyavyāpakatve sati sādhanāvyāpakatvād ardrendhanasaṃyoga upādhiḥ /",
    "GSP29antarkpu:244": "sopādhikatvād vahnimattvaṃ vyāpyatvāsiddham //",
    "GSP29antarkpu:245": "AnTs_57 yasya sādhyābhāvaḥ pramāṇāntareṇa niścitaḥ sa bādhitaḥ /",
    "GSP29antarkpu:246": "yathā vahnir anuṣṇo dravyatvād iti /",
    "GSP29antarkpu:247": "atrānuṣṇatvaṃ sādhyaṃ tadabhāva uṣṇatvaṃ spārśanapratyakṣeṇa gṛhyata iti bādhitatvam //",
    "GSP29antarkpu:248": "AnTs_58 upamitikaraṇam upamānam /",
    "GSP29antarkpu:249": "saṃjñāsaṃjñisaṃbandhajñānam upamitiḥ /",
    "GSP29antarkpu:250": "tatkaraṇaṃ sādṛśyajñānam /",
    "GSP29antarkpu:251": "atideśavākyārthasmaraṇam avāntaravyāpāraḥ /",
    "GSP29antarkpu:252": "tathā hi kaś cid gavayaśabdārtham ajānan kutaś cid āraṇyakapuruṣād gosadṛśo gavaya iti śrutvā vanaṃ gato vākyārthaṃ smaran gosadṛṣaṃ piṇḍaṃ paśyati /",
    "GSP29antarkpu:253": "tadanantaram asau gavayaśabdavācya ity upamitir utpadyte //",
    "GSP29antarkpu:254": "AnTs_59 āptavākyaṃ śabdaḥ /",
    "GSP29antarkpu:255": "āptas tu yathārthavaktā /",
    "GSP29antarkpu:256": "vākyaṃ padasamūhaḥ /",
    "GSP29antarkpu:257": "yathā gām ānayeti /",
    "GSP29antarkpu:258": "śaktaṃ padam /",
    "GSP29antarkpu:259": "asmāt padād ayam artho boddhavya itīśvarasaṃketaḥ śaktiḥ //",
    "GSP29antarkpu:260": "AnTs_60 ākāṅkṣā yogyatā saṃnidhiś ca vākyārthajñānahetuḥ /",
    "GSP29antarkpu:261": "padasya padāntaravyatirekaprayuktānvayānanubhāvakatvam ākāṅkṣā /",
    "GSP29antarkpu:262": "arthābādho yogyatā /",
    "GSP29antarkpu:263": "padānāmavilambenoccāraṇaṃ saṃnidhiḥ //",
    "GSP29antarkpu:264": "AnTs_61 ākāṅkṣādirahitaṃ vākyam apramāṇam /",
    "GSP29antarkpu:265": "yathā gaur aśvaḥ puruṣo hastīti na pramāṇam ākāṅkṣāvirahāt /",
    "GSP29antarkpu:266": "agninā siñced iti na pramāṇaṃ yogyatāvirahāt /",
    "GSP29antarkpu:267": "prahare prehare'sahoccāritāni gām ānayetyādipadāni na",
    "GSP29antarkpu:268": "pramāṇaṃ saṃnidhyabhāvāt //",
    "GSP29antarkpu:269": "AnTs_62 vākyaṃ dvividham /",
    "GSP29antarkpu:270": "vaidikaṃ laukikaṃ ca /",
    "GSP29antarkpu:271": "vaidikam īśvaroktatvāt sarvam eva pramāṇam /",
    "GSP29antarkpu:272": "laukikaṃ tv āptoktaṃ pramāṇam /",
    "GSP29antarkpu:273": "anyad apramāṇam //",
    "GSP29antarkpu:274": "AnTs_63 vākyārthajñānaṃ śabdajñānam /",
    "GSP29antarkpu:275": "tatkaraṇaṃ śabdaḥ //",
    "GSP29antarkpu:276": "AnTs_64 ayathārthānubhavas trividhaḥ saṃśayaviparyayatarkabhedāt /",
    "GSP29antarkpu:277": "ekasmin dharmini viruddhanānādharmavaiśiṣṭyāvagāhi jñānaṃ saṃśayaḥ /",
    "GSP29antarkpu:278": "yathā sthāṇur vā purṣo veti /",
    "GSP29antarkpu:279": "mithyājñānaṃ viparyayaḥ /",
    "GSP29antarkpu:280": "yathā śuktāv idaṃ rajatam iti /",
    "GSP29antarkpu:281": "vyāpyāropeṇa vyāpakāropas tarkaḥ yathā yadi vahnir na syāt tarhi dhūmo'pi",
    "GSP29antarkpu:282": "na syād iti //",
    "GSP29antarkpu:283": "AnTs_65 smṛtir api dvividhā /",
    "GSP29antarkpu:284": "yathārthāyathārthā ca pramājanyā yathārthā /",
    "GSP29antarkpu:285": "apramājanyāyathārthā //",
    "GSP29antarkpu:286": "AnTs_66 sarveśām anukūlatayā vedanīyaṃ sukham //",
    "GSP29antarkpu:287": "AnTs_67 sarveśāṃ pratikūlatayā vedanīyaṃ duḥkham //",
    "GSP29antarkpu:288": "AnTs_68 icchā kāmaḥ //",
    "GSP29antarkpu:289": "AnTs_69 krodho dveṣaḥ //",
    "GSP29antarkpu:290": "AnTs_70 kṛtiḥ prayatnaḥ //",
    "GSP29antarkpu:291": "AnTs_71 vihitakarmajanyo dharmaḥ //",
    "GSP29antarkpu:292": "AnTs_72 niṣiddhakarmajanyas tv adharmaḥ //",
    "GSP29antarkpu:293": "AnTs_73 buddhyādayo'ṣṭāv ātmamātraviśeṣaguṇāḥ //",
    "GSP29antarkpu:294": "AnTs_74 buddhīcchāprayatnā dvividhāḥ /",
    "GSP29antarkpu:295": "nityā anityāś ca /",
    "GSP29antarkpu:296": "nityā īśvarasya /",
    "GSP29antarkpu:297": "anityā jīvasya //",
    "GSP29antarkpu:298": "AnTs_75 saṃskāras trividhaḥ /",
    "GSP29antarkpu:299": "vego bhāvanā sthitisthāpakaś ceti /",
    "GSP29antarkpu:300": "vegaḥ pṛthivyādicatuṣṭayamanovṛttiḥ /",
    "GSP29antarkpu:301": "anubhavajanyā smṛtihetur bhāvanātmamātravṛttiḥ /",
    "GSP29antarkpu:302": "anyathā kṛtasya punas tadavasthāpādakaḥ sthitisthāpakaḥ kaṭādipṛthivīvṛttiḥ //",
    "GSP29antarkpu:303": "AnTs_76 calanātmakaṃ karma /",
    "GSP29antarkpu:304": "ūrdhvadeśasaṃyogahetur utkṣepaṇam /",
    "GSP29antarkpu:305": "adhodeśasaṃyogahetur apakṣepaṇam /",
    "GSP29antarkpu:306": "śarīrasaṃnikṛṣṭasaṃyogahetur ākuñcanam /",
    "GSP29antarkpu:307": "viprakṛṣṭasaṃyogahetuḥ prasāraṇam /",
    "GSP29antarkpu:308": "anyat sarvaṃ gamanam /",
    "GSP29antarkpu:309": "pṛthivyādicatuṣṭayamanomātravṛtti //",
    "GSP29antarkpu:310": "AnTs_77 nityam ekam anekānugataṃ sāmānyam /",
    "GSP29antarkpu:311": "dravyaguṇakarmavṛtti /",
    "GSP29antarkpu:312": "tad dvividhaṃ parāparabhedāt /",
    "GSP29antarkpu:313": "paraṃ sattā /",
    "GSP29antarkpu:314": "aparaṃ dravyatvādiḥ //",
    "GSP29antarkpu:315": "AnTs_78 nityadravyavṛttayo vyāvartakā viśeṣāḥ //",
    "GSP29antarkpu:316": "AnTs_79 nityasaṃbandhaḥ samavāyaḥ /",
    "GSP29antarkpu:317": "ayutasiddhavṛttiḥ /",
    "GSP29antarkpu:318": "yayor dvayor madhya ekam avinaśyadaparāśritam evāvatiṣṭate tāv ayutasiddau /",
    "GSP29antarkpu:319": "yathāvayavāvayavinau guṇaguṇinau kriyākriyāvantau jātivyaktī viśeṣanityadravye ceti //",
    "GSP29antarkpu:320": "AnTs_80 anādiḥ sāntaḥ prāgabhāvaḥ /",
    "GSP29antarkpu:321": "utpatteḥ pūrvaṃ kāryasya /",
    "GSP29antarkpu:322": "sādir anantaḥ pradhvaṃsaḥ /",
    "GSP29antarkpu:323": "utpattyanantaraṃ kāryasya /",
    "GSP29antarkpu:324": "traikālikasaṃsargāvacchinnapratiyogitāko'tyantābhāvaḥ /",
    "GSP29antarkpu:325": "yathā bhūtale ghaṭo nāstīti /",
    "GSP29antarkpu:326": "tādātmyasaṃbandhāvacchinnapratiyogitāko'nyonyābhāvaḥ yathā ghaṭaḥ paṭo na bhavatīti //",
    "GSP29antarkpu:327": "AnTs_81 sarveṣāṃ padārthānāṃ yathāyatham ukteṣv antarbhāvāt saptaiva",
    "GSP29antarkpu:328": "padārthā iti siddham //",
    "GSP29antarkpu:329": "kāṇādanyāyamatayor bālāvyutpattisiddhaye /",
    "GSP29antarkpu:330": "annaṃbhaṭṭena viduṣā racitas tarkasaṃgrahaḥ //"
}