{
    "GR13bhnstcxu:0": " sugirā cittahāriṇyā paśyantyā dṛśyamānayā |",
    "GR13bhnstcxu:1": " jayati+ullāsitānandamahimā parameśvaraḥ || 1",
    "GR13bhnstcxu:2": " yas+sphītas+śrīdayābodhaparamānandasampadā |",
    "GR13bhnstcxu:3": " vidyoddyotitamāhātmyas+sa jayati+aparājitaḥ || 2",
    "GR13bhnstcxu:4": " prasaradbindunādāya śuddhāmṛtamayātmane |",
    "GR13bhnstcxu:5": " namas+anantaprakāśāya śaṃkarakṣīrasindhave || 3",
    "GR13bhnstcxu:6": " dviṣmas+tvām+tvām+stumas+tubhyam+mantrayāmas+ambikāpate |",
    "GR13bhnstcxu:7": " ativāllabhyatas+sādhu viśvak+no dhṛtavān+asi || 4",
    "GR13bhnstcxu:8": " saṃhṛtasparśayogāya sampūrṇāmṛtasūtaye |",
    "GR13bhnstcxu:9": " viyanmāyāsvarūpāya vibhave śambhave namaḥ || 5",
    "GR13bhnstcxu:10": " bhinneṣu+api na bhinnam+yat+chinneṣu+achinnam+eva ca |",
    "GR13bhnstcxu:11": " namāmas+sarvasāmānyam+rūpam+tat+pārameśvaram || 6",
    "GR13bhnstcxu:12": " praṇavordhvārdhamātrātas+api+aṇave mahate punaḥ |",
    "GR13bhnstcxu:13": " brahmāṇḍāt+api nairguṇyaguṇāya sthāṇave namaḥ || 7",
    "GR13bhnstcxu:14": " brahmāṇḍagarbhiṇīm+vyomavyāpinas+sarvatogateḥ |",
    "GR13bhnstcxu:15": " parameśvarahaṃsasya śaktim+haṃsīm+iva stumaḥ || 8",
    "GR13bhnstcxu:16": " nirupādānasambhāram+abhittau+eva tanvate |",
    "GR13bhnstcxu:17": " jagaccitram+namas+tasmai kalāślāghyāya śūline || 9",
    "GR13bhnstcxu:18": " māyājalodarāt+samyak+uddhṛtya vimalīkṛtam |",
    "GR13bhnstcxu:19": " śivajñānam+svatas+dugdham+dehi+ehi harahaṃsa naḥ || 10",
    "GR13bhnstcxu:20": " ṣaṭpramāṇīparicchedabhedayoge+api+abhedine |",
    "GR13bhnstcxu:21": " paramārthaikabhāvāya balim+yāmas+bhavāya te || 11",
    "GR13bhnstcxu:22": " api paśyema gambhīrām+pareṇa jyotiṣā+abhitaḥ |",
    "GR13bhnstcxu:23": " unmṛṣṭatamasam+ramyām+antarbhava bhavadguhām || 12",
    "GR13bhnstcxu:24": " namas+tebhyas+api ye somakalākalitaśekharam |",
    "GR13bhnstcxu:25": " nātham+svapne+api paśyanti paramānandadāyinam || 13",
    "GR13bhnstcxu:26": " bhagavan+bhava bhāvatkam+bhāvam+bhāvayitum+ruciḥ |",
    "GR13bhnstcxu:27": " punarbhavabhayocchedadakṣā kasmai na rocate || 14",
    "GR13bhnstcxu:28": " yāvajjīvam+jagannātha kartavyam+idam+astu naḥ |",
    "GR13bhnstcxu:29": " tvatprasādāt+tvadekāgramanaskatvena yā sthitiḥ || 15",
    "GR13bhnstcxu:30": " śākhāsahasravistīrṇavedāgamamayātmane |",
    "GR13bhnstcxu:31": " namas+anantaphalotpādakalpavṛkṣāya śambhave || 16",
    "GR13bhnstcxu:32": " vāṅmanaḥkāyakarmāṇi viniyojya tvayi prabho |",
    "GR13bhnstcxu:33": " tvanmayībhūya nirdvandvās+kaccit+syāma+api karhicit || 17",
    "GR13bhnstcxu:34": " jagatām+sargasaṃhāratattaddhitaniyuktiṣu |",
    "GR13bhnstcxu:35": " ananyāpekṣasāmarthyaśālini śūline namaḥ || 18",
    "GR13bhnstcxu:36": " vyatītaguṇayogasya mukhyadhyeyasya dhūrjaṭeḥ |",
    "GR13bhnstcxu:37": " nāma+api dhyāyatām+dhyānais+kim+anyālambanais+phalam || 19",
    "GR13bhnstcxu:38": " namas+namas+śivāya+iti mantrasāmarthyam+āśritāḥ |",
    "GR13bhnstcxu:39": " ślāghyās+te śāmbhavīm+bhūtim+upabhoktum+ye+udyatāḥ || 20",
    "GR13bhnstcxu:40": " kas+panthās+yena na prāpyas+kā ca vāk+na+ucyase yayā |",
    "GR13bhnstcxu:41": " kim+dhyānam+yena na dhyeyas+kim+vā kim+na+asi yat+prabho || 21",
    "GR13bhnstcxu:42": " arcitas+ayam+ayam+dhyātas+eṣa toṣitas+iti+ayam |",
    "GR13bhnstcxu:43": " rasas+srotaḥsahasreṇa tvayi me bhava vardhatām || 22",
    "GR13bhnstcxu:44": " namas+niḥśeṣadhīpatrimālālayamayātmane |",
    "GR13bhnstcxu:45": " nāthāya sthāṇave tubhyam+nāgayajñopavītine || 23",
    "GR13bhnstcxu:46": " ajñānatimirasya+ekam+auṣadham+saṃsmṛtis+tava |",
    "GR13bhnstcxu:47": " bhava tattatpradānena prasādas+kriyatām+mayi || 24",
    "GR13bhnstcxu:48": " namas+īśāya niḥśeṣapuruṣārthaprasādhakaḥ |",
    "GR13bhnstcxu:49": " praṇantavyas+praṇāmas+api yadīyas+iha dhīmatām || 25",
    "GR13bhnstcxu:50": " magnais+bhīme bhavāmbhodhau nilaye duḥkhayādasām |",
    "GR13bhnstcxu:51": " bhakticintāmaṇim+śārvam+tatas+prāpya na kim+jitam || 26",
    "GR13bhnstcxu:52": " nirāvaraṇanirdvandvaniścalajñānasampadām |",
    "GR13bhnstcxu:53": " jñeyas+asi kila ke+api+ete ye tvām+jānanti dhūrjaṭe || 27",
    "GR13bhnstcxu:54": " nirguṇas+api guṇajñānām+jñeyas+ekas+jayati+ajaḥ |",
    "GR13bhnstcxu:55": " niṣkāmas+api prakṛtyā yas+kāmanānām+param+phalam || 28",
    "GR13bhnstcxu:56": " śrīratnāmṛtalābhāya kliṣṭam+yatra na kais+suraiḥ |",
    "GR13bhnstcxu:57": " tat+kṣīrodadam+aiśvaryam+tava+eva sahajam+vibho || 29",
    "GR13bhnstcxu:58": " namas+bhaktyā nṛṇām+muktyai bhavate bhava te+avate |",
    "GR13bhnstcxu:59": " smṛtyā nutyā ca dadate śambhave śam+bhave+abhave || 30",
    "GR13bhnstcxu:60": " sarvajñas+sarvakṛt+sarvam+asi+iti jñānaśālinām |",
    "GR13bhnstcxu:61": " vedyam+kim+karma vā nātha na+ānantyāya tvayā+arpyate || 31",
    "GR13bhnstcxu:62": " icchāyās+eva yasya+iyat+phalam+lokatrayātmakam |",
    "GR13bhnstcxu:63": " tasya te nātha kāryāṇām+kas+vetti kiyatī gatiḥ || 32",
    "GR13bhnstcxu:64": " brahmādayas+api tat+yasya karmasopānamālayā |",
    "GR13bhnstcxu:65": " upari+upari dhāvanti labdhum+dhāma namāmi tam || 33",
    "GR13bhnstcxu:66": " ayam+brahmā mahendras+ayam+sūryācandramasau+imau |",
    "GR13bhnstcxu:67": " iti śaktilatā yasya puṣpitā pātu+asau bhavaḥ || 34",
    "GR13bhnstcxu:68": " bhramas+na labhyate yasya bhrāntāntaḥkaraṇais+api |",
    "GR13bhnstcxu:69": " dūragais+api yasya+antas+durgam+astam+stumas+mṛḍam || 35",
    "GR13bhnstcxu:70": " namas+stutau smṛtau dhyāne darśane sparśane tathā |",
    "GR13bhnstcxu:71": " prāptau ca+ānandavṛndāya dayitāya kapardine || 36",
    "GR13bhnstcxu:72": " kim+smayena+iti matvā+api manasā parameśvara |",
    "GR13bhnstcxu:73": " smayena tvanmayas+asmi+iti māmi na+ātmani kim+mudā || 37",
    "GR13bhnstcxu:74": " cintayitvā+api kartavyakoṭīs+cittasya cāpalāt |",
    "GR13bhnstcxu:75": " viśrāmyan+bhava bhāvatkacittānande rame bhṛśam || 38",
    "GR13bhnstcxu:76": " sūkṣmas+api cet+trilokī+iyam+kalāmātram+katham+tava |",
    "GR13bhnstcxu:77": " sthūlas+atha kim+sudarśas+na brahmādibhis+api prabho || 39",
    "GR13bhnstcxu:78": " vācyas+eṣām+tvam+eva+iti na+abhaviṣyat+idam+yadi |",
    "GR13bhnstcxu:79": " kas+kleśam+deva vāgjāleṣu+akariṣyat+sudhīs+tadā || 40",
    "GR13bhnstcxu:80": " krameṇa karmaṇā kena kayā vā prajñayā prabho |",
    "GR13bhnstcxu:81": " dṛśyas+asi+iti+upadeśena prasādas+kriyatām+mama || 41",
    "GR13bhnstcxu:82": " namas+nirupakāryāya trailokyaikopakāriṇe |",
    "GR13bhnstcxu:83": " sarvasya spṛhaṇīyāya niḥspṛhāya kapardine || 42",
    "GR13bhnstcxu:84": " aho kṣetrajñatā sā+iyam+kāryāya mahate satām |",
    "GR13bhnstcxu:85": " yayā+anantaphalām+bhaktim+vapanti tvayi+amī prabho || 43",
    "GR13bhnstcxu:86": " mahatī+iyam+aho māyā tava māyin+yayā+āvṛtaḥ |",
    "GR13bhnstcxu:87": " tvaddhyānanidhilābhe+api mugdhas+lokas+ślathāyate || 44",
    "GR13bhnstcxu:88": " ārambhe bhava sarvatra karma vā karaṇādi vā |",
    "GR13bhnstcxu:89": " viśvam+astu svatantras+tu kartā tatra+ekakas+bhavān || 45",
    "GR13bhnstcxu:90": " triguṇatriparispandadvandvagrastam+jagattrayam |",
    "GR13bhnstcxu:91": " uddhartum+bhavatas+anyasya kasya śaktis+kṛpā+athavā || 46",
    "GR13bhnstcxu:92": " doṣas+apideva kas+doṣas+tvām+āptum+yas+samāsthitaḥ |",
    "GR13bhnstcxu:93": " guṇas+api ca guṇas+kas+nu tvām+na+āptum+yas+samāsthitaḥ || 47",
    "GR13bhnstcxu:94": " rāgas+api+astu jagannātha mama tvayi+eva yas+sthitaḥ |",
    "GR13bhnstcxu:95": " lobhāya+api namas+tasmai tvallābhālambanāya me || 47",
    "GR13bhnstcxu:96": " aho mahat+idam+karma deva tvadbhāvanātmakam |",
    "GR13bhnstcxu:97": " ābrahmakrimi yasmin+no muktaye+adhikriyeta kaḥ || 48",
    "GR13bhnstcxu:98": " ārambhas+sarvakāryāṇām+paryantas+sarvakarmaṇām |",
    "GR13bhnstcxu:99": " tadantarvṛttayas+citrās+tava+eva+īśas+dhiyas+pathi || 49",
    "GR13bhnstcxu:100": " yāvat+uttaram+āsvādasahasraguṇavistaraḥ |",
    "GR13bhnstcxu:101": " tvadbhaktirasapīyūṣāt+nātha na+anyatra dṛśyate || 50",
    "GR13bhnstcxu:102": " upasaṃhṛtakāmāya kāmāyatim+atanvate |",
    "GR13bhnstcxu:103": " avataṃsitasomāya somāya svāmine namaḥ || 51",
    "GR13bhnstcxu:104": " kim+aśaktas+karomi+iti sarvatra+anadhyavasyataḥ |",
    "GR13bhnstcxu:105": " sarvānugrāhikā śaktis+śāṃkarī śaraṇam+mama || 52",
    "GR13bhnstcxu:106": " guṇātītasya nirdiṣṭaniḥśeṣātiśayātmanaḥ |",
    "GR13bhnstcxu:107": " labhyate bhava kutra+aṃśe paras+pratinidhis+tava || 53",
    "GR13bhnstcxu:108": " nirdvandve nirupādhau ca tvayi+ātmani sati prabho |",
    "GR13bhnstcxu:109": " vayam+vañcyāmahe+adya+api māyayā+ameyayā tava || 54",
    "GR13bhnstcxu:110": " aṇimādiguṇāvāptis+sadā+aiśvaryam+bhavakṣayaḥ |",
    "GR13bhnstcxu:111": " amī bhava bhavadbhaktikalpapādapapallavāḥ || 55",
    "GR13bhnstcxu:112": " yā yā dik+tatra na kva+asi sarvas+kālas+bhavanmayaḥ |",
    "GR13bhnstcxu:113": " iti labdhas+api karhi tvam+lapsyase nātha kathyatām || 56",
    "GR13bhnstcxu:114": " namas+prasannasadvṛttamānasaikanivāsine |",
    "GR13bhnstcxu:115": " bhūribhūtisitāṅgāya mahāhaṃsāya śambhave || 57",
    "GR13bhnstcxu:116": " hṛtoddhatatamastāntis+pluṣṭāśeṣabhavendhanā |",
    "GR13bhnstcxu:117": " tvadbodhadīpikā me+astu nātha tvadbhaktidīpikā || 58",
    "GR13bhnstcxu:118": " visṛṣṭānekasadbījagarbham+trailokyanāṭakam |",
    "GR13bhnstcxu:119": " prastāvya hara saṃhartum+tvattas+kas+anyas+kavis+kṣamaḥ || 59",
    "GR13bhnstcxu:120": " namas+sadasatām+kartum+asattvam+sattvam+eva vā |",
    "GR13bhnstcxu:121": " svatantrāya+asvatantrāya vyayaiśvaryaikaśāline || 60",
    "GR13bhnstcxu:122": " trailokye+api+atra yas+yāvān+ānandas+kaścit+īkṣyate |",
    "GR13bhnstcxu:123": " sa bindus+yasya tam+vande devam+ānandasāgaram || 61",
    "GR13bhnstcxu:124": " aho brahmādayas+dhanyās+ye vimuktānyasaṃkatham |",
    "GR13bhnstcxu:125": " namas+namas+śivāya+iti japanti+āhlādavihvalāḥ || 62",
    "GR13bhnstcxu:126": " niṣkāmāya+api kāmānām+anantānām+vidhāyine |",
    "GR13bhnstcxu:127": " anāditve+api viśvasya bhoktre bhava namas+astu te || 63",
    "GR13bhnstcxu:128": " stumas+tribhuvanārambhamūlaprakṛtim+īśvaram |",
    "GR13bhnstcxu:129": " lipseran+na+upakāram+ke yatas+sampūrṇadharmaṇaḥ || 64",
    "GR13bhnstcxu:130": " mahatsu+api+arthakṛcchreṣu mohaughamalinīkṛtāḥ |",
    "GR13bhnstcxu:131": " smṛte yasmin+prasīdanti matayas+tam+śivam+stumaḥ || 65",
    "GR13bhnstcxu:132": " prabho bhavatas+eva+iha prabhuśaktis+abhaṅgurā |",
    "GR13bhnstcxu:133": " yadicchayā pratāyete trailokyasya layodayau || 66",
    "GR13bhnstcxu:134": " kukarmā+api yam+uddiśya devam+syāt+sukṛtam+param |",
    "GR13bhnstcxu:135": " sukṛtasya+api saukṛtyam+yatas+anyatra na sas+asi bhoḥ || 67",
    "GR13bhnstcxu:136": " eṣa muṣṭyā gṛhītas+asi dṛṣṭas+eṣa kva yāsi naḥ |",
    "GR13bhnstcxu:137": " iti bhaktirasādhmātās+dhanyās+dhāvanti dhūrjaṭim || 68",
    "GR13bhnstcxu:138": " stumas+tvām+ṛgyajuḥsāmnām+śukratas+paratas+param |",
    "GR13bhnstcxu:139": " yasya vedātmikājñeyam+aho gambhīrasundarī || 69",
    "GR13bhnstcxu:140": " vidhis+ādis+tathā+antas+asi viśvasya parameśvara |",
    "GR13bhnstcxu:141": " dharmagrāmas+pravṛttas+yas+tvattas+na sa kutas+bhavet || 70",
    "GR13bhnstcxu:142": " namas+te bhavasambhrāntabhrāntim+udbhāvya bhindate |",
    "GR13bhnstcxu:143": " jñānānandam+ca nirdvandvam+deva vṛtvā vivṛṇvate || 71",
    "GR13bhnstcxu:144": " yasyās+prāpyeta paryantaviśeṣas+kais+manorathaiḥ |",
    "GR13bhnstcxu:145": " māyām+ekanimeṣeṇa muṣṇan+tām+pātu nas+śivaḥ || 72",
    "GR13bhnstcxu:146": " vairāgyasya gatim+gurvīm+jñānasya paramām+śriyam |",
    "GR13bhnstcxu:147": " naiḥspṛhyasya parām+koṭim+bibhratām+tvam+prabho prabhuḥ || 73",
    "GR13bhnstcxu:148": " brahmaṇas+api bhavān+brahma kasya na+īśas+tvam+īśituḥ |",
    "GR13bhnstcxu:149": " jagatkalyāṇakalyāṇam+kiyat+tvam+iti vetti kaḥ || 74",
    "GR13bhnstcxu:150": " kim+anyais+bandhubhis+kim+ca suhṛdbhis+svāmibhis+tathā |",
    "GR13bhnstcxu:151": " sarvasthāne mama+īśa tvam+yas+uddhartā bhavārṇavāt || 75",
    "GR13bhnstcxu:152": " jayanti mohamāyādimalasaṃkṣālanakṣamāḥ |",
    "GR13bhnstcxu:153": " śaivayogabalākṛṣṭās+divyapīyūṣavipruṣaḥ || 76",
    "GR13bhnstcxu:154": " gāyatryā gīyate yasya dhiyām+tejas+pracodakam |",
    "GR13bhnstcxu:155": " codayet+api kaccit+nas+sa dhiyas+satpathe prabhuḥ || 77",
    "GR13bhnstcxu:156": " aṣṭamūrte kim+ekasyām+api mūrtau na nas+sthitim |",
    "GR13bhnstcxu:157": " śāśvatīm+kuruṣe yat+vā tuṣṭas+sarvam+kariṣyasi || 78",
    "GR13bhnstcxu:158": " vastutattvam+padārthānām+prāyeṇa+arthakriyākaram |",
    "GR13bhnstcxu:159": " bhavatas+tu+īśa nāma+api mokṣaparyantasiddhidam || 79",
    "GR13bhnstcxu:160": " muhus+muhus+jagaccitrasya+anyānyām+sthitim+ūhitum |",
    "GR13bhnstcxu:161": " śaktis+yā te tayā nātha kas+manasvī na vismitaḥ || 80",
    "GR13bhnstcxu:162": " duṣkaram+sukarīkartum+duḥkham+sukhayitum+tathā |",
    "GR13bhnstcxu:163": " ekavīrā smṛtis+yasya tam+smarāmas+smaradviṣam || 81",
    "GR13bhnstcxu:164": " jayanti gītayas+yāsām+sa geyas+parameśvaraḥ |",
    "GR13bhnstcxu:165": " yannāmnā+api mahātmānas+kīryante pulakāṅkuraiḥ || 82",
    "GR13bhnstcxu:166": " bhavān+iva bhavān+eva bhavet+yadi param+bhava |",
    "GR13bhnstcxu:167": " svaśaktivyūhasaṃvyūḍhatrailokyārambhasaṃhṛtiḥ || 83",
    "GR13bhnstcxu:168": " mantras+asi mantraṇīyas+asi mantrī tvattas+kutas+aparaḥ |",
    "GR13bhnstcxu:169": " sa mahyam+dehi tam+mantram+tvanmantras+syām+yathā prabho || 84",
    "GR13bhnstcxu:170": " bhārūpas+satyasaṃkalpas+tvam+ātmā yasya sas+api+aham |",
    "GR13bhnstcxu:171": " saṃsārī+iti kim+īśa+eṣa svapnas+sas+api kutas+tvayi || 85",
    "GR13bhnstcxu:172": " tat+abhaṅgi tat+agrāmyam+tat+ekam+upapattimat |",
    "GR13bhnstcxu:173": " tvayi karmaphalanyāsakṛtām+aiśvaryam+īśa yat || 86",
    "GR13bhnstcxu:174": " kṣamas+kām+na+āpadam+hantum+kām+dātum+sampadam+na vā |",
    "GR13bhnstcxu:175": " yas+asau sa dayitas+asmākam+devadevas+vṛṣadhvajaḥ || 87",
    "GR13bhnstcxu:176": " māyāmayamalāndhasya divyasya jñānacakṣuṣaḥ |",
    "GR13bhnstcxu:177": " nirmalīkaraṇe nātha tvadbhaktis+paramāñjanam || 88",
    "GR13bhnstcxu:178": " nirbhayam+yat+yat+ānandamayam+ekam+yat+avyayam |",
    "GR13bhnstcxu:179": " padam+dehi+ehi me deva tūrṇam+tat+kim+pratīkṣase || 89",
    "GR13bhnstcxu:180": " aho nisargagambhīras+ghoras+saṃsārasāgaraḥ |",
    "GR13bhnstcxu:181": " aho tattaraṇopāyas+paras+kas+api maheśvaraḥ || 90",
    "GR13bhnstcxu:182": " namas+kṛtakṛtāntānta tubhyam+madanamardine |",
    "GR13bhnstcxu:183": " mastakanyastagaṅgāya yathāyuktārthakāriṇe || 91",
    "GR13bhnstcxu:184": " aiśvaryajñānavairāgyadharmebhyas+api+upari sthitim |",
    "GR13bhnstcxu:185": " nātha prārthayamānānām+tvat+ṛte kā parā gatiḥ || 92",
    "GR13bhnstcxu:186": " tvayi+anicchati kas+śambho śaktas+kubjayitum+tṛṇam |",
    "GR13bhnstcxu:187": " tvadicchānugṛhītas+tu vahet+brāhmīm+dhuram+na kaḥ || 93",
    "GR13bhnstcxu:188": " harapraṇatimāṇikyamukuṭotkaṭamastakāḥ |",
    "GR13bhnstcxu:189": " nameyus+kam+param+kam+vā namayeyus+na dhīdhanāḥ || 94",
    "GR13bhnstcxu:190": " sarvavibhramanirmokaniṣkampam+amṛtahradam |",
    "GR13bhnstcxu:191": " bhavajjñānāmbudhes+madhyam+adhyāsīya+api dhūrjaṭe || 95",
    "GR13bhnstcxu:192": " citram+yat+citradṛṣṭas+api manorathagatas+api vā |",
    "GR13bhnstcxu:193": " paramārthaphalam+nātha paripūrṇam+prayacchasi || 96",
    "GR13bhnstcxu:194": " kas+guṇais+adhikas+tvattas+tvattas+kas+nirguṇas+adhikaḥ |",
    "GR13bhnstcxu:195": " iti nātha numas+kim+tvām+kim+nindāmas+na manmahe || 97",
    "GR13bhnstcxu:196": " kīrtane+api+amṛtaughasya yatprasattes+phalam+tava |",
    "GR13bhnstcxu:197": " tat+pātum+api kas+anyas+alam+kim+u dātum+jagatpate || 98",
    "GR13bhnstcxu:198": " niḥśeṣaprārthanīyārthasārthasiddhinidhānataḥ |",
    "GR13bhnstcxu:199": " tvattas+tvadbhaktim+eva+āptum+prārthaye nātha sarvathā || 99",
    "GR13bhnstcxu:200": " namas+trailokyanāthāya tubhyam+bhava bhavajjuṣām |",
    "GR13bhnstcxu:201": " trilokīnāthatādānanirvināyakaśaktaye || 100",
    "GR13bhnstcxu:202": " niḥśeṣakleśahānasya hetus+kas+iti saṃśaye |",
    "GR13bhnstcxu:203": " svāmin+sas+asi+iti niścitya kas+tvām+na śaraṇam+gataḥ || 101",
    "GR13bhnstcxu:204": " bhuktvā bhogān+bhavabhrāntim+hitvā lapsye param+padam |",
    "GR13bhnstcxu:205": " iti+āśaṃsā+iha śobheta śambhau bhaktimatas+param || 102",
    "GR13bhnstcxu:206": " nātha svapne+api yat+kuryām+brūyām+vā sādhu+asādhu vā |",
    "GR13bhnstcxu:207": " tvadadhīnatvadarpeṇa sarvatra+atra+asmi nirvṛtaḥ || 103",
    "GR13bhnstcxu:208": " jyotiṣām+api yat+jyotis+tatra tvaddhāmni dhāvataḥ |",
    "GR13bhnstcxu:209": " cittasya+īśa tamaḥsparśas+manye vandhyātmajānujaḥ || 104",
    "GR13bhnstcxu:210": " manye nyastapadas+sas+api kṣemye mokṣasya vartmani |",
    "GR13bhnstcxu:211": " manorathas+sthitas+yasya seviṣye śivam+iti+ayam || 105",
    "GR13bhnstcxu:212": " sthityutpattilayais+lokatrayasya+upakriyāsu+iha |",
    "GR13bhnstcxu:213": " ekā+eva+īśa bhavacchaktis+svatantram+tantram+īkṣase || 106",
    "GR13bhnstcxu:214": " trilokyām+iha kas+trātas+tritāpyā na+upatāpitaḥ |",
    "GR13bhnstcxu:215": " tasmai namas+astu te yas+tvam+tannirvāṇāmṛtahradaḥ || 107",
    "GR13bhnstcxu:216": " kṛtrimā+api bhavadbhaktis+akṛtrimaphalodayā |",
    "GR13bhnstcxu:217": " niśchadmā cet+bhavet+eṣā kiṃphalā+iti tvayā+ucyatām || 108",
    "GR13bhnstcxu:218": " tat+cakṣus+īkṣyase yena sā gatis+gamyase yayā |",
    "GR13bhnstcxu:219": " phalam+tat+aja jātam+yat+tvatkathākalpapādapāt || 109",
    "GR13bhnstcxu:220": " śreyasā śreyas+eva+etat+upari tvayi yā sthitiḥ |",
    "GR13bhnstcxu:221": " tadantarāyahṛtaye tvam+īśa śaraṇam+mama || 110",
    "GR13bhnstcxu:222": " aho svādutamas+śarvasevāśaṃsāsudhārasaḥ |",
    "GR13bhnstcxu:223": " kutra kālakalāmātre na yas+navanavāyate || 111",
    "GR13bhnstcxu:224": " muhus+muhus+aviśrāntas+trailokyam+kalpanāśataiḥ |",
    "GR13bhnstcxu:225": " kalpayan+api kas+api+ekas+nirvikalpas+jayati+ajaḥ || 112",
    "GR13bhnstcxu:226": " malatailāktasaṃsāravāsanāvartidāhinā |",
    "GR13bhnstcxu:227": " jñānadīpena deva tvām+kadā nu syām+upasthitaḥ || 113",
    "GR13bhnstcxu:228": " nimeṣam+api yadi+ekam+kṣīṇadoṣe kariṣyasi |",
    "GR13bhnstcxu:229": " padam+citte tadā śambho kim+na sampādayiṣyasi || 114",
    "GR13bhnstcxu:230": " dhanyas+asmi kṛtakṛtyas+asmi mahān+asmi+iti bhāvanā |",
    "GR13bhnstcxu:231": " bhavet+sālambanā tasya yas+tvadālambanas+prabho || 115",
    "GR13bhnstcxu:232": " śubhāśubhasya sarvasya svayam+kartā bhavān+api |",
    "GR13bhnstcxu:233": " bhavadbhaktis+tu jananī śubhasya+eva+īśa kevalam || 116",
    "GR13bhnstcxu:234": " prasanne manasi svāmin+kim+tvam+niviśase kim+u |",
    "GR13bhnstcxu:235": " tvatpraveśāt+prasīdet+tat+iti dolāyate janaḥ || 117",
    "GR13bhnstcxu:236": " niścayas+punar+eṣas+atra tvadadhiṣṭhānam+eva hi |",
    "GR13bhnstcxu:237": " prasādas+manasas+svāmin+sā siddhis+tat+param+padam || 118",
    "GR13bhnstcxu:238": " vacas+cetas+ca kāryam+ca śarīram+mama yat+prabho |",
    "GR13bhnstcxu:239": " tvatprasādena tat+bhūyāt+bhavadbhāvaikabhūṣaṇam || 119",
    "GR13bhnstcxu:240": " stavacintāmaṇim+bhūrimanorathaphalapradam |",
    "GR13bhnstcxu:241": " bhaktilakṣmyālayam+śambhos+bhaṭṭanārāyaṇas+vyadhāt || 120"
}