{
    "XXmrtvts_u:0_0": "Mṛtyuvañcanatārāsādhana",
    "XXmrtvts_u:1_0": "// namaḥ sitatārāyai //",
    "XXmrtvts_u:mvts_1_0": "sitāravindamadhyasthāṃ tāṃbhūtāṃ candraviṣṭarām /",
    "XXmrtvts_u:mvts_1_1": "ābaddhavajraparyaṅkāṃ varadotpaladhāriṇīm //",
    "XXmrtvts_u:mvts_2_0": "śaraccandrakarākārāṃ pṛṣṭhacandrasamāśritām /",
    "XXmrtvts_u:mvts_2_1": "sarvālaṅkārasaṃpūrṇāṃ ṣoḍaśābdavapuḥkramām //",
    "XXmrtvts_u:mvts_3_0": "dhyātvāryatārāṃ hṛdaye tasyāś cakraṃ sitadyuti /",
    "XXmrtvts_u:mvts_3_1": "aṣṭakoṣṭhakam aṣṭābhir akṣaraiḥ paripūritam //",
    "XXmrtvts_u:mvts_4_0": "oṃhāvyañjanamadhyasthasādhyanāmādyanābhikam /",
    "XXmrtvts_u:mvts_4_1": "dhyāyād ekāgracittaḥ san ṣaṇ māsān dṛḍhaniścayaḥ //",
    "XXmrtvts_u:mvts_5_0": "japed akhinnacittaḥ san mantram enaṃ daśākṣaram //",
    "XXmrtvts_u:mvts_5_1": "tatrāiṣa mantraḥ oṃ tāre tuttāre ture svāhā //",
    "XXmrtvts_u:mvts_6_0": "mṛtyudoṣair na liptaḥ syāt tārāhṛccakrabhāvakaḥ /",
    "XXmrtvts_u:mvts_6_1": "lābhī ca sarvasiddhīnāṃ ṣaṇmāsāvadhi tatparaḥ //",
    "XXmrtvts_u:2_0": "// mṛtyuvañcanatārāsādhanam //"
}