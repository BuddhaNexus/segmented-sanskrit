{
    "GK16vamkalsu:0": "pāṇḍitavaraśrīvāmanaviracitāni kāvyālaṅkārasūtrāṇi svavṛttisametāni",
    "GK16vamkalsu:1": "śārīraṃ nāma prathamādhikaraṇam /",
    "GK16vamkalsu:2": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:3": "praṇamya paramaṃ jyotirvāmanena kavipriyā /",
    "GK16vamkalsu:4": "kāvyālaṅkārasūtrāṇāṃ teṣāṃ vṛttirvidhīyate //",
    "GK16vamkalsu:5": "kāvyaṃ grāhyamalaṅkārāt // VKal_1,1.1 //",
    "GK16vamkalsu:6": "saundaryamalaṅkāraḥ // VKal_1,1.2 //",
    "GK16vamkalsu:7": "sa doṣaguṇālaṅkārahānādānābhyām // VKal_1,1.3 //",
    "GK16vamkalsu:8": "śāstrataste // VKal_1,1.4 //",
    "GK16vamkalsu:9": "kāvyaṃ saddṛṣṭādṛṣṭārtham, prītikīrtihetutvāt // VKal_1,1.5 //*",
    "GK16vamkalsu:10": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:11": "arocakinaḥ satṛṇābhyavahāriṇaśca kavayaḥ // VKal_1,2.1 //",
    "GK16vamkalsu:12": "pūrve śiṣyāḥ, vivekitvāt // VKal_1,2.2 //*",
    "GK16vamkalsu:13": "netare, tadviparyayāt // VKal_1,2.3 //*",
    "GK16vamkalsu:14": "na śāstramadravyeṣvarthavat // VKal_1,2.4 //",
    "GK16vamkalsu:15": "na katakaṃ paṅkaprasādanāya // VKal_1,2.5 //",
    "GK16vamkalsu:16": "rītirātmā kāvyasya // VKal_1,2.6 //",
    "GK16vamkalsu:17": "viśiṣṭā padaracanā rītiḥ // VKal_1,2.7 //",
    "GK16vamkalsu:18": "viśeṣo guṇātmā // VKal_1,2.8 //",
    "GK16vamkalsu:19": "sā tridhā --- vaidarbhī gauḍīyā pāñcālī ceti // VKal_1,2.9 //",
    "GK16vamkalsu:20": "vidarbhādiṣu dṛṣṭatvāttatsamākhyā // VKal_1,2.10 //",
    "GK16vamkalsu:21": "samagraguṇopetā vaidarbhī // VKal_1,2.11 //",
    "GK16vamkalsu:22": "ojaḥkāntimatī gauḍīyā // VKal_1,2.12 //",
    "GK16vamkalsu:23": "mādhuryasaukumāryopapannā pāñcālī // VKal_1,2.13 //",
    "GK16vamkalsu:24": "tāsāṃ pūrvā grāhyā, guṇasākalyāt // VKal_1,2.14 //*",
    "GK16vamkalsu:25": "na punaritare, stokaguṇatvāt // VKal_1,2.15 //*",
    "GK16vamkalsu:26": "tadārohaṇārthamitarābhyāsa ityeke // VKal_1,2.16 //",
    "GK16vamkalsu:27": "tattu na; atattvaśīlasya tattvāniṣpatteḥ // VKal_1,2.17 //*",
    "GK16vamkalsu:28": "na śaṇasūtravānābhyāse trasarasūtravānavaicitryalābhaḥ // VKal_1,2.18 //",
    "GK16vamkalsu:29": "sāpi samāsābhāve śuddhavaidarbhī // VKal_1,2.19 //",
    "GK16vamkalsu:30": "tasyāmarthaguṇasaṃpadāsvādyā // VKal_1,2.20 //",
    "GK16vamkalsu:31": "tadupārohādarthaguṇaleśo 'pi // VKal_1,2.21 //",
    "GK16vamkalsu:32": "sāpi vaidarbhī, tātsthyāt // VKal_1,2.22 //*",
    "GK16vamkalsu:33": "tṛtīyo 'dhyāyaḥ /",
    "GK16vamkalsu:34": "loko vidyā prakīrṇa ca kāvyāṅgāni // VKal_1,3.1 //",
    "GK16vamkalsu:35": "lokavṛttaṃ lokaḥ // VKal_1,3.2 //",
    "GK16vamkalsu:36": "śabdasmṛtyabhidhānakośacchandovicitikalākāmaśāstradaṇḍanītipūrvā vidyāḥ //",
    "GK16vamkalsu:37": "śabdasmṛteḥ śabdaśuddhiḥ // VKal_1,3.4 //",
    "GK16vamkalsu:38": "abhidhānakośāt padārthaniścayaḥ // VKal_1,3.5 //",
    "GK16vamkalsu:39": "chandovicitervṛttasaṃśayacchedaḥ // VKal_1,3.6 //",
    "GK16vamkalsu:40": "kalāśāstrebhyaḥ kalātattvasya saṃvit // VKal_1,3.7 //",
    "GK16vamkalsu:41": "kāmaśāstrataḥ kāmopacārasya // VKal_1,3.8 //",
    "GK16vamkalsu:42": "daṇḍanīternayāpanayayoḥ // VKal_1,3.9 //",
    "GK16vamkalsu:43": "itivṛttakuṭilatvaṃ ca tataḥ // VKal_1,3.10 //",
    "GK16vamkalsu:44": "lakṣyajñatvamabhiyogo vṛddhasevāvekṣaṇaṃ pratibhānamavadhānaṃ ca prakīrṇam // VKal_1,3.11",
    "GK16vamkalsu:45": "tatra kāvyaparicayo lakṣyajñatvam // VKal_1,3.12 //",
    "GK16vamkalsu:46": "kāvyabandhodyamo 'bhiyogaḥ // VKal_1,3.13 //",
    "GK16vamkalsu:47": "kāvyopadeśaguruśuśrūṣaṇaṃ vṛddhasevā // VKal_1,3.14 //",
    "GK16vamkalsu:48": "padādhānoddharaṇamavekṣaṇam // VKal_1,3.15 //",
    "GK16vamkalsu:49": "kavitvabījaṃ pratibhānam // VKal_1,3.16 //",
    "GK16vamkalsu:50": "cittaikāgryamavadhānam // VKal_1,3.17 //",
    "GK16vamkalsu:51": "taddeśakālābhyām // VKal_1,3.18 //",
    "GK16vamkalsu:52": "vivikto deśaḥ // VKal_1,3.19 //",
    "GK16vamkalsu:53": "rātriyāmasturīyaḥ kālaḥ // VKal_1,3.20 //",
    "GK16vamkalsu:54": "kāvyaṃ gadyaṃ padyaṃ ca // VKal_1,3.21 //",
    "GK16vamkalsu:55": "gadyaṃ vṛttagandhi cūrṇamutkalikāprāyaṃ ca // VKal_1,3.22 //",
    "GK16vamkalsu:56": "padyabhāgavadvṛttagandhi // VKal_1,3.23 //",
    "GK16vamkalsu:57": "anāviddhalalitapadaṃ cūrṇam // VKal_1,3.24 //",
    "GK16vamkalsu:58": "viparītamutkalikāprāyam // VKal_1,3.25 //",
    "GK16vamkalsu:59": "padyamanekabhedam // VKal_1,3.26 //",
    "GK16vamkalsu:60": "tadanibaddhaṃ nibaddhaṃ ca // VKal_1,3.27 //",
    "GK16vamkalsu:61": "kramasiddhistayoḥ sraguttaṃsavat // VKal_1,3.28 //",
    "GK16vamkalsu:62": "nānibaddhaṃ cakāstyekatejaḥparamāṇuvat // VKal_1,3.29 //",
    "GK16vamkalsu:63": "saṃdarbheṣu daśarūpakaṃ śreyaḥ // VKal_1,3.30 //",
    "GK16vamkalsu:64": "taddhi citraṃ citrapaṭavadviśeṣasākalyāt // VKal_1,3.31 //",
    "GK16vamkalsu:65": "tato 'nyabhedakḷptiḥ // VKal_1,3.32 //",
    "GK16vamkalsu:66": "samāptaṃ cedaṃ śārīraṃ prathamamadhikaraṇam /",
    "GK16vamkalsu:67": "doṣadarśanaṃ nāma dvitīyamadhikaraṇam /",
    "GK16vamkalsu:68": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:69": "guṇaviparyayātmāno doṣāḥ // VKal_2,1.1 //",
    "GK16vamkalsu:70": "arthatastadavagamaḥ // VKal_2,1.2 //",
    "GK16vamkalsu:71": "saukaryāya prapañcaḥ // VKal_2,1.3 //",
    "GK16vamkalsu:72": "duṣṭaṃ padamasādhu kaṣṭaṃ grāmyamapratītamanarthakaṃ ca // VKal_2,1.4 //",
    "GK16vamkalsu:73": "śabdasmṛtiviruddhamasādhu // VKal_2,1.5 //",
    "GK16vamkalsu:74": "śrutivirasaṃ kaṣṭam // VKal_2,1.6 //",
    "GK16vamkalsu:75": "lokamātraprayuktaṃ grāmyam // VKal_2,1.7 //",
    "GK16vamkalsu:76": "śāstramātraprayuktamapratītam // VKal_2,1.8 //",
    "GK16vamkalsu:77": "pūraṇārthamanarthakam // VKal_2,1.9 //",
    "GK16vamkalsu:78": "na vākyālaṅkārārtham // VKal_2,1.10 //",
    "GK16vamkalsu:79": "anyārthaneyagūḍhārthāślīlakliṣṭāni ca // VKal_2,1.11 //",
    "GK16vamkalsu:80": "rūḍhicyutamanyārtham // VKal_2,1.12 //",
    "GK16vamkalsu:81": "kalpitārthaṃ neyārtham // VKal_2,1.13 //",
    "GK16vamkalsu:82": "aprasiddhārthaprayuktaṃ gūḍhārtham // VKal_2,1.14 //",
    "GK16vamkalsu:83": "asamyārthāntaramasamyasmṛtihetuścāślīlam // VKal_2,1.15 //",
    "GK16vamkalsu:84": "na guptalakṣitasaṃvṛtāni // VKal_2,1.16 //",
    "GK16vamkalsu:85": "aprasiddhāsabhyaṃ guptam // VKal_2,1.17 //",
    "GK16vamkalsu:86": "lākṣaṇikāsabhyānvitaṃ lakṣitam // VKal_2,1.18 //",
    "GK16vamkalsu:87": "lokasaṃvītaṃ saṃvṛtam // VKal_2,1.19 //",
    "GK16vamkalsu:88": "tantraividhyam, vrīḍājugupsāmaṅgalātaṅkadāyibhedāt // VKal_2,1.20 //*",
    "GK16vamkalsu:89": "vyavahitārthapratyayaṃ kliṣṭam // VKal_2,1.21 //",
    "GK16vamkalsu:90": "antyābhyāṃ vākyaṃ vyākhyātam // VKal_2,1.22 //",
    "GK16vamkalsu:91": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:92": "bhinnavṛttayatibhraṣṭavisaṃdhīni vākyāni // VKal_2,2.1 //",
    "GK16vamkalsu:93": "svalakṣaṇacyutavṛttaṃ bhinnavṛttam // VKal_2,2.2 //",
    "GK16vamkalsu:94": "virasavirāmaṃ yatibhraṣṭam // VKal_2,2.3 //",
    "GK16vamkalsu:95": "taddhātunāmabhāgabhede svarasaṃdhyakṛte prāyeṇa // VKal_2,2.4 //",
    "GK16vamkalsu:96": "na vṛttadoṣātpṛthagyatidpṣaḥ, vṛttasya yatyātmakatvāt // VKal_2,2.5 //*",
    "GK16vamkalsu:97": "na, lakṣaṇaḥ pṛthaktvāt // VKal_2,2.6 //*",
    "GK16vamkalsu:98": "virūpapadasaṃdhi visaṃdhiḥ // VKal_2,2.7 //",
    "GK16vamkalsu:99": "padasaṃdhervairūpyaṃ viśleṣo 'ślīlatvaṃ kaṣṭatvaṃ ca // VKal_2,2.8 //",
    "GK16vamkalsu:100": "vyarthaikārthasaṃdigdhāyuktāpakramalokavidyā viruddhāni ca // VKal_2,2.9",
    "GK16vamkalsu:101": "vyāhatapūrvottarārthaṃ vyartham // VKal_2,2.10 //",
    "GK16vamkalsu:102": "uktārthapadamekārtham // VKal_2,2.11 //",
    "GK16vamkalsu:103": "na, viśeṣaścedekārthaṃ duṣṭam // VKal_2,2.12 //*",
    "GK16vamkalsu:104": "dhanurjyādhvanau dhanuḥśrutirārūḍheḥ pratipattyai // VKal_2,2.13 //",
    "GK16vamkalsu:105": "karṇāvataṃsaśravaṇakuṇḍalaśiraḥśekhareṣu karṇādinirdeśaḥ saṃnidheḥ //",
    "GK16vamkalsu:106": "muktāhāraśabde muktāśabdaḥ śuddheḥ // VKal_2,2.15 //",
    "GK16vamkalsu:107": "puṣpamālāśabde puṣpapadamutkarṣasya // VKal_2,2.16 //",
    "GK16vamkalsu:108": "karikalabhaśabde kariśabdastādrūpyasya // VKal_2,2.17 //",
    "GK16vamkalsu:109": "viśeṣaṇasya ca // VKal_2,2.18 //",
    "GK16vamkalsu:110": "tadidaṃ prayukteṣu // VKal_2,2.19 //",
    "GK16vamkalsu:111": "saṃśayakṛtsaṃdigdham // VKal_2,2.20 //",
    "GK16vamkalsu:112": "māyādivikalpitārthamprayuktam // VKal_2,2.21 //",
    "GK16vamkalsu:113": "kramahīnārthamapakramam // VKal_2,2.22 //",
    "GK16vamkalsu:114": "deśakālasvabhāvaviruddhārthāni lokaviruddhāni // VKal_2,2.23 //",
    "GK16vamkalsu:115": "kalācaturvargaśāstraviruddhārthāni vidyāviruddhāni // VKal_2,2.24 //",
    "GK16vamkalsu:116": "samāotaṃ cedaṃ doṣadarśanam dvitīyamadhikaraṇam /",
    "GK16vamkalsu:117": "guṇavivecanaṃ nāma tṛtīyamadhikaraṇam /",
    "GK16vamkalsu:118": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:119": "kāvyaśobhāyāḥ kartāro dharmā guṇāḥ // VKal_3,1.1 //",
    "GK16vamkalsu:120": "tadatiśayahetavastvalaṅkārāḥ // VKal_3,1.2 //",
    "GK16vamkalsu:121": "pūrve nityāḥ // VKal_3,1.3 //",
    "GK16vamkalsu:122": "ojaḥprasādaśleṣasamatāsamādhimādhuryasaukumāryodāratārthavyaktikāntayo bandhaguṇāḥ // VKal_3,1.4",
    "GK16vamkalsu:123": "gāḍhabandhatvamojaḥ // VKal_3,1.5 //",
    "GK16vamkalsu:124": "śaithilyaṃ prasādaḥ // VKal_3,1.6 //",
    "GK16vamkalsu:125": "guṇaḥ saṃplavāt // VKal_3,1.7 //",
    "GK16vamkalsu:126": "sa tvanubhavasiddhaḥ // VKal_3,1.8 //",
    "GK16vamkalsu:127": "sāmyotkarṣau ca // VKal_3,1.9 //",
    "GK16vamkalsu:128": "masṛṇatvaṃ śleṣaḥ // VKal_3,1.10 //",
    "GK16vamkalsu:129": "mārgāvhedaḥ samatā // VKal_3,1.11 //",
    "GK16vamkalsu:130": "ārohāvarohakramaḥ samādhiḥ // VKal_3,1.12 //",
    "GK16vamkalsu:131": "na pṛthak, ārohāvarohayorojaḥprasādarūpatvāt // VKal_3,1.13 //*",
    "GK16vamkalsu:132": "na, asaṃpṛktatvāt // VKal_3,1.14 //*",
    "GK16vamkalsu:133": "anaikāntyācca // VKal_3,1.15 //",
    "GK16vamkalsu:134": "ojaḥprasādayoḥ tīvrāvasthā, tāviti cedabhyupagamaḥ // VKal_3,1.16 //*",
    "GK16vamkalsu:135": "viśeṣāpekṣitvāttayoḥ // VKal_3,1.17 //",
    "GK16vamkalsu:136": "ārohāvarohanimittaṃ samādhirākhyāyate // VKal_3,1.18 //",
    "GK16vamkalsu:137": "kramavidhānārthaṃ vā // VKal_3,1.19 //",
    "GK16vamkalsu:138": "pṛthakpadatvaṃ mādhuryam // VKal_3,1.20 //",
    "GK16vamkalsu:139": "ajaraṭhatvaṃ saukumāryam // VKal_3,1.21 //",
    "GK16vamkalsu:140": "vikaṭatvamudāratā // VKal_3,1.22 //",
    "GK16vamkalsu:141": "arthavyaktihetutvamarthavyaktiḥ // VKal_3,1.23 //",
    "GK16vamkalsu:142": "aujjvalyaṃ kāntiḥ // VKal_3,1.24 //",
    "GK16vamkalsu:143": "nāsantaḥ, sadvedyatvāt // VKal_3,1.25 //*",
    "GK16vamkalsu:144": "na bhrāntāḥ, niṣkampatvāt // VKal_3,1.26 //*",
    "GK16vamkalsu:145": "na pāṭhadharmāḥ, sarvatrādṛṣṭeḥ // VKal_3,1.27 //*",
    "GK16vamkalsu:146": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:147": "ta evārthaguṇāḥ // VKal_3,2.1 //",
    "GK16vamkalsu:148": "arthasya prauḍhirojaḥ // VKal_3,2.2 //",
    "GK16vamkalsu:149": "arthavaimalyaṃ prasādaḥ // VKal_3,2.3 //",
    "GK16vamkalsu:150": "ghaṭanā śleṣaḥ // VKal_3,2.4 //",
    "GK16vamkalsu:151": "avaiṣamyaṃ samatā // VKal_3,2.5 //",
    "GK16vamkalsu:152": "arthadṛṣṭiḥ samādhiḥ // VKal_3,2.6 //",
    "GK16vamkalsu:153": "artho dvividhaḥ, ayoniranyacchāyāyonirvā // VKal_3,2.7 //*",
    "GK16vamkalsu:154": "artho vyaktaḥ sūkṣmaśca // VKal_3,2.8 //",
    "GK16vamkalsu:155": "sūkṣmo bhāvyo vāsanīyaśca // VKal_3,2.9 //",
    "GK16vamkalsu:156": "uktivaicitryaṃ mādhuryam // VKal_3,2.10 //",
    "GK16vamkalsu:157": "apāruṣyaṃ saukumāryam // VKal_3,2.11 //",
    "GK16vamkalsu:158": "agrāmyatvamudāratā // VKal_3,2.12 //",
    "GK16vamkalsu:159": "vastusvabhāvasphuṭatvamarthavyaktiḥ // VKal_3,2.13 //",
    "GK16vamkalsu:160": "dīptarasatvaṃ kāntiḥ // VKal_3,2.14 //",
    "GK16vamkalsu:161": "samāotaṃ cedaṃ guṇavivecanaṃ tṛtīyamadhikaraṇam /",
    "GK16vamkalsu:162": "ālaṅkārikaṃ nāma caturthamadhikaraṇam /",
    "GK16vamkalsu:163": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:164": "padamanaikārthamakṣaraṃ cāvṛttaṃ sthānaniyame yamakam // VKal_4,1.1 //",
    "GK16vamkalsu:165": "pādāḥ pādasyaikasyānekasya cādimadhyāntabhāgāḥ sthānāni // VKal_4,1.2 //",
    "GK16vamkalsu:166": "bhaṅgādutkarṣaḥ // VKal_4,1.3 //",
    "GK16vamkalsu:167": "śṛṅkhalā parivartakaścūrṇamiti bhaṅgamārgaḥ // VKal_4,1.4 //",
    "GK16vamkalsu:168": "varṇavicchedacalanaṃ śṛṅkhalā // VKal_4,1.5 //",
    "GK16vamkalsu:169": "saṅgavinivṛttau svarūpāpattiḥ parivartakaḥ // VKal_4,1.6 //",
    "GK16vamkalsu:170": "piṇḍākṣarabhede svarūpalopaścūrṇam // VKal_4,1.7 //",
    "GK16vamkalsu:171": "śeṣaḥ sarūpo 'nuprāsaḥ // VKal_4,1.8 //",
    "GK16vamkalsu:172": "anulbaṇo varṇānuprāsaḥ śreyān // VKal_4,1.9 //",
    "GK16vamkalsu:173": "pādānuprāsaḥ pādayamakavat // VKal_4,1.10 //",
    "GK16vamkalsu:174": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:175": "upamānenopameyasya guṇaleśataḥ sāmyamupamā // VKal_4,2.1 //",
    "GK16vamkalsu:176": "guṇabāhulyataśca kalpitā // VKal_4,2.2 //",
    "GK16vamkalsu:177": "taddvaividhyam, padavākyārthavṛttibhedāt // VKal_4,2.3 //*",
    "GK16vamkalsu:178": "sā pūṇā luptā ca // VKal_4,2.4 //",
    "GK16vamkalsu:179": "guṇadyotakopamānopameyaśabdānāṃ sāmagrye pūrṇā // VKal_4,2.5 //",
    "GK16vamkalsu:180": "lope luptā // VKal_4,2.6 //",
    "GK16vamkalsu:181": "stutinindātattvākhyāneṣu // VKal_4,2.7 //",
    "GK16vamkalsu:182": "hīnatvādhikatvaliṅgavacanabhedāsādṛśyāsaṃbhavāstaddoṣāḥ // VKal_4,2.8 //",
    "GK16vamkalsu:183": "jātipramāṇadharmanyūnatopamānasya hīnatvam // VKal_4,2.9 //",
    "GK16vamkalsu:184": "dharmayorekanirdeśe 'nyasya saṃvit, sāhacaryāt // VKal_4,2.10 //*",
    "GK16vamkalsu:185": "tenādhikarvaṃ vyākhyātam // VKal_4,2.11 //",
    "GK16vamkalsu:186": "upamānopameyayorliṅgavyatyāso liṅgabhedaḥ // VKal_4,2.12 //",
    "GK16vamkalsu:187": "iṣṭaḥ puṃpuṃsakayoḥ prāyeṇa // VKal_4,2.13 //",
    "GK16vamkalsu:188": "laukikyāṃ samāsābhihitāyāmupamāprapañce ca // VKal_4,2.14 //",
    "GK16vamkalsu:189": "tena vacanabhedo vyākhyātaḥ // VKal_4,2.15 //",
    "GK16vamkalsu:190": "apratītaguṇasādṛśyamasādṛśyam // VKal_4,2.16 //",
    "GK16vamkalsu:191": "asādṛśyahatā hyupamā, tanniṣṭhāśca kavayaḥ // VKal_4,2.17 //*",
    "GK16vamkalsu:192": "upamānādhikyāttadapoha ityeke // VKal_4,2.18 //",
    "GK16vamkalsu:193": "na, apuṣṭārthatvāt // VKal_4,2.19 //*",
    "GK16vamkalsu:194": "anupapattirasaṃbhavaḥ // VKal_4,2.20 //",
    "GK16vamkalsu:195": "na viruddho 'tiśayaḥ // VKal_4,2.21 //",
    "GK16vamkalsu:196": "tṛtīyo 'dhyāyaḥ /",
    "GK16vamkalsu:197": "prativastuprabhṛtirupamāprapañcaḥ // VKal_4,3.1 //",
    "GK16vamkalsu:198": "upameyasyoktau samānavastunyāsaḥ prativastu // VKal_4,3.2 //",
    "GK16vamkalsu:199": "anuktau samāsoktiḥ // VKal_4,3.3 //",
    "GK16vamkalsu:200": "kiñciduktāvaprastutapraśaṃsā // VKal_4,3.4 //",
    "GK16vamkalsu:201": "samena vastunānyāpalāpo 'pahnutiḥ // VKal_4,3.5 //",
    "GK16vamkalsu:202": "upamānenopameyasya guṇasāmyāttattvāropo rūpakam // VKal_4,3.6 //",
    "GK16vamkalsu:203": "sa dharmeṣu tantraprayoge śleṣaḥ // VKal_4,3.7 //",
    "GK16vamkalsu:204": "sādṛśyāllakṣaṇā vakroktiḥ // VKal_4,3.8 //",
    "GK16vamkalsu:205": "atadrūpasyānyathādhyavasānamatiśayārthamutprekṣā // VKal_4,3.9 //",
    "GK16vamkalsu:206": "saṃbhāvyadharmatadutkarṣakalpanātiśayoktiḥ // VKal_4,3.10 //",
    "GK16vamkalsu:207": "upamānopameyasaṃśayaḥ saṃdehaḥ // VKal_4,3.11 //",
    "GK16vamkalsu:208": "viruddhābhāsatvaṃ virodhaḥ // VKal_4,3.12 //",
    "GK16vamkalsu:209": "kriyāpratiṣedhe prasiddhatatphalavyaktirvibhāvanā // VKal_4,3.13 //",
    "GK16vamkalsu:210": "ekasyopameyatvopamānatve 'nanvayaḥ // VKal_4,3.14 //",
    "GK16vamkalsu:211": "krameṇopameyopamā // VKal_4,3.15 //",
    "GK16vamkalsu:212": "samavisadṛśābhyāṃ parivartanaṃ parivṛttiḥ // VKal_4,3.16 //",
    "GK16vamkalsu:213": "upameyopamānānāṃ kramasaṃbandhaḥ kramaḥ // VKal_4,3.17 //",
    "GK16vamkalsu:214": "upamānopameyavākyeṣvekā kriyā dīpakam // VKal_4,3.18 //",
    "GK16vamkalsu:215": "tantrividham /",
    "GK16vamkalsu:216": "ādimadhyāntavākyavṛttibhedāt // VKal_4,3.19 //*",
    "GK16vamkalsu:217": "kriyayaiva svatadarthānvayakhyāpanaṃ nidarśanam // VKal_4,3.20 //",
    "GK16vamkalsu:218": "uktisiddhyai vastuno 'rthāntarasyaiva nyasanamarthāntaranyāsaḥ //",
    "GK16vamkalsu:219": "upameyasya guṇātirekatvaṃ vyatirekaḥ // VKal_4,3.22 //",
    "GK16vamkalsu:220": "ekaguṇahānikalpanāyāṃ sāmyadārḍhyaṃ viśeṣoktiḥ // VKal_4,3.23 //",
    "GK16vamkalsu:221": "saṃbhāvyaviśiṣṭakarmākaraṇānnindāstotrārthā vyājastutiḥ // VKal_4,3.24 //",
    "GK16vamkalsu:222": "vyājasya satyasārūpyaṃ vyājoktiḥ // VKal_4,3.25 //",
    "GK16vamkalsu:223": "viśiṣṭena sāmyārthamekakālakriyāyogastulyayogitā // VKal_4,3.26 //",
    "GK16vamkalsu:224": "upamānākṣepaścākṣepaḥ // VKal_4,3.27 //",
    "GK16vamkalsu:225": "vastudvayakriyayostulyakālayorekapadābhidhānaṃ sahoktiḥ // VKal_4,3.28 //",
    "GK16vamkalsu:226": "yatsādṛśyaṃ tatsaṃpattiḥ samāhitam // VKal_4,3.29 //",
    "GK16vamkalsu:227": "alaṅkārasyālaṅkārayonitvaṃ saṃsṛṣṭiḥ // VKal_4,3.30 //",
    "GK16vamkalsu:228": "tadbhedāvupamārūpakotprekṣāvayavau // VKal_4,3.31 //",
    "GK16vamkalsu:229": "upamājanyaṃ rūpakamupamārūpakam // VKal_4,3.32 //",
    "GK16vamkalsu:230": "utprekṣāheturutprekṣāvayavaḥ // VKal_4,3.33 //",
    "GK16vamkalsu:231": "samāotaṃ cedamālaṅkārikaṃ caturthamadhikaraṇam /",
    "GK16vamkalsu:232": "prāyogikaṃ nāma pañcamamadhikaraṇam /",
    "GK16vamkalsu:233": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:234": "naikaṃ padaṃ dviḥ prayojyaṃ prāyeṇa // VKal_5,1.1 //",
    "GK16vamkalsu:235": "nityā saṃhitaikapadavatpādeṣvardhāntavarjam // VKal_5,1.2 //",
    "GK16vamkalsu:236": "na pādāntalaghorgurutvaṃ ca sarvatra // VKal_5,1.3 //",
    "GK16vamkalsu:237": "na gādye samāptaprāyaṃ vṛttam, anyatrodgatādibhyaḥ saṃvādāt // VKal_5,1.4",
    "GK16vamkalsu:238": "na pādādau khalvādayaḥ // VKal_5,1.5 //",
    "GK16vamkalsu:239": "nārdhe kiñcitsamāptaṃ vākyam // VKal_5,1.6 //",
    "GK16vamkalsu:240": "na karmadhārayo bahuvrīhipratipattikaraḥ // VKal_5,1.7 //",
    "GK16vamkalsu:241": "tena viparyayo vyākhyātaḥ // VKal_5,1.8 //",
    "GK16vamkalsu:242": "saṃbhāvyaniṣedhanivartane dvau pratiṣedhau // VKal_5,1.9 //",
    "GK16vamkalsu:243": "viśeṣaṇamātraprayogo viśeṣyapratipattau // VKal_5,1.10 //",
    "GK16vamkalsu:244": "sarvanāmnānusaṃdhirvṛtticchannasya // VKal_5,1.11 //",
    "GK16vamkalsu:245": "saṃbandhasaṃbandhe 'pi ṣaṣṭhī kvacit // VKal_5,1.12 //",
    "GK16vamkalsu:246": "atiprayuktaṃ deśabhāṣāpadam // VKal_5,1.13 //",
    "GK16vamkalsu:247": "liṅgādhyāhārau // VKal_5,1.14 //",
    "GK16vamkalsu:248": "lakṣaṇāśabdāśca // VKal_5,1.15 //",
    "GK16vamkalsu:249": "na tadbāhulyamekatra // VKal_5,1.16 //",
    "GK16vamkalsu:250": "stanādīnāṃ dvitvāviṣṭā jātiḥ prāyeṇa // VKal_5,1.17 //",
    "GK16vamkalsu:251": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:252": "rudrāvityekaśeṣo 'nveṣyaḥ // VKal_5,2.1 //",
    "GK16vamkalsu:253": "miliklabikṣapiprabhṛtīnāṃ dhātutvam, dhātuguṇasyāsamāpteḥ // VKal_5,2.2",
    "GK16vamkalsu:254": "valerātmanepadamanityam, jñāpakāt // VKal_5,2.3 //*",
    "GK16vamkalsu:255": "kṣīyata iti karmakartari // VKal_5,2.4 //",
    "GK16vamkalsu:256": "khidyata iti ca // VKal_5,2.5 //",
    "GK16vamkalsu:257": "mārgerātmaepadamalakṣma // VKal_5,2.6 //",
    "GK16vamkalsu:258": "lolamānādayaścānaśi // VKal_5,2.7 //",
    "GK16vamkalsu:259": "labhergatyarthatvāṇṇicyaṇau kartuḥ karmatvākarmatve // VKal_5,2.8 //",
    "GK16vamkalsu:260": "te-me-śabdau nipāteṣu // VKal_5,2.9 //*",
    "GK16vamkalsu:261": "tiraskṛta iti paribhūte, antardhyupacārāt // VKal_5,2.10 //*",
    "GK16vamkalsu:262": "naikaśabdaḥ, supsupeti samāsāt // VKal_5,2.11 //*",
    "GK16vamkalsu:263": "madhupipāsuprabhṛtīnāṃ samāso gamigāmyādiṣu pāṭhāt // VKal_5,2.12 //",
    "GK16vamkalsu:264": "trivalīśabdaḥ siddhaḥ saṃjñā cet // VKal_5,2.13 //",
    "GK16vamkalsu:265": "bimbādhara iti vṛttau madhyamapadalopinyām // VKal_5,2.14 //",
    "GK16vamkalsu:266": "āmūlalolādiṣu vṛttirvispaṣṭapaṭuvat // VKal_5,2.15 //",
    "GK16vamkalsu:267": "na dhānyaṣaṣṭhādiṣu ṣaṣṭhīsamāsapratiṣedhaḥ, pūraṇena taddhitāntatvāt //",
    "GK16vamkalsu:268": "patrapītimādiṣu guṇavacanena // VKal_5,2.17 //",
    "GK16vamkalsu:269": "avarjyo bahuvrīhirvyadhikaraṇo janmādyuttarapadaḥ // VKal_5,2.18 //",
    "GK16vamkalsu:270": "hastāgrāgrahastādayo guṇaguṇinorbhedābhedāt // VKal_5,2.19 //",
    "GK16vamkalsu:271": "pūrvanipāte 'pabhraṃśo rakṣyaḥ // VKal_5,2.20 //",
    "GK16vamkalsu:272": "nipātenāpyabhihite karmaṇi na karmavibhaktiḥ, parigaṇanasya prāyikatvāt //",
    "GK16vamkalsu:273": "śakyamiti rūpaṃ karmābhidhāyāṃ liṅgavacanasyāpi sāmānyopakramāt //",
    "GK16vamkalsu:274": "hānivadādhikyamapyaṅgānāṃ vikāraḥ // VKal_5,2.23 //",
    "GK16vamkalsu:275": "na kṛmikīṭānāmiti, ekavadbhāvaprasaṅgāt // VKal_5,2.24 //*",
    "GK16vamkalsu:276": "na kharoṣṭrau, uṣṭrakharamiti pāṭhāt // VKal_5,2.25 //*",
    "GK16vamkalsu:277": "āsetyasateḥ // VKal_5,2.26 //",
    "GK16vamkalsu:278": "yudhyediti yudhaḥ kyaci // VKal_5,2.27 //",
    "GK16vamkalsu:279": "viralāyamānādiṣu kyaṅ nirūpyaḥ // VKal_5,2.28 //",
    "GK16vamkalsu:280": "ahetau hanterṇic, curādipāṭhāt // VKal_5,2.29 //*",
    "GK16vamkalsu:281": "anucarīti careṣṭittvāt // VKal_5,2.30 //",
    "GK16vamkalsu:282": "kesarālamityalateraṇi // VKal_5,2.31 //",
    "GK16vamkalsu:283": "patralamiti lāteḥ ke // VKal_5,2.32 //",
    "GK16vamkalsu:284": "mahīdhrādayo mūlavibhujādidarśanāt // VKal_5,2.33 //",
    "GK16vamkalsu:285": "brahmādiṣu hanterniyamādarihādyasiddhiḥ // VKal_5,2.34 //",
    "GK16vamkalsu:286": "brahmavidādayaḥ kṛdantavṛttyā // VKal_5,2.35 //",
    "GK16vamkalsu:287": "tairmahīdharādayo vyākhyātāḥ // VKal_5,2.36 //",
    "GK16vamkalsu:288": "bhidurādayaḥ karmakartari kartari ca // VKal_5,2.37 //",
    "GK16vamkalsu:289": "guṇavistarādayaścintyāḥ // VKal_5,2.38 //",
    "GK16vamkalsu:290": "avatarāpacāyaśabdayordīrghahrasvatvavyatyāso bālānām // VKal_5,2.39 //",
    "GK16vamkalsu:291": "śobheti nipātanāt // VKal_5,2.40 //",
    "GK16vamkalsu:292": "avidhau guroḥ striyāṃ bahulaṃ vivakṣā // VKal_5,2.41 //",
    "GK16vamkalsu:293": "vyavasitādiṣu ktaḥ kartari, cakārāt // VKal_5,2.42 //*",
    "GK16vamkalsu:294": "āheti bhūte 'nyaṇalantabhramād bruvo laṭi // VKal_5,2.43 //",
    "GK16vamkalsu:295": "śabalādibhyaḥ striyāṃ ṭāpo 'prāptiḥ // VKal_5,2.44 //",
    "GK16vamkalsu:296": "prāṇini nīleti cintyam // VKal_5,2.45 //",
    "GK16vamkalsu:297": "manuṣyajātervivakṣāvivakṣe // VKal_5,2.46 //",
    "GK16vamkalsu:298": "ūkārāntādapyūṅ, pravṛtteḥ // VKal_5,2.47 //*",
    "GK16vamkalsu:299": "kārtikīya iti ṭhañ durdharaḥ // VKal_5,2.48 //",
    "GK16vamkalsu:300": "śārvaramiti ca // VKal_5,2.49 //",
    "GK16vamkalsu:301": "śāśvatamiti prayukteḥ // VKal_5,2.50 //",
    "GK16vamkalsu:302": "rājavaṃśyādayaḥ sādhvarthe ye bhavanti // VKal_5,2.51 //",
    "GK16vamkalsu:303": "dāravaśabdo duṣprayuktaḥ // VKal_5,2.52 //",
    "GK16vamkalsu:304": "mugdhimādiṣvivamanij mṛgyaḥ // VKal_5,2.53 //",
    "GK16vamkalsu:305": "aupamyādayaścāturvarṇyavat // VKal_5,2.54 //",
    "GK16vamkalsu:306": "ṣyañaḥ ṣitkaraṇādīkāro bahulam // VKal_5,2.55 //",
    "GK16vamkalsu:307": "dhanvīti vrīhyādiṣu pāṭhāt // VKal_5,2.56 //",
    "GK16vamkalsu:308": "caturasraśobhīti ṇinau // VKal_5,2.57 //",
    "GK16vamkalsu:309": "kañcukīyā iti kyaci // VKal_5,2.58 //",
    "GK16vamkalsu:310": "bauddhapratiyogyapekṣāyāmapyātiśāyanikāḥ // VKal_5,2.59 //",
    "GK16vamkalsu:311": "kauśilādaya ilaci varṇalopāt // VKal_5,2.60 //",
    "GK16vamkalsu:312": "mauktikamiti vinayādipāṭhāt // VKal_5,2.61 //",
    "GK16vamkalsu:313": "prātibhādayaḥ prajñādiṣu // VKal_5,2.62 //",
    "GK16vamkalsu:314": "na sarajasamityanavyayībhāve // VKal_5,2.63 //",
    "GK16vamkalsu:315": "dhṛtadhanuṣītyasaṃjñāyām // VKal_5,2.64 //",
    "GK16vamkalsu:316": "durgandhipada iddurlabhaḥ // VKal_5,2.65 //",
    "GK16vamkalsu:317": "sudatyādayaḥ pratividheyāḥ // VKal_5,2.66 //",
    "GK16vamkalsu:318": "kṣatadṛḍhorsa iti na kap, tadantavidhipratiṣedhāt // VKal_5,2.67 //*",
    "GK16vamkalsu:319": "avaihīti vṛddhiravadyā // VKal_5,2.68 //",
    "GK16vamkalsu:320": "apāṅganetreti lugalabhyaḥ // VKal_5,2.69 //",
    "GK16vamkalsu:321": "neṣṭhāḥ śliṣṭapriyādayaḥ, puṃvadbhāvapratiṣedhāt // VKal_5,2.70 //*",
    "GK16vamkalsu:322": "dṛḍhabhaktiriti sarvatreti // VKal_5,2.71 //",
    "GK16vamkalsu:323": "jambulatādayo hrasvavidheḥ // VKal_5,2.72 //",
    "GK16vamkalsu:324": "tilakādayo 'jirādiṣu // VKal_5,2.73 //",
    "GK16vamkalsu:325": "niśamya-niśamayyaśabdau prakṛtibhedāt // VKal_5,2.74 //",
    "GK16vamkalsu:326": "saṃyamyaniyamyaśabdau, aṇijantatvāt // VKal_5,2.75 //*",
    "GK16vamkalsu:327": "prapīyeti pīṅaḥ // VKal_5,2.76 //",
    "GK16vamkalsu:328": "dūrayatīti bahulagrahaṇāt // VKal_5,2.77 //",
    "GK16vamkalsu:329": "gacchatīprabhṛtiṣvaniṣedhyo num // VKal_5,2.78 //",
    "GK16vamkalsu:330": "mitreṇa goptreti puṃvadbhāvāt // VKal_5,2.79 //",
    "GK16vamkalsu:331": "vetsyasīti padabhaṅgāt // VKal_5,2.80 //",
    "GK16vamkalsu:332": "kāmayānaśabdaḥ siddho 'nādiścet // VKal_5,2.81 //",
    "GK16vamkalsu:333": "sauhṛd-daurhṛdaśabdāvaṇi hṛdbhāvāt // VKal_5,2.82 //",
    "GK16vamkalsu:334": "virama iti nipātanāt // VKal_5,2.83 //",
    "GK16vamkalsu:335": "uparyādiṣu sāmīpye dvirukteṣu dvitīyā // VKal_5,2.84 //",
    "GK16vamkalsu:336": "mandaṃ mandamityaprakārārthatve // VKal_5,2.85 //",
    "GK16vamkalsu:337": "na nidrādrugiti, bhaṣbhāvaprāpteḥ // VKal_5,2.86 //*",
    "GK16vamkalsu:338": "niṣpanda iti patvaṃ cintyam // VKal_5,2.87 //",
    "GK16vamkalsu:339": "nāṅgulisaṅga iti mūrdhanyavidheḥ // VKal_5,2.88 //",
    "GK16vamkalsu:340": "tenāvantisenādayaḥ pratyuktāḥ // VKal_5,2.89 //",
    "GK16vamkalsu:341": "nendravāhane ṇatvam; āhitatvasyāvivakṣitatvāt // VKal_5,2.90 //*",
    "GK16vamkalsu:342": "samāptaṃ cedaṃ prāyogikaṃ pañcamadhikaraṇam /",
    "GK16vamkalsu:343": "samāpto 'yaṃ granthaḥ //"
}