{
    "GK16vamkalsu:vkal_1.0.1": "pāṇḍitavaraśrīvāmanaviracitāni kāvyālaṅkārasūtrāṇi svavṛttisametāni",
    "GK16vamkalsu:vkal_1.0.2": "śārīraṃ nāma prathamādhikaraṇam /",
    "GK16vamkalsu:vkal_1.0.3": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_1.0.4": "praṇamya paramaṃ jyotirvāmanena kavipriyā /",
    "GK16vamkalsu:vkal_1.0.5": "kāvyālaṅkārasūtrāṇāṃ teṣāṃ vṛttirvidhīyate //",
    "GK16vamkalsu:vkal_1.1.1": "kāvyaṃ grāhyamalaṅkārāt // //",
    "GK16vamkalsu:vkal_1.1.2": "saundaryamalaṅkāraḥ // //",
    "GK16vamkalsu:vkal_1.1.3": "sa doṣaguṇālaṅkārahānādānābhyām // //",
    "GK16vamkalsu:vkal_1.1.4": "śāstrataste // //",
    "GK16vamkalsu:vkal_1.1.5": "kāvyaṃ saddṛṣṭādṛṣṭārtham, prītikīrtihetutvāt // //",
    "GK16vamkalsu:vkal_1.1.6": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_1.2.1": "arocakinaḥ satṛṇābhyavahāriṇaśca kavayaḥ // //",
    "GK16vamkalsu:vkal_1.2.2": "pūrve śiṣyāḥ, vivekitvāt // //",
    "GK16vamkalsu:vkal_1.2.3": "netare, tadviparyayāt // //",
    "GK16vamkalsu:vkal_1.2.4": "na śāstramadravyeṣvarthavat // //",
    "GK16vamkalsu:vkal_1.2.5": "na katakaṃ paṅkaprasādanāya // //",
    "GK16vamkalsu:vkal_1.2.6": "rītirātmā kāvyasya // //",
    "GK16vamkalsu:vkal_1.2.7": "viśiṣṭā padaracanā rītiḥ // //",
    "GK16vamkalsu:vkal_1.2.8": "viśeṣo guṇātmā // //",
    "GK16vamkalsu:vkal_1.2.9": "sā tridhā --- vaidarbhī gauḍīyā pāñcālī ceti // //",
    "GK16vamkalsu:vkal_1.2.10": "vidarbhādiṣu dṛṣṭatvāttatsamākhyā // //",
    "GK16vamkalsu:vkal_1.2.11": "samagraguṇopetā vaidarbhī // //",
    "GK16vamkalsu:vkal_1.2.12": "ojaḥkāntimatī gauḍīyā // //",
    "GK16vamkalsu:vkal_1.2.13": "mādhuryasaukumāryopapannā pāñcālī // //",
    "GK16vamkalsu:vkal_1.2.14": "tāsāṃ pūrvā grāhyā, guṇasākalyāt // //",
    "GK16vamkalsu:vkal_1.2.15": "na punaritare, stokaguṇatvāt // //",
    "GK16vamkalsu:vkal_1.2.16": "tadārohaṇārthamitarābhyāsa ityeke // //",
    "GK16vamkalsu:vkal_1.2.17": "tattu na; atattvaśīlasya tattvāniṣpatteḥ // //",
    "GK16vamkalsu:vkal_1.2.18": "na śaṇasūtravānābhyāse trasarasūtravānavaicitryalābhaḥ // //",
    "GK16vamkalsu:vkal_1.2.19": "sāpi samāsābhāve śuddhavaidarbhī // //",
    "GK16vamkalsu:vkal_1.2.20": "tasyāmarthaguṇasaṃpadāsvādyā // //",
    "GK16vamkalsu:vkal_1.2.21": "tadupārohādarthaguṇaleśo 'pi // //",
    "GK16vamkalsu:vkal_1.2.22": "sāpi vaidarbhī, tātsthyāt // //",
    "GK16vamkalsu:vkal_1.2.23": "tṛtīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_1.3.1": "loko vidyā prakīrṇa ca kāvyāṅgāni // //",
    "GK16vamkalsu:vkal_1.3.2": "lokavṛttaṃ lokaḥ // //",
    "GK16vamkalsu:vkal_1.3.3": "śabdasmṛtyabhidhānakośacchandovicitikalākāmaśāstradaṇḍanītipūrvā vidyāḥ //",
    "GK16vamkalsu:vkal_1.3.4": "śabdasmṛteḥ śabdaśuddhiḥ // //",
    "GK16vamkalsu:vkal_1.3.5": "abhidhānakośāt padārthaniścayaḥ // //",
    "GK16vamkalsu:vkal_1.3.6": "chandovicitervṛttasaṃśayacchedaḥ // //",
    "GK16vamkalsu:vkal_1.3.7": "kalāśāstrebhyaḥ kalātattvasya saṃvit // //",
    "GK16vamkalsu:vkal_1.3.8": "kāmaśāstrataḥ kāmopacārasya // //",
    "GK16vamkalsu:vkal_1.3.9": "daṇḍanīternayāpanayayoḥ // //",
    "GK16vamkalsu:vkal_1.3.10": "itivṛttakuṭilatvaṃ ca tataḥ // //",
    "GK16vamkalsu:vkal_1.3.11": "lakṣyajñatvamabhiyogo vṛddhasevāvekṣaṇaṃ pratibhānamavadhānaṃ ca prakīrṇam // //",
    "GK16vamkalsu:vkal_1.3.12": "tatra kāvyaparicayo lakṣyajñatvam // //",
    "GK16vamkalsu:vkal_1.3.13": "kāvyabandhodyamo 'bhiyogaḥ // //",
    "GK16vamkalsu:vkal_1.3.14": "kāvyopadeśaguruśuśrūṣaṇaṃ vṛddhasevā // //",
    "GK16vamkalsu:vkal_1.3.15": "padādhānoddharaṇamavekṣaṇam // //",
    "GK16vamkalsu:vkal_1.3.16": "kavitvabījaṃ pratibhānam // //",
    "GK16vamkalsu:vkal_1.3.17": "cittaikāgryamavadhānam // //",
    "GK16vamkalsu:vkal_1.3.18": "taddeśakālābhyām // //",
    "GK16vamkalsu:vkal_1.3.19": "vivikto deśaḥ // //",
    "GK16vamkalsu:vkal_1.3.20": "rātriyāmasturīyaḥ kālaḥ // //",
    "GK16vamkalsu:vkal_1.3.21": "kāvyaṃ gadyaṃ padyaṃ ca // //",
    "GK16vamkalsu:vkal_1.3.22": "gadyaṃ vṛttagandhi cūrṇamutkalikāprāyaṃ ca // //",
    "GK16vamkalsu:vkal_1.3.23": "padyabhāgavadvṛttagandhi // //",
    "GK16vamkalsu:vkal_1.3.24": "anāviddhalalitapadaṃ cūrṇam // //",
    "GK16vamkalsu:vkal_1.3.25": "viparītamutkalikāprāyam // //",
    "GK16vamkalsu:vkal_1.3.26": "padyamanekabhedam // //",
    "GK16vamkalsu:vkal_1.3.27": "tadanibaddhaṃ nibaddhaṃ ca // //",
    "GK16vamkalsu:vkal_1.3.28": "kramasiddhistayoḥ sraguttaṃsavat // //",
    "GK16vamkalsu:vkal_1.3.29": "nānibaddhaṃ cakāstyekatejaḥparamāṇuvat // //",
    "GK16vamkalsu:vkal_1.3.30": "saṃdarbheṣu daśarūpakaṃ śreyaḥ // //",
    "GK16vamkalsu:vkal_1.3.31": "taddhi citraṃ citrapaṭavadviśeṣasākalyāt // //",
    "GK16vamkalsu:vkal_1.3.32": "tato 'nyabhedakḷptiḥ // //",
    "GK16vamkalsu:vkal_1.3.33": "samāptaṃ cedaṃ śārīraṃ prathamamadhikaraṇam /",
    "GK16vamkalsu:vkal_1.3.34": "doṣadarśanaṃ nāma dvitīyamadhikaraṇam /",
    "GK16vamkalsu:vkal_1.3.35": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_2.1.1": "guṇaviparyayātmāno doṣāḥ // //",
    "GK16vamkalsu:vkal_2.1.2": "arthatastadavagamaḥ // //",
    "GK16vamkalsu:vkal_2.1.3": "saukaryāya prapañcaḥ // //",
    "GK16vamkalsu:vkal_2.1.4": "duṣṭaṃ padamasādhu kaṣṭaṃ grāmyamapratītamanarthakaṃ ca // //",
    "GK16vamkalsu:vkal_2.1.5": "śabdasmṛtiviruddhamasādhu // //",
    "GK16vamkalsu:vkal_2.1.6": "śrutivirasaṃ kaṣṭam // //",
    "GK16vamkalsu:vkal_2.1.7": "lokamātraprayuktaṃ grāmyam // //",
    "GK16vamkalsu:vkal_2.1.8": "śāstramātraprayuktamapratītam // //",
    "GK16vamkalsu:vkal_2.1.9": "pūraṇārthamanarthakam // //",
    "GK16vamkalsu:vkal_2.1.10": "na vākyālaṅkārārtham // //",
    "GK16vamkalsu:vkal_2.1.11": "anyārthaneyagūḍhārthāślīlakliṣṭāni ca // //",
    "GK16vamkalsu:vkal_2.1.12": "rūḍhicyutamanyārtham // //",
    "GK16vamkalsu:vkal_2.1.13": "kalpitārthaṃ neyārtham // //",
    "GK16vamkalsu:vkal_2.1.14": "aprasiddhārthaprayuktaṃ gūḍhārtham // //",
    "GK16vamkalsu:vkal_2.1.15": "asamyārthāntaramasamyasmṛtihetuścāślīlam // //",
    "GK16vamkalsu:vkal_2.1.16": "na guptalakṣitasaṃvṛtāni // //",
    "GK16vamkalsu:vkal_2.1.17": "aprasiddhāsabhyaṃ guptam // //",
    "GK16vamkalsu:vkal_2.1.18": "lākṣaṇikāsabhyānvitaṃ lakṣitam // //",
    "GK16vamkalsu:vkal_2.1.19": "lokasaṃvītaṃ saṃvṛtam // //",
    "GK16vamkalsu:vkal_2.1.20": "tantraividhyam, vrīḍājugupsāmaṅgalātaṅkadāyibhedāt // //",
    "GK16vamkalsu:vkal_2.1.21": "vyavahitārthapratyayaṃ kliṣṭam // //",
    "GK16vamkalsu:vkal_2.1.22": "antyābhyāṃ vākyaṃ vyākhyātam // //",
    "GK16vamkalsu:vkal_2.1.23": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_2.2.1": "bhinnavṛttayatibhraṣṭavisaṃdhīni vākyāni // //",
    "GK16vamkalsu:vkal_2.2.2": "svalakṣaṇacyutavṛttaṃ bhinnavṛttam // //",
    "GK16vamkalsu:vkal_2.2.3": "virasavirāmaṃ yatibhraṣṭam // //",
    "GK16vamkalsu:vkal_2.2.4": "taddhātunāmabhāgabhede svarasaṃdhyakṛte prāyeṇa // //",
    "GK16vamkalsu:vkal_2.2.5": "na vṛttadoṣātpṛthagyatidpṣaḥ, vṛttasya yatyātmakatvāt // //",
    "GK16vamkalsu:vkal_2.2.6": "na, lakṣaṇaḥ pṛthaktvāt // //",
    "GK16vamkalsu:vkal_2.2.7": "virūpapadasaṃdhi visaṃdhiḥ // //",
    "GK16vamkalsu:vkal_2.2.8": "padasaṃdhervairūpyaṃ viśleṣo 'ślīlatvaṃ kaṣṭatvaṃ ca // //",
    "GK16vamkalsu:vkal_2.2.9": "vyarthaikārthasaṃdigdhāyuktāpakramalokavidyā viruddhāni ca // //",
    "GK16vamkalsu:vkal_2.2.10": "vyāhatapūrvottarārthaṃ vyartham // //",
    "GK16vamkalsu:vkal_2.2.11": "uktārthapadamekārtham // //",
    "GK16vamkalsu:vkal_2.2.12": "na, viśeṣaścedekārthaṃ duṣṭam // //",
    "GK16vamkalsu:vkal_2.2.13": "dhanurjyādhvanau dhanuḥśrutirārūḍheḥ pratipattyai // //",
    "GK16vamkalsu:vkal_2.2.14": "karṇāvataṃsaśravaṇakuṇḍalaśiraḥśekhareṣu karṇādinirdeśaḥ saṃnidheḥ //",
    "GK16vamkalsu:vkal_2.2.15": "muktāhāraśabde muktāśabdaḥ śuddheḥ // //",
    "GK16vamkalsu:vkal_2.2.16": "puṣpamālāśabde puṣpapadamutkarṣasya // //",
    "GK16vamkalsu:vkal_2.2.17": "karikalabhaśabde kariśabdastādrūpyasya // //",
    "GK16vamkalsu:vkal_2.2.18": "viśeṣaṇasya ca // //",
    "GK16vamkalsu:vkal_2.2.19": "tadidaṃ prayukteṣu // //",
    "GK16vamkalsu:vkal_2.2.20": "saṃśayakṛtsaṃdigdham // //",
    "GK16vamkalsu:vkal_2.2.21": "māyādivikalpitārthamprayuktam // //",
    "GK16vamkalsu:vkal_2.2.22": "kramahīnārthamapakramam // //",
    "GK16vamkalsu:vkal_2.2.23": "deśakālasvabhāvaviruddhārthāni lokaviruddhāni // //",
    "GK16vamkalsu:vkal_2.2.24": "kalācaturvargaśāstraviruddhārthāni vidyāviruddhāni // //",
    "GK16vamkalsu:vkal_2.2.25": "samāotaṃ cedaṃ doṣadarśanam dvitīyamadhikaraṇam /",
    "GK16vamkalsu:vkal_2.2.26": "guṇavivecanaṃ nāma tṛtīyamadhikaraṇam /",
    "GK16vamkalsu:vkal_2.2.27": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_3.1.1": "kāvyaśobhāyāḥ kartāro dharmā guṇāḥ // //",
    "GK16vamkalsu:vkal_3.1.2": "tadatiśayahetavastvalaṅkārāḥ // //",
    "GK16vamkalsu:vkal_3.1.3": "pūrve nityāḥ // //",
    "GK16vamkalsu:vkal_3.1.4": "ojaḥprasādaśleṣasamatāsamādhimādhuryasaukumāryodāratārthavyaktikāntayo bandhaguṇāḥ // //",
    "GK16vamkalsu:vkal_3.1.5": "gāḍhabandhatvamojaḥ // //",
    "GK16vamkalsu:vkal_3.1.6": "śaithilyaṃ prasādaḥ // //",
    "GK16vamkalsu:vkal_3.1.7": "guṇaḥ saṃplavāt // //",
    "GK16vamkalsu:vkal_3.1.8": "sa tvanubhavasiddhaḥ // //",
    "GK16vamkalsu:vkal_3.1.9": "sāmyotkarṣau ca // //",
    "GK16vamkalsu:vkal_3.1.10": "masṛṇatvaṃ śleṣaḥ // //",
    "GK16vamkalsu:vkal_3.1.11": "mārgāvhedaḥ samatā // //",
    "GK16vamkalsu:vkal_3.1.12": "ārohāvarohakramaḥ samādhiḥ // //",
    "GK16vamkalsu:vkal_3.1.13": "na pṛthak, ārohāvarohayorojaḥprasādarūpatvāt // //",
    "GK16vamkalsu:vkal_3.1.14": "na, asaṃpṛktatvāt // //",
    "GK16vamkalsu:vkal_3.1.15": "anaikāntyācca // //",
    "GK16vamkalsu:vkal_3.1.16": "ojaḥprasādayoḥ tīvrāvasthā, tāviti cedabhyupagamaḥ // //",
    "GK16vamkalsu:vkal_3.1.17": "viśeṣāpekṣitvāttayoḥ // //",
    "GK16vamkalsu:vkal_3.1.18": "ārohāvarohanimittaṃ samādhirākhyāyate // //",
    "GK16vamkalsu:vkal_3.1.19": "kramavidhānārthaṃ vā // //",
    "GK16vamkalsu:vkal_3.1.20": "pṛthakpadatvaṃ mādhuryam // //",
    "GK16vamkalsu:vkal_3.1.21": "ajaraṭhatvaṃ saukumāryam // //",
    "GK16vamkalsu:vkal_3.1.22": "vikaṭatvamudāratā // //",
    "GK16vamkalsu:vkal_3.1.23": "arthavyaktihetutvamarthavyaktiḥ // //",
    "GK16vamkalsu:vkal_3.1.24": "aujjvalyaṃ kāntiḥ // //",
    "GK16vamkalsu:vkal_3.1.25": "nāsantaḥ, sadvedyatvāt // //",
    "GK16vamkalsu:vkal_3.1.26": "na bhrāntāḥ, niṣkampatvāt // //",
    "GK16vamkalsu:vkal_3.1.27": "na pāṭhadharmāḥ, sarvatrādṛṣṭeḥ // //",
    "GK16vamkalsu:vkal_3.1.28": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_3.2.1": "ta evārthaguṇāḥ // //",
    "GK16vamkalsu:vkal_3.2.2": "arthasya prauḍhirojaḥ // //",
    "GK16vamkalsu:vkal_3.2.3": "arthavaimalyaṃ prasādaḥ // //",
    "GK16vamkalsu:vkal_3.2.4": "ghaṭanā śleṣaḥ // //",
    "GK16vamkalsu:vkal_3.2.5": "avaiṣamyaṃ samatā // //",
    "GK16vamkalsu:vkal_3.2.6": "arthadṛṣṭiḥ samādhiḥ // //",
    "GK16vamkalsu:vkal_3.2.7": "artho dvividhaḥ, ayoniranyacchāyāyonirvā // //",
    "GK16vamkalsu:vkal_3.2.8": "artho vyaktaḥ sūkṣmaśca // //",
    "GK16vamkalsu:vkal_3.2.9": "sūkṣmo bhāvyo vāsanīyaśca // //",
    "GK16vamkalsu:vkal_3.2.10": "uktivaicitryaṃ mādhuryam // //",
    "GK16vamkalsu:vkal_3.2.11": "apāruṣyaṃ saukumāryam // //",
    "GK16vamkalsu:vkal_3.2.12": "agrāmyatvamudāratā // //",
    "GK16vamkalsu:vkal_3.2.13": "vastusvabhāvasphuṭatvamarthavyaktiḥ // //",
    "GK16vamkalsu:vkal_3.2.14": "dīptarasatvaṃ kāntiḥ // //",
    "GK16vamkalsu:vkal_3.2.15": "samāotaṃ cedaṃ guṇavivecanaṃ tṛtīyamadhikaraṇam /",
    "GK16vamkalsu:vkal_3.2.16": "ālaṅkārikaṃ nāma caturthamadhikaraṇam /",
    "GK16vamkalsu:vkal_3.2.17": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_4.1.1": "padamanaikārthamakṣaraṃ cāvṛttaṃ sthānaniyame yamakam // //",
    "GK16vamkalsu:vkal_4.1.2": "pādāḥ pādasyaikasyānekasya cādimadhyāntabhāgāḥ sthānāni // //",
    "GK16vamkalsu:vkal_4.1.3": "bhaṅgādutkarṣaḥ // //",
    "GK16vamkalsu:vkal_4.1.4": "śṛṅkhalā parivartakaścūrṇamiti bhaṅgamārgaḥ // //",
    "GK16vamkalsu:vkal_4.1.5": "varṇavicchedacalanaṃ śṛṅkhalā // //",
    "GK16vamkalsu:vkal_4.1.6": "saṅgavinivṛttau svarūpāpattiḥ parivartakaḥ // //",
    "GK16vamkalsu:vkal_4.1.7": "piṇḍākṣarabhede svarūpalopaścūrṇam // //",
    "GK16vamkalsu:vkal_4.1.8": "śeṣaḥ sarūpo 'nuprāsaḥ // //",
    "GK16vamkalsu:vkal_4.1.9": "anulbaṇo varṇānuprāsaḥ śreyān // //",
    "GK16vamkalsu:vkal_4.1.10": "pādānuprāsaḥ pādayamakavat // //",
    "GK16vamkalsu:vkal_4.1.11": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_4.2.1": "upamānenopameyasya guṇaleśataḥ sāmyamupamā // //",
    "GK16vamkalsu:vkal_4.2.2": "guṇabāhulyataśca kalpitā // //",
    "GK16vamkalsu:vkal_4.2.3": "taddvaividhyam, padavākyārthavṛttibhedāt // //",
    "GK16vamkalsu:vkal_4.2.4": "sā pūṇā luptā ca // //",
    "GK16vamkalsu:vkal_4.2.5": "guṇadyotakopamānopameyaśabdānāṃ sāmagrye pūrṇā // //",
    "GK16vamkalsu:vkal_4.2.6": "lope luptā // //",
    "GK16vamkalsu:vkal_4.2.7": "stutinindātattvākhyāneṣu // //",
    "GK16vamkalsu:vkal_4.2.8": "hīnatvādhikatvaliṅgavacanabhedāsādṛśyāsaṃbhavāstaddoṣāḥ // //",
    "GK16vamkalsu:vkal_4.2.9": "jātipramāṇadharmanyūnatopamānasya hīnatvam // //",
    "GK16vamkalsu:vkal_4.2.10": "dharmayorekanirdeśe 'nyasya saṃvit, sāhacaryāt // //",
    "GK16vamkalsu:vkal_4.2.11": "tenādhikarvaṃ vyākhyātam // //",
    "GK16vamkalsu:vkal_4.2.12": "upamānopameyayorliṅgavyatyāso liṅgabhedaḥ // //",
    "GK16vamkalsu:vkal_4.2.13": "iṣṭaḥ puṃpuṃsakayoḥ prāyeṇa // //",
    "GK16vamkalsu:vkal_4.2.14": "laukikyāṃ samāsābhihitāyāmupamāprapañce ca // //",
    "GK16vamkalsu:vkal_4.2.15": "tena vacanabhedo vyākhyātaḥ // //",
    "GK16vamkalsu:vkal_4.2.16": "apratītaguṇasādṛśyamasādṛśyam // //",
    "GK16vamkalsu:vkal_4.2.17": "asādṛśyahatā hyupamā, tanniṣṭhāśca kavayaḥ // //",
    "GK16vamkalsu:vkal_4.2.18": "upamānādhikyāttadapoha ityeke // //",
    "GK16vamkalsu:vkal_4.2.19": "na, apuṣṭārthatvāt // //",
    "GK16vamkalsu:vkal_4.2.20": "anupapattirasaṃbhavaḥ // //",
    "GK16vamkalsu:vkal_4.2.21": "na viruddho 'tiśayaḥ // //",
    "GK16vamkalsu:vkal_4.2.22": "tṛtīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_4.3.1": "prativastuprabhṛtirupamāprapañcaḥ // //",
    "GK16vamkalsu:vkal_4.3.2": "upameyasyoktau samānavastunyāsaḥ prativastu // //",
    "GK16vamkalsu:vkal_4.3.3": "anuktau samāsoktiḥ // //",
    "GK16vamkalsu:vkal_4.3.4": "kiñciduktāvaprastutapraśaṃsā // //",
    "GK16vamkalsu:vkal_4.3.5": "samena vastunānyāpalāpo 'pahnutiḥ // //",
    "GK16vamkalsu:vkal_4.3.6": "upamānenopameyasya guṇasāmyāttattvāropo rūpakam // //",
    "GK16vamkalsu:vkal_4.3.7": "sa dharmeṣu tantraprayoge śleṣaḥ // //",
    "GK16vamkalsu:vkal_4.3.8": "sādṛśyāllakṣaṇā vakroktiḥ // //",
    "GK16vamkalsu:vkal_4.3.9": "atadrūpasyānyathādhyavasānamatiśayārthamutprekṣā // //",
    "GK16vamkalsu:vkal_4.3.10": "saṃbhāvyadharmatadutkarṣakalpanātiśayoktiḥ // //",
    "GK16vamkalsu:vkal_4.3.11": "upamānopameyasaṃśayaḥ saṃdehaḥ // //",
    "GK16vamkalsu:vkal_4.3.12": "viruddhābhāsatvaṃ virodhaḥ // //",
    "GK16vamkalsu:vkal_4.3.13": "kriyāpratiṣedhe prasiddhatatphalavyaktirvibhāvanā // //",
    "GK16vamkalsu:vkal_4.3.14": "ekasyopameyatvopamānatve 'nanvayaḥ // //",
    "GK16vamkalsu:vkal_4.3.15": "krameṇopameyopamā // //",
    "GK16vamkalsu:vkal_4.3.16": "samavisadṛśābhyāṃ parivartanaṃ parivṛttiḥ // //",
    "GK16vamkalsu:vkal_4.3.17": "upameyopamānānāṃ kramasaṃbandhaḥ kramaḥ // //",
    "GK16vamkalsu:vkal_4.3.18": "upamānopameyavākyeṣvekā kriyā dīpakam // //",
    "GK16vamkalsu:vkal_4.3.19": "tantrividham / ādimadhyāntavākyavṛttibhedāt // //",
    "GK16vamkalsu:vkal_4.3.20": "kriyayaiva svatadarthānvayakhyāpanaṃ nidarśanam // //",
    "GK16vamkalsu:vkal_4.3.21": "uktisiddhyai vastuno 'rthāntarasyaiva nyasanamarthāntaranyāsaḥ //",
    "GK16vamkalsu:vkal_4.3.22": "upameyasya guṇātirekatvaṃ vyatirekaḥ // //",
    "GK16vamkalsu:vkal_4.3.23": "ekaguṇahānikalpanāyāṃ sāmyadārḍhyaṃ viśeṣoktiḥ // //",
    "GK16vamkalsu:vkal_4.3.24": "saṃbhāvyaviśiṣṭakarmākaraṇānnindāstotrārthā vyājastutiḥ // //",
    "GK16vamkalsu:vkal_4.3.25": "vyājasya satyasārūpyaṃ vyājoktiḥ // //",
    "GK16vamkalsu:vkal_4.3.26": "viśiṣṭena sāmyārthamekakālakriyāyogastulyayogitā // //",
    "GK16vamkalsu:vkal_4.3.27": "upamānākṣepaścākṣepaḥ // //",
    "GK16vamkalsu:vkal_4.3.28": "vastudvayakriyayostulyakālayorekapadābhidhānaṃ sahoktiḥ // //",
    "GK16vamkalsu:vkal_4.3.29": "yatsādṛśyaṃ tatsaṃpattiḥ samāhitam // //",
    "GK16vamkalsu:vkal_4.3.30": "alaṅkārasyālaṅkārayonitvaṃ saṃsṛṣṭiḥ // //",
    "GK16vamkalsu:vkal_4.3.31": "tadbhedāvupamārūpakotprekṣāvayavau // //",
    "GK16vamkalsu:vkal_4.3.32": "upamājanyaṃ rūpakamupamārūpakam // //",
    "GK16vamkalsu:vkal_4.3.33": "utprekṣāheturutprekṣāvayavaḥ // //",
    "GK16vamkalsu:vkal_4.3.34": "samāotaṃ cedamālaṅkārikaṃ caturthamadhikaraṇam /",
    "GK16vamkalsu:vkal_4.3.35": "prāyogikaṃ nāma pañcamamadhikaraṇam /",
    "GK16vamkalsu:vkal_4.3.36": "prathamo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_5.1.1": "naikaṃ padaṃ dviḥ prayojyaṃ prāyeṇa // //",
    "GK16vamkalsu:vkal_5.1.2": "nityā saṃhitaikapadavatpādeṣvardhāntavarjam // //",
    "GK16vamkalsu:vkal_5.1.3": "na pādāntalaghorgurutvaṃ ca sarvatra // //",
    "GK16vamkalsu:vkal_5.1.4": "na gādye samāptaprāyaṃ vṛttam, anyatrodgatādibhyaḥ saṃvādāt // //",
    "GK16vamkalsu:vkal_5.1.5": "na pādādau khalvādayaḥ // //",
    "GK16vamkalsu:vkal_5.1.6": "nārdhe kiñcitsamāptaṃ vākyam // //",
    "GK16vamkalsu:vkal_5.1.7": "na karmadhārayo bahuvrīhipratipattikaraḥ // //",
    "GK16vamkalsu:vkal_5.1.8": "tena viparyayo vyākhyātaḥ // //",
    "GK16vamkalsu:vkal_5.1.9": "saṃbhāvyaniṣedhanivartane dvau pratiṣedhau // //",
    "GK16vamkalsu:vkal_5.1.10": "viśeṣaṇamātraprayogo viśeṣyapratipattau // //",
    "GK16vamkalsu:vkal_5.1.11": "sarvanāmnānusaṃdhirvṛtticchannasya // //",
    "GK16vamkalsu:vkal_5.1.12": "saṃbandhasaṃbandhe 'pi ṣaṣṭhī kvacit // //",
    "GK16vamkalsu:vkal_5.1.13": "atiprayuktaṃ deśabhāṣāpadam // //",
    "GK16vamkalsu:vkal_5.1.14": "liṅgādhyāhārau // //",
    "GK16vamkalsu:vkal_5.1.15": "lakṣaṇāśabdāśca // //",
    "GK16vamkalsu:vkal_5.1.16": "na tadbāhulyamekatra // //",
    "GK16vamkalsu:vkal_5.1.17": "stanādīnāṃ dvitvāviṣṭā jātiḥ prāyeṇa // //",
    "GK16vamkalsu:vkal_5.1.18": "dvitīyo 'dhyāyaḥ /",
    "GK16vamkalsu:vkal_5.2.1": "rudrāvityekaśeṣo 'nveṣyaḥ // //",
    "GK16vamkalsu:vkal_5.2.2": "miliklabikṣapiprabhṛtīnāṃ dhātutvam, dhātuguṇasyāsamāpteḥ // //",
    "GK16vamkalsu:vkal_5.2.3": "valerātmanepadamanityam, jñāpakāt // //",
    "GK16vamkalsu:vkal_5.2.4": "kṣīyata iti karmakartari // //",
    "GK16vamkalsu:vkal_5.2.5": "khidyata iti ca // //",
    "GK16vamkalsu:vkal_5.2.6": "mārgerātmaepadamalakṣma // //",
    "GK16vamkalsu:vkal_5.2.7": "lolamānādayaścānaśi // //",
    "GK16vamkalsu:vkal_5.2.8": "labhergatyarthatvāṇṇicyaṇau kartuḥ karmatvākarmatve // //",
    "GK16vamkalsu:vkal_5.2.9": "te-me-śabdau nipāteṣu // //",
    "GK16vamkalsu:vkal_5.2.10": "tiraskṛta iti paribhūte, antardhyupacārāt // //",
    "GK16vamkalsu:vkal_5.2.11": "naikaśabdaḥ, supsupeti samāsāt // //",
    "GK16vamkalsu:vkal_5.2.12": "madhupipāsuprabhṛtīnāṃ samāso gamigāmyādiṣu pāṭhāt // //",
    "GK16vamkalsu:vkal_5.2.13": "trivalīśabdaḥ siddhaḥ saṃjñā cet // //",
    "GK16vamkalsu:vkal_5.2.14": "bimbādhara iti vṛttau madhyamapadalopinyām // //",
    "GK16vamkalsu:vkal_5.2.15": "āmūlalolādiṣu vṛttirvispaṣṭapaṭuvat // //",
    "GK16vamkalsu:vkal_5.2.16": "na dhānyaṣaṣṭhādiṣu ṣaṣṭhīsamāsapratiṣedhaḥ, pūraṇena taddhitāntatvāt //",
    "GK16vamkalsu:vkal_5.2.17": "patrapītimādiṣu guṇavacanena // //",
    "GK16vamkalsu:vkal_5.2.18": "avarjyo bahuvrīhirvyadhikaraṇo janmādyuttarapadaḥ // //",
    "GK16vamkalsu:vkal_5.2.19": "hastāgrāgrahastādayo guṇaguṇinorbhedābhedāt // //",
    "GK16vamkalsu:vkal_5.2.20": "pūrvanipāte 'pabhraṃśo rakṣyaḥ // //",
    "GK16vamkalsu:vkal_5.2.21": "nipātenāpyabhihite karmaṇi na karmavibhaktiḥ, parigaṇanasya prāyikatvāt //",
    "GK16vamkalsu:vkal_5.2.22": "śakyamiti rūpaṃ karmābhidhāyāṃ liṅgavacanasyāpi sāmānyopakramāt //",
    "GK16vamkalsu:vkal_5.2.23": "hānivadādhikyamapyaṅgānāṃ vikāraḥ // //",
    "GK16vamkalsu:vkal_5.2.24": "na kṛmikīṭānāmiti, ekavadbhāvaprasaṅgāt // //",
    "GK16vamkalsu:vkal_5.2.25": "na kharoṣṭrau, uṣṭrakharamiti pāṭhāt // //",
    "GK16vamkalsu:vkal_5.2.26": "āsetyasateḥ // //",
    "GK16vamkalsu:vkal_5.2.27": "yudhyediti yudhaḥ kyaci // //",
    "GK16vamkalsu:vkal_5.2.28": "viralāyamānādiṣu kyaṅ nirūpyaḥ // //",
    "GK16vamkalsu:vkal_5.2.29": "ahetau hanterṇic, curādipāṭhāt // //",
    "GK16vamkalsu:vkal_5.2.30": "anucarīti careṣṭittvāt // //",
    "GK16vamkalsu:vkal_5.2.31": "kesarālamityalateraṇi // //",
    "GK16vamkalsu:vkal_5.2.32": "patralamiti lāteḥ ke // //",
    "GK16vamkalsu:vkal_5.2.33": "mahīdhrādayo mūlavibhujādidarśanāt // //",
    "GK16vamkalsu:vkal_5.2.34": "brahmādiṣu hanterniyamādarihādyasiddhiḥ // //",
    "GK16vamkalsu:vkal_5.2.35": "brahmavidādayaḥ kṛdantavṛttyā // //",
    "GK16vamkalsu:vkal_5.2.36": "tairmahīdharādayo vyākhyātāḥ // //",
    "GK16vamkalsu:vkal_5.2.37": "bhidurādayaḥ karmakartari kartari ca // //",
    "GK16vamkalsu:vkal_5.2.38": "guṇavistarādayaścintyāḥ // //",
    "GK16vamkalsu:vkal_5.2.39": "avatarāpacāyaśabdayordīrghahrasvatvavyatyāso bālānām // //",
    "GK16vamkalsu:vkal_5.2.40": "śobheti nipātanāt // //",
    "GK16vamkalsu:vkal_5.2.41": "avidhau guroḥ striyāṃ bahulaṃ vivakṣā // //",
    "GK16vamkalsu:vkal_5.2.42": "vyavasitādiṣu ktaḥ kartari, cakārāt // //",
    "GK16vamkalsu:vkal_5.2.43": "āheti bhūte 'nyaṇalantabhramād bruvo laṭi // //",
    "GK16vamkalsu:vkal_5.2.44": "śabalādibhyaḥ striyāṃ ṭāpo 'prāptiḥ // //",
    "GK16vamkalsu:vkal_5.2.45": "prāṇini nīleti cintyam // //",
    "GK16vamkalsu:vkal_5.2.46": "manuṣyajātervivakṣāvivakṣe // //",
    "GK16vamkalsu:vkal_5.2.47": "ūkārāntādapyūṅ, pravṛtteḥ // //",
    "GK16vamkalsu:vkal_5.2.48": "kārtikīya iti ṭhañ durdharaḥ // //",
    "GK16vamkalsu:vkal_5.2.49": "śārvaramiti ca // //",
    "GK16vamkalsu:vkal_5.2.50": "śāśvatamiti prayukteḥ // //",
    "GK16vamkalsu:vkal_5.2.51": "rājavaṃśyādayaḥ sādhvarthe ye bhavanti // //",
    "GK16vamkalsu:vkal_5.2.52": "dāravaśabdo duṣprayuktaḥ // //",
    "GK16vamkalsu:vkal_5.2.53": "mugdhimādiṣvivamanij mṛgyaḥ // //",
    "GK16vamkalsu:vkal_5.2.54": "aupamyādayaścāturvarṇyavat // //",
    "GK16vamkalsu:vkal_5.2.55": "ṣyañaḥ ṣitkaraṇādīkāro bahulam // //",
    "GK16vamkalsu:vkal_5.2.56": "dhanvīti vrīhyādiṣu pāṭhāt // //",
    "GK16vamkalsu:vkal_5.2.57": "caturasraśobhīti ṇinau // //",
    "GK16vamkalsu:vkal_5.2.58": "kañcukīyā iti kyaci // //",
    "GK16vamkalsu:vkal_5.2.59": "bauddhapratiyogyapekṣāyāmapyātiśāyanikāḥ // //",
    "GK16vamkalsu:vkal_5.2.60": "kauśilādaya ilaci varṇalopāt // //",
    "GK16vamkalsu:vkal_5.2.61": "mauktikamiti vinayādipāṭhāt // //",
    "GK16vamkalsu:vkal_5.2.62": "prātibhādayaḥ prajñādiṣu // //",
    "GK16vamkalsu:vkal_5.2.63": "na sarajasamityanavyayībhāve // //",
    "GK16vamkalsu:vkal_5.2.64": "dhṛtadhanuṣītyasaṃjñāyām // //",
    "GK16vamkalsu:vkal_5.2.65": "durgandhipada iddurlabhaḥ // //",
    "GK16vamkalsu:vkal_5.2.66": "sudatyādayaḥ pratividheyāḥ // //",
    "GK16vamkalsu:vkal_5.2.67": "kṣatadṛḍhorsa iti na kap, tadantavidhipratiṣedhāt // //",
    "GK16vamkalsu:vkal_5.2.68": "avaihīti vṛddhiravadyā // //",
    "GK16vamkalsu:vkal_5.2.69": "apāṅganetreti lugalabhyaḥ // //",
    "GK16vamkalsu:vkal_5.2.70": "neṣṭhāḥ śliṣṭapriyādayaḥ, puṃvadbhāvapratiṣedhāt // //",
    "GK16vamkalsu:vkal_5.2.71": "dṛḍhabhaktiriti sarvatreti // //",
    "GK16vamkalsu:vkal_5.2.72": "jambulatādayo hrasvavidheḥ // //",
    "GK16vamkalsu:vkal_5.2.73": "tilakādayo 'jirādiṣu // //",
    "GK16vamkalsu:vkal_5.2.74": "niśamya-niśamayyaśabdau prakṛtibhedāt // //",
    "GK16vamkalsu:vkal_5.2.75": "saṃyamyaniyamyaśabdau, aṇijantatvāt // //",
    "GK16vamkalsu:vkal_5.2.76": "prapīyeti pīṅaḥ // //",
    "GK16vamkalsu:vkal_5.2.77": "dūrayatīti bahulagrahaṇāt // //",
    "GK16vamkalsu:vkal_5.2.78": "gacchatīprabhṛtiṣvaniṣedhyo num // //",
    "GK16vamkalsu:vkal_5.2.79": "mitreṇa goptreti puṃvadbhāvāt // //",
    "GK16vamkalsu:vkal_5.2.80": "vetsyasīti padabhaṅgāt // //",
    "GK16vamkalsu:vkal_5.2.81": "kāmayānaśabdaḥ siddho 'nādiścet // //",
    "GK16vamkalsu:vkal_5.2.82": "sauhṛd-daurhṛdaśabdāvaṇi hṛdbhāvāt // //",
    "GK16vamkalsu:vkal_5.2.83": "virama iti nipātanāt // //",
    "GK16vamkalsu:vkal_5.2.84": "uparyādiṣu sāmīpye dvirukteṣu dvitīyā // //",
    "GK16vamkalsu:vkal_5.2.85": "mandaṃ mandamityaprakārārthatve // //",
    "GK16vamkalsu:vkal_5.2.86": "na nidrādrugiti, bhaṣbhāvaprāpteḥ // //",
    "GK16vamkalsu:vkal_5.2.87": "niṣpanda iti patvaṃ cintyam // //",
    "GK16vamkalsu:vkal_5.2.88": "nāṅgulisaṅga iti mūrdhanyavidheḥ // //",
    "GK16vamkalsu:vkal_5.2.89": "tenāvantisenādayaḥ pratyuktāḥ // //",
    "GK16vamkalsu:vkal_5.2.90": "nendravāhane ṇatvam; āhitatvasyāvivakṣitatvāt // //",
    "GK16vamkalsu:vkal_5.2.91": "samāptaṃ cedaṃ prāyogikaṃ pañcamadhikaraṇam /",
    "GK16vamkalsu:vkal_5.2.92": "samāpto 'yaṃ granthaḥ //"
}