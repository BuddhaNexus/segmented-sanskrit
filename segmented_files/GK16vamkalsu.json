{
  "GK16vamkalsu:0": "pāṇḍitavaraśrīvāmanaviracitāni",
  "GK16vamkalsu:1": "kāvyālaṅkārasūtrāṇi",
  "GK16vamkalsu:2": "svavṛttisametāni",
  "GK16vamkalsu:3": "--------------------",
  "GK16vamkalsu:4": "śārīraṃ nāma prathamādhikaraṇam /",
  "GK16vamkalsu:5": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:6": "praṇamya paramaṃ jyotirvāmanena kavipriyā /",
  "GK16vamkalsu:7": "kāvyālaṅkārasūtrāṇāṃ teṣāṃ vṛttirvidhīyate //",
  "GK16vamkalsu:8": "kāvyaṃ grāhyamalaṅkārāt // VKal_1,1.1 //",
  "GK16vamkalsu:9": "saundaryamalaṅkāraḥ // VKal_1,1.2 //",
  "GK16vamkalsu:10": "sa doṣaguṇālaṅkārahānādānābhyām // VKal_1,1.3 //",
  "GK16vamkalsu:11": "śāstrataste // VKal_1,1.4 //",
  "GK16vamkalsu:12": "kāvyaṃ saddṛṣṭādṛṣṭārtham, prītikīrtihetutvāt // VKal_1,1.5 //*",
  "GK16vamkalsu:13": "--------------------",
  "GK16vamkalsu:14": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:15": "arocakinaḥ satṛṇābhyavahāriṇaśca kavayaḥ // VKal_1,2.1 //",
  "GK16vamkalsu:16": "pūrve śiṣyāḥ, vivekitvāt // VKal_1,2.2 //*",
  "GK16vamkalsu:17": "netare, tadviparyayāt // VKal_1,2.3 //*",
  "GK16vamkalsu:18": "na śāstramadravyeṣvarthavat // VKal_1,2.4 //",
  "GK16vamkalsu:19": "na katakaṃ paṅkaprasādanāya // VKal_1,2.5 //",
  "GK16vamkalsu:20": "rītirātmā kāvyasya // VKal_1,2.6 //",
  "GK16vamkalsu:21": "viśiṣṭā padaracanā rītiḥ // VKal_1,2.7 //",
  "GK16vamkalsu:22": "viśeṣo guṇātmā // VKal_1,2.8 //",
  "GK16vamkalsu:23": "sā tridhā --- vaidarbhī gauḍīyā pāñcālī ceti // VKal_1,2.9 //",
  "GK16vamkalsu:24": "vidarbhādiṣu dṛṣṭatvāttatsamākhyā // VKal_1,2.10 //",
  "GK16vamkalsu:25": "samagraguṇopetā vaidarbhī // VKal_1,2.11 //",
  "GK16vamkalsu:26": "ojaḥkāntimatī gauḍīyā // VKal_1,2.12 //",
  "GK16vamkalsu:27": "mādhuryasaukumāryopapannā pāñcālī // VKal_1,2.13 //",
  "GK16vamkalsu:28": "tāsāṃ pūrvā grāhyā, guṇasākalyāt // VKal_1,2.14 //*",
  "GK16vamkalsu:29": "na punaritare, stokaguṇatvāt // VKal_1,2.15 //*",
  "GK16vamkalsu:30": "tadārohaṇārthamitarābhyāsa ityeke // VKal_1,2.16 //",
  "GK16vamkalsu:31": "tattu na; atattvaśīlasya tattvāniṣpatteḥ // VKal_1,2.17 //*",
  "GK16vamkalsu:32": "na śaṇasūtravānābhyāse trasarasūtravānavaicitryalābhaḥ // VKal_1,2.18 //",
  "GK16vamkalsu:33": "sāpi samāsābhāve śuddhavaidarbhī // VKal_1,2.19 //",
  "GK16vamkalsu:34": "tasyāmarthaguṇasaṃpadāsvādyā // VKal_1,2.20 //",
  "GK16vamkalsu:35": "tadupārohādarthaguṇaleśo 'pi // VKal_1,2.21 //",
  "GK16vamkalsu:36": "sāpi vaidarbhī, tātsthyāt // VKal_1,2.22 //*",
  "GK16vamkalsu:37": "--------------------",
  "GK16vamkalsu:38": "tṛtīyo 'dhyāyaḥ /",
  "GK16vamkalsu:39": "loko vidyā prakīrṇa ca kāvyāṅgāni // VKal_1,3.1 //",
  "GK16vamkalsu:40": "lokavṛttaṃ lokaḥ // VKal_1,3.2 //",
  "GK16vamkalsu:41": "śabdasmṛtyabhidhānakośacchandovicitikalākāmaśāstradaṇḍanītipūrvā vidyāḥ // VKal_1,3.3 //",
  "GK16vamkalsu:42": "śabdasmṛteḥ śabdaśuddhiḥ // VKal_1,3.4 //",
  "GK16vamkalsu:43": "abhidhānakośāt padārthaniścayaḥ // VKal_1,3.5 //",
  "GK16vamkalsu:44": "chandovicitervṛttasaṃśayacchedaḥ // VKal_1,3.6 //",
  "GK16vamkalsu:45": "kalāśāstrebhyaḥ kalātattvasya saṃvit // VKal_1,3.7 //",
  "GK16vamkalsu:46": "kāmaśāstrataḥ kāmopacārasya // VKal_1,3.8 //",
  "GK16vamkalsu:47": "daṇḍanīternayāpanayayoḥ // VKal_1,3.9 //",
  "GK16vamkalsu:48": "itivṛttakuṭilatvaṃ ca tataḥ // VKal_1,3.10 //",
  "GK16vamkalsu:49": "lakṣyajñatvamabhiyogo vṛddhasevāvekṣaṇaṃ pratibhānamavadhānaṃ ca prakīrṇam // VKal_1,3.11 //",
  "GK16vamkalsu:50": "tatra kāvyaparicayo lakṣyajñatvam // VKal_1,3.12 //",
  "GK16vamkalsu:51": "kāvyabandhodyamo 'bhiyogaḥ // VKal_1,3.13 //",
  "GK16vamkalsu:52": "kāvyopadeśaguruśuśrūṣaṇaṃ vṛddhasevā // VKal_1,3.14 //",
  "GK16vamkalsu:53": "padādhānoddharaṇamavekṣaṇam // VKal_1,3.15 //",
  "GK16vamkalsu:54": "kavitvabījaṃ pratibhānam // VKal_1,3.16 //",
  "GK16vamkalsu:55": "cittaikāgryamavadhānam // VKal_1,3.17 //",
  "GK16vamkalsu:56": "taddeśakālābhyām // VKal_1,3.18 //",
  "GK16vamkalsu:57": "vivikto deśaḥ // VKal_1,3.19 //",
  "GK16vamkalsu:58": "rātriyāmasturīyaḥ kālaḥ // VKal_1,3.20 //",
  "GK16vamkalsu:59": "kāvyaṃ gadyaṃ padyaṃ ca // VKal_1,3.21 //",
  "GK16vamkalsu:60": "gadyaṃ vṛttagandhi cūrṇamutkalikāprāyaṃ ca // VKal_1,3.22 //",
  "GK16vamkalsu:61": "padyabhāgavadvṛttagandhi // VKal_1,3.23 //",
  "GK16vamkalsu:62": "anāviddhalalitapadaṃ cūrṇam // VKal_1,3.24 //",
  "GK16vamkalsu:63": "viparītamutkalikāprāyam // VKal_1,3.25 //",
  "GK16vamkalsu:64": "padyamanekabhedam // VKal_1,3.26 //",
  "GK16vamkalsu:65": "tadanibaddhaṃ nibaddhaṃ ca // VKal_1,3.27 //",
  "GK16vamkalsu:66": "kramasiddhistayoḥ sraguttaṃsavat // VKal_1,3.28 //",
  "GK16vamkalsu:67": "nānibaddhaṃ cakāstyekatejaḥparamāṇuvat // VKal_1,3.29 //",
  "GK16vamkalsu:68": "saṃdarbheṣu daśarūpakaṃ śreyaḥ // VKal_1,3.30 //",
  "GK16vamkalsu:69": "taddhi citraṃ citrapaṭavadviśeṣasākalyāt // VKal_1,3.31 //",
  "GK16vamkalsu:70": "tato 'nyabhedakḷptiḥ // VKal_1,3.32 //",
  "GK16vamkalsu:71": "samāptaṃ cedaṃ śārīraṃ prathamamadhikaraṇam /",
  "GK16vamkalsu:72": "-------------------------------------------------------",
  "GK16vamkalsu:73": "doṣadarśanaṃ nāma dvitīyamadhikaraṇam /",
  "GK16vamkalsu:74": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:75": "guṇaviparyayātmāno doṣāḥ // VKal_2,1.1 //",
  "GK16vamkalsu:76": "arthatastadavagamaḥ // VKal_2,1.2 //",
  "GK16vamkalsu:77": "saukaryāya prapañcaḥ // VKal_2,1.3 //",
  "GK16vamkalsu:78": "duṣṭaṃ padamasādhu kaṣṭaṃ grāmyamapratītamanarthakaṃ ca // VKal_2,1.4 //",
  "GK16vamkalsu:79": "śabdasmṛtiviruddhamasādhu // VKal_2,1.5 //",
  "GK16vamkalsu:80": "śrutivirasaṃ kaṣṭam // VKal_2,1.6 //",
  "GK16vamkalsu:81": "lokamātraprayuktaṃ grāmyam // VKal_2,1.7 //",
  "GK16vamkalsu:82": "śāstramātraprayuktamapratītam // VKal_2,1.8 //",
  "GK16vamkalsu:83": "pūraṇārthamanarthakam // VKal_2,1.9 //",
  "GK16vamkalsu:84": "na vākyālaṅkārārtham // VKal_2,1.10 //",
  "GK16vamkalsu:85": "anyārthaneyagūḍhārthāślīlakliṣṭāni ca // VKal_2,1.11 //",
  "GK16vamkalsu:86": "rūḍhicyutamanyārtham // VKal_2,1.12 //",
  "GK16vamkalsu:87": "kalpitārthaṃ neyārtham // VKal_2,1.13 //",
  "GK16vamkalsu:88": "aprasiddhārthaprayuktaṃ gūḍhārtham // VKal_2,1.14 //",
  "GK16vamkalsu:89": "asamyārthāntaramasamyasmṛtihetuścāślīlam // VKal_2,1.15 //",
  "GK16vamkalsu:90": "na guptalakṣitasaṃvṛtāni // VKal_2,1.16 //",
  "GK16vamkalsu:91": "aprasiddhāsabhyaṃ guptam // VKal_2,1.17 //",
  "GK16vamkalsu:92": "lākṣaṇikāsabhyānvitaṃ lakṣitam // VKal_2,1.18 //",
  "GK16vamkalsu:93": "lokasaṃvītaṃ saṃvṛtam // VKal_2,1.19 //",
  "GK16vamkalsu:94": "tantraividhyam, vrīḍājugupsāmaṅgalātaṅkadāyibhedāt // VKal_2,1.20 //*",
  "GK16vamkalsu:95": "vyavahitārthapratyayaṃ kliṣṭam // VKal_2,1.21 //",
  "GK16vamkalsu:96": "antyābhyāṃ vākyaṃ vyākhyātam // VKal_2,1.22 //",
  "GK16vamkalsu:97": "--------------------",
  "GK16vamkalsu:98": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:99": "bhinnavṛttayatibhraṣṭavisaṃdhīni vākyāni // VKal_2,2.1 //",
  "GK16vamkalsu:100": "svalakṣaṇacyutavṛttaṃ bhinnavṛttam // VKal_2,2.2 //",
  "GK16vamkalsu:101": "virasavirāmaṃ yatibhraṣṭam // VKal_2,2.3 //",
  "GK16vamkalsu:102": "taddhātunāmabhāgabhede svarasaṃdhyakṛte prāyeṇa // VKal_2,2.4 //",
  "GK16vamkalsu:103": "na vṛttadoṣātpṛthagyatidpṣaḥ, vṛttasya yatyātmakatvāt // VKal_2,2.5 //*",
  "GK16vamkalsu:104": "na, lakṣaṇaḥ pṛthaktvāt // VKal_2,2.6 //*",
  "GK16vamkalsu:105": "virūpapadasaṃdhi visaṃdhiḥ // VKal_2,2.7 //",
  "GK16vamkalsu:106": "padasaṃdhervairūpyaṃ viśleṣo 'ślīlatvaṃ kaṣṭatvaṃ ca // VKal_2,2.8 //",
  "GK16vamkalsu:107": "vyarthaikārthasaṃdigdhāyuktāpakramalokavidyā viruddhāni ca // VKal_2,2.9 //",
  "GK16vamkalsu:108": "vyāhatapūrvottarārthaṃ vyartham // VKal_2,2.10 //",
  "GK16vamkalsu:109": "uktārthapadamekārtham // VKal_2,2.11 //",
  "GK16vamkalsu:110": "na, viśeṣaścedekārthaṃ duṣṭam // VKal_2,2.12 //*",
  "GK16vamkalsu:111": "dhanurjyādhvanau dhanuḥśrutirārūḍheḥ pratipattyai // VKal_2,2.13 //",
  "GK16vamkalsu:112": "karṇāvataṃsaśravaṇakuṇḍalaśiraḥśekhareṣu karṇādinirdeśaḥ saṃnidheḥ // VKal_2,2.14 //",
  "GK16vamkalsu:113": "muktāhāraśabde muktāśabdaḥ śuddheḥ // VKal_2,2.15 //",
  "GK16vamkalsu:114": "puṣpamālāśabde puṣpapadamutkarṣasya // VKal_2,2.16 //",
  "GK16vamkalsu:115": "karikalabhaśabde kariśabdastādrūpyasya // VKal_2,2.17 //",
  "GK16vamkalsu:116": "viśeṣaṇasya ca // VKal_2,2.18 //",
  "GK16vamkalsu:117": "tadidaṃ prayukteṣu // VKal_2,2.19 //",
  "GK16vamkalsu:118": "saṃśayakṛtsaṃdigdham // VKal_2,2.20 //",
  "GK16vamkalsu:119": "māyādivikalpitārthamprayuktam // VKal_2,2.21 //",
  "GK16vamkalsu:120": "kramahīnārthamapakramam // VKal_2,2.22 //",
  "GK16vamkalsu:121": "deśakālasvabhāvaviruddhārthāni lokaviruddhāni // VKal_2,2.23 //",
  "GK16vamkalsu:122": "kalācaturvargaśāstraviruddhārthāni vidyāviruddhāni // VKal_2,2.24 //",
  "GK16vamkalsu:123": "samāotaṃ cedaṃ doṣadarśanam dvitīyamadhikaraṇam /",
  "GK16vamkalsu:124": "-----------------------------------------------",
  "GK16vamkalsu:125": "guṇavivecanaṃ nāma tṛtīyamadhikaraṇam /",
  "GK16vamkalsu:126": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:127": "kāvyaśobhāyāḥ kartāro dharmā guṇāḥ // VKal_3,1.1 //",
  "GK16vamkalsu:128": "tadatiśayahetavastvalaṅkārāḥ // VKal_3,1.2 //",
  "GK16vamkalsu:129": "pūrve nityāḥ // VKal_3,1.3 //",
  "GK16vamkalsu:130": "ojaḥprasādaśleṣasamatāsamādhimādhuryasaukumāryodāratārthavyaktikāntayo bandhaguṇāḥ // VKal_3,1.4 //",
  "GK16vamkalsu:131": "gāḍhabandhatvamojaḥ // VKal_3,1.5 //",
  "GK16vamkalsu:132": "śaithilyaṃ prasādaḥ // VKal_3,1.6 //",
  "GK16vamkalsu:133": "guṇaḥ saṃplavāt // VKal_3,1.7 //",
  "GK16vamkalsu:134": "sa tvanubhavasiddhaḥ // VKal_3,1.8 //",
  "GK16vamkalsu:135": "sāmyotkarṣau ca // VKal_3,1.9 //",
  "GK16vamkalsu:136": "masṛṇatvaṃ śleṣaḥ // VKal_3,1.10 //",
  "GK16vamkalsu:137": "mārgāvhedaḥ samatā // VKal_3,1.11 //",
  "GK16vamkalsu:138": "ārohāvarohakramaḥ samādhiḥ // VKal_3,1.12 //",
  "GK16vamkalsu:139": "na pṛthak, ārohāvarohayorojaḥprasādarūpatvāt // VKal_3,1.13 //*",
  "GK16vamkalsu:140": "na, asaṃpṛktatvāt // VKal_3,1.14 //*",
  "GK16vamkalsu:141": "anaikāntyācca // VKal_3,1.15 //",
  "GK16vamkalsu:142": "ojaḥprasādayoḥ tīvrāvasthā, tāviti cedabhyupagamaḥ // VKal_3,1.16 //*",
  "GK16vamkalsu:143": "viśeṣāpekṣitvāttayoḥ // VKal_3,1.17 //",
  "GK16vamkalsu:144": "ārohāvarohanimittaṃ samādhirākhyāyate // VKal_3,1.18 //",
  "GK16vamkalsu:145": "kramavidhānārthaṃ vā // VKal_3,1.19 //",
  "GK16vamkalsu:146": "pṛthakpadatvaṃ mādhuryam // VKal_3,1.20 //",
  "GK16vamkalsu:147": "ajaraṭhatvaṃ saukumāryam // VKal_3,1.21 //",
  "GK16vamkalsu:148": "vikaṭatvamudāratā // VKal_3,1.22 //",
  "GK16vamkalsu:149": "arthavyaktihetutvamarthavyaktiḥ // VKal_3,1.23 //",
  "GK16vamkalsu:150": "aujjvalyaṃ kāntiḥ // VKal_3,1.24 //",
  "GK16vamkalsu:151": "nāsantaḥ, sadvedyatvāt // VKal_3,1.25 //*",
  "GK16vamkalsu:152": "na bhrāntāḥ, niṣkampatvāt // VKal_3,1.26 //*",
  "GK16vamkalsu:153": "na pāṭhadharmāḥ, sarvatrādṛṣṭeḥ // VKal_3,1.27 //*",
  "GK16vamkalsu:154": "--------------------",
  "GK16vamkalsu:155": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:156": "ta evārthaguṇāḥ // VKal_3,2.1 //",
  "GK16vamkalsu:157": "arthasya prauḍhirojaḥ // VKal_3,2.2 //",
  "GK16vamkalsu:158": "arthavaimalyaṃ prasādaḥ // VKal_3,2.3 //",
  "GK16vamkalsu:159": "ghaṭanā śleṣaḥ // VKal_3,2.4 //",
  "GK16vamkalsu:160": "avaiṣamyaṃ samatā // VKal_3,2.5 //",
  "GK16vamkalsu:161": "arthadṛṣṭiḥ samādhiḥ // VKal_3,2.6 //",
  "GK16vamkalsu:162": "artho dvividhaḥ, ayoniranyacchāyāyonirvā // VKal_3,2.7 //*",
  "GK16vamkalsu:163": "artho vyaktaḥ sūkṣmaśca // VKal_3,2.8 //",
  "GK16vamkalsu:164": "sūkṣmo bhāvyo vāsanīyaśca // VKal_3,2.9 //",
  "GK16vamkalsu:165": "uktivaicitryaṃ mādhuryam // VKal_3,2.10 //",
  "GK16vamkalsu:166": "apāruṣyaṃ saukumāryam // VKal_3,2.11 //",
  "GK16vamkalsu:167": "agrāmyatvamudāratā // VKal_3,2.12 //",
  "GK16vamkalsu:168": "vastusvabhāvasphuṭatvamarthavyaktiḥ // VKal_3,2.13 //",
  "GK16vamkalsu:169": "dīptarasatvaṃ kāntiḥ // VKal_3,2.14 //",
  "GK16vamkalsu:170": "samāotaṃ cedaṃ guṇavivecanaṃ tṛtīyamadhikaraṇam /",
  "GK16vamkalsu:171": "-----------------------------------------------",
  "GK16vamkalsu:172": "ālaṅkārikaṃ nāma caturthamadhikaraṇam /",
  "GK16vamkalsu:173": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:174": "padamanaikārthamakṣaraṃ cāvṛttaṃ sthānaniyame yamakam // VKal_4,1.1 //",
  "GK16vamkalsu:175": "pādāḥ pādasyaikasyānekasya cādimadhyāntabhāgāḥ sthānāni // VKal_4,1.2 //",
  "GK16vamkalsu:176": "bhaṅgādutkarṣaḥ // VKal_4,1.3 //",
  "GK16vamkalsu:177": "śṛṅkhalā parivartakaścūrṇamiti bhaṅgamārgaḥ // VKal_4,1.4 //",
  "GK16vamkalsu:178": "varṇavicchedacalanaṃ śṛṅkhalā // VKal_4,1.5 //",
  "GK16vamkalsu:179": "saṅgavinivṛttau svarūpāpattiḥ parivartakaḥ // VKal_4,1.6 //",
  "GK16vamkalsu:180": "piṇḍākṣarabhede svarūpalopaścūrṇam // VKal_4,1.7 //",
  "GK16vamkalsu:181": "śeṣaḥ sarūpo 'nuprāsaḥ // VKal_4,1.8 //",
  "GK16vamkalsu:182": "anulbaṇo varṇānuprāsaḥ śreyān // VKal_4,1.9 //",
  "GK16vamkalsu:183": "pādānuprāsaḥ pādayamakavat // VKal_4,1.10 //",
  "GK16vamkalsu:184": "--------------------",
  "GK16vamkalsu:185": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:186": "upamānenopameyasya guṇaleśataḥ sāmyamupamā // VKal_4,2.1 //",
  "GK16vamkalsu:187": "guṇabāhulyataśca kalpitā // VKal_4,2.2 //",
  "GK16vamkalsu:188": "taddvaividhyam, padavākyārthavṛttibhedāt // VKal_4,2.3 //*",
  "GK16vamkalsu:189": "sā pūṇā luptā ca // VKal_4,2.4 //",
  "GK16vamkalsu:190": "guṇadyotakopamānopameyaśabdānāṃ sāmagrye pūrṇā // VKal_4,2.5 //",
  "GK16vamkalsu:191": "lope luptā // VKal_4,2.6 //",
  "GK16vamkalsu:192": "stutinindātattvākhyāneṣu // VKal_4,2.7 //",
  "GK16vamkalsu:193": "hīnatvādhikatvaliṅgavacanabhedāsādṛśyāsaṃbhavāstaddoṣāḥ // VKal_4,2.8 //",
  "GK16vamkalsu:194": "jātipramāṇadharmanyūnatopamānasya hīnatvam // VKal_4,2.9 //",
  "GK16vamkalsu:195": "dharmayorekanirdeśe 'nyasya saṃvit, sāhacaryāt // VKal_4,2.10 //*",
  "GK16vamkalsu:196": "tenādhikarvaṃ vyākhyātam // VKal_4,2.11 //",
  "GK16vamkalsu:197": "upamānopameyayorliṅgavyatyāso liṅgabhedaḥ // VKal_4,2.12 //",
  "GK16vamkalsu:198": "iṣṭaḥ puṃpuṃsakayoḥ prāyeṇa // VKal_4,2.13 //",
  "GK16vamkalsu:199": "laukikyāṃ samāsābhihitāyāmupamāprapañce ca // VKal_4,2.14 //",
  "GK16vamkalsu:200": "tena vacanabhedo vyākhyātaḥ // VKal_4,2.15 //",
  "GK16vamkalsu:201": "apratītaguṇasādṛśyamasādṛśyam // VKal_4,2.16 //",
  "GK16vamkalsu:202": "asādṛśyahatā hyupamā, tanniṣṭhāśca kavayaḥ // VKal_4,2.17 //*",
  "GK16vamkalsu:203": "upamānādhikyāttadapoha ityeke // VKal_4,2.18 //",
  "GK16vamkalsu:204": "na, apuṣṭārthatvāt // VKal_4,2.19 //*",
  "GK16vamkalsu:205": "anupapattirasaṃbhavaḥ // VKal_4,2.20 //",
  "GK16vamkalsu:206": "na viruddho 'tiśayaḥ // VKal_4,2.21 //",
  "GK16vamkalsu:207": "--------------------",
  "GK16vamkalsu:208": "tṛtīyo 'dhyāyaḥ /",
  "GK16vamkalsu:209": "prativastuprabhṛtirupamāprapañcaḥ // VKal_4,3.1 //",
  "GK16vamkalsu:210": "upameyasyoktau samānavastunyāsaḥ prativastu // VKal_4,3.2 //",
  "GK16vamkalsu:211": "anuktau samāsoktiḥ // VKal_4,3.3 //",
  "GK16vamkalsu:212": "kiñciduktāvaprastutapraśaṃsā // VKal_4,3.4 //",
  "GK16vamkalsu:213": "samena vastunānyāpalāpo 'pahnutiḥ // VKal_4,3.5 //",
  "GK16vamkalsu:214": "upamānenopameyasya guṇasāmyāttattvāropo rūpakam // VKal_4,3.6 //",
  "GK16vamkalsu:215": "sa dharmeṣu tantraprayoge śleṣaḥ // VKal_4,3.7 //",
  "GK16vamkalsu:216": "sādṛśyāllakṣaṇā vakroktiḥ // VKal_4,3.8 //",
  "GK16vamkalsu:217": "atadrūpasyānyathādhyavasānamatiśayārthamutprekṣā // VKal_4,3.9 //",
  "GK16vamkalsu:218": "saṃbhāvyadharmatadutkarṣakalpanātiśayoktiḥ // VKal_4,3.10 //",
  "GK16vamkalsu:219": "upamānopameyasaṃśayaḥ saṃdehaḥ // VKal_4,3.11 //",
  "GK16vamkalsu:220": "viruddhābhāsatvaṃ virodhaḥ // VKal_4,3.12 //",
  "GK16vamkalsu:221": "kriyāpratiṣedhe prasiddhatatphalavyaktirvibhāvanā // VKal_4,3.13 //",
  "GK16vamkalsu:222": "ekasyopameyatvopamānatve 'nanvayaḥ // VKal_4,3.14 //",
  "GK16vamkalsu:223": "krameṇopameyopamā // VKal_4,3.15 //",
  "GK16vamkalsu:224": "samavisadṛśābhyāṃ parivartanaṃ parivṛttiḥ // VKal_4,3.16 //",
  "GK16vamkalsu:225": "upameyopamānānāṃ kramasaṃbandhaḥ kramaḥ // VKal_4,3.17 //",
  "GK16vamkalsu:226": "upamānopameyavākyeṣvekā kriyā dīpakam // VKal_4,3.18 //",
  "GK16vamkalsu:227": "tantrividham / ādimadhyāntavākyavṛttibhedāt // VKal_4,3.19 //*",
  "GK16vamkalsu:228": "kriyayaiva svatadarthānvayakhyāpanaṃ nidarśanam // VKal_4,3.20 //",
  "GK16vamkalsu:229": "uktisiddhyai vastuno 'rthāntarasyaiva nyasanamarthāntaranyāsaḥ // VKal_4,3.21 //",
  "GK16vamkalsu:230": "upameyasya guṇātirekatvaṃ vyatirekaḥ // VKal_4,3.22 //",
  "GK16vamkalsu:231": "ekaguṇahānikalpanāyāṃ sāmyadārḍhyaṃ viśeṣoktiḥ // VKal_4,3.23 //",
  "GK16vamkalsu:232": "saṃbhāvyaviśiṣṭakarmākaraṇānnindāstotrārthā vyājastutiḥ // VKal_4,3.24 //",
  "GK16vamkalsu:233": "vyājasya satyasārūpyaṃ vyājoktiḥ // VKal_4,3.25 //",
  "GK16vamkalsu:234": "viśiṣṭena sāmyārthamekakālakriyāyogastulyayogitā // VKal_4,3.26 //",
  "GK16vamkalsu:235": "upamānākṣepaścākṣepaḥ // VKal_4,3.27 //",
  "GK16vamkalsu:236": "vastudvayakriyayostulyakālayorekapadābhidhānaṃ sahoktiḥ // VKal_4,3.28 //",
  "GK16vamkalsu:237": "yatsādṛśyaṃ tatsaṃpattiḥ samāhitam // VKal_4,3.29 //",
  "GK16vamkalsu:238": "alaṅkārasyālaṅkārayonitvaṃ saṃsṛṣṭiḥ // VKal_4,3.30 //",
  "GK16vamkalsu:239": "tadbhedāvupamārūpakotprekṣāvayavau // VKal_4,3.31 //",
  "GK16vamkalsu:240": "upamājanyaṃ rūpakamupamārūpakam // VKal_4,3.32 //",
  "GK16vamkalsu:241": "utprekṣāheturutprekṣāvayavaḥ // VKal_4,3.33 //",
  "GK16vamkalsu:242": "samāotaṃ cedamālaṅkārikaṃ caturthamadhikaraṇam /",
  "GK16vamkalsu:243": "-----------------------------------------------",
  "GK16vamkalsu:244": "prāyogikaṃ nāma pañcamamadhikaraṇam /",
  "GK16vamkalsu:245": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:246": "naikaṃ padaṃ dviḥ prayojyaṃ prāyeṇa // VKal_5,1.1 //",
  "GK16vamkalsu:247": "nityā saṃhitaikapadavatpādeṣvardhāntavarjam // VKal_5,1.2 //",
  "GK16vamkalsu:248": "na pādāntalaghorgurutvaṃ ca sarvatra // VKal_5,1.3 //",
  "GK16vamkalsu:249": "na gādye samāptaprāyaṃ vṛttam, anyatrodgatādibhyaḥ saṃvādāt // VKal_5,1.4 //*",
  "GK16vamkalsu:250": "na pādādau khalvādayaḥ // VKal_5,1.5 //",
  "GK16vamkalsu:251": "nārdhe kiñcitsamāptaṃ vākyam // VKal_5,1.6 //",
  "GK16vamkalsu:252": "na karmadhārayo bahuvrīhipratipattikaraḥ // VKal_5,1.7 //",
  "GK16vamkalsu:253": "tena viparyayo vyākhyātaḥ // VKal_5,1.8 //",
  "GK16vamkalsu:254": "saṃbhāvyaniṣedhanivartane dvau pratiṣedhau // VKal_5,1.9 //",
  "GK16vamkalsu:255": "viśeṣaṇamātraprayogo viśeṣyapratipattau // VKal_5,1.10 //",
  "GK16vamkalsu:256": "sarvanāmnānusaṃdhirvṛtticchannasya // VKal_5,1.11 //",
  "GK16vamkalsu:257": "saṃbandhasaṃbandhe 'pi ṣaṣṭhī kvacit // VKal_5,1.12 //",
  "GK16vamkalsu:258": "atiprayuktaṃ deśabhāṣāpadam // VKal_5,1.13 //",
  "GK16vamkalsu:259": "liṅgādhyāhārau // VKal_5,1.14 //",
  "GK16vamkalsu:260": "lakṣaṇāśabdāśca // VKal_5,1.15 //",
  "GK16vamkalsu:261": "na tadbāhulyamekatra // VKal_5,1.16 //",
  "GK16vamkalsu:262": "stanādīnāṃ dvitvāviṣṭā jātiḥ prāyeṇa // VKal_5,1.17 //",
  "GK16vamkalsu:263": "--------------------",
  "GK16vamkalsu:264": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:265": "rudrāvityekaśeṣo 'nveṣyaḥ // VKal_5,2.1 //",
  "GK16vamkalsu:266": "miliklabikṣapiprabhṛtīnāṃ dhātutvam, dhātuguṇasyāsamāpteḥ // VKal_5,2.2 //*",
  "GK16vamkalsu:267": "valerātmanepadamanityam, jñāpakāt // VKal_5,2.3 //*",
  "GK16vamkalsu:268": "kṣīyata iti karmakartari // VKal_5,2.4 //",
  "GK16vamkalsu:269": "khidyata iti ca // VKal_5,2.5 //",
  "GK16vamkalsu:270": "mārgerātmaepadamalakṣma // VKal_5,2.6 //",
  "GK16vamkalsu:271": "lolamānādayaścānaśi // VKal_5,2.7 //",
  "GK16vamkalsu:272": "labhergatyarthatvāṇṇicyaṇau kartuḥ karmatvākarmatve // VKal_5,2.8 //",
  "GK16vamkalsu:273": "te-me-śabdau nipāteṣu // VKal_5,2.9 //*",
  "GK16vamkalsu:274": "tiraskṛta iti paribhūte, antardhyupacārāt // VKal_5,2.10 //*",
  "GK16vamkalsu:275": "naikaśabdaḥ, supsupeti samāsāt // VKal_5,2.11 //*",
  "GK16vamkalsu:276": "madhupipāsuprabhṛtīnāṃ samāso gamigāmyādiṣu pāṭhāt // VKal_5,2.12 //",
  "GK16vamkalsu:277": "trivalīśabdaḥ siddhaḥ saṃjñā cet // VKal_5,2.13 //",
  "GK16vamkalsu:278": "bimbādhara iti vṛttau madhyamapadalopinyām // VKal_5,2.14 //",
  "GK16vamkalsu:279": "āmūlalolādiṣu vṛttirvispaṣṭapaṭuvat // VKal_5,2.15 //",
  "GK16vamkalsu:280": "na dhānyaṣaṣṭhādiṣu ṣaṣṭhīsamāsapratiṣedhaḥ, pūraṇena taddhitāntatvāt // VKal_5,2.16 //*",
  "GK16vamkalsu:281": "patrapītimādiṣu guṇavacanena // VKal_5,2.17 //",
  "GK16vamkalsu:282": "avarjyo bahuvrīhirvyadhikaraṇo janmādyuttarapadaḥ // VKal_5,2.18 //",
  "GK16vamkalsu:283": "hastāgrāgrahastādayo guṇaguṇinorbhedābhedāt // VKal_5,2.19 //",
  "GK16vamkalsu:284": "pūrvanipāte 'pabhraṃśo rakṣyaḥ // VKal_5,2.20 //",
  "GK16vamkalsu:285": "nipātenāpyabhihite karmaṇi na karmavibhaktiḥ, parigaṇanasya prāyikatvāt // VKal_5,2.21 //*",
  "GK16vamkalsu:286": "śakyamiti rūpaṃ karmābhidhāyāṃ liṅgavacanasyāpi sāmānyopakramāt // VKal_5,2.22 //",
  "GK16vamkalsu:287": "hānivadādhikyamapyaṅgānāṃ vikāraḥ // VKal_5,2.23 //",
  "GK16vamkalsu:288": "na kṛmikīṭānāmiti, ekavadbhāvaprasaṅgāt // VKal_5,2.24 //*",
  "GK16vamkalsu:289": "na kharoṣṭrau, uṣṭrakharamiti pāṭhāt // VKal_5,2.25 //*",
  "GK16vamkalsu:290": "āsetyasateḥ // VKal_5,2.26 //",
  "GK16vamkalsu:291": "yudhyediti yudhaḥ kyaci // VKal_5,2.27 //",
  "GK16vamkalsu:292": "viralāyamānādiṣu kyaṅ nirūpyaḥ // VKal_5,2.28 //",
  "GK16vamkalsu:293": "ahetau hanterṇic, curādipāṭhāt // VKal_5,2.29 //*",
  "GK16vamkalsu:294": "anucarīti careṣṭittvāt // VKal_5,2.30 //",
  "GK16vamkalsu:295": "kesarālamityalateraṇi // VKal_5,2.31 //",
  "GK16vamkalsu:296": "patralamiti lāteḥ ke // VKal_5,2.32 //",
  "GK16vamkalsu:297": "mahīdhrādayo mūlavibhujādidarśanāt // VKal_5,2.33 //",
  "GK16vamkalsu:298": "brahmādiṣu hanterniyamādarihādyasiddhiḥ // VKal_5,2.34 //",
  "GK16vamkalsu:299": "brahmavidādayaḥ kṛdantavṛttyā // VKal_5,2.35 //",
  "GK16vamkalsu:300": "tairmahīdharādayo vyākhyātāḥ // VKal_5,2.36 //",
  "GK16vamkalsu:301": "bhidurādayaḥ karmakartari kartari ca // VKal_5,2.37 //",
  "GK16vamkalsu:302": "guṇavistarādayaścintyāḥ // VKal_5,2.38 //",
  "GK16vamkalsu:303": "avatarāpacāyaśabdayordīrghahrasvatvavyatyāso bālānām // VKal_5,2.39 //",
  "GK16vamkalsu:304": "śobheti nipātanāt // VKal_5,2.40 //",
  "GK16vamkalsu:305": "avidhau guroḥ striyāṃ bahulaṃ vivakṣā // VKal_5,2.41 //",
  "GK16vamkalsu:306": "vyavasitādiṣu ktaḥ kartari, cakārāt // VKal_5,2.42 //*",
  "GK16vamkalsu:307": "āheti bhūte 'nyaṇalantabhramād bruvo laṭi // VKal_5,2.43 //",
  "GK16vamkalsu:308": "śabalādibhyaḥ striyāṃ ṭāpo 'prāptiḥ // VKal_5,2.44 //",
  "GK16vamkalsu:309": "prāṇini nīleti cintyam // VKal_5,2.45 //",
  "GK16vamkalsu:310": "manuṣyajātervivakṣāvivakṣe // VKal_5,2.46 //",
  "GK16vamkalsu:311": "ūkārāntādapyūṅ, pravṛtteḥ // VKal_5,2.47 //*",
  "GK16vamkalsu:312": "kārtikīya iti ṭhañ durdharaḥ // VKal_5,2.48 //",
  "GK16vamkalsu:313": "śārvaramiti ca // VKal_5,2.49 //",
  "GK16vamkalsu:314": "śāśvatamiti prayukteḥ // VKal_5,2.50 //",
  "GK16vamkalsu:315": "rājavaṃśyādayaḥ sādhvarthe ye bhavanti // VKal_5,2.51 //",
  "GK16vamkalsu:316": "dāravaśabdo duṣprayuktaḥ // VKal_5,2.52 //",
  "GK16vamkalsu:317": "mugdhimādiṣvivamanij mṛgyaḥ // VKal_5,2.53 //",
  "GK16vamkalsu:318": "aupamyādayaścāturvarṇyavat // VKal_5,2.54 //",
  "GK16vamkalsu:319": "ṣyañaḥ ṣitkaraṇādīkāro bahulam // VKal_5,2.55 //",
  "GK16vamkalsu:320": "dhanvīti vrīhyādiṣu pāṭhāt // VKal_5,2.56 //",
  "GK16vamkalsu:321": "caturasraśobhīti ṇinau // VKal_5,2.57 //",
  "GK16vamkalsu:322": "kañcukīyā iti kyaci // VKal_5,2.58 //",
  "GK16vamkalsu:323": "bauddhapratiyogyapekṣāyāmapyātiśāyanikāḥ // VKal_5,2.59 //",
  "GK16vamkalsu:324": "kauśilādaya ilaci varṇalopāt // VKal_5,2.60 //",
  "GK16vamkalsu:325": "mauktikamiti vinayādipāṭhāt // VKal_5,2.61 //",
  "GK16vamkalsu:326": "prātibhādayaḥ prajñādiṣu // VKal_5,2.62 //",
  "GK16vamkalsu:327": "na sarajasamityanavyayībhāve // VKal_5,2.63 //",
  "GK16vamkalsu:328": "dhṛtadhanuṣītyasaṃjñāyām // VKal_5,2.64 //",
  "GK16vamkalsu:329": "durgandhipada iddurlabhaḥ // VKal_5,2.65 //",
  "GK16vamkalsu:330": "sudatyādayaḥ pratividheyāḥ // VKal_5,2.66 //",
  "GK16vamkalsu:331": "kṣatadṛḍhorsa iti na kap, tadantavidhipratiṣedhāt // VKal_5,2.67 //*",
  "GK16vamkalsu:332": "avaihīti vṛddhiravadyā // VKal_5,2.68 //",
  "GK16vamkalsu:333": "apāṅganetreti lugalabhyaḥ // VKal_5,2.69 //",
  "GK16vamkalsu:334": "neṣṭhāḥ śliṣṭapriyādayaḥ, puṃvadbhāvapratiṣedhāt // VKal_5,2.70 //*",
  "GK16vamkalsu:335": "dṛḍhabhaktiriti sarvatreti // VKal_5,2.71 //",
  "GK16vamkalsu:336": "jambulatādayo hrasvavidheḥ // VKal_5,2.72 //",
  "GK16vamkalsu:337": "tilakādayo 'jirādiṣu // VKal_5,2.73 //",
  "GK16vamkalsu:338": "niśamya-niśamayyaśabdau prakṛtibhedāt // VKal_5,2.74 //",
  "GK16vamkalsu:339": "saṃyamyaniyamyaśabdau, aṇijantatvāt // VKal_5,2.75 //*",
  "GK16vamkalsu:340": "prapīyeti pīṅaḥ // VKal_5,2.76 //",
  "GK16vamkalsu:341": "dūrayatīti bahulagrahaṇāt // VKal_5,2.77 //",
  "GK16vamkalsu:342": "gacchatīprabhṛtiṣvaniṣedhyo num // VKal_5,2.78 //",
  "GK16vamkalsu:343": "mitreṇa goptreti puṃvadbhāvāt // VKal_5,2.79 //",
  "GK16vamkalsu:344": "vetsyasīti padabhaṅgāt // VKal_5,2.80 //",
  "GK16vamkalsu:345": "kāmayānaśabdaḥ siddho 'nādiścet // VKal_5,2.81 //",
  "GK16vamkalsu:346": "sauhṛd-daurhṛdaśabdāvaṇi hṛdbhāvāt // VKal_5,2.82 //",
  "GK16vamkalsu:347": "virama iti nipātanāt // VKal_5,2.83 //",
  "GK16vamkalsu:348": "uparyādiṣu sāmīpye dvirukteṣu dvitīyā // VKal_5,2.84 //",
  "GK16vamkalsu:349": "mandaṃ mandamityaprakārārthatve // VKal_5,2.85 //",
  "GK16vamkalsu:350": "na nidrādrugiti, bhaṣbhāvaprāpteḥ // VKal_5,2.86 //*",
  "GK16vamkalsu:351": "niṣpanda iti patvaṃ cintyam // VKal_5,2.87 //",
  "GK16vamkalsu:352": "nāṅgulisaṅga iti mūrdhanyavidheḥ // VKal_5,2.88 //",
  "GK16vamkalsu:353": "tenāvantisenādayaḥ pratyuktāḥ // VKal_5,2.89 //",
  "GK16vamkalsu:354": "nendravāhane ṇatvam; āhitatvasyāvivakṣitatvāt // VKal_5,2.90 //*",
  "GK16vamkalsu:355": "samāptaṃ cedaṃ prāyogikaṃ pañcamadhikaraṇam /",
  "GK16vamkalsu:356": "samāpto 'yaṃ granthaḥ //"
}