{
    "GSP29antarkxu:0": " AnTs_1ab nidhāya hṛdi viśveśaṃ vidhāya guru-vandanam /",
    "GSP29antarkxu:1": " AnTs_1cd bālānāṃ sukha-bodhāya kriyate tarka-saṃgrahaḥ //",
    "GSP29antarkxu:2": " AnTs_2 dravya-guṇa-karma-sāmānya-viśeṣa-samavāya-abhāvāḥ sapta pada-arthāḥ",
    "GSP29antarkxu:3": " AnTs_3 tatra dravyāṇi pṛthivy-ap-tejo-vāyv-ākāśa-kāla-dig-ātma-manāṃsi",
    "GSP29antarkxu:4": " nava-eva //",
    "GSP29antarkxu:5": " AnTs_4",
    "GSP29antarkxu:6": " rūpa-rasa-gandha-sparśa-saṃkhyā-parimāṇa-pṛthaktva-saṃyoga-vibhāga-paratva-aparatva-gurutva-dravatva-sneha-śabda-buddhi-sukha-duḥkha-icchā-dveṣa-prayatna-dharma-adharma-saṃskārāś",
    "GSP29antarkxu:7": " caturviṃśati guṇāḥ //",
    "GSP29antarkxu:8": " AnTs_5 utkṣepaṇa-avakṣepaṇa-ākuñcana-prasāraṇa-gamanāni pañca karmāṇi //",
    "GSP29antarkxu:9": " AnTs_6 param aparaṃ ca+iti dvividhaṃ sāmānyam //",
    "GSP29antarkxu:10": " AnTs_7 nitya-dravya-vṛttayo viśeṣās tv an-antā eva //",
    "GSP29antarkxu:11": " AnTs_8 samavāyas tv eka eva //",
    "GSP29antarkxu:12": " AnTs_9 abhāvaś caturvidhaḥ /",
    "GSP29antarkxu:13": " prāg-abhāvaḥ pradhvaṃsa-abhāvo 'tyanta-abhāvo",
    "GSP29antarkxu:14": " 'nyonya-abhāvaś ca+iti //",
    "GSP29antarkxu:15": " AnTs_10 gandhavatī pṛthivī /",
    "GSP29antarkxu:16": " sā dvividhā nitya-anityā ca /",
    "GSP29antarkxu:17": " nityā",
    "GSP29antarkxu:18": " paramāṇu-rūpā /",
    "GSP29antarkxu:19": " anityā kārya-rūpā /",
    "GSP29antarkxu:20": " punas trividhā śarīra-indriya-viṣaya-bhedāt /",
    "GSP29antarkxu:21": " śarīram asmad-ādīnām /",
    "GSP29antarkxu:22": " indriyaṃ",
    "GSP29antarkxu:23": " gandha-grāhakaṃ ghrāṇaṃ nāsa-agravarti /",
    "GSP29antarkxu:24": " viṣayo mṛt-pāṣāṇa-ādiḥ //",
    "GSP29antarkxu:25": " AnTs_11 śita-sparśavatya āpaḥ /",
    "GSP29antarkxu:26": " tā dvividhāḥ nityā anityāś ca /",
    "GSP29antarkxu:27": " ntyāḥ",
    "GSP29antarkxu:28": " paramāṇu-rūpāḥ /",
    "GSP29antarkxu:29": " anityāḥ kārya-rūpāḥ. /",
    "GSP29antarkxu:30": " punas trividhāḥ.",
    "GSP29antarkxu:31": " śarīra-indriya-viṣaya-bhedāt /",
    "GSP29antarkxu:32": " śarīraṃ varuṇa-loke /",
    "GSP29antarkxu:33": " indriyaṃ",
    "GSP29antarkxu:34": " rasa-grāhakaṃ rasanaṃ jihva-agravarti /",
    "GSP29antarkxu:35": " viṣayaḥ sarit-samudra-ādiḥ //",
    "GSP29antarkxu:36": " AnTs_12 uṣṇa-sparśavat tejaḥ /",
    "GSP29antarkxu:37": " tad dvividhaṃ nityam anityaṃ ca /",
    "GSP29antarkxu:38": " niyyaṃ",
    "GSP29antarkxu:39": " paramāṇu-rūpam /",
    "GSP29antarkxu:40": " anityaṃ kārya-rūpam /",
    "GSP29antarkxu:41": " punas trividhaṃ śarīra-indriya-viṣaya-bhedāt /",
    "GSP29antarkxu:42": " śarīram ādityaloke /",
    "GSP29antarkxu:43": " indriyaṃ rūpa-grāhakaṃ",
    "GSP29antarkxu:44": " cakṣuḥ kṛṣṇatāra-agravarti /",
    "GSP29antarkxu:45": " viṣayaś caturvidhaḥ /",
    "GSP29antarkxu:46": " bhauma-divya-audarya-ākaraja-bhedāt /",
    "GSP29antarkxu:47": " bhaumaṃ vahny-ādikam /",
    "GSP29antarkxu:48": " abindhanaṃ divyaṃ vidyud-ādi /",
    "GSP29antarkxu:49": " bhuktasya pariṇāma-hetur audaryam. ākarajaṃ",
    "GSP29antarkxu:50": " suvarṇa-ādi //",
    "GSP29antarkxu:51": " AnTs_13 rūpa-rahita-sparśavān vāyuḥ /",
    "GSP29antarkxu:52": " sa dvividho nityo 'nityaś ca /",
    "GSP29antarkxu:53": " nityaḥ paramāṇu-rūpaḥ /",
    "GSP29antarkxu:54": " anityaḥ kārya-rūpaḥ /",
    "GSP29antarkxu:55": " punas trividhaḥ śarīra-indriya-viṣaya-bhedāt /",
    "GSP29antarkxu:56": " śarīraṃ vāyu-loke /",
    "GSP29antarkxu:57": " indriyaṃ",
    "GSP29antarkxu:58": " sparśa-grāhakaṃ tvak-sarva-śarīravarti /",
    "GSP29antarkxu:59": " viṣayo vṛkṣa-ādi-kampana-hetuḥ //",
    "GSP29antarkxu:60": " śarīra-antaḥsaṃcārī vāyuḥ prāṇaḥ /",
    "GSP29antarkxu:61": " sa ca+eko 'py upādhi-bhedāt",
    "GSP29antarkxu:62": " prāṇa-apāna-ādi-saṃjñā labhate //",
    "GSP29antarkxu:63": " AnTs_14 śabda-guṇam ākāśam /",
    "GSP29antarkxu:64": " tac ca+ekaṃ vibhu nityaṃ ca //",
    "GSP29antarkxu:65": " AnTs_15 atīta-ādi-vyavahāra-hetuḥ kālaḥ /",
    "GSP29antarkxu:66": " sa ca+eko vibhur nityaś ca //",
    "GSP29antarkxu:67": " AnTs_16 prācya-ādi-vyavahāra-hetur dik /",
    "GSP29antarkxu:68": " sā ca+ekā vibhvī nityā ca //",
    "GSP29antarkxu:69": " AnTs_17 jñāna-adhikaraṇam ātmā /",
    "GSP29antarkxu:70": " sa dvividhaḥ param-ātmā jīva-ātmā ca /",
    "GSP29antarkxu:71": " tatra+īśvaraḥ sarvajñaḥ param-ātmā-eka eva /",
    "GSP29antarkxu:72": " jīva-ātmā pratiśarīraṃ bhinno vibhur nityaś ca //",
    "GSP29antarkxu:73": " AnTs_18 sukha-ādy-upalabdhi-sādhanam indriyaṃ manaḥ /",
    "GSP29antarkxu:74": " tac ca pratyātma-niyatatvād anantaṃ paramāṇu-rūpaṃ nityaṃ ca //",
    "GSP29antarkxu:75": " AnTs_19 cakṣur-mātra-grāhyo guṇo rūpam /",
    "GSP29antarkxu:76": " tac ca śukla-nīla-pīta-rakta-harita-kapiśa-citra-bhedāt saptavidhaṃ",
    "GSP29antarkxu:77": " pṛthivī-jala-tejo-vṛtti /",
    "GSP29antarkxu:78": " tatra pṛthivyāṃ saptavidham /",
    "GSP29antarkxu:79": " abhāsvara-śuklaṃ",
    "GSP29antarkxu:80": " jale /",
    "GSP29antarkxu:81": " bhāsvara-śuklaṃ tejasi //",
    "GSP29antarkxu:82": " AnTs_20 rasana-grāhyo guṇo rasaḥ /",
    "GSP29antarkxu:83": " sa ca madhu-rāmla-lavaṇa-kaṭu-kaṣāya-tikta-bhedāt ṣaḍvidhaḥ /",
    "GSP29antarkxu:84": " pṛthivyāṃ ṣaḍvidhaḥ /",
    "GSP29antarkxu:85": " jale madhura eva //",
    "GSP29antarkxu:86": " AnTs_21 ghrāṇa-grāhyo guṇo gandhaḥ /",
    "GSP29antarkxu:87": " sa ca dvividhaḥ surabhir-asurabhiś ca",
    "GSP29antarkxu:88": " / pṛthivī-mātra-vṛttiḥ /",
    "GSP29antarkxu:89": " AnTs_22 tvag-indriya-mātra-grāhyo guṇo saparśaḥ /",
    "GSP29antarkxu:90": " sa ca trividhaḥ",
    "GSP29antarkxu:91": " śita-uṣṇa-anuṣṇa-aśīta-bhedāt /",
    "GSP29antarkxu:92": " pṛthivy-ap-tejo-vāyu-vṛttiḥ /",
    "GSP29antarkxu:93": " tatra śīto jale /",
    "GSP29antarkxu:94": " uṣṇas tejasi /",
    "GSP29antarkxu:95": " anuṣṇa-aśītaḥ pṛthivī-vāyvoḥ //",
    "GSP29antarkxu:96": " AnTs_23 rūpa-ādi-catuṣṭayaṃ pṛthivyāṃ pākajam anityaṃ ca /",
    "GSP29antarkxu:97": " anyatra+apākajaṃ nityam antyaṃ ca /",
    "GSP29antarkxu:98": " nityagataṃ nityam /",
    "GSP29antarkxu:99": " anityagatam anityam //",
    "GSP29antarkxu:100": " AnTs_24 ekatva-ādi-vyavahāra-hetuḥ saṃkhyā /",
    "GSP29antarkxu:101": " nava-dravya-vṛttir",
    "GSP29antarkxu:102": " ekatva-ādi-parārdha-paryantā /",
    "GSP29antarkxu:103": " ekatvaṃ nityam anityaṃ ca /",
    "GSP29antarkxu:104": " nityagataṃ nityam anityagatam anityam /",
    "GSP29antarkxu:105": " dvitva-ādikaṃ tu sarvatra+a-nityam eva //",
    "GSP29antarkxu:106": " AnTs_25 māna-vyavahāra-kāraṇaṃ parimāṇaṃ /",
    "GSP29antarkxu:107": " nava-dravya-vṛtti /",
    "GSP29antarkxu:108": " tac caturvidham /",
    "GSP29antarkxu:109": " aṇu mahad dīrghaṃ hrasvaṃ ca+iti //",
    "GSP29antarkxu:110": " AnTs_26 pṛthag-vyavahāra-kāraṇaṃ pṛthagtvaṃ /",
    "GSP29antarkxu:111": " sarvadravyavṛtti //",
    "GSP29antarkxu:112": " AnTs_27 saṃyukta-vyavahāra-hetuḥ saṃyogaḥ /",
    "GSP29antarkxu:113": " sarvadravyavṛttiḥ //",
    "GSP29antarkxu:114": " AnTs_28 saṃyoga-nāśako guṇo vibhāgaḥ /",
    "GSP29antarkxu:115": " sarvadravyavṛttiḥ //",
    "GSP29antarkxu:116": " AnTs_29 para-apara-vyavahāra-asādhāraṇa-kāraṇe paratva-aparatve /",
    "GSP29antarkxu:117": " pṛthivy-ādi-catuṣṭaya-mano-vṛtti+iti /",
    "GSP29antarkxu:118": " te dvividhe dik-kṛte kāla-kṛte ca /",
    "GSP29antarkxu:119": " dūra-sthe dik-kṛtaṃ paratvam /",
    "GSP29antarkxu:120": " samīpasthe dik-kṛtam aparatvam /",
    "GSP29antarkxu:121": " jyeṣṭhe",
    "GSP29antarkxu:122": " kāla-kṛthaṃ paratvam /",
    "GSP29antarkxu:123": " kaniṣṭhe kāla-kṛtam aparatvam //",
    "GSP29antarkxu:124": " AnTs_30 ādya-patana-asamavāyi-kāraṇaṃ gurutvaṃ /",
    "GSP29antarkxu:125": " pṛthivī-jala-vṛtti //",
    "GSP29antarkxu:126": " AnTs_31 ādya-syandana-asamavāyi-kāraṇaṃ dravatvaṃ pṛthivy-ap-tejo-vṛtti /",
    "GSP29antarkxu:127": " tad dvividhaṃ sāṃsiddhikaṃ naimittikaṃ ca /",
    "GSP29antarkxu:128": " sāṃsiddhikaṃ jale naimittikaṃ",
    "GSP29antarkxu:129": " pṛthivī-tejasoḥ /",
    "GSP29antarkxu:130": " pṛthivyāṃ ghṛta-ādāv-agni-saṃyogajanyaṃ daravatvam /",
    "GSP29antarkxu:131": " tejasi suvarṇādau //",
    "GSP29antarkxu:132": " AnTs_32 cūrṇa-ādi-piṇḍī-bhāva-hetur guṇaḥ snehaḥ /",
    "GSP29antarkxu:133": " jala-mātra-vṛttiḥ //",
    "GSP29antarkxu:134": " AnTs_33 śrotra-grāhyo guṇaḥ śabdaḥ /",
    "GSP29antarkxu:135": " ākāśa-mātra-vṛttiḥ /",
    "GSP29antarkxu:136": " sa dvividho dvany-ātmako varṇa-ātmakaś ca+iti /",
    "GSP29antarkxu:137": " dvany-ātmako bhery-ādau /",
    "GSP29antarkxu:138": " varṇa-ātmakaḥ saṃskṛta-bhāṣā-ādi-rūpaḥ //",
    "GSP29antarkxu:139": " AnTs_34 sarva-vyavahāra-hetur buddhir jñānam /",
    "GSP29antarkxu:140": " sā dvividhā smṛtir-anubhavaś ca /",
    "GSP29antarkxu:141": " saṃskāra-mātra-janyaṃ jñānaṃ smṛtiḥ /",
    "GSP29antarkxu:142": " tad-bhinnaṃ",
    "GSP29antarkxu:143": " jñānam anubhavaḥ //",
    "GSP29antarkxu:144": " AnTs_35 sa dvividho yathārtho 'yathārthaś ca /",
    "GSP29antarkxu:145": " tad-vati tat-prakārako",
    "GSP29antarkxu:146": " 'nubhavo yathārthaḥ /",
    "GSP29antarkxu:147": " yathā rajata idaṃ rajatam iti jñānam /",
    "GSP29antarkxu:148": " sa eva pramā+ity ucyate /",
    "GSP29antarkxu:149": " tad-abhāva-vati tat-prakārako 'nubhavo 'yathārthaḥ /",
    "GSP29antarkxu:150": " yathā śuktāv idaṃ rajatam iti jñānam //",
    "GSP29antarkxu:151": " AnTs_36 yathārtha-anubhavaś caturvidhaḥ",
    "GSP29antarkxu:152": " pratyakṣa-anumity-upamiti-śabda-bhedāt /",
    "GSP29antarkxu:153": " tat-karaṇam api caturvidhaṃ pratyakṣa-anumāna-upamāna-śabda-bhedāt //",
    "GSP29antarkxu:154": " AnTs_37 asādhāraṇam kāraṇaṃ karaṇam /",
    "GSP29antarkxu:155": " AnTs_38 kārya-niyata-pūrva-vṛtti kāraṇam /",
    "GSP29antarkxu:156": " AnTs_39 kāryaṃ prāg-abhāva-pratiyogi /",
    "GSP29antarkxu:157": " AnTs_40 kāraṇaṃ trividhaṃ samavāyy-asamavāyi-nimitta-bhedāt /",
    "GSP29antarkxu:158": " yat samavetaṃ kāryam utpadyate tat samavāyikāraṇam /",
    "GSP29antarkxu:159": " yathā tantavaḥ paṭasya paṭaś ca khagata-rūpādeḥ /",
    "GSP29antarkxu:160": " kāryeṇa kāraṇena vā saha+ekasminn arthe samavetatve sati yat kāraṇaṃ tad asamavāyikāraṇam",
    "GSP29antarkxu:161": " yathā tantusaṃyogaḥ paṭasya tanturūpaṃ paṭarūpasya /",
    "GSP29antarkxu:162": " tad-ubhaya-bhinnaṃ kāraṇaṃ nimittakāraṇam /",
    "GSP29antarkxu:163": " yathā turī-vema-ādikaṃ paṭasya /",
    "GSP29antarkxu:164": " AnTs_41 tad etat-trividha-kāraṇa-madhye yad asādhāraṇaṃ kāraṇaṃ tad eva karaṇam //",
    "GSP29antarkxu:165": " AnTs_42 tatra pratyakṣa-jñāna-karaṇaṃ pratyakṣam /",
    "GSP29antarkxu:166": " indriya-artha-sannikarṣa-janyaṃ jñānaṃ pratyakṣam /tad dvividhaṃ nirvikalpakaṃ savikalpakaṃ ca+iti /",
    "GSP29antarkxu:167": " tatra nirvikalpakaṃ jñānaṃ nirvikalpakaṃ yathā+idaṃ kiṃcit /",
    "GSP29antarkxu:168": " saparakārakaṃ jñānaṃ savikalpakaṃ yathā",
    "GSP29antarkxu:169": " ḍittho'yam brahmaṇo'yaṃ śyāmo'yam iti //",
    "GSP29antarkxu:170": " AnTs_43 pratyakṣa-jñāna-hetur indriya-artha-sannikarṣaḥ ṣaḍvidhaḥ /",
    "GSP29antarkxu:171": " saṃyogaḥ saṃyukta-samavāyaḥ saṃyukta-samaveta-samavāyaḥ samavāyaḥ",
    "GSP29antarkxu:172": " samaveta-samavāyo viśeṣaṇa-viśeṣya-bhāvaś ca+iti /",
    "GSP29antarkxu:173": " cakṣuṣā",
    "GSP29antarkxu:174": " ghaṭa-pratyakṣa-janane saṃyogaḥ sannikarṣaḥ /",
    "GSP29antarkxu:175": " ghaṭa-rūpa-pratyakṣa-janane",
    "GSP29antarkxu:176": " saṃyukta-samavāyaḥ sannikarṣaḥ cakṣuḥsaṃyukte ghaṭe rūpasya samavāyāt /",
    "GSP29antarkxu:177": " rūpatva-sāmānyapratyakṣe saṃyukta-samaveta-samavāyaḥ sannikarṣaḥ",
    "GSP29antarkxu:178": " cakṣuḥ-saṃyukte ghaṭe rūpaṃ samavetaṃ tatra rūpatvasya samavāyāt /",
    "GSP29antarkxu:179": " śrotreṇa śabdasya+ākāśa-guṇatvāt guṇa-guṇinoś ca samavāyāt /",
    "GSP29antarkxu:180": " śabdatva-sākṣātkāre samaveta-samavāyaḥ sannikarṣaḥ śrotra-samavete śabde śabdatvasya samavāyāt",
    "GSP29antarkxu:181": " abhāvaparatyakṣe viśeṣaṇa-viśeṣya-bhāvaḥ",
    "GSP29antarkxu:182": " sannikarṣo ghaṭa-abhāvavad bhṛtalam ity atra cakṣuḥ-saṃyukte bhūtale",
    "GSP29antarkxu:183": " ghaṭa-abhāvasya viśeṣaṇatvāt /",
    "GSP29antarkxu:184": " evaṃ snnikarṣa-ṣaṭka-janyaṃ jñānaṃ pratyakṣam /",
    "GSP29antarkxu:185": " tatkaraṇam indriyam /",
    "GSP29antarkxu:186": " tasmād indriyaṃ pratyakṣa-pramāṇam iti siddham //",
    "GSP29antarkxu:187": " AnTs_44 anumiti-karaṇam anumānam /",
    "GSP29antarkxu:188": " parāmarśajanyaṃ jñānam anumitiḥ /",
    "GSP29antarkxu:189": " vyāpti-viśiṣṭa-pakṣa-dharmatā-jñānaṃ parāmarśaḥ /",
    "GSP29antarkxu:190": " yathā",
    "GSP29antarkxu:191": " vahni-vyāpya-dhūmavān ayaṃ parvata iti jñānaṃ parāmarśaḥ /",
    "GSP29antarkxu:192": " taj janyaṃ parvato vahnimān iti jñānam anumitiḥ /",
    "GSP29antarkxu:193": " yatra yatra dhūmas tatra+agnir iti",
    "GSP29antarkxu:194": " sāhacarya-niyamo vyāptiḥ /",
    "GSP29antarkxu:195": " vyāpyasya parvata-ādi-vṛttitvaṃ pakṣadharmatā",
    "GSP29antarkxu:196": " AnTs_45 anumānaṃ dvividhaṃ sva-arthaṃ para-arthaṃ ca /",
    "GSP29antarkxu:197": " tatra sva-arthaṃ",
    "GSP29antarkxu:198": " sva-anumiti-hetuḥ /",
    "GSP29antarkxu:199": " tathā hi svayam eva bhūyo darśanena yatra dhūmas tatra agnir iti mahānasādau vyāptiṃ gṛhītvā parvatasamīpaṃ gatas tad-gate",
    "GSP29antarkxu:200": " ca+agnau sandihānaḥ parvate dhūmaṃ paśyan vyāptiṃ smarati yatra dhūmas tatra+agnir iti",
    "GSP29antarkxu:201": " tad-antaraṃ vahni-vyāpya-dhūmavān ayaṃ parvata iti",
    "GSP29antarkxu:202": " jñānam utpadyate /",
    "GSP29antarkxu:203": " ayam eva liṅgaparāmarṣaṃ ity ucyate /",
    "GSP29antarkxu:204": " tasmāt parvato vahnimān iti jñānam anumitir utpadyate /",
    "GSP29antarkxu:205": " tad etat sva-artha-anumānam /",
    "GSP29antarkxu:206": " yat tu svayaṃ dhūmād agnim anumāya para-pratipatty-arthaṃ pañca-avayava-vākyaṃ",
    "GSP29antarkxu:207": " prayukte tat para-artha-anumānam /",
    "GSP29antarkxu:208": " yathā parvato vahnimān dhūmavattvāt /",
    "GSP29antarkxu:209": " yo yo dhūmavān sa vahnimān yathā mahānasaḥ /",
    "GSP29antarkxu:212": " tathā+iti /",
    "GSP29antarkxu:213": " anena pratipāditāl lingāt paro'py agniṃ pratipadyate //",
    "GSP29antarkxu:214": " AnTs_46 pratijñā-hetu-udāharaṇa-upanaya-nigamāni pañca-avayavaḥ /",
    "GSP29antarkxu:215": " parvato vahnimān iti pratijñā /",
    "GSP29antarkxu:216": " dhūmavattvād iti hetuḥ /",
    "GSP29antarkxu:217": " yo yo dhūmavān sa",
    "GSP29antarkxu:218": " so'gnimān yathā mahānasa ity udāharaṇam /",
    "GSP29antarkxu:219": " tathā ca+ayam iti upanayaḥ /",
    "GSP29antarkxu:220": " tasmāt tathā+iti nigamanam //",
    "GSP29antarkxu:221": " AnTs_47 sva-artha-anumiti-para-artha-anumityor liṅga-parāmarśa eva karaṇam /",
    "GSP29antarkxu:222": " tasmāl liṅga-parāmarśā'numānam //",
    "GSP29antarkxu:223": " AnTs_48 liṅgaṃ trividham /",
    "GSP29antarkxu:224": " anvaya-vyatireki kevala-anvayi kevala-vyatireki",
    "GSP29antarkxu:225": " ca+iti /",
    "GSP29antarkxu:226": " anvayena vyatirekeṇa ca vyāptimad anvaya-vyatireki /",
    "GSP29antarkxu:227": " yathā vahnau sādhye dhūmavattvam /",
    "GSP29antarkxu:228": " yatra dhūmas tatra+agnir yathā mahānasa ity",
    "GSP29antarkxu:229": " anvaya-vyāptiḥ /",
    "GSP29antarkxu:230": " yatra vahnir na+asti tatra dhūmo'pi na+asti yathā mahāhrada iti vyatireka-vyāptiḥ /",
    "GSP29antarkxu:231": " anvaya-mātra-vyāptikaṃ kevala-anvayi",
    "GSP29antarkxu:232": " yathā ghaṭo'bhidheyaḥ prameyatvāt paṭavat /",
    "GSP29antarkxu:233": " atra prameyatva-abhidhyatvayor",
    "GSP29antarkxu:234": " vyatirekavyāptir na+asti sarvasya+api prameyatvād abhidheyatvāc ca /",
    "GSP29antarkxu:235": " vyatirekamātravyāptikaṃ kevala-vyatireki yathā pṛthivī tarebhyo bhidyate gandhavattvāt /",
    "GSP29antarkxu:236": " yad itarebhyo na bhidyate na tad gandhavat /",
    "GSP29antarkxu:237": " yathā jalam /",
    "GSP29antarkxu:238": " na ca+iyaṃ tathā /",
    "GSP29antarkxu:239": " tasmān na tathā+iti atra yad gandhavat tad",
    "GSP29antarkxu:240": " itara-bhinnam ity anvayadṛṣṭanto na+asti pṛthivī-mātrasya pakṣatvāt //",
    "GSP29antarkxu:241": " AnTs_49 saṃdigdha-sādhyavān pakṣaḥ /",
    "GSP29antarkxu:242": " yathā dhūmavattve hetau parvataḥ //",
    "GSP29antarkxu:243": " AnTs_50 niścita-sādhyavān sapakṣaḥ /",
    "GSP29antarkxu:244": " yathā tatra+eva mahānasaḥ //",
    "GSP29antarkxu:245": " AnTs_51 niścita-sādhya-abhāvavān vipakṣaḥ /",
    "GSP29antarkxu:246": " yathā tatra+eva mahāhradaḥ //",
    "GSP29antarkxu:247": " AnTs_52 savyabhicāra-viruddha-satpakṣa-asiddha-bādhitāḥ pañca hetvābhāsāḥ //",
    "GSP29antarkxu:248": " AnTs_53 savyabhicāro'naikāntikaḥ /",
    "GSP29antarkxu:249": " sa trividhaḥ /",
    "GSP29antarkxu:250": " sādhāraṇa-asādhāraṇa-anupasaṃhāri-bhedāt /",
    "GSP29antarkxu:251": " tatra sādhya-abhāvavad-vṛttiḥ",
    "GSP29antarkxu:252": " sādhāraṇo anaikāntikaḥ /",
    "GSP29antarkxu:253": " yathā parvato vahnimān prameyatvād iti prameyatvasya vahny-abhāvavati hrade vidyamānatvāt",
    "GSP29antarkxu:254": " sarva-sapakṣa-vipakṣa-vyāvṛtto'sādhāraṇaḥ /",
    "GSP29antarkxu:255": " yathā śabdo nityaḥ śabdatvād iti /",
    "GSP29antarkxu:256": " śabdatvaṃ sarvebhyo nityebhyo'ntyebhyaś ca vyāvṛttaṃ śabda-mātra-vṛtti /",
    "GSP29antarkxu:257": " anvaya-vyatireka-dṛṣṭānta-rahito'nupasaṃhārī /",
    "GSP29antarkxu:258": " yathā sarvam anityaṃ prameyatvād iti /",
    "GSP29antarkxu:259": " atra sarvasya+api pakṣatvād dṛṣṭānto na+asti //",
    "GSP29antarkxu:260": " AnTs_54 sādhya-abhāva-vyāpto hetur viruddhaḥ /",
    "GSP29antarkxu:261": " yatra śabdo nityaḥ kṛtakatvād iti /",
    "GSP29antarkxu:262": " kṛtakatvaṃ hi nityatva-abhāvena+a-nityatvena vyāptam //",
    "GSP29antarkxu:263": " AnTs_55 yasya sādhya-bhāva-sādhakaṃ hetv-antaraṃ vidyate sa satpratipakṣaḥ",
    "GSP29antarkxu:264": " / yathā śabdo nityaḥ śrāvaṇatvāc chabdatvavad iti /",
    "GSP29antarkxu:265": " śabdo'nityaḥ kāryatvād ghaṭavad iti //",
    "GSP29antarkxu:266": " AnTs_56 asiddhas trividhaḥ /",
    "GSP29antarkxu:267": " āśraya-asiddhaḥ svarūpa-asiddho",
    "GSP29antarkxu:268": " vyāpyatva-asiddhaś ca+iti /",
    "GSP29antarkxu:269": " āśraya-asiddho yathā gagana-aravindaṃ surabhy aravindatvāt saro-ja-aravindavat /",
    "GSP29antarkxu:270": " atra gagana-aravindam āśrayaḥ /",
    "GSP29antarkxu:271": " sa ca na+asty eva /",
    "GSP29antarkxu:272": " svarūpa-asiddho yathā śabdo guṇaś cākṣuṣatvāt /",
    "GSP29antarkxu:273": " atra cākṣuṣatvaṃ śabde na+asti śabdasya śrāvaṇatvāt /",
    "GSP29antarkxu:274": " sa-upādhiko",
    "GSP29antarkxu:275": " vyāpyatva-asiddhaḥ /",
    "GSP29antarkxu:276": " sādhya-vyāpakatve sati sādhana-avyāpaka upādhiḥ /",
    "GSP29antarkxu:277": " sādhya-samāna-adhikaraṇa-atyanta-abhāva-apratiyogitvaṃ sādhya-vyāpakatvam",
    "GSP29antarkxu:278": " / sādhanavann iṣṭa-atyanta-abhāva-pratiyogitvaṃ sādhāraṇa-avyapakatvam /",
    "GSP29antarkxu:279": " parvato dhūmavān vahnimattvād ity atra+ārdra-indhasaṃyoga upādhiḥ /",
    "GSP29antarkxu:280": " tathā hi /",
    "GSP29antarkxu:281": " yatra dhūmas tatra+ārdra-indhna-saṃyoga iti sādhya-vyāpakatā /",
    "GSP29antarkxu:282": " yatra vahnis tatra+ārdra-indhana-saṃyoga-abhāvād iti sādhana-avyāpakatā /",
    "GSP29antarkxu:283": " evaṃ",
    "GSP29antarkxu:284": " sādhya-vyāpakatve sati sādhana-avyāpakatvād ardra-indhana-saṃyoga upādhiḥ",
    "GSP29antarkxu:285": " / sa-upādhikatvād vahnimattvaṃ vyāpyatva-asiddham //",
    "GSP29antarkxu:286": " AnTs_57 yasya sādhya-abhāvaḥ pramāṇa-antareṇa niścitaḥ sa bādhitaḥ /",
    "GSP29antarkxu:287": " yathā vahnir anuṣṇo dravyatvād iti /",
    "GSP29antarkxu:288": " atra+anuṣṇatvaṃ sādhyaṃ tad-abhāva uṣṇatvaṃ",
    "GSP29antarkxu:289": " spārśana-pratyakṣeṇa gṛhyata iti bādhitatvam //",
    "GSP29antarkxu:290": " AnTs_58 upamiti-karaṇam upamānam /",
    "GSP29antarkxu:291": " saṃjñā-saṃjñi-saṃbandha-jñānam upamitiḥ",
    "GSP29antarkxu:292": " / tat-karaṇaṃ sādṛśya-jñānam /",
    "GSP29antarkxu:293": " atideśa-vākya-artha-smaraṇam",
    "GSP29antarkxu:294": " avāntara-vyāpāraḥ /",
    "GSP29antarkxu:295": " tathā hi kaś cid gavaya-śabda-artham ajānan kutaś cid",
    "GSP29antarkxu:296": " āraṇyaka-puruṣād go-sadṛśo gavaya iti śrutvā vanaṃ gato vākya-arthaṃ",
    "GSP29antarkxu:297": " smaran go-sadṛṣaṃ piṇḍaṃ paśyati /",
    "GSP29antarkxu:298": " tad-anantaram asau gavaya-śabda-vācya",
    "GSP29antarkxu:299": " ity upamitir utpadyte //",
    "GSP29antarkxu:300": " AnTs_59 āpta-vākyaṃ śabdaḥ /",
    "GSP29antarkxu:301": " āptas tu yathā+artha-vaktā /",
    "GSP29antarkxu:302": " vākyaṃ",
    "GSP29antarkxu:303": " pada-samūhaḥ /",
    "GSP29antarkxu:304": " yathā gām ānaya+iti /",
    "GSP29antarkxu:305": " śaktaṃ padam /",
    "GSP29antarkxu:306": " asmāt padād ayam artho boddhavya iti+īśvara-saṃketaḥ śaktiḥ //",
    "GSP29antarkxu:307": " AnTs_60 ākāṅkṣā yogyatā saṃnidhiś ca vākya-artha-jñāna-hetuḥ /",
    "GSP29antarkxu:308": " padasya",
    "GSP29antarkxu:309": " pada-antara-vyatireka-prayukta-anvaya-ananubhāvakatvam ākāṅkṣā /",
    "GSP29antarkxu:310": " artha-abādho yogyatā /",
    "GSP29antarkxu:311": " padānām-avilambena+uccāraṇaṃ saṃnidhiḥ //",
    "GSP29antarkxu:312": " AnTs_61 ākāṅkṣā-ādi-rahitaṃ vākyam apramāṇam /",
    "GSP29antarkxu:313": " yathā gaur aśvaḥ puruṣo",
    "GSP29antarkxu:314": " hastī+iti na pramāṇam ākāṅkṣā-virahāt /",
    "GSP29antarkxu:315": " agninā siñced iti na pramāṇaṃ yogyatā-virahāt /",
    "GSP29antarkxu:316": " prahare prehare-'saha+uccāritāni gām",
    "GSP29antarkxu:317": " ānaya+ity-ādi-padāni na pramāṇaṃ saṃnidhy-abhāvāt //",
    "GSP29antarkxu:318": " AnTs_62 vākyaṃ dvividham /",
    "GSP29antarkxu:319": " vaidikaṃ laukikaṃ ca /",
    "GSP29antarkxu:320": " vaidikam īśvara-uktatvāt",
    "GSP29antarkxu:321": " sarvam eva pramāṇam /",
    "GSP29antarkxu:322": " laukikaṃ tv āpta-uktaṃ pramāṇam /",
    "GSP29antarkxu:323": " anyad apramāṇam //",
    "GSP29antarkxu:324": " AnTs_63 vākya-artha-jñānaṃ śabda-jñānam /",
    "GSP29antarkxu:325": " tat-karaṇaṃ śabdaḥ //",
    "GSP29antarkxu:326": " AnTs_64 ayathā-artha-anubhavas trividhaḥ saṃśaya-viparyaya-tarka-bhedāt /",
    "GSP29antarkxu:327": " ekasmin dharmini viruddha-nānā-dharma-vaiśiṣṭya-avagāhi jñānaṃ saṃśayaḥ /",
    "GSP29antarkxu:328": " yathā sthāṇur vā purṣo vā+iti /",
    "GSP29antarkxu:329": " mithyā-jñānaṃ viparyayaḥ /",
    "GSP29antarkxu:330": " yathā śuktāv idaṃ rajatam iti /",
    "GSP29antarkxu:331": " vyāpya+āropeṇa vyāpaka-āropas tarkaḥ yathā yadi vahnir na syāt tarhi dhūmo'pi na syād iti",
    "GSP29antarkxu:332": " AnTs_65 smṛtir api dvividhā /",
    "GSP29antarkxu:333": " yathā-artha-ayathā-arthā ca pramā-janyā",
    "GSP29antarkxu:334": " yathā-arthā /",
    "GSP29antarkxu:335": " apramā-janyā+a-yathā-arthā //",
    "GSP29antarkxu:336": " AnTs_66 sarveśām anukūlatayā vedanīyaṃ sukham //",
    "GSP29antarkxu:337": " AnTs_67 sarveśāṃ pratikūlatayā vedanīyaṃ duḥkham //",
    "GSP29antarkxu:338": " AnTs_68 icchā kāmaḥ //",
    "GSP29antarkxu:339": " AnTs_69 krodho dveṣaḥ //",
    "GSP29antarkxu:340": " AnTs_70 kṛtiḥ prayatnaḥ //",
    "GSP29antarkxu:341": " AnTs_71 vihita-karma-janyo dharmaḥ //",
    "GSP29antarkxu:342": " AnTs_72 niṣiddha-karma-janyas tv adharmaḥ //",
    "GSP29antarkxu:343": " AnTs_73 buddhy-ādayo'ṣṭāv ātma-mātraviśeṣa-guṇāḥ //",
    "GSP29antarkxu:344": " AnTs_74 buddhi-icchā-prayatnā dvividhāḥ /",
    "GSP29antarkxu:345": " nityā anityāś ca /",
    "GSP29antarkxu:346": " nityā īśvarasya /",
    "GSP29antarkxu:347": " anityā jīvasya //",
    "GSP29antarkxu:348": " AnTs_75 saṃskāras trividhaḥ /",
    "GSP29antarkxu:349": " vego bhāvanā sthiti-sthāpakaś ca+iti /",
    "GSP29antarkxu:350": " vegaḥ",
    "GSP29antarkxu:351": " pṛthivy-ādi-catuṣṭaya-mano-vṛttiḥ /",
    "GSP29antarkxu:352": " anubhava-janyā smṛti-hetur",
    "GSP29antarkxu:353": " bhāvanā+ātma-mātra-vṛttiḥ /",
    "GSP29antarkxu:354": " anyathā kṛtasya punas tad-avasthā-pādakaḥ",
    "GSP29antarkxu:355": " sthiti-sthāpakaḥ kaṭa-ādi-pṛthivī-vṛttiḥ //",
    "GSP29antarkxu:356": " AnTs_76 calana-ātmakaṃ karma /",
    "GSP29antarkxu:357": " ūrdhva-deśa-saṃyoga-hetur utkṣepaṇam /",
    "GSP29antarkxu:358": " adho-deśa-saṃyoga-hetur apakṣepaṇam /",
    "GSP29antarkxu:359": " śarīra-saṃnikṛṣṭa-saṃyoga-hetur",
    "GSP29antarkxu:360": " ākuñcanam /",
    "GSP29antarkxu:361": " viprakṛṣṭa-saṃyoga-hetuḥ prasāraṇam /",
    "GSP29antarkxu:362": " anyat sarvaṃ gamanam /",
    "GSP29antarkxu:363": " pṛthivy-ādi-catuṣṭaya-mano-mātra-vṛtti //",
    "GSP29antarkxu:364": " AnTs_77 nityam ekam aneka-anugataṃ sāmānyam /",
    "GSP29antarkxu:365": " dravya-guṇa-karma-vṛtti /",
    "GSP29antarkxu:366": " tad dvividhaṃ para-apara-bhedāt /",
    "GSP29antarkxu:367": " paraṃ sattā /",
    "GSP29antarkxu:368": " aparaṃ dravyatva-ādiḥ //",
    "GSP29antarkxu:369": " AnTs_78 nitya-dravya-vṛttayo vyāvartakā viśeṣāḥ //",
    "GSP29antarkxu:370": " AnTs_79 nitya-saṃbandhaḥ samavāyaḥ /",
    "GSP29antarkxu:371": " ayuta-siddha-vṛttiḥ /",
    "GSP29antarkxu:372": " yayor dvayor madhya ekam avinaśyad-apara-āśritam eva+avatiṣṭate tāv ayutasiddau /",
    "GSP29antarkxu:373": " yathā+avayava-avayavinau guṇa-guṇinau kriyā-kriyāvantau jāti-vyaktī",
    "GSP29antarkxu:374": " viśeṣa-nitya-dravye ca+iti //",
    "GSP29antarkxu:375": " AnTs_80 an-ādiḥ sa-antaḥ prāg-abhāvaḥ /",
    "GSP29antarkxu:376": " utpatteḥ pūrvaṃ kāryasya /",
    "GSP29antarkxu:377": " sa-ādir",
    "GSP29antarkxu:378": " an-antaḥ pradhvaṃsaḥ /",
    "GSP29antarkxu:379": " utpatty-an-antaraṃ kāryasya /",
    "GSP29antarkxu:380": " traikālika-saṃsarga-avacchinna-pratiyogitāko'tyanta-abhāvaḥ /",
    "GSP29antarkxu:381": " yathā bhūtale ghaṭo na+asti+iti /",
    "GSP29antarkxu:382": " tādātmya-saṃbandha-avacchinna-pratiyogitāko'nyonya-abhāvaḥ yathā ghaṭaḥ paṭo na bhavati+iti",
    "GSP29antarkxu:383": " AnTs_81 sarveṣāṃ pada-arthānāṃ yathā-yatham ukteṣv antarbhāvāt sapta+eva",
    "GSP29antarkxu:384": " pada-arthā iti siddham //",
    "GSP29antarkxu:385": " kāṇāda-nyāya-matayor bālā-vyutpatti-siddhaye /",
    "GSP29antarkxu:386": " annaṃbhaṭṭena viduṣā racitas tarkasaṃgrahaḥ //"
}