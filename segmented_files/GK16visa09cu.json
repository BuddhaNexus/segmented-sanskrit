{
  "GK16visa09cu:0": "navamaḥ paricchedaḥ",
  "GK16visa09cu:1": "athoddeśakramaprāptamalaṅkāranirūpaṇaṃ bahuvaktavyatvenollaṅghya rītimāha---",
  "GK16visa09cu:2": "************* COMMENTARY *************",
  "GK16visa09cu:3": "Vijñapriyā:",
  "GK16visa09cu:4": "(vi, ka)",
  "GK16visa09cu:5": "uddeśeti---guṇaālaṅkārarītaya ityuddeśe 'laṅkārasya prāguddiṣṭatvena tadullaṅghayam ityarthaḥ /",
  "GK16visa09cu:6": "tadullaṅghane hetumāha---bahuvaktavyatveneti /",
  "GK16visa09cu:7": "etena sūcīkaṭāhanyāyo darśitaḥ /",
  "GK16visa09cu:8": "Locanā:",
  "GK16visa09cu:9": "(lo, a) idānīṃ rītiṃ nirūpayitukāmaḥ saṃgati karoti---athoddeśeti /",
  "GK16visa09cu:10": "bahuvaktavyatvena ullaṅghyakamaṃ sūcīkaṭāhanyāyādityarthaḥ /",
  "GK16visa09cu:11": "********** END OF COMMENTARY **********",
  "GK16visa09cu:12": "padasaṃghaṭanā rītiraṅgasaṃsthāviroṣavat /",
  "GK16visa09cu:13": "upakartro rasādīnāṃ---",
  "GK16visa09cu:14": "************* COMMENTARY *************",
  "GK16visa09cu:15": "Vijñapriyā:",
  "GK16visa09cu:16": "(vi, kha) rasādīnāmityādipadāt samastāsaṃlakṣyakramaparigrahaḥ /",
  "GK16visa09cu:17": "********** END OF COMMENTARY **********",
  "GK16visa09cu:18": "rasādīnāmarthācchabdārthaśarīrasya kāvyasyātmabhūtānām /",
  "GK16visa09cu:19": "Locanā:",
  "GK16visa09cu:20": "(lo, ā) padānāṃ saṅgaṭanā racanā arthāt viśiṣṭā, viśeṣaśca viśeṣalakṣaṇeṣvabhivyaktaḥ /",
  "GK16visa09cu:21": "rasādīnāmiti /",
  "GK16visa09cu:22": "aṅgasaṃsthāviśeṣo yathā loke ātmānamupakaroti tathā kāvyeṣu /",
  "GK16visa09cu:23": "********** END OF COMMENTARY **********",
  "GK16visa09cu:24": "---sā punaḥ syāccaturvidhā // VisSd_9.1 //",
  "GK16visa09cu:25": "vaidarbho cātha gauḍī ca pāñcālī lāṭikā tathā /",
  "GK16visa09cu:26": "sarītiḥ /",
  "GK16visa09cu:27": "tatra---",
  "GK16visa09cu:28": "mādhuryavyañjakervarṇai racanā lalitātmikā // VisSd_9.2 //",
  "GK16visa09cu:29": "avṛttiralpavṛttirvā vaidarbho rītiriṣyate /",
  "GK16visa09cu:30": "yathā---“anaṅgamaṅgalabhuvaḥ--” ityādi /",
  "GK16visa09cu:31": "rudraṭastvāha---",
  "GK16visa09cu:32": "asamastaikasamastā yuktā daśabhirguṇaiśca vaidarbho /",
  "GK16visa09cu:33": "vargadvitīyabahulā svalpaprāṇākṣarā ca suvidheyā //",
  "GK16visa09cu:34": "atra daśaguṇāstanmatoktāḥ śleṣādayaḥ /",
  "GK16visa09cu:35": "************* COMMENTARY *************",
  "GK16visa09cu:36": "Vijñapriyā:",
  "GK16visa09cu:37": "(vi, ga) anaṅgamaṅgalabhuva ityādi vyākhyātam /",
  "GK16visa09cu:38": "ekapadasamastā na tvanekapadairderghasamastā /",
  "GK16visa09cu:39": "svalpaprāṇāni akṣarāṇi īṣat prayatnoccāryyāpi kakārahakārādīni yatra /",
  "GK16visa09cu:40": "śleṣādirdaśaguṇāḥ /",
  "GK16visa09cu:41": "Locanā:",
  "GK16visa09cu:42": "(lo, i) lalitātmikā sukumārasvarūpā /",
  "GK16visa09cu:43": "vṛttiḥ samāsaḥ /",
  "GK16visa09cu:44": "atra rudraṭācāryyamataprakaṭanam /",
  "GK16visa09cu:45": "ekasamastā ekapadasamāsayuktā, svalpaprāṇākṣarā mūrddhnā mṛdvṛttetyādivat na kaṣṭoccāraṇīyavarṇā /",
  "GK16visa09cu:46": "********** END OF COMMENTARY **********",
  "GK16visa09cu:47": "ojaḥ prakāśakairvaṇairbandha āḍambaraḥ punaḥ // VisSd_9.3 //",
  "GK16visa09cu:48": "samāsabahulā gauḍī---",
  "GK16visa09cu:49": "yathā---“cañcadbhuja--” ityādi /",
  "GK16visa09cu:50": "puruṣottamastvāha---",
  "GK16visa09cu:51": "“bahutarasamāsayuktā sumahāprāṇākṣarā ca gauḍīyā /",
  "GK16visa09cu:52": "rītiranuprāsamahimaparatantrā stokavākyā ca” //",
  "GK16visa09cu:53": "************* COMMENTARY *************",
  "GK16visa09cu:54": "Vijñapriyā:",
  "GK16visa09cu:55": "(vi, gha) āḍambara uddatavarṇaghaṭitaḥ /",
  "GK16visa09cu:56": "puruṣottastviti /",
  "GK16visa09cu:57": "anyāpekṣayā prāklikhitabandha āḍambaraḥ;",
  "GK16visa09cu:58": "etanmate uktavarṇānāmanuprasitatvaniyama iti viśeṣastadāhaanuprāsamahimaparatantreti /",
  "GK16visa09cu:59": "pāratantryaṃ vyāptiḥ /",
  "GK16visa09cu:60": "astobhavākya yatirahitavākyā /",
  "GK16visa09cu:61": "Locanā:",
  "GK16visa09cu:62": "(lo, ī) anuprāsamahimaparatantrānuprāsabahulā /",
  "GK16visa09cu:63": "********** END OF COMMENTARY **********",
  "GK16visa09cu:64": "---varṇaiḥ śeṣaiḥ punardvayoḥ /",
  "GK16visa09cu:65": "samastapañcaṣapado bandhaḥ pāñcālikā matā // VisSd_9.4 //",
  "GK16visa09cu:66": "************* COMMENTARY *************",
  "GK16visa09cu:67": "Vijñapriyā:",
  "GK16visa09cu:68": "(vi, ṅa) pāñcālikāṃ rītimāha---varṇairiti /",
  "GK16visa09cu:69": "dvayoḥ śeṣairavaśiṣṭairvarṇairityarthaḥ /",
  "GK16visa09cu:70": "samastapañcamapada iti atra padapadam akṣaram /",
  "GK16visa09cu:71": "tena samāsagatapañcamākṣara ityarthaḥ /",
  "GK16visa09cu:72": "********** END OF COMMENTARY **********",
  "GK16visa09cu:73": "dvayorvaidarbhogauḍyoḥ /",
  "GK16visa09cu:74": "yathā---",
  "GK16visa09cu:75": "“madhurayā madhubodhitamādhavīmadhusamṛddhisamedhitamedhayā /",
  "GK16visa09cu:76": "madhukarāṅganayā muhurunmadadhvanibhṛtā nibhṛtākṣaramujjage” //",
  "GK16visa09cu:77": "************* COMMENTARY *************",
  "GK16visa09cu:78": "Vijñapriyā:",
  "GK16visa09cu:79": "(vi, ca) madhurayeti /",
  "GK16visa09cu:80": "madhukarāṅganayā bhramaryyā nibhṛtākṣaraṃ guptākṣaramasphuṭākṣaraṃ yathā syāttathā muhurujjage 'gīyata /",
  "GK16visa09cu:81": "kīdṛśyā madhurayā dhvanimadhuratayaiva tasyā madhuratā,",
  "GK16visa09cu:82": "tathā madhunā vasantena bodhitāyā udvodhitāyā mādhavīlatāyā madhuno makarandasya samṛddhyā sampattyā samedhitā dīptā medhā buddhiryasyāstādṛśyā /",
  "GK16visa09cu:83": "tathonmadadvanibhṛtā uccatvamevonmadatvam /",
  "GK16visa09cu:84": "atra makāranakārau pañcamavarṇau samāsagatau /",
  "GK16visa09cu:85": "********** END OF COMMENTARY **********",
  "GK16visa09cu:86": "bhojastvāha---",
  "GK16visa09cu:87": "“samastapañcaṣapadāmojaḥ kāntisamanvitām /",
  "GK16visa09cu:88": "madhurāṃ sukumārāṃ ca pāñcālīṃ kavayo viduḥ” //",
  "GK16visa09cu:89": "************* COMMENTARY *************",
  "GK16visa09cu:90": "Vijñapriyā:",
  "GK16visa09cu:91": "(vi, cha) kaṇṭhāraṇakṛto bhojasya sampatalakṣaṇamāha---bhojastviti /",
  "GK16visa09cu:92": "ojaḥ kāntīti /",
  "GK16visa09cu:93": "sābhiprāyatvarūpeṇa ojasā aujjvalyarūpayā kāntvā ca samanvitāmityarthaḥ /",
  "GK16visa09cu:94": "madhurāṃsukumārāṃ ceti /",
  "GK16visa09cu:95": "pṛthak padatvarūpeṇa mādhuryyeṇa saukumāryeṇa ca yuktāmityarthaḥ /",
  "GK16visa09cu:96": "anaṅgamaṅgalabhuva iti vaidarbhyudāharaṇe pañcamamūrddhāna eva varṇā iti /",
  "GK16visa09cu:97": "na tatra samastapañcamavarṇaghaṭitapāñcālītvaprasaktiḥ /",
  "GK16visa09cu:98": "madhurayetyādipāñcālyudāharaṇe samāsagatā eva pañcamavarṇā iti na tatra pañcamamūrddhavarṇaghaṭitavaidarbhotvaprasaktiḥ /",
  "GK16visa09cu:99": "tatra mādhuryyavyañjakairityatra bahuvacanādatra madhukarāṅganayā ityatra tadvyañjakaikavarṇamātrasattvena tatprasaktyabhāvāt /",
  "GK16visa09cu:100": "Locanā:",
  "GK16visa09cu:101": "(lo, u) ojaḥ--kāntī tanmatoktau guṇaviśeṣau /",
  "GK16visa09cu:102": "********** END OF COMMENTARY **********",
  "GK16visa09cu:103": "lāṭī tu rītirvaidarbhopāñcālyorantare sthitā /",
  "GK16visa09cu:104": "Locanā:",
  "GK16visa09cu:105": "(lo, ū) antarā sthitetyanena nātyalpasamāsā na bahulasamāsā, evamanyatra /",
  "GK16visa09cu:106": "********** END OF COMMENTARY **********",
  "GK16visa09cu:107": "yathā---",
  "GK16visa09cu:108": "“ayamudayati mudrābhañjanaḥ padminīnāmudayagirivanālībālamandārapuṣpam /",
  "GK16visa09cu:109": "virahavidhurakokadvandvabandhuvibhindan kupitakapikapolakroḍatāmrastamāṃsi” //",
  "GK16visa09cu:110": "************* COMMENTARY *************",
  "GK16visa09cu:111": "Vijñapriyā:",
  "GK16visa09cu:112": "(vi, ja) ayamudayatīti /",
  "GK16visa09cu:113": "lāṭyudāharaṇe tadubhayamiśraṇaṃ darśayiṣyate /",
  "GK16visa09cu:114": "tadarthastu ayam arthāt sūryya udayati /",
  "GK16visa09cu:115": "kīdṛśaḥ /",
  "GK16visa09cu:116": "padminīnāṃ mudrāyāḥ saṅkocasya bhañjanaḥ tatpuṣpavikāśakaratvāt /",
  "GK16visa09cu:117": "tathā udayagiristhāyā vanaśreṇyā navīnamandārapuṣpasvarūpaḥ /",
  "GK16visa09cu:118": "virahaduḥ khitasya cakravākamithunasya bandhurmitraṃ tadvirahanāśakatvāt /",
  "GK16visa09cu:119": "tamāṃsi bhindan ca kupitasya vānarasya kapolakroḍobhayavat tāmraśca /",
  "GK16visa09cu:120": "kopaśāt kapolasya tāmratā kroḍasya tu svābhāvikī /",
  "GK16visa09cu:121": "atra bhañjaneti,",
  "GK16visa09cu:122": "mandāreti,",
  "GK16visa09cu:123": "dvandvabandhuriti,",
  "GK16visa09cu:124": "bhindanniti pañcamamūrddhavarṇānāṃ vaidarbhoghaṭakatvaṃ taccheṣavarṇānāṃbahūnāmevaṃ pāñcālīghaṭakatvamityubhayamiśraṇam /",
  "GK16visa09cu:125": "********** END OF COMMENTARY **********",
  "GK16visa09cu:126": "kaścidāha---",
  "GK16visa09cu:127": "“mṛdupadasamāsasubhagā yuktairvarṇairna cātibhūyiṣṭhā /",
  "GK16visa09cu:128": "Locanā:",
  "GK16visa09cu:129": "(lo, ṛ) yuktaiḥ saṃyuktaiḥ /",
  "GK16visa09cu:130": "********** END OF COMMENTARY **********",
  "GK16visa09cu:131": "ucitaviśeṣaṇapūritavastunyāsā bhavellāṭī” //",
  "GK16visa09cu:132": "************* COMMENTARY *************",
  "GK16visa09cu:133": "Vijñapriyā:",
  "GK16visa09cu:134": "(vi, jha) kaścidāheti---lāṭīlakṣaṇamiti śeṣaḥ /",
  "GK16visa09cu:135": "mṛdupadeti---padasamāsayorubhayatra mṛdutvānvayaḥ /",
  "GK16visa09cu:136": "yuktaiḥ saṃyuktairvarṇairnacātibhūyiṣṭhā, bhūyiṣṭhasaṃyuktavarṇarahitetyarthaḥ /",
  "GK16visa09cu:137": "uciteti---ucitam uktadoṣarahitaṃ viśeṣaṇaṃ pūritam pratipālitaṃ yena tādṛśasya vastuno nyāsayuktā lāṭī bhavedityarthaḥ /",
  "GK16visa09cu:138": "etadapyuktodāharaṇe eva sambhavati /",
  "GK16visa09cu:139": "tattadguṇavyañjakānāṃ varṇādīnāṃ tattadguṇavyañjanārthamupādeyatvenoktatvāt /",
  "GK16visa09cu:140": "********** END OF COMMENTARY **********",
  "GK16visa09cu:141": "anye tvāhuḥ---",
  "GK16visa09cu:142": "“gauḍī ḍambarabaddhā syādvaidarbho lalitakramā /",
  "GK16visa09cu:143": "pāñcālī miśrabhāvena lāṭī tu mṛdubhaiḥ padaiḥ” //",
  "GK16visa09cu:144": "kvacittu vaktrādyaucityādanyathā racanādayaḥ // VisSd_9.5 //",
  "GK16visa09cu:145": "************* COMMENTARY *************",
  "GK16visa09cu:146": "Vijñapriyā:",
  "GK16visa09cu:147": "(vi, ña)",
  "GK16visa09cu:148": "kvacittattadrasābhāvena tadguṇavyañjanārthamanupādeyatve 'pi vaktrādyaucityād anyathā racanādaya ityāha---kvacidvaktrādīti /",
  "GK16visa09cu:149": "tadguṇavyañjanābhāve 'pi tannibandha evānyathātvam /",
  "GK16visa09cu:150": "Locanā:",
  "GK16visa09cu:151": "(lo, ṝ) iha saṅghaṭanādīnāṃ guṇaparatantratve 'pi viśeṣamāha--kvaciditi /",
  "GK16visa09cu:152": "yadāha sarvatraiva rasāśritā racanādayo vaktrādyapekṣāyāṃ tu kiñcittarātamyamiti /",
  "GK16visa09cu:153": "********** END OF COMMENTARY **********",
  "GK16visa09cu:154": "vāktradītyādiśabdādvācyaprabandhau . racanādītyādiśabdādvṛttivarṇau /",
  "GK16visa09cu:155": "tatra vaktraucityādyathā---",
  "GK16visa09cu:156": "“manthāyastārṇavāmbhaḥ plutakuharacalanmandaradhvānadhaīraḥ koṇāghāteṣu garjatpralayaghanaghaṭānyonyasaṅghaṭṭacaṇḍaḥ /",
  "GK16visa09cu:157": "kṛṣṇākrodhagraḍhūtaḥ kurukulanidhanotpātanirghātavātaḥ kenāsmatsiṃhanādapratirasitasakho dundubhistāḍito 'yam” //",
  "GK16visa09cu:158": "atra vācyakrodhādya(na) bhivyañjakatve 'pi bhīmasenavaktatvenoddhatā racanādayaḥ /",
  "GK16visa09cu:159": "************* COMMENTARY *************",
  "GK16visa09cu:160": "Vijñapriyā:",
  "GK16visa09cu:161": "(vi, ṭa) manthāyastetyādi---raṇasthale dundubhiśabdaṃ śrutvā bhīmasyeyaṃ pṛcchā /",
  "GK16visa09cu:162": "kenāyaṃ dundubhistāḍitaḥ--tāḍanena janitaḥ /",
  "GK16visa09cu:163": "yathā śrute viśeṣaṇānāṃ dundubhau anvayaḥ /",
  "GK16visa09cu:164": "śabdaḥ kīdṛśaḥ /",
  "GK16visa09cu:165": "manthanena mathanena āyastasya prāptāyāsasyārṇavasyāmbhasāpulutaṃ vyāptaṃ kuharaṃ guhā yasya tādṛśasya calataḥ mandarasya dhvānavat dhīraḥ /",
  "GK16visa09cu:166": "koṇasya vādanadaṇḍasyāghāteṣu tāḍaneṣu satsu /",
  "GK16visa09cu:167": "garjjantyāḥ pralayaghanaghaṭāyā anyo 'nyasaṃghaṭṭanena caṇḍaḥ /",
  "GK16visa09cu:168": "yadvā--- ḍhakkāśatasahastrāṇi bherīśataśatāni ca /",
  "GK16visa09cu:169": "ekadā yatra vādyante koṇāghātaḥ sa ucyate //",
  "GK16visa09cu:170": "yatreti nimittasaptamyāṃ yādṛśaśabdanimittaṃ ḍhakkādaya ekadā tāḍyante sa śabdaḥ koṇaāghāta ityarthaḥ /",
  "GK16visa09cu:171": "tathā ca tādṛśaśabdeṣu madhye ayaṃ garjjat ityādi caṇḍa ityarthaḥ /",
  "GK16visa09cu:172": "kṛṣṇāyā draupadyāḥ krodhasya agradūtaḥ tatpravarttakatvāt kurukulanidhanasūcaka utpātabhūto nirghātavātaśca ityarthaḥ /",
  "GK16visa09cu:173": "atreti vācyapadamatra pratipādyaparaṃ tathā ca lakṣyasya dundubhiśabdasya ityarthaḥ /",
  "GK16visa09cu:174": "Locanā:",
  "GK16visa09cu:175": "(lo, ḷ) manthāyastetyādau kodhāvyañjakatvaṃ vākyasya ca nāṭakarūpasyābhineyaduḥ- khāvahatvāt /",
  "GK16visa09cu:176": "anucitamapi dīrghasamāsāntatve 'pi viśeṣamāha---dhīroddhato bhīmaseno vakteti nānaucityamiti bhāvaḥ /",
  "GK16visa09cu:177": "********** END OF COMMENTARY **********",
  "GK16visa09cu:178": "vācyaucityādyathodāhṛte “mūrdhavyādhūyamāna---” ityādau /",
  "GK16visa09cu:179": "Locanā:",
  "GK16visa09cu:180": "(lo, e)",
  "GK16visa09cu:181": "mūrddhavyādhūyamānetyādau ca na vaktraucityam,",
  "GK16visa09cu:182": "nāṭakāntaḥ pātitvācca uktaprakāreṇa prabandhaucityamapi /",
  "GK16visa09cu:183": "kintu kāvyasya uddhatārthaprakāśakatvāduddhataracanā /",
  "GK16visa09cu:184": "********** END OF COMMENTARY **********",
  "GK16visa09cu:185": "prabandhaucityādyathā nāṭakādau raudre 'pyabhinayapratikūlatvena na dīrghasamāsādayaḥ /",
  "GK16visa09cu:186": "evamākhyāyikāyāṃ śṛṅgāre 'pi na masṛṇavarṇādayaḥ /",
  "GK16visa09cu:187": "kathāyāṃ raudre 'pi nātyantamuddhatāḥ /",
  "GK16visa09cu:188": "************* COMMENTARY *************",
  "GK16visa09cu:189": "Vijñapriyā:",
  "GK16visa09cu:190": "(vi, ṭha) ākhyāyikākathe prabandhaviśeṣau /",
  "GK16visa09cu:191": "iti śrīmaheśvaranyāyālaṅkārabhaṭṭācāryyakṛtāyāṃ sāhityadarpaṇaṭīkāyāṃ rītinirūpaṇakhyasya navamaparicchedasya vivaraṇam /",
  "GK16visa09cu:192": "********** END OF COMMENTARY **********",
  "GK16visa09cu:193": "evamanyadapi jñeyam /",
  "GK16visa09cu:194": "Locanā:",
  "GK16visa09cu:195": "(lo, ai) ākhyāyikādilakṣaṇamuktameva /",
  "GK16visa09cu:196": "ākhyāyikāyā gadyamayaprabandhatvenagadyasya ca vikaṭabandhadīrghasamāsabahulaprācuryyautityāt tatra śṛṅgāre 'pi na prāyeṇa sukumāraracanādiḥ /",
  "GK16visa09cu:197": "yaduktaṃ dhvanikṛtā--- “rasabaddhoktamaucityaṃ bhāti sarvatra saṃśritā /",
  "GK16visa09cu:198": "racanāviṣayāpekṣaṃ tattu kiñcid vibhedavat” //",
  "GK16visa09cu:199": "iti kathāprabandhasya gadyamayatve 'pi śṛṅgārarasamayatvādraudādirasapraveśe 'pi nātyantamuddhataracanādayo vidheyāḥ /",
  "GK16visa09cu:200": "evamanyadapi /",
  "GK16visa09cu:201": "yaduktaṃ dhvanikṛtā---“sandānikādiṣu vikaṭabandhaucityād madhyamasamāsadīrgharamāse eva saṅghaṭane” /",
  "GK16visa09cu:202": "iti śrīsāhityadarpaṇalocane rītiviveko nāma navamaḥ paricchedaḥ /",
  "GK16visa09cu:203": "********** END OF COMMENTARY **********",
  "GK16visa09cu:204": "iti sāhityādarpaṇe rītivivecano nāma navamaḥ paricchedaḥ /"
}