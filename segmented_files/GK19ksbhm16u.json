{
  "GK19ksbhm16u:0": "//mausalaṃ parva//",
  "GK19ksbhm16u:1": "nārāyaṇaṃ namaskṛtya naraṃ caiva narottamam /",
  "GK19ksbhm16u:2": "devīṃ sarasvatīṃ vyāsaṃ tato jayamudīrayet // Bhmj_16.1 //",
  "GK19ksbhm16u:3": "mahīṃ mahībhṛtāṃ dhurye dharmarāje praśāsati /",
  "GK19ksbhm16u:4": "ṣaḍviṃśe 'bde mahābhāgā dvārakāṃ munayo yayuḥ // Bhmj_16.2 //",
  "GK19ksbhm16u:5": "tāndṛṣṭvā kauśikamukhānpraharṣanmadaviplutāḥ /",
  "GK19ksbhm16u:6": "sāmbaṃ kṛtvā vadhūveśaṃ papracchurvṛṣṇipuṃgavāḥ // Bhmj_16.3 //",
  "GK19ksbhm16u:7": "api putraḥ sutā vāsyā bhaviṣyati munīśvarāḥ /",
  "GK19ksbhm16u:8": "tacchrutvā te 'vadannasyāḥ kulamṛtyurbhaviṣyati // Bhmj_16.4 //",
  "GK19ksbhm16u:9": "musalaṃ brahmadaṇḍākhyamityuktvādarśanaṃ yayuḥ /",
  "GK19ksbhm16u:10": "lauhaṃ sāmbastato 'sūta musalaṃ vajrasaṃhatam // Bhmj_16.5 //",
  "GK19ksbhm16u:11": "tadāhvako bhayātpiṣṭvā tūrṇaṃ tatyāja sāgare /",
  "GK19ksbhm16u:12": "sarvajño jñātavṛttānto vṛṣṇikṣayamupasthitam // Bhmj_16.6 //",
  "GK19ksbhm16u:13": "vijñāya kaiṭabhārātistīrthayātrārasādyayau /",
  "GK19ksbhm16u:14": "caṇḍāṃśumaṇḍaloccaṇḍaṃ tataścakraṃ suradviṣaḥ // Bhmj_16.7 //",
  "GK19ksbhm16u:15": "ratho dhvajaśca sauvarṇaḥ sahasāntaradhīyata /",
  "GK19ksbhm16u:16": "atha mārjāravicchāye kabandhāvṛtamaṇḍale // Bhmj_16.8 //",
  "GK19ksbhm16u:17": "sūrye vighaṭṭitāśeṣadiktaṭe vāti mārute /",
  "GK19ksbhm16u:18": "muṇḍo viṭaṅkavadano yādavānāṃ gṛhe gṛhe // Bhmj_16.9 //",
  "GK19ksbhm16u:19": "saṃdarśya vadanaṃ prāyādvācānviṣṭo 'pyadṛśyata /",
  "GK19ksbhm16u:20": "cītkārapuruṣaḥ śabdaḥ śārikānāṃ gṛheṣvabhūt // Bhmj_16.10 //",
  "GK19ksbhm16u:21": "gṛdhragomāyunādena rājamārgau'pyapūryata /",
  "GK19ksbhm16u:22": "viparītā prasūtiśca paśūnāmabhavattadā // Bhmj_16.11 //",
  "GK19ksbhm16u:23": "kośeṣu kanakābandhe ratnāni svayamasphuṭan /",
  "GK19ksbhm16u:24": "teṣu ghoranimitteṣu jāteṣvapyaviśaṅkitāḥ // Bhmj_16.12 //",
  "GK19ksbhm16u:25": "babhūvurvṛṣṇayo bhānti na hi nāśeṣu buddhayaḥ /",
  "GK19ksbhm16u:26": "prabhāsamatha saṃprāpte kṛṣṇe yādavavṛṣṇayaḥ // Bhmj_16.13 //",
  "GK19ksbhm16u:27": "tameva deśamabhyetya cakrire vipulotsavam /",
  "GK19ksbhm16u:28": "udyāneṣu vicitreṣu vijahnustatra te sukham // Bhmj_16.14 //",
  "GK19ksbhm16u:29": "gītanṛtyarasāsaktāḥ kalitāḥ kālacakṣuṣā /",
  "GK19ksbhm16u:30": "āpānakelisaṃsakte tato vṛṣṇikule 'bhavat // Bhmj_16.15 //",
  "GK19ksbhm16u:31": "vivāde yuddhasaṃbaddhaḥ śaineyakṛtavarmaṇoḥ /",
  "GK19ksbhm16u:32": "hārdikyaṃ sātyākiḥ prāha suptahā sauptiko bhavān // Bhmj_16.16 //",
  "GK19ksbhm16u:33": "sa tamūce tvayā prāyagato bhūriśravā hataḥ /",
  "GK19ksbhm16u:34": "evaṃ vivādātsaṃgrāmasteṣāmāsītsudāruṇaḥ // Bhmj_16.17 //",
  "GK19ksbhm16u:35": "prāktasmādvajramusalātprajātairvallarīcayaiḥ /",
  "GK19ksbhm16u:36": "tṛṇamapyabhavattatra brahmadaṇḍamadotkaṭam // Bhmj_16.18 //",
  "GK19ksbhm16u:37": "kṣayāya musalībhūtaṃ sahasāndhakavṛṣṇiṣu /",
  "GK19ksbhm16u:38": "yā bhūdāpānabhūsteṣāṃ lalanānayanotsavaḥ // Bhmj_16.19 //",
  "GK19ksbhm16u:39": "saiva mṛtyorabhūddhorasaṃhārāhāramaṇḍapaḥ /",
  "GK19ksbhm16u:40": "sāmbe gade cārudoṣṇi pradyumne kṛtavarmaṇi // Bhmj_16.20 //",
  "GK19ksbhm16u:41": "aniruddhe ca śaineye nihate paśyato hareḥ /",
  "GK19ksbhm16u:42": "vṛtte vṛṣṇikṣaye ghore strīśeṣā yādave kule // Bhmj_16.21 //",
  "GK19ksbhm16u:43": "dvārakā prarurodeva cchinnahārāśrunirjharaiḥ /",
  "GK19ksbhm16u:44": "tataḥ śaurirhaladharaṃ draṣṭuṃ duḥkhākulo vrajan // Bhmj_16.22 //",
  "GK19ksbhm16u:45": "dārukaṃ tatkathayituṃ prāhiṇotsavyasācine /",
  "GK19ksbhm16u:46": "yoṣitāṃ dasturakṣāyai babhrumādāya keśavaḥ // Bhmj_16.23 //",
  "GK19ksbhm16u:47": "gatvā janakamāmantrya rauhiṇeyamupāyayau /",
  "GK19ksbhm16u:48": "athāpaśyanmukhāttasya niḥsṛtaṃ madhusūdanaḥ // Bhmj_16.24 //",
  "GK19ksbhm16u:49": "sahasraśīrṣaṃ bhujagaṃ śvetaṃ śvetācalopamam /",
  "GK19ksbhm16u:50": "vāsukipramukhairnāgairatha pratyudyataiḥ saha // Bhmj_16.25 //",
  "GK19ksbhm16u:51": "dṛṣṭvārṇavaṃ praviṣṭaṃ taṃ varuṇenārcitaṃ punaḥ /",
  "GK19ksbhm16u:52": "gatijñaḥ sarvabhūtānāṃ na śuśoca janārdanaḥ // Bhmj_16.26 //",
  "GK19ksbhm16u:53": "bhave hyabhāvasadbhāve bhāvānāṃ sthiratā kutaḥ /",
  "GK19ksbhm16u:54": "anantadhāmni milite balabhadre sakānane // Bhmj_16.27 //",
  "GK19ksbhm16u:55": "praviśya yogī suṣvāpa nibaddhaprāṇadhāraṇaḥ /",
  "GK19ksbhm16u:56": "taṃ svajyotiṣi saṃsaktaṃ lubdhako mṛgaśaṅkayā // Bhmj_16.28 //",
  "GK19ksbhm16u:57": "jaghāna niśitāgreṇa śareṇa caraṇodare /",
  "GK19ksbhm16u:58": "kṛṣṇaśāradhiyā kṛṣṇo lubdhakeneṣuṇā hataḥ // Bhmj_16.29 //",
  "GK19ksbhm16u:59": "nārāyaṇākhyamaviśatparaṃ dhāma sanātanam /",
  "GK19ksbhm16u:60": "surasiddharṣigandharvaiḥ pūjyamāne saha śriyā // Bhmj_16.30 //",
  "GK19ksbhm16u:61": "viṣṇuṃ praviṣṭe govinde babhūva kṣubhitaṃ jagat /",
  "GK19ksbhm16u:62": "atrāntare dārukena kathite yādavakṣaye // Bhmj_16.31 //",
  "GK19ksbhm16u:63": "śokaṃ trāsaṃ ca dainyaṃ ca bhejire pāṇḍavāḥ param /",
  "GK19ksbhm16u:64": "tato yudhiṣṭhiragirā dvārakāṃ śvetavāhanaḥ // Bhmj_16.32 //",
  "GK19ksbhm16u:65": "dārukenaiva sahitaḥ prayayau śokamūrcchitaḥ /",
  "GK19ksbhm16u:66": "sa prāpa yādavapurīmapadmāmiva padminīm // Bhmj_16.33 //",
  "GK19ksbhm16u:67": "vṛṣṇisiṃhairvirahitā guhāṃ haimavatīmiva /",
  "GK19ksbhm16u:68": "kaṣṭairariṣṭanicayaiḥ spṛṣṭāṃ duḥkhādvilokya tām // Bhmj_16.34 //",
  "GK19ksbhm16u:69": "draṣṭuṃ viveśa śokārtaṃ janakaṃ kaṃsavidviṣaḥ /",
  "GK19ksbhm16u:70": "tārapralāpamukharairvṛtaṃ vṛṣṇivadhūjanaiḥ // Bhmj_16.35 //",
  "GK19ksbhm16u:71": "ṣoḍaśastrīsahasrāṇi kalatraṃ kamalāpateḥ /",
  "GK19ksbhm16u:72": "saubhadrajanako dṛṣṭvā mumoha bhayavihvalam // Bhmj_16.36 //",
  "GK19ksbhm16u:73": "śayānaṃ bhuvi pārtho 'pi vasudevaṃ pralāpinam /",
  "GK19ksbhm16u:74": "vilokya śokavivaśo vajrabhinna ivāpatat // Bhmj_16.37 //",
  "GK19ksbhm16u:75": "kva rāmaḥ kva ca govindaḥ kva pradyumnaḥ kva sātyakiḥ /",
  "GK19ksbhm16u:76": "aho vidherduranteyaṃ śaktirityarjuno 'vadat // Bhmj_16.38 //",
  "GK19ksbhm16u:77": "maṇikaṅkaṇajhāṅkārinṛttagītaghanadhvaniḥ /",
  "GK19ksbhm16u:78": "aviśrāntamabhūdyatra śravaṇānandanirjharaiḥ // Bhmj_16.39 //",
  "GK19ksbhm16u:79": "tatraiva vṛṣṇinārīṇāṃ śṛṇvannārodanadhvanim /",
  "GK19ksbhm16u:80": "niśāṃ nināya bībhatsurnirapāyamayīṃ śriyam // Bhmj_16.40 //",
  "GK19ksbhm16u:81": "tataḥ prātaḥ priyasuto vasudevaḥ priyāṃ tanum /",
  "GK19ksbhm16u:82": "tatyāja devakīmukhyairanuyāto vadhūjanaiḥ // Bhmj_16.41 //",
  "GK19ksbhm16u:83": "tataḥ sāttvatamukhyānāṃ sarveṣāṃ śokavihvalaḥ /",
  "GK19ksbhm16u:84": "pārthaḥ śarīraṃ saṃskṛtya cakāra salilakriyām // Bhmj_16.42 //",
  "GK19ksbhm16u:85": "vajrābhidhānaṃ kṛṣṇasya pautraṃ kuntīsutastataḥ /",
  "GK19ksbhm16u:86": "ādāya vṛṣṇikāntāśca pratasthe dārukānugaḥ // Bhmj_16.43 //",
  "GK19ksbhm16u:87": "atha dvāravatīṃ sphārataraṅgabhujamaṇḍalaiḥ /",
  "GK19ksbhm16u:88": "śaṅkhāṭṭahāsavikaṭo jahāra makarākaraḥ // Bhmj_16.44 //",
  "GK19ksbhm16u:89": "hate vṛṣṇipure kṣipraṃ ratnalobhādivābdhinā /",
  "GK19ksbhm16u:90": "puruṣānugataṃ sarvamityūce duḥkhito janaḥ // Bhmj_16.45 //",
  "GK19ksbhm16u:91": "kalatraṃ vṛṣṇisiṃhānāmādāya śvetavāhanaḥ /",
  "GK19ksbhm16u:92": "indraprasthaṃ vrajanprāpa durgamāṃ vikaṭāṭavīm // Bhmj_16.46 //",
  "GK19ksbhm16u:93": "dasyavastatra gopālā balinaḥ paśujīvinaḥ /",
  "GK19ksbhm16u:94": "adṛṣṭapūrvāḥ svaryoṣitsubhagāḥ sāttvatāṅganāḥ // Bhmj_16.47 //",
  "GK19ksbhm16u:95": "taḍittaralaratnāṃśupuñjaritāmbarāḥ /",
  "GK19ksbhm16u:96": "vilokya lobhavaśagā hartumabhyudyayurbalāt // Bhmj_16.48 //",
  "GK19ksbhm16u:97": "gopānāpatitāndṛṣṭā tānugralaguḍāyudhān /",
  "GK19ksbhm16u:98": "gāṇḍīvadhanvā gāṇḍīpamadhijyamakarotkrudhā // Bhmj_16.49 //",
  "GK19ksbhm16u:99": "kālaśaktihatasyāpi tasyāpi kṣapitadviṣaḥ /",
  "GK19ksbhm16u:100": "na śaktirabhavatkācitkṣīṇeṣorjyāvikarṣaṇe // Bhmj_16.50 //",
  "GK19ksbhm16u:101": "cāpayaṣṭiranāyattā tasyābhūdatiduḥkhagā /",
  "GK19ksbhm16u:102": "vṛddhasyeva daridrasya taruṇī rūpiṇī vadhūḥ // Bhmj_16.51 //",
  "GK19ksbhm16u:103": "tasya kopāgnitaptasya vṛthā bhrūbhaṅgakāriṇaḥ /",
  "GK19ksbhm16u:104": "bhṛtyā ivāvinītasya babhūvuravaśāḥ śarāḥ // Bhmj_16.52 //",
  "GK19ksbhm16u:105": "te divyāstragaṇāḥ kvāpi jagmurutsṛjya taṃ mṛdhe /",
  "GK19ksbhm16u:106": "guṇasaṅgāmanādṛtya dhūrtā mitramivādhanam // Bhmj_16.53 //",
  "GK19ksbhm16u:107": "sa niḥśaśvāsa vipulairmārgaṇaiḥ parivarjitaḥ /",
  "GK19ksbhm16u:108": "kṣīṇakośa iva tyāgī vailakṣyavinatānanaḥ // Bhmj_16.54 //",
  "GK19ksbhm16u:109": "tato niṣpratibhe tatra pārthe sarvāyudhakṣayāt /",
  "GK19ksbhm16u:110": "vṛṣṇipaurāṅganā jahrurnirvivekā vanecarāḥ // Bhmj_16.55 //",
  "GK19ksbhm16u:111": "na tu pradhānanārīṣu teṣāmāsītpragalbhatā /",
  "GK19ksbhm16u:112": "anarhasya mahārheṣu nīcasyojasvitā katham // Bhmj_16.56 //",
  "GK19ksbhm16u:113": "taiḥ prasahya hṛte straiṇe bhagnamāno dhanaṃjayaḥ /",
  "GK19ksbhm16u:114": "pratasthe nijadordaṇḍe sāsūyāḥ pātayandṛśaḥ // Bhmj_16.57 //",
  "GK19ksbhm16u:115": "aho balavatī devī saṃsāre 'sminnanityatā /",
  "GK19ksbhm16u:116": "aho niḥsāraparyantā bhāvānāṃ prabhaviṣṇutā // Bhmj_16.58 //",
  "GK19ksbhm16u:117": "aho sarvakaṣaḥ kālaścitrapākamaho jagat /",
  "GK19ksbhm16u:118": "vijite vijayo gopairduḥkhādityavadajjanaḥ // Bhmj_16.59 //",
  "GK19ksbhm16u:119": "dhiganityavilāsasya vibhramabhrāntikāriṇaḥ /",
  "GK19ksbhm16u:120": "dhāturadbhutanirmāṇavinodaviśarārutām // Bhmj_16.60 //",
  "GK19ksbhm16u:121": "ākhaṇḍalaprabhṛtayaḥ khāṇḍave yena khaṇḍitāḥ /",
  "GK19ksbhm16u:122": "kairāte tripurārātiḥ samare yena toṣitaḥ // Bhmj_16.61 //",
  "GK19ksbhm16u:123": "jitvā tamapi gopālā varākāḥ suhṛdaṃ hareḥ /",
  "GK19ksbhm16u:124": "jahrurviṣṇukalatrāṇi dhikkālasya durantatām // Bhmj_16.62 //",
  "GK19ksbhm16u:125": "revatīṃ rukmiṇīṃ satyabhāmāṃ jāmbavatīṃ tathā /",
  "GK19ksbhm16u:126": "vajraṃ pautraṃ ca kaṃsāreranyāśca yaduyoṣitaḥ // Bhmj_16.63 //",
  "GK19ksbhm16u:127": "ādāya phalguṇaḥ śocannindraprasthaṃ samāviśat /",
  "GK19ksbhm16u:128": "sabāṣpaṃ paurasaṃghātairabhitaḥ parivāritaḥ // Bhmj_16.64 //",
  "GK19ksbhm16u:129": "rājye nidhāya tatraiva vajraṃ yāte dhanaṃjaye /",
  "GK19ksbhm16u:130": "satyabhāmānugā devyaḥ pāvakaṃ viviśuḥ śucā // Bhmj_16.65 //",
  "GK19ksbhm16u:131": "vrajanvṛṣṇiviyogārtaḥ kirīṭi hastināpuram /",
  "GK19ksbhm16u:132": "parāvarajñaṃ bhāvānāṃ vane vyāsaṃ vyalokayat // Bhmj_16.66 //",
  "GK19ksbhm16u:133": "duḥkhānnyavedayattasmai ghoraṃ vṛṣṇikulakṣayam /",
  "GK19ksbhm16u:134": "ātmanaścāttacāpasya gopaiḥ pathi parābhavam // Bhmj_16.67 //",
  "GK19ksbhm16u:135": "tamabravījjñānanidhirmunirāśvāsya mūrcchitam /",
  "GK19ksbhm16u:136": "putra kālavilāsānāṃ sarvametadvijṛmbhitam // Bhmj_16.68 //",
  "GK19ksbhm16u:137": "matirvibhūtayo bhogāḥ prabhāvāḥ śaktayo guṇāḥ /",
  "GK19ksbhm16u:138": "sarvathā kālakalayā nīyate smṛtiśeṣatām // Bhmj_16.69 //",
  "GK19ksbhm16u:139": "dhruvaṃ sarvaparityāge na kuryurmunayo matim /",
  "GK19ksbhm16u:140": "avasānaikavirasā yadi na syādbhavasthitiḥ // Bhmj_16.70 //",
  "GK19ksbhm16u:141": "ityukto muninā pārthaḥ prayayau hastināpuram /",
  "GK19ksbhm16u:142": "prabhāvaṃ vṛṣṇisiṃhasya kalayanbhṛśavihvalaḥ // Bhmj_16.71 //",
  "GK19ksbhm16u:143": "iti śrīkṣemendraviracitāyāṃ bhāratamañjaryāṃ mausalaṃ parva"
}