{
  "SA14abhisamu:0": "kṛtirmaitreyanāthasya //",
  "SA14abhisamu:1": "Oṃ namo maitreyanāthāya //",
  "SA14abhisamu:2": "yā sarvajñatayā nayatyupaśamaṃ śāntaiṣiṇaḥ śravakān",
  "SA14abhisamu:3": "yā mārgajñatayā jagaddhitakritāṃ lokārthasaṃpādikā /",
  "SA14abhisamu:4": "savārkāramidaṃ vadanti munayo viśvaṃ yayā saṃgatā",
  "SA14abhisamu:5": "stasyai śrāvakabodhisattvagaṇino buddhasya manne namaḥ //",
  "SA14abhisamu:6": "sarvākārajñatāmārgaḥ śāsitrā yo 'tra deśita /",
  "SA14abhisamu:7": "dhīmantī vīkṣīṣīraṃstamanālīḍhaṃ parairiti // Abhs_1.1 //",
  "SA14abhisamu:8": "smṛtau cādhāya sūtrārthaṃ dharmacaryā daśātmikā /",
  "SA14abhisamu:9": "sūkhena pratipatsīrannityārambhaprayojanam // Abhs_1.2 //",
  "SA14abhisamu:10": "prajñāpāramitāṣṭābhīḥ padārthaiḥ samudīritā /",
  "SA14abhisamu:11": "sarvākārajñatā mārgajñatā sarvajñatā tataḥ // Abhs_1.3 //",
  "SA14abhisamu:12": "sarvākārābhisaṃbodho murdhaprārpto 'nupūrvikaḥ /",
  "SA14abhisamu:13": "ekakṣaṇabhisaṃbodho dharmakāyaśca te 'ṣṭadhā // Abhs_1.4 //",
  "SA14abhisamu:14": "cittotpādo 'vavādaśca nirvedhāṅgaṃ caturvidhaṃ /",
  "SA14abhisamu:15": "ādhāraḥ pratipatteśca dharmadhātusvabhāvakaḥ // Abhs_1.5 //",
  "SA14abhisamu:16": "ālambanaṃ samuddeśaḥ saṃnāhaprasthitikriye /",
  "SA14abhisamu:17": "saṃbhārāśca saniryāṇāḥ sarvākārajñatā muneḥ // Abhs_1.6 //",
  "SA14abhisamu:18": "dhyāmīkaraṇatādīni śiṣyakhaṅgapathau ca yau /",
  "SA14abhisamu:19": "mahānuśaṃso dṛṅmārga aihikāmutrikairguṇaiḥ // Abhs_1.7 //",
  "SA14abhisamu:20": "kāritramadhimuktiśca stutastobhitaśaṃsitāḥ /",
  "SA14abhisamu:21": "pariṇāme 'numode ca manaskārāvanuttamau // Abhs_1.8 //",
  "SA14abhisamu:22": "nirhāraḥ śuddhiratyantāmityayaṃ bhāvanāpathaḥ /",
  "SA14abhisamu:23": "vijñānāṃ bodhisattvānāmiti mārgajñatoditā // Abhs_1.9 //",
  "SA14abhisamu:24": "prajñayā na bhave sthānaṃ kṛpayā na śame sthitiḥ /",
  "SA14abhisamu:25": "anupāyena dūratvamupāyenāvidūratā // Abhs_1.10 //",
  "SA14abhisamu:26": "vipakṣapratipakṣau ca prayogaḥ samatāsya ca /",
  "SA14abhisamu:27": "dṛṅmārgaḥ śrāvakādīnāmiti sarvajñateṣyate // Abhs_1.11 //",
  "SA14abhisamu:28": "ākārāḥ saprayogāśca guṇā doṣāḥ salakṣaṇāḥ /",
  "SA14abhisamu:29": "mokṣanirvedhabhāgīye śaikṣo 'vaivartiko gaṇaḥ // Abhs_1.12 //",
  "SA14abhisamu:30": "samatābhavaśāntyośca kṣetraśuddhiranuttarā /",
  "SA14abhisamu:31": "sarvākārābhisaṃbodha eṣa sopāyakauśalaḥ // Abhs_1.13 //",
  "SA14abhisamu:32": "liṅgaṃ tasya vivṛddhiśca nirūḍhiścittasaṃsthitiḥ /",
  "SA14abhisamu:33": "caturdhā ca vikalpasya pratipakṣaścaturvidhaḥ // Abhs_1.14 //",
  "SA14abhisamu:34": "pratyekaṃ darsanākhye ca bhāvanākhye ca vartmani /",
  "SA14abhisamu:35": "ānantaryasamādhiśca saha vipratipattibhiḥ // Abhs_1.15 //",
  "SA14abhisamu:36": "mūrdhābhisamayaḥ tredhā daśadhā cānupūrvikaḥ /",
  "SA14abhisamu:37": "ekakṣaṇābhisaṃbodho lakṣaṇena caturvidhiḥ // Abhs_1.16 //",
  "SA14abhisamu:38": "svābhāvikaḥ sasāṃbhogo nairmāṇiko 'parastathā /",
  "SA14abhisamu:39": "dharmakāyaḥ sakāritraścaturdhā samudīritaḥ // Abhs_1.17 //",
  "SA14abhisamu:40": "cittotpādaḥ parārthāya sammyaksaṃbodhikāmatā /",
  "SA14abhisamu:41": "samāsavyāsataḥ sā ca yathāsutraṃ sa cocyate // Abhs_1.18 //",
  "SA14abhisamu:42": "bhūhemacandrajvalanairnidhiratnākarārṇavaiḥ /",
  "SA14abhisamu:43": "vajrācalauṣadhīmitraiścintāmaṇyarkagītibhiḥ // Abhs_1.19 //",
  "SA14abhisamu:44": "nṛpagañjamahāmārgayānaprasravaṇodakaiḥ /",
  "SA14abhisamu:45": "ānandoktinadīmeghairdvāviṃśatividhaḥ sa ca // Abhs_1.20 //",
  "SA14abhisamu:46": "pratipattau ca satyeṣu buddharatnādiṣu triṣu /",
  "SA14abhisamu:47": "asaktāvapariśrāntau pratipatsaṃparigrahe // Abhs_1.21 //",
  "SA14abhisamu:48": "cakṣuḥṣu pañcasu jñeyaḥ ṣaṭsvabhijñāguṇeṣu ca /",
  "SA14abhisamu:49": "dṛṅmārge bhāvanākhye cetyavavādo daśātmakaḥ // Abhs_1.22 //",
  "SA14abhisamu:50": "mṛdutikṣṇendriyau śraddhādṛṣṭiprāptau kulaṃkulau /",
  "SA14abhisamu:51": "ekavīcyantarotpadya kārākārākaniṣṭhagāḥ // Abhs_1.23 //",
  "SA14abhisamu:52": "plutāstrayo bhavasyāgraparamo rūparāgaha /",
  "SA14abhisamu:53": "dṛṣṭadharmaśamaḥ kāyasākṣī khaṅgaśca viṃśatiḥ // Abhs_1.24 //",
  "SA14abhisamu:54": "ālambanata ākārāddhetutvātsaṃparigrahāt /",
  "SA14abhisamu:55": "caturvikalpasaṃyogaṃ yathāsvaṃ bhajatāṃ satām // Abhs_1.25 //",
  "SA14abhisamu:56": "śrāvakebhyaḥ sakhaḍgebhyo bodhisattvasya tāyinaḥ /",
  "SA14abhisamu:57": "mṛdumadhyādhimātrāṇāmūṣmādīnāṃ viśiṣṭatā // Abhs_1.26 //",
  "SA14abhisamu:58": "ālambanamanityādi satyādhāraṃ tadākṛtiḥ /",
  "SA14abhisamu:59": "niṣedho 'miniveśāderheturyānatrayāptaye // Abhs_1.27 //",
  "SA14abhisamu:60": "rūpādyāyavyayau viṣṭhāsthitī prajñaptyavācyate /",
  "SA14abhisamu:61": "rūpādāvasthitisteṣāṃ tadbhāvenāsvabhāvatā // Abhs_1.28 //",
  "SA14abhisamu:62": "tayormithaḥsvabhāvatvaṃ tadanityādyasaṃsthitiḥ /",
  "SA14abhisamu:63": "tāsāṃ tadbhāvaśūnyatvaṃ mithaḥ svabhāvyametayoḥ // Abhs_1.29 //",
  "SA14abhisamu:64": "anudgraho yo dharmāṇāṃ tannimittāsamīkṣaṇam /",
  "SA14abhisamu:65": "parīkṣaṇaṃ ca prajñāyāḥ sarvasyānupalambhataḥ // Abhs_1.30 //",
  "SA14abhisamu:66": "rūpāderasvabhavāvatvaṃ tadabhāvasvabhāvatā /",
  "SA14abhisamu:67": "tadajātiraniryāṇaṃ śuddhistadanimittatā // Abhs_1.31 //",
  "SA14abhisamu:68": "tannimittānadhiṣṭhānānadhimuktirasaṃjñatā /",
  "SA14abhisamu:69": "samādhistasya kāritraṃ vyākṛtirmananākṣayaḥ // Abhs_1.32 //",
  "SA14abhisamu:70": "mithastrikasya svābhāvyaṃ samādheravikalpanā /",
  "SA14abhisamu:71": "iti nirvedhabhāgīyaṃ mṛdumadhyādhimātrataḥ // Abhs_1.33 //",
  "SA14abhisamu:72": "dvaividhyaṃ grāhyakalpasya vastutatpratipakṣataḥ /",
  "SA14abhisamu:73": "moharāśyādibhedena pratyekaṃ navadhā tu saḥ // Abhs_1.34 //",
  "SA14abhisamu:74": "dravyaprajñaptyadhiṣṭhāno dvividho grāhako mataḥ /",
  "SA14abhisamu:75": "svatantrātmādirūpeṇa skandhādyāśrayatastathā // Abhs_1.35 //",
  "SA14abhisamu:76": "cittānavalīnatvādi naīḥsvābhāvyādideśakaḥ /",
  "SA14abhisamu:77": "tadvipakṣaparityāgaḥ sarvathā saṃparigrahaḥ // Abhs_1.36 //",
  "SA14abhisamu:78": "ṣoḍhādhigamadharmasya pratipakṣaprahāṇayoḥ /",
  "SA14abhisamu:79": "tayoḥ paryupayogasya prajñāyāḥ kṛpayā saha // Abhs_1.37 //",
  "SA14abhisamu:80": "śiṣyāsādhāraṇatvasya parārthānukramasya ca /",
  "SA14abhisamu:81": "jñānasyāyatnavṛtteśca pratiṣṭhā gotramucyate // Abhs_1.38 //",
  "SA14abhisamu:82": "dharmadhātorasaṃbhedādgotrabhedo na yujyate /",
  "SA14abhisamu:83": "ādheyadharmabhedāttu tadbhedaḥ parigīyate // Abhs_1.39 //",
  "SA14abhisamu:84": "ālambanaṃ sarvadharmāste punaḥ kuśalādayaḥ /",
  "SA14abhisamu:85": "laukikādhigamākhyāścaṃ ye ca lokettarā matāḥ // Abhs_1.40 //",
  "SA14abhisamu:86": "sāsravānāsravā dharmāḥ saṃskṛtāsaṃskṛtāśca ye /",
  "SA14abhisamu:87": "śiṣyasādhāraṇā dharmā ye cāsādhāranā muneḥ // Abhs_1.41 //",
  "SA14abhisamu:88": "sarvasattvāgratā cittaprahāṇādhigamatraye /",
  "SA14abhisamu:89": "tribhirmahattvairūddeśo vijñeyo 'ya svayaṃbhuvām // Abhs_1.42 //",
  "SA14abhisamu:90": "dānādau ṣaḍvidha teṣāṃ pratyekaṃ saṃgraheṇa yā /",
  "SA14abhisamu:91": "saṃnāhapratipattiḥ sā ṣaḍbhiḥ ṣaṭkairyathodithā // Abhs_1.43 //",
  "SA14abhisamu:92": "dyānārūpyeṣu dānādau mārge maitryādikeṣu ca /",
  "SA14abhisamu:93": "gatopalambhayoge ca trimaṇḍalaviśuddhiṣu // Abhs_1.44 //",
  "SA14abhisamu:94": "uddeśe ṣaṭsvabhijñāsu sarvākārajñatānaye /",
  "SA14abhisamu:95": "prasthānapratipajjñeyā mahāyānādhirohiṇī // Abhs_1.45 //",
  "SA14abhisamu:96": "dayā dānādikaṃ ṣaṭkaṃ śamathaḥ savidarśanaḥ /",
  "SA14abhisamu:97": "yuganaddhaśca yo mārga upāye yacca kauśalam // Abhs_1.46 //",
  "SA14abhisamu:98": "jñānaṃ puṇyaṃ ca mārgaśca dhāraṇī bhūmayo daśa /",
  "SA14abhisamu:99": "pratipakṣaśca vijñeyaḥ saṃbhārapratipatkramaḥ // Abhs_1.47 //",
  "SA14abhisamu:100": "labhyate prathamā bhumirdaśadhā parikarmaṇā /",
  "SA14abhisamu:101": "āśayo hitavastutvaṃ sattveṣu samacittatā // Abhs_1.48 //",
  "SA14abhisamu:102": "tyāgaḥ sevā ca mitrāṇāṃ saddharmālambanaiṣaṇā /",
  "SA14abhisamu:103": "sadā naiṣkramyacittatvaṃ buddhakāyagatā spṛhā // Abhs_1.49 //",
  "SA14abhisamu:104": "dharmasya deśanā satyaṃ daśamaṃ vākyamiṣyate /",
  "SA14abhisamu:105": "jñeyam ca pārikarmaiṣāṃ svabhāvānupalambhataḥ // Abhs_1.50 //",
  "SA14abhisamu:106": "śīlaṃ kṛtajñatā kṣāntiḥ pramodyaṃ mahatī kṛpā /",
  "SA14abhisamu:107": "gauravaṃ guruśuśrūṣā vīryaṃ dānādike 'ṣṭamam // Abhs_1.51 //",
  "SA14abhisamu:108": "atṛptatā śrute dānaṃ dharmasya ca nirāmiṣam /",
  "SA14abhisamu:109": "buddhakṣetrasya saṃśuddhiḥ saṃsārāparikheditā // Abhs_1.52 //",
  "SA14abhisamu:110": "hrīrapatrāpyamityetat pañcadhā mananātmakam /",
  "SA14abhisamu:111": "vanāśālpecChatā tuṣṭirdhūtasaṃlekhasevanam // Abhs_1.53 //",
  "SA14abhisamu:112": "śikṣāyā aparityāgaḥ kāmānāṃ vijugupsanam /",
  "SA14abhisamu:113": "nirvitsarvāstisaṃtyāgo 'navalinānapekṣate // Abhs_1.54 //",
  "SA14abhisamu:114": "saṃstavaṃ kulamātsaryaṃ sthānaṃ saṃgaṇikāvaham /",
  "SA14abhisamu:115": "ātmotkarṣaparāvajñe karmamārgān daśāśubhān // Abhs_1.55 //",
  "SA14abhisamu:116": "mānaṃ stambhaṃ viparyāsaṃ vimatiṃ kleśamarṣaṇam /",
  "SA14abhisamu:117": "vivarjayan samāpneti daśaitān pañcamīṃ bhuvam // Abhs_1.56 //",
  "SA14abhisamu:118": "dānaśīlakṣamāvīryadhyānaprajñāprapūraṇāt /",
  "SA14abhisamu:119": "śiṣyakhaḍgaspṛhātrāsacetasāṃ parivarjakaḥ // Abhs_1.57 //",
  "SA14abhisamu:120": "yācito 'navalinaśca sarvatyāge 'pyadurmanāḥ /",
  "SA14abhisamu:121": "kṛśo 'pi nārthināṃ kṣeptā ṣaṣṭhīṃ bhūmiṃ samaśrute // Abhs_1.58 //",
  "SA14abhisamu:122": "ātmasattvagraho jīvapudgalocChedaśāśvataḥ /",
  "SA14abhisamu:123": "nimittahetvoḥ skandheṣu dhātuṣvāyataneṣu ca // Abhs_1.59 //",
  "SA14abhisamu:124": "traidhātuke pratiṣṭhānaṃ saktirālīnacittatā /",
  "SA14abhisamu:125": "ratnatritayaśīleṣu taddṛṣṭyabhiniveśitā // Abhs_1.60 //",
  "SA14abhisamu:126": "śūnyatāyāṃ vivadaśca tadvirodhaśca viṃśatiḥ /",
  "SA14abhisamu:127": "kalaṅkā yasya vicChinnāḥ saptamīmetyasau bhuvam // Abhs_1.61 //",
  "SA14abhisamu:128": "trivimokṣamukhajñānaṃ trimaṇḍalaviśuddhatā /",
  "SA14abhisamu:129": "karūṇāmananā dharmasamataikanayajñatā // Abhs_1.62 //",
  "SA14abhisamu:130": "anutpādakṣamājñānaṃ dharmāṇāmekadheraṇā /",
  "SA14abhisamu:131": "kalpanāyāḥ samuddhātaḥ saṃjñādṛkkleśavarjanam // Abhs_1.63 //",
  "SA14abhisamu:132": "śamathasya ca nidhyaptiḥ kauśalaṃ ca vidarśane /",
  "SA14abhisamu:133": "cittasya dāntatā jñānaṃ sarvatrāpratighāti ca // Abhs_1.64 //",
  "SA14abhisamu:134": "sakterabhūmiryatrecChaṃ kṣetrāntaragatiḥ samam /",
  "SA14abhisamu:135": "sarvatra svātmabhāvasya darśanaṃ ceti viṃśatiḥ // Abhs_1.65 //",
  "SA14abhisamu:136": "sarvasattvamanojñānamabhijñākrīḍanaṃ śubhā /",
  "SA14abhisamu:137": "buddhakṣetrasya niṣpattirbuddhasevāparīkṣaṇe // Abhs_1.66 //",
  "SA14abhisamu:138": "akṣajñānaṃ jinakṣetraśuddhirmāyopamā sthitiḥ /",
  "SA14abhisamu:139": "saṃcintya ca bhavādānamidaṃ karmāṣṭadhoditam // Abhs_1.67 //",
  "SA14abhisamu:140": "praṇidhānānyanantāni devādīnāṃ rūtajñatā /",
  "SA14abhisamu:141": "nadīva pratibhānānāṃ garbhāvakrāntirūttamā // Abhs_1.68 //",
  "SA14abhisamu:142": "kulajātyośca gotrasya parivārasya janmanaḥ /",
  "SA14abhisamu:143": "naiṣkramyabodhivṛkṣāṇāṃ guṇapūreśca saṃpadaḥ // Abhs_1.69 //",
  "SA14abhisamu:144": "nava bhūmīratikramya buddhabhūmau pratiṣṭhate /",
  "SA14abhisamu:145": "yena jñānena sā jñeyā daśamī bodhisattvabhūḥ // Abhs_1.70 //",
  "SA14abhisamu:146": "pratipakṣo 'ṣṭadhā jñeyo darṣanābhyāsamārgayoḥ /",
  "SA14abhisamu:147": "grāhyagrāhakavikalpānāmaṣṭānāmupaśāntaye // Abhs_1.71 //",
  "SA14abhisamu:148": "uddeśe samatāyāṃ ca sattvārthe yatnavarjane /",
  "SA14abhisamu:149": "atyantāya ca niryāṇaṃ niryāṇaṃ prāptilakṣaṇam // Abhs_1.72 //",
  "SA14abhisamu:150": "sarvākārajñatayāṃ ca niryāṇaṃ mārgagocaram /",
  "SA14abhisamu:151": "niryāṇapratipajjñeyā seyamaṣṭavidhātmikā // Abhs_1.73 //",
  "SA14abhisamu:152": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:153": "sarvākārajñatādhikāraḥ prathamaḥ //",
  "SA14abhisamu:154": "dhyāmīkaraṇatā bhābhirdevānāṃ yogyatāṃ prati /",
  "SA14abhisamu:155": "viṣayo niyato vyāptiḥ svabhāvastasya karma ca // Abhs_2.1 //",
  "SA14abhisamu:156": "caturṇamāryasatyānāmākārānupalambhataḥ /",
  "SA14abhisamu:157": "śrāvakāṇāmayaṃ mārgo jñeyo mārgajñatānaye // Abhs_2.2 //",
  "SA14abhisamu:158": "rūpādiskandhaśūnyatvācChūnyatānāmabhedataḥ /",
  "SA14abhisamu:159": "uṣmāṇaḥ anupalambhena teṣāṃ murdhagataṃ matam // Abhs_2.3 //",
  "SA14abhisamu:160": "kṣāntayasteṣu nityādiyogasthānaniṣedhataḥ /",
  "SA14abhisamu:161": "daśa bhūmīḥ samārabhya vistarāsthānadeśanat // Abhs_2.4 //",
  "SA14abhisamu:162": "agradharmagataṃ proktamāryaśrāvakavartmani /",
  "SA14abhisamu:163": "tatkasya hetorbuddhena buddhvā dharmāsamīkṣaṇāt // Abhs_2.5 //",
  "SA14abhisamu:164": "paropadeśavaiyarthyaṃ svayaṃbodhāt svayaṃbhuvām /",
  "SA14abhisamu:165": "gambhīratā ca jñānasya khaḍgānāmabhidhīyate // Abhs_2.6 //",
  "SA14abhisamu:166": "śuśrūṣā yasya yasyārthe yatra yatra yathā yathā /",
  "SA14abhisamu:167": "sa so 'rthaḥ khyātyaśabdo 'pi tasya tasya tathā tathā // Abhs_2.7 //",
  "SA14abhisamu:168": "grahyārthakalpanāhānādgrāhakasyāprahāṇataḥ /",
  "SA14abhisamu:169": "ādhārataśca vijñeyaḥ khaḍgamārgasya saṃgrahaḥ // Abhs_2.8 //",
  "SA14abhisamu:170": "prajñapteravirodhena dharmatāsūcanākṛtiḥ /",
  "SA14abhisamu:171": "usṣmagam mūrdhagaṃ rūpādyahānādiprabhāvitam // Abhs_2.9 //",
  "SA14abhisamu:172": "adyātmaśūnyatādyābhī rūpāderaparigrahāt /",
  "SA14abhisamu:173": "kṣāntiḥ rūpādyanutpādādyākārairagradharmatā // Abhs_2.10 //",
  "SA14abhisamu:174": "kṣāntijñānakṣaṇaiḥ satyasatyaṃ prati caturvidhaiḥ /",
  "SA14abhisamu:175": "mārgajñatāyāṃ dṛṅmārgaḥ sānuśaṃso 'yamucyate // Abhs_2.11 //",
  "SA14abhisamu:176": "ādhārādheyatābhāvāttathatābuddhayormithaḥ /",
  "SA14abhisamu:177": "paryāyeṇānanujñānaṃ mahattā sāpramāṇatā // Abhs_2.12 //",
  "SA14abhisamu:178": "parimāṇāntatābhāvo rūpāderavadhāraṇam /",
  "SA14abhisamu:179": "tasyāṃ sthitasya buddhatve 'nugrahātyāgatādayaḥ // Abhs_2.13 //",
  "SA14abhisamu:180": "maītryādi śūnyatā prāptirbuddhatvasya parigrahaḥ /",
  "SA14abhisamu:181": "sarvasya vyavadānasya sarvādhivyādhiśātanam // Abhs_2.14 //",
  "SA14abhisamu:182": "nirvāṇagrāhaśāntatvaṃ buddhebho rakṣaṇādikam /",
  "SA14abhisamu:183": "aprāṇivadhamārabhya sarvākārajñatānaye // Abhs_2.15 //",
  "SA14abhisamu:184": "svayaṃ sthitasya sattvānāṃ sthāpanaṃ pariṇāmanam /",
  "SA14abhisamu:185": "danādīnāṃ ca saṃbodhāviti mārgajñatākṣaṇāḥ // Abhs_2.16 //",
  "SA14abhisamu:186": "sarvato damanaṃ nāma sarvataḥ kleśanirjayaḥ /",
  "SA14abhisamu:187": "upamakrāviṣahyatvaṃ bodhirādhārapūjyatā // Abhs_2.17 //",
  "SA14abhisamu:188": "adhimuktistridhā jñeyā svārthā ca svaparārthakā /",
  "SA14abhisamu:189": "parārthikaivetyeṣa ca pratyekaṃ trividheṣyate // Abhs_2.18 //",
  "SA14abhisamu:190": "mṛdvī madhyādhimātrā ca mṛdumṛdvādibhedataḥ /",
  "SA14abhisamu:191": "sā punastrividhetyevaṃ saptaviṃśatidhā matā // Abhs_2.19 //",
  "SA14abhisamu:192": "stutiḥ stobhaḥ praśaṃsā ca prajñāparamitāṃ prati /",
  "SA14abhisamu:193": "adhimokṣasya mātrāṇāṃ navakaistribhiriṣyate // Abhs_2.20 //",
  "SA14abhisamu:194": "viśeṣaḥ pariṇāmastu tasya kāritramuttamam /",
  "SA14abhisamu:195": "nopalambhākṛtiścāsāvaviparyāsalakṣaṇaḥ // Abhs_2.21 //",
  "SA14abhisamu:196": "vivikto buddhapuṇyaughasvabhāvasmṛtigocaraḥ /",
  "SA14abhisamu:197": "sopāyaścānimittaśca buddhairabhyanumoditaḥ // Abhs_2.22 //",
  "SA14abhisamu:198": "traidhātukāprapannaśca pariṇāmo 'parastridhā /",
  "SA14abhisamu:199": "mṛdurmadhyo 'dhimātraśca mahāpuṇyodayātmakaḥ // Abhs_2.23 //",
  "SA14abhisamu:200": "upāyānupalambābhyāṃ śubhamūlānumodanā /",
  "SA14abhisamu:201": "anumode manaskārabhāvaneha vidhīyate // Abhs_2.24 //",
  "SA14abhisamu:202": "svabhāvaḥ śreṣṭhatā tasya sarvasyānabhisaṃskṛtiḥ /",
  "SA14abhisamu:203": "nopalambhena dharmāṇāmarpaṇā ca mahārthatā // Abhs_2.25 //",
  "SA14abhisamu:204": "buddhasevā ca dānādirūpāye yacca kauśalam /",
  "SA14abhisamu:205": "hetavo 'trādhimokṣasya dharmavyasanahetavaḥ // Abhs_2.26 //",
  "SA14abhisamu:206": "mārādhiṣṭhānagambhīradharmatānadhimuktate /",
  "SA14abhisamu:207": "skandhādyabhiniveśaśca pāpamitraparigrahaḥ // Abhs_2.27 //",
  "SA14abhisamu:208": "phalaśuddhiśca rūpādiśuddhireva tayordvayoḥ /",
  "SA14abhisamu:209": "abhinnācChinnatā yasmāditi śuddhirudīritā // Abhs_2.28 //",
  "SA14abhisamu:210": "kleśajñeyatrimārgasya śiṣyakhaḍgajinaurasām /",
  "SA14abhisamu:211": "hānādviśuddhirātyantikī tu buddhasya sarvathā // Abhs_2.29 //",
  "SA14abhisamu:212": "mṛdumṛdvādiko mārgaḥ śuddhirnavasu bhūmiṣu /",
  "SA14abhisamu:213": "adhimātrādhimātrādermalasya pratipakṣataḥ // Abhs_2.30 //",
  "SA14abhisamu:214": "tridhātupratipakṣatvaṃ samatā manameyayoḥ /",
  "SA14abhisamu:215": "mārgasya ceṣyate tasya parihārataḥ // Abhs_2.31 //",
  "SA14abhisamu:216": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:217": "mārgajñatādhikāro dvitīyaḥ //",
  "SA14abhisamu:218": "nāpare na pare tīre nāntarāle tathoḥ sthitā /",
  "SA14abhisamu:219": "adhvanāṃ samatājñānāt prajñāpāramitāmatā // Abhs_3.1 //",
  "SA14abhisamu:220": "anupāyena dūraṃ sā sanimittopalambhataḥ /",
  "SA14abhisamu:221": "upāyakauśalenāsyāḥ samyagāsannatoditā // Abhs_3.2 //",
  "SA14abhisamu:222": "rūpādiskandhaśūnyatve dharmeṣu tryadhvageṣu ca /",
  "SA14abhisamu:223": "dānādau bodhipakṣeṣu caryāsaṃjñā vipakṣatā // Abhs_3.3 //",
  "SA14abhisamu:224": "dānādiṣvanahaṃkāraḥ pareṣāṃ tanniyojanam /",
  "SA14abhisamu:225": "saṅgakoṭīniṣedho 'yaṃ sukṣmaḥ saṅgo jinādiṣu // Abhs_3.4 //",
  "SA14abhisamu:226": "tadgāmbīryaṃ prakṛtyaiva vivekāddharmapaddhateḥ *iti* /",
  "SA14abhisamu:227": "ekaprakṛtikaṃ jñānaṃ dharmāṇāṃ saṅgavarjanam // Abhs_3.5 //",
  "SA14abhisamu:228": "dṛṣṭādipratiṣedhena tasyā durbodhatoditā *iti* /",
  "SA14abhisamu:229": "rūpādibhiravijñānāttadacintyatvamiṣyate // Abhs_3.6 //",
  "SA14abhisamu:230": "evaṃ kṛtvā yathokto vai jñeyaḥ sarvajñatānaye /",
  "SA14abhisamu:231": "ayaṃ vibhāgo niḥśoṣo vipakṣapratipakṣayoḥ // Abhs_3.7 //",
  "SA14abhisamu:232": "rūpādau tadanityādau tadapuriprapūrayoḥ /",
  "SA14abhisamu:233": "tadasaṅgatve caryāyā:",
  "SA14abhisamu:234": "prayogaḥ pratiṣedhataḥ // Abhs_3.8 //",
  "SA14abhisamu:235": "avikāro na kartā ca prayogo duṣkarastridhā /",
  "SA14abhisamu:236": "yathābhavyaṃ phalaprāpteravandhyo 'bhimataśca saḥ // Abhs_3.9 //",
  "SA14abhisamu:237": "aparapratyayo yaśca saptadhā khyātivedakaḥ /",
  "SA14abhisamu:238": "caturdhā mananā tasya rūpādau samatā matā // Abhs_3.10 //",
  "SA14abhisamu:239": "dharmajñānānvayajñānakṣāntijñānakṣaṇātmakaḥ /",
  "SA14abhisamu:240": "duḥkhādisatye dṛṅmārga eṣa sarvajñatānaye // Abhs_3.11 //",
  "SA14abhisamu:241": "rūpaṃ na nityaṃ nānityamatitāntaṃ viśuddhakam /",
  "SA14abhisamu:242": "anutpannāniruddhādi vyomābhaṃ lepavarjitam // Abhs_3.12 //",
  "SA14abhisamu:243": "parigraheṇa nirmuktamavyāhāraṃ svabhāvataḥ /",
  "SA14abhisamu:244": "pravyāhāreṇa nāsyārthaḥ pareṣu prāpyate yataḥ // Abhs_3.13 //",
  "SA14abhisamu:245": "nopalambhakṛdatyantaviśuddhirvyādhyasaṃbhavaḥ /",
  "SA14abhisamu:246": "apāyocChittyakalpatve phalasākṣātkriyāṃ prati // Abhs_3.14 //",
  "SA14abhisamu:247": "asaṃsargo nimittaiśca vastuni vyañjane dvaye /",
  "SA14abhisamu:248": "jñānasya yā cānutpattiriti sarvajñatākṣaṇāḥ // Abhs_3.15 //",
  "SA14abhisamu:249": "iti seyaṃ punaḥ seyaṃ seyaṃ khalu punastridhā /",
  "SA14abhisamu:250": "adhikāratrayasyaiṣā samāptiḥ paridīpitā // Abhs_3.16 //",
  "SA14abhisamu:251": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:252": "sarvajñatādhikāras tṛtīyaḥ //",
  "SA14abhisamu:253": "vastujñānaprakārāṇāmākārā iti lakṣaṇam /",
  "SA14abhisamu:254": "sarvajñatānāṃ traividyāt trividhā eva te matāḥ // Abhs_4.1 //",
  "SA14abhisamu:255": "asadākāramārabhya yāvanniścalatākṛtiḥ /",
  "SA14abhisamu:256": "catvāraḥ pratisatyaṃ te mārge pañcadaśa smṛtāḥ // Abhs_4.2 //",
  "SA14abhisamu:257": "hetau mārge ca duḥkhe ca nirodhe ca yathākramam /",
  "SA14abhisamu:258": "aṣṭau te sapta pañceti ṣoḍaśeti ca kīrtitāḥ // Abhs_4.3 //",
  "SA14abhisamu:259": "smṛtyupasthānamārabhya buddhatvākārapaścimāḥ /",
  "SA14abhisamu:260": "śiṣyāṇāṃ bodhisattvānāṃ buddhānāṃ ca yathākramam // Abhs_4.4 //",
  "SA14abhisamu:261": "saptatriṃśaccatustriṃśattriṃśannava ca te matāḥ /",
  "SA14abhisamu:262": "trisarvajñatvabhedena mārgasatyānurodhataḥ // Abhs_4.5 //",
  "SA14abhisamu:263": "kṛtādhikārā buddheṣu teṣūptaśubhamūlakāḥ /",
  "SA14abhisamu:264": "mitraiḥ sanāthāḥ kalyāṇairasyāḥ śravaṇabhājanam // Abhs_4.6 //",
  "SA14abhisamu:265": "buddhopāsanasaṃpraśnadānaśīlādicaryayā /",
  "SA14abhisamu:266": "udgrahadhāraṇādīnāṃ bhājanatvaṃ satāṃ matam // Abhs_4.7 //",
  "SA14abhisamu:267": "rūpādiṣvanavasthānātteṣu yoganiṣedhataḥ /",
  "SA14abhisamu:268": "tattathatāgambhīratvātteṣāṃ duravagāhataḥ // Abhs_4.8 //",
  "SA14abhisamu:269": "tadaprāmāṇyataḥ kṛcChāccireṇa pratibodhataḥ /",
  "SA14abhisamu:270": "vyākṛtāvavivartyatve niryāṇe sanirantare // Abhs_4.9 //",
  "SA14abhisamu:271": "āsannabodhe kṣipraṃ ca parārthe 'vṛddhyahānitaḥ /",
  "SA14abhisamu:272": "dharmādharmādyadṛṣṭau ca rupācintyādyadarśane // Abhs_4.10 //",
  "SA14abhisamu:273": "rūpādestannimittasya tadbhāvasyāvikalpakaḥ /",
  "SA14abhisamu:274": "phalaratnapradātā ca śuddhakaḥ sāvadhiśca saḥ // Abhs_4.11 //",
  "SA14abhisamu:275": "mārāṇāṃ śaktihānyādiścaturdaśavidho guṇaḥ /",
  "SA14abhisamu:276": "doṣāśca ṣaḍviboddhavyāścaturbhirdaśakaiḥ saha // Abhs_4.12 //",
  "SA14abhisamu:277": "lakṣyate yena tajjñeyaṃ lakṣaṇaṃ trividhaṃ ca tat /",
  "SA14abhisamu:278": "jñānaṃ viśeṣaḥ kāritraṃ svabhāvo yaśca lakṣyate // Abhs_4.13 //",
  "SA14abhisamu:279": "tathāgatasya nirvṛttau loke cālujyanātmake /",
  "SA14abhisamu:280": "sattvānāṃ cittacaryāsu tatsaṃkṣepe bahirgatau // Abhs_4.14 //",
  "SA14abhisamu:281": "akṣayākāratāyāṃ ca sarāgādau pravistṛte /",
  "SA14abhisamu:282": "mahadgate 'pramāṇe ca vijñāne cānidarśane // Abhs_4.15 //",
  "SA14abhisamu:283": "adṛśyacitajñāne ca tadunmiñjādisaṃjñakam /",
  "SA14abhisamu:284": "punastathatākāreṇa teṣāṃ jñānamataḥ param // Abhs_4.16 //",
  "SA14abhisamu:285": "tathatāyāṃ munerbodhe tatparākhyānamityayam /",
  "SA14abhisamu:286": "sarvajñatādhikāreṇa jñānalakṣaṇasaṃgrahaḥ // Abhs_4.17 //",
  "SA14abhisamu:287": "śūnyatve sānimitte c praṇidhānavivarjite /",
  "SA14abhisamu:288": "anutpādanirodhādau dharmatāyā akopane // Abhs_4.18 //",
  "SA14abhisamu:289": "asaṃskare 'vikalpe ca prabhedālakṣaṇatvayoḥ /",
  "SA14abhisamu:290": "mārgajñatādhikāreṇa jñānalakṣaṇamiṣyate // Abhs_4.19 //",
  "SA14abhisamu:291": "svadharmamupaniśritya vihāre tasya satkṛtau /",
  "SA14abhisamu:292": "gurūtve mānanāyāṃ ca tatpūjākṛtakatvayoḥ // Abhs_4.20 //",
  "SA14abhisamu:293": "sarvatra vṛttimajjñānamadṛṣṭasya ca darśakam /",
  "SA14abhisamu:294": "lokasya śūnyatākārasūcakajñāpakākṣagam // Abhs_4.21 //",
  "SA14abhisamu:295": "acintyaśāntatādarśi lokasaṃjñānirodhi ca /",
  "SA14abhisamu:296": "jñānalakṣaṇamityuktaṃ sarvākārajñatānaye // Abhs_4.22 //",
  "SA14abhisamu:297": "acityādiviśeṣeṇa viśiṣṭaiḥ satyagocaraiḥ /",
  "SA14abhisamu:298": "viśeṣalakṣaṇaṃ ṣaḍbhirdaśabhiścoditaṃ kṣaṇaiḥ // Abhs_4.23 //",
  "SA14abhisamu:299": "acintyātulyate meyasaṃkhyeyāsamatikramau /",
  "SA14abhisamu:300": "sarvāryasaṃgraho vijñavedyāsādhāraṇajñate // Abhs_4.24 //",
  "SA14abhisamu:301": "kṣiprajñānyunapūrṇatve pratipatsamudāgamau /",
  "SA14abhisamu:302": "ālambanaṃ ca sādhāraṃ sākalyaṃ saṃparigrahaḥ // Abhs_4.25 //",
  "SA14abhisamu:303": "anāsvādaśca vijñeyo viśeṣaḥ ṣoḍaśātmakaḥ /",
  "SA14abhisamu:304": "viśeṣamārgā mārgebhyo yenānyebhyo viśiṣyate // Abhs_4.26 //",
  "SA14abhisamu:305": "hitaṃ sukhaṃ ca trāṇaṃ ca śaraṇaṃ layanaṃ nṛṇām /",
  "SA14abhisamu:306": "parāyaṇaṃ ca dvīpaṃ ca pariṇāyakasaṃjñakam // Abhs_4.27 //",
  "SA14abhisamu:307": "anābhogaṃ tribhiryānaiḥ phalāsākṣātkriyātmakam /",
  "SA14abhisamu:308": "paścimaṃ gatikāritramidaṃ kāritralakṣaṇam // Abhs_4.28 //",
  "SA14abhisamu:309": "kleśaliṅganimittānāṃ vipakṣapratipakṣayoḥ /",
  "SA14abhisamu:310": "viveko duṣkaraikāntāvuddeśo 'nupalambhakaḥ // Abhs_4.29 //",
  "SA14abhisamu:311": "niṣiddhābhiniveśaśca yaścālambanasaṃjñakaḥ /",
  "SA14abhisamu:312": "vipratyayo 'vighātī ca so 'padāgatyajātikaḥ // Abhs_4.30 //",
  "SA14abhisamu:313": "tathatānupalambhaśca svabhāvaḥ ṣoḍaśātmakaḥ /",
  "SA14abhisamu:314": "lakṣmeva lakṣyate ceticaturthaṃ lakṣaṇaṃ matam // Abhs_4.31 //",
  "SA14abhisamu:315": "animittapradānādisamudāgamakauśalam /",
  "SA14abhisamu:316": "sarvākārāvabodhe 'smin mokṣabhagīyamiṣyate // Abhs_4.32 //",
  "SA14abhisamu:317": "buddhādyālambanā śraddhā vīryaṃ dānādigocaram /",
  "SA14abhisamu:318": "smṛtirāśayasaṃpattiḥ samādhiravikalpanā // Abhs_4.33 //",
  "SA14abhisamu:319": "dharmeṣu sarvairākārairjñānaṃ prajñeti pañcadhā /",
  "SA14abhisamu:320": "tīkṣṇai subodhā saṃbodhirdurbodhā mṛdubhirmatā // Abhs_4.34 //",
  "SA14abhisamu:321": "ālambanaṃ sarvasattvā ūṣmaṇāmiha śakyate /",
  "SA14abhisamu:322": "samacittādirākārasteṣveva daśadhoditaḥ // Abhs_4.35 //",
  "SA14abhisamu:323": "svayaṃ pāpānnivṛttasya dānādyeṣu sthitasya ca /",
  "SA14abhisamu:324": "tayorniyojanānyeṣāṃ varṇavādānukulate // Abhs_4.36 //",
  "SA14abhisamu:325": "mūrdhagam svaparādhāraṃ satyajñānaṃ tathā kṣamā /",
  "SA14abhisamu:326": "tathāgradharmā vijñeyāḥ sattvānāṃ pācanādibhiḥ // Abhs_4.37 //",
  "SA14abhisamu:327": "nirvedhaṅgānyupādaya darśanābhyāsamārgayoḥ /",
  "SA14abhisamu:328": "ye bodhisattvā vartante so 'trāvaivartiko gaṇaḥ // Abhs_4.38 //",
  "SA14abhisamu:329": "rūpādibhyo nivṛttyādyairliṅgairviṃśatidheritaiḥ /",
  "SA14abhisamu:330": "nirvedhāṅgasthitasyedamavaivartikalakṣaṇam // Abhs_4.39 //",
  "SA14abhisamu:331": "rūpādibhyo nivṛttiśca vicikitsākṣaṇakṣayau /",
  "SA14abhisamu:332": "ātmanaḥ kuśalasthasya pareṣāṃ tanniyojanam // Abhs_4.40 //",
  "SA14abhisamu:333": "parādhāraṃ ca dānādi gambhire 'rthe 'pyakāṅkṣaṇam /",
  "SA14abhisamu:334": "maitraṃ kāyādyasaṃvāsaḥ pañcadhāvaraṇena ca // Abhs_4.41 //",
  "SA14abhisamu:335": "sarvānuśayahānaṃ ca smṛtisaṃprajñatā suci /",
  "SA14abhisamu:336": "civarādiśarīre ca kṛmīṇāmasamudbhavaḥ // Abhs_4.42 //",
  "SA14abhisamu:337": "cittākauṭilyamādānaṃ dhūtasyāmatsarāditā /",
  "SA14abhisamu:338": "dharmatāyuktagāmitvaṃ lokārthaṃ narakaiṣaṇā // Abhs_4.43 //",
  "SA14abhisamu:339": "parairaneyatā mārasyānyamārgopadeśinaḥ /",
  "SA14abhisamu:340": "māra ityavabodhaśca caryā buddhānumoditā // Abhs_4.44 //",
  "SA14abhisamu:341": "ūṣmamūrdhasu sa kṣāntiṣvagradharmeṣvavasthitaḥ /",
  "SA14abhisamu:342": "liṅgairamībhirviśatyā saṃbodherna vivartate // Abhs_4.45 //",
  "SA14abhisamu:343": "kṣāntijñānakṣaṇaḥ ṣaṭ ca pañca pañca ca dṛkpathe /",
  "SA14abhisamu:344": "bodhisattvasya vijñeyamavaivartikalakṣaṇam // Abhs_4.46 //",
  "SA14abhisamu:345": "rūpādisaṃjñāvyāvṛttirdārḍhyaṃ cittasya hinayoḥ /",
  "SA14abhisamu:346": "yānayorvinivṛttiśca dhyānādyaṅgaparikṣayaḥ // Abhs_4.47 //",
  "SA14abhisamu:347": "kāyacetolaghutvaṃ ca kāmasevābhyupāyikī /",
  "SA14abhisamu:348": "sadaiva brahmacāritvamājīvasya viśuddhatā // Abhs_4.48 //",
  "SA14abhisamu:349": "skandhādavantarāyeṣu saṃbhāre sendriyādike /",
  "SA14abhisamu:350": "samare matsarādau ca neti yogānuyogayoḥ // Abhs_4.49 //",
  "SA14abhisamu:351": "vihārapratiṣedhaśca dharmasyāṇoralabdhatā /",
  "SA14abhisamu:352": "niścitatvaṃ svabhumau ca bhūmitritayasaṃsthitiḥ // Abhs_4.50 //",
  "SA14abhisamu:353": "dharmārthaṃ jīvitatyāga ityamī ṣoḍaśa kṣaṇāḥ /",
  "SA14abhisamu:354": "avaivartikaliṅgāni dṛṅmārgasthasya dhīmataḥ // Abhs_4.51 //",
  "SA14abhisamu:355": "gambhiro bhāvanāmārgo gāmbhīryaṃ śunyatādikam /",
  "SA14abhisamu:356": "samāropāpavādāntamuktatā sā gabhīratā // Abhs_4.52 //",
  "SA14abhisamu:357": "cintātulananidhyānānyabhīkṣṇaṃ bhāvanāpathaḥ /",
  "SA14abhisamu:358": "nirvedhāṅgeṣu dṛṅmārge bhāvanāmārga eva ca // Abhs_4.53 //",
  "SA14abhisamu:359": "prābandhakatvādiṣṭo 'sau navadhā ca prakārataḥ /",
  "SA14abhisamu:360": "mṛdumadhyādhimātrāṇāṃ punarmṛdvādibhedataḥ // Abhs_4.54 //",
  "SA14abhisamu:361": "asaṃkhyeyādinirdeśāḥ paramārthena na kṣamāḥ /",
  "SA14abhisamu:362": "kṛpāniṣyandabhūtāste saṃvṛtyābhimatā muneḥ // Abhs_4.55 //",
  "SA14abhisamu:363": "hānivṛddhī na yujyete nirālāpasya vastunaḥ /",
  "SA14abhisamu:364": "bhāvanākhyena kiṃ hinaṃ vartmanā kimudāgatam // Abhs_4.56 //",
  "SA14abhisamu:365": "yathā bodhistathaivāsāviṣṭasyārthasya sādhakaḥ /",
  "SA14abhisamu:366": "tathatālakṣaṇā bodhiḥ so 'pi tallakṣaṇo mataḥ // Abhs_4.57 //",
  "SA14abhisamu:367": "pūrveṇa bodhirno yuktā manasā paścimena vā /",
  "SA14abhisamu:368": "dīpadṛṣṭāntayogenagambhīrā dharmatāṣṭadhā // Abhs_4.58 //",
  "SA14abhisamu:369": "utpāde ca nirodhe ca tathatāyāṃ gabhīratā /",
  "SA14abhisamu:370": "jñeye dāne ca caryāyāmadvayopāyakauśale ca // Abhs_4.59 //",
  "SA14abhisamu:371": "svapnopamatvāddharmāṇāṃ bhavaśāntyorakalpanā /",
  "SA14abhisamu:372": "karmābhāvādicodyānāṃ parihārā yathoditāḥ // Abhs_4.60 //",
  "SA14abhisamu:373": "sattvalokasya yā śuddhistasyāḥ suddhyupahārataḥ /",
  "SA14abhisamu:374": "tathā bhājanalokasya buddhakṣetrasya śuddhatā // Abhs_4.61 //",
  "SA14abhisamu:375": "viṣayo 'sya prayogaśca śātravāṇāmatikramaḥ /",
  "SA14abhisamu:376": "apratiṣṭho yathāvedhamasādhāraṇalakṣaṇaḥ // Abhs_4.62 //",
  "SA14abhisamu:377": "asakto 'nupalambhaśca nimittapraṇidhikṣataḥ /",
  "SA14abhisamu:378": "lalliṅgaścāpramāṇaśca daśadhopāyakauśalam // Abhs_4.63 //",
  "SA14abhisamu:379": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:380": "sarvākārābhisaṃbodhādhikāraś caturthaḥ //",
  "SA14abhisamu:381": "svapnāntare 'pi svapnābhasarvadharmekṣaṇādikam /",
  "SA14abhisamu:382": "murdhaprāptasya yogasya liṅgaṃ dvādaśadhā matam // Abhs_5.1 //",
  "SA14abhisamu:383": "jambudvipajaneyattābuddhapūjāśubhādikām /",
  "SA14abhisamu:384": "upamāṃ bahudhā kṛtvā vivṛddhiḥ ṣoḍaśātmikā // Abhs_5.2 //",
  "SA14abhisamu:385": "trisarvajñatvadharmāṇāṃ paripuriranuttarā /",
  "SA14abhisamu:386": "aparityaktasattvārthā nirūḍhirabhidhīyate // Abhs_5.3 //",
  "SA14abhisamu:387": "caturdvipakasāhasradvitrisāhasrakopamaḥ /",
  "SA14abhisamu:388": "kṛtvā puṇyabahutvena samādhiḥ parikīrtitaḥ // Abhs_5.4 //",
  "SA14abhisamu:389": "pravṛttau ca nivṛttau ca pratyekaṃ tau navātmakau /",
  "SA14abhisamu:390": "grāhyau vikalpau vijñeyāvayāthāviṣayātmakau // Abhs_5.5 //",
  "SA14abhisamu:391": "dravyaprajñaptisatsattvavikalpau grāhakau matau /",
  "SA14abhisamu:392": "pṛthagjanāryabhedena pratyekaṃ tau navātmakau // Abhs_5.6 //",
  "SA14abhisamu:393": "grāhyau cenna tathā sto 'rthau kasya tau grāhakau matau /",
  "SA14abhisamu:394": "iti grāhakabhāvena śunyatālakṣaṇaṃ tayoḥ // Abhs_5.7 //",
  "SA14abhisamu:395": "eṣa svabhāve gotre ca pratipatsamudāgame /",
  "SA14abhisamu:396": "jñānasyālambanābhrāntau pratipakṣavipakṣayoḥ // Abhs_5.8 //",
  "SA14abhisamu:397": "svasminnadhigame kartṛtatkāritrakriyāphale /",
  "SA14abhisamu:398": "pravṛttipakṣādhiṣṭhāno vikalpo navadhā mataḥ // Abhs_5.9 //",
  "SA14abhisamu:399": "bhavaśāntiprapātitvānnyūnatve 'dhigamasya ca /",
  "SA14abhisamu:400": "parigrahasyābhāve ca vaikalye pratipadgate // Abhs_5.10 //",
  "SA14abhisamu:401": "parapratyayagāmitve samuddeśanivartane /",
  "SA14abhisamu:402": "prādeśikatve nānātve sthānaprasthānamohayoḥ // Abhs_5.11 //",
  "SA14abhisamu:403": "pṛṣṭhato gamane ceti vikalpo 'yaṃ navātmakaḥ /",
  "SA14abhisamu:404": "nivṛttipakṣādhiṣṭhānaḥ śrāvakādimanobhavaḥ // Abhs_5.12 //",
  "SA14abhisamu:405": "grāhakaḥ prathamo jñeyo gragaṇapratimokṣaṇe /",
  "SA14abhisamu:406": "manaskriyāyāṃ dhātūnāmupaśleṣe trayasya ca // Abhs_5.13 //",
  "SA14abhisamu:407": "sthāne cābhiniveśe ca prajñaptau dharmavastunaḥ /",
  "SA14abhisamu:408": "saktau ca pratipakṣe ca yathecChaṃ ca gatikṣatau // Abhs_5.14 //",
  "SA14abhisamu:409": "yathoddeśamaniryāṇe mārgāmārgāvadhāraṇe /",
  "SA14abhisamu:410": "sanirodhe samutpāde vastuyogaviyogayoḥ // Abhs_5.15 //",
  "SA14abhisamu:411": "sthāne gotrasya nāśe ca prārthanāhetvabhāvayoḥ /",
  "SA14abhisamu:412": "pratyarthikopalambhe ca vikalpo grāhako 'paraḥ // Abhs_5.16 //",
  "SA14abhisamu:413": "bodhau sandarśanānyeṣāṃ taddhetośca parindanā /",
  "SA14abhisamu:414": "tatprāptyanantaro hetuḥ puṇyabāhulyalakṣaṇaḥ // Abhs_5.17 //",
  "SA14abhisamu:415": "kṣayānutpādayorjñāne malānāṃ bodhirūcyate /",
  "SA14abhisamu:416": "kṣayābhāvādanutpādātte hi jñeye yathākramam // Abhs_5.18 //",
  "SA14abhisamu:417": "prakṛtāvaniruddhāyāṃ darśanākhyena vartmanā /",
  "SA14abhisamu:418": "vikalpajātaṃ kiṃ kṣīṇaṃ vānutpattimāgatam // Abhs_5.19 //",
  "SA14abhisamu:419": "sattā ca nāma dharmāṇāṃ jñeye cāvaraṇakṣayaḥ /",
  "SA14abhisamu:420": "kathyate yatparaiḥ śāsturatra vismīyate mayā // Abhs_5.20 //",
  "SA14abhisamu:421": "nāpaneyamataḥ kiṃcitprakṣeptavyaṃ na kiṃcana /",
  "SA14abhisamu:422": "draṣṭavyaṃ bhūtato bhūtaṃ bhūtadarśī vimucyate // Abhs_5.21 //",
  "SA14abhisamu:423": "ekaikasyaiva dānādau teṣāṃ yaḥ saṃgraho mithaḥ /",
  "SA14abhisamu:424": "sa ekakṣaṇikaḥ kṣāntisaṃgṛhīto 'tra dṛkpathe // Abhs_5.22 //",
  "SA14abhisamu:425": "sa samādhiṃ samāpadya tataḥ siṃhavijṛmbhitam /",
  "SA14abhisamu:426": "anulomaṃ vilomaṃ pratītyotpādamīkṣate // Abhs_5.23 //",
  "SA14abhisamu:427": "kāmāptamavadhīkṛtya vijñānamasamāhitam /",
  "SA14abhisamu:428": "sanirodhāḥ samāpattīrgatvāgamya nava dvidhā // Abhs_5.24 //",
  "SA14abhisamu:429": "ekadvitricatuḥpañcaṣaṭsaptāṣṭavyatikramāt /",
  "SA14abhisamu:430": "avaskandasamāpattirāniroodhamatulyatā // Abhs_5.25 //",
  "SA14abhisamu:431": "saṃkṣepe vistare buddhaiḥ sanāthyenāparigrahe /",
  "SA14abhisamu:432": "traikālike guṇābhāve śreyasastrividhe pathi // Abhs_5.26 //",
  "SA14abhisamu:433": "eko grāhyavikalpo 'yaṃ prayogākāragocaraḥ /",
  "SA14abhisamu:434": "dvitīyaścittacaittānāṃ pravṛttiviṣayo mataḥ // Abhs_5.27 //",
  "SA14abhisamu:435": "anutpādastu cittasya bodhimaṇḍāmanaskriyā /",
  "SA14abhisamu:436": "hīnayānamanaskārau saṃbodheramanaskṛtiḥ // Abhs_5.28 //",
  "SA14abhisamu:437": "bhāvane 'bhāvane caiva tadviparyaya eva ca /",
  "SA14abhisamu:438": "ayathārthaśca vijñeyo vikalpo bhāvanāpathe // Abhs_5.29 //",
  "SA14abhisamu:439": "grāhakaḥ prathamo jñeyaḥ sattvaprajñaptigocaraḥ /",
  "SA14abhisamu:440": "dharmaprajñaptyaśūnyatve saktipravicayātmakaḥ // Abhs_5.30 //",
  "SA14abhisamu:441": "kṛtena vastuno yānatritaye ca sa kīrtitaḥ /",
  "SA14abhisamu:442": "dakṣiṇāyā aśuddhau ca caryāyāśca vikopane // Abhs_5.31 //",
  "SA14abhisamu:443": "sattvaprajñaptitaddhetuviṣayo navadhāparaḥ /",
  "SA14abhisamu:444": "bhāvanāmārgasaṃbuddho vipakṣastadvighātataḥ // Abhs_5.32 //",
  "SA14abhisamu:445": "sarvajñatānāṃ tisR^īṇāṃ yathāsvaṃ trividhāvṛtau /",
  "SA14abhisamu:446": "śāntimārge tathatādisaṃprayogaviyogayoḥ // Abhs_5.33 //",
  "SA14abhisamu:447": "asamatve ca duḥkhādau kleśānāṃ prakṛtāvapi /",
  "SA14abhisamu:448": "dvayābhāve ca saṃmohe vikalpaḥ paścimo mataḥ // Abhs_5.34 //",
  "SA14abhisamu:449": "āsāṃ kṣaye satītīnāṃ cirāyocChvasitā iva /",
  "SA14abhisamu:450": "sarvākārajagatsaukhyasādhanā guṇasaṃpadaḥ // Abhs_5.35 //",
  "SA14abhisamu:451": "sarvāḥ sarvābhisāreṇa nikāmaphalaśālinam /",
  "SA14abhisamu:452": "bhajante taṃ mahāsattvaṃ mahodadhimivāpagāḥ // Abhs_5.36 //",
  "SA14abhisamu:453": "trisāhasrajanaṃ śiṣyakhaḍgādhigamasaṃpadi /",
  "SA14abhisamu:454": "bodhisattvasya ca nyāmo pratiṣṭāpya śubhopamāḥ // Abhs_5.37 //",
  "SA14abhisamu:455": "kṛtvā puṇyabahutvena buddhatvapteranantaraḥ /",
  "SA14abhisamu:456": "ānantaryasamādhiḥ sa sarvākārajñatā ca tat // Abhs_5.38 //",
  "SA14abhisamu:457": "ālambanamabhāvo 'sya smṛtiścādhipatirmataḥ /",
  "SA14abhisamu:458": "ākāraḥ śāntatā cātra jalpājalpipravādinām // Abhs_5.39 //",
  "SA14abhisamu:459": "ālambanopapattau ca tatsvabhāvāvadhārraṇe /",
  "SA14abhisamu:460": "sarvākārajñatājñāne paramārthe sasaṃvṛtau // Abhs_5.40 //",
  "SA14abhisamu:461": "prayoge triṣu ratneṣu sopāye samaye muneḥ /",
  "SA14abhisamu:462": "viparyāse samārge ca pratipakṣavipakṣayoḥ // Abhs_5.41 //",
  "SA14abhisamu:463": "lakṣaṇe bhāvanāyāṃ ca matā vipratipattayaḥ /",
  "SA14abhisamu:464": "sarvākārajñatādhārā ṣoḍhā daśa ca cādinām // Abhs_5.42 //",
  "SA14abhisamu:465": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:466": "mūrdhābhisamayādhikāraḥ pañcamaḥ //",
  "SA14abhisamu:467": "dānena prajñayā yāvadbuddhādau smṛtibhiśca sā /",
  "SA14abhisamu:468": "dharmābhāvasvabhāvenetyanupūrvakriyā matā // Abhs_6.1 //",
  "SA14abhisamu:469": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:470": "anupurvābhisamayādhikāraḥ ṣaṣṭhaḥ //",
  "SA14abhisamu:471": "anūsravāṇāṃ sarveṣāmekaikenāpi saṃgrahāt /",
  "SA14abhisamu:472": "ekakṣaṇāvabodho 'yaṃ jñeyo dānādinā muneḥ // Abhs_7.1 //",
  "SA14abhisamu:473": "aradhaṭṭaṃ yathaikāpi padikā purūṣeritā /",
  "SA14abhisamu:474": "sakṛtsarvaṃ calayati jñānamekakṣaṇe tathā // Abhs_7.2 //",
  "SA14abhisamu:475": "vipākadharmatāvasthā sarvaśuklamayī yadā /",
  "SA14abhisamu:476": "prajñāpāramitā jātā jñānamekakṣaṇe tadā // Abhs_7.3 //",
  "SA14abhisamu:477": "svapnopameṣu dharmeṣu sthitvā dānādicaryā /",
  "SA14abhisamu:478": "alakṣatvaṃ dharmāṇāṃ kṣaṇenaikena vindati // Abhs_7.4 //",
  "SA14abhisamu:479": "svapnaṃ taddarśinaṃ caiva dvayayogena nekṣate /",
  "SA14abhisamu:480": "dharmāṇāmadvayaṃ tattvaṃ kṣaṇenaikenapaṣyati // Abhs_7.5 //",
  "SA14abhisamu:481": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:482": "ekakṣaṇābhisamayādhikāraḥ saptamaḥ //",
  "SA14abhisamu:483": "sarvākārāṃ viśuddhiṃ ye dharmāḥ prāptā nirāsravāḥ /",
  "SA14abhisamu:484": "svābhāvikomuneḥ kāyasteśāṃ prakṛtilakṣaṇaḥ // Abhs_8.1 //",
  "SA14abhisamu:485": "bodhipakṣāpramāṇāni vimokṣā anūpurvaśaḥ /",
  "SA14abhisamu:486": "navātmikā samāpattiḥ kṛtsnaṃ daśavidhātmikam // Abhs_8.2 //",
  "SA14abhisamu:487": "abhibhvāyatanānyaṣṭaprakārāṇi prabhedataḥ /",
  "SA14abhisamu:488": "araṇā praṇidhijñānamabhijñāḥ pratisaṃvidaḥ // Abhs_8.3 //",
  "SA14abhisamu:489": "sarvākārāścatasno 'thaśuddhayo vaśitā daśa //",
  "SA14abhisamu:490": "balāni daśa catvāri vaiśāradyānyārakṣaṇam // Abhs_8.4 //",
  "SA14abhisamu:491": "trividhaṃ smṛtyupasthānaṃ tridhāsaṃmoṣadharmatā /",
  "SA14abhisamu:492": "vāsanāyāḥ samuddhāto mahati karūṇā jane // Abhs_8.5 //",
  "SA14abhisamu:493": "āveṇikā munereva dharmā ye 'ṣṭādaśeritāḥ /",
  "SA14abhisamu:494": "sarvākārajñatā ceti dharmakāyo 'bhidhīyate // Abhs_8.6 //",
  "SA14abhisamu:495": "śravakasyāraṇādṛṣṭernṛkleśaparihāritā /",
  "SA14abhisamu:496": "tatkleśasrotaucChittyai grāmādiṣu jināraṇā // Abhs_8.7 //",
  "SA14abhisamu:497": "anābhogamanāsaṅmavyāghātaṃ sadā sthitam /",
  "SA14abhisamu:498": "sarvapraśnāpanudvauddhaṃ praṇidhijñānamiṣyate // Abhs_8.8 //",
  "SA14abhisamu:499": "paripākaṃ gate hetau yasya yasya yadā yadā /",
  "SA14abhisamu:500": "hitaṃ bhavati kartavyaṃ prathate tasya tasya saḥ // Abhs_8.9 //",
  "SA14abhisamu:501": "varṣatyapi hi parjanye naiva bījaṃ prarohati /",
  "SA14abhisamu:502": "samutpāde 'pi buddhānāṃ nābhavyo bhadramaśrute // Abhs_8.10 //",
  "SA14abhisamu:503": "iti kāritravaipulyāt buddho vyāpī nirūcyate /",
  "SA14abhisamu:504": "akṣayatvācca tasyaiva nitya ityapi kathyate // Abhs_8.11 //",
  "SA14abhisamu:505": "dvātriṃśallakṣaṇāśītivyañjanātmā munerayam /",
  "SA14abhisamu:506": "sāṃbhogiko mataḥ kāyo mahāyānopabhogataḥ // Abhs_8.12 //",
  "SA14abhisamu:507": "cakrāṅkahastaḥ krākurmapādo jālāvāddhāṅgulipāṇipādaḥ /",
  "SA14abhisamu:508": "karau sapādau tarūṇau mṛdū ca mamutsadaiḥ saptabhirāśrayo 'sya // Abhs_8.13 //",
  "SA14abhisamu:509": "dīrghāṅgulirvyāyatapārṣṇigātraṃ prājyaṃ tvṛjucChaṅkhapadordhvaromā /",
  "SA14abhisamu:510": "eṇeyajaṅghaśca paṭūrūbāhuḥ kośāvanaddhottamabastiguhyaḥ // Abhs_8.14 //",
  "SA14abhisamu:511": "suvarṇavarṇḥ pratanucChaviśca pradakṣiṇaikaikasujātromā /",
  "SA14abhisamu:512": "ūūrṇāṅkitāsyo haripūrvakāyaḥ skandhau vṛtāvasya citāntarāṃsaḥ // Abhs_8.15 //",
  "SA14abhisamu:513": "hīno rasaḥ khyāti rasottamo 'sya nyagrodhavanmāṇḍalatulyamūrtiḥ /",
  "SA14abhisamu:514": "uṣīṣamūrdhā pṛthucārūjihvo brahmasvaraḥ siṃhahanuḥ suśuklāḥ // Abhs_8.16 //",
  "SA14abhisamu:515": "tulyāḥ pramāṇe 'viralāsca dantā anyūnasaṃkhyā daśikāścatasraḥ /",
  "SA14abhisamu:516": "nīlekśaṇe govṛṣapakṣmanetro dvātriṃśadetāni hi lakṣaṇāni // Abhs_8.17 //",
  "SA14abhisamu:517": "yasya yasyātra yo heturlakṣaṇasya prasādhakaḥ /",
  "SA14abhisamu:518": "tasya tasya prapūryāyaṃ samudāgamalakṣaṇaḥ // Abhs_8.18 //",
  "SA14abhisamu:519": "gurūṇāmanuyānādirdṛḍhatā saṃvaraṃ prati /",
  "SA14abhisamu:520": "saṃgrahāsevanaṃ dānaṃ praṇitasya ca vastunaḥ // Abhs_8.19 //",
  "SA14abhisamu:521": "vadhyamokṣasamādānāṃ vivR6iddhiḥ kuśalasya ca /",
  "SA14abhisamu:522": "ityādiko yathāsutraṃ heturlakṣaṇasādhakaḥ // Abhs_8.20 //",
  "SA14abhisamu:523": "tāmrāḥ snigdhāśca tuṅgāśca nakhā aṅgulayo muneḥ /",
  "SA14abhisamu:524": "vṛttāścitānupūrvāśca guḍhā nirgranthyaḥ śirāḥ // Abhs_8.21 //",
  "SA14abhisamu:525": "guḍhau gulphau samau pādau siṃhebhadvijagopateḥ /",
  "SA14abhisamu:526": "vikrāntaṃ dakṣiṇaṃ cārūgamanamṛjuvṛttatā // Abhs_8.22 //",
  "SA14abhisamu:527": "mṛṣṭānupūrte medhyamṛdutve śuddhagātratā /",
  "SA14abhisamu:528": "pūrṇavyañjanatā cārupṛthumaṇḍalagāratā // Abhs_8.23 //",
  "SA14abhisamu:529": "samakramatvaṃ śuddhatvaṃ netrayoḥ sukumāratā /",
  "SA14abhisamu:530": "adīnossadagātratve susaṃhatanagātratā // Abhs_8.24 //",
  "SA14abhisamu:531": "suvibhaktāṅgatā dhvāntapradhvastālokaśuddhatā /",
  "SA14abhisamu:532": "vṛttamṛṣṭākṣāmakukṣitāśca gabhiratā // Abhs_8.25 //",
  "SA14abhisamu:533": "dakṣiṇāvartatā nābheḥ samantaddarśanīyatā /",
  "SA14abhisamu:534": "samācāraḥ śuciḥ kālatilakāpagatā tanuḥ // Abhs_8.26 //",
  "SA14abhisamu:535": "karau tulamṛdūsnigdhagambhīrāyatalekhatā /",
  "SA14abhisamu:536": "nātyāyataṃ vaco bimbapratibimbopamauṣṭhatā // Abhs_8.27 //",
  "SA14abhisamu:537": "mṛdvī tanvī ca raktā ca jihvā jimūtaghoṣatā /",
  "SA14abhisamu:538": "cārūmañjasvaro daṃṣṭrā vṛttāstīkṣṇāḥ sitāḥ samāḥ // Abhs_8.28 //",
  "SA14abhisamu:539": "anupurvī gatāstuṅgā nāsikā paraṃ śuciḥ /",
  "SA14abhisamu:540": "viśale nayane pakṣmacitaṃ padmadalākṣitā // Abhs_8.29 //",
  "SA14abhisamu:541": "āyataślakṣṇasusnigdhasamaromnau bhruvau bhujau /",
  "SA14abhisamu:542": "pīnāyatau samau karṇāvupaghātavivarjitau // Abhs_8.30 //",
  "SA14abhisamu:543": "lalāṭamaparimlānaṃ prṛthupūrṇottamāṅgatā /",
  "SA14abhisamu:544": "bhramarābhāścitāḥ ślakṣṇā asaṃlulitamūrtayaḥ // Abhs_8.31 //",
  "SA14abhisamu:545": "keśā aparuṣāḥ puṃsāṃ saurabhyānyapahāriṇaḥ /",
  "SA14abhisamu:546": "śrīvatsaḥ svastikaṃ ceti buddhānuvyañjanaṃ matam // Abhs_8.32 //",
  "SA14abhisamu:547": "karoti yena citrāṇi hitāni jagataḥ samam /",
  "SA14abhisamu:548": "ā bhavātso 'nupacChinnaḥ kayo nairmāṇiko muneḥ // Abhs_8.33 //",
  "SA14abhisamu:549": "tathā karmāpyanucChinnamasyāsaṃsāramiṣyate /",
  "SA14abhisamu:550": "gatīnāṃ śamanaṃ karma saṃgrahe ca caturvidhe // Abhs_8.34 //",
  "SA14abhisamu:551": "niveśanaṃ sasaṃkleśe vyavadānāvabodhane /",
  "SA14abhisamu:552": "sattvānāmarthayāthātmye ṣaṭsu pāramitāsu ca // Abhs_8.35 //",
  "SA14abhisamu:553": "buddhamārge prakṛtyaivaśunyatāyāṃ dvayakṣaye /",
  "SA14abhisamu:554": "saṃkete 'nupalambhe ca paripāke ca hehinām // Abhs_8.36 //",
  "SA14abhisamu:555": "bodhisattvasya mārge 'bhiniveśasya nivāraṇe /",
  "SA14abhisamu:556": "bodhiprāptu jinakṣetraviśuddhau niyatiṃ prati // Abhs_8.37 //",
  "SA14abhisamu:557": "aprameye ca sattvarthe buddhasevādike guṇe /",
  "SA14abhisamu:558": "bodheraṅgeṣvanāśe ca karmaṇāṃ satyadarśane // Abhs_8.38 //",
  "SA14abhisamu:559": "viparyāsaprahāṇe ca tadavastukatānaye /",
  "SA14abhisamu:560": "vyavadāne sasaṃbhāre saṃskṛtāsaṃskṛte prati // Abhs_8.39 //",
  "SA14abhisamu:561": "vyatibhedāparijñāne virnāṇe ca niveśanam /",
  "SA14abhisamu:562": "dharmakāyasya karmedaṃ saptavimśatidhā matam // Abhs_8.40 //",
  "SA14abhisamu:563": "// abhisamayālaṅkāre prajñāpāramitopadeśaśāstre",
  "SA14abhisamu:564": "dharmakāyādhikāro 'ṣṭamaḥ //",
  "SA14abhisamu:565": "lakṣaṇaṃ tatprayogastatprakarṣastadanukramaḥ /",
  "SA14abhisamu:566": "tanniṣṭā tadvipākascotyanyaḥ ṣeḍhārthasaṃgrahaḥ // 1 //",
  "SA14abhisamu:567": "viṣayastritayo hetuḥ prayogaścaturātmakaḥ /",
  "SA14abhisamu:568": "dharmakāyaḥ phalaṃ karmetyanyastredhārthasaṃgrahaḥ // 2 //",
  "SA14abhisamu:569": "abhisamayālaṅkāraṃ nāma prajñāpāramitopadeśaśāstraṃ samāptam //",
  "SA14abhisamu:570": "kṛtirāryamaitreyanāthasya //"
}