{
  "S06prajnhsu:0": "Prajñāpāramitā-hṛdaya-sūtra",
  "S06prajnhsu:1": "Oṃ Namo Bhagavatyai Ārya-prajāpāramitāyai!",
  "S06prajnhsu:2": "Ārya Āvalokiteśvaro bodhisattvo gambhīraṃ",
  "S06prajnhsu:3": "prajñāpāramitā-cāryāṃ caramāṇo",
  "S06prajnhsu:4": "vyavalokayati sma,",
  "S06prajnhsu:5": "pañca-skandhās tāṃś ca",
  "S06prajnhsu:6": "svabhāva-śūnyān paśyati sma.",
  "S06prajnhsu:7": "Iha Śāriputra rūpaṃ śūnyatā,",
  "S06prajnhsu:8": "śūnyat'aiva rūpaṃ,",
  "S06prajnhsu:9": "rūpān na pṛthak śūnyatā,",
  "S06prajnhsu:10": "śūnyatāyā na pṛthag rūpaṃ,",
  "S06prajnhsu:11": "yad rūpaṃ sā śūnyatā,",
  "S06prajnhsu:12": "yā śūnyatā tad rūpaṃ.",
  "S06prajnhsu:13": "Evam eva vedanā-saṃjñā-saṃskārā-vijñānam.",
  "S06prajnhsu:14": "Iha Śāriputra sarva-dharmāḥ śūnyatā-lakṣaṇā",
  "S06prajnhsu:15": "anutpannā aniruddhā amalā avimalā anūnā aparipūrṇāḥ.",
  "S06prajnhsu:16": "Tasmāc Chāriputra śūnyatāyāṃ na rūpaṃ",
  "S06prajnhsu:17": "na vedanā na saṃjñā na saṃskārāḥ na vijñānaṃ,",
  "S06prajnhsu:18": "na cakṣuḥ-śrotra-ghrāṇa-jihvā-kāya-manāṃsi,",
  "S06prajnhsu:19": "na rūpa-śabda-gandha-rasa-spraṣṭavya-dharmāḥ,",
  "S06prajnhsu:20": "na cakṣur-dhātur yāvan na mano-vijñāna-dhātuḥ,",
  "S06prajnhsu:21": "n'āvidyā n'āvidyā-kṣayo yāvan na jarā-maraṇaṃ",
  "S06prajnhsu:22": "na jarā-maraṇa-kṣayo na duḥkha-samudaya-nirodha-mārgā",
  "S06prajnhsu:23": "na jñānaṃ na prāptir n'āprāptiḥ.",
  "S06prajnhsu:24": "Tasmāc Chāriputra aprāptitvād bodhisattvo",
  "S06prajnhsu:25": "prajñā-pāramitām āśritya viharaty acitt'āvaraṇaḥ.",
  "S06prajnhsu:26": "Citt'āvaraṇa-nāstitvād atrasto",
  "S06prajnhsu:27": "viparyās'ātikrānto niṣṭhā-nirvāṇaḥ.",
  "S06prajnhsu:28": "Tradhva-vyavasthitāḥ sarva-buddhāḥ",
  "S06prajnhsu:29": "prajñāpāramitām āśritya anuttarāṃ",
  "S06prajnhsu:30": "samyak-sambodhim abhisambuddhāḥ.",
  "S06prajnhsu:31": "Tasmāj jñātavyaṃ prajñāpāramitā mahā-mantro",
  "S06prajnhsu:32": "mahā-vidyā-mantro'nuttara-mantro",
  "S06prajnhsu:33": "samasama-mantraḥ sarva-duḥkha-praśamanaḥ satyam amithyatvāt.",
  "S06prajnhsu:34": "Prajñāpāramitāyām ukto mantraḥ, tadyathā -",
  "S06prajnhsu:35": "Oṃ gate gate pāragate pārasaṃgate bodhi svāhā.",
  "S06prajnhsu:36": "Ity ārya-prajñāpāramitā-hṛdayaṃ samāptam."
}