{
  "S06prajnhsu:0": "Prajñāpāramitā-hṛdaya-sūtra",
  "S06prajnhsu:1": "Oṃ Namo Bhagavatyai Ārya-prajāpāramitāyai!",
  "S06prajnhsu:2": "Ārya Āvalokiteśvaro bodhisattvo gambhīraṃ",
  "S06prajnhsu:3": "prajñāpāramitā-cāryāṃ caramāṇo",
  "S06prajnhsu:4": "vyavalokayati sma, pañca-skandhās tāṃś ca",
  "S06prajnhsu:5": "svabhāva-śūnyān paśyati sma.",
  "S06prajnhsu:6": "Iha Śāriputra rūpaṃ śūnyatā, śūnyat'aiva rūpaṃ,",
  "S06prajnhsu:7": "rūpān na pṛthak śūnyatā,",
  "S06prajnhsu:8": "śūnyatāyā na pṛthag rūpaṃ,",
  "S06prajnhsu:9": "yad rūpaṃ sā śūnyatā,",
  "S06prajnhsu:10": "yā śūnyatā tad rūpaṃ.",
  "S06prajnhsu:11": "Evam eva vedanā-saṃjñā-saṃskārā-vijñānam.",
  "S06prajnhsu:12": "Iha Śāriputra sarva-dharmāḥ śūnyatā-lakṣaṇā",
  "S06prajnhsu:13": "anutpannā aniruddhā amalā avimalā anūnā aparipūrṇāḥ.",
  "S06prajnhsu:14": "Tasmāc Chāriputra śūnyatāyāṃ na rūpaṃ",
  "S06prajnhsu:15": "na vedanā na saṃjñā na saṃskārāḥ na vijñānaṃ,",
  "S06prajnhsu:16": "na cakṣuḥ-śrotra-ghrāṇa-jihvā-kāya-manāṃsi,",
  "S06prajnhsu:17": "na rūpa-śabda-gandha-rasa-spraṣṭavya-dharmāḥ,",
  "S06prajnhsu:18": "na cakṣur-dhātur yāvan na mano-vijñāna-dhātuḥ,",
  "S06prajnhsu:19": "n'āvidyā n'āvidyā-kṣayo yāvan na jarā-maraṇaṃ",
  "S06prajnhsu:20": "na jarā-maraṇa-kṣayo na duḥkha-samudaya-nirodha-mārgā",
  "S06prajnhsu:21": "na jñānaṃ na prāptir n'āprāptiḥ.",
  "S06prajnhsu:22": "Tasmāc Chāriputra aprāptitvād bodhisattvo",
  "S06prajnhsu:23": "prajñā-pāramitām āśritya viharaty acitt'āvaraṇaḥ.",
  "S06prajnhsu:24": "Citt'āvaraṇa-nāstitvād atrasto",
  "S06prajnhsu:25": "viparyās'ātikrānto niṣṭhā-nirvāṇaḥ.",
  "S06prajnhsu:26": "Tradhva-vyavasthitāḥ sarva-buddhāḥ",
  "S06prajnhsu:27": "prajñāpāramitām āśritya anuttarāṃ",
  "S06prajnhsu:28": "samyak-sambodhim abhisambuddhāḥ.",
  "S06prajnhsu:29": "Tasmāj jñātavyaṃ prajñāpāramitā mahā-mantro",
  "S06prajnhsu:30": "mahā-vidyā-mantro'nuttara-mantro",
  "S06prajnhsu:31": "samasama-mantraḥ sarva-duḥkha-praśamanaḥ satyam amithyatvāt.",
  "S06prajnhsu:32": "Prajñāpāramitāyām ukto mantraḥ, tadyathā -",
  "S06prajnhsu:33": "Oṃ gate gate pāragate pārasaṃgate bodhi svāhā.",
  "S06prajnhsu:34": "Ity ārya-prajñāpāramitā-hṛdayaṃ samāptam."
}