    "GR13bhnstciu:231": " nātha nānyatra dṛśyate BhStc_50d",
    "GR13bhnstciu:232": " nātha prārthayamānānāṃ BhStc_92c",
    "GR13bhnstciu:233": " nātha svapne 'pi yat kuryāṃ BhStc_103a",
    "GR13bhnstciu:234": " nāthaṃ svapne 'pi paśyanti BhStc_13c",
    "GR13bhnstciu:235": " nāthāya sthāṇave tubhyaṃ BhStc_23c",
    "GR13bhnstciu:236": " nānantyāya tvayārpyate BhStc_31d",
    "GR13bhnstciu:237": " nābhaviṣyad idaṃ yadi BhStc_40b",
    "GR13bhnstciu:238": " nāmāpi dhyāyatāṃ dhyānaiḥ BhStc_19c",
    "GR13bhnstciu:239": " nimeṣam api yady ekaṃ BhStc_114a",
    "GR13bhnstciu:240": " nirāvaraṇanirdvandva- BhStc_27a",
    "GR13bhnstciu:241": " nirupādānasambhāram BhStc_9a",
    "GR13bhnstciu:242": " nirguṇo 'pi guṇajñānāṃ BhStc_28a",
    "GR13bhnstciu:243": " nirdvandve nirupādhau ca BhStc_54a",
    "GR13bhnstciu:244": " nirbhayaṃ yad yad ānanda- BhStc_89a",
    "GR13bhnstciu:245": " nirmalīkaraṇe nātha BhStc_88c",
    "GR13bhnstciu:246": " nirvikalpo jayaty ajaḥ BhStc_112d",
    "GR13bhnstciu:247": " nirvināyakaśaktaye BhStc_100d",
    "GR13bhnstciu:248": " nilaye duḥkhayādasām BhStc_26b",
    "GR13bhnstciu:249": " niścayaḥ punar eṣo 'tra BhStc_118a",
    "GR13bhnstciu:250": " niścalajñānasampadām BhStc_27b",
    "GR13bhnstciu:251": " niśchadmā ced bhaved eṣā BhStc_108c",
    "GR13bhnstciu:252": " niṣkampam amṛtahradam BhStc_95b",
    "GR13bhnstciu:253": " niṣkāmāyāpi kāmānām BhStc_63a",
    "GR13bhnstciu:254": " niṣkāmo 'pi prakṛtyā yaḥ BhStc_28c",
    "GR13bhnstciu:255": " niḥśeṣakleśahānasya BhStc_101a",
    "GR13bhnstciu:256": " niḥśeṣaprārthanīyārtha- BhStc_99a",
    "GR13bhnstciu:257": " niḥśeṣātiśayātmanaḥ BhStc_53b",
    "GR13bhnstciu:258": " niḥspṛhāya kapardine BhStc_42d",
    "GR13bhnstciu:259": " naiḥspṛhyasya parāṃ koṭiṃ BhStc_73c",
    "GR13bhnstciu:260": " padaṃ citte tadā śambho BhStc_114c",
    "GR13bhnstciu:261": " padaṃ dehy ehi me deva BhStc_89c",
    "GR13bhnstciu:262": " paramānandadāyinam BhStc_13d",
    "GR13bhnstciu:263": " paramānandasampadā BhStc_2b",
    "GR13bhnstciu:264": " paramārthaphalaṃ nātha BhStc_96c",
    "GR13bhnstciu:265": " paramārthaikabhāvāya BhStc_11c",
    "GR13bhnstciu:266": " parameśvarahaṃsasya BhStc_8c",
    "GR13bhnstciu:267": " paraḥ ko 'pi maheśvaraḥ BhStc_90d",
    "GR13bhnstciu:268": " paraḥ pratinidhis tava BhStc_53d",
    "GR13bhnstciu:269": " paripūrṇaṃ prayacchasi BhStc_96d",
    "GR13bhnstciu:270": " pareṇa jyotiṣābhitaḥ BhStc_12b",
    "GR13bhnstciu:271": " paryantaḥ sarvakarmaṇām BhStc_49b",
    "GR13bhnstciu:272": " paśyantyā dṛśyamānayā BhStc_1b",
    "GR13bhnstciu:273": " punarbhavabhayoccheda- BhStc_14c",
    "GR13bhnstciu:274": " puruṣārthaprasādhakaḥ BhStc_25b",
    "GR13bhnstciu:275": " puṣpitā pātv asau bhavaḥ BhStc_34d",
    "GR13bhnstciu:276": " 'py aṇave mahate punaḥ BhStc_7b",
    "GR13bhnstciu:277": " praṇantavyaḥ praṇāmo 'pi BhStc_25c",
    "GR13bhnstciu:278": " praṇavordhvārdhamātrāto BhStc_7a",
    "GR13bhnstciu:279": " prabhuśaktir abhaṅgurā BhStc_66b",
    "GR13bhnstciu:280": " prabho bhavata eveha BhStc_66a",
    "GR13bhnstciu:281": " prasanne manasi svāmin BhStc_117a",
    "GR13bhnstciu:282": " prasaradbindunādāya BhStc_3a",
    "GR13bhnstciu:283": " prasādaḥ kriyatāṃ mama BhStc_41d",
    "GR13bhnstciu:284": " prasādaḥ kriyatāṃ mayi BhStc_24d",
    "GR13bhnstciu:285": " prasādo manasaḥ svāmin BhStc_118c",
    "GR13bhnstciu:286": " prastāvya hara saṃhartuṃ BhStc_59c",
    "GR13bhnstciu:287": " prāptau cānandavṛndāya BhStc_36c",
    "GR13bhnstciu:288": " prāyeṇārthakriyākaram BhStc_79b",
    "GR13bhnstciu:289": " prārthaye nātha sarvathā BhStc_99d",
    "GR13bhnstciu:290": " pluṣṭāśeṣabhavendhanā BhStc_58b",
    "GR13bhnstciu:291": " phalaṃ tad aja jātaṃ yat BhStc_109c",
    "GR13bhnstciu:292": " phalaṃ lokatrayātmakam BhStc_32b",
    "GR13bhnstciu:293": " baliṃ yāmo bhavāya te BhStc_11d",
    "GR13bhnstciu:294": " bibhratāṃ tvaṃ prabho prabhuḥ BhStc_73d",
    "GR13bhnstciu:295": " brahmaṇo 'pi bhavān brahma BhStc_74a",
    "GR13bhnstciu:296": " brahmāṇḍagarbhiṇīṃ vyoma- BhStc_8a",
    "GR13bhnstciu:297": " brahmāṇḍād api nairguṇya- BhStc_7c",
    "GR13bhnstciu:298": " brahmādayo 'pi tad yasya BhStc_33a",
    "GR13bhnstciu:299": " brahmādibhir api prabho BhStc_39d",
    "GR13bhnstciu:300": " brūyāṃ vā sādhv asādhu vā BhStc_103b",
    "GR13bhnstciu:301": " bhakticintāmaṇiṃ śārvaṃ BhStc_26c",
    "GR13bhnstciu:302": " bhaktilakṣmyālayaṃ śambho BhStc_120c",
    "GR13bhnstciu:303": " bhagavan bhava bhāvatkaṃ BhStc_14a",
    "GR13bhnstciu:304": " bhaṭṭanārāyaṇo vyadhāt BhStc_120d",
    "GR13bhnstciu:305": " bhavajjñānāmbudher madhyam BhStc_95c",
    "GR13bhnstciu:306": " bhava tattatpradānena BhStc_24c",
    "GR13bhnstciu:307": " bhavatas tv īśa nāmāpi BhStc_79c",
    "GR13bhnstciu:308": " bhavate bhava te 'vate BhStc_30b",
    "GR13bhnstciu:309": " bhavadbhaktis tu jananī BhStc_116c",
    "GR13bhnstciu:310": " bhavadbhāvaikabhūṣaṇam BhStc_119d",
    "GR13bhnstciu:311": " bhavān iva bhavān eva BhStc_83a",
    "GR13bhnstciu:312": " bhavet sālambanā tasya BhStc_115c",
    "GR13bhnstciu:313": " bhaved yadi paraṃ bhava BhStc_83b",
    "GR13bhnstciu:314": " bhārūpaḥ satyasaṃkalpas BhStc_85a",
    "GR13bhnstciu:315": " bhāvaṃ bhāvayituṃ ruciḥ BhStc_14b",
    "GR13bhnstciu:316": " bhinneṣv api na bhinnaṃ yac BhStc_6a",
    "GR13bhnstciu:317": " bhuktvā bhogān bhavabhrāntiṃ BhStc_102a",
    "GR13bhnstciu:318": " bhūribhūtisitāṅgāya BhStc_57c",
    "GR13bhnstciu:319": " bhedayoge 'py abhedine BhStc_11b",
    "GR13bhnstciu:320": " bhoktre bhava namo 'stu te BhStc_63d",
    "GR13bhnstciu:321": " bhramo na labhyate yasya BhStc_35a",
    "GR13bhnstciu:322": " bhrāntāntaḥkaraṇair api BhStc_35b",
    "GR13bhnstciu:323": " bhrāntim udbhāvya bhindate BhStc_71b",
    "GR13bhnstciu:324": " magnair bhīme bhavāmbhodhau BhStc_26a",
    "GR13bhnstciu:325": " matayas taṃ śivaṃ stumaḥ BhStc_65d",
    "GR13bhnstciu:326": " manasā parameśvara BhStc_37b",
    "GR13bhnstciu:327": " manaskatvena yā sthitiḥ BhStc_15d",
    "GR13bhnstciu:328": " manorathagato 'pi vā BhStc_96b",
    "GR13bhnstciu:329": " manorathaphalapradam BhStc_120b",
    "GR13bhnstciu:330": " manorathaḥ sthito yasya BhStc_105c",
    "GR13bhnstciu:331": " mantrayāmo 'mbikāpate BhStc_4b",
    "GR13bhnstciu:332": " mantrasāmarthyam āśritāḥ BhStc_20b",
    "GR13bhnstciu:333": " mantrī tvattaḥ kuto 'paraḥ BhStc_84b",
    "GR13bhnstciu:334": " mantro 'si mantraṇīyo 'si BhStc_84a",
    "GR13bhnstciu:335": " manye nyastapadaḥ so 'pi BhStc_105a",
    "GR13bhnstciu:336": " manye vandhyātmajānujaḥ BhStc_104d",
    "GR13bhnstciu:337": " mama tvayy eva yaḥ sthitaḥ BhStc_47b",
    "GR13bhnstciu:338": " mayam ekaṃ yad avyayam BhStc_89b",
    "GR13bhnstciu:339": " malatailāktasaṃsāra- BhStc_113a",
    "GR13bhnstciu:340": " malasaṃkṣālanakṣamāḥ BhStc_76b",
    "GR13bhnstciu:341": " mastakanyastagaṅgāya BhStc_91c",
    "GR13bhnstciu:342": " mahatīyam aho māyā BhStc_44a",
    "GR13bhnstciu:343": " mahatsv apy arthakṛcchreṣu BhStc_65a",
    "GR13bhnstciu:344": " mahān asmīti bhāvanā BhStc_115b",
    "GR13bhnstciu:345": " mahāhaṃsāya śambhave BhStc_57d",
    "GR13bhnstciu:346": " mahimā parameśvaraḥ BhStc_1d",
    "GR13bhnstciu:347": " mānasaikanivāsine BhStc_57b",
    "GR13bhnstciu:348": " māmi nātmani kiṃ mudā BhStc_37d",
    "GR13bhnstciu:349": " māyayāmeyayā tava BhStc_54d",
    "GR13bhnstciu:350": " māyājalodarāt samyag BhStc_10a",
    "GR13bhnstciu:351": " māyāmayamalāndhasya BhStc_88a",
    "GR13bhnstciu:352": " māyām ekanimeṣeṇa BhStc_72c",
    "GR13bhnstciu:353": " mālālayamayātmane BhStc_23b",
    "GR13bhnstciu:354": " mukuṭotkaṭamastakāḥ BhStc_94b",
    "GR13bhnstciu:355": " muktaye 'dhikriyeta kaḥ BhStc_48d",
    "GR13bhnstciu:356": " mukhyadhyeyasya dhūrjaṭeḥ BhStc_19b",
    "GR13bhnstciu:357": " mugdho lokaḥ ślathāyate BhStc_44d",
    "GR13bhnstciu:358": " muṣṇaṃs tāṃ pātu naḥ śivaḥ BhStc_72d",
    "GR13bhnstciu:359": " muhur muhur aviśrāntas BhStc_112a",
    "GR13bhnstciu:360": " muhur muhur jagaccitrasy- BhStc_80a",
    "GR13bhnstciu:361": " mūlaprakṛtim īśvaram BhStc_64b",
    "GR13bhnstciu:362": " mokṣaparyantasiddhidam BhStc_79d",
    "GR13bhnstciu:363": " mohaughamalinīkṛtāḥ BhStc_65b",
    "GR13bhnstciu:364": " ya uddhartā bhavārṇavāt BhStc_75d",
    "GR13bhnstciu:365": " yataḥ sampūrṇadharmaṇaḥ BhStc_64d",
    "GR13bhnstciu:366": " yato 'nyatra na so 'si bhoḥ BhStc_67d",
    "GR13bhnstciu:367": " yatprasatteḥ phalaṃ tava BhStc_98b",
    "GR13bhnstciu:368": " yathāyuktārthakāriṇe BhStc_91d",
    "GR13bhnstciu:369": " yadicchayā pratāyete BhStc_66c",
    "GR13bhnstciu:370": " yadīya iha dhīmatām BhStc_25d",
    "GR13bhnstciu:371": " yannāmnāpi mahātmānaḥ BhStc_82c",
    "GR13bhnstciu:372": " yayānantaphalāṃ bhaktiṃ BhStc_43c",
    "GR13bhnstciu:373": " yas tvadālambanaḥ prabho BhStc_115d",
    "GR13bhnstciu:374": " yasya vedātmikājñeyam BhStc_69c",
    "GR13bhnstciu:375": " yasyāḥ prāpyeta paryanta- BhStc_72a",
    "GR13bhnstciu:376": " yaḥ sphītaḥ śrīdayābodha- BhStc_2a",
    "GR13bhnstciu:377": " yā yā dik tatra na kvāsi BhStc_56a",
    "GR13bhnstciu:378": " yāvajjīvaṃ jagannātha BhStc_15a",
    "GR13bhnstciu:379": " yāvad uttaram āsvāda- BhStc_50a",
    "GR13bhnstciu:380": " ye tvāṃ jānanti dhūrjaṭe BhStc_27d",
    "GR13bhnstciu:381": " ye vimuktānyasaṃkatham BhStc_62b",
    "GR13bhnstciu:382": " yo 'sau sa dayito 'smākaṃ BhStc_87c",
    "GR13bhnstciu:383": " rasaḥ srotaḥsahasreṇa BhStc_22c",
    "GR13bhnstciu:384": " rāgo 'py astu jagannātha BhStc_47a",
    "GR13bhnstciu:385": " rūpaṃ tat pārameśvaram BhStc_6d",
    "GR13bhnstciu:386": " lapsyase nātha kathyatām BhStc_56d",
    "GR13bhnstciu:387": " labdhuṃ dhāma namāmi tam BhStc_33d",
    "GR13bhnstciu:388": " labhyate bhava kutrāṃśe BhStc_53c",
    "GR13bhnstciu:389": " lipseran nopakāraṃ ke BhStc_64c",
    "GR13bhnstciu:390": " lobhāyāpi namas tasmai BhStc_47c",
    "GR13bhnstciu:391": " vacaś cetaś ca kāryaṃ ca BhStc_119a",
    "GR13bhnstciu:392": " vapanti tvayy amī prabho BhStc_43d",
    "GR13bhnstciu:393": " vayaṃ vañcyāmahe 'dyāpi BhStc_54c",
    "GR13bhnstciu:394": " vastutattvaṃ padārthānāṃ BhStc_79a",
    "GR13bhnstciu:395": " vahed brāhmīṃ dhuraṃ na kaḥ BhStc_93d",
    "GR13bhnstciu:396": " vāṅmanaḥkāyakarmāṇi BhStc_17a",
    "GR13bhnstciu:397": " vācya eṣāṃ tvam eveti BhStc_40a",
    "GR13bhnstciu:398": " vāsanāvartidāhinā BhStc_113b",
    "GR13bhnstciu:399": " vidyoddyotitamāhātmyaḥ BhStc_2c",
    "GR13bhnstciu:400": " vidhir ādis tathānto 'si BhStc_70a",
    "GR13bhnstciu:401": " viniyojya tvayi prabho BhStc_17b",
    "GR13bhnstciu:402": " vibhave śambhave namaḥ BhStc_5d",
    "GR13bhnstciu:403": " viyanmāyāsvarūpāya BhStc_5c",
    "GR13bhnstciu:404": " viśeṣaḥ kair manorathaiḥ BhStc_72b",
    "GR13bhnstciu:405": " viśrāmyan bhava bhāvatka- BhStc_38c",
    "GR13bhnstciu:406": " viśvaṅ no dhṛtavān asi BhStc_4d",
    "GR13bhnstciu:407": " viśvam astu svatantras tu BhStc_45c",
    "GR13bhnstciu:408": " viśvasya parameśvara BhStc_70b",
    "GR13bhnstciu:409": " visṛṣṭānekasadbīja- BhStc_59a",
    "GR13bhnstciu:410": " vedāgamamayātmane BhStc_16b",
    "GR13bhnstciu:411": " vedyaṃ kiṃ karma vā nātha BhStc_31c",
    "GR13bhnstciu:412": " vairāgyasya gatiṃ gurvīṃ BhStc_73a",
    "GR13bhnstciu:413": " vyatītaguṇayogasya BhStc_19a",
    "GR13bhnstciu:414": " vyayaiśvaryaikaśāline BhStc_60d",
    "GR13bhnstciu:415": " vyāpinaḥ sarvatogateḥ BhStc_8b",
    "GR13bhnstciu:416": " śaktaḥ kubjayituṃ tṛṇam BhStc_93b",
    "GR13bhnstciu:417": " śaktir yā te tayā nātha BhStc_80c",
    "GR13bhnstciu:418": " śaktiṃ haṃsīm iva stumaḥ BhStc_8d",
    "GR13bhnstciu:419": " śambhave śaṃ bhave 'bhave BhStc_30d",
    "GR13bhnstciu:420": " śambhau bhaktimataḥ param BhStc_102d",
    "GR13bhnstciu:421": " śarīraṃ mama yat prabho BhStc_119b",
    "GR13bhnstciu:422": " śaṃkarakṣīrasindhave BhStc_3d",
    "GR13bhnstciu:423": " śākhāsahasravistīrṇa- BhStc_16a",
    "GR13bhnstciu:424": " śālini śūline namaḥ BhStc_18d",
    "GR13bhnstciu:425": " śāśvatīṃ kuruṣe yad vā BhStc_78c",
    "GR13bhnstciu:426": " śāṃkarī śaraṇaṃ mama BhStc_52d",
    "GR13bhnstciu:427": " śivajñānaṃ svato dugdhaṃ BhStc_10c",
    "GR13bhnstciu:428": " śukrataḥ parataḥ param BhStc_69b",
    "GR13bhnstciu:429": " śuddhāmṛtamayātmane BhStc_3b",
    "GR13bhnstciu:430": " śubhasyaiveśa kevalam BhStc_116d",
    "GR13bhnstciu:431": " śubhāśubhasya sarvasya BhStc_116a",
    "GR13bhnstciu:432": " śaivayogabalākṛṣṭā BhStc_76c",
    "GR13bhnstciu:433": " śrīratnāmṛtalābhāya BhStc_29a",
    "GR13bhnstciu:434": " śreyasā śreya evaitad BhStc_110a",
    "GR13bhnstciu:435": " ślāghyās te śāmbhavīṃ bhūtim BhStc_20c",
    "GR13bhnstciu:436": " ṣaṭpramāṇīpariccheda- BhStc_11a",
    "GR13bhnstciu:437": " sa geyaḥ parameśvaraḥ BhStc_82b",
    "GR13bhnstciu:438": " sa jayaty aparājitaḥ BhStc_2d",
    "GR13bhnstciu:439": " sadaiśvaryaṃ bhavakṣayaḥ BhStc_55b",
    "GR13bhnstciu:440": " sa dhiyaḥ satpathe prabhuḥ BhStc_77d",
    "GR13bhnstciu:441": " sa bindur yasya taṃ vande BhStc_61c",
    "GR13bhnstciu:442": " sa mahyaṃ dehi taṃ mantraṃ BhStc_84c",
    "GR13bhnstciu:443": " sampūrṇāmṛtasūtaye BhStc_5b",
    "GR13bhnstciu:444": " sarvajñaḥ sarvakṛt sarvam BhStc_31a",
    "GR13bhnstciu:445": " sarvatrātrāsmi nirvṛtaḥ BhStc_103d",
    "GR13bhnstciu:446": " sarvatrānadhyavasyataḥ BhStc_52b",
    "GR13bhnstciu:447": " sarvavibhramanirmoka- BhStc_95a",
    "GR13bhnstciu:448": " sarvasthāne mameśa tvaṃ BhStc_75c",
    "GR13bhnstciu:449": " sarvasya spṛhaṇīyāya BhStc_42c",
    "GR13bhnstciu:450": " sarvaḥ kālo bhavanmayaḥ BhStc_56b",
    "GR13bhnstciu:451": " sarvānugrāhikā śaktiḥ BhStc_52c",
    "GR13bhnstciu:452": " sahasraguṇavistaraḥ BhStc_50b",
    "GR13bhnstciu:453": " saṃsārīti kim īśaiṣa BhStc_85c",
    "GR13bhnstciu:454": " saṃhṛtasparśayogāya BhStc_5a",
    "GR13bhnstciu:455": " sā gatir gamyase yayā BhStc_109b",
    "GR13bhnstciu:456": " sārthasiddhinidhānataḥ BhStc_99b",
    "GR13bhnstciu:457": " sā siddhis tat paraṃ padam BhStc_118d",
    "GR13bhnstciu:458": " sukṛtasyāpi saukṛtyaṃ BhStc_67c",
    "GR13bhnstciu:459": " sugirā cittahāriṇyā BhStc_1a",
    "GR13bhnstciu:460": " suhṛdbhiḥ svāmibhis tathā BhStc_75b",
    "GR13bhnstciu:461": " sūkṣmo 'pi cet trilokīyaṃ BhStc_39a",
    "GR13bhnstciu:462": " sūryācandramasāv imau BhStc_34b",
    "GR13bhnstciu:463": " sevāśaṃsāsudhārasaḥ BhStc_111b",
    "GR13bhnstciu:464": " seviṣye śivam ity ayam BhStc_105d",
    "GR13bhnstciu:465": " somāya svāmine namaḥ BhStc_51d",
    "GR13bhnstciu:466": " stavacintāmaṇiṃ bhūri- BhStc_120a",
    "GR13bhnstciu:467": " stumas tribhuvanārambha- BhStc_64a",
    "GR13bhnstciu:468": " stumas tvām ṛgyajuḥsāmnāṃ BhStc_69a",
    "GR13bhnstciu:469": " sthityutpattilayair loka- BhStc_106a",
    "GR13bhnstciu:470": " sthūlo 'tha kiṃ sudarśo na BhStc_39c",
    "GR13bhnstciu:471": " smayena tvanmayo 'smīti BhStc_37c",
    "GR13bhnstciu:472": " smṛte yasmin prasīdanti BhStc_65c",
    "GR13bhnstciu:473": " smṛtyā nutyā ca dadate BhStc_30c",
    "GR13bhnstciu:474": " svatantraṃ tantram īkṣase BhStc_106d",
    "GR13bhnstciu:475": " svatantrāyāsvatantrāya BhStc_60c",
    "GR13bhnstciu:476": " svapnaḥ so 'pi kutas tvayi BhStc_85d",
    "GR13bhnstciu:477": " svayaṃ kartā bhavān api BhStc_116b",
    "GR13bhnstciu:478": " svaśaktivyūhasaṃvyūḍha- BhStc_83c",
    "GR13bhnstciu:479": " svāmin so 'sīti niścitya BhStc_101c",
    "GR13bhnstciu:480": " harapraṇatimāṇikya- BhStc_94a",
    "GR13bhnstciu:481": " hitvā lapsye paraṃ padam BhStc_102b",
    "GR13bhnstciu:482": " hṛtoddhatatamastāntiḥ BhStc_58a",
    "GR13bhnstciu:483": " hetuḥ ka iti saṃśaye BhStc_101b"
}