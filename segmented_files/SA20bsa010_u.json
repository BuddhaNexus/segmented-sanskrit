{
  "SA20bsa010_u:0": "Āryamaitreyapraṇītā Madhyāntavibhāgakārikā (Mvk)",
  "SA20bsa010_u:1": "Lakṣaṇaparicchedaḥ prathamaḥ",
  "SA20bsa010_u:2": "Mvk_1.1 lakṣaṇaṃ hyāvṛtistattvaṃ pratipakṣasya bhāvanā /",
  "SA20bsa010_u:3": "tatrāvasthā phalaprāptiryānānuttaryameva ca //",
  "SA20bsa010_u:4": "Mvk_1.2 abhūtaparikalpo 'sti dvayaṃ tatra na vidyate /",
  "SA20bsa010_u:5": "śūnyatā vidyate tvatra tasyāmapi sa vidyate //",
  "SA20bsa010_u:6": "Mvk_1.3 na śūnyaṃ nāpi cāśūnyaṃ tasmāt sarvaṃ vidhīyate /",
  "SA20bsa010_u:7": "sattvādasattvāt sattvācca madhyamā pratipacca sā //",
  "SA20bsa010_u:8": "Mvk_1.4 arthasattvātmavijñaptipratibhāsaṃ prajāyate /",
  "SA20bsa010_u:9": "vijñānaṃ nāsti cāsyārthastadabhāvāttadapyasat //",
  "SA20bsa010_u:10": "Mvk_1.5 abhūtaparikalpatvaṃ siddhamasya bhavatyataḥ /",
  "SA20bsa010_u:11": "na tathā sarvathābhāvāt tatkṣayānmuktiriṣyate //",
  "SA20bsa010_u:12": "Mvk_1.6 kalpitaḥ paratantraśca pariniṣpanna eva ca /",
  "SA20bsa010_u:13": "arthādabhūtakalpācca dvayābhāvācca deśitaḥ //",
  "SA20bsa010_u:14": "Mvk_1.7 upalabdhiṃ samāśritya nopalabdhiḥ prajāyate /",
  "SA20bsa010_u:15": "nopalabdhiṃ samāśritya nopalabdhiḥ prajāyate //",
  "SA20bsa010_u:16": "Mvk_1.8 upalabdhestataḥ siddhā nopalabdhisvabhāvatā /",
  "SA20bsa010_u:17": "tasmācca samatā jñeyā nopalambhopalambhayoḥ //",
  "SA20bsa010_u:18": "Mvk_1.9 abhūtaparikalpaśca cittacaittāstridhātukāḥ /",
  "SA20bsa010_u:19": "tatrārthadṛṣṭirvijñānaṃ tadviśeṣe tu caitasāḥ //",
  "SA20bsa010_u:20": "Mvk_1.10 evaṃ pratyayavijñānaṃ dvitīyaṃ caupabhogikam /",
  "SA20bsa010_u:21": "upabhogaparicchedaprerakāstatra caitasāḥ //",
  "SA20bsa010_u:22": "Mvk_1.11 chādanādropaṇāccaiva nayanātsaṃparigrahāt /",
  "SA20bsa010_u:23": "pūraṇāt triparicchedādupabhogācca karṣaṇāt //",
  "SA20bsa010_u:24": "Mvk_1.12 nibandhanādābhimakhyād duḥkhanāt kliśyate jagat /",
  "SA20bsa010_u:25": "tredhā dvedhā ca saṃkleśaḥ saptadhābhūtakalpanāt //",
  "SA20bsa010_u:26": "Mvk_1.13 lakṣaṇaṃ cātha paryāyastadartho bheda eva ca /",
  "SA20bsa010_u:27": "sādhanañceti vijñeyaṃ śūnyatāyāḥ samāsataḥ //",
  "SA20bsa010_u:28": "Mvk_1.14 dvayābhāvo hyabhāvasya bhāvaḥ śūnyasya lakṣaṇam /",
  "SA20bsa010_u:29": "na bhāvo nāpi cābhāvao na pṛthaktvaikalakṣaṇam //",
  "SA20bsa010_u:30": "Mvk_1.15 tathatā bhūtakoṭiścānimittaṃ paramārthatā /",
  "SA20bsa010_u:31": "dharmadhātuśca paryāyāḥ śūnyatāyāḥ samāsataḥ //",
  "SA20bsa010_u:32": "Mvk_1.16 ananyathāviparyāsatannirodhāryagocaraiḥ /",
  "SA20bsa010_u:33": "hetutvāccāryadharmaṇāṃ paryāyārtho yathākramam //",
  "SA20bsa010_u:34": "Mvk_1.17 saṃkliṣṭā ca viśuddhā ca samalā nirmalā ca sā /",
  "SA20bsa010_u:35": "abdhātukanakākāśaśuddhivacchuddhiriṣyate //",
  "SA20bsa010_u:36": "Mvk_1.18 bhoktṛbhojanataddhehapratiṣṭhāvastuśūnyatā /",
  "SA20bsa010_u:37": "tacca yena yathā dṛṣṭaṃ yadarthaṃ tasya śūnyatā //",
  "SA20bsa010_u:38": "Mvk_1.19 śubhadvayasya prāptyartha sadā sattvahitāya ca /",
  "SA20bsa010_u:39": "saṃsārātyajanārthaṃ ca kuśalasyākṣayāya ca //",
  "SA20bsa010_u:40": "Mvk_1.20 gotrasya ca viśuddhayarthaṃ lakṣaṇavyañjanāptaye /",
  "SA20bsa010_u:41": "śuddhaye bu[ddha]dharmāṇāṃ bodhisattvaḥ prapadyate //",
  "SA20bsa010_u:42": "Mvk_1.21 pudgalasyātha dharmāṇāmabhāvaḥ śūnyatātra hi /",
  "SA20bsa010_u:43": "tadabhāvasya sadbhāvastasmin sā śūnyatāparā //",
  "SA20bsa010_u:44": "Mvk_1.22 saṃkliṣṭā cedbhavennāsau muktāssyuḥ sarvadehinaḥ /",
  "SA20bsa010_u:45": "viśuddhā cedbhavennāsau vyāyāmo niṣphalo bhavet //",
  "SA20bsa010_u:46": "Mvk_1.23 na kliṣṭā nāpi va 'ākliṣṭā śuddhāśuddhā na caiva sā /",
  "SA20bsa010_u:47": "prabhāsvaratvāccittasya kleśasyāgantukatvataḥ //",
  "SA20bsa010_u:48": "// iti lakṣaṇaparicchedaḥ prathamaḥ //",
  "SA20bsa010_u:49": "Āvaraṇaparicchedo dvitīyaḥ",
  "SA20bsa010_u:50": "Mvk_2.1 vyāpi prādeśikodriktasamādānavivarjanam /",
  "SA20bsa010_u:51": "dvayāvaraṇamākhyātaṃ navadhā kleśalakṣaṇam //",
  "SA20bsa010_u:52": "Mvk_2.2 saṃyojanānyāvaraṇamudvegasamupekṣayoḥ /",
  "SA20bsa010_u:53": "tattvadṛṣṭeśca satkāyadṛṣṭestadvastuno 'pi ca //",
  "SA20bsa010_u:54": "Mvk_2.3 nirodhamārgaratneṣu lābhasatkāra eva ca /",
  "SA20bsa010_u:55": "saṅkleśasya parijñāne śubhādau daśadhāparam //",
  "SA20bsa010_u:56": "Mvk_2.4 aprayogo 'nāyatane 'yogavihitaśca yaḥ /",
  "SA20bsa010_u:57": "notpattiramanaskāraḥ sambhārasyāprapūrṇatā //",
  "SA20bsa010_u:58": "Mvk_2.5 gotramitrasya vaidhuryaṃ cittasya parikheditā /",
  "SA20bsa010_u:59": "pratipatteśca vaidhuryaṃ kuduṣṭajanavāsatā //",
  "SA20bsa010_u:60": "Mvk_2.6 dauṣṭhulyamavaśiṣṭatvaṃ trayāt prajñāvipakvatā /",
  "SA20bsa010_u:61": "prakṛtyā caiva dauṣṭhulyaṃ kausīdyaṃ ca pramāditā //",
  "SA20bsa010_u:62": "Mvk_2.7 saktirbhave ca bhoge ca līnacittatvameva ca /",
  "SA20bsa010_u:63": "aśraddhānadhimuktiśca yathārutavicāraṇā //",
  "SA20bsa010_u:64": "Mvk_2.8 saddharme 'gauravaṃ lābhe gurutākṛpatā tathā /",
  "SA20bsa010_u:65": "śrutavyasanamalpatvaṃ samādhyaparikarmitā //",
  "SA20bsa010_u:66": "Mvk_2.9 śubhaṃ bodhiḥ samādānaṃ dhīmattvābhrāntyanāvṛtī /",
  "SA20bsa010_u:67": "nṛtyatrāso 'matsaritvaṃ vaśitvañca śubhādayaḥ //",
  "SA20bsa010_u:68": "Mvk_2.10 trīṇi trīṇi ca eteṣāṃ jñeyānyāvaraṇāni hi /",
  "SA20bsa010_u:69": "pakṣyapāramitābhūmiṣvanyadāvaraṇaṃ punaḥ //",
  "SA20bsa010_u:70": "Mvk_2.11 vastvakauśalakausīdyaṃ samādherdvayahīnatā /",
  "SA20bsa010_u:71": "aropaṇātha daurbalyaṃ dṛṣṭidauṣṭhulyaduṣṭatā //",
  "SA20bsa010_u:72": "Mvk_2.12 aiśvaryasyātha sugateḥ sattvātyāgasya cāvṛtiḥ /",
  "SA20bsa010_u:73": "hānivṛddhyośca doṣāṇāṃ guṇānāmavatāraṇe //",
  "SA20bsa010_u:74": "Mvk_2.13 vimocane 'kṣayatve ca nairantarye śubhasya ca /",
  "SA20bsa010_u:75": "niyatīkaraṇe dharmasambhogaparipācane //",
  "SA20bsa010_u:76": "Mvk_2.14 sarvatragārthe agrārthe niṣyandāgrārtha eva ca /",
  "SA20bsa010_u:77": "niṣparigrahatārtha ca santānābheda eva ca //",
  "SA20bsa010_u:78": "Mvk_2.15 nissaṅkleśaviśuddhyarthe 'nānātvārtha eva ca /",
  "SA20bsa010_u:79": "ahīnānadhikārthe ca caturdhāvaśitāśraye //",
  "SA20bsa010_u:80": "Mvk_2.16 dharmadhātāvavidyeyamakliṣṭā daśadhāvṛtiḥ /",
  "SA20bsa010_u:81": "daśabhūmivipakṣeṇa pratipakṣāstu bhūmayaḥ //",
  "SA20bsa010_u:82": "Mvk_2.17 kleśāvaraṇamākhyātaṃ jñeyāvaraṇameva ca /",
  "SA20bsa010_u:83": "sarvāṇyāvaraṇānīha yatkṣayānmuktiriṣyate //",
  "SA20bsa010_u:84": "// ityāvaraṇaparicchedo dvitīyaḥ //",
  "SA20bsa010_u:85": "Tattvaparicchedastṛtīyaḥ",
  "SA20bsa010_u:86": "Mvk_3.1 mūlalakṣaṇatattvamaviparyāsalakṣaṇam /",
  "SA20bsa010_u:87": "phalahetumayaṃ tattvaṃ sūkṣmaudārikameva ca //",
  "SA20bsa010_u:88": "Mvk_3.2 prasiddhaṃ śuddhiviṣayaṃ saṅgrāhyaṃ bhedalakṣaṇam /",
  "SA20bsa010_u:89": "kauśalyatattvaṃ daśadhā ātmadṛṣṭivipakṣataḥ //",
  "SA20bsa010_u:90": "Mvk_3.3 svabhāvastrividho 'sacca nityaṃ saccāpyatattvataḥ /",
  "SA20bsa010_u:91": "sadasattattvataśceti svabhāvatrayamiṣyate //",
  "SA20bsa010_u:92": "Mvk_3.4 samāropāpavādasya dharmapudgalayoriha /",
  "SA20bsa010_u:93": "grāhyagrāhakayoścāpi bhāvābhāve ca darśanam //",
  "SA20bsa010_u:94": "Mvk_3.5 yajjñānānna pravarteta taddhi tattvasya lakṣaṇam /",
  "SA20bsa010_u:95": "asadartho hyanityārtha utpādadavyayalakṣaṇaḥ //",
  "SA20bsa010_u:96": "Mvk_3.6 samalāmalabhāvena mūlatattve yathākramam /",
  "SA20bsa010_u:97": "duḥkhamādānalakṣmākhyaṃ sambandhenāparaṃ matam //",
  "SA20bsa010_u:98": "Mvk_3.7 abhāvaścāpyatadbhāvaḥ prakṛtiḥ śūnyatā matā /",
  "SA20bsa010_u:99": "alakṣaṇañca nairātmyaṃ tadvilakṣaṇameva ca //",
  "SA20bsa010_u:100": "Mvk_3.8 svalakṣaṇañca nirdiṣṭaṃ duḥkhasatyamato matam /",
  "SA20bsa010_u:101": "vāsanātha samutthānamavisaṃyoga eva ca //",
  "SA20bsa010_u:102": "Mvk_3.9 svabhāvadvayanotpattirmalaśāntidvayaṃ matam /",
  "SA20bsa010_u:103": "parijñāyāṃ prahāṇe ca prāptisākṣātkṛtāvayam //",
  "SA20bsa010_u:104": "Mvk_3.10 mārgasatyaṃ samākhyātaṃ prajñaptipratipattitaḥ /",
  "SA20bsa010_u:105": "tathodbhāvanayaudāraṃ paramārthantu ekataḥ //",
  "SA20bsa010_u:106": "Mvk_3.11 arthaprāptiprapattyā hi paramārthastridhā mataḥ /",
  "SA20bsa010_u:107": "nirvikārāviparyāsapariniṣpattito dvayam //",
  "SA20bsa010_u:108": "Mvk_3.12 lokaprasiddhamekasmāt trayād yuktiprasiddhakam /",
  "SA20bsa010_u:109": "viśuddhagocaraṃ dvedhā ekasmādeva kīrtitam //",
  "SA20bsa010_u:110": "Mvk_3.13 nimittasya vikalpasya nāmnaśca dvayasaṅgrahaḥ /",
  "SA20bsa010_u:111": "samyagjñānasatattvasya ekenaiva ca saṅgrahaḥ //",
  "SA20bsa010_u:112": "Mvk_3.14 pravṛttitattvaṃ dvividhaṃ sanniveśakupannatā /",
  "SA20bsa010_u:113": "ekaṃ lakṣaṇavijñaptiśuddhisamyakprapannatā //",
  "SA20bsa010_u:114": "Mvk_3.15 ekahetutvabhoktṛtvakartṛtvavaśavartane /",
  "SA20bsa010_u:115": "ādhipatyārthanityatve kleśaśuddhyāśraye 'pi ca //",
  "SA20bsa010_u:116": "Mvk_3.16 yogitvāmuktamuktatve ātmadarśanameṣu hi /",
  "SA20bsa010_u:117": "parikalpavikalpārthadharmatārthena teṣu te //",
  "SA20bsa010_u:118": "Mvk_3.17 anekatvābhisaṅkṣepaparicchedārtha āditaḥ /",
  "SA20bsa010_u:119": "grāhakagrāhyatadgrāhabījārthaścāparo mataḥ //",
  "SA20bsa010_u:120": "Mvk_3.18 veditārthaparicchedabhogāyadvārato 'param /",
  "SA20bsa010_u:121": "punarhetuphalāyāsānāropānapavādataḥ //",
  "SA20bsa010_u:122": "Mvk_3.19 aniṣṭeṣṭaviśuddhīnāṃ samotpattyādhipatyayoḥ /",
  "SA20bsa010_u:123": "samprāptisamudācārapāratantryārthato 'param //",
  "SA20bsa010_u:124": "Mvk_3.20 grahaṇasthānasandhānabhogaśuddhidvayārthataḥ /",
  "SA20bsa010_u:125": "phalahetūpayogārthanopayogāttathāparam //",
  "SA20bsa010_u:126": "Mvk_3.21 vedanāsanimittārthatannimittaprapattitaḥ /",
  "SA20bsa010_u:127": "tacchamapratipakṣārthayogādaparamiṣyate //",
  "SA20bsa010_u:128": "Mvk_3.22 guṇadoṣāvikalpena jñānena parataḥ svayam /",
  "SA20bsa010_u:129": "niryāṇādaparaṃ jñeyaṃ saprajñaptisahetukāt //",
  "SA20bsa010_u:130": "nimittāt praśamāt sārthāt paścimaṃ samudāhṛtam //",
  "SA20bsa010_u:131": "// iti tattvaparicchedastṛtīyaḥ //",
  "SA20bsa010_u:132": "Pratipakṣabhāvanāvasthāphalaparicchedaścaturthaḥ",
  "SA20bsa010_u:133": "Mvk_4.1 dauṣṭhulyāt tarṣahetutvād vastutvādavimohataḥ /",
  "SA20bsa010_u:134": "catussatyāvatārāya smṛtyupasthānabhāvanā //",
  "SA20bsa010_u:135": "Mvk_4.2 parijñāte vipakṣe ca pratipakṣe ca sarvathā /",
  "SA20bsa010_u:136": "tadapāyāya vīryaṃ hi caturdhā sampravartate //",
  "SA20bsa010_u:137": "Mvk_4.3 karmaṇyatā sthitestatra sarvārthānāṃ samṛddhaye /",
  "SA20bsa010_u:138": "pañcadoṣaprahāṇāṣṭasaṃskārāsevanānvayā //",
  "SA20bsa010_u:139": "Mvk_4.4 kausīdyamavavādasya sammoṣo laya uddhavaḥ /",
  "SA20bsa010_u:140": "asaṃskāro 'tha saṃskāraḥ pañca doṣā ime matāḥ //",
  "SA20bsa010_u:141": "Mvk_4.5 āśrayo 'thāśritastasya nimittaṃ phalameva ca /",
  "SA20bsa010_u:142": "ālambane 'sammoṣo layauddhatyānubuddhyanā //",
  "SA20bsa010_u:143": "Mvk_4.6 tadapāyābhisaṃskāraḥ śāntau praśaṭhavāhitā /",
  "SA20bsa010_u:144": "ropite mokṣabhāgīye cchandayogādhipatyataḥ //",
  "SA20bsa010_u:145": "Mvk_4.7 ālambane 'sammoṣāvisāravicayasya ca /",
  "SA20bsa010_u:146": "vipakṣasya hi saṃlekhāt pūrvasya phalamuttaram //",
  "SA20bsa010_u:147": "Mvk_4.8 dvau dvau nirvedhabhāgīyāvindriyāṇi balāni ca /",
  "SA20bsa010_u:148": "āśrayāṅgaṃ svabhāvāṅgaṃ niryāṇāṅgaṃ tṛtīyakam //",
  "SA20bsa010_u:149": "Mvk_4.9 caturthamanuśaṃsāṅgaṃ niḥkleśāṅgaṃ tridhā matam /",
  "SA20bsa010_u:150": "nidānenāśrayeṇeha svabhāvena ca deśitam //",
  "SA20bsa010_u:151": "Mvk_4.10 paricchedo 'tha samprāptiḥ parasambhāvanā tridhā /",
  "SA20bsa010_u:152": "vipakṣapratipakṣaśca mārgasyāṅgaṃ tadaṣṭadhā //",
  "SA20bsa010_u:153": "Mvk_4.11 dṛṣṭau śīle 'tha saṃlekhe paravijñaptiriṣyate /",
  "SA20bsa010_u:154": "kleśopakleśavaibhutvavipakṣapratipakṣatā //",
  "SA20bsa010_u:155": "Mvk_4.12 anukūlā viparyastā sānubandhā viparyayā /",
  "SA20bsa010_u:156": "aviparyastaviparyāsānanubandha ca bhāvanā //",
  "SA20bsa010_u:157": "Mvk_4.13 ālambanamanaskāraprāptitastad viśiṣṭatā /",
  "SA20bsa010_u:158": "hetvavasthāvatārākhyā prayogaphalasaṃjñitā //",
  "SA20bsa010_u:159": "Mvk_4.14 kāryākāryaviśiṣṭā ca uttarānuttarā ca sā /",
  "SA20bsa010_u:160": "adhimuktau praveśe ca niryāṇe vyākṛtāvapi //",
  "SA20bsa010_u:161": "Mvk_4.15 kathikatve 'bhiṣeke ca samprāptāvanuśaṃsane /",
  "SA20bsa010_u:162": "kṛtyānuṣṭhāna uddiṣṭā dharmādhātau tridhā punaḥ //",
  "SA20bsa010_u:163": "Mvk_4.16 aśuddhāśuddhaśuddhā ca viśuddhā ca yathārthataḥ /",
  "SA20bsa010_u:164": "pudgalānāṃ vyavasthānaṃ yathāyogamato matam //",
  "SA20bsa010_u:165": "Mvk_4.17 bhājanatvaṃ vipākākhyaṃ balantasyādhipatyataḥ /",
  "SA20bsa010_u:166": "rucirvṛddhirviśuddhiśca phalametad yathākramam //",
  "SA20bsa010_u:167": "Mvk_4.18 uttarottaramādyañca tadabhyāsat samāptitaḥ /",
  "SA20bsa010_u:168": "ānukūlyād vipakṣācca visaṃyogād viśeṣataḥ //",
  "SA20bsa010_u:169": "uttarānuttaratvācca phalamanyat samāsataḥ /",
  "SA20bsa010_u:170": "// iti pratipakṣabhāvanādiparicchedaścaturthaḥ //",
  "SA20bsa010_u:171": "5 Yānānuttaryaparicchedaḥ pañcamaḥ",
  "SA20bsa010_u:172": "Mvk_5.1 ānuttaryaṃprapattau hi punarālambane matam /",
  "SA20bsa010_u:173": "samudāgama uddiṣṭaṃ pratipattistu ṣaḍ vidhā //",
  "SA20bsa010_u:174": "Mvk_5.2 paramātha manaskāre anudharme 'ntavarjane /",
  "SA20bsa010_u:175": "viśiṣṭā cāviśiṣṭā ca paramā dvādaśātmikā //",
  "SA20bsa010_u:176": "Mvk_5.3 audāryamāyatatvañca adhikaro 'kṣayātmatā /",
  "SA20bsa010_u:177": "nairantaryamakṛcchratvaṃ vittatvañca parigrahaḥ //",
  "SA20bsa010_u:178": "Mvk_5.4 ārambhaprāptiniṣyandaniṣpattiḥ paramā matā /",
  "SA20bsa010_u:179": "tataśca paramārthena daśa pāramitā matāḥ //",
  "SA20bsa010_u:180": "Mvk_5.5 dānaṃ śīlaṃ kṣamā vīryaṃ dhyānaṃ prajñā upāyatā /",
  "SA20bsa010_u:181": "praṇidhānaṃ balaṃ jñānametāḥ pāramitā daśa //",
  "SA20bsa010_u:182": "Mvk_5.6 anugraho 'vighātaśca karma tasya ca marṣaṇam /",
  "SA20bsa010_u:183": "guṇavṛddhiśca sāmarthyamavatāravimocane //",
  "SA20bsa010_u:184": "Mvk_5.7 akṣayatvaṃ sadā vṛttirniyataṃ bhogapācane /",
  "SA20bsa010_u:185": "yathāprajñaptito dharmamahāyānamanaskriyā //",
  "SA20bsa010_u:186": "Mvk_5.8 bodhisattvasya satataṃ prajñayā triprakārayā /",
  "SA20bsa010_u:187": "dhātupuṣṭayaipraveśāya cārthasiddhyai bhavatyasau //",
  "SA20bsa010_u:188": "Mvk_5.9 saṃyuktā dharmacaritaiḥ sā jñeyā daśabhiḥ punaḥ /",
  "SA20bsa010_u:189": "lekhanā pūjanā dānaṃ śravaṇaṃ vācanod grahaḥ //",
  "SA20bsa010_u:190": "Mvk_5.10 prakāśanātha svādhyāyaścintanā bhāvanā ca tat /",
  "SA20bsa010_u:191": "ameyapuṇyaskandhaṃ hi caritaṃ tad daśātmakam //",
  "SA20bsa010_u:192": "Mvk_5.11 viśeṣādakṣayatvācca parānugrahato 'śamāt /",
  "SA20bsa010_u:193": "avikṣiptāviparyāsapraṇatā cānudhārmikī //",
  "SA20bsa010_u:194": "Mvk_5.12 vyutthānaṃ viṣaye sārastathāsvādalayoddhavaḥ /",
  "SA20bsa010_u:195": "sambhāvanābhisandhiśca manaskāre 'pyahaṃkṛtiḥ //",
  "SA20bsa010_u:196": "Mvk_5.13 hīnacittañca vikṣepaḥ parijñeyo hi dhīmatā /",
  "SA20bsa010_u:197": "vyañjanārthamanaskāre 'visāre lakṣaṇadvaye //",
  "SA20bsa010_u:198": "Mvk_5.14 aśuddhaśuddhāvāgantukatve 'trāsitānunnatau /",
  "SA20bsa010_u:199": "saṃyogāt saṃstavāccaiva viyogādapyasaṃstavāt //",
  "SA20bsa010_u:200": "Mvk_5.15 arthasattvamasattvañca vyañjane so 'viparyayaḥ /",
  "SA20bsa010_u:201": "dvayena pratibhāsatvaṃ tathā cāvidyamānatā //",
  "SA20bsa010_u:202": "Mvk_5.16 arthe sa cāviparyāsaḥ sadasattvena varjitaḥ /",
  "SA20bsa010_u:203": "tajjalpabhāvito jalpamanaskārastadāśrayaḥ //",
  "SA20bsa010_u:204": "Mvk_5.17 manaskāre 'viparyāso dvayaprakhyānakāraṇe /",
  "SA20bsa010_u:205": "māyādivadasattvañca sattvañcārthasya tanmatam //",
  "SA20bsa010_u:206": "Mvk_5.18 so 'visāre 'viparyāso bhāvābhāvāvisārataḥ /",
  "SA20bsa010_u:207": "sarvasya nāmamātratvaṃ sarvakalpāpravṛttaye //",
  "SA20bsa010_u:208": "Mvk_5.19 svalakṣaṇe 'viparyāsaḥ paramārthe svalakṣaṇe /",
  "SA20bsa010_u:209": "dharmadhātuvinirmukto yasmād dharmo na vidyate //",
  "SA20bsa010_u:210": "Mvk_5.20 sāmānyalakṣaṇantasmāt sa ca tatrāviparyayaḥ /",
  "SA20bsa010_u:211": "viparyastamanaskārāvihāniparihāṇitaḥ //",
  "SA20bsa010_u:212": "Mvk_5.21 tadaśuddhirviśuddhiśca sa ca tatrāviparyayaḥ /",
  "SA20bsa010_u:213": "dharmadhātorviśuddhatvāt prakṛtyā vyomavatpunaḥ //",
  "SA20bsa010_u:214": "Mvk_5.22 dvayasyāgantukatvaṃ hi sa ca tatrāviparyayaḥ /",
  "SA20bsa010_u:215": "saṃkleśaśca viśuddhiśca dharmapudgalayorna hi //",
  "SA20bsa010_u:216": "Mvk_5.23 asattvāt trāsatāmānau nātaḥ so 'trāviparyayaḥ /",
  "SA20bsa010_u:217": "pṛthaktvaikatvamantaśca tīrthyaśrāvakayorapi //",
  "SA20bsa010_u:218": "Mvk_5.24 samāropāpavādānto dvidhā pudgaladharmayoḥ /",
  "SA20bsa010_u:219": "vipakṣapratipakṣāntaḥ śāśvatocchedasaṃjñitaḥ //",
  "SA20bsa010_u:220": "Mvk_5.25 grāhyagrāhakasaṃkleśavyavadāne dvidhā tridhā /",
  "SA20bsa010_u:221": "vikalpadvayatāntaśca sa ca saptavidho mataḥ //",
  "SA20bsa010_u:222": "Mvk_5.26 bhāvābhāve praśāmye 'tha śamane trāsyatadbhaye /",
  "SA20bsa010_u:223": "grāhyagrāhe 'tha samyaktvamithyātve vyāpṛtau na ca //",
  "SA20bsa010_u:224": "Mvk_5.27 ajanmasamakālatve sa vikalpadvayāntatā /",
  "SA20bsa010_u:225": "viśiṣṭā cāviśiṣṭā ca jñeyā daśasu bhūmiṣu //",
  "SA20bsa010_u:226": "Mvk_5.28 vyavasthānantato dhātuḥ sādhyasādhanadhāraṇā /",
  "SA20bsa010_u:227": "avadhārapradhārā ca prativedhaḥ pratānatā //",
  "SA20bsa010_u:228": "Mvk_5.29 pragamaḥ praśaṭhatvañca prakarṣālambanammatam /",
  "SA20bsa010_u:229": "avaikalyāpratikṣepo 'vikṣepaśca prapūraṇā //",
  "SA20bsa010_u:230": "Mvk_5.30 samutpādo nirūḍhiśca karmaṇyatvāpratiṣṭhitā /",
  "SA20bsa010_u:231": "nirāvaraṇatā tasyāpraśrabdhisamudāgamaḥ //",
  "SA20bsa010_u:232": "Mvk_5.31 śāstraṃ madhyavibhāgaṃ hi gūḍhasārārthameva ca /",
  "SA20bsa010_u:233": "mahārthañcaiva sarvārthaṃ sarvānarthapraṇodanam //",
  "SA20bsa010_u:234": "// iti yānānuttarya paricchedaḥ pañcamaḥ //",
  "SA20bsa010_u:235": "samāptā madhyāntavibhāgakārikāḥ"
}