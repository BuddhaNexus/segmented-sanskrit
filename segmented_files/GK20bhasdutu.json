{
    "GK20bhasdutu:0": " Stage director",
    "GK20bhasdutu:1": " Chamberlain:",
    "GK20bhasdutu:2": " Badarayana, a servant of Duryodhana",
    "GK20bhasdutu:3": " Duryodhana: eldest son of Dhrtarastra",
    "GK20bhasdutu:4": " Vasudeva: Krsna,",
    "GK20bhasdutu:5": " incarnation of Visnu",
    "GK20bhasdutu:6": " Sudarsana: Vasudeva's discus",
    "GK20bhasdutu:7": " Dhrtarastra:",
    "GK20bhasdutu:8": " king of Hastinapura",
    "GK20bhasdutu:9": " dūtavākyam/",
    "GK20bhasdutu:10": " nāndyante tataḥ praviśati sūtradhāraḥ/",
    "GK20bhasdutu:11": " sūtradhāraḥ",
    "GK20bhasdutu:12": " pādaḥ pāyādupendrasya sarvalokotsavaḥ sa vaḥ/",
    "GK20bhasdutu:13": " vyāviddho namuciryena tanutāmranakhena khe// 1//",
    "GK20bhasdutu:14": " evamāryamiśrān vijñāpayāmi/",
    "GK20bhasdutu:15": " aye kiṃ nu khalu mayi vijñāpanavyagre śabda iva",
    "GK20bhasdutu:16": " śrūyate/ aṅga",
    "GK20bhasdutu:17": " paśyāmi/",
    "GK20bhasdutu:18": " nepathye/",
    "GK20bhasdutu:19": " bho bhoḥ pratihārādhikṛtāḥ mahārājo duryodhanaḥ samājñāpayati/",
    "GK20bhasdutu:20": " sūtradhāraḥ",
    "GK20bhasdutu:21": " bhavatu vijñātam/",
    "GK20bhasdutu:22": " utpanne dhārtarāṣṭrāṇāṃ virodhe pāṇḍavaiḥ saha/",
    "GK20bhasdutu:23": " mantraśālāṃ racayati bhṛtyo duryodhanājñayā// 2//",
    "GK20bhasdutu:24": " niṣkrāntaḥ/",
    "GK20bhasdutu:25": " sthāpanā/",
    "GK20bhasdutu:26": " tataḥ praviśati kāñcukīyaḥ/",
    "GK20bhasdutu:27": " kāñcukīyaḥ",
    "GK20bhasdutu:28": " bho bhoḥ pratihārādhikṛtāḥ mahārājo duryodhanaḥ samājñāpayati/ adya",
    "GK20bhasdutu:29": " sarvapārthivaiḥ saha",
    "GK20bhasdutu:30": " mantrayitumicchāmi/",
    "GK20bhasdutu:31": " tadāhūyantāṃ sarve rājāna iti/",
    "GK20bhasdutu:32": " parikramyāvalokya/aye ayaṃ",
    "GK20bhasdutu:33": " mahārājo",
    "GK20bhasdutu:34": " duryodhana ita evābhivartate/ ya eṣaḥ",
    "GK20bhasdutu:35": " śyāmo yuvā sitadukūlakṛtottarīyaḥ sacchattracāmaravaro racitāṅgarāgaḥ/",
    "GK20bhasdutu:36": " śrīmānvibhūṣaṇamaṇidyutirañjitāṅgo nakṣatramadhya iva parvagataḥ śaśāṅkaḥ// 3//",
    "GK20bhasdutu:37": " tataḥ praviśati yathānirdiṣṭo duryodhanaḥ/",
    "GK20bhasdutu:38": " duryodhanaḥ uddhūtaroṣamiva me",
    "GK20bhasdutu:39": " hṛdayaṃ saharṣaṃ prāptaṃ",
    "GK20bhasdutu:40": " raṇotsavamimaṃ sahasā vicintya/",
    "GK20bhasdutu:41": " icchāmi pāṇḍavabale varavāraṇānā-",
    "GK20bhasdutu:42": " mutkṛttadantamusalāni",
    "GK20bhasdutu:43": " mukhāni kartum// 4//",
    "GK20bhasdutu:44": " kāñcukīyaḥ",
    "GK20bhasdutu:45": " jayatu mahārājaḥ/",
    "GK20bhasdutu:46": " mahārājaśāsanātsamānītaṃ sarvarājamaṇḍalam/",
    "GK20bhasdutu:47": " duryodhanaḥ",
    "GK20bhasdutu:48": " samyakkṛtam/",
    "GK20bhasdutu:49": " praviśa tvamavarodhanam/",
    "GK20bhasdutu:50": " kāñcukīyaḥ",
    "GK20bhasdutu:51": " yadājñāpayati mahārājaḥ/",
    "GK20bhasdutu:52": " niṣkrāntaḥ/",
    "GK20bhasdutu:53": " duryodhanaḥ",
    "GK20bhasdutu:54": " āryau vaikarṇavarṣadevau ucyatām/",
    "GK20bhasdutu:55": " asti mamaikādaśākṣauhiṇībalasamudayaḥ/ asya kaḥ",
    "GK20bhasdutu:56": " senāpatirbhavitumarhatīti/",
    "GK20bhasdutu:57": " kimāhaturbhavantau/",
    "GK20bhasdutu:58": " mahānkhalvayamarthaḥ/",
    "GK20bhasdutu:59": " mantrayitvā vaktavyamiti/",
    "GK20bhasdutu:60": " sadṛśametat/",
    "GK20bhasdutu:61": " tadāgamyatāṃ mantraśālāmeva praviśāmaḥ/",
    "GK20bhasdutu:62": " ācārya abhivādaye/",
    "GK20bhasdutu:63": " praviśatu bhavānmantraśālām/",
    "GK20bhasdutu:64": " pitāmaha abhivādaye/ praviśatu",
    "GK20bhasdutu:65": " bhavānmantraśālām/",
    "GK20bhasdutu:66": " mātula abhivādaye/",
    "GK20bhasdutu:67": " praviśatu bhavānmantraśālām/ āryau",
    "GK20bhasdutu:68": " vaikarṇavarṣadevau praviśatāṃ bhavantau/",
    "GK20bhasdutu:69": " bho bhoḥ sarvakṣatriyāḥ svairaṃ",
    "GK20bhasdutu:70": " praviśantu bhavantaḥ/",
    "GK20bhasdutu:71": " vayasya karṇa praviśāvastāvat/",
    "GK20bhasdutu:72": " praviśya/ācārya etatkūrmāsanam/",
    "GK20bhasdutu:73": " āsyatām/",
    "GK20bhasdutu:74": " pitāmaha etatsiṃhāsanam/",
    "GK20bhasdutu:75": " āsyatām/ mātula",
    "GK20bhasdutu:76": " etaccarmāsanam/",
    "GK20bhasdutu:77": " āsyatām/",
    "GK20bhasdutu:78": " āryau vaikarṇavarṣadevau āsātāṃ bhavantau/ bho bhoḥ",
    "GK20bhasdutu:79": " sarvakṣatriyāḥ",
    "GK20bhasdutu:80": " svairamāsatāṃ bhavantaḥ/",
    "GK20bhasdutu:81": " kimiti kimiti/",
    "GK20bhasdutu:82": " mahārājo nāsta iti/",
    "GK20bhasdutu:83": " aho sevādharmaḥ/",
    "GK20bhasdutu:84": " nanvayamahamāse/",
    "GK20bhasdutu:85": " vayasya karṇa tvamapyāssva/",
    "GK20bhasdutu:86": " upaviśya/āryau vaikarṇavarṣadevau ucyatām/ asti",
    "GK20bhasdutu:87": " mamaikādaśākṣauhiṇībalasamudayaḥ/ asya kaḥ",
    "GK20bhasdutu:88": " senāpatirbhavitumarhatīti/",
    "GK20bhasdutu:89": " kimāhaturbhavantau/",
    "GK20bhasdutu:90": " atrabhavāngāndhārarājo vakṣyatīti/ bhavatu",
    "GK20bhasdutu:91": " mātulenābhidhīyatām/",
    "GK20bhasdutu:92": " kimāha mātulaḥ/",
    "GK20bhasdutu:93": " atrabhavati gāṅgeye sthite ko.anyaḥ senāpatirbhavitumarhatīti/",
    "GK20bhasdutu:94": " samyagāha mātulaḥ/",
    "GK20bhasdutu:95": " bhavatu bhavatu pitāmaha eva bhavatu/",
    "GK20bhasdutu:96": " vayamapyetadabhilaṣāmaḥ/",
    "GK20bhasdutu:97": " senāninādapaṭahasvanaśaṅkhanādai- ścaṇḍānilāhatamahodadhinādakalpaiḥ/",
    "GK20bhasdutu:98": " gāṅgeyamūrdhni",
    "GK20bhasdutu:99": " patitairabhiṣekatoyaiḥ sārdhaṃ patantu hṛdayāni narādhipānām// 5// praviśya/",
    "GK20bhasdutu:100": " kāñcukīyaḥ",
    "GK20bhasdutu:101": " jayatu mahārājaḥ/",
    "GK20bhasdutu:102": " eṣa khalu pāṇḍavaskandhāvārāddautyenāgataḥ puruṣottamo",
    "GK20bhasdutu:103": " nārāyaṇaḥ/",
    "GK20bhasdutu:104": " duryodhanaḥ",
    "GK20bhasdutu:105": " mā tāvat/ bho bādarāyaṇa kiṃ kiṃ kaṃsabhṛtyo dāmodarastava puruṣottamaḥ/ sa",
    "GK20bhasdutu:106": " gopālakastava",
    "GK20bhasdutu:107": " puruṣottamaḥ/",
    "GK20bhasdutu:108": " bārhadrathāpahṛtaviṣayakīrtibhogastava puruṣottamaḥ/ aho",
    "GK20bhasdutu:109": " pārthivāsannamāśritya",
    "GK20bhasdutu:110": " bhṛtyajanasya samudācāraḥ/",
    "GK20bhasdutu:111": " sagarvaṃ khalvasya vacanam/",
    "GK20bhasdutu:112": " ā apadhvaṃsa/",
    "GK20bhasdutu:113": " kāñcukīyaḥ",
    "GK20bhasdutu:114": " prasīdatu mahārājaḥ/",
    "GK20bhasdutu:115": " saṃbhrameṇa samudācāro vismṛtaḥ/",
    "GK20bhasdutu:116": " pādayoḥ patati/",
    "GK20bhasdutu:117": " duryodhanaḥ",
    "GK20bhasdutu:118": " saṃbhrama iti/",
    "GK20bhasdutu:119": " ā manuṣyāṇāmastyeva saṃbhramaḥ/",
    "GK20bhasdutu:120": " uttiṣṭhotiṣṭha/",
    "GK20bhasdutu:121": " kāñcukīyaḥ",
    "GK20bhasdutu:122": " anugṛhīto.asmi/",
    "GK20bhasdutu:123": " duryodhanaḥ",
    "GK20bhasdutu:124": " idānīṃ prasanno.asmi/",
    "GK20bhasdutu:125": " ka eṣa dūtaḥ prāptaḥ/",
    "GK20bhasdutu:126": " kāñcukīyaḥ",
    "GK20bhasdutu:127": " dūtaḥ prāptaḥ keśavaḥ/",
    "GK20bhasdutu:128": " duryodhanaḥ",
    "GK20bhasdutu:129": " keśava iti/ evameṣṭavyam/",
    "GK20bhasdutu:130": " ayameva samudācāraḥ/",
    "GK20bhasdutu:131": " bho bho rājānaḥ dautyenāgatasya",
    "GK20bhasdutu:132": " keśavasya kiṃ",
    "GK20bhasdutu:133": " yuktam/",
    "GK20bhasdutu:134": " kimāhurbhavantaḥ/",
    "GK20bhasdutu:135": " arghyapradānena pūjayitavyaḥ keśava iti/",
    "GK20bhasdutu:136": " na me rocate/",
    "GK20bhasdutu:137": " grahaṇamasyātra hitaṃ",
    "GK20bhasdutu:138": " paśyāmi/",
    "GK20bhasdutu:139": " grahaṇamupagate tu vāsubhadre hṛtanayanā iva pāṇḍavā bhaveyuḥ/",
    "GK20bhasdutu:140": " gatimatirahiteṣu",
    "GK20bhasdutu:141": " pāṇḍaveṣu",
    "GK20bhasdutu:142": " kṣitirakhilāpi bhavenmamāsapatnā// 6//",
    "GK20bhasdutu:143": " api ca yo.atra keśavasya pratyutthāsyati sa mayā dvādaśasuvarṇabhāreṇa daṇḍyaḥ/",
    "GK20bhasdutu:144": " tadapramattā",
    "GK20bhasdutu:145": " bhavantu bhavantaḥ/",
    "GK20bhasdutu:146": " ātmagatam/",
    "GK20bhasdutu:147": " ko nu khalvidānīṃ mamāpratyutthānasyopāyaḥ/ hanta",
    "GK20bhasdutu:148": " dṛṣṭa",
    "GK20bhasdutu:149": " upāyaḥ/prakāśam/",
    "GK20bhasdutu:150": " bādarāyaṇa ānīyatāṃ sa citrapaṭo nanu yatra",
    "GK20bhasdutu:151": " draupadīkeśāmbarāvakarṣaṇamālikhitam/",
    "GK20bhasdutu:152": " ātmagatam/",
    "GK20bhasdutu:153": " tasmindṛṣṭivinyāsaṃ",
    "GK20bhasdutu:154": " kurvannotthāsyāmi",
    "GK20bhasdutu:155": " keśavasya/",
    "GK20bhasdutu:156": " kāñcukīyaḥ",
    "GK20bhasdutu:157": " yadājñāpayati mahārājaḥ/",
    "GK20bhasdutu:158": " niṣkramya praviśya/",
    "GK20bhasdutu:159": " jayatu mahārājaḥ/",
    "GK20bhasdutu:160": " ayaṃ sa citrapaṭaḥ/",
    "GK20bhasdutu:161": " duryodhanaḥ",
    "GK20bhasdutu:162": " mamāgrataḥ prasāraya/",
    "GK20bhasdutu:163": " kāñcukīyaḥ",
    "GK20bhasdutu:164": " yadājñāpayati mahārājaḥ/",
    "GK20bhasdutu:165": " prasārayati/",
    "GK20bhasdutu:166": " duryodhanaḥ",
    "GK20bhasdutu:167": " aho darśanīyo.ayaṃ citrapaṭaḥ/",
    "GK20bhasdutu:168": " eṣa duḥśāsano draupadīṃ keśahaste gṛhītavān/ eṣā",
    "GK20bhasdutu:169": " khalu",
    "GK20bhasdutu:170": " draupadī/",
    "GK20bhasdutu:171": " duḥśāsanaparāmṛṣṭā saṃbhramotphullalocanā/",
    "GK20bhasdutu:172": " rāhuvaktrāntaragatā candralekheva",
    "GK20bhasdutu:173": " śobhate// 7//",
    "GK20bhasdutu:174": " eṣa durātmā bhīmaḥ sarvarājasamakṣamavamānitāṃ draupadīṃ dṛṣṭvā",
    "GK20bhasdutu:175": " pravṛddhāmarṣaḥ",
    "GK20bhasdutu:176": " sabhāstambhaṃ tulayati/",
    "GK20bhasdutu:177": " eṣa yudhiṣṭhiraḥ",
    "GK20bhasdutu:178": " satyadharmaghṛṇāyukto dyūtavibhraṣṭacetanaḥ/",
    "GK20bhasdutu:179": " karotyapāṅgavikṣepaiḥ śāntāmarṣaṃ",
    "GK20bhasdutu:180": " vṛkodaram// 8//",
    "GK20bhasdutu:181": " eṣa idānīmarjunaḥ/",
    "GK20bhasdutu:182": " roṣākulākṣaḥ sphuritādharoṣṭhastṛṇāya matvā ripumaṇḍalaṃ tat/",
    "GK20bhasdutu:183": " utsādayiṣyanniva",
    "GK20bhasdutu:184": " sarvarājñaḥ",
    "GK20bhasdutu:185": " śanaiḥ samākarṣati gāṇḍivajyām// 9//",
    "GK20bhasdutu:186": " eṣa yudhiṣṭhiro.arjunaṃ nivārayati/",
    "GK20bhasdutu:187": " etau nakulasahadevau",
    "GK20bhasdutu:188": " kṛtaparikarabandhau carmanistriṃśahastau paruṣitamukharāgau",
    "GK20bhasdutu:189": " spaṣṭadaṣṭādharoṣṭhau/",
    "GK20bhasdutu:190": " vigatamaraṇaśaṅkau satvaraṃ bhrātaraṃ me harimiva mṛgapotau tejasābhiprayātau// 10//",
    "GK20bhasdutu:191": " eṣa yudhiṣṭhiraḥ kumārāvupetya nivārayati/",
    "GK20bhasdutu:192": " nīco.ahameva viparītamatiḥ kathaṃ vā roṣaṃ parityajatamadya nayānayajñau/",
    "GK20bhasdutu:193": " dyūtādhikāramavamānamamṛṣyamāṇāḥ sattvādhikeṣu vacanīyaparākramāḥ syuḥ// 11//",
    "GK20bhasdutu:194": " iti/ eṣa gāndhārarājaḥ",
    "GK20bhasdutu:195": " akṣānkṣipansa kitavaḥ prahasansagarvaṃ saṃkocayanniva mudaṃ dviṣatāṃ",
    "GK20bhasdutu:196": " svakīrtyā/ svairāsano",
    "GK20bhasdutu:197": " drupadarājasutāṃ rudantīṃ kākṣeṇa paśyati likhatyapi gāṃ nayajñaḥ// 12//",
    "GK20bhasdutu:198": " etāvācāryapitāmahau tāṃ dṛṣṭvā lajjāyamānau paṭāntāntarhitamukhau sthitau/ aho asya",
    "GK20bhasdutu:199": " varṇāḍhyatā/",
    "GK20bhasdutu:200": " aho bhāvopapannatā/",
    "GK20bhasdutu:201": " aho yuktalekhatā/",
    "GK20bhasdutu:202": " suvyaktamālikhito.ayaṃ",
    "GK20bhasdutu:203": " citrapaṭaḥ/",
    "GK20bhasdutu:204": " prīto.asmi/ ko.atra/",
    "GK20bhasdutu:205": " kāñcukīyaḥ",
    "GK20bhasdutu:206": " jayatu mahārājaḥ/",
    "GK20bhasdutu:207": " duryodhanaḥ",
    "GK20bhasdutu:208": " bādarāyaṇa ānīyatāṃ sa vihagavāhanamātravismito dūtaḥ/",
    "GK20bhasdutu:209": " kāñcukīyaḥ",
    "GK20bhasdutu:210": " yadājñāpayati mahārājaḥ/",
    "GK20bhasdutu:211": " niṣkrāntaḥ/",
    "GK20bhasdutu:212": " duryodhanaḥ",
    "GK20bhasdutu:213": " vayasya karṇa",
    "GK20bhasdutu:214": " prāptaḥ kilādya vacanādiha pāṇḍavānāṃ dautyena bhṛtya iva kṛṣṇamatiḥ sa kṛṣṇaḥ/",
    "GK20bhasdutu:215": " śrotuṃ",
    "GK20bhasdutu:216": " sakhe tvamapi sajjaya karṇa karṇau nārīmṛdūni vacanāni yudhiṣṭhirasya// 13// tataḥ",
    "GK20bhasdutu:217": " praviśati",
    "GK20bhasdutu:218": " vāsudevaḥ kāñcukīyaśca/ vāsudevaḥ",
    "GK20bhasdutu:219": " adya khalu dharmarājavacanāddhanaṃjayākṛtrimamitratayā",
    "GK20bhasdutu:220": " cāhavadarpamanuktagrāhiṇaṃ",
    "GK20bhasdutu:221": " suyodhanaṃ prati mayāpyanucitadautyasamayo.anuṣṭhitaḥ/ atha ca",
    "GK20bhasdutu:222": " kṛṣṇāparābhavabhuvā ripuvāhinībha- kumbhasthalīdalanatīkṣṇagadādharasya/",
    "GK20bhasdutu:223": " bhīmasya",
    "GK20bhasdutu:224": " kopaśikhinā yudhi pārthapattri- caṇḍānilaiśca kuruvaṃśavanaṃ vinaṣṭam// 14//",
    "GK20bhasdutu:225": " idaṃ suyodhanaśibiram/ iha hi",
    "GK20bhasdutu:226": " āvāsāḥ pārthivānāṃ surapurasadṛśāḥ svacchandavihitā vistīrṇāḥ śastraśālā",
    "GK20bhasdutu:227": " bahuvidhakaraṇaiḥ śastrairupacitāḥ/",
    "GK20bhasdutu:228": " heṣante mandurāsthāsturagavaraghaṭā bṛṃhanti",
    "GK20bhasdutu:229": " kariṇa",
    "GK20bhasdutu:230": " aiśvaryaṃ sphītametatsvajanaparibhavādāsannavilayam// 15// bhoḥ",
    "GK20bhasdutu:231": " duṣṭavādī guṇadveṣī śaṭhaḥ svajananirdayaḥ/",
    "GK20bhasdutu:232": " suyodhano hi māṃ dṛṣṭvā naiva",
    "GK20bhasdutu:233": " kāryaṃ kariṣyati// 16//",
    "GK20bhasdutu:234": " bho bādarāyaṇa kiṃ praveṣṭavyam/",
    "GK20bhasdutu:235": " kāñcukīyaḥ",
    "GK20bhasdutu:236": " atha kimatha kim/",
    "GK20bhasdutu:237": " praveṣṭumarhati padmanābhaḥ/",
    "GK20bhasdutu:238": " vāsudevaḥ",
    "GK20bhasdutu:239": " praviśya/ kathaṃ kathaṃ māṃ dṛṣṭvā saṃbhrāntāḥ sarvakṣatriyāḥ/ alamalaṃ",
    "GK20bhasdutu:240": " saṃbhrameṇa/",
    "GK20bhasdutu:241": " svairamāsatāṃ bhavantaḥ/",
    "GK20bhasdutu:242": " duryodhanaḥ",
    "GK20bhasdutu:243": " kathaṃ kathaṃ keśavaṃ dṛṣṭvā saṃbhrāntāḥ sarvakṣatriyāḥ/",
    "GK20bhasdutu:244": " alamalaṃ saṃbhrameṇa/",
    "GK20bhasdutu:245": " smaraṇīyaḥ pūrvamāśrāvito daṇḍaḥ/",
    "GK20bhasdutu:246": " nanvahamājñaptā/",
    "GK20bhasdutu:247": " vāsudevaḥ",
    "GK20bhasdutu:248": " upagamya/bhoḥ suyodhana kimāsse/",
    "GK20bhasdutu:249": " duryodhanaḥ",
    "GK20bhasdutu:250": " āsanātpatitvātmagatam/",
    "GK20bhasdutu:251": " suvyaktaṃ prāpta eva keśavaḥ/",
    "GK20bhasdutu:252": " utsāhena matiṃ kṛtvāpyāsīno.asmi samāhitaḥ/",
    "GK20bhasdutu:253": " keśavasya prabhāvena",
    "GK20bhasdutu:254": " calito.asmyāsanādaham// 17//",
    "GK20bhasdutu:255": " aho bahumāyo.ayaṃ dūtaḥ/",
    "GK20bhasdutu:256": " prakāśam/",
    "GK20bhasdutu:257": " bho dūta etadāsanamāsyatām/",
    "GK20bhasdutu:258": " vāsudevaḥ",
    "GK20bhasdutu:259": " ācārya āsyatāṃ/",
    "GK20bhasdutu:260": " gāṅgeyapramukhā rājānaḥ svairamāsatāṃ bhavantaḥ/",
    "GK20bhasdutu:261": " vayamapyupaviśāmaḥ/",
    "GK20bhasdutu:262": " upaviśya/",
    "GK20bhasdutu:263": " aho darśanīyo.ayaṃ citrapaṭaḥ/ mā tāvat/",
    "GK20bhasdutu:264": " draupadīkeśāmbarākarṣaṇamatrālikhitam/",
    "GK20bhasdutu:265": " aho nu khalu",
    "GK20bhasdutu:266": " suyodhano.ayaṃ svajanāvamānaṃ parākramaṃ paśyati bāliśatvāt/",
    "GK20bhasdutu:267": " ko nāma loke",
    "GK20bhasdutu:268": " svayamātmadoṣamudghāṭayennaṣṭaghṛṇaḥ sabhāsu// 18//",
    "GK20bhasdutu:269": " āḥ apanīyatāmeṣa citrapaṭaḥ/",
    "GK20bhasdutu:270": " duryodhanaḥ",
    "GK20bhasdutu:271": " bādarāyaṇa apanīyatāṃ kila citrapaṭaḥ/",
    "GK20bhasdutu:272": " kāñcukīyaḥ",
    "GK20bhasdutu:273": " yadājñāpayati maharājaḥ/apanayati/",
    "GK20bhasdutu:274": " duryodhanaḥ",
    "GK20bhasdutu:275": " bho dūta",
    "GK20bhasdutu:276": " dharmātmajo vāyusutaśca bhīmo bhrātārjuno me tridaśendrasūnuḥ/ yamau ca",
    "GK20bhasdutu:277": " tāvaśvisutau vinītau",
    "GK20bhasdutu:278": " sarve sabhṛtyāḥ kuśalopapannāḥ// 19// vāsudevaḥ",
    "GK20bhasdutu:279": " sadṛśametadgāndhārīputrasya/",
    "GK20bhasdutu:280": " atha kimatha kim/",
    "GK20bhasdutu:281": " kuśalinaḥ sarve/",
    "GK20bhasdutu:282": " bhavato rājye",
    "GK20bhasdutu:283": " śarīre",
    "GK20bhasdutu:284": " bāhyābhyantare ca kuśalamanāmayaṃ ca pṛṣṭvā vijñāpayanti yudhiṣṭhirādayaḥ",
    "GK20bhasdutu:285": " pāṇḍavāḥ/",
    "GK20bhasdutu:286": " anubhūtaṃ mahadduḥkhaṃ saṃpūrṇaḥ samayaḥ sa ca/",
    "GK20bhasdutu:287": " asmākamapi dharmyaṃ",
    "GK20bhasdutu:288": " yaddāyādyaṃ",
    "GK20bhasdutu:289": " tadvibhajyatām// 20// iti/",
    "GK20bhasdutu:290": " duryodhanaḥ",
    "GK20bhasdutu:291": " kathaṃ kathaṃ dāyādyamiti/ bhoḥ",
    "GK20bhasdutu:292": " vane pitṛvyo mṛgayāprasaṃgataḥ kṛtāparādho muniśāpamāptavān/",
    "GK20bhasdutu:293": " tadāprabhṛtyeva sa",
    "GK20bhasdutu:294": " dāraniḥspṛhaḥ parātmajānāṃ pitṛtāṃ kathaṃ vrajet// 21// vāsudevaḥ",
    "GK20bhasdutu:295": " purāvidaṃ bhavantaṃ pṛcchāmi/",
    "GK20bhasdutu:296": " vicitravīryo viṣayī vipattiṃ kṣayeṇa yātaḥ punarambikāyām/",
    "GK20bhasdutu:297": " vyāsena jāto",
    "GK20bhasdutu:298": " dhṛtarāṣṭra eva labheta",
    "GK20bhasdutu:299": " rājyaṃ janakaḥ kathaṃ te// 22//",
    "GK20bhasdutu:300": " mā mā bhavān/",
    "GK20bhasdutu:301": " evaṃ parasparavirodhavivardhanena śīghraṃ bhavetkurukulaṃ nṛpa nāmaśeṣam/",
    "GK20bhasdutu:302": " tatkartumarhati",
    "GK20bhasdutu:303": " bhavānapakṛtya roṣaṃ yattvāṃ yudhiṣṭhiramukhāḥ praṇayādbruvanti// 23//",
    "GK20bhasdutu:304": " duryodhanaḥ",
    "GK20bhasdutu:305": " bho dūta na jānāti bhavānrājyavyavahāram/",
    "GK20bhasdutu:306": " rājyaṃ nāma nṛpātmajaiḥ sahṛdairjitvā ripūnbhujyate talloke na tu yācyate na tu",
    "GK20bhasdutu:307": " punardīnāya vā",
    "GK20bhasdutu:308": " dīyate/ kāṅkṣā cennṛpatitvamāptumacirātkurvantu te sāhasaṃ svairaṃ vā praviśantu",
    "GK20bhasdutu:309": " śāntamatibhirjuṣṭaṃ śamāyāśramam// 24// vāsudevaḥ",
    "GK20bhasdutu:310": " bhoḥ suyodhana alaṃ bandhujane paruṣamabhidhātum/",
    "GK20bhasdutu:311": " puṇyasaṃcayaprāptāmadhigamya nṛpaśriyam/",
    "GK20bhasdutu:312": " vañcayedyaḥ suhṛdbandhūnsa",
    "GK20bhasdutu:313": " bhavedviphalaśramaḥ// 25//",
    "GK20bhasdutu:314": " duryodhanaḥ",
    "GK20bhasdutu:315": " bho dūta",
    "GK20bhasdutu:316": " syālaṃ tava gurorbhūpaṃ kaṃsaṃ na te dayā/",
    "GK20bhasdutu:317": " kathamasmākamevaṃ syātteṣu",
    "GK20bhasdutu:318": " nityāpakāriṣu// 26//",
    "GK20bhasdutu:319": " vāsudevaḥ",
    "GK20bhasdutu:320": " alaṃ tanmaddoṣato jñātum/",
    "GK20bhasdutu:321": " kṛtvā putraviyogārtāṃ bahuśo jananīṃ mama/",
    "GK20bhasdutu:322": " vṛddhaṃ svapitaraṃ baddhvā hato.ayaṃ",
    "GK20bhasdutu:323": " mṛtyunā",
    "GK20bhasdutu:324": " svayam// 27// duryodhanaḥ",
    "GK20bhasdutu:325": " sarvathā vañcitastvayā kaṃsaḥ/",
    "GK20bhasdutu:326": " alamātmastavena/",
    "GK20bhasdutu:327": " na śauryametat/ paśya/",
    "GK20bhasdutu:328": " jāmātṛnāśavyasanābhitapte roṣābhibhūte magadheśvare.atha/",
    "GK20bhasdutu:329": " palāyamānasya",
    "GK20bhasdutu:330": " bhayāturasya",
    "GK20bhasdutu:331": " śauryaṃ tadetatkva gataṃ tavāsīt// 28// vāsudevaḥ",
    "GK20bhasdutu:332": " bhoḥ suyodhana deśakālāvasthāpekṣitaṃ khalu śauryaṃ nayānugāminām/ iha",
    "GK20bhasdutu:333": " tiṣṭhatu",
    "GK20bhasdutu:334": " tāvadasmadgataḥ parihāsaḥ/",
    "GK20bhasdutu:335": " svakāryamanuṣṭhīyatām/",
    "GK20bhasdutu:336": " kartavyo bhrātṛṣu sneho vismartavyā guṇetarāḥ/",
    "GK20bhasdutu:337": " saṃbandho bandhubhiḥ",
    "GK20bhasdutu:338": " śreyānlokayorubhayorapi//",
    "GK20bhasdutu:339": " 29// duryodhanaḥ devātmajairmanuṣyāṇāṃ kathaṃ vā bandhutā bhavet/",
    "GK20bhasdutu:340": " piṣṭapeṣaṇametāvatparyāptaṃ chidyatāṃ kathā// 30// vāsudevaḥ",
    "GK20bhasdutu:341": " ātmagatam/",
    "GK20bhasdutu:342": " prasādyamānaḥ sāmnāyaṃ na svabhāvaṃ vimuñcati/",
    "GK20bhasdutu:343": " hanta saṃkṣobhayāmyenaṃ",
    "GK20bhasdutu:344": " vacobhiḥ",
    "GK20bhasdutu:345": " paruṣākṣaraiḥ// 31//",
    "GK20bhasdutu:346": " prakāśam/bhoḥ suyodhana kiṃ na jānīṣe.arjunasya balaparākramam/",
    "GK20bhasdutu:347": " duryodhanaḥ",
    "GK20bhasdutu:348": " na jāne/",
    "GK20bhasdutu:349": " vāsudevaḥ",
    "GK20bhasdutu:350": " bhoḥ śrūyatām/",
    "GK20bhasdutu:351": " kairātaṃ vapurāsthitaḥ paśupatiryuddhena saṃtoṣito vahneḥ khāṇḍavamaśnataḥ",
    "GK20bhasdutu:352": " sumahatī vṛṣṭiḥ",
    "GK20bhasdutu:353": " śaraiśchāditā/",
    "GK20bhasdutu:354": " devendrārtikarā nivātakavacā nītāḥ kṣayaṃ līlayā nanvekena tadā",
    "GK20bhasdutu:355": " virāṭanagare",
    "GK20bhasdutu:356": " bhīṣmādayo nirjitāḥ// 32//",
    "GK20bhasdutu:357": " api ca tavāpi pratyakṣamaparaṃ kathayāmi/",
    "GK20bhasdutu:358": " nanu tvaṃ citrasenena nīyamāno nabhastalam/",
    "GK20bhasdutu:359": " vikrośanghoṣayātrāyāṃ phalgunenaiva",
    "GK20bhasdutu:360": " mokṣitaḥ// 33//",
    "GK20bhasdutu:361": " kiṃ bahunā/",
    "GK20bhasdutu:362": " dātumarhasi madvākyādrājyārdhaṃ dhṛtarāṣṭraja/",
    "GK20bhasdutu:363": " anyathā sāgarāntāṃ gāṃ hariṣyanti hi",
    "GK20bhasdutu:364": " pāṇḍavāḥ// 34// duryodhanaḥ",
    "GK20bhasdutu:365": " kathaṃ kathaṃ/",
    "GK20bhasdutu:366": " hariṣyanti hi pāṇḍavāḥ/",
    "GK20bhasdutu:367": " praharati yadi yuddhe māruto bhīmarūpī praharati yadi sākṣātpārtharūpeṇa śakraḥ/",
    "GK20bhasdutu:368": " paruṣavacanadakṣa tvadvacobhirna dāsye tṛṇamapi pitṛbhukte vīryagupte svarājye//",
    "GK20bhasdutu:369": " 35// vāsudevaḥ",
    "GK20bhasdutu:370": " bhoḥ kurukulakalaṅkabhūta ayaśolubdha vayaṃ kila tṛṇāntarābhibhāṣakāḥ/",
    "GK20bhasdutu:371": " duryodhanaḥ",
    "GK20bhasdutu:372": " bho gopālaka tṛṇānyabhibhāṣyo bhavān/",
    "GK20bhasdutu:373": " avadhyāṃ pramadāṃ hatvā hayaṃ govṛṣameva ca/",
    "GK20bhasdutu:374": " mallānapi sunirlajjo vaktumicchasi",
    "GK20bhasdutu:375": " sādhubhiḥ//",
    "GK20bhasdutu:376": " 36// vāsudevaḥ",
    "GK20bhasdutu:377": " bhoḥ suyodhana nanu kṣipasi mām/",
    "GK20bhasdutu:378": " duryodhanaḥ",
    "GK20bhasdutu:379": " nanu satyamevaitat/",
    "GK20bhasdutu:380": " vāsudevaḥ",
    "GK20bhasdutu:381": " gacchāmi tāvat/",
    "GK20bhasdutu:382": " duryodhanaḥ",
    "GK20bhasdutu:383": " gaccha gaccha paśukhuroddhatareṇurūpitāṅgo vrajameva/",
    "GK20bhasdutu:384": " viphalīkṛtaḥ kālaḥ/",
    "GK20bhasdutu:385": " vāsudevaḥ",
    "GK20bhasdutu:386": " evamastu/ na vayamanuktasaṃdeśā gantumicchāmaḥ/",
    "GK20bhasdutu:387": " tadākarṇyatāṃ yudhiṣṭhirasya",
    "GK20bhasdutu:388": " saṃdeśaḥ/",
    "GK20bhasdutu:389": " duryodhanaḥ",
    "GK20bhasdutu:390": " āḥ abhāṣyastvam/",
    "GK20bhasdutu:391": " ahamavadhṛtapāṇḍarātapatro dvijavarahastadhṛtāmbusiktamūrdhā/",
    "GK20bhasdutu:392": " avanatanṛpamaṇḍalānuyātraiḥ",
    "GK20bhasdutu:393": " saha kathayāmi bhavadvidhairna bhāṣe// 37// vāsudevaḥ",
    "GK20bhasdutu:394": " na vyāharati kila māṃ suyodhanaḥ/ bhoḥ",
    "GK20bhasdutu:395": " śaṭha bāndhavaniḥsneha kāka kekara piṅgala/",
    "GK20bhasdutu:396": " tvadarthātkuruvaṃśo.ayamacirānnāśameṣyati// 38//",
    "GK20bhasdutu:397": " bho bho rājānaḥ gacchāmastāvat/",
    "GK20bhasdutu:398": " duryodhanaḥ",
    "GK20bhasdutu:399": " kathaṃ yāsyati kila keśavaḥ/",
    "GK20bhasdutu:400": " duḥśāsana durmarṣaṇa durmukha durbuddhe",
    "GK20bhasdutu:401": " duṣṭeśvara",
    "GK20bhasdutu:402": " dūtasamudācāramatikrāntaḥ keśavo badhyatām/",
    "GK20bhasdutu:403": " kathamaśaktāḥ/",
    "GK20bhasdutu:404": " duḥśāsana na",
    "GK20bhasdutu:405": " samarthaḥ",
    "GK20bhasdutu:406": " khalvasi/",
    "GK20bhasdutu:407": " karituraganihantā kaṃsahantā sa kṛṣṇaḥ paśupakulanivāsādānujīvyānabhijñaḥ/",
    "GK20bhasdutu:408": " hṛtabhujabalavīryaḥ pārthivānāṃ samakṣaṃ svavacanakṛtadoṣo badhyatāmeṣa",
    "GK20bhasdutu:409": " śīghram// 39//",
    "GK20bhasdutu:410": " ayamaśaktaḥ/",
    "GK20bhasdutu:411": " mātula badhyatāmayaṃ keśavaḥ/",
    "GK20bhasdutu:412": " kathaṃ parāṅmukhaḥ patati/ bhavatu/",
    "GK20bhasdutu:413": " ahameva",
    "GK20bhasdutu:414": " pāśairbadhnāmi/",
    "GK20bhasdutu:415": " pāśamudyamyopasarpati/",
    "GK20bhasdutu:416": " vāsudevaḥ",
    "GK20bhasdutu:417": " kathaṃ baddhukāmo māṃ kila suyodhanaḥ/",
    "GK20bhasdutu:418": " bhavatu/",
    "GK20bhasdutu:419": " suyodhanasya sāmarthyaṃ",
    "GK20bhasdutu:420": " paśyāmi/viśvarūpamāsthitaḥ/",
    "GK20bhasdutu:421": " duryodhanaḥ",
    "GK20bhasdutu:422": " bho dūta",
    "GK20bhasdutu:423": " sṛjasi yadi samantāddevamāyāḥ svamāyāḥ praharasi yadi vā tvaṃ durnivāraiḥ",
    "GK20bhasdutu:424": " surāstraiḥ/",
    "GK20bhasdutu:425": " hayagajavṛṣabhāṇāṃ pātanājjātadarpo narapatigaṇamadhye badhyase tvaṃ mayādya// 40//",
    "GK20bhasdutu:426": " āḥ tiṣṭhedānīm/",
    "GK20bhasdutu:427": " kathaṃ na dṛṣṭaḥ keśavaḥ/",
    "GK20bhasdutu:428": " ayaṃ keśavaḥ/",
    "GK20bhasdutu:429": " aho hrasvatvaṃ keśavasya/ āḥ",
    "GK20bhasdutu:430": " tiṣṭhedānīm/",
    "GK20bhasdutu:431": " kathaṃ na dṛṣṭaḥ keśavaḥ/",
    "GK20bhasdutu:432": " ayaṃ keśavaḥ/",
    "GK20bhasdutu:433": " aho dīrghatvaṃ keśavasya/",
    "GK20bhasdutu:434": " kathaṃ na",
    "GK20bhasdutu:435": " dṛṣṭaḥ keśavaḥ/",
    "GK20bhasdutu:436": " ayaṃ keśavaḥ/",
    "GK20bhasdutu:437": " sarvatra mantraśālāyāṃ keśavā bhavanti/ kimidānīṃ",
    "GK20bhasdutu:438": " kariṣye/",
    "GK20bhasdutu:439": " bhavatu dṛṣṭam/",
    "GK20bhasdutu:440": " bho bho rājānaḥ ekenaikaḥ keśavo badhyatām/",
    "GK20bhasdutu:441": " kathaṃ svayameva",
    "GK20bhasdutu:442": " pāśairbaddhāḥ",
    "GK20bhasdutu:443": " patanti rājānaḥ/",
    "GK20bhasdutu:444": " sādhu bho jambhaka sādhu/",
    "GK20bhasdutu:445": " matkārmukodaraviniḥsṛtabāṇajālai- rviddhaṃ kṣaratkṣatajarañjitasarvagātram/",
    "GK20bhasdutu:446": " paśyantu",
    "GK20bhasdutu:447": " pāṇḍutanayāḥ śibiropanītaṃ tvāṃ bāṣparuddhanayanāḥ pariniśvasantaḥ// 41//",
    "GK20bhasdutu:448": " niṣkrāntaḥ/",
    "GK20bhasdutu:449": " vāsudevaḥ",
    "GK20bhasdutu:450": " bhavatu pāṇḍavānāṃ kāryamahameva sādhayāmi/",
    "GK20bhasdutu:451": " bhoḥ sudarśana itastāvat/",
    "GK20bhasdutu:452": " tataḥ praviśati sudarśanaḥ/",
    "GK20bhasdutu:453": " sudarśanaḥ",
    "GK20bhasdutu:454": " eṣa bhoḥ/",
    "GK20bhasdutu:455": " śrutvā giraṃ bhagavato vipulaprasādā- nnirdhāvito.asmi parivāritatoyadaughaḥ/",
    "GK20bhasdutu:456": " kasminkhalu",
    "GK20bhasdutu:457": " prakupitaḥ kamalāyatākṣaḥ kasyādya mūrdhani mayā pravijṛmbhitavyam// 42//",
    "GK20bhasdutu:458": " kva nu khalu bhagavānnārāyaṇaḥ/",
    "GK20bhasdutu:459": " avyaktādiracintyātmā lokasaṃrakṣaṇodyataḥ/",
    "GK20bhasdutu:460": " eko.anekavapuḥ",
    "GK20bhasdutu:461": " śrīmāndviṣadbalaniṣūdanaḥ// 43//",
    "GK20bhasdutu:462": " vilokya/aye ayaṃ bhagavānhastinapuradvāre dūtasamudācāreṇopasthitaḥ/ kutaḥ",
    "GK20bhasdutu:463": " khalvāpaḥ kutaḥ",
    "GK20bhasdutu:464": " khalvāpaḥ/ bhagavati ākāśagaṅge āpastāvat/",
    "GK20bhasdutu:465": " hanta sravati/",
    "GK20bhasdutu:466": " ācamyopasṛtya/jayatu",
    "GK20bhasdutu:467": " bhagavānnārāyaṇaḥ/praṇamati/",
    "GK20bhasdutu:468": " vāsudevaḥ",
    "GK20bhasdutu:469": " sudarśana apratihataparākramo bhava/",
    "GK20bhasdutu:470": " sudarśanaḥ",
    "GK20bhasdutu:471": " anugṛhīto.asmi/",
    "GK20bhasdutu:472": " vāsudevaḥ",
    "GK20bhasdutu:473": " diṣṭyā bhavānkarmakāle prāptaḥ/",
    "GK20bhasdutu:474": " sudarśanaḥ",
    "GK20bhasdutu:475": " kathaṃ kathaṃ karmakāla iti/",
    "GK20bhasdutu:476": " ājñāpayatu bhagavānājñāpayatu/",
    "GK20bhasdutu:477": " kiṃ merumandarakulaṃ parivartayāmi saṃkṣobhayāmi sakalaṃ makarālayaṃ vā/",
    "GK20bhasdutu:478": " nakṣatravaṃśamakhilaṃ bhuvi pātayāmi nāśakyamasti mama deva tava prasādāt// 44//",
    "GK20bhasdutu:479": " vāsudevaḥ",
    "GK20bhasdutu:480": " bhoḥ sudarśana itastāvat/",
    "GK20bhasdutu:481": " bhoḥ suyodhana",
    "GK20bhasdutu:482": " yadi lavaṇajalaṃ vā kandaraṃ vā girīṇāṃ grahagaṇacaritaṃ vā vāyumārgaṃ prayāsi/ mama",
    "GK20bhasdutu:483": " bhujabalayogaprāptasaṃjātavegaṃ bhavatu capala cakraṃ kālacakraṃ tavādya// 45//",
    "GK20bhasdutu:484": " sudarśanaḥ",
    "GK20bhasdutu:485": " bhoḥ suyodhanahataka tiṣṭha tiṣṭha/",
    "GK20bhasdutu:486": " punarvicārya/",
    "GK20bhasdutu:487": " prasīdatu bhagavānnārāyṇaḥ/",
    "GK20bhasdutu:488": " mahībhārāpanayanaṃ kartuṃ jātasya bhūtale/",
    "GK20bhasdutu:489": " asminnevaṃ gate deva nanu",
    "GK20bhasdutu:490": " syādviphalaśramaḥ// 46//",
    "GK20bhasdutu:491": " vāsudevaḥ",
    "GK20bhasdutu:492": " sudarśana roṣātsamudācāro nāvekṣitaḥ/",
    "GK20bhasdutu:493": " gamyatāṃ svanilayameva/",
    "GK20bhasdutu:494": " sudarśanaḥ",
    "GK20bhasdutu:495": " yadājñāpayati bhagavānnārāyaṇaḥ/",
    "GK20bhasdutu:496": " kathaṃ kathaṃ gopālaka iti/",
    "GK20bhasdutu:497": " tricaraṇātikrāntatriloko",
    "GK20bhasdutu:498": " nārāyaṇaḥ khalvatra bhagavān/",
    "GK20bhasdutu:499": " śaraṇaṃ vrajantu bhavantaḥ/",
    "GK20bhasdutu:500": " yāvadgacchāmi/ aye",
    "GK20bhasdutu:501": " etadbhagavadāyudhavaraṃ śārṅgaṃ prāptam/",
    "GK20bhasdutu:502": " tanumṛdulalitāṅgaṃ strīsvabhāvopapannaṃ harikaradhṛtamadhyaṃ",
    "GK20bhasdutu:503": " śatrusaṃghaikakālaḥ/",
    "GK20bhasdutu:504": " kanakakhacitapṛṣṭhaṃ bhāti kṛṣṇasya pārśve navasaliladapārśve cāruvidyullekheva// 47//",
    "GK20bhasdutu:505": " bho bhoḥ śārṅga praśāntaroṣo bhagavānnārāyaṇaḥ/",
    "GK20bhasdutu:506": " gamyatāṃ svanilayameva/ hanta",
    "GK20bhasdutu:507": " nivṛttaḥ/",
    "GK20bhasdutu:508": " yāvadgacchāmi/",
    "GK20bhasdutu:509": " aye iyaṃ kaumodakī prāptā/",
    "GK20bhasdutu:510": " maṇikanakavicitrā citramālottarīyā suraripugaṇagātradhvaṃsane jātatṛṣṇā/",
    "GK20bhasdutu:511": " girivarataṭarūpā",
    "GK20bhasdutu:512": " durnivārātivīryā vrajati nabhasi śīghraṃ meghavṛndānuyātrā// 48//",
    "GK20bhasdutu:513": " he kaumodaki praśāntaroṣo bhagavānnārāyaṇaḥ/",
    "GK20bhasdutu:514": " hanta nivṛttā/",
    "GK20bhasdutu:515": " yāvadgacchāmi/ aye ayaṃ",
    "GK20bhasdutu:516": " pāñcajanyaḥ prāptaḥ/",
    "GK20bhasdutu:517": " pūrṇendukundakumudodarahāragauro nārāyaṇānanasarojakṛtaprasādaḥ/",
    "GK20bhasdutu:518": " yasya svanaṃ",
    "GK20bhasdutu:519": " pralayasāgaraghoṣatulyaṃ garbhā niśamya nipatantyasurāṅganānām// 49//",
    "GK20bhasdutu:520": " he pāñcajanya praśāntaroṣo bhagavān/",
    "GK20bhasdutu:521": " gamyatāṃ svanilayameva/",
    "GK20bhasdutu:522": " hanta nivṛttaḥ/ aye",
    "GK20bhasdutu:523": " nandakāsiḥ",
    "GK20bhasdutu:524": " prāptaḥ/",
    "GK20bhasdutu:525": " vanitāvigraho yuddhe mahāsurabhayaṃkaraḥ/",
    "GK20bhasdutu:526": " prayāti gagane śīghraṃ maholkeva",
    "GK20bhasdutu:527": " vibhātyayam// 50//",
    "GK20bhasdutu:528": " he nandaka praśāntaroṣo bhagavān/",
    "GK20bhasdutu:529": " gamyatāṃ svanilayameva/",
    "GK20bhasdutu:530": " hanta nivṛttaḥ/",
    "GK20bhasdutu:531": " yāvadgacchāmi/ aye",
    "GK20bhasdutu:532": " etāni bhagavadāyudhavarāṇi/",
    "GK20bhasdutu:533": " so.ayaṃ khaḍgaḥ kharāṃśorapahasitatanuḥ svaiḥ karairnandakākhyaḥ seyaṃ",
    "GK20bhasdutu:534": " kaumodakī yā",
    "GK20bhasdutu:535": " suraripukaṭhinoraḥsthalakṣodadakṣā/",
    "GK20bhasdutu:536": " saiṣā śārṅgābhidhānā pralayaghanaravajyāravā",
    "GK20bhasdutu:537": " cāpayaṣṭiḥ so.ayaṃ gambhīraghoṣaḥ śaśikaraviśadaḥ śaṅkharāṭpāñcajanyaḥ// 51// he",
    "GK20bhasdutu:538": " śārṅga kaumodaki pāñcajanya daityāntakṛnnandaka śatruvahne/",
    "GK20bhasdutu:539": " praśāntaroṣo",
    "GK20bhasdutu:540": " bhagavānmurāriḥ",
    "GK20bhasdutu:541": " svasthānamevātra hi gaccha tāvat// 52//",
    "GK20bhasdutu:542": " hanta nivṛttāḥ/",
    "GK20bhasdutu:543": " yāvadgacchāmi/",
    "GK20bhasdutu:544": " aye atyuddhūto vāyuḥ/",
    "GK20bhasdutu:545": " atitapatyādityaḥ/ calitāḥ",
    "GK20bhasdutu:546": " parvatāḥ/",
    "GK20bhasdutu:547": " kṣubdhāḥ sāgarāḥ/",
    "GK20bhasdutu:548": " patitā vṛkṣāḥ/",
    "GK20bhasdutu:549": " bhrāntā meghāḥ/",
    "GK20bhasdutu:550": " pralīnā vāsukiprabhṛtayo",
    "GK20bhasdutu:551": " bhujaṃgeśvarāḥ/",
    "GK20bhasdutu:552": " kiṃ nu khalvidam/",
    "GK20bhasdutu:553": " aye ayaṃ bhagavato vāhanaḥ garuḍaḥ prāptaḥ/",
    "GK20bhasdutu:554": " surāsurāṇāṃ parikhedalabdhaṃ yenāmṛtaṃ mātṛvimokṣaṇārthaṃ/",
    "GK20bhasdutu:555": " ācchinnamāsīddviṣato",
    "GK20bhasdutu:556": " murārestvāmudvahāmīti varo.api dattaḥ// 53//",
    "GK20bhasdutu:557": " he kāśyapapriyasuta garuḍa praśāntaroṣo bhagavāndevadeveśaḥ/ gamyatāṃ",
    "GK20bhasdutu:558": " svanilayameva/ hanta",
    "GK20bhasdutu:559": " nivṛttaḥ/ yāvadgacchāmi/",
    "GK20bhasdutu:560": " ete sthitā viyati kiṃnarayakṣasiddhāḥ devāśca saṃbhramacalanmukuṭottamāṅgāḥ/",
    "GK20bhasdutu:561": " ruṣṭe.acyute",
    "GK20bhasdutu:562": " vigatakāntiguṇāḥ praśāntaṃ śrutvā śrayanti sadanāni nivṛttatāpāḥ// 54//",
    "GK20bhasdutu:563": " yāvadahamapi kāntāṃ meruguhāmeva yāsyāmi/",
    "GK20bhasdutu:564": " niṣkrāntaḥ/ vāsudevaḥ",
    "GK20bhasdutu:565": " yāvadahamapi pāṇḍavaśibirameva yāsyāmi/",
    "GK20bhasdutu:566": " nepathye/",
    "GK20bhasdutu:567": " na khalu na khalu gantavyam/",
    "GK20bhasdutu:568": " vāsudevaḥ",
    "GK20bhasdutu:569": " aye vṛddharājasvara iva/",
    "GK20bhasdutu:570": " bho rājan eṣa sthito.asmi/",
    "GK20bhasdutu:571": " tataḥ praviśati dhṛtarāṣṭraḥ/",
    "GK20bhasdutu:572": " dhṛtarāṣṭraḥ",
    "GK20bhasdutu:573": " kva nu khalu bhagavānnārāyaṇaḥ/",
    "GK20bhasdutu:574": " kva nu khalu bhagavānpāṇḍavaśreyaskaraḥ/ kva nu",
    "GK20bhasdutu:575": " khalu",
    "GK20bhasdutu:576": " bhagavānviprapriyaḥ/",
    "GK20bhasdutu:577": " kva nu khalu bhagavāndevakīnandanaḥ/",
    "GK20bhasdutu:578": " mama putrāparādhāttu śārṅgapāṇe tavādhunā/",
    "GK20bhasdutu:579": " etanme tridaśādhyākṣa pādayoḥ",
    "GK20bhasdutu:580": " patitaṃ",
    "GK20bhasdutu:581": " śiraḥ// 55// patati/ vāsudevaḥ",
    "GK20bhasdutu:582": " hā dhik/ patito.atrabhavān/",
    "GK20bhasdutu:583": " uttiṣṭhottiṣṭha/",
    "GK20bhasdutu:584": " dhṛtarāṣṭraḥ",
    "GK20bhasdutu:585": " anugṛhīto.asmi/",
    "GK20bhasdutu:586": " bhagavan idamarghyaṃ pādyaṃ ca pratigṛhyatām/",
    "GK20bhasdutu:587": " vāsudevaḥ",
    "GK20bhasdutu:588": " sarvaṃ gṛhṇāmi/",
    "GK20bhasdutu:589": " kiṃ te bhūyaḥ priyamupaharāmi/",
    "GK20bhasdutu:590": " dhṛtarāṣṭraḥ",
    "GK20bhasdutu:591": " yadi me bhagavānprasannaḥ kimataḥ paramicchāmi/",
    "GK20bhasdutu:592": " vāsudevaḥ",
    "GK20bhasdutu:593": " gacchatu bhavānpunardarśanāya/",
    "GK20bhasdutu:594": " dhṛtarāṣṭraḥ",
    "GK20bhasdutu:595": " yadājñāpayati bhagavannārāyaṇaḥ/",
    "GK20bhasdutu:596": " niṣkrāntaḥ/",
    "GK20bhasdutu:597": " bharatavākyam/",
    "GK20bhasdutu:598": " imāṃ sāgaraparyantāṃ himavadvindhyakuṇḍalām/",
    "GK20bhasdutu:599": " mahīmekātapatrāṅkāṃ rājasiṃhaḥ praśāstu naḥ// 56//",
    "GK20bhasdutu:600": " niṣkrāntāḥ sarve/",
    "GK20bhasdutu:601": " dūtavākyaṃ",
    "GK20bhasdutu:602": " samāptam/",
    "GK20bhasdutu:603": " ka iti/ tricaraṇātikrāntatriloko",
    "GK20bhasdutu:604": " nārāyaṇaḥ khalvatra bhagavān/",
    "GK20bhasdutu:605": " śaraṇaṃ vrajantu bhavantaḥ/ yāvadga"
}