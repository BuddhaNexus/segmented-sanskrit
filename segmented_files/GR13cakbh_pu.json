{
  "GR13cakbh_pu:0": "[oṃ] namaḥ sūryakalājāla- kāladehāpahāriṇe /",
  "GR13cakbh_pu:1": "ādhārādheyapīṭhāya bhavāyābhavadāyine // CakBhst_1 //",
  "GR13cakbh_pu:2": "namaḥ śiśuniśākānta- kalākamalamāline /",
  "GR13cakbh_pu:3": "paramānandadehāya śivāyāvyaktamūrtaye // CakBhst_2 //",
  "GR13cakbh_pu:4": "namaḥ pāśaughasaṃghaṭṭa- layaviśleṣakāriṇe /",
  "GR13cakbh_pu:5": "mantragarbhodarasthāya harāya paramātmane // CakBhst_3 //",
  "GR13cakbh_pu:6": "yadyapi tvaṃ guṇātīto vākpater apy agocaraḥ /",
  "GR13cakbh_pu:7": "tathāpy ā hṛdayāt stotum udyatā vāk sadā mama // CakBhst_4 //",
  "GR13cakbh_pu:8": "atibhaktirasāveśa- vivaśā viśvatomukha /",
  "GR13cakbh_pu:9": "tvatpreritā yato nātha nāparādho 'sti me kvacit // CakBhst_5 //",
  "GR13cakbh_pu:10": "tvatpādābjarajaḥpūta- citābhūtivibhūṣitāt /",
  "GR13cakbh_pu:11": "gṛhāṇa mattaḥ śrīkaṇṭha! bhāvapūjām akṛtrimām // CakBhst_6 //",
  "GR13cakbh_pu:12": "hṛtpuṣkarākhye snātvāntas tīrthe yoginiṣevite /",
  "GR13cakbh_pu:13": "saṃbodhadhautavasanaḥ śyāmakaṇṭhaṃ yajāmy aham // CakBhst_7 //",
  "GR13cakbh_pu:14": "prabhūtabhūtasaṃbhūta- śoṇitair arghabhājanam /",
  "GR13cakbh_pu:15": "kriyate te mahākāla- kāyakaṃkālakandale // CakBhst_8 //",
  "GR13cakbh_pu:16": "yad yad vikalpanājālaṃ bāhyārthapratipattaye /",
  "GR13cakbh_pu:17": "tat tad dvārādhipakulaṃ tava pūjyaṃ manasvibhiḥ // CakBhst_9 //",
  "GR13cakbh_pu:18": "aśeṣāvaraṇonmukta- guptasauṣuptamaṇḍale /",
  "GR13cakbh_pu:19": "pūjyase nagajākānta! grahaṇe candrasūryayoḥ // CakBhst_10 //",
  "GR13cakbh_pu:20": "hānādānodayārambha- sadasadvibhramojjhitam /",
  "GR13cakbh_pu:21": "jñaptimātrapratiṣṭhānam āsanaṃ trijagatpate // CakBhst_11 //",
  "GR13cakbh_pu:22": "mūrtidānam anābhāsa- śaktibhāsopabṛṃhitam /",
  "GR13cakbh_pu:23": "turyapīṭhapratiṣṭhānaṃ vitarāmi jagatpate // CakBhst_12 //",
  "GR13cakbh_pu:24": "dikkāladeśakalanā- kalaṅkojjhitacetasaḥ /",
  "GR13cakbh_pu:25": "kaḥ karoti budhaḥ sthāṇor āhvānādivisarjanam // CakBhst_13 //",
  "GR13cakbh_pu:26": "sūryaśītāṃunetrābhyāṃ mathitvā śaktivāridhim /",
  "GR13cakbh_pu:27": "parāmṛtarasābhyaṅgaṃ śivāya vinivedaye // CakBhst_14 //",
  "GR13cakbh_pu:28": "indriyeṣv indriyārtheṣu rāgalobhānuyāyiṣu /",
  "GR13cakbh_pu:29": "niḥsnehatvaṃ prabhutvaṃ ca rūkṣam udvartanaṃ tava // CakBhst_15 //",
  "GR13cakbh_pu:30": "saṃśodhyānacchakalayā baindhavaṃ tīrthamañjasā /",
  "GR13cakbh_pu:31": "tadvirāmarasasnānaṃ deyaṃ candrārdhamaulaye // CakBhst_16 //",
  "GR13cakbh_pu:32": "svaprabodhāmbarodāra- vikasadvastragharṣaṇam /",
  "GR13cakbh_pu:33": "viniveśya bhavatsaṃvid- vikārān vikarāmahe // CakBhst_17 //",
  "GR13cakbh_pu:34": "jñānajñeyaparityāga- naganirjharavisrutam /",
  "GR13cakbh_pu:35": "paramānandadaṃ me 'ntas tava pādyam anābilam // CakBhst_18 //",
  "GR13cakbh_pu:36": "amandānandaniḥṣyanda- spandamandirakandarāt /",
  "GR13cakbh_pu:37": "galatkaivalyasalilam ādatsvācamanaṃ vibho // CakBhst_19 //",
  "GR13cakbh_pu:38": "prakṣālya dhāraṇāmbhobhir granthipañcakamauktikam /",
  "GR13cakbh_pu:39": "anarghyam arghaṃ pādebhyaḥ prayacchāmi vṛṣākape // CakBhst_20 //",
  "GR13cakbh_pu:40": "turīyodyānavikasat- saṃvitsaurabhanirbharaiḥ /",
  "GR13cakbh_pu:41": "girīśa tisṛbhir vāgbhiḥ stutipuṣpaiḥ prapūjyase // CakBhst_21 //",
  "GR13cakbh_pu:42": "prāṇavahniśikhāstreṇa bhaṅktvā brahmabilaṃ lasat /",
  "GR13cakbh_pu:43": "dhūpavartinibhābhāso dhūpas te nagajādhava // CakBhst_22 //",
  "GR13cakbh_pu:44": "anāhatā nadaty antar yā gaur dhāmni pare parā /",
  "GR13cakbh_pu:45": "sā ghaṇṭā vādyate śambhor agre granthārthagarbhiṇī // CakBhst_23 //",
  "GR13cakbh_pu:46": "kandānalollasacchakti- śikhāṃ dīpaśikhopamām /",
  "GR13cakbh_pu:47": "uddīpayāmi bhagavan mohadhvāntāpanuttaye // CakBhst_24 //",
  "GR13cakbh_pu:48": "vṛttityāgāmṛtakalā- candraś candrakirīṭinaḥ /",
  "GR13cakbh_pu:49": "samālambhanavevāstu mumukṣūṇāṃ vimuktaye // CakBhst_25 //",
  "GR13cakbh_pu:50": "nādaśaktyuditaṃ dhāma malatrayavivarjitam /",
  "GR13cakbh_pu:51": "uṣṇīṣam astu te rudra vidrāvitanabho 'rgalam // CakBhst_26 //",
  "GR13cakbh_pu:52": "bhavadbhaktisudhāsāra- saṃplāvitahṛdambudheḥ /",
  "GR13cakbh_pu:53": "prollasadbrahmakamalam amalaṃ te 'stu śekharam // CakBhst_27 //",
  "GR13cakbh_pu:54": "bodhāravindasaṃdoho hara hāralatā tava /",
  "GR13cakbh_pu:55": "nityoditāntaravyāpti- kalā candrakalā ca te // CakBhst_28 //",
  "GR13cakbh_pu:56": "ākṛṣyārkakaraiḥ śāktair viśvākāśakuśeśayāt /",
  "GR13cakbh_pu:57": "ciccandrāmṛtaniḥṣyandam āsvādaya jagatpate // CakBhst_29 //",
  "GR13cakbh_pu:58": "mantraṃ manasi tacchaktau tāṃ svadhāmni sadodite /",
  "GR13cakbh_pu:59": "kṛtvā japavidhiṃ samyak śivāya vinivedaye // CakBhst_30 //",
  "GR13cakbh_pu:60": "niḥśeṣārthaparityāge grahaṇe vāmitād dyute /",
  "GR13cakbh_pu:61": "anābilaṃ parāṃ vyāptiṃ mudrāṃ badhnāmi dhūrjaṭe // CakBhst_31 //",
  "GR13cakbh_pu:62": "śaṣkulīkarṇayor baddhvā yo rāvo 'tra vijṛmbhate /",
  "GR13cakbh_pu:63": "tadgītam atha te vādyam ādyasaṃpuṭaghaṭṭanāt // CakBhst_32 //",
  "GR13cakbh_pu:64": "bhavadbhāvarasāveśāt tāṇḍavāḍambaroddhataḥ /",
  "GR13cakbh_pu:65": "mantrādhvani nadāmy antaḥ kim u bāhyārthabhāvanaiḥ // CakBhst_33 //",
  "GR13cakbh_pu:66": "akhaṇḍamaṇḍalākāra- śivatābhāvanāmbaram /",
  "GR13cakbh_pu:67": "upariṣṭān mayā śambhor vitānam upakalpitam // CakBhst_34 //",
  "GR13cakbh_pu:68": "recayitvā karandhreṇa bhācakraṃ hṛdayāmbarāt /",
  "GR13cakbh_pu:69": "dhāryate dhūrjaṭer agre cārucāmaravan mayā // CakBhst_35 //",
  "GR13cakbh_pu:70": "nāsāpuṭakuṭīkoṭi- visṛtaiḥ prasṛtaiḥ puraḥ /",
  "GR13cakbh_pu:71": "tālavṛntair iva vibho vījyase haṃsamārutaiḥ // CakBhst_36 //",
  "GR13cakbh_pu:72": "sarvātapavinirmuktaṃ bhavadbhaktisudhāsitam /",
  "GR13cakbh_pu:73": "ātapatram athāsmābhir dhāryate te mahāśaya // CakBhst_37 //",
  "GR13cakbh_pu:74": "mamatāmandurātyāgā- trailokyākarṣaṇakṣamaḥ /",
  "GR13cakbh_pu:75": "ahaṃkāraturaṅgas te vāhanāya prakalpitaḥ // CakBhst_38 //",
  "GR13cakbh_pu:76": "svasaṃvinnandanānanda- nāgavallīdalojjvalam /",
  "GR13cakbh_pu:77": "sphuratspandendusurabhi tāmbūlaṃ te nivedyate // CakBhst_39 //",
  "GR13cakbh_pu:78": "bhogyabhoktṛvibhedottha- vāsanendhanasaṃcayam /",
  "GR13cakbh_pu:79": "advaitāgnau juhomy antaḥ śāṃkare sreyasām nidhau // CakBhst_40 //",
  "GR13cakbh_pu:80": "prakāśākāśahastābhyām avalambyonmanāsrucam /",
  "GR13cakbh_pu:81": "dharmādharmagalatsneha- pūrṇāṃ vahnau juhomy aje // CakBhst_41 //",
  "GR13cakbh_pu:82": "yuṣmaddhyānaniśānātha- kiraṇair uparañjitaḥ /",
  "GR13cakbh_pu:83": "antarmānavidhau te 'stu cittacintāmaṇir mama // CakBhst_42 //",
  "GR13cakbh_pu:84": "mokṣalakṣmīkarāmbhoja- pātre vinyasya saṃskṛtām /",
  "GR13cakbh_pu:85": "nivedayāmi bhagavan prāṇasarvasvadakṣiṇām // CakBhst_43 //",
  "GR13cakbh_pu:86": "saṃsāravāridheḥ santaḥ paraṃ pāraṃ titīrṣavaḥ /",
  "GR13cakbh_pu:87": "candramauleḥ śrayantūccair bhāvapūjātaraṇḍakam // CakBhst_44 //",
  "GR13cakbh_pu:88": "ittham īśānacaraṇa- nuter yat samupārjitam /",
  "GR13cakbh_pu:89": "śreyas tena jagat kṛtsnaṃ dveṣadoṣād vimucyatām // CakBhst_45 //",
  "GR13cakbh_pu:90": "stotraṃ gṛhāṇa parameśvara viśvasākṣin mānāvamānamatojjhitacittavṛtteḥ /",
  "GR13cakbh_pu:91": "mattas tvadaṅghriyugapīṭhanilīnamauler bhasmāvaguṇṭhitatanor nṛkapālapāṇeḥ // CakBhst_46 //",
  "GR13cakbh_pu:92": "mahāpāśupatodyāne kaścid viśramya tāpasaḥ /",
  "GR13cakbh_pu:93": "cakre ciccakranāthasya śambhor bhāvanutiṃ parām // CakBhst_47 //"
}