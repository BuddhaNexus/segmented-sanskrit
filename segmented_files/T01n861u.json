{
    "T01n861u_001:0": " pañcarakṣādevīstotrāṇi",
    "T01n861u_001:1": " 1 mahāpratisarāstotram",
    "T01n861u_001:2": " om namaḥ śrīmahāpratisarāyai",
    "T01n861u_001:3": " tathāgatādyāstathatāṃ tattvamāpurmahattaram |",
    "T01n861u_001:4": " dhāraṇīdhāraṇādyasyāḥ pratisarāṃ namāmi tām || 1 ||",
    "T01n861u_001:5": " raṇe śakro'jayaddaityān dhāraṇīdhvajadhṛg bahūn |",
    "T01n861u_001:6": " saṃgrāmajayadāṃ bhīmāṃ pratisarāṃ namāmi tām || 2 ||",
    "T01n861u_001:7": " yatprabhāvād brahmadatto'labhad rājyamakaṇṭakam |",
    "T01n861u_001:8": " sārvabhaumapradāṃ devīṃ pratisarāṃ namāmi tām || 3 ||",
    "T01n861u_001:9": " bahvaparādho'pi yadbhakto rājyādhikāramāptavān |",
    "T01n861u_001:10": " śastrādibhītisaṃhatrīṃ pratisarāṃ namāmi tām || 4 ||",
    "T01n861u_001:11": " ratnānyavāpurvaṇijo yāṃ smṛtvodadhinirgatāḥ |",
    "T01n861u_001:12": " sarvabādhāpraśamanīṃ pratisarāṃ namāmi tām || 5 ||",
    "T01n861u_001:13": " śrīmahāpratisarārakṣādevīstotraṃ samāptam |",
    "T01n861u_001:14": " 2 mahāmantrānusāriṇīstotram",
    "T01n861u_001:15": " om namaḥ śrīmahāmantrānusāriṇyai",
    "T01n861u_001:16": " buddhādhiṣṭhānato buddhābhayadāṃ bhayanāśinīm |",
    "T01n861u_001:17": " bhavāmbudhinimagnānāṃ namo mantrānusāriṇīm || 1 ||",
    "T01n861u_001:18": " yanmantroccāraṇādeva ṣaḍītayaḥ sudāruṇāḥ |",
    "T01n861u_001:19": " nāśaṃ prayānti varadāṃ namo mantrāmusāriṇīm || 2 ||",
    "T01n861u_001:20": " mantrānusāriṇo lokān nānye mantrādayo grahāḥ |",
    "T01n861u_001:21": " pīḍayanti priyāṃścāpi namo mantrānusāriṇīm || 3 ||",
    "T01n861u_001:22": " buddho'bhyabhāṣad gāthāstā yanmantrakathanāntaram |",
    "T01n861u_001:23": " yābhiḥ sarvatra svasti syānnamo mantrānusāriṇīm || 4 ||",
    "T01n861u_001:24": " kalau buddhavihīne'smin lokānāṃ hitamācaret |",
    "T01n861u_001:25": " pāpotpātapraśamanīṃ namo mantrānusāriṇīm || 5 ||",
    "T01n861u_001:26": " śrīmahāmantrānusāriṇīstotraṃ samāptam |",
    "T01n861u_001:27": " 3 mahāmāyūrīstotram",
    "T01n861u_001:28": " om namaḥ śrīmahāmāyūrīryai",
    "T01n861u_001:29": " duṣṭaṃ kṛṣṇabhujaṅgaṃ ca naraḥ svāntikaṃ pālayet |",
    "T01n861u_001:30": " yasyā mantrānubhāvena māyūrīṃ praṇamāmi tām || 1 ||",
    "T01n861u_001:31": " brahmādayo lokapālā yaddhāraṇyā samāpnuvan |",
    "T01n861u_001:32": " svāni svānyadhikārāṇi māyūrīṃ praṇamāmi tām || 2 ||",
    "T01n861u_001:33": " svarṇāvabhāsaṃ śikhinaṃ nālabhajjapinaṃ kudhīḥ |",
    "T01n861u_001:34": " amoghenāpi pāśena māyūrīṃ praṇamāmi tām || 3 ||",
    "T01n861u_001:35": " yanmantrajapato jīvāḥ prājīvañchuṣkapādapāḥ |",
    "T01n861u_001:36": " mṛtasaṃjīvinīṃ devīṃ māyūrīṃ praṇamāmi tām || 4 ||",
    "T01n861u_001:37": " yanmantrisaṅgāt pavano mahopadravaśāntikṛt |",
    "T01n861u_001:38": " buddhānāṃ bodhidāṃ nityaṃ māyūrīṃ praṇamāmi tām || 5 ||",
    "T01n861u_001:39": " śrīmahāmāyūrīrakṣādevīstotraṃ samāptam |",
    "T01n861u_001:40": " 4 mahāśītavatīrakṣādevīstotram",
    "T01n861u_001:41": " om namo mahāśītavatyai",
    "T01n861u_001:42": " yaddhāraṇīmanujapan rāhulo bhadramāptavān |",
    "T01n861u_001:43": " viheṭhito grahaiḥ sarvaiḥ śītavatīṃ namāmyaham || 1 ||",
    "T01n861u_001:44": " pāpatāpe śītakarīṃ śītalādyupasargataḥ |",
    "T01n861u_001:45": " śītoṣṇaduḥkhaśamanīṃ śītavatīṃ namāmyaham || 2 ||",
    "T01n861u_001:46": " mantragranthitasūtrāṇāṃ dhāraṇāllakṣayojanam |",
    "T01n861u_001:47": " pathikānāṃ pālayitrīṃ śītavatīṃ namāmyaham || 3 ||",
    "T01n861u_001:48": " śmaśānasthena muninā yā samuccāritā purā |",
    "T01n861u_001:49": " grahopadravaśāntyarthaṃ śītavatīṃ namāmyaham || 4 ||",
    "T01n861u_001:50": " grahābhibhūtavātānāṃ granthipadavidhāriṇām |",
    "T01n861u_001:51": " grahabhītipraśamanīṃ śītavatīṃ namāmyaham || 5 ||",
    "T01n861u_001:52": " śrī mahāśītavatīrakṣādevīstotraṃ samāptam |",
    "T01n861u_001:53": " 5 mahāsāhasrapramardinīstotram",
    "T01n861u_001:54": " om namaḥ śrīmahāsāhasrapramardinye",
    "T01n861u_001:55": " mahāsāhasrike loke sāhasrahitakāriṇām |",
    "T01n861u_001:56": " sahasrasattvajananīṃ naumi sāhasramardinīm || 1 ||",
    "T01n861u_001:57": " sopadravāyāṃ vaiśālyāṃ mahotsavo yataḥ sadā |",
    "T01n861u_001:58": " mahopasargaśamanīṃ naumi sāhasramardinīm || 2 ||",
    "T01n861u_001:59": " yakṣarākṣasabhūtānāṃ damanīṃ duṣṭacetasām |",
    "T01n861u_001:60": " duritopadravahatāṃ naumi sāhasramardinīm || 3 ||",
    "T01n861u_001:61": " yaddhāraṇīpaṭhanato rakṣitaḥ śākyakeśarī |",
    "T01n861u_001:62": " viṣato viṣadigdhāṃ tāṃ naumi sāhasramardinīm || 4 ||",
    "T01n861u_001:63": " madhumiśritabhaiṣajyaṃ sarvaroganivāraṇam |",
    "T01n861u_001:64": " mṛtasañjīvanaṃ loke naumi sāhasramardinīm || 5 ||",
    "T01n861u_001:65": " śrīmahāsāhasramardinīrakṣādevīstotraṃ samāptam |"
}