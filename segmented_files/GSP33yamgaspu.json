{
  "GSP33yamgaspu:0": "svadharmajñānavairāgyasādhyabhaktyekagocaraḥ /",
  "GSP33yamgaspu:1": "nārāyaṇaḥ paraṃ brahma gītāśāstre samīritaḥ // YGas_1 //",
  "GSP33yamgaspu:2": "jñānakarmātmike niṣṭhe yogalakṣe susaṃskṛte /",
  "GSP33yamgaspu:3": "ātmānubhūtisiddhyārthe pūrvaṣaṭkena coditaḥ // YGas_2 //",
  "GSP33yamgaspu:4": "madhyame bhagavattattvayāthātmyāvāptisiddhaye /",
  "GSP33yamgaspu:5": "jñānakarmābhinirvartyo bhaktiyogaḥ prakīrtitaḥ // YGas_3 //",
  "GSP33yamgaspu:6": "pradhānapuruṣavyaktasarveśvaravivecanam /",
  "GSP33yamgaspu:7": "karma dhīr bhaktir ityādipūrvaśeṣo 'ntimoditaḥ // YGas_4 //",
  "GSP33yamgaspu:8": "asthānasnehakāruṇyadharmādharmadhiyākulam /",
  "GSP33yamgaspu:9": "pārthaṃ prapannam uddiśya śāstrāvatraṇaṃ kṛtam // YGas_5 //",
  "GSP33yamgaspu:10": "nityātmāsaṅgakarmehagocarā sāṃkhyayogadhīḥ /",
  "GSP33yamgaspu:11": "dvitīye sthitadhīlakṣyā proktā tanmohaśāntaye // YGas_6 //",
  "GSP33yamgaspu:12": "asaktyā lokarakṣāyai guṇeṣv āropya kartrktām /",
  "GSP33yamgaspu:13": "sarveśvare vā nyasyoktā tṛtīye karmakāryatā // YGas_7 //",
  "GSP33yamgaspu:14": "prasaṅgāt svasvabhāvoktiḥ karmaṇo 'karmatāsya ca /",
  "GSP33yamgaspu:15": "bhedā jñānasya māhātmyam caturthādhyāya ucyate // YGas_8 //",
  "GSP33yamgaspu:16": "karmayogasya saukaryaṃ śaighryaṃ kāścana tadvidhāḥ /",
  "GSP33yamgaspu:17": "brahmajñānprakāraś ca pañcamādhyāya ucyate // YGas_9 //",
  "GSP33yamgaspu:18": "yogābhyāsavidhir yogī caturdhā yogasādhanam /",
  "GSP33yamgaspu:19": "yogasiddhaḥ svayogasya pāramyaṃ ṣaṣṭha ucyate // YGas_10 //",
  "GSP33yamgaspu:20": "svayāthātmyaṃ prakṛtyāsya tirodhiḥ śaraṇāgatiḥ /",
  "GSP33yamgaspu:21": "bhaktabhedaḥ prabuddhasya śraiṣṭhyaṃ saptama ucyate // YGas_11 //",
  "GSP33yamgaspu:22": "aiśvaryākṣarayāthātmyaṃ bhagavaccaraṇārthiṇām /",
  "GSP33yamgaspu:23": "vedyopādeyabhāvānām aṣṭame bhede ucyate // YGas_12 //",
  "GSP33yamgaspu:24": "svamāhātmyaṃ manuṣyatve paratvaṃ ca mahātmanām /",
  "GSP33yamgaspu:25": "viśeṣo navame yogo bhaktirūpaḥ prakīrtitaḥ // YGas_13 //",
  "GSP33yamgaspu:26": "svakalyāṇaguṇānantyakṛtsnasvādhīnatāmatiḥ /",
  "GSP33yamgaspu:27": "bhaktyutpattivivṛdhyarthā vistīrṇā daśamoditā // YGas_14 //",
  "GSP33yamgaspu:28": "ekādaśe 'sya yāthātmyasākṣātkārāvalokanam /",
  "GSP33yamgaspu:29": "dattam uktā vidiprāptyor bhaktyekopāyatā tathā // YGas_15 //",
  "GSP33yamgaspu:30": "bhaktiśraiṣṭhyam upāyoktir aśaktasyātmaniṣṭhatā /",
  "GSP33yamgaspu:31": "tatprakārās tv atiprītir bhakte dvādaśa ucyate // YGas_16 //",
  "GSP33yamgaspu:32": "dehasvarūpam ātmāptihetur ātmaviśodhanam /",
  "GSP33yamgaspu:33": "bandhahetur vivekaś ca trayodaśa udīryate // YGas_17 //",
  "GSP33yamgaspu:34": "guṇabandhavidhau teṣāṃ kartṛtvam tannivartanam /",
  "GSP33yamgaspu:35": "gatitrayasvamūlatvaṃ caturdaśa udīryate // YGas_18 //",
  "GSP33yamgaspu:36": "acinmiśrād viśuddhāc ca cetanāt puruṣottamaḥ /",
  "GSP33yamgaspu:37": "vyāpānād bharaṇāt svāmyād anyaḥ pañcadaśoditaḥ // YGas_19 //",
  "GSP33yamgaspu:38": "devāsuravibhāgoktipūrvikā śāstravaśyatā /",
  "GSP33yamgaspu:39": "tattvānuṣṭhānavijñānasthemne ṣoḍaśa ucyate // YGas_20 //",
  "GSP33yamgaspu:40": "aśāstram āsuraṃ kṛtsnam śāstrīyaṃ guṇataḥ pṛthak /",
  "GSP33yamgaspu:41": "lakṣaṇaṃ śāstrasiddhasya tridhā saptadaśoditam // YGas_21 //",
  "GSP33yamgaspu:42": "īśvare kartṛtābuddhiḥ sattvopādeyatāntime /",
  "GSP33yamgaspu:43": "svakarmapariṇāmaś ca śāstrasārārtha ucyate // YGas_22 //",
  "GSP33yamgaspu:44": "karmayogas tapastīrthadānayajñādisevanam /",
  "GSP33yamgaspu:45": "jñānayogo jitasvāntaiḥ pariśuddhātmani sthitiḥ // YGas_23 //",
  "GSP33yamgaspu:46": "bhaktiyogaḥ parāikāntyaprītyā dhyānādiṣu sthitiḥ /",
  "GSP33yamgaspu:47": "trayānām api yogānāṃ tribhir anyonyasaṃgamaḥ // YGas_24 //",
  "GSP33yamgaspu:48": "nityanaimittikānāṃ ca parārādhanarūpiṇam /",
  "GSP33yamgaspu:49": "ātmaṛṣṭes trayo 'py ete yogadvāreṇa sādhakāḥ // YGas_25 //",
  "GSP33yamgaspu:50": "nirastanikhilājñāno dṛṣṭvātmānaṃ parānugam /",
  "GSP33yamgaspu:51": "pratilabhya parāṃ bhaktiṃ tayaivāvāpnoti tatpadam // YGas_26 //",
  "GSP33yamgaspu:52": "bhaktiyogas tadarthī cet samagrāiśvaryasādhanam /",
  "GSP33yamgaspu:53": "ātmārthī cet trayo 'py ete tatkaivalyasya sādhakāḥ // YGas_27 //",
  "GSP33yamgaspu:54": "aikāntyaṃ bhagavaty eṣāṃ samānam adhikāriṇām /",
  "GSP33yamgaspu:55": "yāvatprāpti parārthī cet tad evātyantam aśunute // YGas_28 //",
  "GSP33yamgaspu:56": "jñānī tu paramaikāntī tadāyattātmajīvanaḥ /",
  "GSP33yamgaspu:57": "tatsaṃśleṣaviyogaikasukhaduḥkhas tadekadhīḥ // YGas_29 //",
  "GSP33yamgaspu:58": "bhagavaddhyānayogoktivandanastutikīrtanaiḥ /",
  "GSP33yamgaspu:59": "labdhātmā tadgataprāṇamanobuddhīndriyakriyaḥ // YGas_30 //",
  "GSP33yamgaspu:60": "nijakarmādibhaktyantaṃ kuryāt prītyaiva kāritaḥ /",
  "GSP33yamgaspu:61": "upāyatāṃ parityajya nyasyed deve tu tām abhīḥ // YGas_31 //",
  "GSP33yamgaspu:62": "aikāntyātyantadāsyaikaratis tatpadam āpnuyāt /",
  "GSP33yamgaspu:63": "tatpradhānam idaṃ śāstram iti gītārthasaṃgrahaṃ // YGas_32 //"
}