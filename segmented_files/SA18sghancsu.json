{
  "SA18sghancsu:0": "śrīghanācārasaṃgrahaḥ",
  "SA18sghancsu:1": "prathamo 'dhyāyaḥ",
  "SA18sghancsu:2": "buddhanamaskāraḥ",
  "SA18sghancsu:3": "prahīṇāśeṣasaṃkleśajñeyāmbhonidhipāragaṃ /",
  "SA18sghancsu:4": "praṇamya śrāmaṇerāṇāmācāraḥ saṃgrahīṣyate // Śghs_1.1 //",
  "SA18sghancsu:5": "iti saṃghasenena saṃvihitoddhāre śrīghanācārasaṃgrahe buddhanamaskāro nāma prathamo 'dhyāyaḥ //",
  "SA18sghancsu:6": "dvitīyo 'dhyāyaḥ",
  "SA18sghancsu:7": "śrāmaṇerabhāvaḥ",
  "SA18sghancsu:8": "śaraṇaṃ gatavān buddhaṃ dharmaṃ saṃghaṃ ca bhaktitaḥ /",
  "SA18sghancsu:9": "śikṣāpañcakamādāya yo 'nupravrajito jinaṃ // Śghs_2.1 //",
  "SA18sghancsu:10": "yāvajjīvaṃ vadhasteyamaithunānṛtavākyataḥ /",
  "SA18sghancsu:11": "virataḥ madyapānācca mahoccaiḥ śayanāsanāt // Śghs_2.2 //",
  "SA18sghancsu:12": "nṛtyāder gandhamālyādyāt vikālāśanatastathā /",
  "SA18sghancsu:13": "rukmarūpyagrahāccaiṣa śrāmaṇero munermataḥ / Śghs_2.3 //",
  "SA18sghancsu:14": "iti saṃghasenena saṃvihitoddhāre śrīghanācārasaṃgrahe śrāmaṇerabhāvo nāma dvitīyo 'dhyāyaḥ //",
  "SA18sghancsu:15": "tṛtīyo 'dhyāyaḥ",
  "SA18sghancsu:16": "śrāmaṇerabhāvopāyaḥ",
  "SA18sghancsu:17": "1 prāṇivadhaviratiśikṣāpadaṃ",
  "SA18sghancsu:18": "saṃvaratyāgaḥ",
  "SA18sghancsu:19": "sakalena śarīreṇa śarīrānyatareṇa vā /",
  "SA18sghancsu:20": "pañcaśākhādinirvṛttaṃ na hanyāt prāṇinaṃ [yatiḥ] // Śghs_3,1.1 //",
  "SA18sghancsu:21": "mṛtyorvarṇa na bhāṣeta śastrahāraṃ na caiṣayet /",
  "SA18sghancsu:22": "vadhakena na cittena māraṇāya niyojayet // Śghs_3,1.2 //",
  "SA18sghancsu:23": "virekavamanālepaviṣaśastrābhisaṃskṛti /",
  "SA18sghancsu:24": "na kuryāt prāṇighātāya na ca garbhasya śātanaṃ // Śghs_3,1.3 //",
  "SA18sghancsu:25": "nimittaṃ na vratī kuryāt svapnolūkādivāśitaiḥ /",
  "SA18sghancsu:26": "[dauḥśīlaṃ] (vāpi kāryaṃ na)",
  "SA18sghancsu:27": "dānaśīlaphalāptibhiḥ // Śghs_3,1.4 //",
  "SA18sghancsu:28": "madhyapaścātpurogeṣu yatraiva vadhakaṃ manaḥ /",
  "SA18sghancsu:29": "[taṃ puruṣaṃ] dhnato hiṃsā nānyamityāha sarvavit // Śghs_3,1.5 //",
  "SA18sghancsu:30": "kūṭāvamārthāvaṣṭambhavetālā[dya]bhisaṃskṛti /",
  "SA18sghancsu:31": "avapayañca kuryānna māraṇāya śarīriṇāṃ // Śghs_3,1.6 //",
  "SA18sghancsu:32": "taskarā[dya]hisenābhyaḥ yatra mārge bhayaṃ[bhavet] /",
  "SA18sghancsu:33": "tatpanthānaṃ na nāśāya dehināṃ kathayedyatiḥ // Śghs_3,1.7 //",
  "SA18sghancsu:34": "bhujagāvartakābhyāṃ ca yatra tīrthe[bhayaṃ bhavet] /",
  "SA18sghancsu:35": "[nadīprataraṇārthāya] tattīrthaṃ na prakāśayet // Śghs_3,1.8 //",
  "SA18sghancsu:36": "kiñcid bhūpataye mā dāstvaṃ varaṃ jīvitaṃ tyaja /",
  "SA18sghancsu:37": "iti baddhāyukte 'yuktaṃ vacanaṃ na vaded [yatiḥ] // Śghs_3,1.9 //",
  "SA18sghancsu:38": "bhujaga [vyāḍayakṣe]bhyaḥ yadvihāre[bhayaṃ bhavet] /",
  "SA18sghancsu:39": "tadvadhakābhiprāyeṇāgantukāya na coddiśet // Śghs_3,1.10 //",
  "SA18sghancsu:40": "vyāghrādivyapadeśena śatruṃ hanyād hi yaḥ śaṭhaḥ /",
  "SA18sghancsu:41": "vihārādāśu[nirdvāraṃ] ghaṭṭanīyaḥ sa cellakaiḥ // Śghs_3,1.11 //",
  "SA18sghancsu:42": "tathā kāryaṃ yathaiko 'pi nānirjīvecca kaścana /",
  "SA18sghancsu:43": "adharmyā na kriyā kāryā saṃsmṛtya krakacopamaṃ // Śghs_3,1.12 //",
  "SA18sghancsu:44": "vadhaśca śrāmaṇerā ye ghātayituṃ vadhīkṛtāḥ /",
  "SA18sghancsu:45": "yugapat tatprayoge te nāśanīyā vihārataḥ // Śghs_3,1.13 //",
  "SA18sghancsu:46": "upakramo nṛsaṃjñā ca naro vadhakacetanā /",
  "SA18sghancsu:47": "jīvitasya kṣayaśceti pañcāṃgāni nṛghātane // Śghs_3,1.14 //",
  "SA18sghancsu:48": "śrāmaṇereṇa nāśāya viṣaśastrādi yat[kṛtaṃ] /",
  "SA18sghancsu:49": "mriyate narastenaivāsau tadāsaṃvarī bhavet // Śghs_3,1.15 //",
  "SA18sghancsu:50": "avakrītādirajñānāt manuṣyaṃ mārayan khalu /",
  "SA18sghancsu:51": "saṃvarakṣobhalābhī sa [yatir] nonmattādikaḥ // Śghs_3,1.16 //",
  "SA18sghancsu:52": "duṣkṛtāni",
  "SA18sghancsu:53": "kāyamaitrādi sattveṣu yatinā kāryaṃ sarvadā /",
  "SA18sghancsu:54": "yasmāt kāruṇikairuktaṃ tasmāt[sattvānna pīḍayet] // Śghs_3,1.17 //",
  "SA18sghancsu:55": "na sattvān pīḍayeddārurajvayaścarmabandhanaiḥ /",
  "SA18sghancsu:56": "jalānalapraveśānutyāgādau na niyojayet // Śghs_3,1.18 //",
  "SA18sghancsu:57": "kṛmigorūpa[kādīnāṃ] mithyājīvaśca hiṃsanaṃ /",
  "SA18sghancsu:58": "karṣaṇe[hi] bhavedyasmāt tasmāt[tad]dūrato tyajet // Śghs_3,1.19 //",
  "SA18sghancsu:59": "nāṃkayed gomahiṣyādi nāsikāṃ nāsya vedhayet /",
  "SA18sghancsu:60": "[yatiśca vividhān sattvān] vetrādinā na tāḍayet // Śghs_3,1.20 //",
  "SA18sghancsu:61": "uddiśya yat kṛtaṃ māṃsavyañjanaṃ [khādato] yateḥ /",
  "SA18sghancsu:62": "nighṛṇaṃ jāyate cittaṃ tasmāttannaiva bhakṣayet // Śghs_3,1.21 //",
  "SA18sghancsu:63": "jñeyaṃ tu tribhirākārairīkṣitaśrutaśaṃkitaiḥ /",
  "SA18sghancsu:64": "īkṣitaṃ yat svayaṃ dṛṣṭaṃ śrutaṃ pratyāyitoditaṃ // Śghs_3,1.22 //",
  "SA18sghancsu:65": "gṛhābaddhamṛgādīnāṃ śrṛṃgapakṣādidarśanāt /",
  "SA18sghancsu:66": "āśaṃkotpadyate [tatra] mamoddiśya hatā na vā // Śghs_3,1.23 //",
  "SA18sghancsu:67": "pṛṣṭo dātā vadedevaṃ tvannimittamamī hatā /",
  "SA18sghancsu:68": "tasmin gṛhe na bhuñjīta virataḥ prāṇināṃ vadhāt // Śghs_3,1.24 //",
  "SA18sghancsu:69": "śyenakādihatā vai[te] śāliyavamudgabhakṣaṇāt /",
  "SA18sghancsu:70": "mātāpitrośca yajñasya kṛte doṣo 'sti nāśnatāṃ // Śghs_3,1.25 //",
  "SA18sghancsu:71": "ekaṃ śrīghanamuddiśya māṃsaṃ matsyādikaṃ kṛtaṃ /",
  "SA18sghancsu:72": "anyeṣāṃ śrāmaṇerāṇāṃ tadbhogāya na kalpate // Śghs_3,1.26 //",
  "SA18sghancsu:73": "nādeyādijalaṃ jānan nāpi pibet sajantukaṃ /",
  "SA18sghancsu:74": "jantavo 'bhimatāḥ sūkṣmās trasikormikirādayaḥ // Śghs_3,1.27 //",
  "SA18sghancsu:75": "vāri[pūtyā]yadhiṣṭheyaḥ karako [hi] sakhallakaḥ /",
  "SA18sghancsu:76": "vighātaparihārārthaṃ kośo 'ntaśaḥ suvāsasaḥ // Śghs_3,1.28 //",
  "SA18sghancsu:77": "divyanetrāndhayorneṣṭaṃ jalasya pratyavekṣaṇaṃ /",
  "SA18sghancsu:78": "hastāṃguṣṭhāgrakāvartadarśino na nivāryate // Śghs_3,1.29 //",
  "SA18sghancsu:79": "locanavibhramo mā bhūditi paśyenna tacciraṃ /",
  "SA18sghancsu:80": "kalpamātraṃ na vai kuryāt māyāśāṭhayasamanvitaḥ // Śghs_3,1.30 //",
  "SA18sghancsu:81": "yenāntareṇa ṣaṣṭayabdaḥ paribhramyate kuñjaraḥ /",
  "SA18sghancsu:82": "śakaṭaṃ vaṃśapūrṇaṃ vā sa kālaḥ salilekṣaṇe // Śghs_3,1.31 //",
  "SA18sghancsu:83": "ūrdhvādhomadhyamāstistraḥ salilasyāpi bhūmayaḥ /",
  "SA18sghancsu:84": "yāyā bhūḥ śuddhimāyāti kāryaṃ kuryāttayā[yatiḥ] // Śghs_3,1.32 //",
  "SA18sghancsu:85": "yatrādhaḥ prāṇakā yānti sthūlāḥ karatalāhatāḥ /",
  "SA18sghancsu:86": "[grāhyaṃ] tadudakaṃ [srāvyaṃ] paṃcāṃgulikayā na cet // Śghs_3,1.33 //",
  "SA18sghancsu:87": "bhaveyurudake yasmin kare lagnā[śca] prāṇinaḥ /",
  "SA18sghancsu:88": "na tatra pāṇipādāsyavastrādi kṣālayedyatiḥ // Śghs_3,1.34 //",
  "SA18sghancsu:89": "kule gatena praṣṭavyaṃ jalaṃ [hi] srāvitaṃ na vā /",
  "SA18sghancsu:90": "neti cet saṃjñiko vācyaḥ srāvayetyitaro na tu // Śghs_3,1.35 //",
  "SA18sghancsu:91": "[kintu] srāvyaṃ jalaṃ pātre kṛtvātmīye sajantukaṃ /",
  "SA18sghancsu:92": "[pṛṣṭvā] yastadānītaṃ muñcet tatraiva [sa] svayaṃ // Śghs_3,1.36 //",
  "SA18sghancsu:93": "saptāhaṃ vā[tathā] yatra pānīyamavatiṣṭhati /",
  "SA18sghancsu:94": "vihāre vā kaṭāhādau sthāpayet tadaśuṣyati // Śghs_3,1.37 //",
  "SA18sghancsu:95": "pravṛṣṭe vā [yatir] deve svacchanīraughavāhini /",
  "SA18sghancsu:96": "nimnagānāṃ patiṃ pātaṃ gacchateti visarjayet // Śghs_3,1.38 //",
  "SA18sghancsu:97": "sudīrghatalikāvaktrarajjudvayā[va]baddhayā /",
  "SA18sghancsu:98": "ghaṭikayodakaṃ muñcedadhastādudake 'dhvagaḥ // Śghs_3,1.39 //",
  "SA18sghancsu:99": "ghaṭikātritayaṃ vīkṣya mārgageṇodapānataḥ /",
  "SA18sghancsu:100": "pariśuddhāścet peyaṃ jalaṃ tat srāvyamanyathā / Śghs_3,1.40 //",
  "SA18sghancsu:101": "na rajjughaṭikādānaṃ śastaṃ saprāṇake jale /",
  "SA18sghancsu:102": "pibeti vaktuṃ dātuṃ vā na jātu kṣamate yatiḥ // Śghs_3,1.41 //",
  "SA18sghancsu:103": "sajantukaṃ taḍāgādi na pareṣāmuccairvadet /",
  "SA18sghancsu:104": "[kṛtyaṃ] kuru samīkṣyeti pṛṣṭaḥ prativadetparān // Śghs_3,1.42 //",
  "SA18sghancsu:105": "upādhyāye 'pi vaktavyaṃ saprāṇakamidaṃ na vā /",
  "SA18sghancsu:106": "taḍāgādīti [vai] śāṭhayaṃ karttavyaṃ [yatinā] na [tu] // Śghs_3,1.43 //",
  "SA18sghancsu:107": "vṛkṣaseko na dātavyo 'pariśuddhena vāriṇā /",
  "SA18sghancsu:108": "upādhyāyādikasnānaṃ karttavyaṃ ca tathaiva na // Śghs_3,1.44 //",
  "SA18sghancsu:109": "mastukāñjikasauvīramaṇḍadaṇḍāhatādikaṃ /",
  "SA18sghancsu:110": "na pibet na yatirdadyāt parasmai na bhuvi tyajet // Śghs_3,1.45 //",
  "SA18sghancsu:111": "samatkuṇaṃ yacchayanamātape tanna śoṣayet /",
  "SA18sghancsu:112": "na hime kardame vā[pi] śītoṣṇavāriṇi kṣipet // Śghs_3,1.46 //",
  "SA18sghancsu:113": "navakarma sa no kuryāt sajantukena [vāriṇā] /",
  "SA18sghancsu:114": "dināntaṃ srāvayitvā tad dhāryaṃ [trikoṇa]khallakaṃ // Śghs_3,1.47 //",
  "SA18sghancsu:115": "bhṛtakaṃ śikṣayedevaṃ vadhāna tvaṃ trikoṇakaṃ /",
  "SA18sghancsu:116": "kapāt[tat] srāvayotkṣipya khallakaṃ ca vimocaya // Śghs_3,1.48 //",
  "SA18sghancsu:117": "trikoṇasaptakenāmbhaḥ srāvyaṃ khallakena tat /",
  "SA18sghancsu:118": "[pratikūpyatha] kāryā[vā] tyaktvā deśāntaraṃ [vrajet] // Śghs_3,1.49 //",
  "SA18sghancsu:119": "abhūtvā sambhavantyete bhūtvā yānti bhavāntaraṃ /",
  "SA18sghancsu:120": "navakarma tadā kārya yathoktavidhikāriṇā // Śghs_3,1.50 //",
  "SA18sghancsu:121": "satpaṭābaddhamañcāderucchīdaccaturāṃgulāt /",
  "SA18sghancsu:122": "nadyādau jalamālokya kāryaṃ ca navakarmaṇi // Śghs_3,1.51 //",
  "SA18sghancsu:123": "chidrakarṇadvayāsaktasūtrabaddhadvidaṇḍakaṃ /",
  "SA18sghancsu:124": "dhāraṇapātrakaṃ dadhyāt khallakasya bahir[yatiḥ] // Śghs_3,1.52 //",
  "SA18sghancsu:125": "kṛpāparītacittena svādu vāri sajantukaṃ /",
  "SA18sghancsu:126": "kṣāre 'mbhasi na moktavyamanāpat mṛṣṭasaṃjñinaḥ // Śghs_3,1.53 //",
  "SA18sghancsu:127": "2 adattādānaviratiśikṣāpadaṃ",
  "SA18sghancsu:128": "saṃvaratyāgaḥ",
  "SA18sghancsu:129": "kālikaṃ [yāmikaṃ] yāvajjīvikaṃ sarpirādikaṃ /",
  "SA18sghancsu:130": "pārihāryamanādheyaṃ kalpyākalpyamakalpikaṃ // Śghs_3,2.1 //",
  "SA18sghancsu:131": "pṛthivyudakadvicatuṣpadāpadakapādapa /",
  "SA18sghancsu:132": "tadgataṃ ca yatiḥ kiñcinnādadīta parasvakaṃ // Śghs_3,2.2 //",
  "SA18sghancsu:133": "kārṣāpaṇacaturbhāgaṃ tadardhamathavā dhanaṃ /",
  "SA18sghancsu:134": "steyacitto harannanyair nāśanīyo[yatir] laghu // Śghs_3,2.3 //",
  "SA18sghancsu:135": "vāṭāddhṛte gaje pādaiś caturbhirayatiś cyute /",
  "SA18sghancsu:136": "niṣkrānte saṃvṛtadvārād dvipahṛtsyād yatiḥ [khalu] // Śghs_3,2.4 //",
  "SA18sghancsu:137": "gulphābaddhekṣadaṇḍāre dūrākṛṣṭe mahītalāt /",
  "SA18sghancsu:138": "saṃvarabhaṃga utkṣipte pañjarādgajavaddvije // Śghs_3,2.5 //",
  "SA18sghancsu:139": "deśānnayed hi sandeśadraviṇaṃ ca sañcārayet /",
  "SA18sghancsu:140": "aṃgāt stainyena cittena saṃvaraghūtamāpnute // Śghs_3,2.6 //",
  "SA18sghancsu:141": "mṛto 'sau matvā śrutvaivādhvagād vai brahmacāriṇaḥ /",
  "SA18sghancsu:142": "sīmāntasthāyine datvā gṛhṇan syādayatiryatiḥ // Śghs_3,2.7 //",
  "SA18sghancsu:143": "yadvittaṃ śrāmaṇerasya mṛtasya nirvṛtārhataḥ /",
  "SA18sghancsu:144": "upādhyāyasya prāpnoti tadityevaṃ jagau muniḥ // Śghs_3,2.8 //",
  "SA18sghancsu:145": "hartturiṣṭāṃ diśaṃ yāyād vratī ced hastinaṃ haret /",
  "SA18sghancsu:146": "asau harttā vratī cauraḥ karipe hṛtasaṃjñini // Śghs_3,2.9 //",
  "SA18sghancsu:147": "pūjācīvaramālāntadvayasya khalu mocanāt /",
  "SA18sghancsu:148": "avanau taccyutau satyāṃ bhraśyate saṃvarād [yatiḥ] // Śghs_3,2.10 //",
  "SA18sghancsu:149": "kṣiptaṃ mauktikahārādi gṛhītvā tad mahākulāt //",
  "SA18sghancsu:150": "nyasya prasevikāyāṃ ca sahāyena ca hārayan // Śghs_3,2.11 //",
  "SA18sghancsu:151": "jaṭālenāthavā vatsa haran ki saṃvaracyutaḥ /",
  "SA18sghancsu:152": "ki pādacyutimātreṇa hyupacāravyatikrame // Śghs_3,2.12 //",
  "SA18sghancsu:153": "rātrau gośakaṭāśvādīn kaścid vadhnāti ced yatiḥ /",
  "SA18sghancsu:154": "etānete hariṣyanti tat kiṃ syāccyutasaṃvaraḥ // Śghs_3,2.13 //",
  "SA18sghancsu:155": "ṛjuke kīlake bandhe kaṭhine hāra[kaṃ] haran /",
  "SA18sghancsu:156": "utkṣepaṇe ca cauraḥ syāt kuṭile śithile na tu // Śghs_3,2.14 //",
  "SA18sghancsu:157": "chidrayitvā ghaṭaṃ sarpiḥ kṣaudraṃ tailaṃ jalaṃ haran /",
  "SA18sghancsu:158": "yatiḥ pūrasya vicchede saṃvarakṣobhamāpnute // Śghs_3,2.15 //",
  "SA18sghancsu:159": "vaṇṭayamāṇe gṛhe bhrātrorbhāgādekasya lipsayā /",
  "SA18sghancsu:160": "bālamātraṃ bhūmiṃ yatiḥ syāt kṣatasaṃvaraḥ // Śghs_3,2.16 //",
  "SA18sghancsu:161": "taskaropāttapātrādi nirāśaḥ punarādadan /",
  "SA18sghancsu:162": "saṃvarād bhraśyate hītotarastvāpnoti nāpadaṃ // Śghs_3,2.17 //",
  "SA18sghancsu:163": "mitrabhāvena labdhaṃ syād dattaṃ gopādibhiś[ca]vā /",
  "SA18sghancsu:164": "tadā [tad] ādadānasya yaternāpattiriṣyate // Śghs_3,2.18 //",
  "SA18sghancsu:165": "bhayopadarśanaṃ kāryaṃ pātracīvaramuktaye /",
  "SA18sghancsu:166": "rājani na vaded gatvaibhiścaurair muṣitā vayaṃ // Śghs_3,2.19 //",
  "SA18sghancsu:167": "niṣkāsayasyupādhyāyācāryāṇāṃ yattadāvathoḥ /",
  "SA18sghancsu:168": "tadityuktvā haredekaḥ syātāṃ dvāvapi taskarau // Śghs_3,2.20 //",
  "SA18sghancsu:169": "svadravyaṃ stainyacittenāpaharantāyimau yatī /",
  "SA18sghancsu:170": "bhāgārdhenāyatistveko niṣkāsitārdhenetaraḥ // Śghs_3,2.21 //",
  "SA18sghancsu:171": "dāsyāmaste samaṃ tiṣṭha dvāre kuryāma caurikāṃ /",
  "SA18sghancsu:172": "ityuktakṛta āpattiḥ sahaiva cauracellakaiḥ // Śghs_3,2.22 //",
  "SA18sghancsu:173": "uṣitvāgantuke yāte vismṛtya pātracīvaraṃ /",
  "SA18sghancsu:174": "āgatya yo yatistacced gopayeccaura eva saḥ // Śghs_3,2.23 //",
  "SA18sghancsu:175": "evamāvāsikā sarve bhavanti kṣatasaṃvarāḥ /",
  "SA18sghancsu:176": "sa eva yadi saṃsmṛtya nayatīha na kilviṣī // Śghs_3,2.24 //",
  "SA18sghancsu:177": "anyena steyacittena pātramanyasya ced yatiḥ /",
  "SA18sghancsu:178": "ānāyayati sa tato doṣavān bhavati vratī // Śghs_3,2.25 //",
  "SA18sghancsu:179": "yāvat pūjā munestāvat saṃghasyāpi pravartate /",
  "SA18sghancsu:180": "iti yo 'nyasya datte sa bhavati kṣatasaṃvaraḥ // Śghs_3,2.26 //",
  "SA18sghancsu:181": "likhitvoddhārakaṃ grāhyaṃ mithaḥ stūpācca saṃghataḥ /",
  "SA18sghancsu:182": "karmādāne ca vaktavyamiyat noktakṛto 'nyathā // Śghs_3,2.27 //",
  "SA18sghancsu:183": "svāṃśamuktaharaścauro[yatyora]madhye tayordvayoḥ /",
  "SA18sghancsu:184": "dāsyāmīti ca viśvāsaṃ kṛtvā prāpnoti nāpadaṃ // Śghs_3,2.28 //",
  "SA18sghancsu:185": "labhase tvaṃ labhe cāhaṃ yaṃ lābhaṃ tadāvayoḥ /",
  "SA18sghancsu:186": "iti lābhe samutpanne tato 'rdhena [sa] taskaraḥ // Śghs_3,2.29 //",
  "SA18sghancsu:187": "dakṣiṇādeśane 'pyevaṃ pāṃśukūlikayordvayoḥ /",
  "SA18sghancsu:188": "bhinatti yaḥ kriyākāraṃ tataḥ sa cyutasaṃvaraḥ // Śghs_3,2.30 //",
  "SA18sghancsu:189": "upakāryapakāribhyaścānnapānādi sāṃghikaṃ /",
  "SA18sghancsu:190": "vihārasvāmirājādicaurādibhyo yathākramaṃ // Śghs_3,2.31 //",
  "SA18sghancsu:191": "śrīghanebhyo 'pi dātavyaṃ prātarāśādi sāṃghikaṃ /",
  "SA18sghancsu:192": "ālepanādi kurvanti vihāre yadi nānyathā // Śghs_3,2.32 //",
  "SA18sghancsu:193": "sīmāntasthāyine datvā bhikṣūpasthānakāriṇe /",
  "SA18sghancsu:194": "dārakaprasthavyājena gṛhṇīyāttaṇḍulādikaṃ // Śghs_3,2.33 //",
  "SA18sghancsu:195": "atyaktāśo[hi]gṛhṇīyāt naṣṭaṃ svapātracīvaraṃ /",
  "SA18sghancsu:196": "matvaivaṃ naṣṭapātrādirāśāṃ naiva tyajed [yatiḥ] // Śghs_3,2.34 //",
  "SA18sghancsu:197": "svādhyāyādhyayanoddeśaṃ kṛtvā ye saṃghaśīlakaṃ /",
  "SA18sghancsu:198": "kriyākāraṃ na kurvanti te jinoditalaṃghinaḥ // Śghs_3,2.35 //",
  "SA18sghancsu:199": "patākālakṣaṇaṃ stūpānna grāhyaṃ yatinā[khalu] /",
  "SA18sghancsu:200": "prāṃśukūlamiti grāhyamaniloddhūtapātitaṃ // Śghs_3,2.36 //",
  "SA18sghancsu:201": "śulkakṛtyaṃ paradravyaṃ na nayet nāpi nāyayeta /",
  "SA18sghancsu:202": "śulkapradānamokṣārthopāyaṃ nopadiśed yatiḥ // Śghs_3,2.37 //",
  "SA18sghancsu:203": "ratnaṃ datvā yatibhyaḥ prāk paścāt mārgayate vaṇik /",
  "SA18sghancsu:204": "visaṃvādayasītyevaṃ dātavyaṃ paribhāṣya tat // Śghs_3,2.38 //",
  "SA18sghancsu:205": "nāsti śulkaṃ munerevaṃ vratināṃ pāribhogike /",
  "SA18sghancsu:206": "krayavikrayakṛddadyāt śulkamanyatra vastuni // Śghs_3,2.39 //",
  "SA18sghancsu:207": "varṣāvāsikalābho [hi] deyo varṣoṣitasya ca /",
  "SA18sghancsu:208": "jīvitabrahmacaryasya yayornāśastayorapi // Śghs_3,2.40 //",
  "SA18sghancsu:209": "prākprāgupagataiḥ kāryā varṣāvāsikayācanā /",
  "SA18sghancsu:210": "paścādupagataiḥ paścāt saṃghe yugapadeva ca // Śghs_3,2.41 //",
  "SA18sghancsu:211": "punargṛhṇāti dīkṣāṃ yo vastukarmajugupsayā /",
  "SA18sghancsu:212": "bhikṣubhāvaṃ parityajya tasmai cāpyāha sarvavit // Śghs_3,2.42 //",
  "SA18sghancsu:213": "avarṣoṣitavibhrāntakudṛṣṭimṛtakās[tathā] /",
  "SA18sghancsu:214": "deśāntaragatā lābhaṃ nārhanti paṃcapudgalāḥ // Śghs_3,2.43 //",
  "SA18sghancsu:215": "amuṣmai deyamityevaṃ lābhotpanne yatau mṛte /",
  "SA18sghancsu:216": "dātavyo niyataṃ tasmai sa vikalpitabhikṣave // Śghs_3,2.44 //",
  "SA18sghancsu:217": "deśāntaragatasyāpi sthāpayet kalpamandire /",
  "SA18sghancsu:218": "traye kurvanti cānarthaṃ tebhyo deyaṃbudharna hi // Śghs_3,2.45 //",
  "SA18sghancsu:219": "ācchādaṃ cīvaraṃ mūlyaṃ dāsyāmīti catuṣpadaiḥ /",
  "SA18sghancsu:220": "āpnoti sammukhībhūtān lābho netareṣu ca // Śghs_3,2.46 //",
  "SA18sghancsu:221": "varṣāvāsikalābhaśca varṣoṣitasya [gacchati] /",
  "SA18sghancsu:222": "tadetarasya doṣo na gṛhṇato dāyakecchayā // Śghs_3,2.47 //",
  "SA18sghancsu:223": "kāliko yāmiko yāvajjīvikaḥ sāptāhikaḥ /",
  "SA18sghancsu:224": "mṛtakṣudrapariṣkāranaityakākālacīvaraṃ // Śghs_3,2.48 //",
  "SA18sghancsu:225": "kuṭīpratiṣṭhājātyādisambodhyādimahodbhavaḥ /",
  "SA18sghancsu:226": "prāpnoti daśadhā lābhaḥ sammukhībhūtapudgalān // Śghs_3,2.49 //",
  "SA18sghancsu:227": "anādheyaṃ [gurudravyaṃ] sāṃghikaṃ śayanāsanaṃ /",
  "SA18sghancsu:228": "vikretavyaṃ na bhoktavyaṃ kṛtvā paudgalikaṃ na ca // Śghs_3,2.50 //",
  "SA18sghancsu:229": "cāturdiśāya buddhāya sammukhībhūtasāṃghike",
  "SA18sghancsu:230": "yatirdravyāntare datte tatra tatropanāmayet // Śghs_3,2.51 //",
  "SA18sghancsu:231": "tūṣṇīṃ datvā gate deyaṃ buddhe muktāphalādikaṃ /",
  "SA18sghancsu:232": "cāturdiksammukhībhūte śayanāsanamaṃśukaṃ // Śghs_3,2.52 //",
  "SA18sghancsu:233": "pṛthu dravyaṃ paropāttaṃ tatsaṃjñī steyacetanā /",
  "SA18sghancsu:234": "sthānacyutiśca paṃcāṃgairyuktaścedayatirbhavet // Śghs_3,2.53 //",
  "SA18sghancsu:235": "dattasvakāparopāttasaṃjñī cānyo 'pi saṃvarī /",
  "SA18sghancsu:236": "na tāvatkālikaṃ svāṃśād gṛhṇan jahāti saṃvaraṃ // Śghs_3,2.54 //",
  "SA18sghancsu:237": "ādāyādattamutplutya khaṃ gatvāśācatuṣṭayaṃ /",
  "SA18sghancsu:238": "tatraiva punarāsīno bhavati kṣatasaṃvaraḥ // Śghs_3,2.55 //",
  "SA18sghancsu:239": "avakrītādirajñānāt paradravyaṃ haran khalu /",
  "SA18sghancsu:240": "saṃvarakṣobhalābhī sa yatirnonmattādikaḥ // Śghs_3,2.56 //",
  "SA18sghancsu:241": "3 maithunaviratiśikṣāpadaṃ",
  "SA18sghancsu:242": "saṃvaratyāgaḥ",
  "SA18sghancsu:243": "nṛstrīpaṇḍakaviṇmūtravaktrarandhreṃ'gajāta[kaṃ] /",
  "SA18sghancsu:244": "kṣiptvā nigamayet svādaṃ sa [yatiḥ] kṣatasaṃvaraḥ // Śghs_3,3.1 //",
  "SA18sghancsu:245": "mānuṣā'mānuṣāste tu tiryañcaśca narādayaḥ /",
  "SA18sghancsu:246": "mṛtāḥ suptāśca jāgranto jñeyā [hi] maithunāśrayāḥ // Śghs_3,3.2 //",
  "SA18sghancsu:247": "hastinīmāditaḥ [kṛtvā] yāvat kukkuṭajāti[kāṃ] /",
  "SA18sghancsu:248": "kṣudrikāṃ mahatīṃ gacchan[sa yatiś] cyavate vratāt // Śghs_3,3.3 //",
  "SA18sghancsu:249": "duṣkṛtāni",
  "SA18sghancsu:250": "raktaḥ san na striyaṃ paśyet śabdamākarṇayenna ca /",
  "SA18sghancsu:251": "nirvastraḥ san na copeyādaṃgajāta na nirbhujet // Śghs_3,3.4 //",
  "SA18sghancsu:252": "bhittidvaye kṛtāṃ śuṣkāṃ klinnāṃ saṃyojya [koṭarīṃ] /",
  "SA18sghancsu:253": "[vipratipattito] nāśyaḥ saṃgrahe sati nānyadā // Śghs_3,3.5 //",
  "SA18sghancsu:254": "adha ūrdhvaṃ trikhaṇḍāyā vinīlādhmātikāṃ ca na /",
  "SA18sghancsu:255": "na cāsthisaṃkalāṃ yāyāt nābhyaktāṃ rudhirādibhiḥ // Śghs_3,3.6 //",
  "SA18sghancsu:256": "sitāsthisaṃkalāṃ śailadārupustamayīṃ[yatiḥ] /",
  "SA18sghancsu:257": "na ca gacchet striyaṃ yo 'tra virato maithunātsadā // Śghs_3,3.7 //",
  "SA18sghancsu:258": "dhyānamiddhagataṃ mattamabhibhūtaṃ ca nidrayā /",
  "SA18sghancsu:259": "vikṣiptaṃ vedanātunnaṃ gṛhītvābhiniṣīdati // Śghs_3,3.8 //",
  "SA18sghancsu:260": "ādimadhyāvasāneṣu svādayet dhyānavyutthitaḥ /",
  "SA18sghancsu:261": "vihārād ghaṭṭanīyaḥ so 'bhāve tu svādanasya na // Śghs_3,3.9 //",
  "SA18sghancsu:262": "nānāvyaṃjana[ko]petarasaśālyupabhogavat /",
  "SA18sghancsu:263": "āsvādanā ca tailācchaśauṇḍāsavapānavat // Śghs_3,3.10 //",
  "SA18sghancsu:264": "anāsvādo yuvagrīvāmṛtakukkurabandhavat /",
  "SA18sghancsu:265": "vijñeyaḥ sukumārasya pradīptalohasparśavat // Śghs_3,3.11 //",
  "SA18sghancsu:266": "avakrītādi[rajñānāt] /",
  "SA18sghancsu:267": "[āpattibhāg yatirnaivonmattavikṣiptacetasaḥ] // Śghs_3,3.12 //",
  "SA18sghancsu:268": "rāgā[manuṣyasaṃyogāt] praguṇībhūtāṃgajāta[kaṃ] /",
  "SA18sghancsu:269": "na spṛśet śukramokṣārthaṃ sa pṛthivyādidhātubhiḥ // Śghs_3,3.13 //",
  "SA18sghancsu:270": "tiryak[pramatta]saṃyogāt śukramuktirbhaved yadi /",
  "SA18sghancsu:271": "[sva]mānasaṃ yatigarhet kṛtaṃ me nu virūpakaṃ // Śghs_3,3.14 //",
  "SA18sghancsu:272": "majjajakṣaraṇaṃ yasya yateḥ svapneṣu jāyate /",
  "SA18sghancsu:273": "svapnaśukravinirmuktau taddoṣo na manāgapi // Śghs_3,3.15 //",
  "SA18sghancsu:274": "rāgāviṣṭena cittena nārīṃ na śrīghanaḥ spṛśet /",
  "SA18sghancsu:275": "[anyaṃ spṛśettathārūpaṃ nāpi ca] puṃnapuṃsakaṃ // Śghs_3,3.16 //",
  "SA18sghancsu:276": "stanakakṣauṣṭhanābhyūrupārśva kukṣimalabhramān /",
  "SA18sghancsu:277": "saṃraktaḥ [san] yatiḥ strīṇāṃ na nindet na ca saṃstuyāt // Śghs_3,3.17 //",
  "SA18sghancsu:278": "yadyad vadet striyaṃ[tāvat] kartukāmo yathā yathā /",
  "SA18sghancsu:279": "tattat tathā tathā brūyāt śrīghano nāntike striyāḥ // Śghs_3,3.18 //",
  "SA18sghancsu:280": "agrā bhavati sā yoṣid yā māṃ paricared rahaḥ /",
  "SA18sghancsu:281": "lābhinī cāyurādīnāṃ vyādhihīnā yaśasvinī // Śghs_3,3.19 //",
  "SA18sghancsu:282": "ityādi na vaco brūyād rāgāgnipluṣṭamānasaḥ /",
  "SA18sghancsu:283": "purastācchroghanaḥ strīṇāṃ punapuṃsakayostathā // Śghs_3,3.20 //",
  "SA18sghancsu:284": "vivāhāvāhayordautyaṃ na kuryāt śrāmaṇerakaḥ /",
  "SA18sghancsu:285": "[na ca kuryāt tathārūpanimittamapi dautyakaṃ] // Śghs_3,3.21 //",
  "SA18sghancsu:286": "dāpayeyamimāṃ śrāddhāṃ tathāsmai dārikāmiti /",
  "SA18sghancsu:287": "arthasaṃsyandanāsūtraṃ ghaṭate na [sa] bhāṣituṃ // Śghs_3,3.22 //",
  "SA18sghancsu:288": "bījārthaṃ na vratī dadyāt [svayameva] gavādikaṃ /",
  "SA18sghancsu:289": "etāneva parebhyo 'pi kadācinna [hi] prārthayet // Śghs_3,3.23 //",
  "SA18sghancsu:290": "tulyametāsu tvaṃ tiṣṭha mā striyā caikayā vasa /",
  "SA18sghancsu:291": "ityevaṃ hi bahustrīkaṃ puruṣaṃ na yatirvadet // Śghs_3,3.24 //",
  "SA18sghancsu:292": "4 mṛṣāvādaviratiśikṣāpadaṃ",
  "SA18sghancsu:293": "pratyekabuddhasaṃbuddhatacchiṣyajñānadarśanaṃ /",
  "SA18sghancsu:294": "mamāstīti bruvan mithyā nāśyo 'nyatrābhimānataḥ // Śghs_3,4.1 //",
  "SA18sghancsu:295": "jñānaṃ satyābhisaṃbodhirabhijñāḥ paṃca darśanaṃ /",
  "SA18sghancsu:296": "dharmānvayādikajñānāṃgīkaraṇe kṣamo na saḥ // Śghs_3,4.2 //",
  "SA18sghancsu:297": "āryamlecchavacolekhalipyā [ca] hastamudrayā /",
  "SA18sghancsu:298": "jñānādi pratijānīte boddhavyamapi tanmṛṣā // Śghs_3,4.3 //",
  "SA18sghancsu:299": "vṛkṣamūle 'hamekākī viharāmi rame [gṛhe] /",
  "SA18sghancsu:300": "samāhitaṃ ca me cittaṃ na ca vācyamatīndriyaṃ // Śghs_3,4.4 //",
  "SA18sghancsu:301": "buddhādīnāṃ vaded varṇamātmano na kathaṃcana /",
  "SA18sghancsu:302": "parakīyaṃ guṇaṃ pṛṣṭo brūyāttatsaṃmukhaṃ na tu // Śghs_3,4.5 //",
  "SA18sghancsu:303": "dṛṣṭaṃ śrutaṃ mataṃ yadyad vijñātaṃ caiva tad vadet /",
  "SA18sghancsu:304": "[śrīghano] vaiparītyena gadanmithyābhidhāyakaḥ // Śghs_3,4.6 //",
  "SA18sghancsu:305": "vastu cālīkasaṃjñī ca cittaṃ vinihitaṃ [tathā] /",
  "SA18sghancsu:306": "mṛṣāsaṃjñī vadedvācaṃ paṃcāṃgo 'nṛtavādikaḥ // Śghs_3,4.7 //",
  "SA18sghancsu:307": "catustridvayeka[kai] raṃgairyukto yo 'tra mṛṣāṃ vadeta /",
  "SA18sghancsu:308": "anyaiḥ sa yatibhirjñeyo mṛṣāvādīti [śrīghanaḥ] // Śghs_3,4.8 //",
  "SA18sghancsu:309": "abhūtenānidānena yatiḥ [prāṇivadhādinā] /",
  "SA18sghancsu:310": "dhvaṃsanacyāvanākūtaḥ saṃmukhaṃ na ca codayet // Śghs_3,4.9 //",
  "SA18sghancsu:311": "kṣudrāpattinimittena leśamātreṇa tena vā /",
  "SA18sghancsu:312": "śrāmaṇero[hi] pratyakṣaṃ [śrīghanān] na kṣipet parān // Śghs_3,4.10 //",
  "SA18sghancsu:313": "jātiliṃgakriyāvādair hīnamadhyottamairvratī /",
  "SA18sghancsu:314": "jihmaniṣpratibhānārthaṃ na prāṇān jātu codayet // Śghs_3,4.11 //",
  "SA18sghancsu:315": "upakleśagadākrośāpattyādyairhi yatiṃ vratī /",
  "SA18sghancsu:316": "āśayena puroktena brūyānnaiva kadācana // Śghs_3,4.12 //",
  "SA18sghancsu:317": "bhedaṃ yāsyati so 'nyena saṃdhāsyati mayā saha /",
  "SA18sghancsu:318": "yatira jātyādivādena paiśunyaṃ na samācaret // Śghs_3,4.13 //",
  "SA18sghancsu:319": "5 surāmaireyamadyapānaviratiśikṣāpadaṃ",
  "SA18sghancsu:320": "madyaṃ surā ca maireyaṃ pramādasyāspadaṃ yataḥ /",
  "SA18sghancsu:321": "na parasmai[hi] taddeyaṃ na ca peyaṃ śvamūtravata // Śghs_3,5.1 //",
  "SA18sghancsu:322": "valkalaiḥ saguḍaiḥ kācidaparā kevalena ca /",
  "SA18sghancsu:323": "piṣṭakiṇvajalairebhirapi saṃcīyate surā // Śghs_3,5.2 //",
  "SA18sghancsu:324": "maireyaṃ guḍadhātryambudhātakīsaṃskṛtaṃ hi yat /",
  "SA18sghancsu:325": "kuśāgreṇāpi tat pātuṃ yatīnāṃ na prakalpate // Śghs_3,5.3 //",
  "SA18sghancsu:326": "kodravānnaṃ na bhuñjīta tatpalāle ca na svapet //",
  "SA18sghancsu:327": "pītaṃ sata madayed yacca pātavyaṃ tadapīha na // Śghs_3,5.4 //",
  "SA18sghancsu:328": "6 uccaśayanamahāśayanaviratiśikṣāpadaṃ",
  "SA18sghancsu:329": "śayane yatirāsīta jināṣṭāṃgulapādake /",
  "SA18sghancsu:330": "[vinā] śalyena[maṃce vā] hemarūpyakṛte na tu // Śghs_3,6.1 //",
  "SA18sghancsu:331": "sāṃghike śayane dadyāt tat pratyāstaraṇaṃ yatiḥ /",
  "SA18sghancsu:332": "vastrakambalayoriṣṭaṃ yat tridvayekapuṭaṃ kramāt // Śghs_3,6.2 //",
  "SA18sghancsu:333": "nyasya gartteṣu tatpādānatirekapramāṇakān /",
  "SA18sghancsu:334": "prakṣipya pratipādeṣu ṭaṃkitvā kīlakeṣu vā // Śghs_3,6.3 //",
  "SA18sghancsu:335": "evaṃ vekṣuyavādīnāṃ rakṣārthaṃ maṃcake svapet /",
  "SA18sghancsu:336": "yatirna laṃbayetpādāvadhvago dīrghapādake // Śghs_3,6.4 //",
  "SA18sghancsu:337": "alpaṃ ced dvādaśebhyo hi deyamekaṃ vihārakaṃ /",
  "SA18sghancsu:338": "nirmuṣṭahastaṃ svalpatve bahutve tu viparyayāt / Śghs_3,6.5 //",
  "SA18sghancsu:339": "stokaṃ ced navakaḥ pīṭhe vṛddho maṃce[tadā] svapet /",
  "SA18sghancsu:340": "svaptavyaṃ vṛkṣamūle ['pi] navenābhyavakāśake // Śghs_3,6.6 //",
  "SA18sghancsu:341": "antarvarṣāsu nopasthāpyo 'nyaḥ sabrahmacārikaḥ /",
  "SA18sghancsu:342": "yatinā [sva] vihāre[tu] yathāvṛddhikayā yatiḥ // Śghs_3,6.7 //",
  "SA18sghancsu:343": "saṃskāraparibhogārthaṃ paripālanārthameva ca /",
  "SA18sghancsu:344": "uddiśyate [yatibhyo vai] sāṃghikaṃ śayanāsanaṃ // Śghs_3,6.8 //",
  "SA18sghancsu:345": "hīnamadhyottamaṃ [sthāpyaṃ bhikṣubhyo] layanatrayaṃ /",
  "SA18sghancsu:346": "vṛddhādyāgantukebhyastaduddiśeddaharakramāt // Śghs_3,6.9 //",
  "SA18sghancsu:347": "śayanāsanamatyuccaṃ pratiṣiddhaṃ yaterhi tat /",
  "SA18sghancsu:348": "madauddhatyakaraṃ yasmād lokāvadhyānakāraṇaṃ // Śghs_3,6.10 //",
  "SA18sghancsu:349": "7 nṛtyagītavāditraviratiśikṣāpadaṃ",
  "SA18sghancsu:350": "bāhuvikṣepakaṃ nṛtyaṃ gītamuccaiḥ svareṇa ca /",
  "SA18sghancsu:351": "vīṇāvaṃśamṛdaṃgādivāditraṃ na ca vādayet // Śghs_3,7.1 //",
  "SA18sghancsu:352": "8 gandhamālyavilepanaviratiśikṣāpadaṃ",
  "SA18sghancsu:353": "candanādika[gandhaṃ hi dhārayed] vyādhito yatiḥ /",
  "SA18sghancsu:354": "dattvā [ca] munaye pūrvaṃ bhiṣagādervacanena [saḥ] // Śghs_3,8.1 //",
  "SA18sghancsu:355": "na tiṣṭhet prakaṭe deśe kṣālayitvā bahirvrajet /",
  "SA18sghancsu:356": "anenaiva vidhānena dhārayet kusumasrajaṃ // Śghs_3,8.2 //",
  "SA18sghancsu:357": "netraduḥkhaśiraḥśūlapūtanāgraharugvataḥ /",
  "SA18sghancsu:358": "mālayā veṣṭayet śīrṣaṃ mekhalāṃ na ca laṃbayet // Śghs_3,8.3 //",
  "SA18sghancsu:359": "haritālādicūrṇena tailena vā sugandhinā /",
  "SA18sghancsu:360": "na vadanādikaṃ gātraṃ varṇārthaṃ marṣayed yatiḥ // Śghs_3,8.4 //",
  "SA18sghancsu:361": "yaterapratirūpatvād gandhamālya[vilepanaṃ] /",
  "SA18sghancsu:362": "rāgauddhatyakaraṃ yasmāt tasmāttad garhitaṃ jinaiḥ // Śghs_3,8.5 //",
  "SA18sghancsu:363": "na darpaṇe nirīkṣeta mukhaṃ vā tailabhājane /",
  "SA18sghancsu:364": "muktvānyenekṣaṇīyaṃ[hi] śirovyathāgadāturaṃ // Śghs_3,8.6 //",
  "SA18sghancsu:365": "vibhūṣaṇāya tailenātmano nābhyaṃjayed mukhaṃ /",
  "SA18sghancsu:366": "sphuṭitāsyaśirorogādanyenābhyaṃjanaṃ na ca // Śghs_3,8.7 //",
  "SA18sghancsu:367": "na kuṃkumādicūrṇena maṇḍayed vadanaṃ yatiḥ /",
  "SA18sghancsu:368": "piṭakaśamanārthaṃ tu lepa iṣṭastilā[dibhiḥ] // Śghs_3,8.8 //",
  "SA18sghancsu:369": "9 vikālabhojanaviratiśikṣāpadaṃ",
  "SA18sghancsu:370": "rātreranyatra[bhuṃjāne mayi babhūvālparogatā] /",
  "SA18sghancsu:371": "tathaiva [khalu] vaḥ kāryaṃ [bhoktavyaṃ dinayevaca] // Śghs_3,9.1 //",
  "SA18sghancsu:372": "viṇmūtragarta[ke] pātaḥ caurāhibhiḥ samāgamaḥ /",
  "SA18sghancsu:373": "udarārthaṃ gṛhe no 'ṭantītyavadhyātavān janaḥ // Śghs_3,9.2 //",
  "SA18sghancsu:374": "sāyaṃ prātaśca piṇḍārthamaṭanti bhuṃjate [tathā] /",
  "SA18sghancsu:375": "sabhācatvaraśrṛṃgāṭamandireṣu gṛheṣu ca // Śghs_3,9.3 //",
  "SA18sghancsu:376": "sugataḥ prāha bhuṃjīta kāle yatiḥ //",
  "SA18sghancsu:377": "uditādityamadhyāhnakeśamātrānatītake // Śghs_3,9.4 //",
  "SA18sghancsu:378": "sakturodanakulmāṣaṃ matsya- māṃsaṃ ca kālikaṃ /",
  "SA18sghancsu:379": "yāvajjīvikatailādipippalīpānavarjitaṃ // Śghs_3,9.5 //",
  "SA18sghancsu:380": "yatkiṃcidadyate dantairgalake vā praveśyate /",
  "SA18sghancsu:381": "kukṣiviṣṭabhmakaṃ kāle tattadadyādyathodite // Śghs_3,9.6 //",
  "SA18sghancsu:382": "saktvādyāmiṣanirmuktaṃ nīrajaṃ kardamādikaṃ /",
  "SA18sghancsu:383": "varṣopalakamaśnīyāt pipāsārtto yatirhimaṃ // Śghs_3,9.7 //",
  "SA18sghancsu:384": "kalpyavārigrahe tāvat prakṣālyau na [saśoṇitau] /",
  "SA18sghancsu:385": "hastau dvau cāṃgulīkṣāle na grāhyaṃ kalpikodakaṃ // Śghs_3,9.8 //",
  "SA18sghancsu:386": "māyāśāṭhaya[yutaḥ] kuryāt na ca prakṣālanaṃ tayoḥ /",
  "SA18sghancsu:387": "saśabdau [yadi] tau śuddhau vedyau snigdhatvaco yateḥ // Śghs_3,9.9 //",
  "SA18sghancsu:388": "na dāsyatha[mahyaṃ] bhoktuṃ kṣuttṛṣṇāpīḍito [mriye] /",
  "SA18sghancsu:389": "ityevaṃ [sa] viruktaḥ syāt kṣeptavyo maṇḍakuṇḍa[ke] // Śghs_3,9.10 //",
  "SA18sghancsu:390": "aṣṭāsu dhautadhānyaṃ yat pātre diśedanāmiṣe /",
  "SA18sghancsu:391": "sphuṭaṃ tu yadi pātuṃ tannaiva glānāya kalpate // Śghs_3,9.11 //",
  "SA18sghancsu:392": "lobhotyāpitavākkāyakarmaṇā samupārjitaṃ /",
  "SA18sghancsu:393": "vihāracīvarāhāraṃ varjayed viṣabhaktavat // Śghs_3,9.12 //",
  "SA18sghancsu:394": "madyaśastraviṣaprāṇilākṣavikrayaṇaṃ vratī /",
  "SA18sghancsu:395": "na kārayet na kuryānna jīvikāṃ tena kalpayet // Śghs_3,9.13 //",
  "SA18sghancsu:396": "satkārārthaṃ [ca lābhārtha] kuhanā hyakṣasaṃvaraḥ /",
  "SA18sghancsu:397": "vastrādikasya prāptayarthaṃ lapanā cāṭuvāditā // Śghs_3,9.14 //",
  "SA18sghancsu:398": "naiṣpeṣikatvamannādi-lābhāya parakutsanaṃ /",
  "SA18sghancsu:399": "anyadīyasya pātrādernaimittikaṃ [tu] varṇanaṃ // Śghs_3,9.15 //",
  "SA18sghancsu:400": "lābhena lipsā lābhānāṃ vṛddhiṣaryeṣaṇaṃ ca [yat] /",
  "SA18sghancsu:401": "yatirvivarjayedetān mithyājīvān purīṣavat // Śghs_3,9.16 //",
  "SA18sghancsu:402": "krīḍāparopaghātaṃ ca kāyavāgucchitaṃ [sadā] /",
  "SA18sghancsu:403": "vratī vivarjayennityaṃ viṭpradigdhamivoragaṃ // Śghs_3,9.17 //",
  "SA18sghancsu:404": "vipaṇitaragulmādau dyūtabandhagṛhe tathā /",
  "SA18sghancsu:405": "agocaro yadākhyātastiṣṭhettatra na cellakaḥ // Śghs_3,9.18 //",
  "SA18sghancsu:406": "10 jātarūparajatapratigrahaṇaviratiśikṣāpadaṃ",
  "SA18sghancsu:407": "kāyena hastapādābhyāṃ vastrachatrādibhirvratī /",
  "SA18sghancsu:408": "rajataṃ jātarūpaṃ ca na spṛśet jvalitāgnivat // Śghs_3,10.1 //",
  "SA18sghancsu:409": "iti saṃghasenena saṃvihitoddhāre śrīghanācārasaṃgrahe śrāmaṇerabhāvopāyo nāma tṛtīyo 'dhyāya //",
  "SA18sghancsu:410": "caturtho 'dhyāyaḥ",
  "SA18sghancsu:411": "anye cācārāḥ",
  "SA18sghancsu:412": "campūkalahadantādakāṣṭho vṛddhāntiko yatiḥ /",
  "SA18sghancsu:413": "snānapānāśanastotravyagro nagnaikavastra[kaḥ] // Śghs_4.1 //",
  "SA18sghancsu:414": "mṛtkarmavyāpṛtas tūrṇaṃ gacchan [ca] dvitribhūmikaṃ /",
  "SA18sghancsu:415": "āśu vrajan pacan pātraṃ vyagraś cīvarakarmaṇā // Śghs_4.2 //",
  "SA18sghancsu:416": "cūrṇacailakriyāsakto 'ktākṣo[ca] dhūmramāpiban /",
  "SA18sghancsu:417": "likhan yo pustakaṃ vāpi yo vācayati so 'pi na // Śghs_4.3 //",
  "SA18sghancsu:418": "viṭprasrāvakuṭīṃ gacchan tiṣṭhan tamasi voddiśan /",
  "SA18sghancsu:419": "vāsayannatha prāvṛṇvana na vandyo yatinā yatiḥ // Śghs_4.4 //",
  "SA18sghancsu:420": "avaguṇṭhitaśīrṣeṇa sopānatkena jānunoḥ /",
  "SA18sghancsu:421": "muṇḍasphoṭaṃ hi jaṃghāyāṃ na dadyādavinā yathā // Śghs_4.5 //",
  "SA18sghancsu:422": "samavasthāniṣaṇṇena vandyamānena padyuge /",
  "SA18sghancsu:423": "gṛhītvā karkaṭagrāhaṃ vācyamārogyamastviti // Śghs_4.6 //",
  "SA18sghancsu:424": "bhikṣuvinayasaṃkṣepādācārāntaramuddhṛtaṃ /",
  "SA18sghancsu:425": "śrotavyo 'nyadupādhyāyād bhāvyaṃ sagauraveṇa ca // Śghs_4.7 //",
  "SA18sghancsu:426": "śatadvayena saṃhatya śrīghanācāra saṃgrahaṃ /",
  "SA18sghancsu:427": "sukṛtaṃ karma yattena jano prāpnotvasaṃskṛtaṃ // Śghs_4.8 //",
  "SA18sghancsu:428": "iti saṃghasenena saṃvihitoddhāre śrīghanācārasaṃgrahe",
  "SA18sghancsu:429": "anye cācārā nāma caturtho 'dhyāyaḥ //",
  "SA18sghancsu:430": "iti śrīghanācārasaṃgrahaḥ //"
}