{
    "GSP33brahmsuu:0": " athāto brahmajijñāsā / BBs_1,1.1 /",
    "GSP33brahmsuu:1": " janmādyasya yataḥ / BBs_1,1.2 /",
    "GSP33brahmsuu:2": " śāstrayonitvāt / BBs_1,1.3 /",
    "GSP33brahmsuu:3": " tat tu samanvayāt / BBs_1,1.4 /",
    "GSP33brahmsuu:4": " īkṣater nāśabdam / BBs_1,1.5 /",
    "GSP33brahmsuu:5": " gauṇaś cen nātmaśabdāt / BBs_1,1.6 /",
    "GSP33brahmsuu:6": " tanniṣṭhasya mokṣopadeśāt / BBs_1,1.7 /",
    "GSP33brahmsuu:7": " heyatvāvacanāc ca / BBs_1,1.8 /",
    "GSP33brahmsuu:8": " pratijñāvirodhāt / BBs_1,1.9 /",
    "GSP33brahmsuu:9": " svāpyayāt / BBs_1,1.10 /",
    "GSP33brahmsuu:10": " gatisāmānyāt / BBs_1,1.11 /",
    "GSP33brahmsuu:11": " śrutatvāc ca / BBs_1,1.12 /",
    "GSP33brahmsuu:12": " ānandamayo 'bhyāsāt / BBs_1,1.13 /",
    "GSP33brahmsuu:13": " vikāraśabdān neti cen na prācuryāt / BBs_1,1.14 /",
    "GSP33brahmsuu:14": " taddhetuvyapadeśāc ca / BBs_1,1.15 /",
    "GSP33brahmsuu:15": " māntravarṇikameva ca gīyate / BBs_1,1.16 /",
    "GSP33brahmsuu:16": " netaro 'nupapatteḥ / BBs_1,1.17 /",
    "GSP33brahmsuu:17": " bhedavyapadeśāc ca / BBs_1,1.18 /",
    "GSP33brahmsuu:18": " kāmāc ca nānumānāpekṣā / BBs_1,1.19 /",
    "GSP33brahmsuu:19": " asminn asya ca tadyogaṃ śāsti / BBs_1,1.20 /",
    "GSP33brahmsuu:20": " antas taddharmopadeśāt / BBs_1,1.21 /",
    "GSP33brahmsuu:21": " bhedavyapadeśāc cānyaḥ / BBs_1,1.22 /",
    "GSP33brahmsuu:22": " ākāśas talliṅgāt / BBs_1,1.23 /",
    "GSP33brahmsuu:23": " ata eva prāṇaḥ / BBs_1,1.24 /",
    "GSP33brahmsuu:24": " jyotiś caraṇābhidhānāt / BBs_1,1.25 /",
    "GSP33brahmsuu:25": " chando 'bhidhānān neti cen na tathā ceto'rpaṇanigadāt tathā hi darśanam / BBs_1,1.26 /",
    "GSP33brahmsuu:26": " bhūtādipādavyapadeśopapatteś caivam / BBs_1,1.27 /",
    "GSP33brahmsuu:27": " upadeśabhedān neti cen nobhayasminn apy avirodhāt / BBs_1,1.28 /",
    "GSP33brahmsuu:28": " prāṇas tathānugamāt / BBs_1,1.29 /",
    "GSP33brahmsuu:29": " na vaktur ātmopadeśād iti ced adhyātmasaṃbandhabhūmā hy asmin / BBs_1,1.30 /",
    "GSP33brahmsuu:30": " śāstradṛṣṭyā tūpadeśo vāmadevavat / BBs_1,1.31 /",
    "GSP33brahmsuu:31": " jīvamukhyaprāṇaliṅgān neti cen nopāsātraividhyādāśritatvād iha tadyogāt / BBs_1,1.32 /",
    "GSP33brahmsuu:32": " sarvatra prasiddhopadeśāt / BBs_1,2.1 /",
    "GSP33brahmsuu:33": " vivakṣitaguṇopapatteś ca / BBs_1,2.2 /",
    "GSP33brahmsuu:34": " anupapattes tu na śārīraḥ / BBs_1,2.3 /",
    "GSP33brahmsuu:35": " karmakartṛvyapadeśāc ca / BBs_1,2.4 /",
    "GSP33brahmsuu:36": " śabdaviśeṣāt / BBs_1,2.5 /",
    "GSP33brahmsuu:37": " smṛteś ca / BBs_1,2.6 /",
    "GSP33brahmsuu:38": " arbhakaustvāt tadvyapadeśāc ca neti cen na nicāyyatvād evaṃ vyomavac ca / BBs_1,2.7 /",
    "GSP33brahmsuu:39": " saṃbhogaprāptir iti cen na vaiśeṣyāt / BBs_1,2.8 /",
    "GSP33brahmsuu:40": " attā carācaragrahaṇāt / BBs_1,2.9 /",
    "GSP33brahmsuu:41": " prakaraṇāc ca / BBs_1,2.10 /",
    "GSP33brahmsuu:42": " guhāṃ praviṣṭāv ātmānau hi taddarśanāt / BBs_1,2.11 /",
    "GSP33brahmsuu:43": " viśeṣaṇāc ca / BBs_1,2.12 /",
    "GSP33brahmsuu:44": " antara upapatteḥ / BBs_1,2.13 /",
    "GSP33brahmsuu:45": " sthānādivyapadeśāc ca / BBs_1,2.14 /",
    "GSP33brahmsuu:46": " sukhaviśiṣṭābhidhānād eva ca / BBs_1,2.15 /",
    "GSP33brahmsuu:47": " ata eva ca sa brahma / BBs_1,2.16 /",
    "GSP33brahmsuu:48": " śrutopaniṣatkagatyabhidhānāc ca / BBs_1,2.17 /",
    "GSP33brahmsuu:49": " anavasthiter asaṃbhavāc ca netaraḥ / BBs_1,2.18 /",
    "GSP33brahmsuu:50": " antaryāmyadhidaivādhilokādiṣu taddharmavyapadeśāt / BBs_1,2.19 /",
    "GSP33brahmsuu:51": " na ca smārtam ataddharmābhilāpāc chārīraś ca / BBs_1,2.20 /",
    "GSP33brahmsuu:52": " ubhaye 'pi hi bhedenainam adhīyate / BBs_1,2.21 /",
    "GSP33brahmsuu:53": " adṛśyatvādiguṇako dharmokteḥ / BBs_1,2.22 /",
    "GSP33brahmsuu:54": " viśeṣaṇabhedavyapadeśābhyāṃ ca netarau / BBs_1,2.23 /",
    "GSP33brahmsuu:55": " rūpopanyāsāc ca / BBs_1,2.24 /",
    "GSP33brahmsuu:56": " vaiśvānaraḥ sādhāraṇaśabdaviśeṣāt / BBs_1,2.25 /",
    "GSP33brahmsuu:57": " smaryamāṇam anumānaṃ syād iti / BBs_1,2.26 /",
    "GSP33brahmsuu:58": " śabdādibhyo 'ntaḥpratiṣṭhānāc ca neti cen na tathā dṛṣṭyupadeśād asambhavāt puruṣamapi cainam adhīyate / BBs_1,2.27 /",
    "GSP33brahmsuu:59": " ata eva na devatā bhūtaṃ ca / BBs_1,2.28 /",
    "GSP33brahmsuu:60": " sākṣād apy avirodhaṃ jaiminiḥ / BBs_1,2.29 /",
    "GSP33brahmsuu:61": " abhivyakter ity āśmarathyaḥ / BBs_1,2.30 /",
    "GSP33brahmsuu:62": " anusmṛter bādariḥ / BBs_1,2.31 /",
    "GSP33brahmsuu:63": " saṃpatter iti jaiminis tathā hi darśayati / BBs_1,2.32 /",
    "GSP33brahmsuu:64": " āmananti cainam asmin / BBs_1,2.33 /",
    "GSP33brahmsuu:65": " dyubhvādyāyatanaṃ svaśabdāt / BBs_1,3.1 /",
    "GSP33brahmsuu:66": " muktopasṛpyavyapadeśāc ca / BBs_1,3.2 /",
    "GSP33brahmsuu:67": " nānumānam atacchabdāt prāṇabhṛc ca / BBs_1,3.3 /",
    "GSP33brahmsuu:68": " bhedavyapadeśāt / BBs_1,3.4 /",
    "GSP33brahmsuu:69": " prakaraṇāt / BBs_1,3.5 /",
    "GSP33brahmsuu:70": " sthityadanābhyāṃ ca / BBs_1,3.6 /",
    "GSP33brahmsuu:71": " bhūmā saṃprasādād adhyupadeśāt / BBs_1,3.7 /",
    "GSP33brahmsuu:72": " dharmopapatteś ca / BBs_1,3.8 /",
    "GSP33brahmsuu:73": " akṣaram ambarāntadhṛteḥ / BBs_1,3.9 /",
    "GSP33brahmsuu:74": " sā ca praśāsanāt / BBs_1,3.10 /",
    "GSP33brahmsuu:75": " anyabhāvavyāvṛtteśca / BBs_1,3.11 /",
    "GSP33brahmsuu:76": " īkṣatikarmavyapadeśāt saḥ / BBs_1,3.12 /",
    "GSP33brahmsuu:77": " dahara uttarebhyaḥ / BBs_1,3.13 /",
    "GSP33brahmsuu:78": " gatiśabdābhyāṃ tathā hi dṛṣṭaṃ liṅgaṃ ca / BBs_1,3.14 /",
    "GSP33brahmsuu:79": " dhṛteś ca mahimno 'syāsminn upalabdheḥ / BBs_1,3.15 /",
    "GSP33brahmsuu:80": " prasiddheś ca / BBs_1,3.16 /",
    "GSP33brahmsuu:81": " itaraparāmarśāt sa iti cen nāsaṃbhavāt / BBs_1,3.17 /",
    "GSP33brahmsuu:82": " uttarāc ced āvirbhūtasvarūpas tu / BBs_1,3.18 /",
    "GSP33brahmsuu:83": " anyārthaś ca parāmarśaḥ / BBs_1,3.19 /",
    "GSP33brahmsuu:84": " alpaśruter iti cet tad uktam / BBs_1,3.20 /",
    "GSP33brahmsuu:85": " anukṛtes tasya ca / BBs_1,3.21 /",
    "GSP33brahmsuu:86": " api ca smaryate / BBs_1,3.22 /",
    "GSP33brahmsuu:87": " śabdād eva pramitaḥ / BBs_1,3.23 /",
    "GSP33brahmsuu:88": " hṛdyapekṣayā tu manuṣyādhikāratvāt / BBs_1,3.24 /",
    "GSP33brahmsuu:89": " tadupary api bādarāyaṇaḥ saṃbhavāt / BBs_1,3.25 /",
    "GSP33brahmsuu:90": " virodhaḥ karmaṇīti cen nānekapratipatter darśanāt / BBs_1,3.26 /",
    "GSP33brahmsuu:91": " śabda iti cen nātaḥ prabhavāt pratyakṣānumānābhyām / BBs_1,3.27 /",
    "GSP33brahmsuu:92": " ata eva ca nityatvam / BBs_1,3.28 /",
    "GSP33brahmsuu:93": " samānanāmarūpatvāccāvṛttāvapyavirodho darśanāt smṛteś ca / BBs_1,3.29 /",
    "GSP33brahmsuu:94": " madhvādiṣv asaṃbhavād anadhikāraṃ jaiminiḥ / BBs_1,3.30 /",
    "GSP33brahmsuu:95": " jyotiṣi bhāvāc ca / BBs_1,3.31 /",
    "GSP33brahmsuu:96": " bhāvaṃ tu bādarāyaṇo 'sti hi / BBs_1,3.32 /",
    "GSP33brahmsuu:97": " śugasya tadanādaraśravaṇāt tadādravaṇāt sūcyate hi / BBs_1,3.33 /",
    "GSP33brahmsuu:98": " kṣatriyatvagateś ca / BBs_1,3.34 /",
    "GSP33brahmsuu:99": " uttaratra caitrarathena liṅgāt / BBs_1,3.35 /",
    "GSP33brahmsuu:100": " saṃskāraparāmarśāt tadabhāvābhilāpāc ca / BBs_1,3.36 /",
    "GSP33brahmsuu:101": " tadabhāvanirdhāraṇe ca pravṛtteḥ / BBs_1,3.37 /",
    "GSP33brahmsuu:102": " śravaṇādhyayanārthapratiṣedhāt / BBs_1,3.38 /",
    "GSP33brahmsuu:103": " smṛteś ca / BBs_1,3.39 /",
    "GSP33brahmsuu:104": " kampanāt / BBs_1,3.40 /",
    "GSP33brahmsuu:105": " jyotir darśanāt / BBs_1,3.41 /",
    "GSP33brahmsuu:106": " ākāśo 'rthāntaratvādivyapadeśāt / BBs_1,3.42 /",
    "GSP33brahmsuu:107": " suṣuptyutkrāntyor bhedena / BBs_1,3.43 /",
    "GSP33brahmsuu:108": " patyādiśabdebhyaḥ / BBs_1,3.44 /",
    "GSP33brahmsuu:109": " ānumānikam apy ekeṣām iti cen na śarīra-rūpaka-vinyasta-gṛhīter darśayati ca / BBs_1,4.1 /",
    "GSP33brahmsuu:110": " sūkṣmaṃ tu tadarhatvāt / BBs_1,4.2 /",
    "GSP33brahmsuu:111": " tadadhīnatvād arthavat / BBs_1,4.3 /",
    "GSP33brahmsuu:112": " jñeyatvāvacanāc ca / BBs_1,4.4 /",
    "GSP33brahmsuu:113": " vadatīti cen na prājño hi prakaraṇāt / BBs_1,4.5 /",
    "GSP33brahmsuu:114": " trayāṇām eva caivam upanyāsaḥ praśnaś ca / BBs_1,4.6 /",
    "GSP33brahmsuu:115": " mahadvac ca / BBs_1,4.7 /",
    "GSP33brahmsuu:116": " camasavadaviśeṣāt / BBs_1,4.8 /",
    "GSP33brahmsuu:117": " jyotirupakramā tu tathā hy adhīyata eke / BBs_1,4.9 /",
    "GSP33brahmsuu:118": " kalpanopadeśāc ca madhvādivadavirodhaḥ / BBs_1,4.10 /",
    "GSP33brahmsuu:119": " na saṃkhyopasaṃgrahādapi jñānābhāvād atirekāc ca / BBs_1,4.11 /",
    "GSP33brahmsuu:120": " prāṇādayo vākyaśeṣāt / BBs_1,4.12 /",
    "GSP33brahmsuu:121": " jyotiṣaikeṣām asatyanne / BBs_1,4.13 /",
    "GSP33brahmsuu:122": " kāraṇatvena cākāśādiṣu yathāvyapadiṣṭokteḥ / BBs_1,4.14 /",
    "GSP33brahmsuu:123": " samākarṣāt / BBs_1,4.15 /",
    "GSP33brahmsuu:124": " jagadvācitvāt / BBs_1,4.16 /",
    "GSP33brahmsuu:125": " jīvamukhyaprāṇaliṅgān neti cet tadvyākhyātam / BBs_1,4.17 /",
    "GSP33brahmsuu:126": " anyārthaṃ tu jaiminiḥ praśnavyākhyānābhyām api caivam eke / BBs_1,4.18 /",
    "GSP33brahmsuu:127": " vākyānvayāt / BBs_1,4.19 /",
    "GSP33brahmsuu:128": " pratijñāsiddher liṅgam āśmarathyaḥ / BBs_1,4.20 /",
    "GSP33brahmsuu:129": " utkramiṣyata evaṃ bhāvād ity auḍulomiḥ / BBs_1,4.21 /",
    "GSP33brahmsuu:130": " avasthiter iti kāśakṛtsnaḥ / BBs_1,4.22 /",
    "GSP33brahmsuu:131": " prakṛtiś ca pratijñādṛṣṭāntānuparodhāt / BBs_1,4.23 /",
    "GSP33brahmsuu:132": " abhidhyopadeśāc ca / BBs_1,4.24 /",
    "GSP33brahmsuu:133": " sākṣāc cobhayāmnānāt / BBs_1,4.25 /",
    "GSP33brahmsuu:134": " ātmakṛteḥ / BBs_1,4.26 /",
    "GSP33brahmsuu:135": " pariṇāmāt / BBs_1,4.27 /",
    "GSP33brahmsuu:136": " yoniś ca hi gīyate / BBs_1,4.28 /",
    "GSP33brahmsuu:137": " etena sarve vyākhyātā vyākhyātāḥ / BBs_1,4.29 /",
    "GSP33brahmsuu:138": " smṛtyanavakāśadoṣaprasaṅga iti cen nānyasmṛtyanavakāśadoṣaprasaṅgāt / BBs_2,1.1 /",
    "GSP33brahmsuu:139": " itareṣāṃ cānupalabdheḥ / BBs_2,1.2 /",
    "GSP33brahmsuu:140": " etena yogaḥ pratyuktaḥ / BBs_2,1.3 /",
    "GSP33brahmsuu:141": " na vilakṣaṇatvād asya tathātvaṃ ca śabdāt / BBs_2,1.4 /",
    "GSP33brahmsuu:142": " abhimānivyapadeśas tu viśeṣānugatibhyām / BBs_2,1.5 /",
    "GSP33brahmsuu:143": " dṛśyate tu / BBs_2,1.6 /",
    "GSP33brahmsuu:144": " asad iti cen na pratiṣedhamātratvāt / BBs_2,1.7 /",
    "GSP33brahmsuu:145": " apītau tadvatprasaṅgād asamañjasam / BBs_2,1.8 /",
    "GSP33brahmsuu:146": " na tu dṛṣṭāntabhāvāt / BBs_2,1.9 /",
    "GSP33brahmsuu:147": " svapakṣadoṣāc ca / BBs_2,1.10 /",
    "GSP33brahmsuu:148": " tarkāpratiṣṭhānād api / BBs_2,1.11 /",
    "GSP33brahmsuu:149": " anyathānumeyam iti ced evam apy anirmokṣaprasaṅgaḥ / BBs_2,1.12 /",
    "GSP33brahmsuu:150": " etena śiṣṭāparigrahā api vyākhyātāḥ / BBs_2,1.13 /",
    "GSP33brahmsuu:151": " bhoktrāpatter avibhāgaś cet syāl lokavat / BBs_2,1.14 /",
    "GSP33brahmsuu:152": " tadananyatvam ārambhaṇaśabdādibhyaḥ / BBs_2,1.15 /",
    "GSP33brahmsuu:153": " bhāve copalabdheḥ / BBs_2,1.16 /",
    "GSP33brahmsuu:154": " satvāc cāparasya / BBs_2,1.17 /",
    "GSP33brahmsuu:155": " asadvyapadeśān neti cen na dharmāntareṇa vākyaśeṣādyukteḥ śabdāntarāc ca / BBs_2,1.18 /",
    "GSP33brahmsuu:156": " paṭavac ca / BBs_2,1.19 /",
    "GSP33brahmsuu:157": " yathā ca prāṇādiḥ / BBs_2,1.20 /",
    "GSP33brahmsuu:158": " itaravyapadeśād dhitākaraṇādidoṣaprasaktiḥ / BBs_2,1.21 /",
    "GSP33brahmsuu:159": " adhikaṃ tu bhedanirdeśāt / BBs_2,1.22 /",
    "GSP33brahmsuu:160": " aśmādivac ca tadanupapattiḥ / BBs_2,1.23 /",
    "GSP33brahmsuu:161": " upasaṃhāradarśanān neti cen na kṣīravad dhi / BBs_2,1.24 /",
    "GSP33brahmsuu:162": " devādivad api loke / BBs_2,1.25 /",
    "GSP33brahmsuu:163": " kṛtsnaprasaktir niravayavatvaśabdakopo vā / BBs_2,1.26 /",
    "GSP33brahmsuu:164": " śrutes tu śabdamūlatvāt / BBs_2,1.27 /",
    "GSP33brahmsuu:165": " ātmani caivaṃ vicitrāś ca hi / BBs_2,1.28 /",
    "GSP33brahmsuu:166": " svapakṣadoṣāc ca / BBs_2,1.29 /",
    "GSP33brahmsuu:167": " sarvopetā ca taddarśanāt / BBs_2,1.30 /",
    "GSP33brahmsuu:168": " vikaraṇatvān neti cet tad uktam / BBs_2,1.31 /",
    "GSP33brahmsuu:169": " na prayojanavattvāt / BBs_2,1.32 /",
    "GSP33brahmsuu:170": " lokavat tu līlākaivalyam / BBs_2,1.33 /",
    "GSP33brahmsuu:171": " vaiṣamyanairghṛṇye na sāpekṣatvāt tathā hi darśayati / BBs_2,1.34 /",
    "GSP33brahmsuu:172": " na karmāvibhāgād iti cen nānāditvād upapadyate cāpy upalabhyate ca / BBs_2,1.35 /",
    "GSP33brahmsuu:173": " sarvadharmopapatteś ca / BBs_2,1.36 /",
    "GSP33brahmsuu:174": " racanānupapatteś ca nānumānaṃ pravṛtteś ca / BBs_2,2.1 /",
    "GSP33brahmsuu:175": " payo 'mbuvac cet tatrāpi / BBs_2,2.2 /",
    "GSP33brahmsuu:176": " vyatirekānavasthiteś cānapekṣatvāt / BBs_2,2.3 /",
    "GSP33brahmsuu:177": " anyatrābhāvāc ca na tṛṇādivat / BBs_2,2.4 /",
    "GSP33brahmsuu:178": " puruṣāśmavad iti cet tathāpi / BBs_2,2.5 /",
    "GSP33brahmsuu:179": " aṅgitvānupapatteś ca / BBs_2,2.6 /",
    "GSP33brahmsuu:180": " anyathānumitau ca jñaśaktiviyogāt / BBs_2,2.7 /",
    "GSP33brahmsuu:181": " abhyupagame 'py arthābhāvāt / BBs_2,2.8 /",
    "GSP33brahmsuu:182": " vipratiṣedhāc cāsamañjasam / BBs_2,2.9 /",
    "GSP33brahmsuu:183": " mahaddīrghavad vā hrasvaparimaṇḍalābhyām / BBs_2,2.10 /",
    "GSP33brahmsuu:184": " ubhayathāpi na karmātastadabhāvaḥ / BBs_2,2.11 /",
    "GSP33brahmsuu:185": " samavāyābhyupagamāc ca sāmyād anavasthiteḥ / BBs_2,2.12 /",
    "GSP33brahmsuu:186": " nityam eva ca bhāvāt / BBs_2,2.13 /",
    "GSP33brahmsuu:187": " rūpādimattvāc ca viparyayo darśanāt / BBs_2,2.14 /",
    "GSP33brahmsuu:188": " ubhayathā ca doṣāt / BBs_2,2.15 /",
    "GSP33brahmsuu:189": " aparigrahāc cātyantam anapekṣā / BBs_2,2.16 /",
    "GSP33brahmsuu:190": " samudāya ubhayahetuke 'pi tadaprāptiḥ / BBs_2,2.17 /",
    "GSP33brahmsuu:191": " itaretarapratyayatvād upapannam iti cen na saṅghātabhāvānimittatvāt / BBs_2,2.18 /",
    "GSP33brahmsuu:192": " uttarotpāde ca pūrvanirodhāt / BBs_2,2.19 /",
    "GSP33brahmsuu:193": " asati pratijñoparodho yaugapadyamanyathā / BBs_2,2.20 /",
    "GSP33brahmsuu:194": " pratisaṃkhyāpratisaṃkhyānirodhāprāptir avicchedāt / BBs_2,2.21 /",
    "GSP33brahmsuu:195": " ubhayathā ca doṣāt / BBs_2,2.22 /",
    "GSP33brahmsuu:196": " ākāśe cāviśeṣāt / BBs_2,2.23 /",
    "GSP33brahmsuu:197": " anusmṛteś ca / BBs_2,2.24 /",
    "GSP33brahmsuu:198": " nāsato 'dṛṣṭatvāt / BBs_2,2.25 /",
    "GSP33brahmsuu:199": " udāsīnānām api caivaṃ siddhiḥ / BBs_2,2.26 /",
    "GSP33brahmsuu:200": " nābhāva upalabdheḥ / BBs_2,2.27 /",
    "GSP33brahmsuu:201": " vaidharmyāc ca na svapnādivat / BBs_2,2.28 /",
    "GSP33brahmsuu:202": " na bhāvo 'nupalabdheḥ / BBs_2,2.29 /",
    "GSP33brahmsuu:203": " sarvathānupapatteś ca / BBs_2,2.30 /",
    "GSP33brahmsuu:204": " naikasminn asambhavāt / BBs_2,2.31 /",
    "GSP33brahmsuu:205": " evaṃ cātmākārtsnyam / BBs_2,2.32 /",
    "GSP33brahmsuu:206": " na ca paryāyād apy avirodho vikārādibhyaḥ / BBs_2,2.33 /",
    "GSP33brahmsuu:207": " antyāvasthiteś cobhayanityatvād aviśeṣaḥ / BBs_2,2.34 /",
    "GSP33brahmsuu:208": " patyur asāmañjasyāt / BBs_2,2.35 /",
    "GSP33brahmsuu:209": " adhiṣṭhānānupapatteś ca / BBs_2,2.36 /",
    "GSP33brahmsuu:210": " karaṇavac cen na bhogādibhyaḥ / BBs_2,2.37 /",
    "GSP33brahmsuu:211": " antavattvam asarvajñatā vā / BBs_2,2.38 /",
    "GSP33brahmsuu:212": " utpattyasaṃbhavāt / BBs_2,2.39 /",
    "GSP33brahmsuu:213": " na ca kartuḥ karaṇam / BBs_2,2.40 /",
    "GSP33brahmsuu:214": " vijñānādibhāve vā tadapratiṣedhaḥ / BBs_2,2.41 /",
    "GSP33brahmsuu:215": " vipratiṣedhāc ca / BBs_2,2.42 /",
    "GSP33brahmsuu:216": " na viyadaśruteḥ / BBs_2,3.1 /",
    "GSP33brahmsuu:217": " asti tu / BBs_2,3.2 /",
    "GSP33brahmsuu:218": " gauṇyasaṃbhavāc chabdāc ca / BBs_2,3.3 /",
    "GSP33brahmsuu:219": " syāc caikasya brahmaśabdavat / BBs_2,3.4 /",
    "GSP33brahmsuu:220": " pratijñāhānir avyatirekāt / BBs_2,3.5 /",
    "GSP33brahmsuu:221": " śabdebhyaḥ / BBs_2,3.6 /",
    "GSP33brahmsuu:222": " yāvadvikāraṃ tu vibhāgo lokavat / BBs_2,3.7 /",
    "GSP33brahmsuu:223": " etena mātariśvā vyākhyātaḥ / BBs_2,3.8 /",
    "GSP33brahmsuu:224": " asaṃbhavas tu sato 'nupapatteḥ / BBs_2,3.9 /",
    "GSP33brahmsuu:225": " tejo 'tas tathā hy āha / BBs_2,3.10 /",
    "GSP33brahmsuu:226": " āpaḥ / BBs_2,3.11 /",
    "GSP33brahmsuu:227": " pṛthivī / BBs_2,3.12 /",
    "GSP33brahmsuu:228": " adhikārarūpaśabdāntarebhyaḥ / BBs_2,3.13 /",
    "GSP33brahmsuu:229": " tadabhidhyānād eva tu talliṅgāt saḥ / BBs_2,3.14 /",
    "GSP33brahmsuu:230": " viparyayeṇa tu kramo 'ta upapadyate ca / BBs_2,3.15 /",
    "GSP33brahmsuu:231": " antarā vijñānamanasī krameṇa talliṅgād iti cen nāviśeṣāt / BBs_2,3.16 /",
    "GSP33brahmsuu:232": " carācaravyapāśrayas tu syāt tadvyapadeśo bhāktas tadbhāvabhāvitvāt / BBs_2,3.17 /",
    "GSP33brahmsuu:233": " nātmā śruter nityatvāc ca tābhyaḥ / BBs_2,3.18 /",
    "GSP33brahmsuu:234": " jño 'ta eva / BBs_2,3.19 /",
    "GSP33brahmsuu:235": " utkrāntigatyāgatīnām / BBs_2,3.20 /",
    "GSP33brahmsuu:236": " svātmanā cottarayoḥ / BBs_2,3.21 /",
    "GSP33brahmsuu:237": " nāṇuratacchruter iti cen netarādhikārāt / BBs_2,3.22 /",
    "GSP33brahmsuu:238": " svaśabdonmānābhyāṃ ca / BBs_2,3.23 /",
    "GSP33brahmsuu:239": " avirodhaś candanavat / BBs_2,3.24 /",
    "GSP33brahmsuu:240": " avasthitivaiśeṣyād iti cen nābhyupagamād dhṛdi hi / BBs_2,3.25 /",
    "GSP33brahmsuu:241": " guṇādvā lokavat / BBs_2,3.26 /",
    "GSP33brahmsuu:242": " vyatireko gandhavat tathā hi darśayati / BBs_2,3.27 /",
    "GSP33brahmsuu:243": " pṛthagupadeśāt / BBs_2,3.28 /",
    "GSP33brahmsuu:244": " tadguṇasāratvāt tu tadvyapadeśaḥ prājñavat / BBs_2,3.29 /",
    "GSP33brahmsuu:245": " yāvadātmabhāvitvāc ca na doṣas taddarśanāt / BBs_2,3.30 /",
    "GSP33brahmsuu:246": " puṃstvādivat tv asya sato 'bhivyaktiyogāt / BBs_2,3.31 /",
    "GSP33brahmsuu:247": " nityopalabdhyanupalabdhiprasaṅgo 'nyataraniyamo vānyathā / BBs_2,3.32 /",
    "GSP33brahmsuu:248": " kartā śāstrārthavattvāt / BBs_2,3.33 /",
    "GSP33brahmsuu:249": " upādānād vihāropadeśāc ca / BBs_2,3.34 /",
    "GSP33brahmsuu:250": " vyapadeśāc ca kriyāyāṃ na cen nirdeśaviparyayaḥ / BBs_2,3.35 /",
    "GSP33brahmsuu:251": " upalabdhivadaniyamaḥ / BBs_2,3.36 /",
    "GSP33brahmsuu:252": " śaktiviparyayāt / BBs_2,3.37 /",
    "GSP33brahmsuu:253": " samādhyabhāvāc ca / BBs_2,3.38 /",
    "GSP33brahmsuu:254": " yathā ca takṣobhayathā / BBs_2,3.39 /",
    "GSP33brahmsuu:255": " parāt tu tacchruteḥ / BBs_2,3.40 /",
    "GSP33brahmsuu:256": " kṛtaprayatnāpekṣas tu vihitapratiṣiddhāvaiyarthyādibhyaḥ / BBs_2,3.41 /",
    "GSP33brahmsuu:257": " aṃśo nānāvyapadeśād anyathā cāpi dāśakitavāditvam adhīyata eke / BBs_2,3.42 /",
    "GSP33brahmsuu:258": " mantravarṇāt / BBs_2,3.43 /",
    "GSP33brahmsuu:259": " api smaryate / BBs_2,3.44 /",
    "GSP33brahmsuu:260": " prakāśādivat tu naivaṃ paraḥ / BBs_2,3.45 /",
    "GSP33brahmsuu:261": " smaranti ca / BBs_2,3.46 /",
    "GSP33brahmsuu:262": " anujñāparihārau dehasambandhāj jyotirādivat / BBs_2,3.47 /",
    "GSP33brahmsuu:263": " asantateś cāvyatikaraḥ / BBs_2,3.48 /",
    "GSP33brahmsuu:264": " ābhāsa eva ca / BBs_2,3.49 /",
    "GSP33brahmsuu:265": " adṛṣṭāniyamāt / BBs_2,3.50 /",
    "GSP33brahmsuu:266": " abhisandhyādiṣv api caivam / BBs_2,3.51 /",
    "GSP33brahmsuu:267": " pradeśabhedād iti cen nāntarbhāvāt / BBs_2,3.52 /",
    "GSP33brahmsuu:268": " tathā prāṇāḥ / BBs_2,4.1 /",
    "GSP33brahmsuu:269": " gauṇyasaṃbhavāt tatprāk śruteś ca / BBs_2,4.2 /",
    "GSP33brahmsuu:270": " tatpūrvakatvād vācaḥ / BBs_2,4.3 /",
    "GSP33brahmsuu:271": " sapta gater viśeṣitatvāc ca / BBs_2,4.4 /",
    "GSP33brahmsuu:272": " hastādayas tu sthite 'to naivam / BBs_2,4.5 /",
    "GSP33brahmsuu:273": " aṇavaś ca / BBs_2,4.6 /",
    "GSP33brahmsuu:274": " śreṣṭhaś ca / BBs_2,4.7 /",
    "GSP33brahmsuu:275": " na vāyukriye pṛthagupadeśāt / BBs_2,4.8 /",
    "GSP33brahmsuu:276": " cakṣurādivat tu tatsahaśiṣṭyādibhyaḥ / BBs_2,4.9 /",
    "GSP33brahmsuu:277": " akaraṇatvāc ca na doṣas tathā hi darśayati / BBs_2,4.10 /",
    "GSP33brahmsuu:278": " pañcavṛttir manovat vyapadiśyate / BBs_2,4.11 /",
    "GSP33brahmsuu:279": " aṇuś ca / BBs_2,4.12 /",
    "GSP33brahmsuu:280": " jyotir ādyadhiṣṭhānaṃ tu tadāmananātprāṇavatā śabdāt / BBs_2,4.13 /",
    "GSP33brahmsuu:281": " tasya ca nityatvāt / BBs_2,4.14 /",
    "GSP33brahmsuu:282": " ta indriyāṇi tadvyapadeśād anyatra śreṣṭhāt / BBs_2,4.15 /",
    "GSP33brahmsuu:283": " bhedaśruter vailakṣaṇyāc ca / BBs_2,4.16 /",
    "GSP33brahmsuu:284": " saṃjñāmūrtikḷptis tu trivṛtkurvata upadeśāt / BBs_2,4.17 /",
    "GSP33brahmsuu:285": " māṃsādi bhaumaṃ yathāśabdamitarayoś ca / BBs_2,4.18 /",
    "GSP33brahmsuu:286": " vaiśeṣyāt tu tadvādas tadvādaḥ / BBs_2,4.19 /",
    "GSP33brahmsuu:287": " tadantarapratipattau raṃhati saṃpariṣvaktaḥ praśnanirūpaṇābhyām / BBs_3,1.1 /",
    "GSP33brahmsuu:288": " tryātmakatvāt tu bhūyastvāt / BBs_3,1.2 /",
    "GSP33brahmsuu:289": " prāṇagateś ca / BBs_3,1.3 /",
    "GSP33brahmsuu:290": " agnyādiśruter iti cen na bhāktatvāt / BBs_3,1.4 /",
    "GSP33brahmsuu:291": " prathame 'śravaṇād iti cen na tā eva hy upapatteḥ / BBs_3,1.5 /",
    "GSP33brahmsuu:292": " aśrutatvād iti cen neṣṭādikāriṇāṃ pratīteḥ / BBs_3,1.6 /",
    "GSP33brahmsuu:293": " bhāktaṃ vānātmavittvāt tathā hi darśayati / BBs_3,1.7 /",
    "GSP33brahmsuu:294": " kṛtātyaye 'nuśayavān dṛṣṭasmṛtibhyāṃ yathetamanevaṃ ca / BBs_3,1.8 /",
    "GSP33brahmsuu:295": " caraṇād iti cen na tadupalakṣaṇārtheti kārṣṇājiniḥ / BBs_3,1.9 /",
    "GSP33brahmsuu:296": " ānarthakyam iti cen na tadapekṣatvāt / BBs_3,1.10 /",
    "GSP33brahmsuu:297": " sukṛtaduṣkṛte eveti tu bādariḥ / BBs_3,1.11 /",
    "GSP33brahmsuu:298": " aniṣṭādikāriṇām api ca śrutam / BBs_3,1.12 /",
    "GSP33brahmsuu:299": " saṃyamane tv anubhūyetareṣāmārohāv arohau tadgatidarśanāt / BBs_3,1.13 /",
    "GSP33brahmsuu:300": " smaranti ca / BBs_3,1.14 /",
    "GSP33brahmsuu:301": " api sapta / BBs_3,1.15 /",
    "GSP33brahmsuu:302": " tatrāpi tadvyāpārādavirodhaḥ / BBs_3,1.16 /",
    "GSP33brahmsuu:303": " vidyākarmaṇor iti tu prakṛtatvāt / BBs_3,1.17 /",
    "GSP33brahmsuu:304": " na tṛtīye tathopalabdheḥ / BBs_3,1.18 /",
    "GSP33brahmsuu:305": " smaryate 'pi ca loke / BBs_3,1.19 /",
    "GSP33brahmsuu:306": " darśanāc ca / BBs_3,1.20 /",
    "GSP33brahmsuu:307": " tṛtīyaśabdāvarodhaḥ saṃśokajasya / BBs_3,1.21 /",
    "GSP33brahmsuu:308": " tatsvābhāvyāpattirupapatteḥ / BBs_3,1.22 /",
    "GSP33brahmsuu:309": " nāticireṇa viśeṣāt / BBs_3,1.23 /",
    "GSP33brahmsuu:310": " anyādhiṣṭhite pūrvavadabhilāpāt / BBs_3,1.24 /",
    "GSP33brahmsuu:311": " aśuddham iti cen na śabdāt / BBs_3,1.25 /",
    "GSP33brahmsuu:312": " retaḥsigyogo 'tha / BBs_3,1.26 /",
    "GSP33brahmsuu:313": " yoneḥśarīram / BBs_3,1.27 /",
    "GSP33brahmsuu:314": " sandhye sṛṣṭirāha hi / BBs_3,2.1 /",
    "GSP33brahmsuu:315": " nirmātāraṃ caike putrādayaś ca / BBs_3,2.2 /",
    "GSP33brahmsuu:316": " māyāmātraṃ tu kārtsnyenānabhivyaktasvarūpatvāt / BBs_3,2.3 /",
    "GSP33brahmsuu:317": " parābhidhyānāt tu tirohitaṃ tato hyasya bandhaviparyayau / BBs_3,2.4 /",
    "GSP33brahmsuu:318": " dehayogādvā so 'pi / BBs_3,2.5 /",
    "GSP33brahmsuu:319": " sūcakaś ca hi śruterācakṣate ca tadvidaḥ / BBs_3,2.6 /",
    "GSP33brahmsuu:320": " tadabhāvo nāḍīṣu tacchruterātmani ca / BBs_3,2.7 /",
    "GSP33brahmsuu:321": " ataḥ prabodho 'smāt / BBs_3,2.8 /",
    "GSP33brahmsuu:322": " sa eva tu karmānusmṛtiśabdavidhibhyaḥ / BBs_3,2.9 /",
    "GSP33brahmsuu:323": " mugdher'dhasaṃpattiḥ pariśeṣāt / BBs_3,2.10 /",
    "GSP33brahmsuu:324": " na sthānato 'pi parasyobhayaliṅgaṃ sarvatra hi / BBs_3,2.11 /",
    "GSP33brahmsuu:325": " bhedād iti cen na pratyekamatadvacanāt / BBs_3,2.12 /",
    "GSP33brahmsuu:326": " api caivam eke / BBs_3,2.13 /",
    "GSP33brahmsuu:327": " arūpavadeva hi tatpradhānatvāt / BBs_3,2.14 /",
    "GSP33brahmsuu:328": " prakāśavaccāvaiyarthyāt / BBs_3,2.15 /",
    "GSP33brahmsuu:329": " āha ca tanmātram / BBs_3,2.16 /",
    "GSP33brahmsuu:330": " darśayati cātho api smaryate / BBs_3,2.17 /",
    "GSP33brahmsuu:331": " ata eva copamā sūryakādivat / BBs_3,2.18 /",
    "GSP33brahmsuu:332": " ambuvadagrahaṇāt tu na tathātvam / BBs_3,2.19 /",
    "GSP33brahmsuu:333": " vṛddhihrāsabhāktvamantarbhāvādubhayasāmañjasyādevaṃ darśanāc ca / BBs_3,2.20 /",
    "GSP33brahmsuu:334": " prakṛtaitāvattvaṃ hi pratiṣedhati tato bravīti ca bhūyaḥ / BBs_3,2.21 /",
    "GSP33brahmsuu:335": " tadavyaktamāha hi / BBs_3,2.22 /",
    "GSP33brahmsuu:336": " api saṃrādhane pratyakṣānumānābhyām / BBs_3,2.23 /",
    "GSP33brahmsuu:337": " prakāśādivaccāvaiśeṣyaṃ prakāśaś ca karmaṇyabhyāsāt / BBs_3,2.24 /",
    "GSP33brahmsuu:338": " ato 'nantena tathā hi liṅgam / BBs_3,2.25 /",
    "GSP33brahmsuu:339": " ubhayavyapadeśāttvahikuṇḍalavat / BBs_3,2.26 /",
    "GSP33brahmsuu:340": " prakāśāśrayavadvā tejastvāt / BBs_3,2.27 /",
    "GSP33brahmsuu:341": " pūrvavadvā / BBs_3,2.28 /",
    "GSP33brahmsuu:342": " pratiṣedhāc ca / BBs_3,2.29 /",
    "GSP33brahmsuu:343": " paramatassetūnmānasaṃbandhabhedavyapadeśebhyaḥ / BBs_3,2.30 /",
    "GSP33brahmsuu:344": " sāmānyāt tu / BBs_3,2.31 /",
    "GSP33brahmsuu:345": " buddhyarthaḥ pādavat / BBs_3,2.32 /",
    "GSP33brahmsuu:346": " sthānaviśeṣātprakāśādivat / BBs_3,2.33 /",
    "GSP33brahmsuu:347": " upapatteś ca / BBs_3,2.34 /",
    "GSP33brahmsuu:348": " tathānyapratiṣedhāt / BBs_3,2.35 /",
    "GSP33brahmsuu:349": " anena sarvagatatvamāyāmaśabdādibhyaḥ / BBs_3,2.36 /",
    "GSP33brahmsuu:350": " phalamata upapatteḥ / BBs_3,2.37 /",
    "GSP33brahmsuu:351": " śrutatvāc ca / BBs_3,2.38 /",
    "GSP33brahmsuu:352": " dharmaṃ jaiminirata eva / BBs_3,2.39 /",
    "GSP33brahmsuu:353": " pūrvaṃ tu bādarāyaṇo hetuvyapadeśāt / BBs_3,2.40 /",
    "GSP33brahmsuu:354": " sarvavedāntapratyayaṃ codanādyaviśeṣāt / BBs_3,3.1 /",
    "GSP33brahmsuu:355": " bhedān neti ced ekasyām api / BBs_3,3.2 /",
    "GSP33brahmsuu:356": " svādhyāyasya tathātve hi samācāre 'dhikārāc ca savavac ca tanniyamaḥ / BBs_3,3.3 /",
    "GSP33brahmsuu:357": " darśayati ca / BBs_3,3.4 /",
    "GSP33brahmsuu:358": " upasaṃhāror'thābhedādvidhiśeṣavatsamāne ca / BBs_3,3.5 /",
    "GSP33brahmsuu:359": " anyathātvaṃ śabdād iti cen nāviśeṣāt / BBs_3,3.6 /",
    "GSP33brahmsuu:360": " na vā prakaraṇabhedāt parovarīyastvādivat / BBs_3,3.7 /",
    "GSP33brahmsuu:361": " saṃjñātaś cet tad uktam asti tu tad api / BBs_3,3.8 /",
    "GSP33brahmsuu:362": " vyāpteś ca samañjasam / BBs_3,3.9 /",
    "GSP33brahmsuu:363": " sarvābhedādanyatreme / BBs_3,3.10 /",
    "GSP33brahmsuu:364": " ānandādayaḥ pradhānasya / BBs_3,3.11 /",
    "GSP33brahmsuu:365": " priyaśirastvādyaprāptirupacayāpacayau hi bhede / BBs_3,3.12 /",
    "GSP33brahmsuu:366": " itare tvarthasāmānyāt / BBs_3,3.13 /",
    "GSP33brahmsuu:367": " ādhyānāya prayojanābhāvāt / BBs_3,3.14 /",
    "GSP33brahmsuu:368": " ātmaśabdāc ca / BBs_3,3.15 /",
    "GSP33brahmsuu:369": " ātmagṛhītir itaravad uttarāt / BBs_3,3.16 /",
    "GSP33brahmsuu:370": " anvayād iti cet syād avadhāraṇāt / BBs_3,3.17 /",
    "GSP33brahmsuu:371": " kāryākhyānādapūrvam / BBs_3,3.18 /",
    "GSP33brahmsuu:372": " samāna evaṃ cābhedāt / BBs_3,3.19 /",
    "GSP33brahmsuu:373": " sambandhādevamanyatrāpi / BBs_3,3.20 /",
    "GSP33brahmsuu:374": " na vā viśeṣāt / BBs_3,3.21 /",
    "GSP33brahmsuu:375": " darśayati ca / BBs_3,3.22 /",
    "GSP33brahmsuu:376": " saṃbhṛtidyuvyāptyapi cātaḥ / BBs_3,3.23 /",
    "GSP33brahmsuu:377": " puruṣavidyāyāmapi cetareṣāmanāmnānāt / BBs_3,3.24 /",
    "GSP33brahmsuu:378": " vedhādyarthabhedāt / BBs_3,3.25 /",
    "GSP33brahmsuu:379": " hānau tūpāyanaśabdaśeṣatvāt kuśācchandasstutyupagānavattaduktam / BBs_3,3.26 /",
    "GSP33brahmsuu:380": " sāṃparāye tartavyābhāvāt tathā hy anye / BBs_3,3.27 /",
    "GSP33brahmsuu:381": " chandata ubhayāvirodhāt / BBs_3,3.28 /",
    "GSP33brahmsuu:382": " gater arthavattvam ubhayathānyathā hi virodhaḥ / BBs_3,3.29 /",
    "GSP33brahmsuu:383": " upapannas tallakṣaṇārthopalabdher lokavat / BBs_3,3.30 /",
    "GSP33brahmsuu:384": " yāvadadhikāram avasthitir ādhikārikāṇām / BBs_3,3.31 /",
    "GSP33brahmsuu:385": " aniyamassarveṣāmavirodhaśśabdānumānābhyām / BBs_3,3.32 /",
    "GSP33brahmsuu:386": " akṣaradhiyāṃ tvavarodhassāmānyatadbhāvābhyāmaupasadavattaduktam / BBs_3,3.33 /",
