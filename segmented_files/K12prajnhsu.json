{
    "K12prajnhsu:0": " Prajñāpāramitā-hṛdaya-sūtra",
    "K12prajnhsu:1": " Oṃ Namo Bhagavatyai Ārya-prajāpāramitāyai/",
    "K12prajnhsu:2": " Ārya Āvalokiteśvaro bodhisattvo gambhīraṃ",
    "K12prajnhsu:3": " prajñāpāramitā-cāryāṃ caramāṇo",
    "K12prajnhsu:4": " vyavalokayati sma,",
    "K12prajnhsu:5": " pañca-skandhās tāṃś ca",
    "K12prajnhsu:6": " svabhāva-śūnyān paśyati sma.",
    "K12prajnhsu:7": " Iha Śāriputra rūpaṃ śūnyatā,",
    "K12prajnhsu:8": " śūnyat'aiva rūpaṃ,",
    "K12prajnhsu:9": " rūpān na pṛthak śūnyatā,",
    "K12prajnhsu:10": " śūnyatāyā na pṛthag rūpaṃ,",
    "K12prajnhsu:11": " yad rūpaṃ sā śūnyatā,",
    "K12prajnhsu:12": " yā śūnyatā tad rūpaṃ.",
    "K12prajnhsu:13": " Evam eva vedanā-saṃjñā-saṃskārā-vijñānam.",
    "K12prajnhsu:14": " Iha Śāriputra sarva-dharmāḥ śūnyatā-lakṣaṇā",
    "K12prajnhsu:15": " anutpannā aniruddhā amalā avimalā anūnā aparipūrṇāḥ.",
    "K12prajnhsu:16": " Tasmāc Chāriputra śūnyatāyāṃ na rūpaṃ",
    "K12prajnhsu:17": " na vedanā na saṃjñā na saṃskārāḥ na vijñānaṃ,",
    "K12prajnhsu:18": " na cakṣuḥ-śrotra-ghrāṇa-jihvā-kāya-manāṃsi,",
    "K12prajnhsu:19": " na rūpa-śabda-gandha-rasa-spraṣṭavya-dharmāḥ,",
    "K12prajnhsu:20": " na cakṣur-dhātur yāvan na mano-vijñāna-dhātuḥ,",
    "K12prajnhsu:21": " n'āvidyā n'āvidyā-kṣayo yāvan na jarā-maraṇaṃ",
    "K12prajnhsu:22": " na jarā-maraṇa-kṣayo na duḥkha-samudaya-nirodha-mārgā",
    "K12prajnhsu:23": " na jñānaṃ na prāptir n'āprāptiḥ.",
    "K12prajnhsu:24": " Tasmāc Chāriputra aprāptitvād bodhisattvo",
    "K12prajnhsu:25": " prajñā-pāramitām āśritya viharaty acitt'āvaraṇaḥ.",
    "K12prajnhsu:26": " Citt'āvaraṇa-nāstitvād atrasto",
    "K12prajnhsu:27": " viparyās'ātikrānto niṣṭhā-nirvāṇaḥ.",
    "K12prajnhsu:28": " Tradhva-vyavasthitāḥ sarva-buddhāḥ",
    "K12prajnhsu:29": " prajñāpāramitām āśritya anuttarāṃ",
    "K12prajnhsu:30": " samyak-sambodhim abhisambuddhāḥ.",
    "K12prajnhsu:31": " Tasmāj jñātavyaṃ prajñāpāramitā mahā-mantro",
    "K12prajnhsu:32": " mahā-vidyā-mantro'nuttara-mantro",
    "K12prajnhsu:33": " samasama-mantraḥ sarva-duḥkha-praśamanaḥ satyam amithyatvāt.",
    "K12prajnhsu:34": " Prajñāpāramitāyām ukto mantraḥ, tadyathā -",
    "K12prajnhsu:35": " Oṃ gate gate pāragate pārasaṃgate bodhi svāhā.",
    "K12prajnhsu:36": " Ity ārya-prajñāpāramitā-hṛdayaṃ samāptam."
}