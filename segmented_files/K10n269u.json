{
    "K10n269u_001:0": " āryatriratnānusmṛtisūtram",
    "K10n269u_001:1": " (kha) saṃskṛtapāṭhaḥ",
    "K10n269u_001:2": " bhāratīyabhāṣāyām-āryatriratnānusmṛtisūtram",
    "K10n269u_001:3": " bhoṭabhāṣāyām-phags-pā-dakon-mcchog-gsum-jes-su-ḍran-pāī-mdo",
    "K10n269u_001:4": " namaḥ sarvabuddhabodhisattvebhyaḥ",
    "K10n269u_001:5": " (buddhānusmṛtiḥ)",
    "K10n269u_001:6": " ityapi buddho bhagavāṃstathāgato'rhan samyaksaṃbuddho vidyācaraṇasampannaḥ sugato lokavidanuttaraḥ puruṣadamyasārathiḥ śāstā devamanuṣyāṇāṃ buddho bhagavāniti |",
    "K10n269u_001:7": " niṣyandaḥ sa tathāgataḥ puṇyānām,",
    "K10n269u_001:8": " avipraṇāśaḥ kuśalamūlānām,",
    "K10n269u_001:9": " alaṅkṛtaḥ kṣāntyā,",
    "K10n269u_001:10": " ālayaḥ puṇyanidhānānām,",
    "K10n269u_001:11": " citrito'nuvyañjanaiḥ kusumito lakṣaṇaiḥ,",
    "K10n269u_001:12": " pratirūpo gocareṇa,",
    "K10n269u_001:13": " apratikūlo darśanena,",
    "K10n269u_001:14": " abhiratiḥ śraddhā(śuddhā)",
    "K10n269u_001:15": " dhimuktānām,",
    "K10n269u_001:16": " anabhibhūtaḥ prajñayā,",
    "K10n269u_001:17": " anavamardanīyo balaiḥ,",
    "K10n269u_001:18": " śāstā sarvasattvānām,",
    "K10n269u_001:19": " pitā bodhisattvānām,",
    "K10n269u_001:20": " rājā āryapudgalānām,",
    "K10n269u_001:21": " sārthavāhaḥ nirvāṇanagarasamprasthitānām,",
    "K10n269u_001:22": " aprameyo jñānena,",
    "K10n269u_001:23": " acintyaḥ pratibhānena,",
    "K10n269u_001:24": " viśuddhaḥ svareṇa,",
    "K10n269u_001:25": " āsvadanīyo ghoṣeṇa,",
    "K10n269u_001:26": " asecanako rūpeṇa,",
    "K10n269u_001:27": " apratisamaḥ kāyena,",
    "K10n269u_001:28": " aliptaḥ kāmaiḥ,",
    "K10n269u_001:29": " anupalipto rūpaiḥ asaṃsṛṣṭa ārūpyaiḥ,",
    "K10n269u_001:30": " vipramuktaḥ skandhebhyaḥ,",
    "K10n269u_001:31": " visamprayukto dhātubhiḥ,",
    "K10n269u_001:32": " saṃvṛta āyatanaiḥ,",
    "K10n269u_001:33": " pracchinno granthaiḥ,",
    "K10n269u_001:34": " vimuktaḥ paridāghnaiḥ,",
    "K10n269u_001:35": " parimuktastṛṣṇayā,",
    "K10n269u_001:36": " oghāduttīrṇaḥ paripūrṇo jñānena,",
    "K10n269u_001:37": " pratiṣṭhito'tītānāgatapratyutpannānāṃ buddhānāṃ bhagavatāṃ jñāne,",
    "K10n269u_001:38": " apratiṣṭhito nirvāṇe,",
    "K10n269u_001:39": " sthito bhūtakoṭyām,",
    "K10n269u_001:40": " sthitaḥ sarvasattvālokanīyāyāṃ bhūmau,",
    "K10n269u_001:41": " sarva ime tathāgatānāṃ viśeṣataḥ samyag guṇāḥ |",
    "K10n269u_001:42": " (dharmānusmṛtiḥ)",
    "K10n269u_001:43": " saddharmastu ādau kalyāṇaḥ,",
    "K10n269u_001:44": " madhye kalyāṇaḥ,",
    "K10n269u_001:45": " paryavasāne kalyāṇaḥ,",
    "K10n269u_001:46": " svarthaḥ suvyañjanaḥ,",
    "K10n269u_001:47": " kevalaḥ,",
    "K10n269u_001:48": " paripūrṇaḥ,",
    "K10n269u_001:49": " pariśuddhaḥ,",
    "K10n269u_001:50": " paryavadātaḥ,",
    "K10n269u_001:51": " svākhyātaḥ bhagavato dharmaḥ,",
    "K10n269u_001:52": " sāndṛṣṭikaḥ,",
    "K10n269u_001:53": " nirjvaraḥ,",
    "K10n269u_001:54": " ākālikaḥ,",
    "K10n269u_001:55": " aupanāyikaḥ,",
    "K10n269u_001:56": " aihipaśyikaḥ,",
    "K10n269u_001:57": " pratyātmavedanīyo vijñaiḥ,",
    "K10n269u_001:58": " svākhyāto bhagavato dharmavinayaḥ supraveditaḥ nairyāṇikaḥ,",
    "K10n269u_001:59": " saṃbodhigāmī,",
    "K10n269u_001:60": " abhinnaḥ saṃstūpaḥ,",
    "K10n269u_001:61": " sapratiśaraṇaḥ,",
    "K10n269u_001:62": " chinnaplotikaḥ |",
    "K10n269u_001:63": " (saṃghānusmṛtiḥ)",
    "K10n269u_001:64": " supratipanno bhagavata āryasaṃghaḥ,",
    "K10n269u_001:65": " nyāyapratipannaḥ,",
    "K10n269u_001:66": " ṛjupratipannaḥ,",
    "K10n269u_001:67": " sāmīcīpratipannaḥ,",
    "K10n269u_001:68": " añjalīkaraṇīyaḥ,",
    "K10n269u_001:69": " sāmīcīkaraṇīyaḥ,",
    "K10n269u_001:70": " puṇyaśrīkṣetraḥ,",
    "K10n269u_001:71": " mahādakṣiṇāpariśodhakaḥ,",
    "K10n269u_001:72": " prāhavanīyaḥ,",
    "K10n269u_001:73": " āhavanīyaḥ |",
    "K10n269u_001:74": " || āryatriratnānusmṛtisūtraṃ samāptam ||"
}