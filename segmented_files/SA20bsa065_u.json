{
  "SA20bsa065_u:0": "ācārya vasubandhuviracitam Pañcaskandhaprakaraṇam",
  "SA20bsa065_u:1": "prathamādhikāraḥ - skandhoddeśanirdeśakaḥ",
  "SA20bsa065_u:2": "pañcaskandhāḥ",
  "SA20bsa065_u:3": "rūpaskandhaḥ vedanāskandhaḥ saṃjñāskandhaḥ saṃskāraskandhaḥ vijñānaskandhaśca // 1 //",
  "SA20bsa065_u:4": "rūpasya dvau bhedau",
  "SA20bsa065_u:5": "rūpaṃ katamat /",
  "SA20bsa065_u:6": "yat kiñcid rūpaṃ sarvaṃ taccatvāri mahābhūtāni catvāri ca mahābhūtānyupādāya // 2 //",
  "SA20bsa065_u:7": "mahābhūtanāmāni",
  "SA20bsa065_u:8": "katamāni catvāri mahābhūtāni /",
  "SA20bsa065_u:9": "pṛthivī dhātuḥ abdhātuḥ tejodhātuḥvāyudhātuśca // 3 //",
  "SA20bsa065_u:10": "mahābhūtalakṣaṇāni",
  "SA20bsa065_u:11": "tatra pṛthivīdhātuḥ katamaḥ / kaṭhinatā // 4 //",
  "SA20bsa065_u:12": "abdhātuḥ katamaḥ /",
  "SA20bsa065_u:13": "niṣyandatā // 5 //",
  "SA20bsa065_u:14": "tejodhātuḥ katamaḥ uṣṇatā // 6 //",
  "SA20bsa065_u:15": "vāyudhātuḥ katamaḥ /",
  "SA20bsa065_u:16": "paunaḥ punyakampanatā // 7 //",
  "SA20bsa065_u:17": "upādāyarūpanāmāni",
  "SA20bsa065_u:18": "upādayarūpaṃ katamat /",
  "SA20bsa065_u:19": "cakṣurindriyam,",
  "SA20bsa065_u:20": "śrotrendriyam,",
  "SA20bsa065_u:21": "ghraṇendriyam,",
  "SA20bsa065_u:22": "jihvendriyam,",
  "SA20bsa065_u:23": "kāyendriyam,",
  "SA20bsa065_u:24": "rūpam,",
  "SA20bsa065_u:25": "śabdaḥ,",
  "SA20bsa065_u:26": "gandhaḥ,",
  "SA20bsa065_u:27": "rasaḥ,",
  "SA20bsa065_u:28": "spraṣṭavyaikadeśaḥ,",
  "SA20bsa065_u:29": "abijñaptiśca // 8 //",
  "SA20bsa065_u:30": "upādāyarūpalakṣaṇāni cakṣurindrayaṃ katamat /",
  "SA20bsa065_u:31": "varṇabiṣayo rūpaprasādaḥ // 9 //",
  "SA20bsa065_u:32": "śrotrendrayaṃ katamat /",
  "SA20bsa065_u:33": "śabdaviṣayo rūpaprasādaḥ // 10 //",
  "SA20bsa065_u:34": "ghrāṇendriyam katamat /",
  "SA20bsa065_u:35": "gandhaviṣayo rūpaprasādaḥ // 11 //",
  "SA20bsa065_u:36": "jihvendriyam katamat /",
  "SA20bsa065_u:37": "rasaviṣayo rūpaprasādaḥ // 12 //",
  "SA20bsa065_u:38": "kāyendriyam katamat /",
  "SA20bsa065_u:39": "spraṣṭavyaviṣayo rūpaprasādaḥ // 13 //",
  "SA20bsa065_u:40": "rūpaṃ katamat /",
  "SA20bsa065_u:41": "cakṣurgocaraḥ /",
  "SA20bsa065_u:42": "varṇaḥ saṃsthānam bijñaptiśca // 14 //",
  "SA20bsa065_u:43": "śabdaḥ katamaḥ /",
  "SA20bsa065_u:44": "śrotragocaraḥ /",
  "SA20bsa065_u:45": "niṣpannḥ aniṣpannaḥ ubhayaḥ /",
  "SA20bsa065_u:46": "upāttacatumarhābhutahetukaḥ anupāttacaturmahābhūtahetukaḥ ubhayacaturmahābhūtahetukaśca // 15 //",
  "SA20bsa065_u:47": "gandhaḥ katamaḥ /",
  "SA20bsa065_u:48": "ghrāṇagocaraḥ /",
  "SA20bsa065_u:49": "surabhiḥ asurabhiḥ tadanyaśca // 16 //",
  "SA20bsa065_u:50": "rasaḥ katamaḥjihvāgocaraḥ /",
  "SA20bsa065_u:51": "madhuraḥ amalaḥ lavaṇaḥ kaṭuḥ tiktaḥ kaṣāyaśc // 17 //",
  "SA20bsa065_u:52": "spraṣṭavyaḥ katamaḥ /",
  "SA20bsa065_u:53": "kāyagocaraḥ /",
  "SA20bsa065_u:54": "mahābhūtānyupādāya ślakṣṇatvam karkasatbam gurutvam laghutvam śītatvam jighatsā pipāsā ca // 18 //",
  "SA20bsa065_u:55": "abijñaptirūpaṃ katamat /",
  "SA20bsa065_u:56": "vijñaptisamādhisaṃbhūtaṃ rūpam anidarśanam apratigham // 19 //",
  "SA20bsa065_u:57": "rūpaskandhaḥ soniddeśaḥ pariniṣṭhitaḥ /",
  "SA20bsa065_u:58": "atha vedanā salakṣaṇaprabhedā vedanā katamā /",
  "SA20bsa065_u:59": "trividho 'nubhavaḥ /",
  "SA20bsa065_u:60": "sukhaḥ duḥkhaḥ aduḥkhāsukhaśca // 20 //",
  "SA20bsa065_u:61": "sukho yasmin niruddhe saṃyogecchā jāyate // 21 //",
  "SA20bsa065_u:62": "duḥkho yasminnutpanne viyogecchā jāyate // 22 //",
  "SA20bsa065_u:63": "aduḥkhāsukho yasminnutpanne nobhayecchā jāyane // 23 //",
  "SA20bsa065_u:64": "vedanaskandhaḥ sonideśanirdeśaḥ parinisṭhataḥ /",
  "SA20bsa065_u:65": "atha saṃjñā lakṣaṇa prabhedā saṃjñā katamā /",
  "SA20bsa065_u:66": "viṣayanimittod grahaṇam // 24 //",
  "SA20bsa065_u:67": "sā tridhā parittā mahadgatā apramāṇā ca // 25 //",
  "SA20bsa065_u:68": "saṃjñā skandhaḥ soddeśanirdeśaḥ parinisṭhitaḥ /",
  "SA20bsa065_u:69": "saṃskārāṇāṃ dvaubhedau",
  "SA20bsa065_u:70": "saṃskārāḥ katame /",
  "SA20bsa065_u:71": "vedanāsaṃjñābhyāmanye caitāḥ cittaviprayuktāśca // 26 //",
  "SA20bsa065_u:72": "tatra caitadharmāḥ katame /",
  "SA20bsa065_u:73": "ye dharmārścittena saṃprayuktāḥ // 27 //",
  "SA20bsa065_u:74": "caitaparisaṃkhyānam",
  "SA20bsa065_u:75_0": "te katame / sparśaḥ manaskāraḥ vedanā saṃjñā cetanā chandaḥ adhimokṣaḥ smṛtiḥ samādhiḥ",
  "SA20bsa065_u:75_1": "prajñā śraddhā hrīḥ apatrapā alobhaḥ kuśalamūlam adveṣaḥkuśalamūlam amohaḥkuśalamūlam",
  "SA20bsa065_u:75_2": "vīryam praśrabdhiḥ apramādaḥ upekṣā avihiṃsā rāga pratighaḥ mānaḥ avidyā dṛṣṭiḥ vicikitsā",
  "SA20bsa065_u:75_3": "krodha upanāhaḥ mrakṣaḥ pradāśaḥ īrṣyā mātsaryarm māyā śāṭhyam madaḥ ahiṃsā āhrikyam",
  "SA20bsa065_u:75_4": "anapatrāpyam styānam,",
  "SA20bsa065_u:76": "auddhatyam,",
  "SA20bsa065_u:77": "āśraddhyam,",
  "SA20bsa065_u:78": "kauśīddhyam pramādaḥ muṣitāsmṛti,",
  "SA20bsa065_u:79": "bikṣepaḥ,",
  "SA20bsa065_u:80": "aprasajanyam,",
  "SA20bsa065_u:81": "kaukrityam,",
  "SA20bsa065_u:82": "middham,",
  "SA20bsa065_u:83": "vitarkaḥ, vicāraśca // 28 //",
  "SA20bsa065_u:84": "eteśāṃ paśca sarvatragāḥ /",
  "SA20bsa065_u:85": "pañcapratiniyataviṣayā /",
  "SA20bsa065_u:86": "ekādaśakuśalāḥ /",
  "SA20bsa065_u:87": "ṣaṭkleśāḥ /",
  "SA20bsa065_u:88": "śiṣṭā upakleśāḥ /",
  "SA20bsa065_u:89": "catvāro 'nyatrāpi bhavantītyaniyatāḥ // 29 //",
  "SA20bsa065_u:90": "caittalakṣaṇāni",
  "SA20bsa065_u:91": "sparśaḥkatamaḥ /",
  "SA20bsa065_u:92": "trikasaṃnipāte indriyavikāraparicchedaḥ // 30 //",
  "SA20bsa065_u:93": "manaskāraḥ katamaḥ /",
  "SA20bsa065_u:94": "cetasa ābhogaḥ // 31 //",
  "SA20bsa065_u:95": "cetanā katamā /",
  "SA20bsa065_u:96": "guṇadoṣānubhayeṣu cittābhisaṃskāro manaskarma // 32 //",
  "SA20bsa065_u:97": "chandaḥ katamaḥ /",
  "SA20bsa065_u:98": "abhiprete vastunyabhilāṣaḥ // 33 //",
  "SA20bsa065_u:99": "adhimokṣaḥ katamaḥ /",
  "SA20bsa065_u:100": "niścite vastuni tathaivāvadhāraṇam // 34 //",
  "SA20bsa065_u:101": "smṛtiḥ katamā /",
  "SA20bsa065_u:102": "saṃstute vastunyasaṃpramoṣaḥ // 35 //",
  "SA20bsa065_u:103": "samādhiḥ katamaḥ /",
  "SA20bsa065_u:104": "upaparīkṣye vastuni cittasyaikāgratā // 36 //",
  "SA20bsa065_u:105": "prajñā katamā /",
  "SA20bsa065_u:106": "tatraiva pravicayo yogāyogavihito 'nyathā ca // 37 //",
  "SA20bsa065_u:107": "śraddā katamā /",
  "SA20bsa065_u:108": "karmaphalāsatyaratneṣvabhisaṃpratyayaḥ /",
  "SA20bsa065_u:109": "cetaso 'bhilāṣaḥ cetasaḥ prasādaḥ // 38 //",
  "SA20bsa065_u:110": "hrīḥ / katamā ātmānaṃ dharmaṃ vādhipatiṃ kṛtvā svayamavayena lajjā // 39 //",
  "SA20bsa065_u:111": "apatrapā katamā /",
  "SA20bsa065_u:112": "lokamadhipatiṃ kṛtvā parato 'vadhena lajjā // 40 //",
  "SA20bsa065_u:113": "alobhaḥ katamaḥ /",
  "SA20bsa065_u:114": "lobha pratipakṣaḥ /",
  "SA20bsa065_u:115": "cetaso dānakṣetrāparigrahaḥ // 41 //",
  "SA20bsa065_u:116": "adveṣaḥ katamaḥ /",
  "SA20bsa065_u:117": "dveṣapratipakṣaḥ / maitri // 42 //",
  "SA20bsa065_u:118": "amohaḥ katamaḥ /",
  "SA20bsa065_u:119": "mohapratipakṣaḥ /",
  "SA20bsa065_u:120": "yathābhūtasaṃpratipattiḥ // 43 //",
  "SA20bsa065_u:121": "vīryaṃ katamat kausīddyapratipakṣaḥ kuśale cetaso 'bhyutsāhaḥ // 44 //",
  "SA20bsa065_u:122": "praśrabdhiḥ katamā /",
  "SA20bsa065_u:123": "dauṣṭhulyapratipakṣaḥ kāyacittakarmaṇyatā // 45 //",
  "SA20bsa065_u:124": "apramādaḥ katamaḥ /",
  "SA20bsa065_u:125": "pramādapratipakṣaḥ /",
  "SA20bsa065_u:126": "alobhādīn vīryāntān niśritya akuśalānāṃ dharmāṇāṃ prahāṇam /",
  "SA20bsa065_u:127": "tatpratipakṣāṇāṃ kuśalānāṃ dharmāṇāṃ bhāvanā // 46 //",
  "SA20bsa065_u:128": "upekṣā katamā /",
  "SA20bsa065_u:129": "alobhādīn vīryantān niśritya cittasamatā /",
  "SA20bsa065_u:130": "cittapraśastā /",
  "SA20bsa065_u:131": "cittānābhogatā /",
  "SA20bsa065_u:132": "yayākliṣṭadharmānavakāśenākliṣṭeṣu sthitiḥ // 47 //",
  "SA20bsa065_u:133": "avihiṃsā katamā /",
  "SA20bsa065_u:134": "vihiṃsāpratipakṣaḥ karuṇā // 48 ///",
  "SA20bsa065_u:135": "rāgaḥ katamaḥ /",
  "SA20bsa065_u:136": "upādānaskandheṣvabhyarthanā /",
  "SA20bsa065_u:137": "adhyavasānam // 49 //",
  "SA20bsa065_u:138": "pratighaḥ katamaḥ /",
  "SA20bsa065_u:139": "satveṣvāghātacittatā // 50 //",
  "SA20bsa065_u:140": "mānaḥ katamaḥ /",
  "SA20bsa065_u:141": "mānaḥ saptavidhaḥ /",
  "SA20bsa065_u:142": "atimānaḥ, mānātimānaḥ asmimānaḥ,",
  "SA20bsa065_u:143": "abhimānaḥ,",
  "SA20bsa065_u:144": "ūnamānaḥ,",
  "SA20bsa065_u:145": "mithyāmānaśca // 51 //",
  "SA20bsa065_u:146": "atimānaḥ katamaḥ /",
  "SA20bsa065_u:147": "hīnāt śreyānahamasmi sadṛśena vā sadṛśo 'smīti cintayato yā cittasyonnatiḥ // 53 //",
  "SA20bsa065_u:148": "mānātimānaḥ katamaḥ /",
  "SA20bsa065_u:149": "śreyaso 'hameva śreyāniti cintayato yā cittasyonnatiḥ // 54 //",
  "SA20bsa065_u:150": "asmimānaḥ katamaḥ /",
  "SA20bsa065_u:151": "pañcasūpādānaskandheṣvātmānamātmīyaṃ vā paśyato yā cittasyonnati // 55 //",
  "SA20bsa065_u:152": "abhimānaḥ katamaḥ /",
  "SA20bsa065_u:153": "aprāpta uttare viśeṣādhigame prāpto mayeti cintayato yā cittasyonnatiḥ // 56 //",
  "SA20bsa065_u:154": "ūnamānaḥ katamaḥ /",
  "SA20bsa065_u:155": "bahṛvantaraviśiṣṭādalpāntarahino 'smīti cintayato yā cittasyonnatiḥ // 57 //",
  "SA20bsa065_u:156": "mithyāmānaḥ katamaḥ /",
  "SA20bsa065_u:157": "aguṇavato guṇavānahamasmīti cintayato yā cittasyonnatiḥ // 58 //",
  "SA20bsa065_u:158": "aviddhyā katamā /",
  "SA20bsa065_u:159": "karmaphalasatyaratnānāmajñānam /",
  "SA20bsa065_u:160": "sā punaḥ sahajā parikalpitā ca // 59 //",
  "SA20bsa065_u:161": "rāgacaritasya rāgapratighayoḥ rāgacaritasyāviddyāyāścetyakuśalānāṃ trīṇī mūlāni /",
  "SA20bsa065_u:162": "lobho 'kuśalamūlaṃ ddheṣo 'kuśalamūlaṃ,",
  "SA20bsa065_u:163": "moho 'kuśalamūlaṃ ca // 60 //",
  "SA20bsa065_u:164": "dṛṣṭiḥ katamā pañcadṛṣṭyaḥ - satkāyadṛṣṭiḥ,",
  "SA20bsa065_u:165": "antargrāhyadṛṣṭiḥ,",
  "SA20bsa065_u:166": "mithyādṛṣṭiḥ,",
  "SA20bsa065_u:167": "dṛṣṭiparāmarśaḥ,",
  "SA20bsa065_u:168": "śīlavrataparāmarśaśca // 61 //",
  "SA20bsa065_u:169": "satkāyadṛṣṭiḥ katamā /",
  "SA20bsa065_u:170": "pañcopādāna - skandhānātmata ātmīyato vā paśyato yā kliṣṭā prajñā // 62 //",
  "SA20bsa065_u:171": "antargrāhyadṛṣṭiḥ katamā /",
  "SA20bsa065_u:172": "tānevādhipatiṃ kṛtvā śāśvatata ucchedato vā paśyato yā kliṣṭā prajñā // 63 //",
  "SA20bsa065_u:173": "mithyādṛṣṭiḥ katamā /",
  "SA20bsa065_u:174": "hetuṃ vā phalaṃ vā kriyāṃ vāpavadataḥ sadāvastu vināśayato yā kliṣṭā prajñā // 64 //",
  "SA20bsa065_u:175": "dṛṣṭiparāmarśaḥ katamaḥ /",
  "SA20bsa065_u:176": "tā eva tikhro dṛṣṭistadāśrayāṃśca skandhān śreṣṭhataḥ paramataśca paśyato yā kliṣṭā prajñā // 65 //",
  "SA20bsa065_u:177": "śīlabrataparāmarśaḥ katamaḥ /",
  "SA20bsa065_u:178": "śīlaṃ vrataṃ tadāśrayāṃśca skandhānaśuddhito muktito naiyāṇikataśca paśyato yā kliṣṭā prajñā // 66 //",
  "SA20bsa065_u:179": "vicikitsā katamā /",
  "SA20bsa065_u:180": "satyādiṣu yā dvikoṭikā matiḥ // 67 //",
  "SA20bsa065_u:181": "eteṣāṃ kleśānāṃ paścimāstistro dṛṣṭayaḥ vīcikitsā ca parikalpitāḥ /",
  "SA20bsa065_u:182": "śiṣṭāḥ sahajāḥ parikalpitāśca // 68 //",
  "SA20bsa065_u:183": "krodhaḥ katamaḥ /",
  "SA20bsa065_u:184": "vartaṃmānamapakāramāgamya yā cetasa āghātacetanā // 69 //",
  "SA20bsa065_u:185": "upanāhaḥ katamaḥ /",
  "SA20bsa065_u:186": "vairānubandha // 70 //",
  "SA20bsa065_u:187": "mrakṣaḥ katamaḥ ātmano 'vaddhapracchādanā // 71 //",
  "SA20bsa065_u:188": "pradāśaḥ katamaḥ caṇḍavacodāśitā // 72 //",
  "SA20bsa065_u:189": "īrṣyā katamā parasaṃpattau cetaso vyāropa // 73 //",
  "SA20bsa065_u:190": "mātsarya katamat /",
  "SA20bsa065_u:191": "dānavirodhī cetasa āgrahaḥ // 74 //",
  "SA20bsa065_u:192": "māyā katamā /",
  "SA20bsa065_u:193": "paravaścanābhūtārthasadarśanatā // 75 //",
  "SA20bsa065_u:194": "śāṭhyaṃ katamat /",
  "SA20bsa065_u:195": "svadoṣapracchādanopāyasaṃgṛhitaṃ cetasaḥ kauṭilyam // 76 //",
  "SA20bsa065_u:196": "madaḥ katamaḥ /",
  "SA20bsa065_u:197": "svasaṃpattau raktasyoddharṣaḥ /",
  "SA20bsa065_u:198": "cetasaḥ paryādānam // 77 //",
  "SA20bsa065_u:199": "vihiṃsā katamā /",
  "SA20bsa065_u:200": "satveṣu viheṭhanā // 78 //",
  "SA20bsa065_u:201": "āhrīkyaṃ katamat /",
  "SA20bsa065_u:202": "avadyena svayamalajjā // 79 //",
  "SA20bsa065_u:203": "anapatrāp katamat /",
  "SA20bsa065_u:204": "avadyena parato 'lajjā // 80",
  "SA20bsa065_u:205": "satyānaṃ katamat /",
  "SA20bsa065_u:206": "cittākarmaṇyatā /",
  "SA20bsa065_u:207": "staimityam // 81 //",
  "SA20bsa065_u:208": "auddhatyaṃ katamat /",
  "SA20bsa065_u:209": "cittasyāvyupasamaḥ // 82 //",
  "SA20bsa065_u:210": "āśraddhyaṃ katamat /",
  "SA20bsa065_u:211": "karmaphalasatyaratneṣvanabhisaṃpratyayaḥ /",
  "SA20bsa065_u:212": "śraddhāvipakṣaścetaso 'prasādaḥ // 83 //",
  "SA20bsa065_u:213": "kauśiddyaṃ katamatṃḥ kuśale cetaso 'nabhyutsāho vīryavipakṣa // 84 //",
  "SA20bsa065_u:214": "pramādaḥ katamaḥ /",
  "SA20bsa065_u:215": "yā lobhadveṣamohekauśīddyaiścitasyānārakṣā kuśalasyābhāvanā // 85 //",
  "SA20bsa065_u:216": "muṣitāsmṛtiḥ katamā /",
  "SA20bsa065_u:217": "kliṣṭāsmṛtiḥ /",
  "SA20bsa065_u:218": "kuśalāpratipatiḥ // 86 //",
  "SA20bsa065_u:219": "vikṣepaḥ katamaḥ /",
  "SA20bsa065_u:220": "rāgadveṣamohāṃśikaḥ pañcakāmaguṇeṣu cittasya visāraḥ // 87 //",
  "SA20bsa065_u:221": "aprasajanyaṃ katamat kleśasaṃprayuktā prajñā /",
  "SA20bsa065_u:222": "tayā kāyavākcittacaryā asaṃviditeva pravartate // 88 //",
  "SA20bsa065_u:223": "kaukṛtyaṃ katamat /",
  "SA20bsa065_u:224": "cetaso vipratisāraḥ // 89 //",
  "SA20bsa065_u:225": "middhaṃ katamat /",
  "SA20bsa065_u:226": "asvataṃtravṛtticetaso 'bhisaṃkṣepaḥ // 90 //",
  "SA20bsa065_u:227": "vitarkaḥ katamaḥ paryeṣako manojalpaḥ cetanāprajñāviśeṣaḥ yā cittasyaudārikatā // 91 //",
  "SA20bsa065_u:228": "vicāraḥ katamaḥ /",
  "SA20bsa065_u:229": "paryeṣako manojalpaḥ tathaiva yā cittasya sūkṣmatā // 92 //",
  "SA20bsa065_u:230": "// ittaviprayuktaparisaṃkhyānam //",
  "SA20bsa065_u:231": "cittaviprayuttasaṃskārāḥ katame /",
  "SA20bsa065_u:232": "ye rūpacitacaittadhikāre prajñaptāḥ,",
  "SA20bsa065_u:233": "ta evānyatrāprajñaptāḥ // 93 //",
  "SA20bsa065_u:234": "te katame / prāpti,",
  "SA20bsa065_u:235": "asaṃjñisamāpattiḥ,",
  "SA20bsa065_u:236": "nirodhasamāpattiḥ,",
  "SA20bsa065_u:237": "āsaṃjñikam,",
  "SA20bsa065_u:238": "jīvitendriyam,",
  "SA20bsa065_u:239": "nikāyasabhāgatā,",
  "SA20bsa065_u:240": "jāti,",
  "SA20bsa065_u:241": "jarā,",
  "SA20bsa065_u:242": "sthitiḥ,",
  "SA20bsa065_u:243": "anityatā,",
  "SA20bsa065_u:244": "nāmakāyaḥ,",
  "SA20bsa065_u:245": "padakāyaḥ,",
  "SA20bsa065_u:246": "vyañjanakāyaḥ,",
  "SA20bsa065_u:247": "pṛthagjanatvamityevamādibhedasamādānāḥ // 94 //",
  "SA20bsa065_u:248": "ittaviprayuktalakṣaṇāni //",
  "SA20bsa065_u:249": "tatra prāptiḥ katamā /",
  "SA20bsa065_u:250": "pratilambhaḥ samanvāgamaśca /",
  "SA20bsa065_u:251": "bījam, vaśitā,",
  "SA20bsa065_u:252": "abhimukhībhāvaśca yathāyogam // 95 //",
  "SA20bsa065_u:253": "asaṃjñisamāpattiḥ katamṃ /",
  "SA20bsa065_u:254": "śubhakṛtsnavītarāgasyoparyavītarāgasya niḥsaraṇasaṃjñāpūrvakeṇa manasikāreṇāvasthāvarāṇāṃ cittacaittadharmāṇāṃ yo nirodhaḥ // 96 //",
  "SA20bsa065_u:255": "nirodhasamāpattiḥ katamā /",
  "SA20bsa065_u:256": "ākiñcanyāyatanabītarāgasya bhavāgratāduccalitasya śāntavihāra - saṃjñāpūrvakeṇa manasikāreṇāsthāvarāṇāṃ cittacaittadharmāṇāṃ tadekatyānāṃ ca sthāvarāṇāṃ yo nirodhaḥ // 97 //",
  "SA20bsa065_u:257": "āsaṃjñikaṃ katamat /",
  "SA20bsa065_u:258": "asaṃjñisatvanikāyadeveṣupapannasyāsthāvarāṇāṃ cittacaittadharmāṇāṃ nirodhaḥ // 98 //",
  "SA20bsa065_u:259": "jīvitendriyam katamat /",
  "SA20bsa065_u:260": "nikāyasabhāgeṣu pūrvakarmānuviddhyeṣu yaḥ saṃskārāṇāṃ sthitikālaniyamaḥ // 99 //",
  "SA20bsa065_u:261": "nikāyasabhāgaḥ katamaḥ /",
  "SA20bsa065_u:262": "yā satvānāmātmabhāvasadṛśatā // 100 //",
  "SA20bsa065_u:263": "jātiḥkatamā /",
  "SA20bsa065_u:264": "nikāyasabhāge saṃskārāṇāmabhūtvā yo bhāṣaḥ // 101 //",
  "SA20bsa065_u:265": "jarā katamā /",
  "SA20bsa065_u:266": "tathā teṣāṃ prabandhānyathātvam // 102 //",
  "SA20bsa065_u:267": "sthiti katamā /",
  "SA20bsa065_u:268": "tathā teṣāṃ prabandhanvayaḥ // 103 //",
  "SA20bsa065_u:269": "anityatā katamā /",
  "SA20bsa065_u:270": "tathā teṣāṃ prabandhavināśaḥ // 104 //",
  "SA20bsa065_u:271": "nāmakāyaḥ katamaḥ /",
  "SA20bsa065_u:272": "dharmāṇāṃ svabhāvādhivacanam // 105 //",
  "SA20bsa065_u:273": "pādakāyaḥ katamaḥ /",
  "SA20bsa065_u:274": "dharmāṇāṃ viśeṣādhivacanam // 106 //",
  "SA20bsa065_u:275": "vyañjanakāyaḥ katamaḥ /",
  "SA20bsa065_u:276": "akṣarāṇi /",
  "SA20bsa065_u:277": "tadubhayābhivyañjanatāmupādāya // 107 //",
  "SA20bsa065_u:278": "vākyāni tāni /",
  "SA20bsa065_u:279": "nāmapadāśreyeṇārthavacanatāmupādāya // 108 //",
  "SA20bsa065_u:280": "akṣaraṃ punaḥ paryāyākṣaraṇatāmupādāya // 109 //",
  "SA20bsa065_u:281": "pṛthagjanatvaṃ katamat /",
  "SA20bsa065_u:282": "āryadharmāṇāmapratilambhaḥ // 110 //",
  "SA20bsa065_u:283": "iti saṃskāraskandho nāma //",
  "SA20bsa065_u:284": "saṃskāraskandhaḥ soddyeśanirdeśaḥ pariniṣṭhitaḥ // 111 //",
  "SA20bsa065_u:285": "// vijñānalakṣaṇam //",
  "SA20bsa065_u:286": "vijñānaṃ katamat /",
  "SA20bsa065_u:287": "ālambanaṃbijñaptiḥ // 112 //",
  "SA20bsa065_u:288": "cittamanasī vijñānaparyāyau sanirvacano taccitaṃ mano 'pi citrīkāratā manoniśrayatāṃ ca upādāya // 113 //",
  "SA20bsa065_u:289": "// salakṣaṇacittanirdeśaḥ //",
  "SA20bsa065_u:290": "mūlacittamālayavijñānaṃ tadyathedaṃ sarvasaṃskārāṇāṃ saṃcitaṃ bījam // 114 //",
  "SA20bsa065_u:291": "tatpunarālambanamapyuparichinnamekasantānavarti /",
  "SA20bsa065_u:292": "yathā nirodhasamāpatyasaṃjñisamāpatyā - saṃjñikebhyovyutthitasyāsmād viṣayavijñaptikaṃ nāma pravṛtti vijñānaṃ jāyate /",
  "SA20bsa065_u:293": "ālambanapratyayamapekṣya bhinnākāreṣu vṛttitāmucchiddya punarjātatāṃ saṃsāre pravṛttinivṛtitāṃ copādāya tadālambanaṃ nāma vijñānam // 115 //",
  "SA20bsa065_u:294": "ālayavijñānaṃ hi sarvabījālayatāmātmabhāvālayahetutāmātmabhāve sthititāṃ copādāya // 116 //",
  "SA20bsa065_u:295": "ādānavijñānamapi tat /",
  "SA20bsa065_u:296": "ātmabhāvādānatāmupādāya // 117 //",
  "SA20bsa065_u:297": "// salakṣaṇamanonirdeśaḥ //",
  "SA20bsa065_u:298": "mūlamana ālayavijñānamāśritya nityamātmamohātmadṛṣṭyātmamānātmasnehaiḥ saṃprayuktaṃ vijñānamekajātīyasantānavarti arhatvāryamārganirodhasamāpattikāle vyāvartate // 118 //",
  "SA20bsa065_u:299": "vijñānaskandhaḥ soniddeśanirdeśaḥ pariniṣṭhitaḥ //",
  "SA20bsa065_u:300": "// skandhanirvacanam //",
  "SA20bsa065_u:301": "kimarthaḥ skandhaḥ rāśyarthaḥ /",
  "SA20bsa065_u:302": "kālagotrākāragativiṣayabhinnānāṃ rūpādīnāmabhisaṃkṣepatāmupādāya // 119 //",
  "SA20bsa065_u:303": "skandhoddyeśanirdeśakaḥ prathamādhikāraḥ pariniṣṭhitaḥ //",
  "SA20bsa065_u:304": "dvitīyādhikāraḥ",
  "SA20bsa065_u:305": "āyatanoddyeśanirdeśakaḥ",
  "SA20bsa065_u:306": "dvādaśāyatanāni /",
  "SA20bsa065_u:307": "cakṣurāyatanam,",
  "SA20bsa065_u:308": "rūpāyatanam,",
  "SA20bsa065_u:309": "śrotrāyatanam,",
  "SA20bsa065_u:310": "śabdāyatanam,",
  "SA20bsa065_u:311": "ghrāṇāyatanam,",
  "SA20bsa065_u:312": "gandhāyatanam,",
  "SA20bsa065_u:313": "jihvāyatanam,",
  "SA20bsa065_u:314": "rasāyatanam,",
  "SA20bsa065_u:315": "kāyāyatanam,",
  "SA20bsa065_u:316": "spraṣṭavyāyatanam,",
  "SA20bsa065_u:317": "mana āyatanam,",
  "SA20bsa065_u:318": "dharmāyatanaṃ ca // 120 //",
  "SA20bsa065_u:319": "/ 'ayatanānāṃ salakṣaṇaprapañcaḥ //",
  "SA20bsa065_u:320": "cakṣurādīni rūpaśabdagandharasāyatanāni yathoktapūrvāṇi // 121 //",
  "SA20bsa065_u:321": "spraṣṭavyāyatanaṃ catvāri mahābhūtāni uktaśca yaḥ spraṣṭabyaikadeśaḥ // 122 //",
  "SA20bsa065_u:322": "mana āyatanaṃ yo vijñānaskandhaḥ // 123 //",
  "SA20bsa065_u:323": "dharmāyatanaṃ vedanā,",
  "SA20bsa065_u:324": "saṃjñā,",
  "SA20bsa065_u:325": "saṃskārāḥ,",
  "SA20bsa065_u:326": "avijñaptiḥ,",
  "SA20bsa065_u:327": "asaṃskṛtāśca // 124 //",
  "SA20bsa065_u:328": "/ 'saṃskṛtāḥ salakṣaṇaprabhedāḥ //",
  "SA20bsa065_u:329": "asaṃskṛtā katame /",
  "SA20bsa065_u:330": "ākāśam, apratisaṃkhyānirodhaḥ,",
  "SA20bsa065_u:331": "pratisaṃkhyānirodhaḥ tathatā ca // 125 //",
  "SA20bsa065_u:332": "tatrākāśaṃ katamat /",
  "SA20bsa065_u:333": "rūpāvakāśaḥ // 126 //",
  "SA20bsa065_u:334": "apratisaṃkhyānirodhaḥ katamaḥ /",
  "SA20bsa065_u:335": "yo nirodho na visaṃyogaḥ // 127 //",
  "SA20bsa065_u:336": "sakleśāpratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 128 //",
  "SA20bsa065_u:337": "pratisaṃkhyānirodhaḥ katamaḥ /",
  "SA20bsa065_u:338": "yo nirodho visaṃyogaḥ // 129 //",
  "SA20bsa065_u:339": "sakleśapratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 130 //",
  "SA20bsa065_u:340": "tathatā katamā /",
  "SA20bsa065_u:341": "yā dharmāṇāṃ dharmatā /",
  "SA20bsa065_u:342": "dharmanairātmyam // 131 //",
  "SA20bsa065_u:343": "/ 'ayatananirvacanam //",
  "SA20bsa065_u:344": "kimupādāyāyatanamiti /",
  "SA20bsa065_u:345": "vijñānotpādadvāratāmupādāya // 132 //",
  "SA20bsa065_u:346": "āyatanoddyeśanirdeśako dvitīyādhikāraḥ pariniṣṭhitaḥ /",
  "SA20bsa065_u:347": "tṛtīyādhikāraḥ",
  "SA20bsa065_u:348": "dhātūddeśanirdeśakaḥ",
  "SA20bsa065_u:349": "// dhātunāmāni //",
  "SA20bsa065_u:350": "aṣṭadaśa dhātavaḥ /",
  "SA20bsa065_u:351": "cakṣurdhātuḥ,",
  "SA20bsa065_u:352": "rūpadhātuḥ,",
  "SA20bsa065_u:353": "cakṣurvijñānadhātu,",
  "SA20bsa065_u:354": "śrotradhātuḥ,",
  "SA20bsa065_u:355": "śabdadhātuḥ,",
  "SA20bsa065_u:356": "śrotravijñānadhātuḥ,",
  "SA20bsa065_u:357": "ghrāṇadhātuḥ,",
  "SA20bsa065_u:358": "gandhadhātuḥ,",
  "SA20bsa065_u:359": "ghrāṇavijñānadhātuḥ,",
  "SA20bsa065_u:360": "jihvādhātuḥ,",
  "SA20bsa065_u:361": "rasadhātuḥ,",
  "SA20bsa065_u:362": "jihvāvijñānadhātuḥ,",
  "SA20bsa065_u:363": "kāyadhātuḥ,",
  "SA20bsa065_u:364": "spraṣṭavyadhātuḥ,",
  "SA20bsa065_u:365": "kāyabijñānadhātuḥ,",
  "SA20bsa065_u:366": "manodhātuḥ,",
  "SA20bsa065_u:367": "dharmadhātuḥ,",
  "SA20bsa065_u:368": "manovijñānadhātuśca // 133 //",
  "SA20bsa065_u:369": "// dhātuprapaścaḥ //",
  "SA20bsa065_u:370": "cakṣurādayo dhātavo rūpādayo dhātavaśca yathāyatanāni // 134 //",
  "SA20bsa065_u:371": "ṣaḍvijñānadhātavaścakṣurāddyāśrayā rūpādyālambanāvijñaptayaḥ // 135 //",
  "SA20bsa065_u:372": "manodhātusteṣāmeva samanantaranirudhyaṃ ṣaḍvijñānadeśanāśrayatāmupādāya // 136 //",
  "SA20bsa065_u:373": "// dhātuvyavasthanam //",
  "SA20bsa065_u:374": "ebamaṣṭādaśadhātuvyavasthānam /",
  "SA20bsa065_u:375": "yo rūpaskandhastāni daśāyatanadhātavaḥ // 137 //",
  "SA20bsa065_u:376": "dharmāyatanaṃ dharmadhātvekadeśaḥ // 138 //",
  "SA20bsa065_u:377": "yo vijñānaskandhastanmana āyatanam,",
  "SA20bsa065_u:378": "saptama manodhātavaśca // 139 //",
  "SA20bsa065_u:380": "anye ye trayaḥ skandhā rūpadhātvekadeśaśca,",
  "SA20bsa065_u:381": "te 'saṃskṛtai sahitā dharmāyatanaṃ dharmadhātuśca // 140 //",
  "SA20bsa065_u:382": "// dhātunirvacanam //",
  "SA20bsa065_u:383": "kimupādāya dhātava iti /",
  "SA20bsa065_u:384": "ākārakātmalakṣaṇadhāraṇatāmupādāya // 141 //",
  "SA20bsa065_u:385": "// skandhāyatanadhātukrameṇa dharmadeśanāyāḥ prayojanam //",
  "SA20bsa065_u:386": "kimartha skandhādikrameṇa deśanā /",
  "SA20bsa065_u:387": "trividhātmagrāhapratipakṣeṇāyaṃ kramaḥ // 142 //",
  "SA20bsa065_u:388": "ātmagrāhastrividhaḥ ekātmagrāhaḥ,",
  "SA20bsa065_u:389": "vedakātmagrāhaḥ,",
  "SA20bsa065_u:390": "kārakātmagrāhaśca // 143 //",
  "SA20bsa065_u:391": "// 'ṣṭādaśadhātuvikalpāḥ //",
  "SA20bsa065_u:392": "aṣṭādaśadhātuṣu kati rūpiṇaḥ /",
  "SA20bsa065_u:393": "ye rūpaskandhasvabhāvāḥ // 144 //",
  "SA20bsa065_u:394": "katyarūpiṇaḥ / śiṣṭāḥ // 145 //",
  "SA20bsa065_u:395": "kati sanidarśanāḥ /",
  "SA20bsa065_u:396": "rūpadhāturekaḥ sanidarśanaviṣayaḥ // 146 //",
  "SA20bsa065_u:397": "katyanidarśanāḥ / śiṣṭāḥ // 147 //",
  "SA20bsa065_u:398": "kati sapratidhāḥ /",
  "SA20bsa065_u:399": "daśa rūpiṇaḥ /",
  "SA20bsa065_u:400": "yatra yatpratidhātastatra tat sapratigham // 148 //",
  "SA20bsa065_u:401": "katyapratighāḥ / śiṣṭāḥ // 149 //",
  "SA20bsa065_u:402": "kati sāsravāḥ /",
  "SA20bsa065_u:403": "pañcadaśa paścimānāṃ trayāṇāmekadeśaśca /",
  "SA20bsa065_u:404": "te kleśotpādapratyakṣacaryāviṣayatāmupādāyaṃ // 150 //",
  "SA20bsa065_u:405": "katyayanāstravāḥ /",
  "SA20bsa065_u:406": "paścimānāṃ trayāṇāmekadeśaḥ // 151 //",
  "SA20bsa065_u:407": "kati kāmapratisaṃyuktāḥ / sarva // 152 //",
  "SA20bsa065_u:408": "kati rūpapratisaṃyuktāḥ /",
  "SA20bsa065_u:409": "caturdaśa sthāpayitvā gandharasaghrāṇajihvāvijñānadhātūn // 153 //",
  "SA20bsa065_u:410": "katyārūpyasaṃprayuktāḥ /",
  "SA20bsa065_u:411": "paścimāśrayaḥ // 154 //",
  "SA20bsa065_u:412": "katyapratisaṃyuktāḥ /",
  "SA20bsa065_u:413": "trayāṇāmekadeśaḥ // 155 //",
  "SA20bsa065_u:414": "kati skandhasaṃgrahītāḥ /",
  "SA20bsa065_u:415": "asaṃskṛtān sthāpayitvā // 156 //",
  "SA20bsa065_u:416": "katyupādānaskandhasaṃgṛhītāḥ ye sāsravāḥ // 157 //",
  "SA20bsa065_u:417": "kati kuśalākuśalāvyākṛtāḥ /",
  "SA20bsa065_u:418": "daśa trividhāḥ /",
  "SA20bsa065_u:419": "sapta manodhātavaḥ,",
  "SA20bsa065_u:420": "rūpaśabdadharmadhātavaśca /",
  "SA20bsa065_u:421": "śiṣṭā avyākṛtāḥ // 158 //",
  "SA20bsa065_u:422": "katyādhyātmikāḥ /",
  "SA20bsa065_u:423": "dvādaśa /",
  "SA20bsa065_u:424": "sthāpayitvā rūpaśabda gandharasaspraṣṭāvyadharmadhātūn // 159 //",
  "SA20bsa065_u:425": "kati bāhyāḥ /",
  "SA20bsa065_u:426": "ṣaṭ /",
  "SA20bsa065_u:427": "ye sthāpitāḥ // 160 //",
  "SA20bsa065_u:428": "kati sālambanāḥ /",
  "SA20bsa065_u:429": "saptacittadhātavaḥ dharmadhātvekadeśo 'pi yaścaittaḥ // 161 //",
  "SA20bsa065_u:430": "katyanālambanāḥ /",
  "SA20bsa065_u:431": "śiṣṭā daśa dharmadhātvekadeśaśca // 162 //",
  "SA20bsa065_u:432": "kati savitarkāḥ /",
  "SA20bsa065_u:433": "manodhātuḥ manovijñānadhātuḥ dharmadhātvekadeśaśca // 163 //",
  "SA20bsa065_u:434": "kati niṣpannāḥ /",
  "SA20bsa065_u:435": "pañcādhyātmikāḥ /",
  "SA20bsa065_u:436": "caturṇāmekadeśaḥ // 164 //",
  "SA20bsa065_u:437": "katyaniṣpannāḥ śiṣṭā daśa,",
  "SA20bsa065_u:438": "caturṇāmekadeśaśca // 165 //",
  "SA20bsa065_u:439": "kati sabhāgāḥ /",
  "SA20bsa065_u:440": "ādhyātmikāḥ pañca rūpiṇaḥ /",
  "SA20bsa065_u:441": "svavijñānasahitaviṣayasadṛśatāmupādāya // 166 //",
  "SA20bsa065_u:442": "kati tatsabhāgāḥ /",
  "SA20bsa065_u:443": "ta eva svavijñānavirahitasvānvayasadṛśatāmupādāya // 167 //",
  "SA20bsa065_u:444": "dhātūddyeśanirdeśakastṛtīyādhikaraḥ pariniṣṭitaḥ /",
  "SA20bsa065_u:445": "ācāryavasubandhuviracit pañcaskandhaprakaraṇaṃ samāptam /",
  "SA20bsa065_u:446": "pañcaskandhaprakaraṇaṃ śāstriṇā śāntibhikṣuṇā /",
  "SA20bsa065_u:447": "bhoṭānuvādamāgamya saṃskṛte punaruddhṛtam",
  "SA20bsa065_u:448": "yadatra sukṛtaṃ sarva tadācāryasya dhīmataḥ /",
  "SA20bsa065_u:449": "yat kukṛtaṃ tadāgo me kṣantavyaṃ sādhusūribhiḥ //"
}