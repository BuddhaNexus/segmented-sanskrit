{
  "SA20bsa065_u:0": "ācārya vasubandhuviracitam Pañcaskandhaprakaraṇam",
  "SA20bsa065_u:1": "prathamādhikāraḥ - skandhoddeśanirdeśakaḥ",
  "SA20bsa065_u:2": "pañcaskandhāḥ",
  "SA20bsa065_u:3": "rūpaskandhaḥ vedanāskandhaḥ saṃjñāskandhaḥ saṃskāraskandhaḥ vijñānaskandhaśca // 1 //",
  "SA20bsa065_u:4": "rūpasya dvau bhedau",
  "SA20bsa065_u:5": "rūpaṃ katamat /",
  "SA20bsa065_u:6": "yat kiñcid rūpaṃ sarvaṃ taccatvāri mahābhūtāni catvāri ca mahābhūtānyupādāya // 2 //",
  "SA20bsa065_u:7": "mahābhūtanāmāni",
  "SA20bsa065_u:8": "katamāni catvāri mahābhūtāni / pṛthivī dhātuḥ abdhātuḥ tejodhātuḥvāyudhātuśca // 3 //",
  "SA20bsa065_u:9": "mahābhūtalakṣaṇāni",
  "SA20bsa065_u:10": "tatra pṛthivīdhātuḥ katamaḥ / kaṭhinatā // 4 //",
  "SA20bsa065_u:11": "abdhātuḥ katamaḥ / niṣyandatā // 5 //",
  "SA20bsa065_u:12": "tejodhātuḥ katamaḥ uṣṇatā // 6 //",
  "SA20bsa065_u:13": "vāyudhātuḥ katamaḥ / paunaḥ punyakampanatā // 7 //",
  "SA20bsa065_u:14": "upādāyarūpanāmāni",
  "SA20bsa065_u:15": "upādayarūpaṃ katamat /",
  "SA20bsa065_u:16": "cakṣurindriyam,",
  "SA20bsa065_u:17": "śrotrendriyam,",
  "SA20bsa065_u:18": "ghraṇendriyam,",
  "SA20bsa065_u:19": "jihvendriyam,",
  "SA20bsa065_u:20": "kāyendriyam,",
  "SA20bsa065_u:21": "rūpam,",
  "SA20bsa065_u:22": "śabdaḥ,",
  "SA20bsa065_u:23": "gandhaḥ,",
  "SA20bsa065_u:24": "rasaḥ,",
  "SA20bsa065_u:25": "spraṣṭavyaikadeśaḥ,",
  "SA20bsa065_u:26": "abijñaptiśca // 8 //",
  "SA20bsa065_u:27": "upādāyarūpalakṣaṇāni cakṣurindrayaṃ katamat / varṇabiṣayo rūpaprasādaḥ // 9 //",
  "SA20bsa065_u:28": "śrotrendrayaṃ katamat / śabdaviṣayo rūpaprasādaḥ // 10 //",
  "SA20bsa065_u:29": "ghrāṇendriyam katamat / gandhaviṣayo rūpaprasādaḥ // 11 //",
  "SA20bsa065_u:30": "jihvendriyam katamat / rasaviṣayo rūpaprasādaḥ // 12 //",
  "SA20bsa065_u:31": "kāyendriyam katamat / spraṣṭavyaviṣayo rūpaprasādaḥ // 13 //",
  "SA20bsa065_u:32": "rūpaṃ katamat / cakṣurgocaraḥ / varṇaḥ saṃsthānam bijñaptiśca // 14 //",
  "SA20bsa065_u:33": "śabdaḥ katamaḥ /",
  "SA20bsa065_u:34": "śrotragocaraḥ /",
  "SA20bsa065_u:35": "niṣpannḥ aniṣpannaḥ ubhayaḥ /",
  "SA20bsa065_u:36": "upāttacatumarhābhutahetukaḥ anupāttacaturmahābhūtahetukaḥ ubhayacaturmahābhūtahetukaśca // 15 //",
  "SA20bsa065_u:37": "gandhaḥ katamaḥ / ghrāṇagocaraḥ / surabhiḥ asurabhiḥ tadanyaśca // 16 //",
  "SA20bsa065_u:38": "rasaḥ katamaḥjihvāgocaraḥ / madhuraḥ amalaḥ lavaṇaḥ kaṭuḥ tiktaḥ kaṣāyaśc // 17 //",
  "SA20bsa065_u:39": "spraṣṭavyaḥ katamaḥ /",
  "SA20bsa065_u:40": "kāyagocaraḥ /",
  "SA20bsa065_u:41": "mahābhūtānyupādāya ślakṣṇatvam karkasatbam gurutvam laghutvam śītatvam jighatsā pipāsā ca // 18 //",
  "SA20bsa065_u:42": "abijñaptirūpaṃ katamat / vijñaptisamādhisaṃbhūtaṃ rūpam anidarśanam apratigham // 19 //",
  "SA20bsa065_u:43": "rūpaskandhaḥ soniddeśaḥ pariniṣṭhitaḥ /",
  "SA20bsa065_u:44": "atha vedanā salakṣaṇaprabhedā vedanā katamā /",
  "SA20bsa065_u:45": "trividho 'nubhavaḥ /",
  "SA20bsa065_u:46": "sukhaḥ duḥkhaḥ aduḥkhāsukhaśca // 20 //",
  "SA20bsa065_u:47": "sukho yasmin niruddhe saṃyogecchā jāyate // 21 //",
  "SA20bsa065_u:48": "duḥkho yasminnutpanne viyogecchā jāyate // 22 //",
  "SA20bsa065_u:49": "aduḥkhāsukho yasminnutpanne nobhayecchā jāyane // 23 //",
  "SA20bsa065_u:50": "vedanaskandhaḥ sonideśanirdeśaḥ parinisṭhataḥ /",
  "SA20bsa065_u:51": "atha saṃjñā lakṣaṇa prabhedā saṃjñā katamā / viṣayanimittod grahaṇam // 24 //",
  "SA20bsa065_u:52": "sā tridhā parittā mahadgatā apramāṇā ca // 25 //",
  "SA20bsa065_u:53": "saṃjñā skandhaḥ soddeśanirdeśaḥ parinisṭhitaḥ /",
  "SA20bsa065_u:54": "saṃskārāṇāṃ dvaubhedau",
  "SA20bsa065_u:55": "saṃskārāḥ katame / vedanāsaṃjñābhyāmanye caitāḥ cittaviprayuktāśca // 26 //",
  "SA20bsa065_u:56": "tatra caitadharmāḥ katame / ye dharmārścittena saṃprayuktāḥ // 27 //",
  "SA20bsa065_u:57": "caitaparisaṃkhyānam",
  "SA20bsa065_u:58": "te katame /",
  "SA20bsa065_u:59_0": "sparśaḥ manaskāraḥ vedanā saṃjñā cetanā chandaḥ adhimokṣaḥ smṛtiḥ samādhiḥ prajñā",
  "SA20bsa065_u:59_1": "śraddhā hrīḥ apatrapā alobhaḥ kuśalamūlam adveṣaḥkuśalamūlam amohaḥkuśalamūlam vīryam",
  "SA20bsa065_u:59_2": "praśrabdhiḥ apramādaḥ upekṣā avihiṃsā rāga pratighaḥ mānaḥ avidyā dṛṣṭiḥ vicikitsā",
  "SA20bsa065_u:59_3": "krodha upanāhaḥ mrakṣaḥ pradāśaḥ īrṣyā mātsaryarm māyā śāṭhyam madaḥ ahiṃsā āhrikyam",
  "SA20bsa065_u:59_4": "anapatrāpyam styānam,",
  "SA20bsa065_u:60": "auddhatyam,",
  "SA20bsa065_u:61": "āśraddhyam,",
  "SA20bsa065_u:62": "kauśīddhyam pramādaḥ muṣitāsmṛti,",
  "SA20bsa065_u:63": "bikṣepaḥ,",
  "SA20bsa065_u:64": "aprasajanyam,",
  "SA20bsa065_u:65": "kaukrityam,",
  "SA20bsa065_u:66": "middham,",
  "SA20bsa065_u:67": "vitarkaḥ, vicāraśca // 28 //",
  "SA20bsa065_u:68": "eteśāṃ paśca sarvatragāḥ /",
  "SA20bsa065_u:69": "pañcapratiniyataviṣayā /",
  "SA20bsa065_u:70": "ekādaśakuśalāḥ /",
  "SA20bsa065_u:71": "ṣaṭkleśāḥ /",
  "SA20bsa065_u:72": "śiṣṭā upakleśāḥ /",
  "SA20bsa065_u:73": "catvāro 'nyatrāpi bhavantītyaniyatāḥ // 29 //",
  "SA20bsa065_u:74": "caittalakṣaṇāni",
  "SA20bsa065_u:75": "sparśaḥkatamaḥ / trikasaṃnipāte indriyavikāraparicchedaḥ // 30 //",
  "SA20bsa065_u:76": "manaskāraḥ katamaḥ / cetasa ābhogaḥ // 31 //",
  "SA20bsa065_u:77": "cetanā katamā / guṇadoṣānubhayeṣu cittābhisaṃskāro manaskarma // 32 //",
  "SA20bsa065_u:78": "chandaḥ katamaḥ / abhiprete vastunyabhilāṣaḥ // 33 //",
  "SA20bsa065_u:79": "adhimokṣaḥ katamaḥ / niścite vastuni tathaivāvadhāraṇam // 34 //",
  "SA20bsa065_u:80": "smṛtiḥ katamā / saṃstute vastunyasaṃpramoṣaḥ // 35 //",
  "SA20bsa065_u:81": "samādhiḥ katamaḥ / upaparīkṣye vastuni cittasyaikāgratā // 36 //",
  "SA20bsa065_u:82": "prajñā katamā / tatraiva pravicayo yogāyogavihito 'nyathā ca // 37 //",
  "SA20bsa065_u:83": "śraddā katamā / karmaphalāsatyaratneṣvabhisaṃpratyayaḥ / cetaso 'bhilāṣaḥ cetasaḥ prasādaḥ // 38 //",
  "SA20bsa065_u:84": "hrīḥ / katamā ātmānaṃ dharmaṃ vādhipatiṃ kṛtvā svayamavayena lajjā // 39 //",
  "SA20bsa065_u:85": "apatrapā katamā / lokamadhipatiṃ kṛtvā parato 'vadhena lajjā // 40 //",
  "SA20bsa065_u:86": "alobhaḥ katamaḥ / lobha pratipakṣaḥ / cetaso dānakṣetrāparigrahaḥ // 41 //",
  "SA20bsa065_u:87": "adveṣaḥ katamaḥ / dveṣapratipakṣaḥ / maitri // 42 //",
  "SA20bsa065_u:88": "amohaḥ katamaḥ / mohapratipakṣaḥ / yathābhūtasaṃpratipattiḥ // 43 //",
  "SA20bsa065_u:89": "vīryaṃ katamat kausīddyapratipakṣaḥ kuśale cetaso 'bhyutsāhaḥ // 44 //",
  "SA20bsa065_u:90": "praśrabdhiḥ katamā / dauṣṭhulyapratipakṣaḥ kāyacittakarmaṇyatā // 45 //",
  "SA20bsa065_u:91": "apramādaḥ katamaḥ /",
  "SA20bsa065_u:92": "pramādapratipakṣaḥ /",
  "SA20bsa065_u:93": "alobhādīn vīryāntān niśritya akuśalānāṃ dharmāṇāṃ prahāṇam /",
  "SA20bsa065_u:94": "tatpratipakṣāṇāṃ kuśalānāṃ dharmāṇāṃ bhāvanā // 46 //",
  "SA20bsa065_u:95": "upekṣā katamā /",
  "SA20bsa065_u:96": "alobhādīn vīryantān niśritya cittasamatā /",
  "SA20bsa065_u:97": "cittapraśastā /",
  "SA20bsa065_u:98": "cittānābhogatā /",
  "SA20bsa065_u:99": "yayākliṣṭadharmānavakāśenākliṣṭeṣu sthitiḥ // 47 //",
  "SA20bsa065_u:100": "avihiṃsā katamā / vihiṃsāpratipakṣaḥ karuṇā // 48 ///",
  "SA20bsa065_u:101": "rāgaḥ katamaḥ / upādānaskandheṣvabhyarthanā / adhyavasānam // 49 //",
  "SA20bsa065_u:102": "pratighaḥ katamaḥ / satveṣvāghātacittatā // 50 //",
  "SA20bsa065_u:103": "mānaḥ katamaḥ /",
  "SA20bsa065_u:104": "mānaḥ saptavidhaḥ /",
  "SA20bsa065_u:105": "atimānaḥ, mānātimānaḥ asmimānaḥ, abhimānaḥ, ūnamānaḥ,mithyāmānaśca // 51 //",
  "SA20bsa065_u:106": "atimānaḥ katamaḥ /",
  "SA20bsa065_u:107": "hīnāt śreyānahamasmi sadṛśena vā sadṛśo 'smīti cintayato yā cittasyonnatiḥ // 53 //",
  "SA20bsa065_u:108": "mānātimānaḥ katamaḥ / śreyaso 'hameva śreyāniti cintayato yā cittasyonnatiḥ // 54 //",
  "SA20bsa065_u:109": "asmimānaḥ katamaḥ / pañcasūpādānaskandheṣvātmānamātmīyaṃ vā paśyato yā cittasyonnati // 55 //",
  "SA20bsa065_u:110": "abhimānaḥ katamaḥ / aprāpta uttare viśeṣādhigame prāpto mayeti cintayato yā cittasyonnatiḥ // 56 //",
  "SA20bsa065_u:111": "ūnamānaḥ katamaḥ / bahṛvantaraviśiṣṭādalpāntarahino 'smīti cintayato yā cittasyonnatiḥ // 57 //",
  "SA20bsa065_u:112": "mithyāmānaḥ katamaḥ / aguṇavato guṇavānahamasmīti cintayato yā cittasyonnatiḥ // 58 //",
  "SA20bsa065_u:113": "aviddhyā katamā / karmaphalasatyaratnānāmajñānam / sā punaḥ sahajā parikalpitā ca // 59 //",
  "SA20bsa065_u:114": "rāgacaritasya rāgapratighayoḥ rāgacaritasyāviddyāyāścetyakuśalānāṃ trīṇī mūlāni /",
  "SA20bsa065_u:115": "lobho 'kuśalamūlaṃ ddheṣo 'kuśalamūlaṃ, moho 'kuśalamūlaṃ ca // 60 //",
  "SA20bsa065_u:116": "dṛṣṭiḥ katamā pañcadṛṣṭyaḥ - satkāyadṛṣṭiḥ,",
  "SA20bsa065_u:117": "antargrāhyadṛṣṭiḥ,",
  "SA20bsa065_u:118": "mithyādṛṣṭiḥ,",
  "SA20bsa065_u:119": "dṛṣṭiparāmarśaḥ,",
  "SA20bsa065_u:120": "śīlavrataparāmarśaśca // 61 //",
  "SA20bsa065_u:121": "satkāyadṛṣṭiḥ katamā / pañcopādāna - skandhānātmata ātmīyato vā paśyato yā kliṣṭā prajñā // 62 //",
  "SA20bsa065_u:122": "antargrāhyadṛṣṭiḥ katamā /",
  "SA20bsa065_u:123": "tānevādhipatiṃ kṛtvā śāśvatata ucchedato vā paśyato yā kliṣṭā prajñā // 63 //",
  "SA20bsa065_u:124": "mithyādṛṣṭiḥ katamā /",
  "SA20bsa065_u:125": "hetuṃ vā phalaṃ vā kriyāṃ vāpavadataḥ sadāvastu vināśayato yā kliṣṭā prajñā // 64 //",
  "SA20bsa065_u:126": "dṛṣṭiparāmarśaḥ katamaḥ /",
  "SA20bsa065_u:127": "tā eva tikhro dṛṣṭistadāśrayāṃśca skandhān śreṣṭhataḥ paramataśca paśyato yā kliṣṭā prajñā // 65 //",
  "SA20bsa065_u:128": "śīlabrataparāmarśaḥ katamaḥ /",
  "SA20bsa065_u:129": "śīlaṃ vrataṃ tadāśrayāṃśca skandhānaśuddhito muktito naiyāṇikataśca paśyato yā kliṣṭā prajñā // 66 //",
  "SA20bsa065_u:130": "vicikitsā katamā / satyādiṣu yā dvikoṭikā matiḥ // 67 //",
  "SA20bsa065_u:131": "eteṣāṃ kleśānāṃ paścimāstistro dṛṣṭayaḥ vīcikitsā ca parikalpitāḥ /",
  "SA20bsa065_u:132": "śiṣṭāḥ sahajāḥ parikalpitāśca // 68 //",
  "SA20bsa065_u:133": "krodhaḥ katamaḥ / vartaṃmānamapakāramāgamya yā cetasa āghātacetanā // 69 //",
  "SA20bsa065_u:134": "upanāhaḥ katamaḥ / vairānubandha // 70 //",
  "SA20bsa065_u:135": "mrakṣaḥ katamaḥ ātmano 'vaddhapracchādanā // 71 //",
  "SA20bsa065_u:136": "pradāśaḥ katamaḥ caṇḍavacodāśitā // 72 //",
  "SA20bsa065_u:137": "īrṣyā katamā parasaṃpattau cetaso vyāropa // 73 //",
  "SA20bsa065_u:138": "mātsarya katamat / dānavirodhī cetasa āgrahaḥ // 74 //",
  "SA20bsa065_u:139": "māyā katamā / paravaścanābhūtārthasadarśanatā // 75 //",
  "SA20bsa065_u:140": "śāṭhyaṃ katamat / svadoṣapracchādanopāyasaṃgṛhitaṃ cetasaḥ kauṭilyam // 76 //",
  "SA20bsa065_u:141": "madaḥ katamaḥ / svasaṃpattau raktasyoddharṣaḥ / cetasaḥ paryādānam // 77 //",
  "SA20bsa065_u:142": "vihiṃsā katamā / satveṣu viheṭhanā // 78 //",
  "SA20bsa065_u:143": "āhrīkyaṃ katamat / avadyena svayamalajjā // 79 //",
  "SA20bsa065_u:144": "anapatrāp katamat / avadyena parato 'lajjā // 80",
  "SA20bsa065_u:145": "satyānaṃ katamat / cittākarmaṇyatā / staimityam // 81 //",
  "SA20bsa065_u:146": "auddhatyaṃ katamat / cittasyāvyupasamaḥ // 82 //",
  "SA20bsa065_u:147": "āśraddhyaṃ katamat /",
  "SA20bsa065_u:148": "karmaphalasatyaratneṣvanabhisaṃpratyayaḥ /",
  "SA20bsa065_u:149": "śraddhāvipakṣaścetaso 'prasādaḥ // 83 //",
  "SA20bsa065_u:150": "kauśiddyaṃ katamatṃḥ kuśale cetaso 'nabhyutsāho vīryavipakṣa // 84 //",
  "SA20bsa065_u:151": "pramādaḥ katamaḥ / yā lobhadveṣamohekauśīddyaiścitasyānārakṣā kuśalasyābhāvanā // 85 //",
  "SA20bsa065_u:152": "muṣitāsmṛtiḥ katamā / kliṣṭāsmṛtiḥ / kuśalāpratipatiḥ // 86 //",
  "SA20bsa065_u:153": "vikṣepaḥ katamaḥ / rāgadveṣamohāṃśikaḥ pañcakāmaguṇeṣu cittasya visāraḥ // 87 //",
  "SA20bsa065_u:154": "aprasajanyaṃ katamat kleśasaṃprayuktā prajñā /",
  "SA20bsa065_u:155": "tayā kāyavākcittacaryā asaṃviditeva pravartate // 88 //",
  "SA20bsa065_u:156": "kaukṛtyaṃ katamat / cetaso vipratisāraḥ // 89 //",
  "SA20bsa065_u:157": "middhaṃ katamat / asvataṃtravṛtticetaso 'bhisaṃkṣepaḥ // 90 //",
  "SA20bsa065_u:158": "vitarkaḥ katamaḥ paryeṣako manojalpaḥ cetanāprajñāviśeṣaḥ yā cittasyaudārikatā // 91 //",
  "SA20bsa065_u:159": "vicāraḥ katamaḥ / paryeṣako manojalpaḥ tathaiva yā cittasya sūkṣmatā // 92 //",
  "SA20bsa065_u:160": "// ittaviprayuktaparisaṃkhyānam //",
  "SA20bsa065_u:161": "cittaviprayuttasaṃskārāḥ katame /",
  "SA20bsa065_u:162": "ye rūpacitacaittadhikāre prajñaptāḥ, ta evānyatrāprajñaptāḥ // 93 //",
  "SA20bsa065_u:163": "te katame /",
  "SA20bsa065_u:164": "prāpti,",
  "SA20bsa065_u:165": "asaṃjñisamāpattiḥ,",
  "SA20bsa065_u:166": "nirodhasamāpattiḥ,",
  "SA20bsa065_u:167": "āsaṃjñikam,",
  "SA20bsa065_u:168": "jīvitendriyam,",
  "SA20bsa065_u:169": "nikāyasabhāgatā,",
  "SA20bsa065_u:170": "jāti,",
  "SA20bsa065_u:171": "jarā,",
  "SA20bsa065_u:172": "sthitiḥ,",
  "SA20bsa065_u:173": "anityatā,",
  "SA20bsa065_u:174": "nāmakāyaḥ,",
  "SA20bsa065_u:175": "padakāyaḥ,",
  "SA20bsa065_u:176": "vyañjanakāyaḥ,",
  "SA20bsa065_u:177": "pṛthagjanatvamityevamādibhedasamādānāḥ // 94 //",
  "SA20bsa065_u:178": "ittaviprayuktalakṣaṇāni //",
  "SA20bsa065_u:179": "tatra prāptiḥ katamā /",
  "SA20bsa065_u:180": "pratilambhaḥ samanvāgamaśca /",
  "SA20bsa065_u:181": "bījam, vaśitā,abhimukhībhāvaśca yathāyogam // 95 //",
  "SA20bsa065_u:182": "asaṃjñisamāpattiḥ katamṃ /",
  "SA20bsa065_u:183": "śubhakṛtsnavītarāgasyoparyavītarāgasya niḥsaraṇasaṃjñāpūrvakeṇa manasikāreṇāvasthāvarāṇāṃ cittacaittadharmāṇāṃ yo nirodhaḥ // 96 //",
  "SA20bsa065_u:184": "nirodhasamāpattiḥ katamā /",
  "SA20bsa065_u:185": "ākiñcanyāyatanabītarāgasya bhavāgratāduccalitasya śāntavihāra - saṃjñāpūrvakeṇa manasikāreṇāsthāvarāṇāṃ cittacaittadharmāṇāṃ tadekatyānāṃ ca sthāvarāṇāṃ yo nirodhaḥ // 97 //",
  "SA20bsa065_u:186": "āsaṃjñikaṃ katamat /",
  "SA20bsa065_u:187": "asaṃjñisatvanikāyadeveṣupapannasyāsthāvarāṇāṃ cittacaittadharmāṇāṃ nirodhaḥ // 98 //",
  "SA20bsa065_u:188": "jīvitendriyam katamat /",
  "SA20bsa065_u:189": "nikāyasabhāgeṣu pūrvakarmānuviddhyeṣu yaḥ saṃskārāṇāṃ sthitikālaniyamaḥ // 99 //",
  "SA20bsa065_u:190": "nikāyasabhāgaḥ katamaḥ / yā satvānāmātmabhāvasadṛśatā // 100 //",
  "SA20bsa065_u:191": "jātiḥkatamā / nikāyasabhāge saṃskārāṇāmabhūtvā yo bhāṣaḥ // 101 //",
  "SA20bsa065_u:192": "jarā katamā / tathā teṣāṃ prabandhānyathātvam // 102 //",
  "SA20bsa065_u:193": "sthiti katamā / tathā teṣāṃ prabandhanvayaḥ // 103 //",
  "SA20bsa065_u:194": "anityatā katamā / tathā teṣāṃ prabandhavināśaḥ // 104 //",
  "SA20bsa065_u:195": "nāmakāyaḥ katamaḥ / dharmāṇāṃ svabhāvādhivacanam // 105 //",
  "SA20bsa065_u:196": "pādakāyaḥ katamaḥ / dharmāṇāṃ viśeṣādhivacanam // 106 //",
  "SA20bsa065_u:197": "vyañjanakāyaḥ katamaḥ / akṣarāṇi / tadubhayābhivyañjanatāmupādāya // 107 //",
  "SA20bsa065_u:198": "vākyāni tāni / nāmapadāśreyeṇārthavacanatāmupādāya // 108 //",
  "SA20bsa065_u:199": "akṣaraṃ punaḥ paryāyākṣaraṇatāmupādāya // 109 //",
  "SA20bsa065_u:200": "pṛthagjanatvaṃ katamat / āryadharmāṇāmapratilambhaḥ // 110 //",
  "SA20bsa065_u:201": "iti saṃskāraskandho nāma //",
  "SA20bsa065_u:202": "saṃskāraskandhaḥ soddyeśanirdeśaḥ pariniṣṭhitaḥ // 111 //",
  "SA20bsa065_u:203": "// vijñānalakṣaṇam //",
  "SA20bsa065_u:204": "vijñānaṃ katamat / ālambanaṃbijñaptiḥ // 112 //",
  "SA20bsa065_u:205": "cittamanasī vijñānaparyāyau sanirvacano taccitaṃ mano 'pi citrīkāratā manoniśrayatāṃ ca upādāya // 113 //",
  "SA20bsa065_u:206": "// salakṣaṇacittanirdeśaḥ //",
  "SA20bsa065_u:207": "mūlacittamālayavijñānaṃ tadyathedaṃ sarvasaṃskārāṇāṃ saṃcitaṃ bījam // 114 //",
  "SA20bsa065_u:208": "tatpunarālambanamapyuparichinnamekasantānavarti /",
  "SA20bsa065_u:209": "yathā nirodhasamāpatyasaṃjñisamāpatyā - saṃjñikebhyovyutthitasyāsmād viṣayavijñaptikaṃ nāma pravṛtti vijñānaṃ jāyate /",
  "SA20bsa065_u:210": "ālambanapratyayamapekṣya bhinnākāreṣu vṛttitāmucchiddya punarjātatāṃ saṃsāre pravṛttinivṛtitāṃ copādāya tadālambanaṃ nāma vijñānam // 115 //",
  "SA20bsa065_u:211": "ālayavijñānaṃ hi sarvabījālayatāmātmabhāvālayahetutāmātmabhāve sthititāṃ copādāya // 116 //",
  "SA20bsa065_u:212": "ādānavijñānamapi tat / ātmabhāvādānatāmupādāya // 117 //",
  "SA20bsa065_u:213": "// salakṣaṇamanonirdeśaḥ //",
  "SA20bsa065_u:214": "mūlamana ālayavijñānamāśritya nityamātmamohātmadṛṣṭyātmamānātmasnehaiḥ saṃprayuktaṃ vijñānamekajātīyasantānavarti arhatvāryamārganirodhasamāpattikāle vyāvartate // 118 //",
  "SA20bsa065_u:215": "vijñānaskandhaḥ soniddeśanirdeśaḥ pariniṣṭhitaḥ //",
  "SA20bsa065_u:216": "// skandhanirvacanam //",
  "SA20bsa065_u:217": "kimarthaḥ skandhaḥ rāśyarthaḥ /",
  "SA20bsa065_u:218": "kālagotrākāragativiṣayabhinnānāṃ rūpādīnāmabhisaṃkṣepatāmupādāya // 119 //",
  "SA20bsa065_u:219": "skandhoddyeśanirdeśakaḥ prathamādhikāraḥ pariniṣṭhitaḥ //",
  "SA20bsa065_u:220": "dvitīyādhikāraḥ",
  "SA20bsa065_u:221": "āyatanoddyeśanirdeśakaḥ",
  "SA20bsa065_u:222": "dvādaśāyatanāni /",
  "SA20bsa065_u:223": "cakṣurāyatanam,",
  "SA20bsa065_u:224": "rūpāyatanam,",
  "SA20bsa065_u:225": "śrotrāyatanam,",
  "SA20bsa065_u:226": "śabdāyatanam,",
  "SA20bsa065_u:227": "ghrāṇāyatanam,",
  "SA20bsa065_u:228": "gandhāyatanam,",
  "SA20bsa065_u:229": "jihvāyatanam,",
  "SA20bsa065_u:230": "rasāyatanam,",
  "SA20bsa065_u:231": "kāyāyatanam,",
  "SA20bsa065_u:232": "spraṣṭavyāyatanam,",
  "SA20bsa065_u:233": "mana āyatanam,",
  "SA20bsa065_u:234": "dharmāyatanaṃ ca // 120 //",
  "SA20bsa065_u:235": "/ 'ayatanānāṃ salakṣaṇaprapañcaḥ //",
  "SA20bsa065_u:236": "cakṣurādīni rūpaśabdagandharasāyatanāni yathoktapūrvāṇi // 121 //",
  "SA20bsa065_u:237": "spraṣṭavyāyatanaṃ catvāri mahābhūtāni uktaśca yaḥ spraṣṭabyaikadeśaḥ // 122 //",
  "SA20bsa065_u:238": "mana āyatanaṃ yo vijñānaskandhaḥ // 123 //",
  "SA20bsa065_u:239": "dharmāyatanaṃ vedanā, saṃjñā, saṃskārāḥ, avijñaptiḥ, asaṃskṛtāśca // 124 //",
  "SA20bsa065_u:240": "/ 'saṃskṛtāḥ salakṣaṇaprabhedāḥ //",
  "SA20bsa065_u:241": "asaṃskṛtā katame / ākāśam, apratisaṃkhyānirodhaḥ, pratisaṃkhyānirodhaḥ tathatā ca // 125 //",
  "SA20bsa065_u:242": "tatrākāśaṃ katamat / rūpāvakāśaḥ // 126 //",
  "SA20bsa065_u:243": "apratisaṃkhyānirodhaḥ katamaḥ / yo nirodho na visaṃyogaḥ // 127 //",
  "SA20bsa065_u:244": "sakleśāpratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 128 //",
  "SA20bsa065_u:245": "pratisaṃkhyānirodhaḥ katamaḥ / yo nirodho visaṃyogaḥ // 129 //",
  "SA20bsa065_u:246": "sakleśapratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 130 //",
  "SA20bsa065_u:247": "tathatā katamā / yā dharmāṇāṃ dharmatā / dharmanairātmyam // 131 //",
  "SA20bsa065_u:248": "/ 'ayatananirvacanam //",
  "SA20bsa065_u:249": "kimupādāyāyatanamiti / vijñānotpādadvāratāmupādāya // 132 //",
  "SA20bsa065_u:250": "āyatanoddyeśanirdeśako dvitīyādhikāraḥ pariniṣṭhitaḥ /",
  "SA20bsa065_u:251": "tṛtīyādhikāraḥ",
  "SA20bsa065_u:252": "dhātūddeśanirdeśakaḥ",
  "SA20bsa065_u:253": "// dhātunāmāni //",
  "SA20bsa065_u:254": "aṣṭadaśa dhātavaḥ /",
  "SA20bsa065_u:255": "cakṣurdhātuḥ,",
  "SA20bsa065_u:256": "rūpadhātuḥ,",
  "SA20bsa065_u:257": "cakṣurvijñānadhātu,",
  "SA20bsa065_u:258": "śrotradhātuḥ,",
  "SA20bsa065_u:259": "śabdadhātuḥ,",
  "SA20bsa065_u:260": "śrotravijñānadhātuḥ,",
  "SA20bsa065_u:261": "ghrāṇadhātuḥ,",
  "SA20bsa065_u:262": "gandhadhātuḥ,",
  "SA20bsa065_u:263": "ghrāṇavijñānadhātuḥ,",
  "SA20bsa065_u:264": "jihvādhātuḥ,",
  "SA20bsa065_u:265": "rasadhātuḥ,",
  "SA20bsa065_u:266": "jihvāvijñānadhātuḥ,",
  "SA20bsa065_u:267": "kāyadhātuḥ,",
  "SA20bsa065_u:268": "spraṣṭavyadhātuḥ,",
  "SA20bsa065_u:269": "kāyabijñānadhātuḥ,",
  "SA20bsa065_u:270": "manodhātuḥ,",
  "SA20bsa065_u:271": "dharmadhātuḥ,",
  "SA20bsa065_u:272": "manovijñānadhātuśca // 133 //",
  "SA20bsa065_u:273": "// dhātuprapaścaḥ //",
  "SA20bsa065_u:274": "cakṣurādayo dhātavo rūpādayo dhātavaśca yathāyatanāni // 134 //",
  "SA20bsa065_u:275": "ṣaḍvijñānadhātavaścakṣurāddyāśrayā rūpādyālambanāvijñaptayaḥ // 135 //",
  "SA20bsa065_u:276": "manodhātusteṣāmeva samanantaranirudhyaṃ ṣaḍvijñānadeśanāśrayatāmupādāya // 136 //",
  "SA20bsa065_u:277": "// dhātuvyavasthanam //",
  "SA20bsa065_u:278": "ebamaṣṭādaśadhātuvyavasthānam / yo rūpaskandhastāni daśāyatanadhātavaḥ // 137 //",
  "SA20bsa065_u:279": "dharmāyatanaṃ dharmadhātvekadeśaḥ // 138 //",
  "SA20bsa065_u:280": "yo vijñānaskandhastanmana āyatanam, saptama manodhātavaśca //",
  "SA20bsa065_u:281": "139 //",
  "SA20bsa065_u:282": "anye ye trayaḥ skandhā rūpadhātvekadeśaśca,",
  "SA20bsa065_u:283": "te 'saṃskṛtai sahitā dharmāyatanaṃ dharmadhātuśca // 140 //",
  "SA20bsa065_u:284": "// dhātunirvacanam //",
  "SA20bsa065_u:285": "kimupādāya dhātava iti / ākārakātmalakṣaṇadhāraṇatāmupādāya // 141 //",
  "SA20bsa065_u:286": "// skandhāyatanadhātukrameṇa dharmadeśanāyāḥ prayojanam //",
  "SA20bsa065_u:287": "kimartha skandhādikrameṇa deśanā / trividhātmagrāhapratipakṣeṇāyaṃ kramaḥ // 142 //",
  "SA20bsa065_u:288": "ātmagrāhastrividhaḥ ekātmagrāhaḥ, vedakātmagrāhaḥ, kārakātmagrāhaśca // 143 //",
  "SA20bsa065_u:289": "// 'ṣṭādaśadhātuvikalpāḥ //",
  "SA20bsa065_u:290": "aṣṭādaśadhātuṣu kati rūpiṇaḥ / ye rūpaskandhasvabhāvāḥ // 144 //",
  "SA20bsa065_u:291": "katyarūpiṇaḥ / śiṣṭāḥ // 145 //",
  "SA20bsa065_u:292": "kati sanidarśanāḥ / rūpadhāturekaḥ sanidarśanaviṣayaḥ // 146 //",
  "SA20bsa065_u:293": "katyanidarśanāḥ / śiṣṭāḥ // 147 //",
  "SA20bsa065_u:294": "kati sapratidhāḥ / daśa rūpiṇaḥ / yatra yatpratidhātastatra tat sapratigham // 148 //",
  "SA20bsa065_u:295": "katyapratighāḥ / śiṣṭāḥ // 149 //",
  "SA20bsa065_u:296": "kati sāsravāḥ /",
  "SA20bsa065_u:297": "pañcadaśa paścimānāṃ trayāṇāmekadeśaśca /",
  "SA20bsa065_u:298": "te kleśotpādapratyakṣacaryāviṣayatāmupādāyaṃ // 150 //",
  "SA20bsa065_u:299": "katyayanāstravāḥ / paścimānāṃ trayāṇāmekadeśaḥ // 151 //",
  "SA20bsa065_u:300": "kati kāmapratisaṃyuktāḥ / sarva // 152 //",
  "SA20bsa065_u:301": "kati rūpapratisaṃyuktāḥ / caturdaśa sthāpayitvā gandharasaghrāṇajihvāvijñānadhātūn // 153 //",
  "SA20bsa065_u:302": "katyārūpyasaṃprayuktāḥ / paścimāśrayaḥ // 154 //",
  "SA20bsa065_u:303": "katyapratisaṃyuktāḥ / trayāṇāmekadeśaḥ // 155 //",
  "SA20bsa065_u:304": "kati skandhasaṃgrahītāḥ / asaṃskṛtān sthāpayitvā // 156 //",
  "SA20bsa065_u:305": "katyupādānaskandhasaṃgṛhītāḥ ye sāsravāḥ // 157 //",
  "SA20bsa065_u:306": "kati kuśalākuśalāvyākṛtāḥ /",
  "SA20bsa065_u:307": "daśa trividhāḥ /",
  "SA20bsa065_u:308": "sapta manodhātavaḥ, rūpaśabdadharmadhātavaśca /",
  "SA20bsa065_u:309": "śiṣṭā avyākṛtāḥ // 158 //",
  "SA20bsa065_u:310": "katyādhyātmikāḥ / dvādaśa / sthāpayitvā rūpaśabda gandharasaspraṣṭāvyadharmadhātūn // 159 //",
  "SA20bsa065_u:311": "kati bāhyāḥ / ṣaṭ / ye sthāpitāḥ // 160 //",
  "SA20bsa065_u:312": "kati sālambanāḥ / saptacittadhātavaḥ dharmadhātvekadeśo 'pi yaścaittaḥ // 161 //",
  "SA20bsa065_u:313": "katyanālambanāḥ / śiṣṭā daśa dharmadhātvekadeśaśca // 162 //",
  "SA20bsa065_u:314": "kati savitarkāḥ / manodhātuḥ manovijñānadhātuḥ dharmadhātvekadeśaśca // 163 //",
  "SA20bsa065_u:315": "kati niṣpannāḥ / pañcādhyātmikāḥ / caturṇāmekadeśaḥ // 164 //",
  "SA20bsa065_u:316": "katyaniṣpannāḥ śiṣṭā daśa, caturṇāmekadeśaśca // 165 //",
  "SA20bsa065_u:317": "kati sabhāgāḥ / ādhyātmikāḥ pañca rūpiṇaḥ / svavijñānasahitaviṣayasadṛśatāmupādāya // 166 //",
  "SA20bsa065_u:318": "kati tatsabhāgāḥ / ta eva svavijñānavirahitasvānvayasadṛśatāmupādāya // 167 //",
  "SA20bsa065_u:319": "dhātūddyeśanirdeśakastṛtīyādhikaraḥ pariniṣṭitaḥ /",
  "SA20bsa065_u:320": "ācāryavasubandhuviracit pañcaskandhaprakaraṇaṃ samāptam /",
  "SA20bsa065_u:321": "pañcaskandhaprakaraṇaṃ śāstriṇā śāntibhikṣuṇā /",
  "SA20bsa065_u:322": "bhoṭānuvādamāgamya saṃskṛte punaruddhṛtam",
  "SA20bsa065_u:323": "yadatra sukṛtaṃ sarva tadācāryasya dhīmataḥ /",
  "SA20bsa065_u:324": "yat kukṛtaṃ tadāgo me kṣantavyaṃ sādhusūribhiḥ //"
}