{
    "T01n911u_001:0": " vasudhārānāmadhāraṇīstotram",
    "T01n911u_001:1": " om namo bhagavatyai āryaśrīvasudhārāyai",
    "T01n911u_001:2": " divyarūpī surūpī ca saumyarūpī balapradā |",
    "T01n911u_001:3": " vasudharī vasudhāraṇī vasuśrī śrīkarī varā || 1 ||",
    "T01n911u_001:4": " dharaṇī dhāraṇī dhātā śaraṇyā bhaktavatsalā |",
    "T01n911u_001:5": " prajñāpāramitā devī prajñā śrībuddhivardhinī || 2 ||",
    "T01n911u_001:6": " vidyādharī śivā sūkṣmā śāstā sarvatra mātṛkā |",
    "T01n911u_001:7": " taruṇī tāru(ra)",
    "T01n911u_001:8": " ṇī devī vidyādāneśvareśvarī || 3 ||",
    "T01n911u_001:9": " bhūṣitā bhūtamātā ca sarvābharaṇabhūṣitā |",
    "T01n911u_001:10": " durdāntatrāsanī bhītā ugrā ugraparākramā || 4 ||",
    "T01n911u_001:11": " dānapāramitā devī varṣaṇī divyarūpiṇī |",
    "T01n911u_001:12": " nidhānaṃ sarvamāṅgalyā kīrtirlakṣmīryaśaḥśubhā || 5 ||",
    "T01n911u_001:13": " dahanī mālinī caṇḍī śabarī sarvamātrikā |",
    "T01n911u_001:14": " kṛtāntaśāsanī raudrī kaumārī viśvarūpiṇī || 6 ||",
    "T01n911u_001:15": " vīryapāramitā devī jagadānandarocanī |",
    "T01n911u_001:16": " tāpasī ugrarūpī ca ṛddhisiddhibalapradā || 7 ||",
    "T01n911u_001:17": " dhanyā puṇyā mahābhāgā ajitā jitavikramā |",
    "T01n911u_001:18": " jagadekahitā vidyā saṃgrāme tāraṇī śubhā || 8 ||",
    "T01n911u_001:19": " kṣāntipāramitā devī śīlinī dhyānadhyāyino |",
    "T01n911u_001:20": " padminī padmadhārī ca padmapriyā padmāsanī || 9 ||",
    "T01n911u_001:21": " śuddharūpī mahātejā hemavarṇā prabhākarī |",
    "T01n911u_001:22": " cintāmaṇimahādevī prajñāpustakadhāriṇī || 10 ||",
    "T01n911u_001:23": " nidhānaṃ kūṭimāruḍhidhanyāgāradhanapriyā |",
    "T01n911u_001:24": " traidhātukaṃ mahā ādi divyābharaṇabhūṣiṇī || 11 ||",
    "T01n911u_001:25": " mātarī sarvabuddhānāṃ ratnadhāte(tvī)",
    "T01n911u_001:26": " śvareśvarī |",
    "T01n911u_001:27": " śūnyatā bhāvanī devī bhāvābhāvavivarjitā || 12 ||",
    "T01n911u_001:28": " vainye(ne)ya kiṃ na vinyastā divyakleśanichedanī |",
    "T01n911u_001:29": " bhī(bhe) dinī sarvamārāṇāṃ saptapātālakṣobhinī (ṇī) || 13 ||",
    "T01n911u_001:30": " brahmāṇī vedamātā ca guhyā ca guhyavāsinī |",
    "T01n911u_001:31": " sarasvatī viśālākṣī caturbrahmavihāriṇī || 14 ||",
    "T01n911u_001:32": " tāthāgatī mahāramyā vajriṇī dharmadhāriṇī |",
    "T01n911u_001:33": " karmadhāteśvarī vidyā viśvajvālābhamaṇḍalī || 15 ||",
    "T01n911u_001:34": " bodha(dhi)nī sarvasattvānāṃ bodhyaṅgakṛtaśekharī |",
    "T01n911u_001:35": " dhyānā dhīrmuktisaṃpannā advayadvayabhāvinī || 16 ||",
    "T01n911u_001:36": " sarvārthasādhanī bhadrā strīrūpāmitavikramā |",
    "T01n911u_001:37": " darśinī buddhamārgāṇāṃ naṣṭamārgapradarśinī || 17 ||",
    "T01n911u_001:38": " vāgīśvarī mahāśāntirgoptrī dhātrī dhanapradā |",
    "T01n911u_001:39": " strīrūpadhāriṇī siddhā yoginī yogajeśvarī || 18 ||",
    "T01n911u_001:40": " manoharī mahākrāntiḥ saubhāgyapriyadarśinī |",
    "T01n911u_001:41": " sārthavāhakṛpādṛṣṭiḥ sarvatāthāgatātmakī || 19 ||",
    "T01n911u_001:42": " namaste'stu mahādevī sarvasattvārthadāyinī |",
    "T01n911u_001:43": " namaste divyarūpī ca vasudhārā namo'stu te || 20 ||",
    "T01n911u_001:44": " aṣṭottaraśataṃ nāma trikālaṃ yaṃ paṭhet pumān |",
    "T01n911u_001:45": " prāpnoti niyataṃ siddhimīpsitārthamanorathān || 21 ||",
    "T01n911u_001:46": " yadajñānakṛtaṃ pāpam ānantaryasudāruṇam |",
    "T01n911u_001:47": " tatsarvaṃ kṣapayatyāśu smaraṇāt sa(rva)bhadrakam || 22 ||",
    "T01n911u_001:48": " athavā śīlasaṃpannaḥ saptajātismaro bhavet |",
    "T01n911u_001:49": " priyaścādeyavākyena rūpavān priyadarśanaḥ || 23 ||",
    "T01n911u_001:50": " viprakṣatriyakuleṣu ādeyamupajāyate |",
    "T01n911u_001:51": " ante bhūmīśvaraṃ prāptaḥ paścāt prāpta(ḥ)",
    "T01n911u_001:52": " sukhāvatīm || 24 ||",
    "T01n911u_001:53": " śrīvasudhārānāmadhāraṇīstotraṃ",
    "T01n911u_001:54": " samyaksaṃbuddhabhāṣitaṃ samāptam |"
}