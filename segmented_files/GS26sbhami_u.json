{
  "GS26sbhami_u:0": "Samantabhadra: Āptamīmāṃsā",
  "GS26sbhami_u:1": "devāgamanabhoyānacāmarādivibhūtayaḥ /",
  "GS26sbhami_u:2": "māyāviṣvapi dṛśyante nātastvamasi no mahān // SAm_1 //",
  "GS26sbhami_u:3": "adhyātmaṃ bahirapyeṣa vigrahādimahodayaḥ /",
  "GS26sbhami_u:4": "divyaḥ satyo divaukassvapyasti rāgādimatsu saḥ // SAm_2 //",
  "GS26sbhami_u:5": "tīrthakṛtsamayānāṃ ca parasparavirodhataḥ /",
  "GS26sbhami_u:6": "sarveṣāmāptatā nāsti kaścideva bhavedguruḥ // SAm_3 //",
  "GS26sbhami_u:7": "doṣāvaraṇayorhānirniḥśeṣāstyatiśāyanāt /",
  "GS26sbhami_u:8": "kvacidyathā svahetubhyo bahirantarmalakṣayaḥ // SAm_4 //",
  "GS26sbhami_u:9": "sūkṣmāntaritadūrārthāḥ pratyakṣāḥ kasyacidyathā /",
  "GS26sbhami_u:10": "anumeyatvato 'gnyādiriti sarvajñasaṃsthitiḥ // SAm_5 //",
  "GS26sbhami_u:11": "sa tvamevāsi nirdoṣo yuktiśāstrāvirodhavāk /",
  "GS26sbhami_u:12": "avirodho yadiṣṭaṃ te prasiddhena na bādhyate // SAm_6 //",
  "GS26sbhami_u:13": "tvanmatāmṛtabāhyānāṃ sarvathaikāntavādinām /",
  "GS26sbhami_u:14": "āptābhimānadagdhānāṃ sveṣṭaṃ dṛṣṭena bādhyate // SAm_7 //",
  "GS26sbhami_u:15": "kuśalākuśalaṃ karma paralokaśca na kvacit /",
  "GS26sbhami_u:16": "ekāntagraharakteṣu nātha svaparavairiṣu // SAm_8 //",
  "GS26sbhami_u:17": "bhāvaikānte padārthānāmabhāvānāmapahnavāt /",
  "GS26sbhami_u:18": "sarvātmakamanādyantamasvarūpamatāvakam // SAm_9 //",
  "GS26sbhami_u:19": "kāryadravyamanādi syātprāgabhāvasya nihnave /",
  "GS26sbhami_u:20": "pradhvaṃsasya ca dharmasya pracyave 'nantatāṃ vrajet // SAm_10 //",
  "GS26sbhami_u:21": "sarvātmakaṃ tadekaṃ syādanyāpohavyatikrame /",
  "GS26sbhami_u:22": "anyatra samavāyena vyapadiśyeta sarvathā // SAm_11 //",
  "GS26sbhami_u:23": "abhāvaikāntapakṣe 'pi bhāvāpahnavavādinām /",
  "GS26sbhami_u:24": "bodhavākyaṃ pramāṇaṃ na kena sādhanadūṣaṇam // SAm_12 //",
  "GS26sbhami_u:25": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:26": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_13 //",
  "GS26sbhami_u:27": "kathaṃcitte sadeveṣṭaṃ kathañcidasadeva tat /",
  "GS26sbhami_u:28": "tathobhayamavācyaṃ ca nayayogānna sarvathā // SAm_14 //",
  "GS26sbhami_u:29": "sadeva sarvaṃ ko necchetsvarūpādicatuṣṭayāt /",
  "GS26sbhami_u:30": "asadeva viparyāsānna cenna vyavatiṣṭhate // SAm_15 //",
  "GS26sbhami_u:31": "kramārpitadvayād dvaitaṃ sahāvācyamaśaktitaḥ /",
  "GS26sbhami_u:32": "avaktavyottarāḥ śeṣāstrayo bhaṅgāḥ svahetutaḥ // SAm_16 //",
  "GS26sbhami_u:33": "astitvaṃ pratiṣedhyenāvinābhāvyekadharmiṇi /",
  "GS26sbhami_u:34": "viśeṣaṇatvātsādharmyaṃ yathā bhedavivakṣayā // SAm_17 //",
  "GS26sbhami_u:35": "nāstitvaṃ pratiṣedhyenāvinābhāvyekadharmiṇi /",
  "GS26sbhami_u:36": "viśeṣaṇatvādvaidharmyaṃ yathā 'bhedavivakṣayā // SAm_18 //",
  "GS26sbhami_u:37": "vidheyapratiṣedhyātmā viśeṣyaḥ śabdagocaraḥ /",
  "GS26sbhami_u:38": "sādhyadharmo yathā heturahetuścāpyapekṣayā // SAm_19 //",
  "GS26sbhami_u:39": "śeṣabhaṅgāśca netavyā yathoktanayayogataḥ /",
  "GS26sbhami_u:40": "na ca kaścidvirodho 'sti munīndra tava śāsane // SAm_20 //",
  "GS26sbhami_u:41": "evaṃ vidhiniṣedhābhyāmanavasthitamarthakṛt /",
  "GS26sbhami_u:42": "neti cenna yathākāryaṃ bahirantarupādhibhiḥ // SAm_21 //",
  "GS26sbhami_u:43": "dharme dharme 'nya evārtho dharmiṇo 'nantadharmaṇaḥ /",
  "GS26sbhami_u:44": "aṅgitve 'nyatamāntasya śeṣāntānāṃ tadaṅgatā // SAm_22 //",
  "GS26sbhami_u:45": "ekānekavikalpādāvuttaratrāpi yojayet /",
  "GS26sbhami_u:46": "prakriyāṃ bhaṅginīmenāṃ nayairnayaviśāradaḥ // SAm_23 //",
  "GS26sbhami_u:47": "advaitākāntapakṣe 'pi dṛṣṭo bhedo virudhyate /",
  "GS26sbhami_u:48": "kārakāṇāṃ kriyāyāśca naikaṃ svasmātprajāyate // SAm_24 //",
  "GS26sbhami_u:49": "karmadvaitaṃ phaladvaitaṃ lokadvaitaṃ ca no bhavet /",
  "GS26sbhami_u:50": "vidyāvidyādvayaṃ na syādbandhamokṣadvayaṃ tathā // SAm_25 //",
  "GS26sbhami_u:51": "hetoradvaitasiddhiśced dvaitaṃ syāddhetusādhyayoḥ /",
  "GS26sbhami_u:52": "hetunā cedvinā siddhirdvaitaṃ vāṅmātrato na kim // SAm_26 //",
  "GS26sbhami_u:53": "advaitaṃ na vinā dvaitādaheturiva hetunā /",
  "GS26sbhami_u:54": "saṃjñinaḥ pratiṣedho na pratiṣedhyādṛte kvacit // SAm_27 //",
  "GS26sbhami_u:55": "pṛthaktvaikāntapakṣe 'pi pṛthaktvādapṛthakkṛtau /",
  "GS26sbhami_u:56": "pṛthaktve na pṛthaktvaṃ syādanekastho hyasau guṇaḥ // SAm_28 //",
  "GS26sbhami_u:57": "santānaḥ samudāyaśca sādharmyaṃ ca niraṅkuśaḥ /",
  "GS26sbhami_u:58": "pretyabhāvaśca tatsarvaṃ na syādekatvanihnave // SAm_29 //",
  "GS26sbhami_u:59": "sadātmanā ca bhinnaṃ cejjñānaṃ jñeyād dvidhā 'pyasat /",
  "GS26sbhami_u:60": "jñānābhāve kathaṃ jñeyaṃ bahirantaśca te dviṣām // SAm_30 //",
  "GS26sbhami_u:61": "sāmānyārthā giro 'nyeṣāṃ viśeṣo nābhilapyate /",
  "GS26sbhami_u:62": "sāmānyābhāvatasteṣāṃ mṛṣaiva sakalā giraḥ // SAm_31 //",
  "GS26sbhami_u:63": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:64": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_32 //",
  "GS26sbhami_u:65": "anapekṣe pṛthaktvaikyaṃ hyavastu dvayahetutaḥ /",
  "GS26sbhami_u:66": "tadevaikyaṃ pṛthaktvaṃ ca svabhedaiḥ sādhanaṃ yathā // SAm_33 //",
  "GS26sbhami_u:67": "satsāmānyāttu sarvaikyaṃ pṛthagdravyādibhedataḥ /",
  "GS26sbhami_u:68": "bhedābhedavivakṣāyāmasādhāraṇahetuvat // SAm_34 //",
  "GS26sbhami_u:69": "vivakṣā cāvivakṣā ca viśeṣye 'nantadharmiṇi /",
  "GS26sbhami_u:70": "sato viśeṣaṇasyātra nāsatastaistadarthibhiḥ // SAm_35 //",
  "GS26sbhami_u:71": "pramāṇagocarau santau bhedābhedau na saṃvṛtī /",
  "GS26sbhami_u:72": "tāvekatrāviruddhau te guṇamukhyavivakṣayā // SAm_36 //",
  "GS26sbhami_u:73": "nityatvaikāntapakṣe 'pi vikriyā nopapadyate /",
  "GS26sbhami_u:74": "prāgeva kārakābhāvaḥ kva pramāṇaṃ kva tatphalam // SAm_37 //",
  "GS26sbhami_u:75": "pramāṇakārakairvyaktaṃ vyaktaṃ cedindriyārthavat /",
  "GS26sbhami_u:76": "te ca nityaṃ vikāryaṃ kiṃ sādhoste śāsanādbahiḥ // SAm_38 //",
  "GS26sbhami_u:77": "yadi satsarvathā kāryaṃ puṃvannotpattumarhati /",
  "GS26sbhami_u:78": "pariṇāmaprakḷptiśca nityatvaikāntabādhinī // SAm_39 //",
  "GS26sbhami_u:79": "puṇyapāpakriyā na syātpretyabhāvaḥ phalaṃ kutaḥ /",
  "GS26sbhami_u:80": "bandhamokṣau ca teṣāṃ na yeṣāṃ tvaṃ nāsi nāyakaḥ // SAm_40 //",
  "GS26sbhami_u:81": "kṣaṇikaikāntapakṣe 'pi pretyabhāvādyasambhavaḥ /",
  "GS26sbhami_u:82": "pratyabhijñādyabhāvānna kāryārambhaḥ kutaḥ phalam // SAm_41 //",
  "GS26sbhami_u:83": "yadyasatsarvathā kāryaṃ tanmā jani khapuṣpavat /",
  "GS26sbhami_u:84": "mopādānaniyāmo bhūnmāśvāsaḥ kāryajanmani // SAm_42 //",
  "GS26sbhami_u:85": "na hetuphalabhāvādiranyabhāvādananvayāt /",
  "GS26sbhami_u:86": "santānāntaravannaikaḥ santānastadvataḥ pṛthak // SAm_43 //",
  "GS26sbhami_u:87": "anyeṣvananyaśabdo 'yaṃ saṃvṛtirna mṛṣā katham /",
  "GS26sbhami_u:88": "mukhyārthaḥ saṃvṛtirnāsti vinā mukhyānna saṃvṛtiḥ // SAm_44 //",
  "GS26sbhami_u:89": "catuṣkoṭervikalpasya sarvānteṣūktyayogataḥ /",
  "GS26sbhami_u:90": "tattvānyatvamavācyaṃ ca tayoḥ santānatadvatoḥ // SAm_45 //",
  "GS26sbhami_u:91": "avaktavyacatuṣkoṭivikalpo 'pi na kathyatām /",
  "GS26sbhami_u:92": "asarvāntamavastu syādaviśeṣyaviśeṣaṇam // SAm_46 //",
  "GS26sbhami_u:93": "dravyādyantarabhāvena niṣedhaḥ saṃjñinaḥ sataḥ /",
  "GS26sbhami_u:94": "asadbhedo na bhāvastu sthānaṃ vidhiniṣedhayoḥ // SAm_47 //",
  "GS26sbhami_u:95": "avastvanabhilāpyaṃ syātsarvāntaiḥ parivarjitam /",
  "GS26sbhami_u:96": "vastvevāvastutāṃ yāti prakriyāyā viparyayāt // SAm_48 //",
  "GS26sbhami_u:97": "sarvāntāścedavaktavyāsteṣāṃ kiṃ vacanaṃ punaḥ /",
  "GS26sbhami_u:98": "saṃvṛttiścenmṛṣaivaiṣā paramārthaviparyayāt // SAm_49 //",
  "GS26sbhami_u:99": "aśakyatvādavācyaṃ kimabhāvātkimabodhataḥ /",
  "GS26sbhami_u:100": "ādyatoktidvayaṃ na syātkiṃ vyājenocyatāṃ sphuṭam // SAm_50 //",
  "GS26sbhami_u:101": "hinastyanabhisaṃdhātṛ na hinastyabhisandhimat /",
  "GS26sbhami_u:102": "badhyate taddvayāpetaṃ cittaṃ baddhaṃ na mucyate // SAm_51 //",
  "GS26sbhami_u:103": "ahetukatvānāśasya hiṃsāheturna hiṃsakaḥ /",
  "GS26sbhami_u:104": "cittasantatināśaśca mokṣo nāṣṭāṅgahetukaḥ // SAm_52 //",
  "GS26sbhami_u:105": "virūpakāryārambhāya yadi hetusamāgamaḥ /",
  "GS26sbhami_u:106": "āśrayibhyāmananyo 'sāvaviśeṣādayuktavat // SAm_53 //",
  "GS26sbhami_u:107": "skandhāḥ santatayaścaiva saṃvṛtitvādasaṃskṛtāḥ /",
  "GS26sbhami_u:108": "sthityutpattivyayāsteṣāṃ na syuḥ kharaviṣāṇavat // SAm_54 //",
  "GS26sbhami_u:109": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:110": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_55 //",
  "GS26sbhami_u:111": "nityaṃ tatpratyabhijñānānnākasmāttadavicchidā /",
  "GS26sbhami_u:112": "kṣaṇikaṃ kālabhedātte buddhyasañcaradoṣataḥ // SAm_56 //",
  "GS26sbhami_u:113": "na sāmānyātmanodeti na vyeti vyaktamanvayāt /",
  "GS26sbhami_u:114": "vyetyudeti viśeṣātte sahaikatrodayādi sat // SAm_57 //",
  "GS26sbhami_u:115": "kāryotpādaḥ kṣayo heturniyamāllakṣaṇātpṛthak /",
  "GS26sbhami_u:116": "na tau jātyādyavasthānādanapekṣāḥ khapuṣpavat // SAm_58 //",
  "GS26sbhami_u:117": "ghaṭamaulisuvarṇārthī nāśotpādasthitiṣvayam /",
  "GS26sbhami_u:118": "śokapramohamādhyasthyaṃ jano yāti sahetukam // SAm_59 //",
  "GS26sbhami_u:119": "payovrato na dadhyatti na payo 'tti dadhivrataḥ /",
  "GS26sbhami_u:120": "agorasavrato nobhe tasmāttattvaṃ trayātmakam // SAm_60 //",
  "GS26sbhami_u:121": "kāryakāraṇanānātvaṃ guṇaguṇyanyatāpi ca /",
  "GS26sbhami_u:122": "sāmānyatadvadanyatvaṃ caikāntena yadīṣyate // SAm_61 //",
  "GS26sbhami_u:123": "ekasyānekavṛttirna bhāgābhāvādbahūni vā /",
  "GS26sbhami_u:124": "bhāgitvādvāsya naikatvaṃ doṣo vṛtteranārhate // SAm_62 //",
  "GS26sbhami_u:125": "deśakālaviśeṣe 'pi syādvṛttiryutasiddhivat /",
  "GS26sbhami_u:126": "samānadeśatā na syānmūrtakāraṇakāryayoḥ // SAm_63 //",
  "GS26sbhami_u:127": "āśrayāśrayibhāvānna svātantryaṃ samavāyinām /",
  "GS26sbhami_u:128": "ityayuktaḥ sa sambandho na yuktaḥ samavāyibhiḥ // SAm_64 //",
  "GS26sbhami_u:129": "sāmānyaṃ samavāyaścāpyekaikatra samāptitaḥ /",
  "GS26sbhami_u:130": "antareṇāśrayaṃ na syānnāśotpādiṣu ko vidhiḥ // SAm_65 //",
  "GS26sbhami_u:131": "sarvathānabhisambandhaḥ sāmānyasamavāyayoḥ /",
  "GS26sbhami_u:132": "tābhyāmartho na sambaddhastāni trīṇi khapuṣpavat // SAm_66 //",
  "GS26sbhami_u:133": "ananyataikānte 'ṇūnāṃ saṃghāte 'pi vibhāgavat /",
  "GS26sbhami_u:134": "asaṃhatatvaṃ syādbhūtacatuṣkaṃ bhrāntireva sā // SAm_67 //",
  "GS26sbhami_u:135": "kāryabhrānteraṇubhrāntiḥ kāryaliṅgaṃ hi kāraṇam /",
  "GS26sbhami_u:136": "ubhayābhāvatastatsthaṃ guṇajātītaracca na // SAm_68 //",
  "GS26sbhami_u:137": "ekatve 'nyatarābhāvaḥ śeṣābhāvo 'vinābhuvaḥ /",
  "GS26sbhami_u:138": "dvitvasaṅkhyāvirodhaśca saṃvṛtiścenmṛṣaiva sā // SAm_69 //",
  "GS26sbhami_u:139": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:140": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_70 //",
  "GS26sbhami_u:141": "dravyaparyāyayoraikyaṃ tayoravyatirekataḥ /",
  "GS26sbhami_u:142": "pariṇāmaviśeṣācca śaktimacchaktibhāvataḥ // SAm_71 //",
  "GS26sbhami_u:143": "saṃjñāsaṃkhyāviśeṣācca svalakṣaṇaviśeṣataḥ /",
  "GS26sbhami_u:144": "prayojanādibhedācca tannānātvaṃ na sarvathā // SAm_72 //",
  "GS26sbhami_u:145": "yadyāpekṣikasiddhiḥ syānna dvayaṃ vyavatiṣṭhate /",
  "GS26sbhami_u:146": "anāpekṣikasiddhau ca na sāmānyaviśeṣatā // SAm_73 //",
  "GS26sbhami_u:147": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:148": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_74 //",
  "GS26sbhami_u:149": "dharmadharmyavinābhāvaḥ sidhyatyanyonyavīkṣayā /",
  "GS26sbhami_u:150": "na svarūpaṃ svato hyetatkārakajñāpakāṅgavat // SAm_75 //",
  "GS26sbhami_u:151": "siddhaṃ ceddhetutaḥ sarvaṃ na pratyakṣādito gatiḥ /",
  "GS26sbhami_u:152": "siddhaṃ cedāgamātsarvaṃ viruddhārthamatānyapi // SAm_76 //",
  "GS26sbhami_u:153": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:154": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_77 //",
  "GS26sbhami_u:155": "vaktaryanāpte yaddhetoḥ sādhyaṃ taddhetusādhitam /",
  "GS26sbhami_u:156": "āpte vaktari tadvākyātsādhyamāgamasādhitam // SAm_78 //",
  "GS26sbhami_u:157": "antaraṅgārthataikānte buddhivākyaṃ mṛṣākhilam /",
  "GS26sbhami_u:158": "pramāṇābhāsamevātastatpramāṇādṛte katham // SAm_79 //",
  "GS26sbhami_u:159": "sādhyasādhanavijñapteryadi vijñaptimātratā /",
  "GS26sbhami_u:160": "na sādhyaṃ na ca hetuśca pratijñāhetudoṣataḥ // SAm_80 //",
  "GS26sbhami_u:161": "bahiraṅgārthataikānte pramāṇābhāsanihnavāt /",
  "GS26sbhami_u:162": "sarveṣāṃ kāryasiddhiḥ syādviruddhārthābhidhāyinām // SAm_81 //",
  "GS26sbhami_u:163": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:164": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_82 //",
  "GS26sbhami_u:165": "bhāvaprameyāpekṣāyāṃ pramāṇābhāsanihnavaḥ /",
  "GS26sbhami_u:166": "bahiḥprameyāpekṣāyāṃ pramāṇaṃ tannibhaṃ ca te // SAm_83 //",
  "GS26sbhami_u:167": "jīvaśabdaḥ sabāhyārthaḥ saṃjñātvāddhetuśabdavat /",
  "GS26sbhami_u:168": "māyādibhrāntisaṃjñāśca māyādyaiḥ svaiḥ pramoktivat // SAm_84 //",
  "GS26sbhami_u:169": "buddhiśabdārthasaṃjñāstāstisro buddhyādivācikāḥ /",
  "GS26sbhami_u:170": "tulyā buddhyādibodhāśca trayastatpratibimbikāḥ // SAm_85 //",
  "GS26sbhami_u:171": "vaktṛśrotṛpramātṝṇāṃ vākyabodhapramāḥ pṛthak /",
  "GS26sbhami_u:172": "bhrāntāveva pramābhrāntau bāhyārthau tādṛśetarau // SAm_86 //",
  "GS26sbhami_u:173": "buddhiśabdapramāṇatvaṃ bāhyārthe sati nāsati /",
  "GS26sbhami_u:174": "satyānṛtavyavasthaivaṃ yujyate 'rthāptyanāptiṣu // SAm_87 //",
  "GS26sbhami_u:175": "daivādevārthasiddhiśceddaivaṃ pauruṣataḥ katham /",
  "GS26sbhami_u:176": "daivataścedanirmokṣaḥ pauruṣaṃ niṣphalaṃ bhavet // SAm_88 //",
  "GS26sbhami_u:177": "pauruṣādeva siddhiścetpauruṣaṃ daivataḥ katham /",
  "GS26sbhami_u:178": "pauruṣāccedamoghaṃ syātsarvaprāṇiṣu pauruṣam // SAm_89 //",
  "GS26sbhami_u:179": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:180": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_90 //",
  "GS26sbhami_u:181": "abuddhipūrvāpekṣāyāmiṣṭāniṣṭaṃ svadaivataḥ /",
  "GS26sbhami_u:182": "buddhipūrvavyapekṣāyāmiṣṭāniṣṭaṃ svapauruṣāt // SAm_91 //",
  "GS26sbhami_u:183": "pāpaṃ dhruvaṃ pare duḥkhātpuṇyaṃ ca sukhato yadi /",
  "GS26sbhami_u:184": "acetanākaṣāyau ca badhyeyātāṃ nimittataḥ // SAm_92 //",
  "GS26sbhami_u:185": "puṇyaṃ dhruvaṃ svato duḥkhātpāpaṃ ca sukhato yadi /",
  "GS26sbhami_u:186": "vītarāgo munirvidvāṃstābhyāṃ yuñjyānnimittataḥ // SAm_93 //",
  "GS26sbhami_u:187": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:188": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_94 //",
  "GS26sbhami_u:189": "viśuddhisaṃkleśāṅgaṃ cetsvaparasthaṃ sukhāsukham /",
  "GS26sbhami_u:190": "puṇyapāpāsravau yuktau na cedvyarthastavārhataḥ // SAm_95 //",
  "GS26sbhami_u:191": "ajñānācceddhruvo bandho jñeyānantyānna kevalī /",
  "GS26sbhami_u:192": "jñānastokādvimokṣaścedajñānād bahuto 'nyathā // SAm_96 //",
  "GS26sbhami_u:193": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /",
  "GS26sbhami_u:194": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_97 //",
  "GS26sbhami_u:195": "ajñānānmohato bandho nājñānādvītamohataḥ /",
  "GS26sbhami_u:196": "jñānastokācca mokṣaḥ syādamohānmohito 'nyathā // SAm_98 //",
  "GS26sbhami_u:197": "kāmādiprabhavaścitraḥ karmabandhānurūpataḥ /",
  "GS26sbhami_u:198": "tacca karma svahetubhyo jīvāste śuddhyaśuddhitaḥ // SAm_99 //",
  "GS26sbhami_u:199": "śuddhyaśuddhī punaḥ śaktī te pākyāpākyaśaktivat /",
  "GS26sbhami_u:200": "sādyanādī tayorvyaktī svabhāvo 'tarkagocaraḥ // SAm_100 //",
  "GS26sbhami_u:201": "tattvajñānaṃ pramāṇaṃ te yugapat sarvabhāsanam /",
  "GS26sbhami_u:202": "kramabhāvi ca yajjñānaṃ syādvādanayasaṃskṛtam // SAm_101 //",
  "GS26sbhami_u:203": "upekṣā phalamādyasya śeṣasyādānahānadhīḥ /",
  "GS26sbhami_u:204": "pūrvaṃ vājñānanāśo vā sarvasyāsya svagocare // SAm_102 //",
  "GS26sbhami_u:205": "vākyeṣvanekāntadyotī gamyaṃ prati viśeṣakaḥ /",
  "GS26sbhami_u:206": "syānnipāto 'rthayogitvāttava kevalināmapi // SAm_103 //",
  "GS26sbhami_u:207": "syādvādaḥ sarvathaikāntayogātkiṃvṛttacidvidhiḥ /",
  "GS26sbhami_u:208": "saptabhaṅganayāpekṣo heyādeyaviśeṣakaḥ // SAm_104 //",
  "GS26sbhami_u:209": "syādvādakevalajñāne sarvatattvaprakāśane /",
  "GS26sbhami_u:210": "bhedaḥ sākṣādasākṣācca hyavastvanyatamaṃ bhavet // SAm_105 //",
  "GS26sbhami_u:211": "sadharmaṇaiva sādhyasya sādharmyādavirodhataḥ /",
  "GS26sbhami_u:212": "syādvādapravibhaktārthaviśeṣavyañjako nayaḥ // SAm_106 //",
  "GS26sbhami_u:213": "nayopanayaikāntānāṃ trikālānāṃ samuccayaḥ /",
  "GS26sbhami_u:214": "avibhrāḍ bhāvasambandho dravyamekamanekadhā // SAm_107 //",
  "GS26sbhami_u:215": "mithyāsamūho mithyā cenna mithyaikāntatāsti naḥ /",
  "GS26sbhami_u:216": "nirapekṣā nayā mithyā sāpekṣā vastu te 'rthakṛt // SAm_108 //",
  "GS26sbhami_u:217": "niyamyate 'rtho vākyena vidhinā vāraṇena vā /",
  "GS26sbhami_u:218": "tathānyathā ca so 'vaśyamaviśeṣyatvamanyathā // SAm_109 //",
  "GS26sbhami_u:219": "tadatadvastu vāgeṣā tadevetyanuśāsati /",
  "GS26sbhami_u:220": "na satyā syānmṛṣāvākyaiḥ kathaṃ tattvārthadeśanā // SAm_110 //",
  "GS26sbhami_u:221": "vāksvabhāvo 'nyavāgarthapratiṣedhaniraṅkuśaḥ /",
  "GS26sbhami_u:222": "āha ca svārthasāmānyaṃ tādṛgvācyaṃ khapuṣpavat // SAm_111 //",
  "GS26sbhami_u:223": "sāmānyavāgviśeṣe cenna śabdārthā mṛṣā hi sā /",
  "GS26sbhami_u:224": "abhipretaviśeṣāpteḥ syātkāraḥ satyalāñchanaḥ // SAm_112 //",
  "GS26sbhami_u:225": "vidheyamīpsitārthāṅgaṃ pratiṣedhavirodhi yat /",
  "GS26sbhami_u:226": "tathaivādeyaheyatvamiti syādvādasaṃsthitiḥ // SAm_113 //",
  "GS26sbhami_u:227": "itīyamāptamimāṃsā vihitā hitamicchatā /",
  "GS26sbhami_u:228": "samyaṅmithyopadeśārthaviśeṣapratipattaye // SAm_114 //",
  "GS26sbhami_u:229": "jayati jagati kleśāveśaprapaṃcahimāṃśumān",
  "GS26sbhami_u:230": "vihitaviṣamaikāntadhvāntapramāṇanayāṃśumān /",
  "GS26sbhami_u:231": "yatipatirajo yasyādhṛṣṭānmatāmbunidherlavān",
  "GS26sbhami_u:232": "svamatamatayastīrthyā nānā pare samupāsate // SAm_115 //"
}