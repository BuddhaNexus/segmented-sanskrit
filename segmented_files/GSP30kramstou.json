{
  "GSP30kramstou:0": "ayaṃ duḥkhavrātavrataparigame pāraṇavidhir",
  "GSP30kramstou:1": "mahāsaukhyāsāraprasaraṇarase durdinam idam /",
  "GSP30kramstou:2": "yad anyanyakkṛtyā viṣamaviśikhaploṣaṇaguror vibhoḥ stotre",
  "GSP30kramstou:3": "śaśvat pratiphalati ceto gatabhayam // 1 //",
  "GSP30kramstou:4": "vimṛśya svātmānaṃ vimṛśati punaḥ stutyacaritaṃ tathā",
  "GSP30kramstou:5": "stotā stotre prakaṭayati bhedaikaviṣaye /",
  "GSP30kramstou:6": "vimṛṣṭaś ca svātmā nikhilaviṣayajñānasamaye taditthaṃ",
  "GSP30kramstou:7": "tvatstotre 'ham iha satataṃ yatnarahitaḥ // 2 //",
  "GSP30kramstou:8": "anāmṛṣṭaḥ svātmā na hi bhavati bhāvapramitibhāj anāmṛṣṭaḥ",
  "GSP30kramstou:9": "svātmety api hi na vinā 'marśanavidheḥ /",
  "GSP30kramstou:10": "śivaś cāsau svātmā sphurad akhilabhāvaikasarasas tato 'ham",
  "GSP30kramstou:11": "tvatstotre pravaṇahṛdayo nityasukhitaḥ // 3 //",
  "GSP30kramstou:12": "vicitrair jātyādibhramaṇaparipāṭīparikarair avāptam",
  "GSP30kramstou:13": "sārvajñaṃ hṛdaya yad ayatnena bhavatā /",
  "GSP30kramstou:14": "tad antas tvadbodhaprasarasaraṇībhūtamahasi sphuṭam vāci",
  "GSP30kramstou:15": "prāpya prakaṭaya vibhoḥ stotram adhunā // 4 //",
  "GSP30kramstou:16": "vidhunvāno bandhābhimatabhavamārgasthitim imāṃ",
  "GSP30kramstou:17": "rasīkṛtyānantastutihutavahaploṣitabhedām /",
  "GSP30kramstou:18": "vicitrasvasphārasphuritamahimārambharabhasāt piban bhāvān",
  "GSP30kramstou:19": "etān varada madamatto 'smi sukhitaḥ // 5 //",
  "GSP30kramstou:20": "bhava prājyaiśvaryaprathitabahuśakter bhagavato vicitraṃ",
  "GSP30kramstou:21": "cāritraṃ hṛdayam adhiśete yadi tataḥ /",
  "GSP30kramstou:22": "kathaṃ stotraṃ kuryād atha ca kurute tena sahasā",
  "GSP30kramstou:23": "śivaikātmyaprāptau śivanatir upāyaḥ prathamakaḥ // 6 //",
  "GSP30kramstou:24": "jvaladrūpaṃ bhāsvatpacanam atha dāhaṃ prakaṭanam",
  "GSP30kramstou:25": "vimucyānyad vahneḥ kim api ghaṭate naiva hi vapuḥ /",
  "GSP30kramstou:26": "stuve saṃvidraśmīn yadi nijanijāṃs tena sa nuto bhaven",
  "GSP30kramstou:27": "nānyaḥ kaścid bhavati parameśasya vibhavaḥ // 7 //",
  "GSP30kramstou:28": "vicitrārambhatve galitaniyame yaḥ kila rasaḥ",
  "GSP30kramstou:29": "paricchedābhāvāt paramaparipūrṇatvam asamam /",
  "GSP30kramstou:30": "svayaṃ bhāsāṃ yogaḥ sakalabhavabhāvaikamayatāviruddhair",
  "GSP30kramstou:31": "dharmaughaiḥ paracitir anarghocitaguṇā // 8 //",
  "GSP30kramstou:32": "itīdṛkṣair rūpair varada vividhaṃ te kila vapur vibhāti",
  "GSP30kramstou:33": "svāṃśe 'smin jagati gatabhedaṃ bhagavataḥ /",
  "GSP30kramstou:34": "tad evaitatstotuṃ hṛdayam atha gīrbāhyakaraṇaprabandhāś ca",
  "GSP30kramstou:35": "syur me satatam aparityaktarabhasaḥ // 9 //",
  "GSP30kramstou:36": "tavaivaikasyāntaḥ sphuritamahaso bodhajaladher",
  "GSP30kramstou:37": "vicitrormivrātaprasaraṇaraso yaḥ svarasataḥ /",
  "GSP30kramstou:38": "ta evāmī sṛṣṭisthitilayamayasphūrjitarucāṃ",
  "GSP30kramstou:39": "śaśāṅkārkāgnīnāṃ yugapad udayāpāyavibhavāḥ // 10 //",
  "GSP30kramstou:40": "ataś citrācitrakramataditarādisthitijuṣo vibhoḥ śaktiḥ",
  "GSP30kramstou:41": "śaśvad vrajati na vibhedaṃ katham api /",
  "GSP30kramstou:42": "tad etasyāṃ bhūmāv akulam iti te yat kila padaṃ",
  "GSP30kramstou:43": "tadekāgrībhūyān mama hṛdayabhūr bhairava vibho // 11 //",
  "GSP30kramstou:44": "amuṣmāt saṃpūrnāt vata rasamahollāsasarasān nijāṃ śaktiṃ",
  "GSP30kramstou:45": "bhedaṃ gamayasi nijecchāprasarataḥ /",
  "GSP30kramstou:46": "anarghaṃ svātantryaṃ tava tadidam atyadbhutamayīṃ",
  "GSP30kramstou:47": "bhavacchaktiṃ stunvan vigalitabhayo 'haṃ śivamayaḥ // 12 //",
  "GSP30kramstou:48": "idantāvad rūpaṃ tava bhagavataḥ śaktisarasaṃ",
  "GSP30kramstou:49": "kramābhāvād eva prasabhavigalat kālakalanam /",
  "GSP30kramstou:50": "manaḥśaktyā vācāpy atha karaṇacakrair bahir atho ghaṭādyais",
  "GSP30kramstou:51": "tadrūpaṃ yugapad adhitiṣṭheyam aniśam // 13 //",
  "GSP30kramstou:52": "kramollāsaṃ tasyāṃ bhuvi viracayan bhedakalanāṃ",
  "GSP30kramstou:53": "svaśaktīnāṃ deva prathayasi sadā svātmani tataḥ /",
  "GSP30kramstou:54": "kriyājñānecchākhyāṃ sthitilayamahāsṛṣṭivibhavāṃ trirūpaṃ",
  "GSP30kramstou:55": "bhūyāsaṃ samadhiśayituṃ vyagrahṛdayaḥ // 14 //",
  "GSP30kramstou:56": "purā sṛṣṭir līnā hutavahamayī yātra vilaset",
  "GSP30kramstou:57": "parollāsaunmukhyaṃ vrajati śaśisaṃsparśasubhagā /",
  "GSP30kramstou:58": "hutāśendusphārobhayavibhavabhāg bhairavavibho taveyaṃ",
  "GSP30kramstou:59": "sṛṣṭyākhyā mama manasi nityaṃ vilasatām // 15 //",
  "GSP30kramstou:60": "visṛṣṭe bhāvāṃśe bahir atiśayāsvādavirase yadā",
  "GSP30kramstou:61": "tatraiva tvaṃ bhajasi rabhasād raktimayatām /",
  "GSP30kramstou:62": "tadā raktā devī tava sakalabhāveṣu nanu māṃ kriyād",
  "GSP30kramstou:63": "raktāpānakramaghaṭitagoṣṭhīgataghṛṇam // 16 //",
  "GSP30kramstou:64": "bahir vṛttiṃ hātuṃ citibhuvam udārāṃ nivasituṃ yadā",
  "GSP30kramstou:65": "bhāvābhedaṃ prathayasi vinaṣṭormicapalaḥ /",
  "GSP30kramstou:66": "sthiter nāśaṃ devī kalayati tadā sā tava vibho sthiteḥ",
  "GSP30kramstou:67": "sāṃsārikyāḥ kalayatu vināśaṃ mama sadā // 17 //",
  "GSP30kramstou:68": "jagatsaṃhāreṇa praśamayitukāmaḥ svarabhasāt",
  "GSP30kramstou:69": "svaśaṅkātaṅkākhyaṃ vidhim atha niṣedhaṃ prathayasi /",
  "GSP30kramstou:70": "yamaṃ sṛṣṭvetthaṃ tvaṃ punar api ca śaṅkāṃ vidalayan",
  "GSP30kramstou:71": "mahādevī seyaṃ mama bhavabhayaṃ saṃdalayatām // 18 //",
  "GSP30kramstou:72": "vilīne śaṅkaughe sapadi paripūrṇe ca vibhave gate",
  "GSP30kramstou:73": "lokācāre galitavibhave śāstraniyame /",
  "GSP30kramstou:74": "anantaṃ bhogyaughaṃ grasitum abhito laṃpaṭarasā vibho",
  "GSP30kramstou:75": "saṃhārākhyā mama hṛdi bhidāṃśaṃ praharatu // 19 //",
  "GSP30kramstou:76": "taditthaṃ devībhiḥ sapadi dalite bhedavibhave",
  "GSP30kramstou:77": "vikalpaprāṇāsau pravilasati mātṛsthitir alam /",
  "GSP30kramstou:78": "ataḥ saṃhārāṃśam nijahṛdi vimṛśya sthitimayī",
  "GSP30kramstou:79": "prasannā syān mṛtyupralayakaraṇī me bhagavatī // 20 //",
  "GSP30kramstou:80": "taditthaṃ te tisro nijavibhavavisphāraṇavaśād avāptāḥ",
  "GSP30kramstou:81": "ṣaṭcakrakramakṛtapadaṃ śaktaya imāḥ /",
  "GSP30kramstou:82": "kramād unmeṣeṇa pravidadhati citrāṃ bhavadaśām imābhyo",
  "GSP30kramstou:83": "devībhyaḥ pravaṇahṛdayaḥ syāṃ gatabhayaḥ // 21 //",
  "GSP30kramstou:84": "imāṃ rundhe bhūmiṃ bhavabhayabhidātaṅkakaraṇīm imāṃ",
  "GSP30kramstou:85": "bodhaikāntadrutirasamayīm cāpi vidadhe /",
  "GSP30kramstou:86": "taditthaṃ saṃrodhaṃ drutim atha vilupyāśubhatatīr",
  "GSP30kramstou:87": "yatheṣṭaṃ cācāraṃ bhajati lasatāṃ sā mama hṛdi // 22 //",
  "GSP30kramstou:88": "kriyābuddhyakṣādeḥ parimitapade mānapadavīm avāptasya",
  "GSP30kramstou:89": "sphāraṃ nijanijarucā saṃharati yā /",
  "GSP30kramstou:90": "iyaṃ mārta.ṅdasya sthitipadayujaḥ sāram akhilam haṭhād",
  "GSP30kramstou:91": "ākarṣantī kṛśatu mama bhedaṃ bhavabhayāt // 23 //",
  "GSP30kramstou:92": "samagrām akṣālīṃ kramavirahitā sātmani muhur",
  "GSP30kramstou:93": "niveśyānantāntarbahalitamahāraśminivahā /",
  "GSP30kramstou:94": "parā divyānandaṃ kalayitum udārādaravatī prasannā me",
  "GSP30kramstou:95": "bhūyāt hṛdayapadavīṃ bhūṣayatu ca // 24 //",
  "GSP30kramstou:96": "pramāṇe saṃlīne śivapadalasadvaibhavavaśāt",
  "GSP30kramstou:97": "śarīraprāṇādir mitakṛtakamātṛsthitimayaḥ /",
  "GSP30kramstou:98": "yadā kālopādhiḥ pralayapadam āsādayati te tadā devī yāsau",
  "GSP30kramstou:99": "lasati mama sā syāc chivamayī // 25 //",
  "GSP30kramstou:100": "prakāśākhyā saṃvit kramavirahitā śūnyapadato",
  "GSP30kramstou:101": "bahirlīnātyantam prasarati samācchādakatayā /",
  "GSP30kramstou:102": "tato 'py antaḥsāre galitarabhasād akramatayā mahākālī",
  "GSP30kramstou:103": "seyaṃ mama kalayatāṃ kālam akhilam // 26 //",
  "GSP30kramstou:104": "tato devyāṃ yasyāṃ paramaparipūrṇasthitijuṣi kramaṃ",
  "GSP30kramstou:105": "vicchidyāśu sthitimatirasāt saṃvidadhati /",
  "GSP30kramstou:106": "pramāṇaṃ mātāraṃ mitim atha samagraṃ jagad idaṃ sthitāṃ",
  "GSP30kramstou:107": "kroḍīkṛtya śrayatu mama cittaṃ citim imām // 27 //",
  "GSP30kramstou:108": "anargalasvātmamaye maheśe tiṣṭhanti",
  "GSP30kramstou:109": "yasmin vibhuśaktayas tāḥ /",
  "GSP30kramstou:110": "taṃ śaktimantaṃ praṇamāmi devaṃ",
  "GSP30kramstou:111": "manthānasaṃjñaṃ jagadekasāram // 28 //",
  "GSP30kramstou:112": "itthaṃ svaśaktikiraṇaughanutiprabandhān ākarṇya deva",
  "GSP30kramstou:113": "yadi me vrajasi prasādam /",
  "GSP30kramstou:114": "tenāśu sarvajanatāṃ",
  "GSP30kramstou:115": "nijaśāsanāṃśusaṃśāntitākhilatamaḥpaṭalāṃ vidheyāḥ // 29 //",
  "GSP30kramstou:116": "ṣaṭṣaṣṭinām eke varṣe navamyām asite 'hani /",
  "GSP30kramstou:117": "mayā 'bhinavaguptena mārgaśīrṣe stutaḥ śivaḥ //",
  "GSP30kramstou:118": "iti śrī abhinavaguptapādācāryakṛtaṃ kramastotraṃ sampūrṇam //",
  "GSP30kramstou:119": "Gandharva-nagaram /",
  "GSP30kramstou:120": "DSO Sanskrit Archive"
}