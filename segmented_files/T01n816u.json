{
    "T01n816u_001:0": " avalokiteśvarastotram",
    "T01n816u_001:1": " vāsukināgarājakṛtam",
    "T01n816u_001:2": " om namo'valokiteśvarāya",
    "T01n816u_001:3": " jaṭādharaṃ saumyaviśālalocanaṃ",
    "T01n816u_001:4": " sadāprasannānanacandramaṇḍalam |",
    "T01n816u_001:5": " surāsurairvanditapādapaṅkajaṃ",
    "T01n816u_001:6": " namāmi nāthaṃ maṇipadmasaṃbhavam ||1 ||",
    "T01n816u_001:7": " sarojapatrāyatadivyalocanaṃ",
    "T01n816u_001:8": " kudṛṣṭisaṃśodhitaśuddhalocanam |",
    "T01n816u_001:9": " kṛpāmṛtārdraṃ jagadekalocanaṃ",
    "T01n816u_001:10": " namāmi nāthaṃ maṇipadmasaṃbhavam ||2 ||",
    "T01n816u_001:11": " hārenduhārārdhahimādhikojjavalaṃ",
    "T01n816u_001:12": " nighṛṣṭagaṇḍāmalalolakuṇḍalam |",
    "T01n816u_001:13": " gabhastimālākulakoṭisaṃkulaṃ",
    "T01n816u_001:14": " namāmi nāthaṃ maṇipadmasaṃbhavam ||3 ||",
    "T01n816u_001:15": " prabuddhadharmādhvani dharmadhātukaṃ",
    "T01n816u_001:16": " sahasrabāhuṃ dvicatuśca ṣaḍbhujam |",
    "T01n816u_001:17": " khadhātunā tulyamanantabāhukaṃ",
    "T01n816u_001:18": " namāmi nāthaṃ maṇipadmasaṃbhavam ||4 ||",
    "T01n816u_001:19": " upāyaprajñodadhimanthanodbhavaṃ",
    "T01n816u_001:20": " tridhātusaṃrakṣaṇahetusaṃbhavam |",
    "T01n816u_001:21": " jinendramauliṃ jinadhātusaṃbhavaṃ",
    "T01n816u_001:22": " namāmi nāthaṃ maṇipadmasaṃbhavam ||5 ||",
    "T01n816u_001:23": " padmopari gataṃ nāthaṃ padmahastaṃ jaṭādharam |",
    "T01n816u_001:24": " āryāvalokitaṃ vande sarvasattvānukampakam ||6 ||",
    "T01n816u_001:25": " śrīvāsukināgarājakṛtamāryāvalokiteśvarastotraṃ samāptam |"
}