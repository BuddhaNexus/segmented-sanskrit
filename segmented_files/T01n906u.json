{
    "T01n906u_001:0": " vajravilāsinīsādhanāstavaḥ",
    "T01n906u_001:1": " ravivikasvarabhāsvarasundarī-",
    "T01n906u_001:2": " prabalakeśarasaṃvarakarṇikāt |",
    "T01n906u_001:3": " udayamastakaśṛṅgasamullasa-",
    "T01n906u_001:4": " nnavasupūrṇaśaśāṅkakarāṅkitām || 1 ||",
    "T01n906u_001:5": " vividhapuṣparasāsavavedyapi",
    "T01n906u_001:6": " drutavilambitamadhyanadaddhvaniḥ |",
    "T01n906u_001:7": " alirupetya niśārdhamahotsave",
    "T01n906u_001:8": " dhayati tadgatadhīriti vismayaḥ || 2 ||",
    "T01n906u_001:9": " udayate'stamupaiti niśākaraḥ",
    "T01n906u_001:10": " punarato'pyudayācalamaulitām |",
    "T01n906u_001:11": " ubhayakoṭikalāpinirīkṣaṇe",
    "T01n906u_001:12": " na ca śaśī na vibhāti divākaraḥ || 3 ||",
    "T01n906u_001:13": " ravirudeti sarojadalāntare",
    "T01n906u_001:14": " śikharato'pi vidhuḥ pravilīyate |",
    "T01n906u_001:15": " vrajati cordhvamasau vaḍavānalo",
    "T01n906u_001:16": " gilati rāhuradhaḥ śaśibhāskarau || 4 ||",
    "T01n906u_001:17": " kamalinīkamalāsanalocana-",
    "T01n906u_001:18": " prakaṭagocaragocaramīlanam |",
    "T01n906u_001:19": " prathamamaṅgamidaṃ kuliśāmbuja-",
    "T01n906u_001:20": " dvayanimīlanamatra tato'param || 5 ||",
    "T01n906u_001:21": " sapadi sādhakabījanigharṣaṇāt",
    "T01n906u_001:22": " tritayamambarameti caturthakam |",
    "T01n906u_001:23": " sahajamakṣayadhāmakalāvalī-",
    "T01n906u_001:24": " kalitakālavilāpanaleliham || 6 ||",
    "T01n906u_001:25": " yadi suśikṣitavajravilāsinīgurumukhādhigamo'dhigato bhavet |",
    "T01n906u_001:26": " bhavati yasya tadottamamadhyamādhamaviniścayameti sa tattvadṛk || 7 ||",
    "T01n906u_001:27": " śrīvajravilāsinīsādhanāstavaḥ samāptaḥ |"
}