{
  "GK18dhandasu:0": "daśarūpakam /",
  "GK18dhandasu:1": "[pariccheda 1]",
  "GK18dhandasu:2": "namastasmai gaṇeśāya yatkaṇṭhaḥ puṣkarāyate /",
  "GK18dhandasu:3": "madābhegaghanadhvāno nīlakaṇṭhasya tāṇd.ave // DhDaś_1.1 //",
  "GK18dhandasu:4": "daśarūpānukāreṇa mādyanti bhāvakaḥ /",
  "GK18dhandasu:5": "namaḥ sarvavide tasmai viṣṇave bharatāya ca // DhDaś_1.2 //",
  "GK18dhandasu:6": "kasyacideva kadācid dayayā viṣayaṃ sarasvatī viduṣaḥ /",
  "GK18dhandasu:7": "ghaṭayati kamapi tamanyo jajati jano yena vaidagdhīm // DhDaś_1.3 //",
  "GK18dhandasu:8": "uddhṛtyoddhṛtya sāraṃ yamakhilanigamān nāṭyavedaṃ viriñciś",
  "GK18dhandasu:9": "cakre yasya prayogaṃ munirapi bharatastāṇḍavaṃ nīlakaṇṭhaḥ /",
  "GK18dhandasu:10": "śarvāṇī lāsyamasya pratipadamaparaṃ lakṣma kaḥ kartumīṣṭe",
  "GK18dhandasu:11": "nāṭyānāṃ kintu kiñcit praguṇaracanayā lakṣaṇaṃ saṅkṣipāmi // DhDaś_1.4 //",
  "GK18dhandasu:12": "vyākīrṇe mandabuddhīnāṃ jāyate mativibhramaḥ /",
  "GK18dhandasu:13": "tasyārthastatpadaistena saṅkṣipya kriyate 'ñjasā // DhDaś_1.5 //",
  "GK18dhandasu:14": "ānandanisyandiṣu rūpakeṣu",
  "GK18dhandasu:15": "vyutpattimātraṃ phalamalpabuddhiḥ /",
  "GK18dhandasu:16": "yo'pītihāsādivadāha sādhus",
  "GK18dhandasu:17": "tasmai namaḥ svāduparāṅmukhāya // DhDaś_1.6 //",
  "GK18dhandasu:18": "avasthānukṛtirnāṭyaṃ rūpaṃ dṛśyatayocyate /",
  "GK18dhandasu:19": "rūpakaṃ tat samāropād daśadhaiva rasāśrayam // DhDaś_1.7 //",
  "GK18dhandasu:20": "nāṭakaṃ saprakaraṇaṃ bhāṇaḥ prahasanaṃ ḍimaḥ /",
  "GK18dhandasu:21": "vyāyogasamavakārau vīthyaṅkehāmṛgā iti // DhDaś_1.8 //",
  "GK18dhandasu:22": "anyada bhāvāśrayaṃ nṛtyaṃ nṛttaṃ tālalayāśrayam /",
  "GK18dhandasu:23": "ādyaṃ padārthābhinayo mārgo deśo tathā param // DhDaś_1.9 //",
  "GK18dhandasu:24": "madhuroddhatabhedena tad dvayaṃ dvividhaṃ punaḥ /",
  "GK18dhandasu:25": "lāsyatāṇḍavarūpeṇa nāṭakādyupakārakam // DhDaś_1.10 //",
  "GK18dhandasu:26": "vastu netā rasasteṣāṃ bhedako vastu ca dvidhā /",
  "GK18dhandasu:27": "tatrādhikārikaṃ mukhyamaṅgaṃ prāsaṅgikaṃ viduḥ // DhDaś_1.11 //",
  "GK18dhandasu:28": "adhikāraḥ phalasvāmyamadhikāro ca tatprabhuḥ /",
  "GK18dhandasu:29": "tanniravartyamabhivyāpi vṛttaṃ syādādhikārikam // DhDaś_1.12 //",
  "GK18dhandasu:30": "prāsaṅgikaṃ parārthasya svārtho yasya prasaṅgataḥ /",
  "GK18dhandasu:31": "sānubandhaṃ patākākhyaṃ prakarī ca pradeśabhāk // DhDaś_1.13 //",
  "GK18dhandasu:32": "prāsaṅgikaṃ parārthasya svārtho yasya prasaṅgataḥ /",
  "GK18dhandasu:33": "sānubandhaṃ tulyasaṃvidhānaviśeṣaṇam // DhDaś_1.14 //",
  "GK18dhandasu:34": "prakhyātotpādyamiśratvabhedāt tredhāpi tat tridhā /",
  "GK18dhandasu:35": "prakhyātamitihāsāderutpādyaṃ kavikalpitam /",
  "GK18dhandasu:36": "miśraṃ ca saṅkarāt tābhyāṃ divyanartyādibhedataḥ // DhDaś_1.15 //",
  "GK18dhandasu:37": "kāryaṃ trivargastat śuddhamekānekānubandhi ca /",
  "GK18dhandasu:38": "svalpoddiṣṭastu taddheturbījaṃ vistāryanekadhā /",
  "GK18dhandasu:39": "avāntarārthavicchede binduracchedakāraṇam // DhDaś_1.16 //",
  "GK18dhandasu:40": "bījabindupatākākhyaprakarokāryalakṣaṇāḥ /",
  "GK18dhandasu:41": "arthaprakṛtayaḥ pañca tā etāḥ parikīrtitāḥ // DhDaś_1.17 //",
  "GK18dhandasu:42": "avasthāḥ pañca kāryasya prārabdhasya phalārthibhiḥ /",
  "GK18dhandasu:43": "ārambhayatnaprāptyāśāniyatāptiphalāgamāḥ // DhDaś_1.18 //",
  "GK18dhandasu:44": "autsukyamātramārambhaḥ phalalābhāya bhūyase /",
  "GK18dhandasu:45": "prayatnastu tadaprāptau vyāpāro'titvarānvitaḥ // DhDaś_1.19 //",
  "GK18dhandasu:46": "upāyāpāyaśaṅkābhyāṃ prāptyāśā prāptisambhavaḥ /",
  "GK18dhandasu:47": "apāyābhāvataḥ prāptirniyatāptiḥ suniścitā /",
  "GK18dhandasu:48": "samagraphalasampattiḥ phalayogo yathoditaḥ // DhDaś_1.20 //",
  "GK18dhandasu:49": "arthaprakṛtayaḥ pañca pañcāvasthāsamanvitāḥ /",
  "GK18dhandasu:50": "yathāsaṅkhyena jāyante mukhādyāḥ pañca sandhayaḥ // DhDaś_1.21 //",
  "GK18dhandasu:51": "antaraikārthasambandhaḥ sandhirekānvaye sati /",
  "GK18dhandasu:52": "mukhapratimukhe garbhaḥ sāvamarśopasaṃhṛtiḥ // DhDaś_1.22 //",
  "GK18dhandasu:53": "mukhaṃ bījasamutpattirnānārtharasasambhavā /",
  "GK18dhandasu:54": "aṅgāni dvādaśaitasya bījārambhasamanvayāt // DhDaś_1.23 //",
  "GK18dhandasu:55": "upakṣepaḥ parikaraḥ parinyāso vilobhanam /",
  "GK18dhandasu:56": "yuktiḥ prāptiḥ samādhānaṃ vidhānaṃ paribhāvanā /",
  "GK18dhandasu:57": "udbhedabhedakaraṇānyanvarthānyatha lakṣaṇam // DhDaś_1.24 //",
  "GK18dhandasu:58": "bījanyāsas upakṣepaḥ tadbāhulyaṃ parikriyā /",
  "GK18dhandasu:59": "tannniṣpattiḥ parinyāso guṇākhyānād vilobhanam // DhDaś_1.25 //",
  "GK18dhandasu:60": "sampradhāraṇamarthānāṃ yuktiḥ prāptiḥ sukhāgamaḥ /",
  "GK18dhandasu:61": "bījāgamaḥ samādhānaṃ vidhānaṃ sukhaduḥkhakṛt // DhDaś_1.26 //",
  "GK18dhandasu:62": "paribhāvodbhutāveśa udbhedo gūḍhabhedanam /",
  "GK18dhandasu:63": "karaṇaṃ prakṛtārambho bhedaḥ protsāhanā matā // DhDaś_1.27 //",
  "GK18dhandasu:64": "lakṣyālakṣya ivodbhedastasya pratimukhaṃ bhavet /",
  "GK18dhandasu:65": "binduprayatnānugamādaṅgānyasya trayodaśa // DhDaś_1.28 //",
  "GK18dhandasu:66": "vilāsaḥ parisarpaśca vidhūtaṃ śamanarmaṇo /",
  "GK18dhandasu:67": "narmadyutiḥ pragayaṇaṃ nirodhaḥ paryupāsanam // DhDaś_1.29 //",
  "GK18dhandasu:68": "vajraṃ puṣpamupanyāpto varṇa saṃhāra ityapi /",
  "GK18dhandasu:69": "ratyarthehā vilāsaḥ syād dṛṣṭanaṣṭānusarpaṇam /",
  "GK18dhandasu:70": "parisarpā vidhūtaṃ syādaratistacchamaḥ śamaḥ // DhDaś_1.30 //",
  "GK18dhandasu:71": "parihāsavatto narma dhṛtistajjā dyutirmatā /",
  "GK18dhandasu:72": "uttarā vāk pragayaṇaṃ hitarodho nirodhanam // DhDaś_1.31 //",
  "GK18dhandasu:73": "paryupāstiranunayaḥ puṣpaṃ vākyaṃ viśeṣavat /",
  "GK18dhandasu:74": "upanyāsastu sopāyaṃ vajraṃ pratyakṣaniṣṭhuram /",
  "GK18dhandasu:75": "cāturvarṇopagamanaṃ varṇasaṃhāra iṣyate // DhDaś_1.32 //",
  "GK18dhandasu:76": "garbhastu dṛṣṭanaṣṭasya bījasyānveṣaṇaṃ muhuḥ /",
  "GK18dhandasu:77": "dvādaśāṅgaḥ patākā syān na vā syāt prāptisambhavaḥ // DhDaś_1.33 //",
  "GK18dhandasu:78": "abhūtāharaṇaṃ mārgā rūpodāharaṇe kramaḥ /",
  "GK18dhandasu:79": "saṅgahaścānumānaṃ ca toṭakādhivale tathā // DhDaś_1.34 //",
  "GK18dhandasu:80": "udvegasambhramākṣepā lakṣaṇaṃ ca praṇīyate /",
  "GK18dhandasu:81": "abhūtāharaṇaṃ chadma mārgastattvārthakīrtanam // DhDaś_1.35 //",
  "GK18dhandasu:82": "rūpaṃ vitarkavad vākyaṃ sotkarṣaṃ syādudāhṛtiḥ /",
  "GK18dhandasu:83": "kramaḥ sañcintyamānāptir bhāvajñānamathāpare // DhDaś_1.36 //",
  "GK18dhandasu:84": "saṅgrahaḥ sāmadānoktir abhyūho liṅgato'numā /",
  "GK18dhandasu:85": "adhivalamabhisandhiḥ saṃrabdhaṃ toṭakaṃ vacaḥ // DhDaś_1.37 //",
  "GK18dhandasu:86": "udvego'richa(?)",
  "GK18dhandasu:87": "tā bhītiḥ śaṅkātrāsau ca sambhramaḥ /",
  "GK18dhandasu:88": "garbhabījasamudbhedādākṣepaḥ parikīrtitaḥ // DhDaś_1.38 //",
  "GK18dhandasu:89": "krodhenāvamṛśed yatra vyasanād yā vilobhanāt /",
  "GK18dhandasu:90": "garbhanirbhinnabījārthaḥ so'vamarśo'ṅgasaṅgahaḥ // DhDaś_1.39 //",
  "GK18dhandasu:91": "tatrāpavādasampheṭau vidravadravaśaktayaḥ /",
  "GK18dhandasu:92": "dyutiḥ prasaṅgaśchalanaṃ vyavasāyo virodhanam /",
  "GK18dhandasu:93": "prayocanā vicalanamādānaṃ ca taryodaśa // DhDaś_1.40 //",
  "GK18dhandasu:94": "doṣaprakhyāpavādaḥ syāt sampheṭo roṣabhāṣaṇam /",
  "GK18dhandasu:95": "vidravo vadhabandhādir dravo gurutiraskṛtiḥ // DhDaś_1.41 //",
  "GK18dhandasu:96": "virodhaśamanaṃ śaktistarjanodvejane dyutiḥ /",
  "GK18dhandasu:97": "gurukīrtanaṃ prasaṅgaśchalanaṃ cāvamānanam // DhDaś_1.42 //",
  "GK18dhandasu:98": "vyavasāyaḥ khaśaktyuktiḥ saṃrabdhānāṃ virodhanam /",
  "GK18dhandasu:99": "siddhāmantraṇato bhāvidarśikā syāt prayocanā /",
  "GK18dhandasu:100": "vikatthanā vicalanam ādānaṃ kāryasaṅgrahaḥ // DhDaś_1.43 //",
  "GK18dhandasu:101": "bījavanto mukhādyarthā viprakīrṇā yathāyatham /",
  "GK18dhandasu:102": "aikārthyamupanīyante yatra nirvahaṇaṃ hi tat // DhDaś_1.44 //",
  "GK18dhandasu:103": "sandhirvibodho grathanaṃ nirṇayaḥ paribhāṣaṇam /",
  "GK18dhandasu:104": "prasādānandasamayāḥ kṛtibhāṣopagūhanāḥ /",
  "GK18dhandasu:105": "pūrvabhāvopasaṃhārau praśastiśca caturdaśa // DhDaś_1.45 //",
  "GK18dhandasu:106": "sandhirbījopagamanaṃ vibodhaḥ kāryamārgaṇam /",
  "GK18dhandasu:107": "grathanaṃ tadupakṣepo 'nubhūtākhyā tu nirṇayaḥ // DhDaś_1.46 //",
  "GK18dhandasu:108": "paribhāṣā mitho jalpaḥ prasādaḥ paryupāsanam /",
  "GK18dhandasu:109": "ānando vāñchitāvāptiḥ samayo duḥkhanirgamaḥ // DhDaś_1.47 //",
  "GK18dhandasu:110": "kṛtirlabdhārthaśamanaṃ mānādyāptiśca bhāṣaṇam /",
  "GK18dhandasu:111": "kāryadṛṣṭyadbhutaprāpto pūrvabhāvopagūhane /",
  "GK18dhandasu:112": "varāptiḥ kāvyasaṃhāraḥ praśastiḥ śubhaśaṃsanam // DhDaś_1.48 //",
  "GK18dhandasu:113": "iṣṭasyārthasya racanā gopyaguptiḥ prakāśanam /",
  "GK18dhandasu:114": "rāgaḥ prayogasyāścaryaṃ vṛttāntasyānupakṣayaḥ // DhDaś_1.49 //",
  "GK18dhandasu:115": "dvedhā vibhāgaḥ kartavyaḥ sarvasyāpoha vastunaḥ /",
  "GK18dhandasu:116": "sūcyameva bhavet kiñcid dṛśyaśravyamathāparam // DhDaś_1.50 //",
  "GK18dhandasu:117": "nīraso'nucitastatra saṃsūcyo vastuvistaraḥ /",
  "GK18dhandasu:118": "dṛśyastu madhurodāttarasabhāvanirantaraḥ // DhDaś_1.51 //",
  "GK18dhandasu:119": "arthopakṣepakaiḥ sūcyaṃ pañcabhiḥ pratipādayet /",
  "GK18dhandasu:120": "viṣkambhacūlikāṅgāsyāṅkāvatārapraveśakaiḥ // DhDaś_1.52 //",
  "GK18dhandasu:121": "vṛttavartiṣyamāṇānāṃ kathāṃśānāṃ nidarśakaḥ /",
  "GK18dhandasu:122": "saṅkṣepārthastu viṣkambho madhyapātraprayojitaḥ /",
  "GK18dhandasu:123": "ekānekakṛtaḥ śuddhaḥ saṅkīrṇo nīcamadhyamaiḥ // DhDaś_1.53 //",
  "GK18dhandasu:124": "tadvadevānudāttoktyā nīcapātraprayojitaḥ /",
  "GK18dhandasu:125": "praveśo'ṅkadvayasyāntaḥ śeṣārthasyopasūcakaḥ // DhDaś_1.54 //",
  "GK18dhandasu:126": "antaryavanikāsaṃsthaiścūlikārthasya sūcanā /",
  "GK18dhandasu:127": "aṅkāntapātrairaṅkāsyaṃ chinnāṅkasyārthasūcanāt // DhDaś_1.55 //",
  "GK18dhandasu:128": "aṅkāvatārastvaṅkānte pāto'ṅkasyāvibhāgataḥ /",
  "GK18dhandasu:129": "ebhiḥ saṃsūcayet sūcyaṃ dṛśyamaṅkaiḥ pradarśayet // DhDaś_1.56 //",
  "GK18dhandasu:130": "nāṭyadharmamapekṣyaitat punarvastu tridheṣyate /",
  "GK18dhandasu:131": "sarveṣāṃ niyatasyaiva śrāvyamaśrāvyameva ca // DhDaś_1.57 //",
  "GK18dhandasu:132": "sarvaśrāvyaṃ prakāśaṃ syādaśrāvyaṃ svagataṃ matam /",
  "GK18dhandasu:133": "dvidhānyan nāṭyadharmākhyaṃ janāntamapavāritam // DhDaś_1.58 //",
  "GK18dhandasu:134": "tripatākākareṇānyānapavāryāntarā kathām /",
  "GK18dhandasu:135": "anyonyāmantraṇaṃ yat syāj janānte taj janāntikam /",
  "GK18dhandasu:136": "rahasyaṃ kathyate 'nyasya parāvṛttyāpavāritam // DhDaś_1.59 //",
  "GK18dhandasu:137": "kiṃ bravīṣyevamityādi vinā pātraṃ bravīti yat /",
  "GK18dhandasu:138": "śrutvevānaktamapyekastat syādākāśamāpitam // DhDaś_1.60 //",
  "GK18dhandasu:139": "ityādyaśeṣamiha vastuvibhedajātaṃ",
  "GK18dhandasu:140": "rāmāyaṇādi ca vibhāvya bṛhatkathāṃ ca /",
  "GK18dhandasu:141": "āsūtrayet tadanu netṛrasānuguṇyāc",
  "GK18dhandasu:142": "citrāṃ kathāmucitacāruvacaḥprapañcaiḥ // DhDaś_1.61 // @#--------------------------------------------------------------------------------",
  "GK18dhandasu:144": "[pariccheda 2]",
  "GK18dhandasu:145": "netā vinīto madhurastyāgī dakṣaḥ priyagvadaḥ /",
  "GK18dhandasu:146": "raktalokaḥ śucirvāṅmī rūḍhavaṃśaḥ sthiro yuvā // DhDaś_2.1 //",
  "GK18dhandasu:147": "buddhyutsāhasmṛtiprajñākalāmānasamanvitaḥ /",
  "GK18dhandasu:148": "śūro dṛḍhaśca tejasvī śāstracakṣuśca dhārmikaḥ /",
  "GK18dhandasu:149": "bhedaiścaturdhā lalitaśāntodāttoddhatairayam // DhDaś_2.2 //",
  "GK18dhandasu:150": "niścinto dhīralalitaḥ kalāsaktaḥ sukhī mṛduḥ /",
  "GK18dhandasu:151": "sāmānyagu.ayuktastu dhīraśānto dvijādikaḥ // DhDaś_2.3 //",
  "GK18dhandasu:152": "mahāsattvo'tigambhīraḥ kṣamāvānavikatthanaḥ /",
  "GK18dhandasu:153": "sthiro nigūḍhāhaṅkāro dhīrodātto dṛḍhavrataḥ // DhDaś_2.4 //",
  "GK18dhandasu:154": "darpamātsaryabhūyiṣṭho māyāchadmaparāyaṇaḥ /",
  "GK18dhandasu:155": "dhīroddhatastvahaṅkāro calaścaṇḍo vikattthanaḥ // DhDaś_2.5 //",
  "GK18dhandasu:156": "sa dakṣiṇaḥ śaṭho dhṛṣṭaḥ pūrvāṃ pratyanyayā hṛtaḥ /",
  "GK18dhandasu:157": "dakṣiṇo'syāṃ sahṛdayaḥ gūḍhavipriyakṛt śaṭhaḥ /",
  "GK18dhandasu:158": "vyaktāṅgavaikṛto dhṛṣṭo'nukūlastvekanāyikaḥ // DhDaś_2.6 //",
  "GK18dhandasu:159": "patākānāyakastvanyaḥ pīṭhamardo vicakṣaṇaḥ /",
  "GK18dhandasu:160": "tasyaivānucaro bhaktaḥ kiñcidūnaśca tadguṇaiḥ // DhDaś_2.7 //",
  "GK18dhandasu:161": "ekavidyo viṭaścānyo hāsyakṛc ca vidūṣakaḥ /",
  "GK18dhandasu:162": "lubdho dhīroddhatastabdhaḥ(?)",
  "GK18dhandasu:163": "pāpakṛd vyasanī ripuḥ // DhDaś_2.8 //",
  "GK18dhandasu:164": "śobhā vilāso mādhuryaṃ gāmbhīryaṃ dhairyatejasī /",
  "GK18dhandasu:165": "lalitaudāryamityaṣṭau sattvajāḥ pauruṣā guṇāḥ // DhDaś_2.9 //",
  "GK18dhandasu:166": "nīce dhṛṇādhike spardhā śobhāyāṃ śauryadakṣate /",
  "GK18dhandasu:167": "gatiḥ sadhairyā dṛṣṭiśca vilāse sammitaṃ vacaḥ // DhDaś_2.10 //",
  "GK18dhandasu:168": "ślakṣṇo vikāro mādhuryaṃ saṅkṣobhe sumahatyapi /",
  "GK18dhandasu:169": "gāmbhīryaṃ yatprabhāvena vikāro nopalakṣyate // DhDaś_2.11 //",
  "GK18dhandasu:170": "vyavasāyādacalanaṃ sthairyaṃ vighnakulādapi /",
  "GK18dhandasu:171": "adhikṣepādyasahanaṃ tejaḥ prāṇātyayeṣvapi // DhDaś_2.12 //",
  "GK18dhandasu:172": "śṛṅgārākāraceṣṭātvaṃ sahajaṃ lalitaṃ mṛdu /",
  "GK18dhandasu:173": "priyoktyājīvitād dānamaudāryaṃ sadupagrahaḥ // DhDaś_2.13 //",
  "GK18dhandasu:174": "svānyā sādhāraṇastrīti tadguṇā nāyikā tridhā /",
  "GK18dhandasu:175": "mugdhā madhyā pragalbheti svīyā śīlārjavādiyuk // DhDaś_2.14 //",
  "GK18dhandasu:176": "mugdhā navavayaḥkāmā ratau vāmā mṛduḥ krudhi /",
  "GK18dhandasu:177": "madhyodyadyāvanānaṅgā mohāntasuratakṣamā // DhDaś_2.15 //",
  "GK18dhandasu:178": "dhīrā sotprāsavakroktyā madhyā sāśru kṛtāgamam /",
  "GK18dhandasu:179": "khedayed dayitaṃ kopādadhīrā paruṣākṣaram // DhDaś_2.16 //",
  "GK18dhandasu:180": "yauvanāndhā smaronmattā pragalbhā dayitāṅgake /",
  "GK18dhandasu:181": "vilīyamānevānandād ratārambhe'pyacetanā // DhDaś_2.17 //",
  "GK18dhandasu:182": "sāvahitthādarodāste ratau dhīretarā krudhā /",
  "GK18dhandasu:183": "santarjya tāḍayen madhyā madhyādhīreva taṃ vadet /",
  "GK18dhandasu:184": "dvedhā jyeṣṭhā kaniṣṭā cetyamugdhā dvādaśoditāḥ // DhDaś_2.18 //",
  "GK18dhandasu:185": "anyastrī kanyakoḍhā ca nānyoḍhāṅgirase kvacit /",
  "GK18dhandasu:186": "kanyānurāgamicchātaḥ kuryādaṅgāṅgisaṃśrayam // DhDaś_2.19 //",
  "GK18dhandasu:187": "sādhāraṇastrī gaṇikā kalāprāgalbhyadhaurtyayuk /",
  "GK18dhandasu:188": "channakāmasukhārthājñasvatantrāhaṃyupaṇḍakān // DhDaś_2.20 //",
  "GK18dhandasu:189": "rakteva rañjayedāḍhyān niḥsvān mātrā vivāsayet /",
  "GK18dhandasu:190": "raktaiva tvaprahasane naiṣā divyanṛpāśraye // DhDaś_2.21 //",
  "GK18dhandasu:191": "āsāmaṣṭāvavasthāḥ syuḥ svādhīnapatikādikāḥ /",
  "GK18dhandasu:192": "āsannāyattaramaṇā hṛṣṭā svādhīnabhartṛkā // DhDaś_2.22 //",
  "GK18dhandasu:193": "mudā vāsakasajjā svaṃ maṇḍayatyeṣyati priye /",
  "GK18dhandasu:194": "cirayatyavyalīke tu virahotkaṇṭhitonmanāḥ // DhDaś_2.23 //",
  "GK18dhandasu:195": "jñāte'nyāsaṅgavivṛte khaṇḍterṣyākaṣāyitā /",
  "GK18dhandasu:196": "kalahāntaritāmarṣād vidhūte'nuśayārtiyuk // DhDaś_2.24 //",
  "GK18dhandasu:197": "vipralabdhoktasamayamaprāpte'tivimānitā /",
  "GK18dhandasu:198": "dūradeśābtarasthe tu kāryataḥ proṣitapriyā /",
  "GK18dhandasu:199": "kāmārtābhisaret kāntaṃ sārayed vābhisārikā // DhDaś_2.25 //",
  "GK18dhandasu:200": "cintāniḥśvāsakhedāśruvaivarṇyaglānyabhūṣaṇaiḥ /",
  "GK18dhandasu:201": "yuktāḥ ṣaḍantyā dve cādye kroḍaujjvalyapraharṣitaiḥ // DhDaś_2.26 //",
  "GK18dhandasu:202": "dūtyo dāsī sakhī kārūrdhātreyī prativeśikā /",
  "GK18dhandasu:203": "liṅginī śilpinī svaṃ ca netṛmitraguṇānvitāḥ // DhDaś_2.27 //",
  "GK18dhandasu:204": "yauvane sattvajāḥ strīṇāmalaṅkārāstu viṃśatiḥ /",
  "GK18dhandasu:205": "bhāvo hāvaśca helā ca trayastatra śarīrajāḥ // DhDaś_2.28 //",
  "GK18dhandasu:206": "śobhā kāntiśca dīptiśca mādhuryaṃ ca pragalbhatā /",
  "GK18dhandasu:207": "audāryaṃ dhairyamityete sapta bhāvā ayatnajāḥ // DhDaś_2.29 //",
  "GK18dhandasu:208": "līlā vilāso vicchittirvibhramaḥ kilakiñcitam /",
  "GK18dhandasu:209": "moṭṭāyitaṃ kuṭṭamitaṃ bibboko lalitaṃ tathā /",
  "GK18dhandasu:210": "vihṛtaṃ ceti vijñeyā daśa bhāvāḥ svabhāvajāḥ // DhDaś_2.30 //",
  "GK18dhandasu:211": "nirvikārātmakāt sattvād bhāvastatrādyavikriyā /",
  "GK18dhandasu:212": "hevākasastu śṛṅgāro hāvo'kṣibhrūvikārakṛt // DhDaś_2.31 //",
  "GK18dhandasu:213": "sa eva helā suvyaktaśṛṅgārarasasūcikā /",
  "GK18dhandasu:214": "rūpopabhogatāruṇyaiḥ śobhāṅgānāṃ vibhūṣaṇam // DhDaś_2.32 //",
  "GK18dhandasu:215": "manmathāvāpitacchāyā saiva kāntiriti smṛtā /",
  "GK18dhandasu:216": "anulabaṇatvaṃ mādhuryaṃ dīptiḥ kāntestu vistaraḥ // DhDaś_2.33 //",
  "GK18dhandasu:217": "niḥsādhvasatvaṃ prāgalbhyaṃ audāryaṃ praśrayaḥ sadā /",
  "GK18dhandasu:218": "cāpalāvihatā dhairyaṃ cidvṛttiravikatthanā // DhDaś_2.34 //",
  "GK18dhandasu:219": "priyānukaraṇaṃ līlā madhurāṅgaviceṣṭitaiḥ /",
  "GK18dhandasu:220": "tātkāliko viśeṣastu vilāso'ṅgakriyādiṣu // DhDaś_2.35 //",
  "GK18dhandasu:221": "ākalparacanālpāpi vicchittiḥ kāntipopa(?)kṛt /",
  "GK18dhandasu:222": "vibhramastvarayā kāle bhūṣāsthānaviparyayaḥ // DhDaś_2.36 //",
  "GK18dhandasu:223": "krodhāśruharṣabhītyādeḥ saṅkaraḥ kilakiñcitam /",
  "GK18dhandasu:224": "moṭṭāyitaṃ tu tadbhāvabhāvaneṣṭakathādiṣu // DhDaś_2.37 //",
  "GK18dhandasu:225": "sānandāntaḥ kuṭṭamitaṃ kupyet keśādharagrahe /",
  "GK18dhandasu:226": "garvābhimānādiṣṭe'pi bibboko'nādarakriyā // DhDaś_2.38 //",
  "GK18dhandasu:227": "sukumārāṅgavinyāso masṛṇo lalitaṃ bhavet /",
  "GK18dhandasu:228": "prāptakālaṃ na yad brūyād vrīḍayā vihṛtaṃ hi tat // DhDaś_2.39 //",
  "GK18dhandasu:229": "mantrī svaṃ vobhayaṃ vāpi sakhā tasyārthacintane /",
  "GK18dhandasu:230": "mantriṇā lalitaḥ śeṣāḥ mantrisvāyattasiddhayaḥ // DhDaś_2.40 //",
  "GK18dhandasu:231": "ṛtvikpurohitau dharme tapasvibrahmavādinaḥ /",
  "GK18dhandasu:232": "suhṛtkumārāṭavikā daṇḍe sāmantasainikāḥ // DhDaś_2.41 //",
  "GK18dhandasu:233": "antaḥpure varṣavarāḥ kirātā mūkavāmanāḥ /",
  "GK18dhandasu:234": "mlecchābhīraśakārādyāḥ svasvakāryopayoginaḥ // DhDaś_2.42 //",
  "GK18dhandasu:235": "jyeṣṭhamadhyādhamatvena sarveṣāṃ ca trirūpatā /",
  "GK18dhandasu:236": "tāratamyād yathoktānāṃ guṇānāṃ cottamāditā /",
  "GK18dhandasu:237": "evaṃ nāṭye vidhātavyo nāyakaḥ saparicchadaḥ // DhDaś_2.43 //",
  "GK18dhandasu:238": "tadvyāpārātmikā vṛttiścaturdhā tatra kaiśikī /",
  "GK18dhandasu:239": "gatinṛtyavilāsādyairmṛduḥ śṛṅgāraceṣṭitaiḥ /",
  "GK18dhandasu:240": "narmataisphiñjatatsphoṭatadgaibhaiścaturaṅgikā // DhDaś_2.44 //",
  "GK18dhandasu:241": "vaidagdhyakrīḍitaṃ narma priyopacchandanātmakam /",
  "GK18dhandasu:242": "hāsyenaiva saśṛṅgārabhayena vihitaṃ tridhā // DhDaś_2.45 //",
  "GK18dhandasu:243": "ātmopakṣepasambhogamānaiḥ śṛṅgāryapi tridhā /",
  "GK18dhandasu:244": "śuddhamaṅgaṃ bhayaṃ dvedhā vāgveṣaceṣṭitaiḥ /",
  "GK18dhandasu:245": "sarvaṃ sahāsyamityevaṃ narmāsṭādaśadhoditam // DhDaś_2.46 //",
  "GK18dhandasu:246": "narmasphiñjaḥ sukhārambho bhayānto navasaṅgame /",
  "GK18dhandasu:247": "narmasphoṭastu bhāvānāṃ sūcito'lparaso lavaiḥ // DhDaś_2.47 //",
  "GK18dhandasu:248": "channanetrapratīcāro narmagarbho'rthahetave /",
  "GK18dhandasu:249": "aṅgaiḥ sahāsyanirhāsyairebhireṣātra kaiśikī // DhDaś_2.48 //",
  "GK18dhandasu:250": "viśokā sāttvatī sattvaśauryatyāgadayājavaiḥ /",
  "GK18dhandasu:251": "saṃlāpotthāpakāvasyāṃ sāṅghātyaḥ parivartakaḥ // DhDaś_2.49 //",
  "GK18dhandasu:252": "saṃlāpako gabhīroktirnānābhāvarasā mithaḥ /",
  "GK18dhandasu:253": "utthāpakastu yatrādau yuddhāyotthāpayet param // DhDaś_2.50 //",
  "GK18dhandasu:254": "mantrārthadaivaśaktyādeḥ sāṅghātyaḥ saṅghabhedanam /",
  "GK18dhandasu:255": "prārabdhotthānakāryānyakaraṇāt parivartakaḥ // DhDaś_2.51 //",
  "GK18dhandasu:256": "ebhiraṅgaiścaturdheyaṃ sāttvatyārabhaṭī punaḥ /",
  "GK18dhandasu:257": "māyendrajālasaṅgrāmakrodhodbhrāntādiceṣṭitaiḥ /",
  "GK18dhandasu:258": "saṅkṣiptikā syāt sampheṭo vastūtthānāvapātane // DhDaś_2.52 //",
  "GK18dhandasu:259": "saṅkṣiptavasturacanā saṅkṣiptiḥ śilpayogataḥ /",
  "GK18dhandasu:260": "pūrvanetṛnivṛttyānye netrantaraparigrahaḥ // DhDaś_2.53 //",
  "GK18dhandasu:261": "sampheṭastu samāghātaḥ kruddhasaṃrabdhayordvayoḥ /",
  "GK18dhandasu:262": "māyādyutthāpitaṃ vastu vastūtthāpanamiṣyate /",
  "GK18dhandasu:263": "avapātastu niṣkrāmapraveśatrāsavidravaiḥ // DhDaś_2.54 //",
  "GK18dhandasu:264": "ebhiraṅgaiścaturdheyaṃ nārthavṛttirataḥ parā /",
  "GK18dhandasu:265": "caturthī bhāratī sāpi vācyā nāṭakalakṣaṇe // DhDaś_2.55 //",
  "GK18dhandasu:266": "kaiśikīṃ sāttvatīṃ cārthavṛttimārabhaṭīmiti /",
  "GK18dhandasu:267": "paṭhantaḥ pañcamīṃ vṛttimaudbhaṭāḥ pratijānate // DhDaś_2.56 //",
  "GK18dhandasu:268": "śṛṅgāre kauśikī vīre sāttvatyārabhaṭī punaḥ /",
  "GK18dhandasu:269": "rase raudre ca bībhatse vṛttiḥ sarvatra bhāratī // DhDaś_2.57 //",
  "GK18dhandasu:270": "deśabhāṣākriyāveṣalakṣaṇāḥ syuḥ pravṛttayaḥ /",
  "GK18dhandasu:271": "lokādevāvagamyaitā yathaucityaṃ prayojayet // DhDaś_2.58 //",
  "GK18dhandasu:272": "pādyaṃ tu saṃskṛtaṃ nṝṇāmanīcānāṃ kṛtātmanām /",
  "GK18dhandasu:273": "liṅginīnāṃ mahādevyā mantrijāveśyayoḥ kvacit // DhDaś_2.59 //",
  "GK18dhandasu:274": "strīṇāṃ tu prākṛtaṃ prāyaḥ śūrasenyadhameṣu ca /",
  "GK18dhandasu:275": "piśācātyantanīcādau paiśācaṃ māgadhaṃ tathā // DhDaś_2.60 //",
  "GK18dhandasu:276": "yaddeśaṃ nīcapātraṃ yat taddeśaṃ tasya bhāṣitam /",
  "GK18dhandasu:277": "kāryataścottamādīnāṃ kāryo bhāṣāvyatikramaḥ // DhDaś_2.61 //",
  "GK18dhandasu:278": "bhagavanto varairvācyā vidvaddevarṣiliṅginaḥ /",
  "GK18dhandasu:279": "viprāmātyāgrajāścāryā naṭīsūtramṛtau mithaḥ // DhDaś_2.62 //",
  "GK18dhandasu:280": "rathī sūtena cāyuṣmān pūjyaiḥ śiṣyātmajānujāḥ /",
  "GK18dhandasu:281": "vatseti tātaḥ pūjyo'pi sugṛhītābhidhastu taiḥ // DhDaś_2.63 //",
  "GK18dhandasu:282": "bhāvo'nugena sūtrī ca mārṣetyetena so'pi ca /",
  "GK18dhandasu:283": "devaḥ svāmīti nṛpatirbhṛtyairbhaṭṭeti cādhamaiḥ // DhDaś_2.64 //",
  "GK18dhandasu:284": "āmantraṇīyāḥ pativaj jyeṣṭhamadhyādhamaiḥ striyaḥ /",
  "GK18dhandasu:285": "samā haleti praṣyā ca hañje veśyājjukā tathā // DhDaś_2.65 //",
  "GK18dhandasu:286": "kuṭṭinyamvetyanugataiḥ pūjyā vā jaratī janaiḥ /",
  "GK18dhandasu:287": "vidūṣakeṇa bhavatī rājñī ceṭīti śabdyate // DhDaś_2.66 //",
  "GK18dhandasu:288": "ceṣṭāguṇodāhṛtisattvabhāvān",
  "GK18dhandasu:289": "aśeṣato netṛdaśāvibhinnān /",
  "GK18dhandasu:290": "ko vaktumīśo bharato na yo vā",
  "GK18dhandasu:291": "yo vā na devaḥ śaśikhaṇḍamauliḥ // DhDaś_2.67 // ==============================",
  "GK18dhandasu:293": "[pariccheda 3]",
  "GK18dhandasu:294": "prakṛtitvādathānyeṣāṃ bhūyorasaparigrahāt /",
  "GK18dhandasu:295": "sampūrṇalakṣaṇatvāc ca pūrvaṃ nāṭakamucyate // DhDaś_3.1 //",
  "GK18dhandasu:296": "pūrvaraṅgaṃ vidhāyādau sūtradhāre vinirgate /",
  "GK18dhandasu:297": "praviśya tadvadaparaḥ kāvyamāsthāpayen naṭaḥ // DhDaś_3.2 //",
  "GK18dhandasu:298": "divyamartye sa tadrūpo miśramanyatarastayoḥ /",
  "GK18dhandasu:299": "sūcayed vastu bījaṃ vā mukhaṃ pātramathāpi vā // DhDaś_3.3 //",
  "GK18dhandasu:300": "raṅgaṃ prasādya madhuraiḥ ślokaiḥ kāvyārthasūcakaiḥ /",
  "GK18dhandasu:301": "ṛtuṃ kañcidupādāya bhāratīṃ vṛttimāśrayet // DhDaś_3.4 //",
  "GK18dhandasu:302": "bhāratī saṃskṛtaprāyo vāgvyāpāro naṭāśrayaḥ /",
  "GK18dhandasu:303": "bhedaiḥ prarocanāyuktairvīrthīprahasanāmukhaiḥ // DhDaś_3.5 //",
  "GK18dhandasu:304": "unmukhīkaraṇaṃ tatra praśaṃsātaḥ prarocanā /",
  "GK18dhandasu:305": "vīthī prahasanaṃ cāpi svaprasaṅge'bhidhāsyate /",
  "GK18dhandasu:306": "vīthyaṅgāvyāmukhāṅgatvāducyante'traiva tat punaḥ // DhDaś_3.6 //",
  "GK18dhandasu:307": "sūtradhāro naṭīṃ brūte mārṣaṃ vātha vidūṣakam /",
  "GK18dhandasu:308": "svakāryaṃ prastutākṣepi citroktyā yat tadāmukham // DhDaś_3.7 //",
  "GK18dhandasu:309": "prastāvanā vā tatra syuḥ kathādghātaḥ pravṛttakam /",
  "GK18dhandasu:310": "prayogātiśayaścātha vīthyaṅgāni trayodaśa // DhDaś_3.8 //",
  "GK18dhandasu:311": "svetivṛttasamaṃ vākyamarthaṃ vā yatra sūtriṇaḥ /",
  "GK18dhandasu:312": "gṛhītvā praviśet pātraṃ kathodghāto dvidhaiva saḥ // DhDaś_3.9 //",
  "GK18dhandasu:313": "kālasāmyasamākṣiptapraveśaḥ syāt pravṛttakam /",
  "GK18dhandasu:314": "eṣo'yamityupakṣepāt sūtradhāraprayogataḥ /",
  "GK18dhandasu:315": "pātrapraveśo yatraiṣa prayogātiśayo mataḥ // DhDaś_3.10 //",
  "GK18dhandasu:316": "udghātyakāvalagite prapañcatrigate chalam /",
  "GK18dhandasu:317": "vākkelyadhivale gaṇḍamavasyanditanālike /",
  "GK18dhandasu:318": "asatpralāpavyāhāramṛdavāni trayodaśa // DhDaś_3.11 //",
  "GK18dhandasu:319": "gūḍhārthapadaparyāyamālā praśnottarasya vā /",
  "GK18dhandasu:320": "yatrānyonyaṃ samālāpo dvedhodghātyaṃ taducyate // DhDaś_3.12 //",
  "GK18dhandasu:321": "yatraikatra samāveśāt kāryamanyat prasādhyate /",
  "GK18dhandasu:322": "prastute'nyatra vānyat syāt ta(?)",
  "GK18dhandasu:323": "cāvalagitaṃ dvidhā // DhDaś_3.13 //",
  "GK18dhandasu:324": "asadbhūtamithaḥstotraṃ prapañco hāsyakṛn mataḥ /",
  "GK18dhandasu:325": "śrutisāmyādanekārthayojanaṃ trigataṃ tviha /",
  "GK18dhandasu:326": "naṭāditritayālāpaḥ pūrvaraṅge tadiṣyate // DhDaś_3.14 //",
  "GK18dhandasu:327": "priyābhairapriyairvākyairvilobhya chalanā chalam /",
  "GK18dhandasu:328": "vinivṛttyāsya vākkelī dvistriḥ pratyuktito'pi vā // DhDaś_3.15 //",
  "GK18dhandasu:329": "anyonyavākyādhikyoktiḥ spardhayādhivalaṃ bhavet /",
  "GK18dhandasu:330": "gaṇḍaḥ prastutasambandhibhinnārthaṃ sahasoditam // DhDaś_3.16 //",
  "GK18dhandasu:331": "rasoktasyānyathā vyākhyā yatrāvasyandita hi tat /",
  "GK18dhandasu:332": "sopahāsā nigūḍhārthā nālikaiva prahelikā // DhDaś_3.17 //",
  "GK18dhandasu:333": "asambaddhakathāprāyo'satpralāpo yathottaraḥ /",
  "GK18dhandasu:334": "anyārthameva vyāhāro hāsyalāsyakaraṃ vacaḥ /",
  "GK18dhandasu:335": "doṣā guṇā guṇā doṣā yatra syurmṛdavaṃ hi tat // DhDaś_3.18 //",
  "GK18dhandasu:336": "eṣāmanyatamenārthaṃ pātraṃ cākṣipya sūtrabhṛt /",
  "GK18dhandasu:337": "prastāvanānte nirgacchet tato vastu prapañcayet // DhDaś_3.19 //",
  "GK18dhandasu:338": "abhigamyaguṇairyukto dhīrodāttaḥ pratāpavān /",
  "GK18dhandasu:339": "kīrtikāmo mahotsāhastrayyāstrātā mahīpatiḥ // DhDaś_3.20 //",
  "GK18dhandasu:340": "prakhyātavaṃśo rājarṣirdivyo vā yatra nāyakaḥ /",
  "GK18dhandasu:341": "tatprakhyātaṃ vidhātavyaṃ vṛttamatrādhikārikam // DhDaś_3.21 //",
  "GK18dhandasu:342": "yat tatrānucitaṃ kiñcin nāyakasya rasasya vā /",
  "GK18dhandasu:343": "viruddhaṃ tat parityājyamanyathā vā prakalpayet // DhDaś_3.22 //",
  "GK18dhandasu:344": "ādyantamevaṃ niścitya pañcadhā tad vibhajya ca /",
  "GK18dhandasu:345": "khaṇḍaśaḥ sandhisañjñāṃśca vibhāgānapi khaṇḍayet // DhDaś_3.23 //",
  "GK18dhandasu:346": "catuḥṣaṣṭistu tāni syuraṅgānītyaparaṃ tathā /",
  "GK18dhandasu:347": "patākāvṛttamapyūnamekādyairanusandhibhiḥ /",
  "GK18dhandasu:348": "aṅgānyatra yathālābhamasandhiṃ prakarīṃ nyaset // DhDaś_3.24 //",
  "GK18dhandasu:349": "ādau viṣkambhakaṃ kuryādaṅkaṃ vā kāryayuktitaḥ /",
  "GK18dhandasu:350": "apekṣitaṃ parityajya nīrasaṃ vastuvistaram /",
  "GK18dhandasu:351": "yadā sandarśayet śeṣaṃ kuryād viṣkambhakaṃ tadā // DhDaś_3.25 //",
  "GK18dhandasu:352": "yadā tu sarasaṃ vastu mūlādeva pravartate /",
  "GK18dhandasu:353": "ādāveva tadāṅkaḥ syādāmukhākṣepasaṃśrayaḥ // DhDaś_3.26 //",
  "GK18dhandasu:354": "pratyakṣanetṛcarito binduvyāptipuraskṛtaḥ /",
  "GK18dhandasu:355": "aṅko nānāprakārārthasaṃvidhānarasāśrayaḥ // DhDaś_3.27 //",
  "GK18dhandasu:356": "anubhāvavibhāvābhyāṃ sthāyinā vyabhicāribhiḥ /",
  "GK18dhandasu:357": "gṛhītamuktaiḥ kartavyamaṅginaḥ paripoṣaṇaṃ // DhDaś_3.28 //",
  "GK18dhandasu:358": "na cātirasato vastu dūraṃ vicchinnatāṃ nayet /",
  "GK18dhandasu:359": "rasaṃ vā na tirodadhyād vastvalaṅkāralakṣaṇaiḥ // DhDaś_3.29 //",
  "GK18dhandasu:360": "eko raso'ṅgīkartavyo vīraḥ śṛṅgāra eva vā /",
  "GK18dhandasu:361": "aṅgamanye rasāḥ sarve kuryān nirvahaṇe'dbhutam // DhDaś_3.30 //",
  "GK18dhandasu:362": "dūrādhvānaṃ vadhaṃ yuddhaṃ rājyadeśādiviplavam /",
  "GK18dhandasu:363": "saṃrodhaṃ bhojanaṃ snānaṃ surataṃ cānulepanam // DhDaś_3.31 //",
  "GK18dhandasu:364": "ambaragrahaṇādīni pratyakṣāṇi na nirdiśet /",
  "GK18dhandasu:365": "nādhikārivadhaṃ kvāpi tyājyamāvaśyakaṃ na ca // DhDaś_3.32 //",
  "GK18dhandasu:366": "ekāhācaritaikārthamitthamāsannanāyakam /",
  "GK18dhandasu:367": "pātraistricaturairaṅkaṃ teṣāmante'sya nirgamaḥ // DhDaś_3.33 //",
  "GK18dhandasu:368": "patākāsthānakānyatra bindurante ca bījavat /",
  "GK18dhandasu:369": "evamaṅkāḥ prakartavyāḥ praveśādipuraskṛtāḥ /",
  "GK18dhandasu:370": "pañcāṅkametadavaraṃ daśāṅkaṃ nāṭakaṃ param // DhDaś_3.34 //",
  "GK18dhandasu:371": "atha prakaraṇe vṛttamutpādyaṃ lokasaṃśrayam /",
  "GK18dhandasu:372": "amātyavipravaṇijāmekaṃ kuryāc ca nāyakam // DhDaś_3.35 //",
  "GK18dhandasu:373": "dhīrapraśāntaṃ sāpāyaṃ dharmakāmārthatatparam /",
  "GK18dhandasu:374": "śepaṃ nāṭakavat sandhipraveśakarasādikam // DhDaś_3.36 //",
  "GK18dhandasu:375": "nāyikā tu dvidhā netuḥ kulastrī gaṇikā tathā /",
  "GK18dhandasu:376": "kvacidekaiva kulajā kvāpi dvayaṃ kvacit // DhDaś_3.37 //",
  "GK18dhandasu:377": "kulajābhyantarā bāhyā veśyā nātikramo'nayoḥ /",
  "GK18dhandasu:378": "ābhiḥ prakaraṇaṃ tredhā saṅkīrṇaṃ dhūrtasaṅkulam // DhDaś_3.38 //",
  "GK18dhandasu:379": "lakṣyate nāṭikāpyatra saṅkīrṇānyanivṛttaye /",
  "GK18dhandasu:380": "tatra vastu prakaraṇān nāṭakān nāyako nṛpaḥ /",
  "GK18dhandasu:381": "prakhyāto dhīralalitaḥ śṛṅgāro'ṅgī salakṣaṇaḥ // DhDaś_3.39 //",
  "GK18dhandasu:382": "strīprāyacaturaṅkādibhedakaṃ yadi ceṣyate /",
  "GK18dhandasu:383": "ekadvitryaṅkapātrādibhedenānantarūpatā // DhDaś_3.40 //",
  "GK18dhandasu:384": "devī tatra bhavej jyeṣṭhā pragalbhā nṛpavaṃśajā /",
  "GK18dhandasu:385": "gambhīrā māninī kṛcchāt tadvaśān netṛsaṅgamaḥ // DhDaś_3.41 //",
  "GK18dhandasu:386": "nāyikā tādṛśī mugdhā divyā cātimanoharā /",
  "GK18dhandasu:387": "antaḥpurādisambandhādāsannā śrutidarśanaiḥ // DhDaś_3.42 //",
  "GK18dhandasu:388": "anurāgo navāvastho netustasyāṃ yathottaram /",
  "GK18dhandasu:389": "netā tatra pravarteta devītrāsena śaṅkitaḥ /",
  "GK18dhandasu:390": "kaiśikyaṅgaiścaturbhiśca yaktāṅkairiva nāṭikā // DhDaś_3.43 //",
  "GK18dhandasu:391": "bhāṇastu dhūrtacaritaṃ svānubhūtaṃ pareṇa vā /",
  "GK18dhandasu:392": "yatropavarṇayedeko nipuṇaḥ paṇḍito viṭaḥ // DhDaś_3.44 //",
  "GK18dhandasu:393": "sambodhanoktipratyuktī kuryādākāśabhāpitaiḥ /",
  "GK18dhandasu:394": "sūcayed vīraśṛṅgārau śauryasaubhāgyasaṃstavaiḥ // DhDaś_3.45 //",
  "GK18dhandasu:395": "bhūyasā bhāratī vṛttirekāṅkaṃ vastu kalpitam /",
  "GK18dhandasu:396": "mukhanirvahaṇe sāṅge lāsyāṅgāni daśāpi ca // DhDaś_3.46 //",
  "GK18dhandasu:397": "geyaṃ padaṃ sthitaṃ pādyamāsīnaṃ puṣpagaṇḍikā /",
  "GK18dhandasu:398": "pracchedakastrigūḍhaṃ ca saindhavākhyaṃ dvigūḍhakam // DhDaś_3.47 //",
  "GK18dhandasu:399": "uttamottamakaṃ caiva uktapratyuktameva ca /",
  "GK18dhandasu:400": "lāsye daśavidhaṃ hyetadaṅganirdeśakalpanam // DhDaś_3.48 //",
  "GK18dhandasu:401": "tadvat prahasanaṃ tredhā śuddhavaikṛtasaṅkaraiḥ /",
  "GK18dhandasu:402": "pākhaṇḍivipraprabhṛticeṭaceṭīviṭākulam /",
  "GK18dhandasu:403": "ceṣṭitaṃ vepabhāṣābhiḥ śuddhaṃ hāsyavaconvitam // DhDaś_3.49 //",
  "GK18dhandasu:404": "kāmukādivaciveṣaiḥ ṣaṇḍhakañcukitāpasaiḥ /",
  "GK18dhandasu:405": "vikṛtaṃ saṅkarād vīthyā saṅkīrṇaṃ dhūrtasaṅkulam /",
  "GK18dhandasu:406": "rasastu bhūyasā kāryaḥ ṣaḍvidho hāsya eva tu // DhDaś_3.50 //",
  "GK18dhandasu:407": "ḍime vastu prasiddhaṃ syād vṛttayaḥ kaiśikīṃ vinā /",
  "GK18dhandasu:408": "netāro devagandharvayakṣarakṣomahoragāḥ /",
  "GK18dhandasu:409": "bhūtapretapiśācādyāḥ ṣoḍaśātyantamuddhatāḥ // DhDaś_3.51 //",
  "GK18dhandasu:410": "rasairahāsyaśṛṅgāraiḥ ṣaḍbhirdīptaiḥ samanvitaḥ /",
  "GK18dhandasu:411": "māyendrajālasaṅgrāmakrodhodbhrāntādiceṣṭitaiḥ // DhDaś_3.52 //",
  "GK18dhandasu:412": "candrasūryoparāgaiśca nyāyye raudrarase'ṅgini /",
  "GK18dhandasu:413": "caturaṅkaścatuḥsandhirnirvimarśo ḍimaḥ smṛtaḥ // DhDaś_3.53 //",
  "GK18dhandasu:414": "khyātetivṛtto vyāyogaḥ khyātoddhatanarāśrayaḥ /",
  "GK18dhandasu:415": "hīno garbhavimarśābhyāṃ dīptāḥ syurḍimavad rasāḥ // DhDaś_3.54 //",
  "GK18dhandasu:416": "astrīnimittasaṅgrāmo jāmadagnyajaye yathā /",
  "GK18dhandasu:417": "ekāhācaritaikāṅko vyāyogo bahubhirnaraiḥ // DhDaś_3.55 //",
  "GK18dhandasu:418": "kāryaṃ samavakāre'pi āmukhaṃ nāṭakādivat /",
  "GK18dhandasu:419": "khyātaṃ devāsuraṃ vastu nirvimarśāstu sandhayaḥ // DhDaś_3.56 //",
  "GK18dhandasu:420": "vṛttayo mandakaiśikyo netāro devadānavāḥ /",
  "GK18dhandasu:421": "dvādaśodāttavikhyātāḥ phalaṃ teṣāṃ pṛthak pṛthak // DhDaś_3.57 //",
  "GK18dhandasu:422": "bahuvīrarasāḥ sarve yadvadambhodhimanthane /",
  "GK18dhandasu:423": "aṅkaiḥstribhistrikapaṭastriśṛṅgārastrividravaḥ // DhDaś_3.58 //",
  "GK18dhandasu:424": "dvisandhiraṅkaḥ prathamaḥ kāryo dvādaśanālikaḥ /",
  "GK18dhandasu:425": "caturdvinālikāvantyau nālikā ghaṭikādvayam // DhDaś_3.59 //",
  "GK18dhandasu:426": "vastusvabhāvadaivārikṛtāḥ syuḥ kapaṭāstrayaḥ /",
  "GK18dhandasu:427": "nagaroparodhayuddhe vātāgnyādikavidravāḥ // DhDaś_3.60 //",
  "GK18dhandasu:428": "dharmārthakāmaiḥ śṛṅgāro nātra vindupraveśakau /",
  "GK18dhandasu:429": "vīthyaṅgāni yathālābhaṃ kuryāt prahasane yathā // DhDaś_3.61 //",
  "GK18dhandasu:430": "vīthī tu kaiśikīvṛttau sandhyaṅgāṅkaistu bhāṇavat /",
  "GK18dhandasu:431": "rasaḥ sūcyastu śṛṅgāraḥ spṛśedapi rasāntaram // DhDaś_3.62 //",
  "GK18dhandasu:432": "yuktā prastāvanākhyātairaṅgairudghātyakādibhiḥ /",
  "GK18dhandasu:433": "evaṃ vīthī vidhātavyā dvyekapātraprayojitā // DhDaś_3.63 //",
  "GK18dhandasu:434": "utsṛṣṭikāṅke prakhyātaṃ vṛttaṃ buddhyā prapañcayet /",
  "GK18dhandasu:435": "rasastu karuṇaḥ sthāyī netāraḥ prākṛtā narāḥ // DhDaś_3.64 //",
  "GK18dhandasu:436": "bhāṇavat sandhivṛttyaṅgairyuktaḥ strīparidevitaiḥ /",
  "GK18dhandasu:437": "vācā yuddhaṃ vidhātavyaṃ tathā jayaparājayau // DhDaś_3.65 //",
  "GK18dhandasu:438": "miśramohāmṛge vṛttaṃ caturaṅkaṃ trisandhimat /",
  "GK18dhandasu:439": "naradivyāvaniyamān nāyakapratināyakau // DhDaś_3.66 //",
  "GK18dhandasu:440": "khyātau dhīroddhatāvantyo viparyāsādayuktakṛt /",
  "GK18dhandasu:441": "divyastriyamanicchantīmapahārādinecchataḥ // DhDaś_3.67 //",
  "GK18dhandasu:442": "śṛṅgārābhāsamapyasya kiñcit kiñcit pradarśayet /",
  "GK18dhandasu:443": "saṃrambhaṃ paramānīya yuddhaṃ vyājān nivārayet /",
  "GK18dhandasu:444": "vadhaprāptasya kurvīta vadhaṃ naiva mahātmanaḥ // DhDaś_3.68 //",
  "GK18dhandasu:445": "itthaṃ vicintya daśarūpakalakṣmamārgam",
  "GK18dhandasu:446": "ālokya vastu paribhāvya kaviprabandhān /",
  "GK18dhandasu:447": "kuryādayatnavadalaṅkṛtibhiḥ prabandhaṃ",
  "GK18dhandasu:448": "vākyairudāramadhuraiḥ sphuṭamandavṛttaiḥ // DhDaś_3.69 // ================================",
  "GK18dhandasu:450": "[pariccheda 4]",
  "GK18dhandasu:451": "vibhāvairanubhāvaiśca sāttvikairvyabhicāribhiḥ /",
  "GK18dhandasu:452": "ānīyamānaḥ svādyatvaṃ sthāyī bhāvo rasaḥ smṛtaḥ // DhDaś_4.1 //",
  "GK18dhandasu:453": "jñāyamānatayā tatra vibhāvo bhāvapoṣakṛt /",
  "GK18dhandasu:454": "ālambanoddīpanatvaprabhedena sa ca dvidhā // DhDaś_4.2 //",
  "GK18dhandasu:455": "anubhāvo vikārastu bhāvasaṃsūcanātmakaḥ /",
  "GK18dhandasu:456": "hetukāryātmanoḥ siddhistayoḥ saṃvyavahārataḥ // DhDaś_4.3 //",
  "GK18dhandasu:457": "sukhaduḥkhādikairbhāvairbhāvastadbhāvabhāvanam /",
  "GK18dhandasu:458": "pṛthag bhāvā bhavantyanye'nubhāvatve'pi sāttvikāḥ /",
  "GK18dhandasu:459": "sattvādeva samutpattestac ca tadbhāvabhāvanam // DhDaś_4.4 //",
  "GK18dhandasu:460": "stambhapralayaromāñcāḥ svedo vaivarṇyavepathū /",
  "GK18dhandasu:461": "aśruvaisvaryamityaṣṭau stambho'smin niṣkriyāṅgatā /",
  "GK18dhandasu:462": "pralayo naṣṭasañjñatvaṃ śeṣāḥ suvyaktalakṣaṇāḥ // DhDaś_4.5 //",
  "GK18dhandasu:463": "viśeṣādābhimukhyena caranto vyabhicāriṇaḥ /",
  "GK18dhandasu:464": "sthāyinyunmagnanirmagnāḥ kallolā iva vāridhau // DhDaś_4.6 //",
  "GK18dhandasu:465": "nirvedaglāniśaṅkāśramadhṛtijaḍatāharṣadainyaugryacintās",
  "GK18dhandasu:466": "trāserṣyāmarṣagarvāḥ smṛtimaraṇamadāḥ suptanidrāvibodhāḥ /",
  "GK18dhandasu:467": "vrīḍāpasmāramohāḥ samatiralasatāvegatarkāvahitthā",
  "GK18dhandasu:468": "vyādhyunmādau vipādotsukacapalayutāstriṃśadete trayaśca // DhDaś_4.7 //",
  "GK18dhandasu:469": "tattvajñānāpadīrṣyādernirvedaḥ svāvamānanam /",
  "GK18dhandasu:470": "tatra cintāśruniḥśvāsavaiavarṇyocchvāsadīnatāḥ // DhDaś_4.8 //",
  "GK18dhandasu:471": "ratyādyāyāsatṛṭkṣudbhirglānirniṣprāṇateha ca /",
  "GK18dhandasu:472": "vaivarṇyakampānutsāhakṣāmāṅgavacanakriyāḥ // DhDaś_4.9 //",
  "GK18dhandasu:473": "anarthapratibhā śaṅkā parakrauryāt svadurnayāt /",
  "GK18dhandasu:474": "kampaśoṣābhivīkṣādiratra varṇasvarānyatā // DhDaś_4.10 //",
  "GK18dhandasu:475": "śramaḥ khedo'dhvaratyādeḥ svedo'smin mardanādayaḥ /",
  "GK18dhandasu:476": "santoṣo jñānaśaktyāderdhṛtiravyagrabhogakṛt // DhDaś_4.11 //",
  "GK18dhandasu:477": "apratipattirjaḍatā syādiṣṭāniṣṭadarśanaśrutibhiḥ /",
  "GK18dhandasu:478": "animiṣanayananirīkṣaṇatūṣṇīmbhāvādayastatra // DhDaś_4.12 //",
  "GK18dhandasu:479": "prasattirutsavādibhyo harṣo'śrusvedagadgadāḥ /",
  "GK18dhandasu:480": "daurgatyādyairanaujasyaṃ dainyaṃ kārṣṇyābhṛjādimat // DhDaś_4.13 //",
  "GK18dhandasu:481": "duṣṭe'parādhadaurmukhyakrauryaiścaṇḍatvamugratā /",
  "GK18dhandasu:482": "tatra svedaśiraḥkampatarjanātāḍanādayaḥ // DhDaś_4.14 //",
  "GK18dhandasu:483": "dhyānaṃ cintehitānāpteḥ śūnyatāśvāsatāpakṛt /",
  "GK18dhandasu:484": "garjitādermanaḥkṣobhastrāso'trotkampitādayaḥ // DhDaś_4.15 //",
  "GK18dhandasu:485": "parotkarṣākṣamāsūyā garvadaurjanyamanyujā /",
  "GK18dhandasu:486": "doṣoktyavajñe srukuṭimanyukrodheṅgitāni ca // DhDaś_4.16 //",
  "GK18dhandasu:487": "adhikṣepāpamānāderamarṣo'bhiniviṣṭatā /",
  "GK18dhandasu:488": "tatra khedaśiraḥkampatarjanātāḍanādayaḥ // DhDaś_4.17 //",
  "GK18dhandasu:489": "garvo'bhijanalāvaṇyabalaiśvaryādibhirmadaḥ /",
  "GK18dhandasu:490": "karmāṇyādharpaṇāvajñā savilāsāṅgavīkṣaṇam // DhDaś_4.18 //",
  "GK18dhandasu:491": "sadṛśajñānacintādyaiḥ saṃskārāt smṛtiratra ca /",
  "GK18dhandasu:492": "jñātatvenārthabhāsinyāṃ bhrūsamunnayanādayaḥ /",
  "GK18dhandasu:493": "maraṇaṃ suprasiddhatvādanarthatvāc ca nocyate // DhDaś_4.19 //",
  "GK18dhandasu:494": "harṣotkarṣo madaḥ pānāt skhaladaṅgavacogatiḥ /",
  "GK18dhandasu:495": "nidrā hāso'tra ruditaṃ jyeṣṭhamadhyādhamādiṣu /",
  "GK18dhandasu:496": "suptaṃ nidrodbhavaṃ tatra śvāsocchāsakriyā param // DhDaś_4.20 //",
  "GK18dhandasu:497": "manaḥsammīlanaṃ nidrā cintālasyaklamādibhiḥ /",
  "GK18dhandasu:498": "tatra jṛmbhāṅgabhaṅgākṣimīlanotsvapnatādayaḥ /",
  "GK18dhandasu:499": "vivodhaḥ pariṇāmādestatra jṛmbhākṣimardane // DhDaś_4.21 //",
  "GK18dhandasu:500": "durācārādibhirvrīḍā dhārṣṭyābhāvastamunnayet /",
  "GK18dhandasu:501": "sācīkṛtāṅgāvaraṇavaivarṇyādhomukhādibhiḥ // DhDaś_4.22 //",
  "GK18dhandasu:502": "āveśo grahaduḥkhādyairapasmāro yathāvidhiḥ /",
  "GK18dhandasu:503": "bhūpātakampaprakhedalālāphenodgamādayaḥ // DhDaś_4.23 //",
  "GK18dhandasu:504": "moho vicittatā bhītiduḥkhāveśānucintanaiḥ /",
  "GK18dhandasu:505": "tatrājñānabhramāghātaghūrṇanādarśanādayaḥ // DhDaś_4.24 //",
  "GK18dhandasu:506": "bhrānticchedopadeśābhyāṃ śāstrādestattvadhīrmatiḥ /",
  "GK18dhandasu:507": "ālasyaṃ śramagarbhādejahyajṛmbhāsitādimat // DhDaś_4.25 //",
  "GK18dhandasu:508": "āvegaḥ samgramo'sminnabhisarajanite śastranāgābhiyogo",
  "GK18dhandasu:509": "vātāt pāṃsūpadigdhastvaritapadagatirvarṣaje piṇḍitāṅgaḥ /",
  "GK18dhandasu:510": "utpātāt srastatāṅgeṣvahitahitakṛte śokaharṣānubhāvā",
  "GK18dhandasu:511": "vahnerdhūmākulāsyaḥ karijamanubhayastammakampāpasārāḥ // DhDaś_4.26 //",
  "GK18dhandasu:512": "tarko vicāraḥ sandehād bhrūśiroṅgulinartakaḥ /",
  "GK18dhandasu:513": "lajjādyairvikriyāguptāvavahitthāṅgavikriyā /",
  "GK18dhandasu:514": "vyādhayaḥ sannipātādyāsteṣāmanyatra vistaraḥ // DhDaś_4.27 //",
  "GK18dhandasu:515": "aprekṣākāritonmādaḥ sannipātagrahādibhiḥ /",
  "GK18dhandasu:516": "assminnavasthā ruditagītahāsāsitādayaḥ // DhDaś_4.28 //",
  "GK18dhandasu:517": "prārabdhakāryāsiddhyāderviṣādaḥ sattvasaṅkṣayaḥ /",
  "GK18dhandasu:518": "niḥśvāsocchvāsahṛttāpasahāyānveṣaṇādikṛt // DhDaś_4.29 //",
  "GK18dhandasu:519": "kālākṣamatvamautmukyaṃ ramyecchāratisamgramaiḥ /",
  "GK18dhandasu:520": "tatrocchvāsatvaniḥśvāsahṛttāpasvedavibhramāḥ // DhDaś_4.30 //",
  "GK18dhandasu:521": "mātsaryadveṣarāgādeścāpalaṃ tvanavasthitiḥ /",
  "GK18dhandasu:522": "tatra bhartsanapāruṣyasvacchandācaraṇādayaḥ // DhDaś_4.31 //",
  "GK18dhandasu:523": "viruddhairaviruddhairvā bhāvairvicchidyate /",
  "GK18dhandasu:524": "ātmabhāvaṃ nayatyanyān sa sthāyī lavaṇākaraḥ // DhDaś_4.32 //",
  "GK18dhandasu:525": "ratyutsāhajgupsāḥ krodho hāsaḥ smayo bhayaṃ śokaḥ /",
  "GK18dhandasu:526": "śamamapi kecit prāhuḥ puṣṭirnāṭyeṣu naitasya // DhDaś_4.33 //",
  "GK18dhandasu:527": "nirvedādiratādrūpyādasthāyī svadate katham /",
  "GK18dhandasu:528": "vairasyāyaiva tatpoṣastenāṣṭau sthāyino matāḥ // DhDaś_4.34 //",
  "GK18dhandasu:529": "vācyā prakaraṇādibhyo buddhisthā vā yathā kriyā /",
  "GK18dhandasu:530": "vākyārthaḥ kārakairyuktā sthāyī bhāvastathetaraiḥ // DhDaś_4.35 //",
  "GK18dhandasu:531": "rasaḥ sa eva svādvyatvād rasikasyaiva vartanāt /",
  "GK18dhandasu:532": "nānukāryasya vṛttatvāt kāvyasyātatparatvataḥ // DhDaś_4.36 //",
  "GK18dhandasu:533": "draṣṭuḥ pratītirvrīḍerṣyārāgadveṣaprasaṅgataḥ /",
  "GK18dhandasu:534": "laukikasya svaramaṇīsaṃyuktasyeva darśanāt // DhDaś_4.37 //",
  "GK18dhandasu:535": "dhīrodāttādyavasthānāṃ rāmādiḥ pratipādakaḥ /",
  "GK18dhandasu:536": "vibhāvayati ratyādīn svadante rasikasya te // DhDaś_4.38 //",
  "GK18dhandasu:537": "krīḍatāṃ mṛnmayairyadvad bālānāṃ dviradādibhiḥ /",
  "GK18dhandasu:538": "svotsāhaḥ svadate tadvacchrotṛṇāmarjunādibhiḥ // DhDaś_4.39 //",
  "GK18dhandasu:539": "kāvyārthabhāvanāsvādo nartakasya na vāryate /",
  "GK18dhandasu:540": "svādaḥ kāvyārthasambhedānātmānandasamudbhavaḥ // DhDaś_4.40 //",
  "GK18dhandasu:541": "vikāśavistarakṣobhavikṣepaiḥ sa caturbidhaḥ /",
  "GK18dhandasu:542": "śṛṅgāravīrabībhatsaraudreṣu manasaḥ kramāt // DhDaś_4.41 //",
  "GK18dhandasu:543": "hāsyādbhutabhayotkarṣakaruṇānāṃ ta eva hi /",
  "GK18dhandasu:544": "atastajjanyatā teṣāmata evāvadhāraṇam // DhDaś_4.42 //",
  "GK18dhandasu:545": "śamaprakarṣo nirvācyo muditādestadātmatā /",
  "GK18dhandasu:546": "padārthairindunirvedaromāñcādisvarūpakaiḥ /",
  "GK18dhandasu:547": "kāvyād vibhāvasañcāryanubhāvaprakhyatāṃ gataiḥ // DhDaś_4.43 //",
  "GK18dhandasu:548": "bhāvitaḥ svadate sthāyī rasaḥ sa parikīrtitaḥ /",
  "GK18dhandasu:549": "lakṣaṇaikyaṃ vibhāvaikyādabhedād rasabhāvayoḥ // DhDaś_4.44 //",
  "GK18dhandasu:550": "ramyadeśakalākālaveṣabhogādisevanaiḥ /",
  "GK18dhandasu:551": "pramodātmā ratiḥ saiva yūnoranyonyaraktayoḥ /",
  "GK18dhandasu:552": "prahṛṣyamāṇā śṛṅgāro madhurāṅgaviceṣṭitaiḥ // DhDaś_4.45 //",
  "GK18dhandasu:553": "ye sattvajāḥ sthāyin eva cāṣṭau",
  "GK18dhandasu:554": "triṃśat trayo ye vyabhicāriṇaśca /",
  "GK18dhandasu:555": "ekonapañcāśadamī hi bhāvāḥ",
  "GK18dhandasu:556": "yuktyā nibaddhāḥ paripoṣayanti /",
  "GK18dhandasu:557": "ālasyamaugryaṃ maraṇaṃ jugupsā",
  "GK18dhandasu:558": "tasyāśrayādvaitaviruddhamiṣṭam // DhDaś_4.46 //",
  "GK18dhandasu:559": "tatrāyogo'nurāge'pi navayorekacittayoḥ /",
  "GK18dhandasu:560": "pāratantryeṇa daivād vā viprakarṣādasaṅgamaḥ // DhDaś_4.47 //",
  "GK18dhandasu:561": "daśāvasthaḥ sa tatrādāvabhilāṣo'tha cintanam /",
  "GK18dhandasu:562": "smṛtirguṇakathodvegapralāponmādasañjvarāḥ /",
  "GK18dhandasu:563": "jaḍatā maraṇaṃ ceti duravasthaṃ yathottaram // DhDaś_4.48 //",
  "GK18dhandasu:564": "abhilāpaḥ spṛhā tatra kānte sarvāṅgasundare /",
  "GK18dhandasu:565": "dṛṣṭe śrute vā tatrāpi vismayānandasādhvasāḥ // DhDaś_4.49 //",
  "GK18dhandasu:566": "sākṣāt pratikṛtisvapnacchāyāmāyāsu darśanam /",
  "GK18dhandasu:567": "śrutirvyājāt sakhīgītamāgadhādiguṇastuteḥ // DhDaś_4.50 //",
  "GK18dhandasu:568": "sānubhāvavibhāvāstu cintādyāḥ pūrvadarśitāḥ /",
  "GK18dhandasu:569": "daśāvasthatvamācāryaiḥ prāyo vṛtyā nidarśitam /",
  "GK18dhandasu:570": "mahākaviprabandheṣu dṛśyate tadanantatā // DhDaś_4.51 //",
  "GK18dhandasu:571": "dṛṣṭe śrute'bhilāṣāc ca kiṃ nautsukyaṃ prajāyate /",
  "GK18dhandasu:572": "aprāptau kiṃ na nirvedo glāniḥ kiṃ nāticintanāt // DhDaś_4.52 //",
  "GK18dhandasu:573": "viprayogastu viśleṣo rūḍhavibhrambhayordvidhā /",
  "GK18dhandasu:574": "mānapravāsabhedena māno'pi praṇayerṣyayoḥ // DhDaś_4.53 //",
  "GK18dhandasu:575": "tatra praṇayamānaḥ syāt kopāvasitayordvayoḥ /",
  "GK18dhandasu:576": "strīṇāmīrṣyākṛto mānaḥ kopo'nyāsaṅgini priye /",
  "GK18dhandasu:577": "śrute vānumite dṛṣṭe śrutistatra sakhīmukhāt // DhDaś_4.54 //",
  "GK18dhandasu:578": "utsvapnāyitabhogāṅkagotraskhalanakalpitaḥ /",
  "GK18dhandasu:579": "tridhānumāniko dṛṣṭaḥ sākṣādindriyagocaraḥ // DhDaś_4.55 //",
  "GK18dhandasu:580": "yathottaraṃ guruḥ ṣaḍbhirupāyaistamupācaret /",
  "GK18dhandasu:581": "sāmnā bhedena dānena natyupekṣārasāntaraiḥ // DhDaś_4.56 //",
  "GK18dhandasu:582": "tatra oriyavacaḥ sāma bhedastatsakhyupārjanam /",
  "GK18dhandasu:583": "dānaṃ vyājena bhūṣādeḥ pādayoḥ patanaṃ natiḥ // DhDaś_4.57 //",
  "GK18dhandasu:584": "sāmādau tu parikṣīṇe syādupekṣāvadhīraṇam /",
  "GK18dhandasu:585": "rabhasatrāsaharṣādeḥ kopabhraṃśo rasāntaram /",
  "GK18dhandasu:586": "kopaceṣṭāśca nārīṇāṃ prāgeva pratipāditāḥ // DhDaś_4.58 //",
  "GK18dhandasu:587": "kāryataḥ sambhramācchāpāt pravāso bhinnadeśatā /",
  "GK18dhandasu:588": "dvayostatrāśruniḥśvāsakārśyalambālakāditā // DhDaś_4.59 //",
  "GK18dhandasu:589": "dvitīyaḥ sahasotpanno divyamānupaviplavāt /",
  "GK18dhandasu:590": "svarūpānyatvakaraṇācchāpajaḥ sannidhāvapi // DhDaś_4.60 //",
  "GK18dhandasu:591": "mṛte tvekatra yatrānyaḥ pralapecchoka eva saḥ /",
  "GK18dhandasu:592": "vyāśrayatvān na śṛṅgāraḥ pratyāpanne tu netaraḥ // DhDaś_4.61 //",
  "GK18dhandasu:593": "praṇayāyogayorutkā pravāse proṣitapriyā /",
  "GK18dhandasu:594": "kalahāntariterṣyāyāṃ vipralavdhā ca khaṇḍitā // DhDaś_4.62 //",
  "GK18dhandasu:595": "anukūlau niṣevete yatrānyonyaṃ vilāsinau /",
  "GK18dhandasu:596": "darśanasparśanādīni sa sambhogo mudānvitaḥ // DhDaś_4.63 //",
  "GK18dhandasu:597": "ceṣṭāstatra pravartante līlādyā daśa yoṣitām /",
  "GK18dhandasu:598": "dākṣiṇyamārdavapremṇāmanurūpāḥ priyaṃ prati // DhDaś_4.64 //",
  "GK18dhandasu:599": "ramayec cāṭukṛt kāntaḥ kalākrīḍādibhiśca tām /",
  "GK18dhandasu:600": "na grāmyamācaret kiñcin narmabhraṃśakaraṃ na ca // DhDaś_4.65 //",
  "GK18dhandasu:601": "vīraḥ pratāpavinayādhyavasāyasattva-",
  "GK18dhandasu:602": "mohāviṣādanayavismayavikramādyaiḥ /",
  "GK18dhandasu:603": "utsāhabhūḥ sa ca dayāraṇadānayogāt",
  "GK18dhandasu:604": "tredhā kilātra matigarvadhṛtipraharṣāḥ // DhDaś_4.66 //",
  "GK18dhandasu:605": "bībhatsaḥ kṛmipūtigandhivamathuprāyairjugupsaikabhūr",
  "GK18dhandasu:606": "udvego rudhirāntrakīkasavasāmāṃsādibhiḥ kṣobhaṇaḥ /",
  "GK18dhandasu:607": "vairāgyāj jaghanastanādiṣu ghṛṇāśuddho'nubhāvairvṛto",
  "GK18dhandasu:608": "nāsāvaktravikūṇanādibhirihāvegārtiśaṅkādayaḥ // DhDaś_4.67 //",
  "GK18dhandasu:609": "krodho matsaravairivaikṛtamayaiḥ poṣo'sya raudro'nujaḥ",
  "GK18dhandasu:610": "kṣobhaḥ khādharadaṃśakampabhrukuṭisvedāsyarāgairyutaḥ /",
  "GK18dhandasu:611": "śatrollāsavikatthamāṃsadharaṇīghātapratijñāgrahair",
  "GK18dhandasu:612": "atrāmarṣamadau smṛtiścapalatāsūyāgryavegādayaḥ // DhDaś_4.68 //",
  "GK18dhandasu:613": "vikṛtākṛtivāgveṣairātmano'tha parasya vā /",
  "GK18dhandasu:614": "hāsaḥ syāt paripoṣo'sya hāsyastriprakṛtiḥ smṛtaḥ // DhDaś_4.69 //",
  "GK18dhandasu:615": "smitamiha vikāsinayanaṃ kiñcillakṣyadvijaṃ tu hasitaṃ syāt /",
  "GK18dhandasu:616": "madhurasvaraṃ vihasitaṃ saśiraḥkampamidamupahasitam // DhDaś_4.70 //",
  "GK18dhandasu:617": "apahasitaṃ sāsrākṣaṃ vikṣiptāṅgaṃ bhavatyatihasitam /",
  "GK18dhandasu:618": "dve dve hasite caiṣāṃ jyeṣṭhe madhye'dhame kramaśaḥ // DhDaś_4.71 //",
  "GK18dhandasu:619": "nidrālasyaśramaglānimūrchāśca sahacāriṇaḥ /",
  "GK18dhandasu:620": "atilokaiḥ padārthaiḥ syād vismayātmā raso'dbhutaḥ // DhDaś_4.72 //",
  "GK18dhandasu:621": "karmāsya sādhuvādāśruvepathusvedagadgadāḥ /",
  "GK18dhandasu:622": "harṣāvegadhṛtiprāyā bhavanti vyabhicāriṇaḥ // DhDaś_4.73 //",
  "GK18dhandasu:623": "vikṛtasvarasattvāderbhayabhāvo bhayānakaḥ /",
  "GK18dhandasu:624": "sarvāṅgavepathusvedaśoṣavaicittyalakṣaṇaḥ /",
  "GK18dhandasu:625": "dainyasambhramasammohatrāsādistatsahodaraḥ // DhDaś_4.74 //",
  "GK18dhandasu:626": "iṣṭanāśādaniṣṭāptau śokātmā karuṇo'nu tam /",
  "GK18dhandasu:627": "niḥśvāsocchvāsaruditastambhapralapitādayaḥ // DhDaś_4.75 //",
  "GK18dhandasu:628": "svāpāpasmāradainyādhimaraṇālasyasambhramāḥ /",
  "GK18dhandasu:629": "viṣādajaḍayonmādacintādyā vyabhicāriṇaḥ // DhDaś_4.76 //",
  "GK18dhandasu:630": "prītibhaktyādayo bhāvā mṛgayākṣādayo rasāḥ /",
  "GK18dhandasu:631": "harṣotsāhādiṣu spaṣṭamantarbhāvān na kīrtitāḥ // DhDaś_4.77 //",
  "GK18dhandasu:632": "ṣaṭtriṃśadbhūṣaṇādīni sāmādīnyekaviṃśatiḥ /",
  "GK18dhandasu:633": "lakṣyasandhyantarāṅgāni sālaṅkāreṣu teṣu ca // DhDaś_4.78 //",
  "GK18dhandasu:634": "ramyaṃ jugupsitamudāramathāpi nīcam",
  "GK18dhandasu:635": "ugraṃ prasādi gahanaṃ vikṛtaṃ ca vastu /",
  "GK18dhandasu:636": "yadvāpyavastu kavibhāvakabhāvyamānaṃ",
  "GK18dhandasu:637": "tan nāsti yan na rasabhāvamupaiti loke // DhDaś_4.79 //",
  "GK18dhandasu:638": "viṣṇoḥ sutenāpi dhanañjayena",
  "GK18dhandasu:639": "vidvanmanorāganibandhahetuḥ /",
  "GK18dhandasu:640": "āviṣkṛṭaṃ muñjamahīśagoṣṭhī-",
  "GK18dhandasu:641": "vaidagdhyabhājā daśarūpametat // DhDaś_4.80 //"
}