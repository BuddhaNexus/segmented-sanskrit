{
  "GS25amark1pu:0": "amarakośa evaṃ nāmaliṅgānuśāsanaṃ khāṇḍa 1",
  "GS25amark1pu:1": "nāmaliṅgānuśāsanaṃ nāma amarakoṣaḥ",
  "GS25amark1pu:2": "prathamaṃ kāṇḍam|",
  "GS25amark1pu:3": "maṅgalācaraṇam|",
  "GS25amark1pu:4": "(1.0.1) yasya jñānadayāsindhoragādhasyānaghā guṇāḥ",
  "GS25amark1pu:5": "(1.0.2) sevyatāmakṣayo dhīrāḥ sa śriye cāmṛtāya ca",
  "GS25amark1pu:6": "prastāvanā",
  "GS25amark1pu:7": "(1.0.3) samāhṛtyānyatantrāṇi saṃkṣiptaiḥ pratisaṃskṛtaiḥ",
  "GS25amark1pu:8": "(1.0.4) saṃpūrṇamucyate vargairnāmaliṅgānuśāsanam",
  "GS25amark1pu:9": "paribhāṣā",
  "GS25amark1pu:10": "(1.0.5) prāyaśo rūpabhedena sāhacaryācca kutracit",
  "GS25amark1pu:11": "(1.0.6) strīpuṃnapuṃsakaṃ jñeyaṃ tadviśeṣavidheḥ kvacit",
  "GS25amark1pu:12": "(1.0.7) bhedākhyānāya na dvandvo naikaśeṣo na saṃkaraḥ",
  "GS25amark1pu:13": "(1.0.8) kṛto 'tra bhinnaliṅgānāmanuktānāṃ kramādṛte",
  "GS25amark1pu:14": "(1.0.9) triliṅgyāṃ triṣviti padaṃ mithune tu dvayoriti",
  "GS25amark1pu:15": "(1.0.10) niṣiddhaliṅgaṃ śeṣārthaṃ tvantāthādi na pūrvabhāk",
  "GS25amark1pu:16": "svargavargaḥ",
  "GS25amark1pu:17": "(1.1.11) svaravyayaṃ svarganākatridivatridaśālayāḥ",
  "GS25amark1pu:18": "(1.1.12) suraloko dyodivau dve striyāṃ klībe triviṣṭapam",
  "GS25amark1pu:19": "(1.1.13) amarā nirjarā devāstridaśā vibudhāḥ surāḥ",
  "GS25amark1pu:20": "(1.1.14) suparvāṇaḥ sumanasastridiveśā divaukasaḥ",
  "GS25amark1pu:21": "(1.1.15) āditeyā diviṣado lekhā aditinandanāḥ",
  "GS25amark1pu:22": "(1.1.16) ādityā ṛbhavo 'svapnā amartyā amṛtāndhasaḥ",
  "GS25amark1pu:23": "(1.1.17) barhirmukhāḥ ṛtubhujo gīrvāṇā dānavārayaḥ",
  "GS25amark1pu:24": "(1.1.18) vṛndārakā daivatāni puṃsi vā devatāḥ striyām",
  "GS25amark1pu:25": "(1.1.19) ādityaviśvavasavastuṣitābhāsvarānilāḥ",
  "GS25amark1pu:26": "(1.1.20) mahārājikasādhyāśca rudrāśca gaṇadevatāḥ",
  "GS25amark1pu:27": "(1.1.21) vidyādharāpsaroyakṣarakṣogandharvakiṃnarāḥ",
  "GS25amark1pu:28": "(1.1.22) piśāco guhyakaḥ siddho bhūto 'mī devayonayaḥ",
  "GS25amark1pu:29": "(1.1.23) asurā daityadaiteyadanujendrāridānavāḥ",
  "GS25amark1pu:30": "(1.1.24) śukraśiṣyā ditisutāḥ pūrvadevāḥ suradviṣaḥ",
  "GS25amark1pu:31": "(1.1.25) sarvajñaḥ sugataḥ buddho dharmarājastathāgataḥ",
  "GS25amark1pu:32": "(1.1.26) samantabhadro bhagavānmārajillokajijjinaḥ",
  "GS25amark1pu:33": "(1.1.27) ṣaḍabhijño daśabalo 'dvayavādī vināyakaḥ",
  "GS25amark1pu:34": "(1.1.28) munīndraḥ śrīghanaḥ śāstā muniḥ śākyamunistu yaḥ",
  "GS25amark1pu:35": "(1.1.29) sa śākyasiṃhaḥ sarvārthasiddhaḥ śauddhodaniśca saḥ",
  "GS25amark1pu:36": "(1.1.30) gautamaścārkabandhuśca māyādevīsutaśca saḥ",
  "GS25amark1pu:37": "(1.1.31) brahmātmabhūḥ surajyeṣṭhaḥ parameṣṭhī pitāmahaḥ",
  "GS25amark1pu:38": "(1.1.32) hiraṇyagarbho lokeśaḥ svayaṃbhūścaturānanaḥ",
  "GS25amark1pu:39": "(1.1.33) dhātābjayonirdruhiṇo viriñciḥ kamalāsanaḥ",
  "GS25amark1pu:40": "(1.1.34) sraṣṭā prajāpatirvedhā vidhātā viśvasṛgvidhiḥ",
  "GS25amark1pu:41": "(1.1.35) nābhijanmāṇḍajaḥ pūrvo nidhanaḥ kamalodbhavaḥ",
  "GS25amark1pu:42": "(1.1.36) sadānando rajomūrtiḥ satyako haṃsavāhanaḥ",
  "GS25amark1pu:43": "(1.1.37) viṣṇurnārāyaṇaḥ kṛṣṇo vaikuṇṭho viṣṭaraśravāḥ",
  "GS25amark1pu:44": "(1.1.38) dāmodaro hṛṣīkeśaḥ keśavo mādhavaḥ svabhūḥ",
  "GS25amark1pu:45": "(1.1.39) daityāriḥ puṇḍarīkākṣo govindo garuḍadhvajaḥ",
  "GS25amark1pu:46": "(1.1.40) pītāmbaro 'cyutaḥ śārṅgī viṣvakseno janārdanaḥ",
  "GS25amark1pu:47": "(1.1.41) upendra indrāvarajaścakrapāṇiścaturbhujaḥ",
  "GS25amark1pu:48": "(1.1.42) padmanābho madhuripurvāsudevastrivikramaḥ",
  "GS25amark1pu:49": "(1.1.43) devakīnandanaḥ śauriḥ śrīpatiḥ puruṣottamaḥ",
  "GS25amark1pu:50": "(1.1.44) vanamālī balidhvaṃsī kaṃsārātiradhokṣajaḥ",
  "GS25amark1pu:51": "(1.1.45) viśvambharaḥ kaiṭabhajidvidhuḥ śrīvatsalāñchanaḥ",
  "GS25amark1pu:52": "(1.1.46) purāṇapuruṣo yajñapuruṣo narakāntakaḥ",
  "GS25amark1pu:53": "(1.1.47) jalaśāyī viśvarūpo mukundo muramardanaḥ",
  "GS25amark1pu:54": "(1.1.48) vasudevo 'sya janakaḥ sa evānakadundubhiḥ",
  "GS25amark1pu:55": "(1.1.49) balabhadraḥ pralambaghno baladevo 'cyutāgrajaḥ",
  "GS25amark1pu:56": "(1.1.50) revatīramaṇo rāmaḥ kāmapālo halāyudhaḥ",
  "GS25amark1pu:57": "(1.1.51) nīlāmbaro rauhiṇeyastālāṅko musalī halī",
  "GS25amark1pu:58": "(1.1.52) saṃkarṣaṇaḥ sīrapāṇiḥ kālindībhedano balaḥ",
  "GS25amark1pu:59": "(1.1.53) madano manmatho māraḥ pradyumno mīnaketanaḥ",
  "GS25amark1pu:60": "(1.1.54) kandarpo darpako 'naṅgaḥ kāmaḥ pañcaśaraḥ smaraḥ",
  "GS25amark1pu:61": "(1.1.55) śambarārirmanasijaḥ kusumeṣurananyajaḥ",
  "GS25amark1pu:62": "(1.1.56) puṣpadhanvā ratipatirmakaradhvaja ātmabhūḥ",
  "GS25amark1pu:63": "(1.1.57) aravindamaśokaṃ ca cūtaṃ ca navamallikā",
  "GS25amark1pu:64": "(1.1.58) nīlotpalaṃ ca pañcaite pañcabāṇasya sāyakāḥ",
  "GS25amark1pu:65": "(1.1.59) unmādanastāpanaśca śoṣaṇaḥ stambhanastathā",
  "GS25amark1pu:66": "(1.1.60) saṃmohanaśca kāmaśca pañca bāṇāḥ prakīrtitāḥ",
  "GS25amark1pu:67": "(1.1.61) brahmasūrviśvaketuḥ syādaniruddha uṣāpatiḥ",
  "GS25amark1pu:68": "(1.1.62) lakṣmīḥ padmālayā padmā kamalā śrīrharipriyā",
  "GS25amark1pu:69": "(1.1.63) indirā lokamātā mā kṣīrodatanayā ramā",
  "GS25amark1pu:70": "(1.1.64) bhārgavī lokajananī kṣīrasāgarakanyakā",
  "GS25amark1pu:71": "(1.1.65) śaṅkho lakṣmīpateḥ pāñcajanyaścakraṃ sudarśanaḥ",
  "GS25amark1pu:72": "(1.1.66) kaumodakī gadā khaḍgo nandakaḥ kaustubho maṇiḥ",
  "GS25amark1pu:73": "(1.1.67) cāpaḥ śārṅgaṃ murārestu śrīvatso lāñchanaṃ smṛtam",
  "GS25amark1pu:74": "(1.1.68) aśvāśca śaivyasugrīvameghapuṣpabalāhakāḥ",
  "GS25amark1pu:75": "(1.1.69) sārathirdāruko mantrī hyuddhavaścānujo gadaḥ",
  "GS25amark1pu:76": "(1.1.70) garutmāngaruḍastārkṣyo vainateyaḥ khageśvaraḥ",
  "GS25amark1pu:77": "(1.1.71) nāgāntako viṣṇurathaḥ suparṇaḥ pannagāśanaḥ",
  "GS25amark1pu:78": "(1.1.72) śambhurīśaḥ paśupatiḥ śivaḥ śūlī maheśvaraḥ",
  "GS25amark1pu:79": "(1.1.73) īśvaraḥ śarva īśānaḥ śaṃkaraścandraśekharaḥ",
  "GS25amark1pu:80": "(1.1.74) bhūteśaḥ khaṇḍaparaśurgirīśo giriśo mṛḍaḥ",
  "GS25amark1pu:81": "(1.1.75) mṛtyuñjayaḥ kṛttivāsāḥ pinākī pramathādhipaḥ",
  "GS25amark1pu:82": "(1.1.76) ugraḥ kapardī śrīkaṇṭhaḥ śitikaṇṭhaḥ kapālabhṛt",
  "GS25amark1pu:83": "(1.1.77) vāmadevo mahādevo virūpākṣastrilocanaḥ",
  "GS25amark1pu:84": "(1.1.78) kṛśānuretāḥ sarvajño dhūrjaṭirnīlalohitaḥ",
  "GS25amark1pu:85": "(1.1.79) haraḥ smaraharo bhargastryambakastripurāntakaḥ",
  "GS25amark1pu:86": "(1.1.80) gaṅgādharo 'ndhakaripuḥ kratudhvaṃsī vṛṣadhvajaḥ",
  "GS25amark1pu:87": "(1.1.81) vyomakeśo bhavo bhīmaḥ sthāṇū rudra umāpatiḥ",
  "GS25amark1pu:88": "(1.1.82) ahirbudhnyo 'ṣṭamūrtiśca gajāriśca mahānaṭaḥ",
  "GS25amark1pu:89": "(1.1.83) kapardo 'sya jaṭājūṭaḥ pināko 'jagavaṃ dhanuḥ",
  "GS25amark1pu:90": "(1.1.84) pramathā: syuḥ pāriṣadā brāhmītyādyāstu mātaraḥ",
  "GS25amark1pu:91": "(1.1.85) vibhūtirbhūtiraiśvaryamaṇimādikamaṣṭadhā",
  "GS25amark1pu:92": "(1.1.86) aṇimā mahimā caiva garimā laghimā tathā",
  "GS25amark1pu:93": "(1.1.87) prāptiḥ prākāmyamīśitvaṃ vaśitvaṃ cāṣṭa siddhayaḥ",
  "GS25amark1pu:94": "(1.1.88) umā kātyāyanī gaurī kālī haimavatīśvarī",
  "GS25amark1pu:95": "(1.1.89) śivā bhavānī rudrāṇī śarvāṇī sarvamaṅgalā",
  "GS25amark1pu:96": "(1.1.90) aparṇā pārvatī durgā mṛḍānī caṇḍikāmbikā",
  "GS25amark1pu:97": "(1.1.91) āryā dākṣāyaṇī caiva girijā menakātmajā",
  "GS25amark1pu:98": "(1.1.92) karmamoṭī tu cāmuṇḍā carmamuṇḍā tu carcikā",
  "GS25amark1pu:99": "(1.1.93) vināyako vighnarājadvaimāturagaṇādhipāḥ",
  "GS25amark1pu:100": "(1.1.94) apyekadantaherambalambodaragajānanāḥ",
  "GS25amark1pu:101": "(1.1.95) kārtikeyo mahāsenaḥ śarajanmā ṣaḍānanaḥ",
  "GS25amark1pu:102": "(1.1.96) pārvatīnandanaḥ skandaḥ senānīragnibhūrguhaḥ",
  "GS25amark1pu:103": "(1.1.97) bāhuleyastārakajidviśākhaḥ śikhivāhanaḥ",
  "GS25amark1pu:104": "(1.1.98) ṣāṇmāturaḥ śaktidharaḥ kumāraḥ krauñcadāraṇaḥ",
  "GS25amark1pu:105": "(1.1.99) śṛṅgī bhṛṅgī riṭistuṇḍī nandiko nandikeśvaraḥ",
  "GS25amark1pu:106": "(1.1.100) indro marutvānmaghavā biḍaujāḥ pākaśāsanaḥ",
  "GS25amark1pu:107": "(1.1.101) vR^īddhaśravāḥ sunāsīraḥ puruhūtaḥ purandaraḥ",
  "GS25amark1pu:108": "(1.1.102) jiṣṇurlekharṣabhaḥ śakraḥ śatamanyurdivaspatiḥ",
  "GS25amark1pu:109": "(1.1.103) sutrāmā gotrabhidvajrī vāsavo vṛtrahā vṛṣā",
  "GS25amark1pu:110": "(1.1.104) bāstoṣpatiḥ surapatirbalārātiḥ śacīpatiḥ",
  "GS25amark1pu:111": "(1.1.105) jambhabhedī harihayaḥ svārāṇnamucisūdanaḥ",
  "GS25amark1pu:112": "(1.1.106) saṃkrandano duścyavanasturāṣāṇmeghavāhanaḥ",
  "GS25amark1pu:113": "(1.1.107) ākhaṇḍalaḥ sahasrākṣa ṛbhukṣāstasya tu priyā",
  "GS25amark1pu:114": "(1.1.108) pulomajā śacīndrāṇī nagarī tvamarāvatī",
  "GS25amark1pu:115": "(1.1.109) haya uccaiḥśravā sūto mātalirnandanaṃ vanam",
  "GS25amark1pu:116": "(1.1.110) syātprāsādo vaijayanto jayantaḥ pākaśāsaniḥ",
  "GS25amark1pu:117": "(1.1.111) airāvato 'bhramātaṅgairāvaṇābhramuvallabhāḥ",
  "GS25amark1pu:118": "(1.1.112) hrādinī vajramastrī syāt kuliśaṃ bhiduraṃ paviḥ",
  "GS25amark1pu:119": "(1.1.113) śatakoṭiḥ svaruḥ śambo dambholiraśanirdvayoḥ",
  "GS25amark1pu:120": "(1.1.114) vyomayānaṃ vimāno 'strī nāradādyāḥ surarṣayaḥ",
  "GS25amark1pu:121": "(1.1.115) syāt sudharmā devasabhā pīyūṣamamṛtaṃ sudhā",
  "GS25amark1pu:122": "(1.1.116) mandākinī viyadgaṅgā svarṇadī suradīrghikā",
  "GS25amark1pu:123": "(1.1.117) meruḥ sumerurhemādrī ratnasānuḥ surālayaḥ",
  "GS25amark1pu:124": "(1.1.118) pañcaite devataravo mandāraḥ pārijātakaḥ",
  "GS25amark1pu:125": "(1.1.119) santānaḥ kalpavṛkṣaśca puṃsi vā haricandanam",
  "GS25amark1pu:126": "(1.1.120) sanatkumāro vaidhātraḥ svarvaidyāvaśvinīsutau",
  "GS25amark1pu:127": "(1.1.121) nāsatyāvaśvinau dasrāvāśvineyau ca tāvubhau",
  "GS25amark1pu:128": "(1.1.122) striyāṃ bahuṣvapsarasaḥ svarveśyā urvaśīmukhāḥ",
  "GS25amark1pu:129": "(1.1.123) hāhā hūhūścaivamādyā gandharvāstridivaukasām",
  "GS25amark1pu:130": "(1.1.124) agnirvaiśvānaro vahnirvītihotro dhanañjayaḥ",
  "GS25amark1pu:131": "(1.1.125) kṛpīṭayonirjvalano jātavedāstanūnapāt",
  "GS25amark1pu:132": "(1.1.126) barhiḥ śuṣmā kṛṣṇavartmā śociṣkeśa uparbudhaḥ",
  "GS25amark1pu:133": "(1.1.127) āśrayāśo bṛhadbhānuḥ kṛśānuḥ pāvako 'nalaḥ",
  "GS25amark1pu:134": "(1.1.128) rohitāśvo vāyusakhaḥ śikhāvānāśuśukṣaṇiḥ",
  "GS25amark1pu:135": "(1.1.129) hiraṇyaretā hutabhug dahano havyavāhanaḥ",
  "GS25amark1pu:136": "(1.1.130) saptārcirdamunāḥ śukraścitrabhānurvibhāvasuḥ",
  "GS25amark1pu:137": "(1.1.131) śucirappittamaurvastu vāḍavo vaḍavānalaḥ",
  "GS25amark1pu:138": "(1.1.132) vahnerdvayorjvālakīlāvarcirhetiḥ śikhāḥ striyām",
  "GS25amark1pu:139": "(1.1.133) triṣu sphuliṅgo 'gnikaṇaḥ saṃtāpaḥ saṃjvaraḥ samau",
  "GS25amark1pu:140": "(1.1.134) ulkā syāt nirgatajvālā bhūtirbhasitabhasmanī",
  "GS25amark1pu:141": "(1.1.135) kṣāro rakṣā ca dāvastu davo vanahutāśanaḥ",
  "GS25amark1pu:142": "(1.1.136) dharmarājaḥ pitṛpatiḥ samavartī paretarāṭ",
  "GS25amark1pu:143": "(1.1.137) kṛtānto yamunābhrātā śamano yamarāḍ yamaḥ",
  "GS25amark1pu:144": "(1.1.138) kālo daṇḍadharaḥ śrāddhadevo vaivasvato 'ntakaḥ",
  "GS25amark1pu:145": "(1.1.139) rākṣasaḥ koṇapaḥ kravyāt krvyādo 'srapa āśaraḥ",
  "GS25amark1pu:146": "(1.1.140) rātriñcaro rātricaraḥ karburo nikaṣātmajaḥ",
  "GS25amark1pu:147": "(1.1.141) yātudhānaḥ puṇyajano nairṛto yāturakṣasī",
  "GS25amark1pu:148": "(1.1.142) pracetā varuṇaḥ pāśī yādasāṃpatirappatiḥ",
  "GS25amark1pu:149": "(1.1.143) śvasanaḥ sparśano vāyurmātariśvā sadāgatiḥ",
  "GS25amark1pu:150": "(1.1.144) pṛṣadaśvo gandhavaho gandhavāhānilāśugāḥ",
  "GS25amark1pu:151": "(1.1.145) samīramārutamarut jagatprāṇasamīraṇāḥ",
  "GS25amark1pu:152": "(1.1.146) nabhasvadvātapavanapavamānaprabhañjanāḥ",
  "GS25amark1pu:153": "(1.1.147) prakampano mahāvāto jhañjhāvātaḥ savṛṣṭikaḥ",
  "GS25amark1pu:154": "(1.1.148) prāṇo 'pānaḥ samānaścodānavyānau ca vāyavaḥ",
  "GS25amark1pu:155": "(1.1.149) śarīrasthā ime raṃhastarasī tu rayaḥ syadaḥ",
  "GS25amark1pu:156": "(1.1.150) javo 'tha śīghraṃ tvaritaṃ laghu kṣipramaraṃ drutam",
  "GS25amark1pu:157": "(1.1.151) satvaraṃ capalaṃ tūrṇamavilambitamāśu ca",
  "GS25amark1pu:158": "(1.1.152) satate 'nāratāśrāntasaṃtatāviratāniśam",
  "GS25amark1pu:159": "(1.1.153) nityānavaratājasramapyathātiśayo bharaḥ",
  "GS25amark1pu:160": "(1.1.154) ativelabhṛśātyarthātimātrodgāḍhanirbharam",
  "GS25amark1pu:161": "(1.1.155) tīvraikāntanitāntāni gāḍhabāḍhadṛḍhāni ca",
  "GS25amark1pu:162": "(1.1.156) klībe śīghrādyasattve syāt triṣveṣāṃ sattvagāmi yat",
  "GS25amark1pu:163": "(1.1.157) kuberastryambakasakho yakṣarāḍ guhyakeśvaraḥ",
  "GS25amark1pu:164": "(1.1.158) manuṣyadharmā dhanado rājarājo dhanādhipaḥ",
  "GS25amark1pu:165": "(1.1.159) kinnareśo vaiśravaṇaḥ paulastyo naravāhanaḥ",
  "GS25amark1pu:166": "(1.1.160) yakṣaikapiṅgailavilaśrīdapuṇyajaneśvarāḥ",
  "GS25amark1pu:167": "(1.1.161) asyodyānaṃ caitrarathaṃ putrastu nalakūbaraḥ",
  "GS25amark1pu:168": "(1.1.162) kailāsaḥ sthānamalakā pūrvimānaṃ tu puṣpakam",
  "GS25amark1pu:169": "(1.1.163) syāt kinnaraḥ kimpuruṣasturaṅgavadano mayuḥ",
  "GS25amark1pu:170": "(1.1.164) nidhirnāśevadhirbhedāḥ padmaśaṅkhādayo nidheḥ",
  "GS25amark1pu:171": "(1.1.165) mahāpadmaśca padmaśca śaṅkho makarakacchapau",
  "GS25amark1pu:172": "(1.1.166) mukundakundanīlāśca kharvaśca nidhayo nava",
  "GS25amark1pu:173": "vyomavargaḥ",
  "GS25amark1pu:174": "(1.2.167) dyodivau dve striyāmabhraṃ vyoma puṣkaramambaram",
  "GS25amark1pu:175": "(1.2.168) nabho 'ntarikṣaṃ gaganamanantaṃ suravartma kham",
  "GS25amark1pu:176": "(1.2.169) viyad viṣṇupadaṃ vā tu puṃsyākāśavihāyasī",
  "GS25amark1pu:177": "(1.2.170) vihāsayo 'pi nāko 'pi dyurapi syāt tadavyam",
  "GS25amark1pu:178": "(1.2.171) tārāpatho 'ntarikṣaṃ ca meghādhvā ca mahābilam",
  "GS25amark1pu:179": "(1.2.172) vihāyāḥ śakune puṃsi gagane puṃnapuṃsakam",
  "GS25amark1pu:180": "digvargaḥ",
  "GS25amark1pu:181": "(1.3.173) diśastu kakubhaḥ kāṣṭhā āśāśca haritaśca tāḥ",
  "GS25amark1pu:182": "(1.3.174) prācyavācīpratīcyastāḥ pūrvadakṣiṇapaścimāḥ",
  "GS25amark1pu:183": "(1.3.175) uttarādigudīcī syāddiśyaṃ tu triṣu digbhave",
  "GS25amark1pu:184": "(1.3.176) avāgbhavamavācīnamudīcīcīnamudagbhavam",
  "GS25amark1pu:185": "(1.3.177) pratyagbhavaṃ pratīcīnaṃ prācīnaṃ prāgbhavaṃ triṣu",
  "GS25amark1pu:186": "(1.3.178) indro vahniḥ pitṛpatirnairṛto varuṇo marut",
  "GS25amark1pu:187": "(1.3.179) kubera {ī}śaḥ patayaḥ pūrvādīnāṃ diśāṃ kramāt",
  "GS25amark1pu:188": "(1.3.180) raviḥ śukro mahīsūnuḥ svarbhānurbhānujo vidhuḥ",
  "GS25amark1pu:189": "(1.3.181) budho bṛhaspatiśceti diśāṃ caiva tathā grahāḥ",
  "GS25amark1pu:190": "(1.3.182) airāvataḥ puṇḍarīko vāmanaḥ kumudo 'ñjanaḥ",
  "GS25amark1pu:191": "(1.3.183) puṣpadantaḥ sārvabhaumaḥ supratīkaśca diggajāḥ",
  "GS25amark1pu:192": "(1.3.184) kariṇyo 'bhramukapilāpiṅgalānupamāḥ kramāt",
  "GS25amark1pu:193": "(1.3.185) tāmrakarṇī śubhradantī cāṅganā cāñjanāvatī",
  "GS25amark1pu:194": "(1.3.186) klībāvyayaṃ tvapadiśaṃ diśormadhye vidik striyām",
  "GS25amark1pu:195": "(1.3.187) abhyantaraṃ tvantarālaṃ cakravālaṃ tu maṇḍalam",
  "GS25amark1pu:196": "(1.3.188) abhraṃ megho vārivāhaḥ stanayitnurbalāhakaḥ",
  "GS25amark1pu:197": "(1.3.189) dhārādharo jaladharastaḍitvān vārido 'mbubhṛt",
  "GS25amark1pu:198": "(1.3.190) ghanajīmūtamudirajalamugdhūmayonayaḥ",
  "GS25amark1pu:199": "(1.3.191) kādambinī meghamālā triṣu meghabhave 'bhriyam",
  "GS25amark1pu:200": "(1.3.192) stanitaṃ garjitam meghanirghoṣe rasitādi ca",
  "GS25amark1pu:201": "(1.3.193) śampā śatahradāhrādinyairāvatyaḥ kṣaṇaprabhā",
  "GS25amark1pu:202": "(1.3.194) taḍitsaudāminī vidyuccñcalā capalā api",
  "GS25amark1pu:203": "(1.3.195) sphūrjathurvajranirghoṣo meghajyotiriraṃmadaḥ",
  "GS25amark1pu:204": "(1.3.196) indrāyudhaṃ śakradhanustadeva ṛjurohitam",
  "GS25amark1pu:205": "(1.3.197) vṛṣṭivarṣaṃ tadvighāte 'vagrāhāvagrahau samau",
  "GS25amark1pu:206": "(1.3.198) dhārāsampāta āsāraḥ śīkarombukaṇāḥ smṛtāḥ",
  "GS25amark1pu:207": "(1.3.199) varṣopalastu karakā meghacchanne 'hni durdinam",
  "GS25amark1pu:208": "(1.3.200) antardhā vyavadhā puṃsi tvantardhirapavāraṇam",
  "GS25amark1pu:209": "(1.3.201) apidhānatirodhānapidhānācchādanāni ca",
  "GS25amark1pu:210": "(1.3.202) himāṃśuścandramāścandra induḥ kumudabāndhavaḥ",
  "GS25amark1pu:211": "(1.3.203) vidhuḥ sudhāṃśuḥ śubhrāṃśuroṣadhīśo niśāpatiḥ",
  "GS25amark1pu:212": "(1.3.204) abjo jaivātṛkaḥ somo glaurmṛgāṅkaḥ kalānidhiḥ",
  "GS25amark1pu:213": "(1.3.205) dvijarājaḥ śaśadharo nakṣatreśaḥ kṣapākaraḥ",
  "GS25amark1pu:214": "(1.3.206) kalā tu ṣoḍaśo bhāgo bimbo 'strī maṇḍalaṃ triṣu",
  "GS25amark1pu:215": "(1.3.207) bhittaṃ śakalakhaṇḍe vā puṃsyardho 'rdhaṃ sameṃśake",
  "GS25amark1pu:216": "(1.3.208) candrikā kaumudī jyotsnā prasādstu prasannatā",
  "GS25amark1pu:217": "(1.3.209) kalaṅkāṅkau lāñchanaṃ ca cihnaṃ lakṣma ca lakṣaṇam",
  "GS25amark1pu:218": "(1.3.210) suṣamā paramā śobhā śobhā kāntirdyutiścchaviḥ",
  "GS25amark1pu:219": "(1.3.211) avaśyāyastu nīhārastuṣārastuhinaṃ himam",
  "GS25amark1pu:220": "(1.3.212) prāleyaṃ mihikā cātha himānī himasaṃhatiḥ",
  "GS25amark1pu:221": "(1.3.213) śītaṃ guṇe tadvadarthāḥ suṣīmaḥ śiśiro jaḍaḥ",
  "GS25amark1pu:222": "(1.3.214) tuṣāraḥ śītalaḥ śīto himaḥ saptānyaliṅgakāḥ",
  "GS25amark1pu:223": "(1.3.215) dhruva auttānapādiḥ syāt agastyaḥ kumbhasambhavaḥ",
  "GS25amark1pu:224": "(1.3.216) maitrāvaruṇirasyaiva lopāmudrā sadharmiṇī",
  "GS25amark1pu:225": "(1.3.217) nakṣatramṛkṣaṃ bhaṃ tārā tārakāpyuḍu vā striyām",
  "GS25amark1pu:226": "(1.3.218) dākṣāyiṇyo 'śvinītyāditārā aśvayugaśvinī",
  "GS25amark1pu:227": "(1.3.219) rādhāviśākhā puṣye tu sidhyatiṣyau śraviṣṭhayā",
  "GS25amark1pu:228": "(1.3.220) samā dhaniṣṭhāḥ syuḥ proṣṭhapadā bhādrapadāḥ striyaḥ",
  "GS25amark1pu:229": "(1.3.221) mṛgaśīrṣaṃ mṛgaśirastasminnevāgrahāyaṇī",
  "GS25amark1pu:230": "(1.3.222) ilvalāstacchirodeśe tārakā nivasanti yāḥ",
  "GS25amark1pu:231": "(1.3.223) bṛhaspatiḥ surācāryo gīṣpatirdhiṣaṇo guruḥ",
  "GS25amark1pu:232": "(1.3.224) jīva āṅgiraso vācaspatiścitraśikhaṇḍijaḥ",
  "GS25amark1pu:233": "(1.3.225) śukro daityaguruḥ kāvya uśanā bhārgavaḥ kaviḥ",
  "GS25amark1pu:234": "(1.3.226) aṅgārakaḥ kujo bhaumo lohitāṅgo mahīsutaḥ",
  "GS25amark1pu:235": "(1.3.227) rauhiṇeyo budhaḥ saumyaḥ samau sauriśanaiśvarau",
  "GS25amark1pu:236": "(1.3.228) tamastu rāhuḥ svarbhānuḥ saiṃhikeyo vidhuntudaḥ",
  "GS25amark1pu:237": "(1.3.229) saptarṣayo marīcyatrimukhāścitraśikhaṇḍinaḥ",
  "GS25amark1pu:238": "(1.3.230) rāśīnāmudayo lagnaṃ te tu meṣavṛṣādayaḥ",
  "GS25amark1pu:239": "(1.3.231) sūrasūryāryamādityadvādaśātmadivākarāḥ",
  "GS25amark1pu:240": "(1.3.232) bhāskarāhaskarabradhnaprabhākaravibhākarāḥ",
  "GS25amark1pu:241": "(1.3.233) bhāsvadvivasvatsaptāśvaharidaśvoṣṇaraśmayaḥ",
  "GS25amark1pu:242": "(1.3.234) vikartanārkamārtaṇḍamihirāruṇapūṣaṇaḥ",
  "GS25amark1pu:243": "(1.3.235) dyumaṇistaraṇirmitraścitrabhānurvirocanaḥ",
  "GS25amark1pu:244": "(1.3.236) vibhāvasurgrahapatistviṣāṃpatiraharpatiḥ",
  "GS25amark1pu:245": "(1.3.237) bhānurhaṃsaḥ sahasrāṃśustapanaḥ savitā raviḥ",
  "GS25amark1pu:246": "(1.3.238) padmākṣastejasāṃrāśiśchāyānāthastamisrahā",
  "GS25amark1pu:247": "(1.3.239) karmasākṣī jagaccakṣurlokabandhustrayītanuḥ",
  "GS25amark1pu:248": "(1.3.240) pradyotano dinamaṇiḥ khadyoto lokabāndhavaḥ",
  "GS25amark1pu:249": "(1.3.241) ino bhago bhāmanidhiścāṃ 'śumālyañjinīpatiḥ",
  "GS25amark1pu:250": "(1.3.242) māṭharaḥ piṅgalo daṇḍaścaṇḍāṃśoḥ paripārśvakāḥ",
  "GS25amark1pu:251": "(1.3.243) sūrasūto 'ruṇo 'nūruḥ kāśyapirgaruḍāgrajaḥ",
  "GS25amark1pu:252": "(1.3.244) pariveṣastuparidhirupasūryakamaṇḍale",
  "GS25amark1pu:253": "(1.3.245) kiraṇosramayūkhāṃ 'śugabhastighṛṇiraśmayaḥ",
  "GS25amark1pu:254": "(1.3.246) bhānuḥ karo marīciḥ strīpuṃsayordīdhitiḥ striyām",
  "GS25amark1pu:255": "(1.3.247) syuḥ prabhārugrucistviḍbhābhāśchavidyutidīptayaḥ",
  "GS25amark1pu:256": "(1.3.248) rociḥ śocirubhe klībe prakāśo dyota ātapaḥ",
  "GS25amark1pu:257": "(1.3.249) koṣṇaṃ kavoṣṇaṃ mandoṣṇaṃ kaduṣṇaṃ triṣu tadvati",
  "GS25amark1pu:258": "(1.3.250) tigmaṃ tīkṣṇaṃ kharaṃ tadvanmṛgatṛṣṇā marīcikā",
  "GS25amark1pu:259": "kālavargaḥ",
  "GS25amark1pu:260": "(1.4.251) kālo diṣṭo 'pyanehāpi samayo 'pyatha pakṣatiḥ",
  "GS25amark1pu:261": "(1.4.252) pratipad dve ime strītve tadādyāstithayo dvayoḥ",
  "GS25amark1pu:262": "(1.4.253) ghasro dināhanī vā tu klībe divasavāsarau",
  "GS25amark1pu:263": "(1.4.254) pratyūṣo 'harmukhaṃ kalyamuṣaḥpratyuṣasī api",
  "GS25amark1pu:264": "(1.4.255) vyuṣṭaṃ vibhātaṃ dve klībe puṃsi gosarga iṣyate",
  "GS25amark1pu:265": "(1.4.256) prabhātaṃ ca dinānte tu sāyaṃ sandhyā pitṛprasūḥ",
  "GS25amark1pu:266": "(1.4.257) prāhṇāparāhṇamadhyāhnastrisandhyamatha śarvarī",
  "GS25amark1pu:267": "(1.4.258) niśā niśīthinī rātristriyāmā kṣaṇadā kṣapā",
  "GS25amark1pu:268": "(1.4.259) vibhāvarī tamasvinyau rajanī yāminī tamī",
  "GS25amark1pu:269": "(1.4.260) tamisrā tāmasī rātrirjyautsnī candrikayānvitā",
  "GS25amark1pu:270": "(1.4.261) āgāmivartamānārhayuktāyāṃ niśi pakṣiṇī",
  "GS25amark1pu:271": "(1.4.262) gaṇarātraṃ niśā bahvyaḥ pradoṣo rajanīmukham",
  "GS25amark1pu:272": "(1.4.263) ardharātraniśīthau dvau dvau yāmapraharau samau",
  "GS25amark1pu:273": "(1.4.264) sa parvasandhiḥ pratipatpañcadaśyoryadantaram",
  "GS25amark1pu:274": "(1.4.265) pakṣāntau pañcadaśyau dve paurṇamāsī tu paurṇimā",
  "GS25amark1pu:275": "(1.4.266) kalāhīne sānumatiḥ pūrṇe rākā niśākare",
  "GS25amark1pu:276": "(1.4.267) amāvāsyā tvamāvasyā darśaḥ sūryendusaṃgamaḥ",
  "GS25amark1pu:277": "(1.4.268) sā dṛṣṭenduḥ sinīvālī sā naṣṭendukalā kuhūḥ",
  "GS25amark1pu:278": "(1.4.269) uparāgo graho rāhugraste tvindau ca pūṣṇi ca",
  "GS25amark1pu:279": "(1.4.270) sopaplavoparaktau dvau agnyutpāta upāhitaḥ",
  "GS25amark1pu:280": "(1.4.271) ekayoktyā puṣpavantau divākaraniśākarau",
  "GS25amark1pu:281": "(1.4.272) aṣṭādaśa nimeṣāstu kāṣṭā triṃśat tu tāḥ kalā",
  "GS25amark1pu:282": "(1.4.273) tāstu triṃśat kṣaṇaste tu muhūrto dvādaśāstriyām",
  "GS25amark1pu:283": "(1.4.274) te tu triṃśadahorātraḥ pakṣaste daśapañca ca",
  "GS25amark1pu:284": "(1.4.275) pakṣau pūrvāparau śuklakṛṣṇau māsastu tāvubhau",
  "GS25amark1pu:285": "(1.4.276) dvau dvau mārgādi māsau syādṛtustairayanaṃ tribhiḥ",
  "GS25amark1pu:286": "(1.4.277) ayane dve gatirudagdakṣiṇārkasya vatsaraḥ",
  "GS25amark1pu:287": "(1.4.278) samarātridive kāle viṣuvadviṣuvaṃ ca tat",
  "GS25amark1pu:288": "(1.4.279) puṣpayuktā paurṇamāsī pauṣī māse tu yatra sā",
  "GS25amark1pu:289": "(1.4.280) nāmnā sa pauṣo māghādyāścaivamekādaśāpare",
  "GS25amark1pu:290": "(1.4.281) mārgaśīrṣe sahā mārga āgrahāyaṇikaśca saḥ",
  "GS25amark1pu:291": "(1.4.282) pauṣe taiṣasahasyau dvau tapā māghe 'tha phālgune",
  "GS25amark1pu:292": "(1.4.283) syāttapasyaḥ phālgunikaḥ syāccaitre caitriko madhuḥ",
  "GS25amark1pu:293": "(1.4.284) vaiśākhe mādhavo rādho jyeṣṭhe śukraḥ śucistvayam",
  "GS25amark1pu:294": "(1.4.285) āṣāḍhe śrāvaṇe tu syānnabhāḥ śrāvaṇikaśca saḥ",
  "GS25amark1pu:295": "(1.4.286) syurnabhasyaprauṣṭhapadabhādrabhādrapadāḥ samāḥ",
  "GS25amark1pu:296": "(1.4.287) syādāśvina iṣo 'pyāśvayujo 'pi syāttukārtike",
  "GS25amark1pu:297": "(1.4.288) bāhulorjau kārtikiko hemantaḥ śiśiro 'striyām",
  "GS25amark1pu:298": "(1.4.289) vasante puṣpasamayaḥ surabhirgrīṣma ūṣmakaḥ",
  "GS25amark1pu:299": "(1.4.290) nidāgha uṣṇopagama uṣṇa ūṣmāgamastapaḥ",
  "GS25amark1pu:300": "(1.4.291) striyāṃ prāvṛṭ striyāṃ bhūmni varṣā atha śaratstriyām",
  "GS25amark1pu:301": "(1.4.292) ṣaḍamī ṛtavaḥ puṃsi mārgādīnāṃ yugaiḥ kramāt",
  "GS25amark1pu:302": "(1.4.293) saṃvatsaro vatsaro 'bdo hāyano 'strī śaratsamāḥ",
  "GS25amark1pu:303": "(1.4.294) māsena syādahorātraḥ paitro varṣeṇa daivataḥ",
  "GS25amark1pu:304": "(1.4.295) daive yugasahasre dve brāhmaḥ kalpau tu tau nṛṇām",
  "GS25amark1pu:305": "(1.4.296) manvantaraṃ tu divyānāṃ yugānāmekasaptatiḥ",
  "GS25amark1pu:306": "(1.4.297) saṃvartaḥ pralayaḥ kalpaḥ kṣayaḥ kalpānta ityapi",
  "GS25amark1pu:307": "(1.4.298) astrī paṅkaṃ pumānpāpmā pāpaṃ kilbiṣakalmaṣam",
  "GS25amark1pu:308": "(1.4.299) kaluṣaṃ vṛjinaino 'ghamaṃho duritaduṣkṛtam",
  "GS25amark1pu:309": "(1.4.300) syāddharmamastriyāṃ puṇyaśreyasī sukṛtaṃ vṛṣaḥ",
  "GS25amark1pu:310": "(1.4.301) mutprītiḥ pramado harṣaḥ pramodāmodasammadāḥ",
  "GS25amark1pu:311": "(1.4.302) syādānandathurānandaḥ śarmaśātasukhāni ca",
  "GS25amark1pu:312": "(1.4.303) śvaḥ śreyasaṃ śivaṃ bhadraṃ kalyāṇaṃ maṅgalaṃ śubham",
  "GS25amark1pu:313": "(1.4.304) bhāvukaṃ bhavikaṃ bhavyaṃ kuśalaṃ kṣemamastriyām",
  "GS25amark1pu:314": "(1.4.305) śastaṃ cātha triṣu dravye pāpaṃ puṇyaṃ sukhādi ca",
  "GS25amark1pu:315": "(1.4.306) matallikā macarcikā prakāṇḍamuddhatallajau",
  "GS25amark1pu:316": "(1.4.307) praśastavācakānyamūnyayaḥ śubhāvaho vidhiḥ",
  "GS25amark1pu:317": "(1.4.308) daivaṃ diṣṭaṃ bhāgadheyaṃ bhāgyaṃ strī niyatirvidhiḥ",
  "GS25amark1pu:318": "(1.4.309) heturnā kāraṇaṃ bījaṃ nidānaṃ tvādikāraṇam",
  "GS25amark1pu:319": "(1.4.310) kṣetrajña ātmā puruṣaḥ pradhānaṃ prakṛtiḥ striyām",
  "GS25amark1pu:320": "(1.4.311) viśeṣaḥ kāliko 'vasthā guṇāḥ sattvaṃ rajastamaḥ",
  "GS25amark1pu:321": "(1.4.312) janurjananajanmāni janirutpattirudbhavaḥ",
  "GS25amark1pu:322": "(1.4.313) prāṇī tu cetano janmī jantujanyuśarīriṇaḥ",
  "GS25amark1pu:323": "(1.4.314) jātirjātaṃ ca sāmānyaṃ vyaktistu pṛthagātmatā",
  "GS25amark1pu:324": "(1.4.315) cittaṃ tu ceto hṛdayaṃ svāntaṃ hṛnmānasaṃ manaḥ",
  "GS25amark1pu:325": "dhīvargaḥ",
  "GS25amark1pu:326": "(1.5.316) buddhirmanīṣā dhiṣaṇā dhīḥ prajñā śemuṣī matiḥ",
  "GS25amark1pu:327": "(1.5.317) prekṣopalabdhiścitsaṃvitpratipajjñapticetanāḥ",
  "GS25amark1pu:328": "(1.5.318) dhīrdhāraṇāvatī medhā saṃkalpaḥ karma mānasam",
  "GS25amark1pu:329": "(1.5.319) avadhānaṃ samādhānaṃ praṇidhānam tathaiva ca",
  "GS25amark1pu:330": "(1.5.320) cittābhogo manaskāraścarcā saṃkhyā vicāraṇā",
  "GS25amark1pu:331": "(1.5.321) vimarśo bhāvanā caiva vāsanā ca nigadyate",
  "GS25amark1pu:332": "(1.5.322) adhyāhārastarka ūho vicikitsā tu saṃśayaḥ",
  "GS25amark1pu:333": "(1.5.323) sandehadvāparau cātha samau nirṇayaniścayau",
  "GS25amark1pu:334": "(1.5.324) mithyādṛṣṭirnāstikatā vyāpādo drohacintanam",
  "GS25amark1pu:335": "(1.5.325) samau siddhāntarāddhāntau bhrāntirmithyāmatirbhramaḥ",
  "GS25amark1pu:336": "(1.5.326) saṃvidāgūḥ pratijñānaṃ niyamāśravasaṃśravāḥ",
  "GS25amark1pu:337": "(1.5.327) aṅgīkārābhyupagamapratiśravasamādhayaḥ",
  "GS25amark1pu:338": "(1.5.328) mokṣe dhīrjñānamanyatra vijñānaṃ śilpaśāstrayoḥ",
  "GS25amark1pu:339": "(1.5.329) muktiḥ kaivalyanirvāṇaśreyoniḥśreyasāmṛtam",
  "GS25amark1pu:340": "(1.5.330) mokṣo 'pavargo 'thājñānamavidyāhaṃmatiḥ striyām",
  "GS25amark1pu:341": "(1.5.331) rūpaṃ śabdo gandharasasparśāśca viṣayā amī",
  "GS25amark1pu:342": "(1.5.332) gocarā indriyārthāśca hṛṣīkaṃ viṣayīndriyam",
  "GS25amark1pu:343": "(1.5.333) karmendriyaṃ tu pāyvādi manonetrādi dhīndriyam",
  "GS25amark1pu:344": "(1.5.334) tuvarastu kaṣāyo 'strī madhuro lavaṇaḥ kaṭuḥ",
  "GS25amark1pu:345": "(1.5.335) tikto 'mlaśca rasāḥ puṃsi tadvatsu ṣaḍamī triṣu",
  "GS25amark1pu:346": "(1.5.336) vimardotthe parimalo gandhe janamanohare",
  "GS25amark1pu:347": "(1.5.337) āmodaḥ so 'tinirhārī vācyaliṅgatvamāguṇāt",
  "GS25amark1pu:348": "(1.5.338) samākarṣī tu nirhārī surabhirghrāṇatarpaṇaḥ",
  "GS25amark1pu:349": "(1.5.339) iṣṭagandhaḥ sugandhiḥ syādāmodī mukhavāsanaḥ",
  "GS25amark1pu:350": "(1.5.340) pūtigandhastu durgandho visraṃ syādāmagandhi yat",
  "GS25amark1pu:351": "(1.5.341) śuklaśubhraśuciśvetaviśadaśyetapāṇḍarāḥ",
  "GS25amark1pu:352": "(1.5.342) avadātaḥ sito gauro 'valakṣo dhavalo 'rjunaḥ",
  "GS25amark1pu:353": "(1.5.343) hariṇaḥ pāṇḍuraḥ pāṇḍurīṣatpāṇḍustu dhūsaraḥ",
  "GS25amark1pu:354": "(1.5.344) kṛṣṇe nīlāsitaśyāmakālaśyāmalamecakāḥ",
  "GS25amark1pu:355": "(1.5.345) pīto gauro haridrābhaḥ palāśo harito harit",
  "GS25amark1pu:356": "(1.5.346) lohito rohito raktaḥ śoṇaḥ kokanadacchaviḥ",
  "GS25amark1pu:357": "(1.5.347) avyaktarāgastvaruṇaḥ śvetaraktastu pāṭalaḥ",
  "GS25amark1pu:358": "(1.5.348) śyāvaḥ syātkapiśo dhūmradhūmalau kṛṣṇalohite",
  "GS25amark1pu:359": "(1.5.349) kaḍāraḥ kapilaḥ piṅgapiśaṅgau kadrupiṅgalau",
  "GS25amark1pu:360": "(1.5.350) citraṃ kirmīrakalmāṣaśabalaitāśca karbure",
  "GS25amark1pu:361": "(1.5.351) guṇe śuklādayaḥ puṃsi guṇiliṅgāstu tadvati",
  "GS25amark1pu:362": "śabdavargaḥ",
  "GS25amark1pu:363": "(1.6.352) brāhmī tu bhāratī bhāṣā gīrvāgvāṇī sarasvatī",
  "GS25amark1pu:364": "(1.6.353) vyāhāra uktirlapitaṃ bhāṣitaṃ vacanaṃ vacaḥ",
  "GS25amark1pu:365": "(1.6.354) apabhraṃśo 'paśabdaḥ syācchāstre śabdastu vācakaḥ",
  "GS25amark1pu:366": "(1.6.355) tiṅ subantacayo vākyaṃ kriyā vā kārakānvitā",
  "GS25amark1pu:367": "(1.6.356) śrutiḥ strī veda āmnāyastrayī dharmastu tadvidhiḥ",
  "GS25amark1pu:368": "(1.6.357) striyāmṛk sāmayajuṣī iti vedāstrayastrayī",
  "GS25amark1pu:369": "(1.6.358) śikṣetyādi śruteraṅgamoṅkārapraṇavau samau",
  "GS25amark1pu:370": "(1.6.359) itihāsaḥ purāvṛttamudāttādyāstrayaḥ svarāḥ",
  "GS25amark1pu:371": "(1.6.360) ānvīkṣikī daṇḍanītistarkavidyārthaśāstrayoḥ",
  "GS25amark1pu:372": "(1.6.361) ākhyāyikopalabdhārthā purāṇaṃ pañcalakṣaṇam",
  "GS25amark1pu:373": "(1.6.362) prabandhakalpanā kathā pravahlikā prahelikā",
  "GS25amark1pu:374": "(1.6.363) smṛtistu dharmasaṃhitā samāhṛtistu saṃgrahaḥ",
  "GS25amark1pu:375": "(1.6.364) samasyā tu samāsārthā kiṃvadantī janaśrutiḥ",
  "GS25amark1pu:376": "(1.6.365) vārtā pravṛttirvṛttānta udantaḥ syādathāhvayaḥ",
  "GS25amark1pu:377": "(1.6.366) ākhyāhve abhidhānaṃ ca nāmadheyaṃ ca nāma ca",
  "GS25amark1pu:378": "(1.6.367) hūtirākāraṇāhvānaṃ saṃhūtirbahubhiḥ kṛtā",
  "GS25amark1pu:379": "(1.6.368) vivādo vyavahāraḥ syādupanyāsastu vāṅmukham",
  "GS25amark1pu:380": "(1.6.369) upoddhāta udāhāraḥ śapanaṃ śapathaḥ pumān",
  "GS25amark1pu:381": "(1.6.370) praśno 'nuyogaḥ pṛcchā ca prativākyottare same",
  "GS25amark1pu:382": "(1.6.371) mithyābhiyogo 'bhyākhyānamatha mithyābhiśaṃsanam",
  "GS25amark1pu:383": "(1.6.372) abhiśāpaḥ praṇādastu śabdaḥ syādanurāgajaḥ",
  "GS25amark1pu:384": "(1.6.373) yaśaḥ kīrtiḥ samajñā ca stavaḥ stotraṃ stutirnutiḥ",
  "GS25amark1pu:385": "(1.6.374) āmreḍitaṃ dvistriruktamuccairghuṣṭaṃ tu ghoṣaṇā",
  "GS25amark1pu:386": "(1.6.375) kākuḥ striyāṃ vikāro yaḥ śokabhītyādibhirdhvaneḥ",
  "GS25amark1pu:387": "(1.6.376) avarṇākṣepanirvādaparīvādāpavādavat.",
  "GS25amark1pu:388": "(1.6.377) upakrośo jugupsā ca kutsā nindā ca garhaṇe",
  "GS25amark1pu:389": "(1.6.378) pāruṣyamativādaḥ syād bhartsanaṃ tvapakāragīḥ",
  "GS25amark1pu:390": "(1.6.379) yaḥ saninda upālambhastatra syātparibhāṣaṇam",
  "GS25amark1pu:391": "(1.6.380) tatra tvākṣāraṇā yaḥ syādākrośo maithunaṃ prati",
  "GS25amark1pu:392": "(1.6.381) syādābhāṣaṇamālāpaḥ pralāpo 'narthakaṃ vacaḥ",
  "GS25amark1pu:393": "(1.6.382) anulāpo muhurbhāṣā vilāpaḥ paridevanam",
  "GS25amark1pu:394": "(1.6.383) vipralāpo virodhoktiḥ saṃlāpo bhāṣaṇaṃ mithaḥ",
  "GS25amark1pu:395": "(1.6.384) supralāpaḥ suvacanamapalāpastu nihnavaḥ",
  "GS25amark1pu:396": "(1.6.385) codyamākṣepābhiyogau śāpākrośau dureṣaṇā",
  "GS25amark1pu:397": "(1.6.386) astrī cāṭu caṭu ślāghā premṇā mithyāvikatthanam",
  "GS25amark1pu:398": "(1.6.387) sandeśavāgvācikaṃ syādvāgbhedāstu triṣūttare",
  "GS25amark1pu:399": "(1.6.388) ruśatī vāgakalyāṇī syātkalyā tu śubhātmikā",
  "GS25amark1pu:400": "(1.6.389) atyarthamadhuraṃ sāntvaṃ saṃgataṃ hṛdayaṅgamam",
  "GS25amark1pu:401": "(1.6.390) niṣṭhuraṃ paruṣaṃ grāmyamaślīlaṃ sūnṛtaṃ priye",
  "GS25amark1pu:402": "(1.6.391) satye 'tha saṃkulakliṣṭe parasparaparāhate",
  "GS25amark1pu:403": "(1.6.392) luptavarṇapadaṃ grastaṃ nirastaṃ tvaritoditam",
  "GS25amark1pu:404": "(1.6.393) jambūkṛtaṃ saniṣṭīvamabaddhaṃ syādanarthakam",
  "GS25amark1pu:405": "(1.6.394) anakṣaramavācyaṃ syādāhataṃ tu mṛṣārthakam",
  "GS25amark1pu:406": "(1.6.395) solluṭhanaṃ tu sotprāsaṃ maṇitaṃ ratikūjitam",
  "GS25amark1pu:407": "(1.6.396) śrāvyaṃ hṛdyaṃ manohāri vispaṣṭaṃ prakaṭoditam",
  "GS25amark1pu:408": "(1.6.397) atha mliṣṭamavispaṣṭaṃ vitathaṃ tvanṛtaṃ vacaḥ",
  "GS25amark1pu:409": "(1.6.398) satyaṃ tathyamṛtaṃ samyagamūni triṣu tadvati",
  "GS25amark1pu:410": "(1.6.399) śabde ninādaninadadhvanidhvānaravasvanāḥ",
  "GS25amark1pu:411": "(1.6.400) svānanirghoṣanirhrādanādanisvānanisvanāḥ",
  "GS25amark1pu:412": "(1.6.401) āravārāvasaṃrāvavirāvā atha marmaraḥ",
  "GS25amark1pu:413": "(1.6.402) svanite vastraparṇānāṃ bhūṣaṇānāṃ tu śiñjitam",
  "GS25amark1pu:414": "(1.6.403) nikvāṇo nikvaṇaḥ kvāṇaḥ kvaṇaḥ kvaṇanamityapi",
  "GS25amark1pu:415": "(1.6.404) vīṇāyāḥ kvaṇite prādeḥ prakvāṇaprakvaṇādayaḥ",
  "GS25amark1pu:416": "(1.6.405) kolāhalaḥ kalakalastiraścāṃ vāśitaṃ rutam",
  "GS25amark1pu:417": "(1.6.406) strī pratiśrutpratidhvāne gītaṃ gānamime same",
  "GS25amark1pu:418": "nāṭyavargaḥ",
  "GS25amark1pu:419": "(1.7.407) niṣādarṣabhagāndhāraṣaḍjamadhyamadhaivatāḥ",
  "GS25amark1pu:420": "(1.7.408) pañcamaścetyamī sapta tantrīkaṇṭhotthitāḥ svarāḥ",
  "GS25amark1pu:421": "(1.7.409) kākalī tu kale sūkṣme dhvanī tu madhurāsphuṭe",
  "GS25amark1pu:422": "(1.7.410) kalo mandrastu gambhīre tāro 'tyuccaistrayastriṣu",
  "GS25amark1pu:423": "(1.7.411) nṛṇāmurasi madhyastho dvāviṃśatividho dhvaniḥ",
  "GS25amark1pu:424": "(1.7.412) sa mandraḥ kaṇṭhamadhyasthastāraḥ śirasi gīyate",
  "GS25amark1pu:425": "(1.7.413) samanvitalayastvekatālo vīṇā tu vallakī",
  "GS25amark1pu:426": "(1.7.414) tripañcī sā tu tantrībhiḥ saptabhiḥ parivādinī",
  "GS25amark1pu:427": "(1.7.415) tataṃ vīṇādikaṃ vādyamānaddhaṃ murajādikam",
  "GS25amark1pu:428": "(1.7.416) vaṃśādikaṃ tu suṣiraṃ kāṃsyatālādikaṃ ghanam",
  "GS25amark1pu:429": "(1.7.417) caturvidhamidaṃ vādyaṃ vāditrātodyanāmakam",
  "GS25amark1pu:430": "(1.7.418) mṛdaṅgā murajā bhedāstvaṅkyāliṅgyordhvakāstrayaḥ",
  "GS25amark1pu:431": "(1.7.419) syād yaśaḥpaṭaho ḍhakkā bherī strī dundubhiḥ pumān",
  "GS25amark1pu:432": "(1.7.420) ānakaḥ paṭaho 'strī syāt koṇo vīṇādi vādanam",
  "GS25amark1pu:433": "(1.7.421) vīṇādaṇḍaḥ pravālaḥ syāt kakubhastu prasevakaḥ",
  "GS25amark1pu:434": "(1.7.422) kolambakastu kāyo 'syā upanāho nibandhanam",
  "GS25amark1pu:435": "(1.7.423) vādyaprabhedā ḍamarumaḍḍuḍiṇḍimajharjharāḥ",
  "GS25amark1pu:436": "(1.7.424) mardalaḥ paṇavo 'nye ca nartakīlāsike same",
  "GS25amark1pu:437": "(1.7.425) vilambitaṃ drutaṃ madhyaṃ tattvamogho ghanaṃ kramāt",
  "GS25amark1pu:438": "(1.7.426) tālaḥ kālakriyāmānaṃ layaḥ sāmyamamathāstriyām",
  "GS25amark1pu:439": "(1.7.427) tāṇḍavaṃ naṭanaṃ nāṭyaṃ lāsyaṃ nṛtyaṃ ca nartane",
  "GS25amark1pu:440": "(1.7.428) tauryatrikaṃ nṛtyagītavādyaṃ nāṭyamidaṃ trayam",
  "GS25amark1pu:441": "(1.7.429) bhrakuṃsaśca bhrukuṃsaśca bhrūkuṃsaśceti nartakaḥ",
  "GS25amark1pu:442": "(1.7.430) strīveṣadhārī puruṣo nāṭyoktau gaṇikāñjukā",
  "GS25amark1pu:443": "(1.7.431) bhaginīpatirāvutto bhāvo vidvānathāvukaḥ",
  "GS25amark1pu:444": "(1.7.432) janako yuvarājastu kumāro bhartṛdārakaḥ",
  "GS25amark1pu:445": "(1.7.433) rājā bhaṭṭārako devastatsutā bhartṛdārikā",
  "GS25amark1pu:446": "(1.7.434) devī kṛtābhiṣekāyāmitarāsu tu bhaṭṭinī",
  "GS25amark1pu:447": "(1.7.435) abrahmaṇyamavadhyoktau rājaśyālastu rāṣṭriyaḥ",
  "GS25amark1pu:448": "(1.7.436) ambā mātātha bālā syādvāsūrāryastu māriṣaḥ",
  "GS25amark1pu:449": "(1.7.437) attikā bhaginī jyeṣṭhā niṣṭhā nirvahaṇe same",
  "GS25amark1pu:450": "(1.7.438) haṇḍe hañje halāhvāne nīcāṃ ceṭīṃ sakhīṃ prati",
  "GS25amark1pu:451": "(1.7.439) aṅgahāro 'ṅgavikṣepo vyañjakābhinayau samau",
  "GS25amark1pu:452": "(1.7.440) nirvṛtte tvaṅgasattvābhyāṃ dve triṣvāṅgikasāttvike",
  "GS25amark1pu:453": "(1.7.441) śṛṅgāravīrakaruṇādbhutahāsyabhayānakāḥ",
  "GS25amark1pu:454": "(1.7.442) bībhatsaraudrau ca rasāḥ śṛṅgāraḥ śucirujjvalaḥ",
  "GS25amark1pu:455": "(1.7.443) utsāhavardhano vīraḥ kāruṇyaṃ karuṇā ghṛṇā",
  "GS25amark1pu:456": "(1.7.444) kṛpā dayānukampā syādanukrośo 'pyatho hasaḥ",
  "GS25amark1pu:457": "(1.7.445) hāso hāsyaṃ ca bībhatsaṃ vikṛtaṃ triṣvidaṃ dvayam",
  "GS25amark1pu:458": "(1.7.446) vismayo 'dbhutamāścaryaṃ citramapyatha bhairavam",
  "GS25amark1pu:459": "(1.7.447) dāruṇaṃ bhīṣaṇaṃ bhīṣmaṃ ghoraṃ bhīmaṃ bhayānakam",
  "GS25amark1pu:460": "(1.7.448) bhayaṅkaraṃ pratibhayaṃ raudraṃ tūgramamī triṣu",
  "GS25amark1pu:461": "(1.7.449) caturdaśa darastrāso bhītirbhīḥ sādhvasaṃ bhayam",
  "GS25amark1pu:462": "(1.7.450) vikāro mānaso bhāvo 'nubhāvo bhāvabodhakaḥ",
  "GS25amark1pu:463": "(1.7.451) garvo 'bhimāno 'haṅkāro mānaścittasamunnatiḥ",
  "GS25amark1pu:464": "(1.7.452) darpo 'valoko 'vaṣṭambhaścittodrekaḥ smayo madaḥ",
  "GS25amark1pu:465": "(1.7.453) anādaraḥ paribhavaḥ parībhāvastiraskriyā",
  "GS25amark1pu:466": "(1.7.454) rīḍhāvamānanāvajñāvahelanamasūrkṣaṇam",
  "GS25amark1pu:467": "(1.7.455) mandākṣaṃ hrīstrapā vrīḍā lajjā sāpatrapānyataḥ",
  "GS25amark1pu:468": "(1.7.456) kṣāntistitikṣābhidhyā tu parasya viṣaye spṛhā",
  "GS25amark1pu:469": "(1.7.457) akṣāntirīrṣyāsūyā tu doṣāropo guṇeṣvapi",
  "GS25amark1pu:470": "(1.7.458) vairaṃ virodho vidveṣo manyuśokau tu śuk striyām",
  "GS25amark1pu:471": "(1.7.459) paścāttāpo 'nutāpaśca vipratīsāra ityapi",
  "GS25amark1pu:472": "(1.7.460) kopakrodhāmarṣaroṣapratighā ruṭ kṛdhau striyau",
  "GS25amark1pu:473": "(1.7.461) śucau tu carite śīlamunmādaścittavibhramaḥ",
  "GS25amark1pu:474": "(1.7.462) premā nā priyatā hārdaṃ premasneho 'tha dohadam",
  "GS25amark1pu:475": "(1.7.463) icchā kāṅkṣā spṛhehā tṛḍ vāñchā lipsā manorathaḥ",
  "GS25amark1pu:476": "(1.7.464) kāmo 'bhilāṣastarṣaśca so 'tyarthaṃ lālasā dvayoḥ",
  "GS25amark1pu:477": "(1.7.465) upādhirnā dharmacintā puṃsyādhirmānasī vyathā",
  "GS25amark1pu:478": "(1.7.466) syāccintā smṛtirādhyānamutkaṇṭhotkalike same",
  "GS25amark1pu:479": "(1.7.467) utsāho 'dhyavasāyaḥ syāt sa vīryamatiśaktibhāk",
  "GS25amark1pu:480": "(1.7.468) kapaṭo 'strī vyājadambhopadhayaś chadmakaitave",
  "GS25amark1pu:481": "(1.7.469) kusṛtirnikṛtiḥ śāṭhyaṃ pramādo 'navadhānatā",
  "GS25amark1pu:482": "(1.7.470) kautūhalaṃ kautukaṃ ca kutukaṃ ca kutūhalam",
  "GS25amark1pu:483": "(1.7.471) strīṇāṃ vilāsabibbokavibhramā lalitaṃ tathā",
  "GS25amark1pu:484": "(1.7.472) helā līletyamī hāvāḥkriyāḥ śṛṅgārabhāvajāḥ",
  "GS25amark1pu:485": "(1.7.473) dravakeliparīhāsāḥ krīḍā līlā ca narma ca",
  "GS25amark1pu:486": "(1.7.474) vyājo 'padeśo lakṣyaṃ ca krīḍā khelā ca kūrdanam",
  "GS25amark1pu:487": "(1.7.475) gharmo nidāghaḥ svedaḥ syātpralayo naṣṭaceṣṭatā",
  "GS25amark1pu:488": "(1.7.476) avahitthākāraguptiḥ samau saṃvegasaṃbhramau",
  "GS25amark1pu:489": "(1.7.477) syādācchuritakaṃ hāsaḥ sotprāsaḥ sa manāk smitam",
  "GS25amark1pu:490": "(1.7.478) madhyamaḥ syādvihasitaṃ romāñco romaharṣaṇam",
  "GS25amark1pu:491": "(1.7.479) kranditaṃ ruditam kruṣṭaṃ jṛmbhastu triṣu jṛmbhaṇam",
  "GS25amark1pu:492": "(1.7.480) vipralambho visaṃvādo riṅgaṇaṃ skhalanaṃ same",
  "GS25amark1pu:493": "(1.7.481) syānnidrā śayanaṃ svāpaḥ svapnaḥ saṃveśa ityapi",
  "GS25amark1pu:494": "(1.7.482) tandrī pramīlā bhrakuṭirbhrukuṭirbhrūkuṭiḥ striyām",
  "GS25amark1pu:495": "(1.7.483) adṛṣṭiḥ syādasaumye 'kṣṇi saṃsiddhiprakṛtī tvime",
  "GS25amark1pu:496": "(1.7.484) svarūpaṃ ca svabhāvaśca nisargaścātha vepathuḥ",
  "GS25amark1pu:497": "(1.7.485) kampo 'tha kṣaṇa uddharṣo maha uddhava utsavaḥ",
  "GS25amark1pu:498": "pātālabhogivargaḥ}",
  "GS25amark1pu:499": "(1.8.486) adhobhuvanapātālaṃ balisadma rasātalam",
  "GS25amark1pu:500": "(1.8.487) nāgaloko 'tha kuharaṃ suṣiraṃ vivaraṃ bilam",
  "GS25amark1pu:501": "(1.8.488) chidraṃ nirvyathanaṃ rokaṃ randhraṃ śvabhraṃ vapā suṣiḥ",
  "GS25amark1pu:502": "(1.8.489) gartāvaṭau bhuvi śvabhre sarandhre suṣiraṃ triṣu",
  "GS25amark1pu:503": "(1.8.490) andhakāro 'striyāṃ dhvāntaṃ tamisraṃ timiraṃ tamaḥ",
  "GS25amark1pu:504": "(1.8.491) dhvānte gāḍhe 'ndhatamasaṃ kṣīṇe 'vatamasaṃ tamaḥ",
  "GS25amark1pu:505": "(1.8.492) viṣvaksaṃtamasaṃ nāgāḥ kādraveyāstadīśvare",
  "GS25amark1pu:506": "(1.8.493) śeṣo 'nanto vāsukistu sarparājo 'tha gonase",
  "GS25amark1pu:507": "(1.8.494) tilitsaḥ syādajagare śayurvāhasa ityubhau",
  "GS25amark1pu:508": "(1.8.495) alagardo jalavyālaḥ samau rājilaḍuṇḍumau",
  "GS25amark1pu:509": "(1.8.496) māludhāno mātulāhirnirmukto muktakañcukaḥ",
  "GS25amark1pu:510": "(1.8.497) sarpaḥ pṛdākurbhujago bhujaṅgo 'hirbhujaṅgamaḥ",
  "GS25amark1pu:511": "(1.8.498) āśīviṣo viṣadharaścakrī vyālaḥ sarīsṛpaḥ",
  "GS25amark1pu:512": "(1.8.499) kuṇḍalī gūḍhapāccakṣuḥśravāḥ kākodaraḥ phaṇī",
  "GS25amark1pu:513": "(1.8.500) darvīkaro dīrghapṛṣṭho dandaśūko bileśayaḥ",
  "GS25amark1pu:514": "(1.8.501) uragaḥ pannago bhogī jihmagaḥ pavanāśanaḥ",
  "GS25amark1pu:515": "(1.8.502) lelihāno dvirasano gokarṇaḥ kañcukī tathā",
  "GS25amark1pu:516": "(1.8.503) kumbhīnasaḥ phaṇadharo harirbhogadharastathā",
  "GS25amark1pu:517": "(1.8.504) aheḥ śarīraṃ bhogaḥ syādāśīrapyahidaṃṣṭrikā",
  "GS25amark1pu:518": "(1.8.505) triṣvāheyaṃ viṣāsthyādi sphaṭāyāṃ tu phaṇā dvayoḥ",
  "GS25amark1pu:519": "(1.8.506) samau kañcukanirmokau kṣveḍastu garalaṃ viṣam",
  "GS25amark1pu:520": "(1.8.507) puṃsi klībe ca kākolakālakūṭahalāhalāḥ",
  "GS25amark1pu:521": "(1.8.508) saurāṣṭrikaḥ śauklikeyo brahmaputraḥ pradīpanaḥ",
  "GS25amark1pu:522": "(1.8.509) dārado vatsanābhaśca viṣabhedā amī nava",
  "GS25amark1pu:523": "(1.8.510) viṣavaidyo jāṅguliko vyālagrāhyahituṇḍikaḥ",
  "GS25amark1pu:524": "narakavargaḥ}",
  "GS25amark1pu:525": "(1.9.511) syānnārakastu narako nirayo durgatiḥ striyām",
  "GS25amark1pu:526": "(1.9.512) tadbhedāstapanāvīcimahārauravarauravāḥ",
  "GS25amark1pu:527": "(1.9.513) saṃghātaḥ kālasūtraṃ cetyādyāḥ sattvāstu nārakāḥ",
  "GS25amark1pu:528": "(1.9.514) pretā vaitaraṇī sindhuḥ syādalakṣmīstu nirrtiḥ",
  "GS25amark1pu:529": "(1.9.515) viṣṭirājūḥ kāraṇā tu yātanā tīvravedanā",
  "GS25amark1pu:530": "(1.9.516) pīḍā bādhā vyathā duḥkhamāmanasyaṃ prasūtijam",
  "GS25amark1pu:531": "(1.9.517) syātkaṣṭaṃ kṛcchramābhīlaṃ triṣveṣāṃ bhedyagāmi yat",
  "GS25amark1pu:532": "vārivargaḥ}",
  "GS25amark1pu:533": "(1.10.518) samudro 'bdhirakūpāraḥ pārāvāraḥ saritpatiḥ",
  "GS25amark1pu:534": "(1.10.519) udanvānudadhiḥ sindhuḥ sarasvānsāgaro 'rṇavaḥ",
  "GS25amark1pu:535": "(1.10.520) ratnākaro jalanidhiryādaḥpatirapāmpatiḥ",
  "GS25amark1pu:536": "(1.10.521) tasya prabhedāḥ kṣīrodo lavaṇodastathāpare",
  "GS25amark1pu:537": "(1.10.522) āpaḥ strī bhūmni vārvāri salilaṃ kamalaṃ jalaṃ",
  "GS25amark1pu:538": "(1.10.523) payaḥ kīlālamamṛtaṃ jīvanaṃ bhuvanaṃ vanam",
  "GS25amark1pu:539": "(1.10.524) kabandhamudakaṃ pāthaḥ puṣkaraṃ sarvatomukham",
  "GS25amark1pu:540": "(1.10.525) ambhorṇastoyapānīyanīrakṣīro 'mbuśambaram",
  "GS25amark1pu:541": "(1.10.526) meghapuṣpaṃ ghanarasastriṣu dve āpyamammayam",
  "GS25amark1pu:542": "(1.10.527) bhaṅgastaraṅga ūrmirvā striyāṃ vīcirathormiṣu",
  "GS25amark1pu:543": "(1.10.528) mahatsūllolakallolau syādāvarto 'mbhasāṃ bhramaḥ",
  "GS25amark1pu:544": "(1.10.529) pṛṣanti bindupṛṣatāḥ pumāṃso vipruṣaḥ striyām",
  "GS25amark1pu:545": "(1.10.530) cakrāṇi puṭabhedāḥ syurbhramāśca jalanirgamāḥ",
  "GS25amark1pu:546": "(1.10.531) kūlaṃ rodhaśca tīraṃ ca pratīraṃ ca taṭaṃ triṣu",
  "GS25amark1pu:547": "(1.10.532) pārāvāre parārvācī tīre pātraṃ tadantaram",
  "GS25amark1pu:548": "(1.10.533) dvīpo 'striyāmantarīpaṃ yadantarvāriṇastaṭam",
  "GS25amark1pu:549": "(1.10.534) toyotthitaṃ tatpulinaṃ saikataṃ sikatāmayam",
  "GS25amark1pu:550": "(1.10.535) niṣadvarastu jambālaḥ paṅko 'strī śādakardamau",
  "GS25amark1pu:551": "(1.10.536) jalocchvāsāḥ parīvāhāḥ kūpakāstu vidārakāḥ",
  "GS25amark1pu:552": "(1.10.537) nāvyaṃ triliṅgaṃ nautārye striyāṃ naustaraṇistariḥ",
  "GS25amark1pu:553": "(1.10.538) uḍupaṃ tu plavaḥ kolaḥ sroto 'mbusaraṇaṃ svataḥ",
  "GS25amark1pu:554": "(1.10.539) ātarastarapaṇyaṃ syād droṇī kāṣṭāmbuvāhinī",
  "GS25amark1pu:555": "(1.10.540) sāṃyātrikaḥ potavaṇik karṇadhārastu nāvikaḥ",
  "GS25amark1pu:556": "(1.10.541) niyāmakāḥ potavāhāḥ kūpako guṇavṛkṣakaḥ",
  "GS25amark1pu:557": "(1.10.542) naukādaṇḍaḥ kṣepaṇī syādaritraṃ kenipātakaḥ",
  "GS25amark1pu:558": "(1.10.543) abhriḥ strī kāṣṭakuddālaḥ sekapātraṃ tu secanam",
  "GS25amark1pu:559": "(1.10.544) klībe 'rdhanāvaṃ nāvo 'rdhe 'tītanauke 'tinu triṣu",
  "GS25amark1pu:560": "(1.10.545) triṣvāgādhātprasanno 'cchaḥ kaluṣo 'naccha āvilaḥ",
  "GS25amark1pu:561": "(1.10.546) nimnaṃ gabhīraṃ gambhīramuttānaṃ tadviparyaye",
  "GS25amark1pu:562": "(1.10.547) agādhamatalasparśe kaivarte dāśadhīvarau",
  "GS25amark1pu:563": "(1.10.548) ānāyaḥ puṃsi jālaṃ syācchaṇasūtraṃ pavitrakam",
  "GS25amark1pu:564": "(1.10.549) matsyādhānī kuveṇī syād baḍiśaṃ matsyavedhanam",
  "GS25amark1pu:565": "(1.10.550) pṛthuromā jhaṣo matsyo mīno vaisāriṇo 'ṇḍajaḥ",
  "GS25amark1pu:566": "(1.10.551) visāraḥ śakulī cātha gaḍakaḥ śakulārbhakaḥ",
  "GS25amark1pu:567": "(1.10.552) sahasradaṃṣṭraḥ pāṭhīna ulūpī śiśukaḥ samau",
  "GS25amark1pu:568": "(1.10.553) nalamīnaścilicimaḥ proṣṭhī tu śapharī dvayoḥ",
  "GS25amark1pu:569": "(1.10.554) kṣudrāṇḍamatsyasaṃghātaḥ potādhānamatho jhaṣāḥ",
  "GS25amark1pu:570": "(1.10.555) rohito madguraḥ śālo rājīvaḥ śakulastimiḥ",
  "GS25amark1pu:571": "(1.10.556) timiṅgalādayaścātha yādāṃsi jalajantavaḥ",
  "GS25amark1pu:572": "(1.10.557) tadbhedāḥ śiśumārodraśaṅkavo makarādayaḥ",
  "GS25amark1pu:573": "(1.10.558) syātkulīraḥ karkaṭakaḥ kūrme kamaṭhakacchapau",
  "GS25amark1pu:574": "(1.10.559) grāho 'vahāro nakrastu kumbhīro 'tha mahīlatā",
  "GS25amark1pu:575": "(1.10.560) gaṇḍūpadaḥ kiñculako nihākā godhikā same",
  "GS25amark1pu:576": "(1.10.561) raktapā tu jalaukāyāṃ striyāṃ bhūmni jalaukasaḥ",
  "GS25amark1pu:577": "(1.10.562) muktāsphoṭaḥ striyāṃ śuktiḥ śaṅkhaḥ syātkamburastriyau",
  "GS25amark1pu:578": "(1.10.563) kṣudraśaṅkhāḥ śaṅkhanakhāḥ śambūkā jalaśuktayaḥ",
  "GS25amark1pu:579": "(1.10.564) bheke maṇḍūkavarṣābhūśālūraplavadardurāḥ",
  "GS25amark1pu:580": "(1.10.565) śilī gaṇḍūpadī bhekī varṣābhvī kamaṭhī ḍuliḥ",
  "GS25amark1pu:581": "(1.10.566) madgurasya priyā śṛṅgī durnāmā dīrghakośikā",
  "GS25amark1pu:582": "(1.10.567) jalāśayā jalādhārāstatrāgādhajalo hradaḥ",
  "GS25amark1pu:583": "(1.10.568) āhāvastu nipānaṃ syādupakūpajalāśaye",
  "GS25amark1pu:584": "(1.10.569) puṃsyevāndhuḥ prahiḥ kūpa udapānṃ tu puṃsi vā",
  "GS25amark1pu:585": "(1.10.570) nemistrikāsya vīnāho mukhabandhanamasya yat",
  "GS25amark1pu:586": "(1.10.571) puṣkariṇyāṃ tu khātaṃ syādakhātaṃ devakhātakam",
  "GS25amark1pu:587": "(1.10.572) padmākarastaḍāgo 'strī kāsāraḥ sarasī saraḥ",
  "GS25amark1pu:588": "(1.10.573) veśantaḥ palvalaṃ cālpasaro vāpī tu dīrghikā",
  "GS25amark1pu:589": "(1.10.574) kheyaṃ tu parikhādhārastvambhasāṃ yatra dhāraṇam",
  "GS25amark1pu:590": "(1.10.575) syādālavālamāvālamāvāpo 'tha nadī sarit",
  "GS25amark1pu:591": "(1.10.576) taraṅgiṇī śaivalinī taṭinī hrādinī dhunī",
  "GS25amark1pu:592": "(1.10.577) srotasvinī dvīpavatī sravantī nimnagāpagā",
  "GS25amark1pu:593": "(1.10.578) kūlaṅkaṣā nirjhariṇī rodhovakrā sarasvatī",
  "GS25amark1pu:594": "(1.10.579) gaṅgā viṣṇupadī jahnutanayā suranimnagā",
  "GS25amark1pu:595": "(1.10.580) bhāgīrathī tripathagā trisrotā bhīṣmasūrapi",
  "GS25amark1pu:596": "(1.10.581) kālindī sūryatanayā yamunā śamanasvasā",
  "GS25amark1pu:597": "(1.10.582) revā tu narmadā somodbhavā mekalakanyakā",
  "GS25amark1pu:598": "(1.10.583) karatoyā sadānīrā bāhudā saitavāhinī",
  "GS25amark1pu:599": "(1.10.584) śatadrustu śutudriḥ syādvipāśā tu vipāṭ striyām",
  "GS25amark1pu:600": "(1.10.585) śoṇo hiraṇyavāhaḥ syātkulyālpā kṛtrimā sarit",
  "GS25amark1pu:601": "(1.10.586) śarāvatī vetravatī candrabhāgā sarasvatī",
  "GS25amark1pu:602": "(1.10.587) kāverī sarito 'nyāśca sambhedaḥ sindhusaṅgamaḥ",
  "GS25amark1pu:603": "(1.10.588) dvayoḥ praṇālī payasaḥ padavyāṃ triṣu tūttarau",
  "GS25amark1pu:604": "(1.10.589) devikāyāṃ sarayvāṃ ca bhave dāvikasāravau",
  "GS25amark1pu:605": "(1.10.590) saugandhikaṃ tu kalhāraṃ hallakaṃ raktasandhyakam",
  "GS25amark1pu:606": "(1.10.591) syādutpalaṃ kuvalayamatha nīlāmbujanma ca",
  "GS25amark1pu:607": "(1.10.592) indīvaraṃ ca nīle 'sminsite kumudakairave",
  "GS25amark1pu:608": "(1.10.593) śālūkameṣāṃ kandaḥ syādvāriparṇī tu kumbhikā",
  "GS25amark1pu:609": "(1.10.594) jalanīlī tu śaivālaṃ śaivalo 'tha kumudvatī",
  "GS25amark1pu:610": "(1.10.595) kumudinyāṃ nalinyāṃ tu visinīpadminīmukhāḥ",
  "GS25amark1pu:611": "(1.10.596) vā puṃsi padmaṃ nalinamaravindaṃ mahotpalam",
  "GS25amark1pu:612": "(1.10.597) sahasrapatraṃ kamalaṃ śatapatraṃ kuśeśayam",
  "GS25amark1pu:613": "(1.10.598) paṅkeruhaṃ tāmarasaṃ sārasaṃ sarasīruham",
  "GS25amark1pu:614": "(1.10.599) bisaprasūnarājīvapuṣkarāmbhoruhāṇi ca",
  "GS25amark1pu:615": "(1.10.600) puṇḍarīkaṃ sitāmbhojamatha raktasaroruhe",
  "GS25amark1pu:616": "(1.10.601) raktotpalaṃ kokanadaṃ nālo nālamathāstriyām",
  "GS25amark1pu:617": "(1.10.602) mṛṇālaṃ bisamabjādikadambe khaṇḍamastriyām",
  "GS25amark1pu:618": "(1.10.603) karahāṭaḥ śiphākandaḥ kiñjalkaḥ kesaro 'striyām",
  "GS25amark1pu:619": "(1.10.604) saṃvartikā navadalaṃ bījakośo varāṭakaḥ",
  "GS25amark1pu:620": "kāṇḍasamāptiḥ",
  "GS25amark1pu:621": "(1.11.605) uktaṃ svarvyomadikkāladhīśabdādi sanāṭyakam",
  "GS25amark1pu:622": "(1.11.606) pātālabhoginarakaṃ vāri caiṣāṃ ca saṅgatam",
  "GS25amark1pu:623": "(1.11.607) ityamarasiṃhakṛtau nāmaliṅgānuśāsane",
  "GS25amark1pu:624": "(1.11.608) svarādikāṇḍaḥ prathamaḥ sāṅga eva samarthitaḥ"
}