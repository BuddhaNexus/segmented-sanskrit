    "GR13kirtm_iu:676": " sthānāśrayavaśādbhavet KirT_4.9d",
    "GR13kirtm_iu:677": " sthitatvātsarvadā śakter KirT_5.2a",
    "GR13kirtm_iu:678": " sthitākṣobhyā tataḥsmṛtā KirT_4.20f",
    "GR13kirtm_iu:679": " sthitā nyagrodhabījavat KirT_4.21d",
    "GR13kirtm_iu:680": " sthitā prakāśikā kāryān KirT_2.14a",
    "GR13kirtm_iu:681": " sthitiścoktā śivāgame KirT_6.13d",
    "GR13kirtm_iu:682": " sthito māyodare katham KirT_2.6b",
    "GR13kirtm_iu:683": " sthityupāyavivakṣayā KirT_6.14d",
    "GR13kirtm_iu:684": " sthūlakāryāsu sūkṣmāpi KirT_4.21c",
    "GR13kirtm_iu:685": " sthūlasyāsya samantataḥ KirT_4.22b",
    "GR13kirtm_iu:686": " sthūlaṃ vicitrakaṃ kāryaṃ KirT_3.12a",
    "GR13kirtm_iu:687": " spandenāpyanumīyate KirT_6.25d",
    "GR13kirtm_iu:688": " sphuṭaṃ dīpāndhakāravat KirT_2.15b",
    "GR13kirtm_iu:689": " sphuṭaṃ yatra kvaciddṛṣṭaṃ KirT_6.23a",
    "GR13kirtm_iu:690": " svakarmamalahetutaḥ KirT_2.16b",
    "GR13kirtm_iu:691": " svata eva vikāreṇa KirT_4.15c",
    "GR13kirtm_iu:692": " svatantratvātprabhutvataḥ KirT_4.29d",
    "GR13kirtm_iu:693": " svataḥ kāryātprakāśikā KirT_2.12d",
    "GR13kirtm_iu:694": " svato na vikṛtistasmād KirT_4.17c",
    "GR13kirtm_iu:695": " svarūpaṃ dyotayatyāśu KirT_5.10a",
    "GR13kirtm_iu:696": " svalpamapyatra kiṃ na tat KirT_6.22b",
    "GR13kirtm_iu:697": " svārjite 'pi niyamyate KirT_1.18b",
    "GR13kirtm_iu:698": " haṭhavattanudhāraṇam KirT_4.10b",
    "GR13kirtm_iu:699": " haraṃ dṛṣṭvābravīttārkṣyaḥ KirT_1.1c"
}