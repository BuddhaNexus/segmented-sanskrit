    "GR13devikaiu:244": " lokācāraṃ ca sarvaśaḥ Dka_64d",
    "GR13devikaiu:245": " loko 'yaṃ śaṅkarātmakaḥ Dka_81d",
    "GR13devikaiu:246": " varjayedyadi kautukam Dka_68d",
    "GR13devikaiu:247": " varṇāśramasamanvitān Dka_48b",
    "GR13devikaiu:248": " vaśyākarṣaṇamohanam Dka_72b",
    "GR13devikaiu:249": " vāṅmanaḥkāyakarmabhiḥ Dka_83b",
    "GR13devikaiu:250": " vāyubhiścalanaṃ manaḥ Dka_9d",
    "GR13devikaiu:251": " vikṣiptaṃ śamayet punaḥ Dka_39b",
    "GR13devikaiu:252": " vijñāptimātro hi sadā viśuddhaḥ Dka_53a",
    "GR13devikaiu:253": " vijñeyaṃ muktilakṣaṇam Dka_40d",
    "GR13devikaiu:254": " vidveṣastambhane tathā Dka_71d",
    "GR13devikaiu:255": " vindan loke na sīdati Dka_19d",
    "GR13devikaiu:256": " vivādaṃ lokagoṣṭhīṃ ca Dka_75c",
    "GR13devikaiu:257": " viṣaye lolupaṃ cittaṃ Dka_27a",
    "GR13devikaiu:258": " veditavyaṃ na kiñcana Dka_18d",
    "GR13devikaiu:259": " vairāgyeṇa vapustyāgo Dka_45a",
    "GR13devikaiu:260": " vyāpakaṃ yo na bhāvayet Dka_24b",
    "GR13devikaiu:261": " vyāptaṃ caiva tu nirmalam Dka_36d",
    "GR13devikaiu:262": " vyomākāraṃ mahāśūnyaṃ Dka_24a",
    "GR13devikaiu:263": " śaktidhyānamudāhṛtam Dka_14b",
    "GR13devikaiu:264": " śāstrakoṭiśatairapi Dka_3d",
    "GR13devikaiu:265": " śāstragoṣṭhīṃ na kurvīta Dka_76a",
    "GR13devikaiu:266": " śivastatraiva tiṣṭhati Dka_81b",
    "GR13devikaiu:267": " śivādyavaniparyanto Dka_81c",
    "GR13devikaiu:268": " śraddaddhāno nirākulaḥ Dka_4d",
    "GR13devikaiu:269": " sa indraḥ sa ṣaḍānanaḥ Dka_6b",
    "GR13devikaiu:270": " sa upāyo mimokṣasya Dka_8a",
    "GR13devikaiu:271": " sa guruḥ sarvadevāśca Dka_6c",
    "GR13devikaiu:272": " sa tapaśca na saṃśayaḥ Dka_9b",
    "GR13devikaiu:273": " sadā kuryānnirāśrayam Dka_38d",
    "GR13devikaiu:274": " sadābhibhūtaye cittaṃ Dka_33a",
    "GR13devikaiu:275": " sadupāttaguṇastu saḥ Dka_8b",
    "GR13devikaiu:276": " sanātanaṃ brahma nirantaraṃ yat Dka_60a",
    "GR13devikaiu:277": " santataṃ janavarjitaḥ Dka_78b",
    "GR13devikaiu:278": " santyajetsarvakarmāṇi Dka_64c",
    "GR13devikaiu:279": " sabāhyābhyantaraṃ priye Dka_29b",
    "GR13devikaiu:280": " sa brahmabhūto 'mṛtatāmupaiti Dka_60d",
    "GR13devikaiu:281": " sa brahmā sa śivo viṣṇuḥ Dka_6a",
    "GR13devikaiu:282": " samadṛṣṭistu kartavyā Dka_75a",
    "GR13devikaiu:283": " samanindāpraśaṃsaśca Dka_74c",
    "GR13devikaiu:284": " samayācāraniḥśeṣān Dka_65a",
    "GR13devikaiu:285": " samo 'mitre ca mitre ca Dka_73a",
    "GR13devikaiu:286": " samo loṣṭe ca kāñcane Dka_73b",
    "GR13devikaiu:287": " samprāpte naiva cālayet Dka_39d",
    "GR13devikaiu:288": " sa yogī sa tapodhanaḥ Dka_6d",
    "GR13devikaiu:289": " sarvajñaḥ san prakāśate Dka_78d",
    "GR13devikaiu:290": " sarvatattvādyasambhinnaṃ Dka_28a",
    "GR13devikaiu:291": " sarvato vigataspṛhaḥ Dka_51d",
    "GR13devikaiu:292": " sarvatra yasmātsatataṃ vimuktaḥ Dka_53b",
    "GR13devikaiu:293": " sarvadvandvavinirmuktaḥ Dka_78a",
    "GR13devikaiu:294": " sarvabhūtalaye jāte Dka_36a",
    "GR13devikaiu:295": " sarvabhūtasamastathā Dka_74d",
    "GR13devikaiu:296": " sarvabhūtābhayapradaḥ Dka_5b",
    "GR13devikaiu:297": " sarvametat parityajet Dka_31d",
    "GR13devikaiu:298": " sarvametadabhīpsitam Dka_26b",
    "GR13devikaiu:299": " sarvametanna kartavyaṃ Dka_17c",
    "GR13devikaiu:300": " sarvaśūnyapade sthitvā Dka_27c",
    "GR13devikaiu:301": " sarvasmādbhinna evāhaṃ Dka_32c",
    "GR13devikaiu:302": " sarvākāraṃ nirākāraṃ Dka_20c",
    "GR13devikaiu:303": " sarvālambanavarjitam Dka_40b",
    "GR13devikaiu:304": " sarvālambanaśūnyaṃ ca Dka_41a",
    "GR13devikaiu:305": " sarvālambavinirmuktaṃ Dka_14c",
    "GR13devikaiu:306": " sarvāvastho 'pi yogadhṛk Dka_68b",
    "GR13devikaiu:307": " sarvāsaktiṃ visarjayet Dka_47d",
    "GR13devikaiu:308": " sarve te paśubandhāḥ syur Dka_67a",
    "GR13devikaiu:309": " sarveṣāmapi muktyarthaṃ Dka_1a",
    "GR13devikaiu:310": " sarvopādhivinirmuktaṃ Dka_47a",
    "GR13devikaiu:311": " saṃkalpaṃ ca vikalpaṃ ca Dka_65c",
    "GR13devikaiu:312": " saṃsārasya ca bandhanam Dka_43d",
    "GR13devikaiu:313": " saṃsārī sa bhavelloke Dka_24c",
    "GR13devikaiu:314": " sā citirmokṣadāyinī Dka_46d",
    "GR13devikaiu:315": " sāntarbahiścarmapaṭāvanaddham Dka_54b",
    "GR13devikaiu:316": " sā prajñā tadiha sthairyaṃ Dka_8c",
    "GR13devikaiu:317": " sāmprataṃ śṛṇu taṃ mayā Dka_61d",
    "GR13devikaiu:318": " siddhimicchanti sādhakāḥ Dka_79d",
    "GR13devikaiu:319": " siddhīśca vividhākārāḥ Dka_66a",
    "GR13devikaiu:320": " suṣuptirjāgṛtiścaiva Dka_31c",
    "GR13devikaiu:321": " so 'hamasmi na saṃśayaḥ Dka_52d",
    "GR13devikaiu:322": " stutinindākarāstasya Dka_84a",
    "GR13devikaiu:323": " sthirameva tu kārayet Dka_34d",
    "GR13devikaiu:324": " snānaṃ vastraṃ ca bhojanam Dka_82d",
    "GR13devikaiu:325": " svapnaśceti caturvidhaḥ Dka_31b",
    "GR13devikaiu:326": " svapnādyavasthācyutiniṣprapañcaḥ Dka_58d",
    "GR13devikaiu:327": " svabhāvaṃ bhāvayedbudhaḥ Dka_48d",
    "GR13devikaiu:328": " svayameva pralīyate Dka_30d",
    "GR13devikaiu:329": " svayameva praśāmyati Dka_30b",
    "GR13devikaiu:330": " svayameva vipatsyate Dka_45d",
    "GR13devikaiu:331": " svayaṃpatitapuṣpaistu Dka_71a",
    "GR13devikaiu:332": " svasaṃvedyaṃ virājate Dka_20d",
    "GR13devikaiu:333": " hṛtsaroje hyahaṃrūpā Dka_46a",
    "GR13devikaiu:334": " heturnāsti phalaṃ nāsti Dka_22a",
    "GR13devikaiu:335": " homo naiva ca sādhanam Dka_62b"
}