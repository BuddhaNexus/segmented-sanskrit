{
    "GSP33brahmsuu:0": " athāto brahmajijñāsā |",
    "GSP33brahmsuu:1": " janmādyasya yataḥ |",
    "GSP33brahmsuu:2": " śāstrayonitvāt |",
    "GSP33brahmsuu:3": " tat tu samanvayāt |",
    "GSP33brahmsuu:4": " īkṣater nāśabdam |",
    "GSP33brahmsuu:5": " gauṇaś cen nātmaśabdāt |",
    "GSP33brahmsuu:6": " tanniṣṭhasya mokṣopadeśāt |",
    "GSP33brahmsuu:7": " heyatvāvacanāc ca |",
    "GSP33brahmsuu:8": " pratijñāvirodhāt |",
    "GSP33brahmsuu:9": " svāpyayāt |",
    "GSP33brahmsuu:10": " gatisāmānyāt |",
    "GSP33brahmsuu:11": " śrutatvāc ca |",
    "GSP33brahmsuu:12": " ānandamayo 'bhyāsāt |",
    "GSP33brahmsuu:13": " vikāraśabdān neti cen na prācuryāt |",
    "GSP33brahmsuu:14": " taddhetuvyapadeśāc ca |",
    "GSP33brahmsuu:15": " māntravarṇikameva ca gīyate |",
    "GSP33brahmsuu:16": " netaro 'nupapatteḥ |",
    "GSP33brahmsuu:17": " bhedavyapadeśāc ca |",
    "GSP33brahmsuu:18": " kāmāc ca nānumānāpekṣā |",
    "GSP33brahmsuu:19": " asminn asya ca tadyogaṃ śāsti |",
    "GSP33brahmsuu:20": " antas taddharmopadeśāt |",
    "GSP33brahmsuu:21": " bhedavyapadeśāc cānyaḥ |",
    "GSP33brahmsuu:22": " ākāśas talliṅgāt |",
    "GSP33brahmsuu:23": " ata eva prāṇaḥ |",
    "GSP33brahmsuu:24": " jyotiś caraṇābhidhānāt |",
    "GSP33brahmsuu:25": " chando 'bhidhānān neti cen na tathā ceto'rpaṇanigadāt tathā hi darśanam |",
    "GSP33brahmsuu:26": " bhūtādipādavyapadeśopapatteś caivam |",
    "GSP33brahmsuu:27": " upadeśabhedān neti cen nobhayasminn apy avirodhāt |",
    "GSP33brahmsuu:28": " prāṇas tathānugamāt |",
    "GSP33brahmsuu:29": " na vaktur ātmopadeśād iti ced adhyātmasaṃbandhabhūmā hy asmin |",
    "GSP33brahmsuu:30": " śāstradṛṣṭyā tūpadeśo vāmadevavat |",
    "GSP33brahmsuu:31": " jīvamukhyaprāṇaliṅgān neti cen nopāsātraividhyādāśritatvād iha tadyogāt |",
    "GSP33brahmsuu:32": " sarvatra prasiddhopadeśāt |",
    "GSP33brahmsuu:33": " vivakṣitaguṇopapatteś ca |",
    "GSP33brahmsuu:34": " anupapattes tu na śārīraḥ |",
    "GSP33brahmsuu:35": " karmakartṛvyapadeśāc ca |",
    "GSP33brahmsuu:36": " śabdaviśeṣāt |",
    "GSP33brahmsuu:37": " smṛteś ca |",
    "GSP33brahmsuu:38": " arbhakaustvāt tadvyapadeśāc ca neti cen na nicāyyatvād evaṃ vyomavac ca |",
    "GSP33brahmsuu:39": " saṃbhogaprāptir iti cen na vaiśeṣyāt |",
    "GSP33brahmsuu:40": " attā carācaragrahaṇāt |",
    "GSP33brahmsuu:41": " prakaraṇāc ca |",
    "GSP33brahmsuu:42": " guhāṃ praviṣṭāv ātmānau hi taddarśanāt |",
    "GSP33brahmsuu:43": " viśeṣaṇāc ca |",
    "GSP33brahmsuu:44": " antara upapatteḥ |",
    "GSP33brahmsuu:45": " sthānādivyapadeśāc ca |",
    "GSP33brahmsuu:46": " sukhaviśiṣṭābhidhānād eva ca |",
    "GSP33brahmsuu:47": " ata eva ca sa brahma |",
    "GSP33brahmsuu:48": " śrutopaniṣatkagatyabhidhānāc ca |",
    "GSP33brahmsuu:49": " anavasthiter asaṃbhavāc ca netaraḥ |",
    "GSP33brahmsuu:50": " antaryāmyadhidaivādhilokādiṣu taddharmavyapadeśāt |",
    "GSP33brahmsuu:51": " na ca smārtam ataddharmābhilāpāc chārīraś ca |",
    "GSP33brahmsuu:52": " ubhaye 'pi hi bhedenainam adhīyate |",
    "GSP33brahmsuu:53": " adṛśyatvādiguṇako dharmokteḥ |",
    "GSP33brahmsuu:54": " viśeṣaṇabhedavyapadeśābhyāṃ ca netarau |",
    "GSP33brahmsuu:55": " rūpopanyāsāc ca |",
    "GSP33brahmsuu:56": " vaiśvānaraḥ sādhāraṇaśabdaviśeṣāt |",
    "GSP33brahmsuu:57": " smaryamāṇam anumānaṃ syād iti |",
    "GSP33brahmsuu:58": " śabdādibhyo 'ntaḥpratiṣṭhānāc ca neti cen na tathā dṛṣṭyupadeśād",
    "GSP33brahmsuu:59": " asambhavāt puruṣamapi cainam adhīyate |",
    "GSP33brahmsuu:60": " ata eva na devatā bhūtaṃ ca |",
    "GSP33brahmsuu:61": " sākṣād apy avirodhaṃ jaiminiḥ |",
    "GSP33brahmsuu:62": " abhivyakter ity āśmarathyaḥ |",
    "GSP33brahmsuu:63": " anusmṛter bādariḥ |",
    "GSP33brahmsuu:64": " saṃpatter iti jaiminis tathā hi darśayati |",
    "GSP33brahmsuu:65": " āmananti cainam asmin |",
    "GSP33brahmsuu:66": " dyubhvādyāyatanaṃ svaśabdāt |",
    "GSP33brahmsuu:67": " muktopasṛpyavyapadeśāc ca |",
    "GSP33brahmsuu:68": " nānumānam atacchabdāt prāṇabhṛc ca |",
    "GSP33brahmsuu:69": " bhedavyapadeśāt |",
    "GSP33brahmsuu:70": " prakaraṇāt |",
    "GSP33brahmsuu:71": " sthityadanābhyāṃ ca |",
    "GSP33brahmsuu:72": " bhūmā saṃprasādād adhyupadeśāt |",
    "GSP33brahmsuu:73": " dharmopapatteś ca |",
    "GSP33brahmsuu:74": " akṣaram ambarāntadhṛteḥ |",
    "GSP33brahmsuu:75": " sā ca praśāsanāt |",
    "GSP33brahmsuu:76": " anyabhāvavyāvṛtteśca |",
    "GSP33brahmsuu:77": " īkṣatikarmavyapadeśāt saḥ |",
    "GSP33brahmsuu:78": " dahara uttarebhyaḥ |",
    "GSP33brahmsuu:79": " gatiśabdābhyāṃ tathā hi dṛṣṭaṃ liṅgaṃ ca |",
    "GSP33brahmsuu:80": " dhṛteś ca mahimno 'syāsminn upalabdheḥ |",
    "GSP33brahmsuu:81": " prasiddheś ca |",
    "GSP33brahmsuu:82": " itaraparāmarśāt sa iti cen nāsaṃbhavāt |",
    "GSP33brahmsuu:83": " uttarāc ced āvirbhūtasvarūpas tu |",
    "GSP33brahmsuu:84": " anyārthaś ca parāmarśaḥ |",
    "GSP33brahmsuu:85": " alpaśruter iti cet tad uktam |",
    "GSP33brahmsuu:86": " anukṛtes tasya ca |",
    "GSP33brahmsuu:87": " api ca smaryate |",
    "GSP33brahmsuu:88": " śabdād eva pramitaḥ |",
    "GSP33brahmsuu:89": " hṛdyapekṣayā tu manuṣyādhikāratvāt |",
    "GSP33brahmsuu:90": " tadupary api bādarāyaṇaḥ saṃbhavāt |",
    "GSP33brahmsuu:91": " virodhaḥ karmaṇīti cen nānekapratipatter darśanāt |",
    "GSP33brahmsuu:92": " śabda iti cen nātaḥ prabhavāt pratyakṣānumānābhyām |",
    "GSP33brahmsuu:93": " ata eva ca nityatvam |",
    "GSP33brahmsuu:94": " samānanāmarūpatvāccāvṛttāvapyavirodho darśanāt smṛteś ca |",
    "GSP33brahmsuu:95": " madhvādiṣv asaṃbhavād anadhikāraṃ jaiminiḥ |",
    "GSP33brahmsuu:96": " jyotiṣi bhāvāc ca |",
    "GSP33brahmsuu:97": " bhāvaṃ tu bādarāyaṇo 'sti hi |",
    "GSP33brahmsuu:98": " śugasya tadanādaraśravaṇāt tadādravaṇāt sūcyate hi |",
    "GSP33brahmsuu:99": " kṣatriyatvagateś ca |",
    "GSP33brahmsuu:100": " uttaratra caitrarathena liṅgāt |",
    "GSP33brahmsuu:101": " saṃskāraparāmarśāt tadabhāvābhilāpāc ca |",
    "GSP33brahmsuu:102": " tadabhāvanirdhāraṇe ca pravṛtteḥ |",
    "GSP33brahmsuu:103": " śravaṇādhyayanārthapratiṣedhāt |",
    "GSP33brahmsuu:104": " smṛteś ca |",
    "GSP33brahmsuu:105": " kampanāt |",
    "GSP33brahmsuu:106": " jyotir darśanāt |",
    "GSP33brahmsuu:107": " ākāśo 'rthāntaratvādivyapadeśāt |",
    "GSP33brahmsuu:108": " suṣuptyutkrāntyor bhedena |",
    "GSP33brahmsuu:109": " patyādiśabdebhyaḥ |",
    "GSP33brahmsuu:110": " ānumānikam apy ekeṣām iti cen na śarīra-rūpaka-vinyasta-gṛhīter darśayati ca |",
    "GSP33brahmsuu:111": " sūkṣmaṃ tu tadarhatvāt |",
    "GSP33brahmsuu:112": " tadadhīnatvād arthavat |",
    "GSP33brahmsuu:113": " jñeyatvāvacanāc ca |",
    "GSP33brahmsuu:114": " vadatīti cen na prājño hi prakaraṇāt |",
    "GSP33brahmsuu:115": " trayāṇām eva caivam upanyāsaḥ praśnaś ca |",
    "GSP33brahmsuu:116": " mahadvac ca |",
    "GSP33brahmsuu:117": " camasavadaviśeṣāt |",
    "GSP33brahmsuu:118": " jyotirupakramā tu tathā hy adhīyata eke |",
    "GSP33brahmsuu:119": " kalpanopadeśāc ca madhvādivadavirodhaḥ |",
    "GSP33brahmsuu:120": " na saṃkhyopasaṃgrahādapi jñānābhāvād atirekāc ca |",
    "GSP33brahmsuu:121": " prāṇādayo vākyaśeṣāt |",
    "GSP33brahmsuu:122": " jyotiṣaikeṣām asatyanne |",
    "GSP33brahmsuu:123": " kāraṇatvena cākāśādiṣu yathāvyapadiṣṭokteḥ |",
    "GSP33brahmsuu:124": " samākarṣāt |",
    "GSP33brahmsuu:125": " jagadvācitvāt |",
    "GSP33brahmsuu:126": " jīvamukhyaprāṇaliṅgān neti cet tadvyākhyātam |",
    "GSP33brahmsuu:127": " anyārthaṃ tu jaiminiḥ praśnavyākhyānābhyām api caivam eke |",
    "GSP33brahmsuu:128": " vākyānvayāt |",
    "GSP33brahmsuu:129": " pratijñāsiddher liṅgam āśmarathyaḥ |",
    "GSP33brahmsuu:130": " utkramiṣyata evaṃ bhāvād ity auḍulomiḥ |",
    "GSP33brahmsuu:131": " avasthiter iti kāśakṛtsnaḥ |",
    "GSP33brahmsuu:132": " prakṛtiś ca pratijñādṛṣṭāntānuparodhāt |",
    "GSP33brahmsuu:133": " abhidhyopadeśāc ca |",
    "GSP33brahmsuu:134": " sākṣāc cobhayāmnānāt |",
    "GSP33brahmsuu:135": " ātmakṛteḥ |",
    "GSP33brahmsuu:136": " pariṇāmāt |",
    "GSP33brahmsuu:137": " yoniś ca hi gīyate |",
    "GSP33brahmsuu:138": " etena sarve vyākhyātā vyākhyātāḥ |",
    "GSP33brahmsuu:139": " smṛtyanavakāśadoṣaprasaṅga iti cen nānyasmṛtyanavakāśadoṣaprasaṅgāt |",
    "GSP33brahmsuu:140": " itareṣāṃ cānupalabdheḥ |",
    "GSP33brahmsuu:141": " etena yogaḥ pratyuktaḥ |",
    "GSP33brahmsuu:142": " na vilakṣaṇatvād asya tathātvaṃ ca śabdāt |",
    "GSP33brahmsuu:143": " abhimānivyapadeśas tu viśeṣānugatibhyām |",
    "GSP33brahmsuu:144": " dṛśyate tu |",
    "GSP33brahmsuu:145": " asad iti cen na pratiṣedhamātratvāt |",
    "GSP33brahmsuu:146": " apītau tadvatprasaṅgād asamañjasam |",
    "GSP33brahmsuu:147": " na tu dṛṣṭāntabhāvāt |",
    "GSP33brahmsuu:148": " svapakṣadoṣāc ca |",
    "GSP33brahmsuu:149": " tarkāpratiṣṭhānād api |",
    "GSP33brahmsuu:150": " anyathānumeyam iti ced evam apy anirmokṣaprasaṅgaḥ |",
    "GSP33brahmsuu:151": " etena śiṣṭāparigrahā api vyākhyātāḥ |",
    "GSP33brahmsuu:152": " bhoktrāpatter avibhāgaś cet syāl lokavat |",
    "GSP33brahmsuu:153": " tadananyatvam ārambhaṇaśabdādibhyaḥ |",
    "GSP33brahmsuu:154": " bhāve copalabdheḥ |",
    "GSP33brahmsuu:155": " satvāc cāparasya |",
    "GSP33brahmsuu:156": " asadvyapadeśān neti cen na dharmāntareṇa vākyaśeṣādyukteḥ śabdāntarāc ca |",
    "GSP33brahmsuu:157": " paṭavac ca |",
    "GSP33brahmsuu:158": " yathā ca prāṇādiḥ |",
    "GSP33brahmsuu:159": " itaravyapadeśād dhitākaraṇādidoṣaprasaktiḥ |",
    "GSP33brahmsuu:160": " adhikaṃ tu bhedanirdeśāt |",
    "GSP33brahmsuu:161": " aśmādivac ca tadanupapattiḥ |",
    "GSP33brahmsuu:162": " upasaṃhāradarśanān neti cen na kṣīravad dhi |",
    "GSP33brahmsuu:163": " devādivad api loke |",
    "GSP33brahmsuu:164": " kṛtsnaprasaktir niravayavatvaśabdakopo vā |",
    "GSP33brahmsuu:165": " śrutes tu śabdamūlatvāt |",
    "GSP33brahmsuu:166": " ātmani caivaṃ vicitrāś ca hi |",
    "GSP33brahmsuu:167": " svapakṣadoṣāc ca |",
    "GSP33brahmsuu:168": " sarvopetā ca taddarśanāt |",
    "GSP33brahmsuu:169": " vikaraṇatvān neti cet tad uktam |",
    "GSP33brahmsuu:170": " na prayojanavattvāt |",
    "GSP33brahmsuu:171": " lokavat tu līlākaivalyam |",
    "GSP33brahmsuu:172": " vaiṣamyanairghṛṇye na sāpekṣatvāt tathā hi darśayati |",
    "GSP33brahmsuu:173": " na karmāvibhāgād iti cen nānāditvād upapadyate cāpy upalabhyate ca |",
    "GSP33brahmsuu:174": " sarvadharmopapatteś ca |",
    "GSP33brahmsuu:175": " racanānupapatteś ca nānumānaṃ pravṛtteś ca |",
    "GSP33brahmsuu:176": " payo 'mbuvac cet tatrāpi |",
    "GSP33brahmsuu:177": " vyatirekānavasthiteś cānapekṣatvāt |",
    "GSP33brahmsuu:178": " anyatrābhāvāc ca na tṛṇādivat |",
    "GSP33brahmsuu:179": " puruṣāśmavad iti cet tathāpi |",
    "GSP33brahmsuu:180": " aṅgitvānupapatteś ca |",
    "GSP33brahmsuu:181": " anyathānumitau ca jñaśaktiviyogāt |",
    "GSP33brahmsuu:182": " abhyupagame 'py arthābhāvāt |",
    "GSP33brahmsuu:183": " vipratiṣedhāc cāsamañjasam |",
    "GSP33brahmsuu:184": " mahaddīrghavad vā hrasvaparimaṇḍalābhyām |",
    "GSP33brahmsuu:185": " ubhayathāpi na karmātastadabhāvaḥ |",
    "GSP33brahmsuu:186": " samavāyābhyupagamāc ca sāmyād anavasthiteḥ |",
    "GSP33brahmsuu:187": " nityam eva ca bhāvāt |",
    "GSP33brahmsuu:188": " rūpādimattvāc ca viparyayo darśanāt |",
    "GSP33brahmsuu:189": " ubhayathā ca doṣāt |",
    "GSP33brahmsuu:190": " aparigrahāc cātyantam anapekṣā |",
    "GSP33brahmsuu:191": " samudāya ubhayahetuke 'pi tadaprāptiḥ |",
    "GSP33brahmsuu:192": " itaretarapratyayatvād upapannam iti cen na saṅghātabhāvānimittatvāt |",
    "GSP33brahmsuu:193": " uttarotpāde ca pūrvanirodhāt |",
    "GSP33brahmsuu:194": " asati pratijñoparodho yaugapadyamanyathā |",
    "GSP33brahmsuu:195": " pratisaṃkhyāpratisaṃkhyānirodhāprāptir avicchedāt |",
    "GSP33brahmsuu:196": " ubhayathā ca doṣāt |",
    "GSP33brahmsuu:197": " ākāśe cāviśeṣāt |",
    "GSP33brahmsuu:198": " anusmṛteś ca |",
    "GSP33brahmsuu:199": " nāsato 'dṛṣṭatvāt |",
    "GSP33brahmsuu:200": " udāsīnānām api caivaṃ siddhiḥ |",
    "GSP33brahmsuu:201": " nābhāva upalabdheḥ |",
    "GSP33brahmsuu:202": " vaidharmyāc ca na svapnādivat |",
    "GSP33brahmsuu:203": " na bhāvo 'nupalabdheḥ |",
    "GSP33brahmsuu:204": " sarvathānupapatteś ca |",
    "GSP33brahmsuu:205": " naikasminn asambhavāt |",
    "GSP33brahmsuu:206": " evaṃ cātmākārtsnyam |",
    "GSP33brahmsuu:207": " na ca paryāyād apy avirodho vikārādibhyaḥ |",
    "GSP33brahmsuu:208": " antyāvasthiteś cobhayanityatvād aviśeṣaḥ |",
    "GSP33brahmsuu:209": " patyur asāmañjasyāt |",
    "GSP33brahmsuu:210": " adhiṣṭhānānupapatteś ca |",
    "GSP33brahmsuu:211": " karaṇavac cen na bhogādibhyaḥ |",
    "GSP33brahmsuu:212": " antavattvam asarvajñatā vā |",
    "GSP33brahmsuu:213": " utpattyasaṃbhavāt |",
    "GSP33brahmsuu:214": " na ca kartuḥ karaṇam |",
    "GSP33brahmsuu:215": " vijñānādibhāve vā tadapratiṣedhaḥ |",
    "GSP33brahmsuu:216": " vipratiṣedhāc ca |",
    "GSP33brahmsuu:218": " asti tu |",
    "GSP33brahmsuu:219": " gauṇyasaṃbhavāc chabdāc ca |",
    "GSP33brahmsuu:220": " syāc caikasya brahmaśabdavat |",
    "GSP33brahmsuu:221": " pratijñāhānir avyatirekāt |",
    "GSP33brahmsuu:222": " śabdebhyaḥ |",
    "GSP33brahmsuu:223": " yāvadvikāraṃ tu vibhāgo lokavat |",
    "GSP33brahmsuu:224": " etena mātariśvā vyākhyātaḥ |",
    "GSP33brahmsuu:225": " asaṃbhavas tu sato 'nupapatteḥ |",
    "GSP33brahmsuu:226": " tejo 'tas tathā hy āha |",
    "GSP33brahmsuu:227": " āpaḥ |",
    "GSP33brahmsuu:228": " pṛthivī |",
    "GSP33brahmsuu:229": " adhikārarūpaśabdāntarebhyaḥ |",
    "GSP33brahmsuu:230": " tadabhidhyānād eva tu talliṅgāt saḥ |",
    "GSP33brahmsuu:231": " viparyayeṇa tu kramo 'ta upapadyate ca |",
    "GSP33brahmsuu:232": " antarā vijñānamanasī krameṇa talliṅgād iti cen nāviśeṣāt |",
    "GSP33brahmsuu:233": " carācaravyapāśrayas tu syāt tadvyapadeśo bhāktas tadbhāvabhāvitvāt |",
    "GSP33brahmsuu:234": " nātmā śruter nityatvāc ca tābhyaḥ |",
    "GSP33brahmsuu:235": " jño 'ta eva |",
    "GSP33brahmsuu:236": " utkrāntigatyāgatīnām |",
    "GSP33brahmsuu:237": " svātmanā cottarayoḥ |",
    "GSP33brahmsuu:238": " nāṇuratacchruter iti cen netarādhikārāt |",
    "GSP33brahmsuu:239": " svaśabdonmānābhyāṃ ca |",
    "GSP33brahmsuu:240": " avirodhaś candanavat |",
    "GSP33brahmsuu:241": " avasthitivaiśeṣyād iti cen nābhyupagamād dhṛdi hi |",
    "GSP33brahmsuu:242": " guṇādvā lokavat |",
    "GSP33brahmsuu:243": " vyatireko gandhavat tathā hi darśayati |",
    "GSP33brahmsuu:244": " pṛthagupadeśāt |",
    "GSP33brahmsuu:245": " tadguṇasāratvāt tu tadvyapadeśaḥ prājñavat |",
    "GSP33brahmsuu:246": " yāvadātmabhāvitvāc ca na doṣas taddarśanāt |",
    "GSP33brahmsuu:247": " puṃstvādivat tv asya sato 'bhivyaktiyogāt |",
    "GSP33brahmsuu:248": " nityopalabdhyanupalabdhiprasaṅgo 'nyataraniyamo vānyathā |",
    "GSP33brahmsuu:249": " kartā śāstrārthavattvāt |",
    "GSP33brahmsuu:250": " upādānād vihāropadeśāc ca |",
    "GSP33brahmsuu:251": " vyapadeśāc ca kriyāyāṃ na cen nirdeśaviparyayaḥ |",
    "GSP33brahmsuu:252": " upalabdhivadaniyamaḥ |",
    "GSP33brahmsuu:253": " śaktiviparyayāt |",
    "GSP33brahmsuu:254": " samādhyabhāvāc ca |",
    "GSP33brahmsuu:255": " yathā ca takṣobhayathā |",
    "GSP33brahmsuu:256": " parāt tu tacchruteḥ |",
    "GSP33brahmsuu:257": " kṛtaprayatnāpekṣas tu vihitapratiṣiddhāvaiyarthyādibhyaḥ |",
    "GSP33brahmsuu:258": " aṃśo nānāvyapadeśād anyathā cāpi dāśakitavāditvam adhīyata eke |",
    "GSP33brahmsuu:259": " mantravarṇāt |",
    "GSP33brahmsuu:260": " api smaryate |",
    "GSP33brahmsuu:261": " prakāśādivat tu naivaṃ paraḥ |",
    "GSP33brahmsuu:262": " smaranti ca |",
    "GSP33brahmsuu:263": " anujñāparihārau dehasambandhāj jyotirādivat |",
    "GSP33brahmsuu:264": " asantateś cāvyatikaraḥ |",
    "GSP33brahmsuu:265": " ābhāsa eva ca |",
    "GSP33brahmsuu:266": " adṛṣṭāniyamāt |",
    "GSP33brahmsuu:267": " abhisandhyādiṣv api caivam |",
    "GSP33brahmsuu:268": " pradeśabhedād iti cen nāntarbhāvāt |",
    "GSP33brahmsuu:269": " tathā prāṇāḥ |",
    "GSP33brahmsuu:270": " gauṇyasaṃbhavāt tatprāk śruteś ca |",
    "GSP33brahmsuu:271": " tatpūrvakatvād vācaḥ |",
    "GSP33brahmsuu:272": " sapta gater viśeṣitatvāc ca |",
    "GSP33brahmsuu:273": " hastādayas tu sthite 'to naivam |",
    "GSP33brahmsuu:274": " aṇavaś ca |",
    "GSP33brahmsuu:275": " śreṣṭhaś ca |",
    "GSP33brahmsuu:276": " na vāyukriye pṛthagupadeśāt |",
    "GSP33brahmsuu:277": " cakṣurādivat tu tatsahaśiṣṭyādibhyaḥ |",
    "GSP33brahmsuu:278": " akaraṇatvāc ca na doṣas tathā hi darśayati |",
    "GSP33brahmsuu:279": " pañcavṛttir manovat vyapadiśyate |",
    "GSP33brahmsuu:280": " aṇuś ca |",
    "GSP33brahmsuu:281": " jyotir ādyadhiṣṭhānaṃ tu tadāmananātprāṇavatā śabdāt |",
    "GSP33brahmsuu:282": " tasya ca nityatvāt |",
    "GSP33brahmsuu:283": " ta indriyāṇi tadvyapadeśād anyatra śreṣṭhāt |",
    "GSP33brahmsuu:284": " bhedaśruter vailakṣaṇyāc ca |",
    "GSP33brahmsuu:285": " saṃjñāmūrtikḷptis tu trivṛtkurvata upadeśāt |",
    "GSP33brahmsuu:286": " māṃsādi bhaumaṃ yathāśabdamitarayoś ca |",
    "GSP33brahmsuu:287": " vaiśeṣyāt tu tadvādas tadvādaḥ |",
    "GSP33brahmsuu:288": " tadantarapratipattau raṃhati saṃpariṣvaktaḥ praśnanirūpaṇābhyām |",
    "GSP33brahmsuu:289": " tryātmakatvāt tu bhūyastvāt |",
    "GSP33brahmsuu:290": " prāṇagateś ca |",
    "GSP33brahmsuu:291": " agnyādiśruter iti cen na bhāktatvāt |",
    "GSP33brahmsuu:292": " prathame 'śravaṇād iti cen na tā eva hy upapatteḥ |",
    "GSP33brahmsuu:293": " aśrutatvād iti cen neṣṭādikāriṇāṃ pratīteḥ |",
    "GSP33brahmsuu:294": " bhāktaṃ vānātmavittvāt tathā hi darśayati |",
    "GSP33brahmsuu:295": " kṛtātyaye 'nuśayavān dṛṣṭasmṛtibhyāṃ yathetamanevaṃ ca |",
    "GSP33brahmsuu:296": " caraṇād iti cen na tadupalakṣaṇārtheti kārṣṇājiniḥ |",
    "GSP33brahmsuu:297": " ānarthakyam iti cen na tadapekṣatvāt |",
    "GSP33brahmsuu:298": " sukṛtaduṣkṛte eveti tu bādariḥ |",
    "GSP33brahmsuu:299": " aniṣṭādikāriṇām api ca śrutam |",
    "GSP33brahmsuu:300": " saṃyamane tv anubhūyetareṣāmārohāv arohau tadgatidarśanāt |",
    "GSP33brahmsuu:301": " smaranti ca |",
    "GSP33brahmsuu:302": " api sapta |",
    "GSP33brahmsuu:303": " tatrāpi tadvyāpārādavirodhaḥ |",
    "GSP33brahmsuu:304": " vidyākarmaṇor iti tu prakṛtatvāt |",
    "GSP33brahmsuu:305": " na tṛtīye tathopalabdheḥ |",
    "GSP33brahmsuu:306": " smaryate 'pi ca loke |",
    "GSP33brahmsuu:307": " darśanāc ca |",
    "GSP33brahmsuu:308": " tṛtīyaśabdāvarodhaḥ saṃśokajasya |",
    "GSP33brahmsuu:309": " tatsvābhāvyāpattirupapatteḥ |",
    "GSP33brahmsuu:310": " nāticireṇa viśeṣāt |",
    "GSP33brahmsuu:311": " anyādhiṣṭhite pūrvavadabhilāpāt |",
    "GSP33brahmsuu:312": " aśuddham iti cen na śabdāt |",
    "GSP33brahmsuu:313": " retaḥsigyogo 'tha |",
    "GSP33brahmsuu:314": " yoneḥśarīram |",
    "GSP33brahmsuu:315": " sandhye sṛṣṭirāha hi |",
    "GSP33brahmsuu:316": " nirmātāraṃ caike putrādayaś ca |",
    "GSP33brahmsuu:317": " māyāmātraṃ tu kārtsnyenānabhivyaktasvarūpatvāt |",
    "GSP33brahmsuu:318": " parābhidhyānāt tu tirohitaṃ tato hyasya bandhaviparyayau |",
    "GSP33brahmsuu:319": " dehayogādvā so 'pi |",
    "GSP33brahmsuu:320": " sūcakaś ca hi śruterācakṣate ca tadvidaḥ |",
    "GSP33brahmsuu:321": " tadabhāvo nāḍīṣu tacchruterātmani ca |",
    "GSP33brahmsuu:322": " ataḥ prabodho 'smāt |",
    "GSP33brahmsuu:323": " sa eva tu karmānusmṛtiśabdavidhibhyaḥ |",
    "GSP33brahmsuu:324": " mugdher'dhasaṃpattiḥ pariśeṣāt |",
    "GSP33brahmsuu:325": " na sthānato 'pi parasyobhayaliṅgaṃ sarvatra hi |",
    "GSP33brahmsuu:326": " bhedād iti cen na pratyekamatadvacanāt |",
    "GSP33brahmsuu:327": " api caivam eke |",
    "GSP33brahmsuu:328": " arūpavadeva hi tatpradhānatvāt |",
    "GSP33brahmsuu:329": " prakāśavaccāvaiyarthyāt |",
    "GSP33brahmsuu:330": " āha ca tanmātram |",
    "GSP33brahmsuu:331": " darśayati cātho api smaryate |",
    "GSP33brahmsuu:332": " ata eva copamā sūryakādivat |",
    "GSP33brahmsuu:333": " ambuvadagrahaṇāt tu na tathātvam |",
    "GSP33brahmsuu:334": " vṛddhihrāsabhāktvamantarbhāvādubhayasāmañjasyādevaṃ darśanāc ca |",
    "GSP33brahmsuu:335": " prakṛtaitāvattvaṃ hi pratiṣedhati tato bravīti ca bhūyaḥ |",
    "GSP33brahmsuu:336": " tadavyaktamāha hi |",
    "GSP33brahmsuu:337": " api saṃrādhane pratyakṣānumānābhyām |",
    "GSP33brahmsuu:338": " prakāśādivaccāvaiśeṣyaṃ prakāśaś ca karmaṇyabhyāsāt |",
    "GSP33brahmsuu:339": " ato 'nantena tathā hi liṅgam |",
    "GSP33brahmsuu:340": " ubhayavyapadeśāttvahikuṇḍalavat |",
    "GSP33brahmsuu:341": " prakāśāśrayavadvā tejastvāt |",
    "GSP33brahmsuu:342": " pūrvavadvā |",
    "GSP33brahmsuu:343": " pratiṣedhāc ca |",
    "GSP33brahmsuu:344": " paramatassetūnmānasaṃbandhabhedavyapadeśebhyaḥ |",
    "GSP33brahmsuu:345": " sāmānyāt tu |",
    "GSP33brahmsuu:346": " buddhyarthaḥ pādavat |",
    "GSP33brahmsuu:347": " sthānaviśeṣātprakāśādivat |",
    "GSP33brahmsuu:348": " upapatteś ca |",
    "GSP33brahmsuu:349": " tathānyapratiṣedhāt |",
    "GSP33brahmsuu:350": " anena sarvagatatvamāyāmaśabdādibhyaḥ |",
    "GSP33brahmsuu:351": " phalamata upapatteḥ |",
    "GSP33brahmsuu:352": " śrutatvāc ca |",
    "GSP33brahmsuu:353": " dharmaṃ jaiminirata eva |",
    "GSP33brahmsuu:354": " pūrvaṃ tu bādarāyaṇo hetuvyapadeśāt |",
    "GSP33brahmsuu:355": " sarvavedāntapratyayaṃ codanādyaviśeṣāt |",
    "GSP33brahmsuu:356": " bhedān neti ced ekasyām api |",
    "GSP33brahmsuu:357": " svādhyāyasya tathātve hi samācāre 'dhikārāc ca savavac ca tanniyamaḥ |",
    "GSP33brahmsuu:358": " darśayati ca |",
    "GSP33brahmsuu:359": " upasaṃhāror'thābhedādvidhiśeṣavatsamāne ca |",
    "GSP33brahmsuu:360": " anyathātvaṃ śabdād iti cen nāviśeṣāt |",
    "GSP33brahmsuu:361": " na vā prakaraṇabhedāt parovarīyastvādivat |",
    "GSP33brahmsuu:362": " saṃjñātaś cet tad uktam asti tu tad api |",
    "GSP33brahmsuu:363": " vyāpteś ca samañjasam |",
    "GSP33brahmsuu:364": " sarvābhedādanyatreme |",
    "GSP33brahmsuu:365": " ānandādayaḥ pradhānasya |",
    "GSP33brahmsuu:366": " priyaśirastvādyaprāptirupacayāpacayau hi bhede |",
    "GSP33brahmsuu:367": " itare tvarthasāmānyāt |",
    "GSP33brahmsuu:368": " ādhyānāya prayojanābhāvāt |",
    "GSP33brahmsuu:369": " ātmaśabdāc ca |",
    "GSP33brahmsuu:370": " ātmagṛhītir itaravad uttarāt |",
    "GSP33brahmsuu:371": " anvayād iti cet syād avadhāraṇāt |",
    "GSP33brahmsuu:372": " kāryākhyānādapūrvam |",
    "GSP33brahmsuu:373": " samāna evaṃ cābhedāt |",
    "GSP33brahmsuu:374": " sambandhādevamanyatrāpi |",
    "GSP33brahmsuu:375": " na vā viśeṣāt |",
    "GSP33brahmsuu:376": " darśayati ca |",
    "GSP33brahmsuu:377": " saṃbhṛtidyuvyāptyapi cātaḥ |",
    "GSP33brahmsuu:378": " puruṣavidyāyāmapi cetareṣāmanāmnānāt |",
    "GSP33brahmsuu:379": " vedhādyarthabhedāt |",
    "GSP33brahmsuu:380": " hānau tūpāyanaśabdaśeṣatvāt kuśācchandasstutyupagānavattaduktam |",
    "GSP33brahmsuu:381": " sāṃparāye tartavyābhāvāt tathā hy anye |",
    "GSP33brahmsuu:382": " chandata ubhayāvirodhāt |",
    "GSP33brahmsuu:383": " gater arthavattvam ubhayathānyathā hi virodhaḥ |",
    "GSP33brahmsuu:384": " upapannas tallakṣaṇārthopalabdher lokavat |",
    "GSP33brahmsuu:385": " yāvadadhikāram avasthitir ādhikārikāṇām |",
    "GSP33brahmsuu:386": " aniyamassarveṣāmavirodhaśśabdānumānābhyām |",
    "GSP33brahmsuu:387": " akṣaradhiyāṃ tvavarodhassāmānyatadbhāvābhyāmaupasadavattaduktam |",
    "GSP33brahmsuu:388": " iyadāmananāt |",
    "GSP33brahmsuu:389": " antarā bhūtagrāmavatsvātmano 'nyathā bhedānupapattir iti cen nopadeśavat |",
    "GSP33brahmsuu:390": " vyatihāro viśiṃṣanti hītaravat |",
    "GSP33brahmsuu:391": " saiva hi satyādayaḥ |",
    "GSP33brahmsuu:392": " kāmādītaratra tatra cā'yatanādibhyaḥ |",
    "GSP33brahmsuu:393": " ādarādalopaḥ |",
    "GSP33brahmsuu:394": " upasthite 'tastadvacanāt |",
    "GSP33brahmsuu:395": " tannirdhāraṇāniyamastaddṛṣṭeḥ pṛthagghyapratibandhaḥ phalam |",
    "GSP33brahmsuu:396": " pradānavadeva taduktam |",
    "GSP33brahmsuu:397": " liṅgabhūyastvāttaddhi balīyastadapi |",
    "GSP33brahmsuu:398": " pūrvavikalpaḥ prakaraṇātsyāt kriyāmānasavat |",
    "GSP33brahmsuu:399": " atideśāc ca |",
    "GSP33brahmsuu:400": " vidyaiva tu nirdhāraṇāddarśanāc ca |",
    "GSP33brahmsuu:401": " śrutyādibalīyastvāc ca na bādhaḥ |",
    "GSP33brahmsuu:402": " anubandhādibhyaḥ prajñāntarapṛthaktvavaddṛṣṭaś ca taduktam |",
    "GSP33brahmsuu:403": " na sāmānyādapyupalabdhermṛtyuvanna hi lokāpattiḥ |",
    "GSP33brahmsuu:404": " pareṇa ca śabdasya tādvidhyaṃ bhūyastvāt tv anubandhaḥ |",
    "GSP33brahmsuu:405": " eka ātmanaḥ śarīre bhāvāt |",
    "GSP33brahmsuu:406": " vyatirekastadbhāvabhāvitvānna tūpalabdhivat |",
    "GSP33brahmsuu:407": " aṅgāvabaddhāstu na śākhāsu hi prativedam |",
    "GSP33brahmsuu:408": " mantrādivadvāvirodhaḥ |",
    "GSP33brahmsuu:409": " bhūmnaḥ kratuvajjyāyasvaṃ tathā hi darśayati |",
    "GSP33brahmsuu:410": " nānā śabdādibhedāt |",
    "GSP33brahmsuu:411": " vikalpo 'viśiṣṭaphalatvāt |",
    "GSP33brahmsuu:412": " kāmyāstu yathākāmaṃ samuccīyeranna vā pūrvahetvabhāvāt |",
    "GSP33brahmsuu:413": " aṅgeṣu yathāśrayabhāvaḥ |",
    "GSP33brahmsuu:414": " śiṣṭeś ca |",
    "GSP33brahmsuu:415": " samāhārāt |",
    "GSP33brahmsuu:416": " guṇasādhāraṇyaśruteś ca |",
    "GSP33brahmsuu:417": " na vā tatsahabhāvāśruteḥ |",
    "GSP33brahmsuu:418": " darśanāc ca |",
    "GSP33brahmsuu:419": " puruṣārtho 'taḥ śabdād iti bādarāyaṇaḥ |",
    "GSP33brahmsuu:420": " śeṣatvātpuruṣārthavādo yathānyeṣv iti jaiminiḥ |",
    "GSP33brahmsuu:421": " ācāradarśanāt |",
    "GSP33brahmsuu:422": " tacchruteḥ |",
    "GSP33brahmsuu:423": " samanvārambhaṇāt |",
    "GSP33brahmsuu:424": " tadvato vidhānāt |",
    "GSP33brahmsuu:425": " niyamāt |",
    "GSP33brahmsuu:426": " adhikopadeśāt tu bādarāyaṇasyaivaṃ taddarśanāt |",
    "GSP33brahmsuu:427": " tulyaṃ tu darśanam |",
    "GSP33brahmsuu:428": " asārvatrikī |",
    "GSP33brahmsuu:429": " vibhāgaḥ śatavat |",
    "GSP33brahmsuu:430": " adhyayanamātravataḥ |",
    "GSP33brahmsuu:431": " nāviśeṣāt |",
    "GSP33brahmsuu:432": " stutaye 'numatirvā |",
    "GSP33brahmsuu:433": " kāmakāreṇa caike |",
    "GSP33brahmsuu:434": " upamardaṃ ca |",
    "GSP33brahmsuu:435": " ūrdhvaretassu ca śabde hi |",
    "GSP33brahmsuu:436": " parāmarśaṃ jaiminiracodanāccāpavadati hi |",
    "GSP33brahmsuu:437": " anuṣṭheyaṃ bādarāyaṇassāmyaśruteḥ |",
    "GSP33brahmsuu:438": " vidhir vā dhāraṇavat |",
    "GSP33brahmsuu:439": " stutimātram upādānād iti cen nāpūrvatvāt |",
    "GSP33brahmsuu:440": " bhāvaśabdāc ca |",
    "GSP33brahmsuu:441": " pāriplavārthā iti cen na viśeṣitatvāt |",
    "GSP33brahmsuu:442": " tathā caikavākyopabandhāt |",
    "GSP33brahmsuu:443": " ata eva cāgnīndhanādyanapekṣā |",
    "GSP33brahmsuu:444": " sarvāpekṣā ca yajñādiśruter aśvavat |",
    "GSP33brahmsuu:445": " śamadamādyupetas syāt tathāpi tu tadvidhes tadaṅgatayā teṣām apy avaśyānuṣṭheyatvāt |",
    "GSP33brahmsuu:446": " sarvān nānumatiś ca prāṇātyaye taddarśanāt |",
    "GSP33brahmsuu:447": " abādhāc ca |",
    "GSP33brahmsuu:448": " api smaryate |",
    "GSP33brahmsuu:449": " śabdaś cāto 'kāmakāre |",
    "GSP33brahmsuu:450": " vihitatvāc cā'śramakarmāpi |",
    "GSP33brahmsuu:451": " sahakāritvena ca |",
    "GSP33brahmsuu:452": " sarvathāpi ta evobhayaliṅgāt |",
    "GSP33brahmsuu:453": " anabhibhavaṃ ca darśayati |",
    "GSP33brahmsuu:454": " antarā cāpi tu taddṛṣṭeḥ |",
    "GSP33brahmsuu:455": " api smaryate |",
    "GSP33brahmsuu:456": " viśeṣānugrahaś ca |",
    "GSP33brahmsuu:457": " atas tv itarajjyāyo liṅgāc ca |",
    "GSP33brahmsuu:458": " tadbhūtasya tu nātadbhāvo jaiminer api niyamāt tadrūpābhāvebhyaḥ |",
    "GSP33brahmsuu:459": " na cādhikārikam api patanānumānāt tadayogāt |",
    "GSP33brahmsuu:460": " upapūrvam apīty eke bhāvamaśanavat tad uktam |",
    "GSP33brahmsuu:461": " bahis tūbhayathāpi smṛter ācārāc ca |",
    "GSP33brahmsuu:462": " svāminaḥ phalaśruter ity ātreyaḥ |",
    "GSP33brahmsuu:463": " ārtvijyam ity auḍulomiḥ tasmai hi parikrīyate |",
    "GSP33brahmsuu:464": " sahakāryantaravidhiḥ pakṣeṇa tṛtīyaṃ tadvato vidhyādivat |",
    "GSP33brahmsuu:465": " kṛtsnabhāvāt tu gṛhiṇopasaṃhāraḥ |",
    "GSP33brahmsuu:466": " maunavad itareṣām apy upadeśāt |",
    "GSP33brahmsuu:467": " anāviṣkurvann anvayāt |",
    "GSP33brahmsuu:468": " aihikam aprastutapratibandhe taddarśanāt |",
    "GSP33brahmsuu:469": " evaṃ muktiphalāniyamas tadavasthāvadhṛtes tadavasthāvadhṛteḥ |",
    "GSP33brahmsuu:470": " āvṛttir asakṛdupadeśāt |",
    "GSP33brahmsuu:471": " liṅgāc ca |",
    "GSP33brahmsuu:472": " ātmeti tūpagacchanti grāhayanti ca |",
    "GSP33brahmsuu:473": " na pratīke na hi saḥ |",
    "GSP33brahmsuu:474": " brahmadṛṣṭir utkarṣāt |",
    "GSP33brahmsuu:475": " ādityādimatayaś cāṅga upapatteḥ |",
    "GSP33brahmsuu:476": " āsīnaḥ saṃbhavāt |",
    "GSP33brahmsuu:477": " dhyānāc ca |",
    "GSP33brahmsuu:478": " acalatvaṃ cāpekṣya |",
    "GSP33brahmsuu:479": " smaranti ca |",
    "GSP33brahmsuu:480": " yatraikāgratā tatrāviśeṣāt |",
    "GSP33brahmsuu:481": " āprayāṇāt tatrāpi hi dṛṣṭam |",
    "GSP33brahmsuu:482": " tadadhigama uttarapūrvāghayor aśleṣavināśau tadvyapadeśāt |",
    "GSP33brahmsuu:483": " itarasyāpy evam asaṃśleṣaḥ pāte tu |",
    "GSP33brahmsuu:484": " anārabdhakārye eva tu pūrve tadavadheḥ |",
    "GSP33brahmsuu:485": " agnihotrādi tu tatkāryāyaiva taddarśanāt |",
    "GSP33brahmsuu:486": " ato 'nyāpi hy ekeṣām ubhayoḥ |",
    "GSP33brahmsuu:487": " yad eva vidyayeti hi |",
    "GSP33brahmsuu:488": " bhogena tv itare kṣapayitvātha saṃpadyate |",
    "GSP33brahmsuu:489": " vāṅmanasi darśanāc chabdāc ca |",
    "GSP33brahmsuu:490": " ata eva sarvāṇyanu |",
    "GSP33brahmsuu:491": " tanmanaḥ prāṇa uttarāt |",
    "GSP33brahmsuu:492": " so 'dhyakṣe tadupagamādibhyaḥ |",
    "GSP33brahmsuu:493": " bhūteṣu tacchruteḥ |",
    "GSP33brahmsuu:494": " naikasmin darśayato hi |",
    "GSP33brahmsuu:495": " samānā cāsṛtyupakramād amṛtatvaṃ cānupoṣya |",
    "GSP33brahmsuu:496": " tadāpīteḥ saṃsāravyapadeśāt |",
    "GSP33brahmsuu:497": " sūkṣmaṃ pramāṇataś ca tathopalabdheḥ |",
    "GSP33brahmsuu:498": " nopamardenātaḥ |",
    "GSP33brahmsuu:499": " asyaiva copapatter ūṣmā |",
    "GSP33brahmsuu:500": " pratiṣedhād iti cen na śārīrāt spaṣṭo hyekeṣām |",
    "GSP33brahmsuu:501": " smaryate ca |",
    "GSP33brahmsuu:502": " tāni pare tathā hy āha |",
    "GSP33brahmsuu:503": " avibhāgo vacanāt |",
    "GSP33brahmsuu:504": " tadokograjvalanaṃ tatprakāśitadvāro vidyāsāmarthyāt taccheṣagatyanusmṛtiyogāc |",
    "GSP33brahmsuu:505": " raśmyanusārī |",
    "GSP33brahmsuu:506": " niśi neti cen na sambandhasya yāvaddehabhāvitvād darśayati ca |",
    "GSP33brahmsuu:507": " ataś cāyane 'pi dakṣiṇe |",
    "GSP33brahmsuu:508": " yoginaḥ prati smaryete smārte caite |",
    "GSP33brahmsuu:509": " arcirādinā tatprathiteḥ |",
    "GSP33brahmsuu:510": " vāyumabdādaviśeṣaviśeṣābhyām |",
    "GSP33brahmsuu:511": " taṭito 'dhi varuṇaḥ saṃbandhāt |",
    "GSP33brahmsuu:512": " ātivāhikās talliṅgāt |",
    "GSP33brahmsuu:513": " vaidyutenaiva tatas tacchruteḥ |",
    "GSP33brahmsuu:514": " kāryaṃ bādarirasya gatyupapatteḥ |",
    "GSP33brahmsuu:515": " viśeṣitatvāc ca |",
    "GSP33brahmsuu:516": " sāmīpyāt tu tadvyapadeśaḥ |",
    "GSP33brahmsuu:517": " kāryātyaye tadadhyakṣeṇa sahātaḥ param abhidhānāt |",
    "GSP33brahmsuu:518": " smṛteś ca |",
    "GSP33brahmsuu:519": " paraṃ jaiminir mukhyatvāt |",
    "GSP33brahmsuu:520": " darśanāc ca |",
    "GSP33brahmsuu:521": " na ca kārye pratyabhisandhiḥ |",
    "GSP33brahmsuu:522": " apratīkālambanān nayatīti bādarāyaṇa ubhayathā ca doṣāt tatkratuś ca |",
    "GSP33brahmsuu:523": " viśeṣaṃ ca darśayati |",
    "GSP33brahmsuu:524": " saṃpadyāvirbhāvaḥ svena śabdāt |",
    "GSP33brahmsuu:525": " muktaḥ pratijñānāt |",
    "GSP33brahmsuu:526": " ātmā prakaraṇāt |",
    "GSP33brahmsuu:527": " avibhāgena dṛṣṭatvāt |",
    "GSP33brahmsuu:528": " brāhmeṇa jaiminir upanyāsādibhyaḥ |",
    "GSP33brahmsuu:529": " cititanmātreṇa tadātmakatvād ity auḍulomiḥ |",
    "GSP33brahmsuu:530": " evam apy upanyāsāt pūrvabhāvād avirodhaṃ bādarāyaṇaḥ |",
    "GSP33brahmsuu:531": " saṃkalpād eva tacchruteḥ |",
    "GSP33brahmsuu:532": " ata eva cānanyādhipatiḥ |",
    "GSP33brahmsuu:533": " abhāvaṃ bādarir āha hy evam |",
    "GSP33brahmsuu:534": " bhāvaṃ jaiminir vikalpāmananāt |",
    "GSP33brahmsuu:535": " dvādaśāhavad ubhayavidhaṃ bādarāyaṇo 'taḥ |",
    "GSP33brahmsuu:536": " tanvabhāve sandhyavad upapatteḥ |",
    "GSP33brahmsuu:537": " bhāve jāgradvat |",
    "GSP33brahmsuu:538": " pradīpavadāveśas tathā hi darśayati |",
    "GSP33brahmsuu:539": " svāpyayasaṃpatyor anyatarāpekṣam āviṣkṛtaṃ hi |",
    "GSP33brahmsuu:540": " jagadvyāpāravarjaṃ prakaraṇād asaṃnihitatvāc ca |",
    "GSP33brahmsuu:541": " pratyakṣopadeśād iti cen nādhikārikamaṇḍalasthokteḥ |",
    "GSP33brahmsuu:542": " vikārāvarti ca tathā hi sthitim āha |",
    "GSP33brahmsuu:543": " darśayataś caivaṃ pratyakṣānumāne |",
    "GSP33brahmsuu:544": " bhogamātrasāmyaliṅgāc ca |",
    "GSP33brahmsuu:545": " anāvṛttiḥ śabdād anāvṛttiḥ śabdāt |"
}