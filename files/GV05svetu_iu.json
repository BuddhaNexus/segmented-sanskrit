    "GV05svetu_iu:393": " sarvataḥpāṇipādaṃ tat SvetUp_3.16a",
    "GV05svetu_iu:394": " sarvataḥśrutimal loke SvetUp_3.16c",
    "GV05svetu_iu:395": " sarvato'kṣiśiromukhaṃ SvetUp_3.16b",
    "GV05svetu_iu:396": " sarvabhūtaguhāśayaḥ SvetUp_3.11b",
    "GV05svetu_iu:397": " sarvam āvṛtya tiṣṭhati SvetUp_3.16d",
    "GV05svetu_iu:398": " sarvam etad viśvam adhitiṣṭhaty eko SvetUp_5.5c",
    "GV05svetu_iu:399": " sarvavyāpinam ātmānaṃ SvetUp_1.16a",
    "GV05svetu_iu:400": " sarvavyāpī sa bhagavāṃs SvetUp_3.11c",
    "GV05svetu_iu:401": " sarvavyāpī sarvabhūtāntarātmā SvetUp_6.11b",
    "GV05svetu_iu:402": " sarvasya prabhum īśānaṃ SvetUp_3.17c",
