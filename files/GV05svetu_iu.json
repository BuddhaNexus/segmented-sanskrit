    "GV05svetu_iu:404": " sarvaṃ proktaṃ trividhaṃ brahmam etat SvetUp_1.12d",
    "GV05svetu_iu:405": " sarvājīve sarvasaṃsthe bṛhante SvetUp_1.6a",
    "GV05svetu_iu:406": " sarvātmānaṃ sarvagataṃ vibhutvāt SvetUp_3.21b",
    "GV05svetu_iu:407": " sarvā diśa ūrdhvam adhaś ca tiryak SvetUp_5.4a",
    "GV05svetu_iu:408": " sarvādhipatyaṃ kurute mahātmā SvetUp_5.3d",
    "GV05svetu_iu:409": " sarvānanaśirogrīvaḥ SvetUp_3.11a",
    "GV05svetu_iu:410": " sarvāṃl lokān īśata īśanībhiḥ SvetUp_3.1b",
    "GV05svetu_iu:411": " sarvendriyaguṇābhāsaṃ SvetUp_3.17a",
    "GV05svetu_iu:412": " sarvendriyavivarjitaṃ SvetUp_3.17b",
    "GV05svetu_iu:413": " savitā prasuvāti tān SvetUp_2.3d",
    "GV05svetu_iu:414": " savitrā prasavena SvetUp_2.7a",
