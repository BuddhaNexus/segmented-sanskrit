{
    "T06bsa019_u:0": " // namo mañjuśriye kumārabhūtāya //",
    "T06bsa019_u:1": " ācāryavasubandhupraṇītaḥ Trisvabhāvanirdeśaḥ (Tsn)",
    "T06bsa019_u:2": " kalpitaḥ paratantraśca pariniṣpanna eva ca /",
    "T06bsa019_u:3": " trayaḥ svabhāvā dhīrāṇāṃ gambhīrajñeyamiṣyate // Tsn_1 //",
    "T06bsa019_u:4": " yat khyāti paratantro 'sau yathā khyāti sa kalpitaḥ /",
    "T06bsa019_u:5": " pratyayādhīnavṛttitvāt kalpanāmātrabhāvataḥ // Tsn_2 //",
    "T06bsa019_u:6": " tasya khyāturyathākhyānaṃ yā sadāvidyamānatā /",
    "T06bsa019_u:7": " jñeyaḥ sa pariniṣpannasvabhāvo 'nanyathātvataḥ // Tsn_3 //",
    "T06bsa019_u:8": " tatra kiṃ khyātyasatkalpaḥ kathaṃ khyāti dvayātmanā /",
    "T06bsa019_u:9": " tasya kā nāstitā tena yā tatrādvayadharmatā // Tsn_4 //",
    "T06bsa019_u:10": " asatkalpo 'tra kaścittaṃ yatastena hi kalpyate /",
    "T06bsa019_u:11": " yathā ca kalpayatyartha tathātyantaṃ na vidyate // Tsn_5 //",
    "T06bsa019_u:12": " taddhetuphalabhāvena cittaṃ dvividhamiṣyate /",
    "T06bsa019_u:13": " yadālayākhyaṃ vijñānaṃ pravṛttyākhyaṃ ca saptadhā // Tsn_6 //",
    "T06bsa019_u:14": " saṃkleśavāsanābījaiścitatvāccittamucyate /",
    "T06bsa019_u:15": " cittamādyaṃ dvitīyaṃ tu citrākārapravṛttitaḥ // Tsn_7 //",
    "T06bsa019_u:16": " samāsato 'bhūtakalpaḥ sa caiṣa trividho mataḥ /",
    "T06bsa019_u:17": " vaipākikastathā naimittiko 'nyaḥ prātibhāsikaḥ // Tsn_8 //",
    "T06bsa019_u:18": " prathamo mūlavijñānaṃ tadvipākātmakaṃ yataḥ /",
    "T06bsa019_u:19": " anyaḥ pravṛttivijñānaṃ dṛśyadṛgvittivṛttitaḥ // Tsn_9 //",
    "T06bsa019_u:20": " sadasattvād dvayaikatvāt saṃkleśavyavadānayoḥ /",
    "T06bsa019_u:21": " lakṣaṇābhedataśceṣṭā svabhāvānāṃ gaṃbhīratā // Tsn_10 //",
    "T06bsa019_u:22": " sattvena gṛhyate yasmādatyantābhāva eva ca /",
    "T06bsa019_u:23": " svabhāvaḥ kalpitastena sadasallakṣaṇo mataḥ // Tsn_11 //",
    "T06bsa019_u:24": " vidyate bhrāntibhāvena yathākhyānaṃ na vidyate /",
    "T06bsa019_u:25": " paratantro yatastena sadasallakṣaṇo mataḥ // Tsn_12 //",
    "T06bsa019_u:26": " advayatvena yaccāsti dvayasyābhāva eva ca /",
    "T06bsa019_u:27": " svabhāvastena niṣpannaḥ sadasallakṣaṇo mataḥ // Tsn_13 //",
    "T06bsa019_u:28": " dvaividhyāt kalpitārthasya tadasattvaikabhāvataḥ /",
    "T06bsa019_u:29": " svabhāvaḥ kalpito bālairdvayaikatvātmako mataḥ // Tsn_14 //",
    "T06bsa019_u:30": " prakhyānād dvayabhāvena bhrāntimātraikabhāvataḥ /",
    "T06bsa019_u:31": " svabhāvaḥ paratantrākhyo dvayaikatvātmako mataḥ // Tsn_15 //",
    "T06bsa019_u:32": " dvayābhāvasvabhāvatvādadvayaikasvabhāvataḥ /",
    "T06bsa019_u:33": " svabhāvaḥ pariniṣpanno dvayaikatvātmako mataḥ // Tsn_16 //",
    "T06bsa019_u:34": " kalpitaḥ paratantraśca jñeyaṃ saṃkleśalakṣaṇam /",
    "T06bsa019_u:35": " pariniṣpanna iṣṭastu vyavadānasya lakṣaṇam // Tsn_17 //",
    "T06bsa019_u:36": " asaddvayasvabhāvatvāt tadabhāvasvabhāvataḥ /",
    "T06bsa019_u:37": " svabhāvāt kalpitājjñeyo niṣpanno 'bhinnalakṣaṇaḥ // Tsn_18 //",
    "T06bsa019_u:38": " advayatvasvabhāvatvād dvayābhāvasvabhāvataḥ /",
    "T06bsa019_u:39": " niṣpannāt kalpitaścaiva vijñeyo 'bhinnalakṣaṇaḥ // Tsn_19 //",
    "T06bsa019_u:40": " yathākhyānamasadbhāvāt tathāsattvasvabhāvataḥ /",
    "T06bsa019_u:41": " svabhāvāt paratantrākhyānniṣpanno 'bhinnalakṣaṇaḥ // Tsn_20 //",
    "T06bsa019_u:42": " asaddvayasvabhāvatvād yathākhyānāsvabhāvataḥ /",
    "T06bsa019_u:43": " niṣpannāt paratantro 'pi vijñeyo 'bhinnalakṣaṇaḥ // Tsn_21 //",
    "T06bsa019_u:44": " kramabhedaḥ svabhāvānāṃ vyavahārādhikārataḥ /",
    "T06bsa019_u:45": " tatpraveśādhikārācca vyutpattyarthaṃ vidhīyate // Tsn_22 //",
    "T06bsa019_u:46": " kalpito vyavahārātmā vyavahartrātmako 'paraḥ /",
    "T06bsa019_u:47": " vyavahārasamucchedasvabhāvaścānya iṣyate // Tsn_23 //",
    "T06bsa019_u:48": " dvayābhāvātmakaḥ pūrvaṃ paratantraḥ praviśyate /",
    "T06bsa019_u:49": " tataḥ praviśyate tatra kalpamātramasaddvayam // Tsn_24 //",
    "T06bsa019_u:50": " tato dvayābhāvabhāvo niṣpanno 'tra praviśyate /",
    "T06bsa019_u:51": " tathā hyasāveva tadā astināstīti cocyate // Tsn_25 //",
    "T06bsa019_u:52": " trayo 'pyete svabhāvā hi advayālambalakṣaṇāḥ /",
    "T06bsa019_u:53": " abhāvādatathābhāvāt tadabhāvasvabhāvataḥ // Tsn_26 //",
    "T06bsa019_u:54": " māyākṛtaṃ mantravaśāt khyāti hastyātmanā yathā /",
    "T06bsa019_u:55": " ākāramātraṃ tatrāsti hastī nāsti tu sarvathā // Tsn_27 //",
    "T06bsa019_u:56": " svabhāvaḥ kalpito hastī paratantrastadākṛtiḥ /",
    "T06bsa019_u:57": " yastatra hastyabhāvo 'sau pariniṣpanna iṣyate // Tsn_28 //",
    "T06bsa019_u:58": " asatkalpastathā khyāti mūlacittād dvayātmanā /",
    "T06bsa019_u:59": " dvayamatyantato nāsti tatrāstyākṛtimātrakam // Tsn_29 //",
    "T06bsa019_u:60": " mantravanmūlavijñānaṃ kāṣṭhavattathatā matā /",
    "T06bsa019_u:61": " hastyākāravadeṣṭavyo vikalpo hastivad dvayam // Tsn_30 //",
    "T06bsa019_u:62": " arthatattvaprativedhe yugapallakṣaṇatrayam /",
    "T06bsa019_u:63": " parijñā ca prahāṇaṃ ca prāptiśceṣṭā yathākramam // Tsn_31 //",
    "T06bsa019_u:64": " parijñānupalambho 'tra hānirakhyānamiṣyate /",
    "T06bsa019_u:65": " upalambhanimittā tu prāptiḥ sākṣātkriyāpi sā // Tsn_32 //",
    "T06bsa019_u:66": " dvayasyānupalambhena dvayākāro vigacchati /",
    "T06bsa019_u:67": " vigamāt tasya niṣpanno dvayābhāvo 'dhigamyate // Tsn_33 //",
    "T06bsa019_u:68": " hastino 'nupalambhaśca vigamaśca tadākṛteḥ /",
    "T06bsa019_u:69": " upalambhaśca kāṣṭhasya māyāyāṃ yugapad yathā // Tsn_34 //",
    "T06bsa019_u:70": " viruddhadhīvāraṇatvād buddhyā vaiyarthyadarśanāt /",
    "T06bsa019_u:71": " jñānatrayānuvṛtteśca mokṣāpattirayatnataḥ // Tsn_35 //",
    "T06bsa019_u:72": " cittamātropalambhena jñeyārthānupalambhatā /",
    "T06bsa019_u:73": " jñeyārthānupalambhena syāccittānupalambhatā // Tsn_36 //",
    "T06bsa019_u:74": " dvayoranupalambhena dharmadhātūpalabhbhatā /",
    "T06bsa019_u:75": " dharmadhātūpalambhena syād vibhutvopalambhatā // Tsn_37 //",
    "T06bsa019_u:76": " upalabdhavibhutvaśca svaparārthaprasiddhitaḥ /",
    "T06bsa019_u:77": " prāpnotyanuttarāṃ bodhiṃ dhīmān kāyatrayātmikām // Tsn_38 //",
    "T06bsa019_u:78": " iti trisvabhāvanirdeśaḥ samāptaḥ //",
    "T06bsa019_u:79": " kṛtirācāryavasubandhupādānāmiti //"
}