{
  "SA07davyasou:0": "śrī siddhārthaḥ kapilanagare bodhisattvaḥ saputrāṃ",
  "SA07davyasou:1": "devīṃ tyaktvā sakalavibhavaṃ cakravartiśriyaṃ ca /",
  "SA07davyasou:2": "Yaśo_1 devīṃ tyaktvā sakalavibhavaṃ cakravartiśriyaṃ ca /",
  "SA07davyasou:3": "māsy āṣāḍhe viratamanasā pūrṇimāyāṃ niśīthe",
  "SA07davyasou:4": "niścakrāma śramaṇapadabhṛn mokṣam anveṣṭukāmaḥ //",
  "SA07davyasou:5": "atha prabhāte śayanāt samutthitā",
  "SA07davyasou:6": "sutena sārdhaṃ mahiṣī yaśodharā /",
  "SA07davyasou:7": "Yaśo_2 sutena sārdhaṃ mahiṣī yaśodharā /",
  "SA07davyasou:8": "didṛkṣayā svāmivarasya satvaraṃ",
  "SA07davyasou:9": "gatāvidūre śayanīyamandiram //",
  "SA07davyasou:10": "yathā pure vañcayituṃ sa me patī",
  "SA07davyasou:11": "rahogataḥ kutra cid eva gūḍhavān /",
  "SA07davyasou:12": "Yaśo_3 rahogataḥ kutra cid eva gūḍhavān /",
  "SA07davyasou:13": "vicintya saudhe 'ntarabāhiraṃ bhṛśaṃ",
  "SA07davyasou:14": "parīkṣyamāṇāpi dadarśa naiva tam //",
  "SA07davyasou:15": "dadhāva tasmāt sacivasya mandiraṃ",
  "SA07davyasou:16": "gavākṣam udghāṭya drutaṃ drutaṃ śucā /",
  "SA07davyasou:17": "Yaśo_4 gavākṣam udghāṭya drutaṃ drutaṃ śucā /",
  "SA07davyasou:18": "kapāṭam āhatya dṛḍhaṃ muhur muhur",
  "SA07davyasou:19": "vyarīravac channa bhavan bhavann iti //",
  "SA07davyasou:20": "apekṣya kiṃ cit kvaṇam antarād gṛhe",
  "SA07davyasou:21": "vyatiṣṭhatālpaṃ pratihārasannidhau /",
  "SA07davyasou:22": "Yaśo_5 vyatiṣṭhatālpaṃ pratihārasannidhau /",
  "SA07davyasou:23": "vicārabhaṅgāc chvasatī ca pāṇinā",
  "SA07davyasou:24": "pratāḍayām āsa kapolayugmakam //",
  "SA07davyasou:25": "tato gatā sā nṛpamandurālayaṃ",
  "SA07davyasou:26": "turaṅgamaṃ nātra dadarśa kanthakam /",
  "SA07davyasou:27": "Yaśo_6 turaṅgamaṃ nātra dadarśa kanthakam /",
  "SA07davyasou:28": "kuto yayau kiṃ rahasā sa vallabho",
  "SA07davyasou:29": "vitarkayantīti babhūva mūrchitā //",
  "SA07davyasou:30": "vicintya yat kiṃ cid itas tataḥ śiraś",
  "SA07davyasou:31": "cacāla jaghrau ca mukhe sutasya sā /",
  "SA07davyasou:32": "Yaśo_7 cacāla jaghrau ca mukhe sutasya sā /",
  "SA07davyasou:33": "bhujātapatreṇa vilocanadvayān",
  "SA07davyasou:34": "nyavārayat sā sravadaśruśīkaram //",
  "SA07davyasou:35": "aśaknuvantī nitarāṃ samudgamāṃ",
  "SA07davyasou:36": "manovyathāṃ soḍhum udagramanyunā /",
  "SA07davyasou:37": "Yaśo_8 manovyathāṃ soḍhum udagramanyunā /",
  "SA07davyasou:38": "patipradattaṃ maṇikaṇṭhabhūṣaṇaṃ",
  "SA07davyasou:39": "pravālamālām api kuṇḍaladvayam //",
  "SA07davyasou:40": "lalāṭikāṃ hemamayāṅgulīyakaṃ",
  "SA07davyasou:41": "hiraṇyasūtreṇa nibaddhamekhalām /",
  "SA07davyasou:42": "Yaśo_9 hiraṇyasūtreṇa nibaddhamekhalām /",
  "SA07davyasou:43": "vimucya śīrṣābharaṇaṃ ca kaṅkaṇe",
  "SA07davyasou:44": "sasarja dhig dhig vadatī sasambhramam //",
  "SA07davyasou:45": "atītaḍat pādatalena bhūtalaṃ",
  "SA07davyasou:46": "jajṛmbha ūrdhvaṃ vicakarṣa mūrdhajam /",
  "SA07davyasou:47": "Yaśo_10 jajṛmbha ūrdhvaṃ vicakarṣa mūrdhajam /",
  "SA07davyasou:48": "jajalpa vākyaṃ vigatārtham ākulaṃ",
  "SA07davyasou:49": "rurāva bhīteva ruroda gadgadam //",
  "SA07davyasou:50": "niveśya bālaṃ śayane punaḥ punaḥ",
  "SA07davyasou:51": "samutkṣipantī yugapat karadvayam /",
  "SA07davyasou:52": "Yaśo_11 samutkṣipantī yugapat karadvayam /",
  "SA07davyasou:53": "vikampya śīrṣaṃ ca yathā vicetanā",
  "SA07davyasou:54": "jughoṣa śaśvāsa papāta vivyathe //",
  "SA07davyasou:55": "nipīḍyamānāpi bubhukṣayāniśaṃ",
  "SA07davyasou:56": "na bhojanaṃ bhoktum iyeṣa kiṃ cana /",
  "SA07davyasou:57": "Yaśo_12 na bhojanaṃ bhoktum iyeṣa kiṃ cana /",
  "SA07davyasou:58": "tṛṣāturā nodakabindum apy asau",
  "SA07davyasou:59": "papau ca kāye 'py babhūva vepathuḥ //",
  "SA07davyasou:60": "nihanyate dhīsmṛtiśaktidhīratā",
  "SA07davyasou:61": "śarīravarṇaṃ balam indriyeṣv api /",
  "SA07davyasou:62": "Yaśo_13 śarīravarṇaṃ balam indriyeṣv api /",
  "SA07davyasou:63": "vinaśyate jīvam anukrameṇa ca",
  "SA07davyasou:64": "durantateyaṃ priyaviprayogatā //",
  "SA07davyasou:65": "niṣadya bhūmyāṃ vinatārdhavigrahā",
  "SA07davyasou:66": "śiraś ca pāṇidvayam aṅkamastake /",
  "SA07davyasou:67": "Yaśo_14 śiraś ca pāṇidvayam aṅkamastake /",
  "SA07davyasou:68": "niveśya kiṃ cit pariṇamya kaṃdharaṃ",
  "SA07davyasou:69": "mamajja cintājaladhāv apāśraye //",
  "SA07davyasou:70": "yaśodharāyā avamānanakriyā",
  "SA07davyasou:71": "asahya siddhārthakumāra ekakaḥ /",
  "SA07davyasou:72": "Yaśo_15 asahya siddhārthakumāra ekakaḥ /",
  "SA07davyasou:73": "nivāsam utsṛjya gato nu kānanam",
  "SA07davyasou:74": "iti pravādo 'pi bhaved aho mama //",
  "SA07davyasou:75": "yad asti kāryaṃ triṣu mandireṣu tac",
  "SA07davyasou:76": "cakāra kāryakṣamasevikā yathā /",
  "SA07davyasou:77": "Yaśo_16 cakāra kāryakṣamasevikā yathā /",
  "SA07davyasou:78": "gatau sthitau vā śayane ca vipriyaṃ",
  "SA07davyasou:79": "kadā canāhaṃ na cakāra kiṃ cana //",
  "SA07davyasou:80": "vicintayantī sahasāgatāpadaṃ",
  "SA07davyasou:81": "pragamya devāyatanaṃ priyaṃ prati /",
  "SA07davyasou:82": "Yaśo_17 pragamya devāyatanaṃ priyaṃ prati /",
  "SA07davyasou:83": "sugandhipuspair api dhūmadīpakair",
  "SA07davyasou:84": "apūpujad devagaṇaṃ sagauravam //",
  "SA07davyasou:85": "bhujadvayaṃ mūrdhni niveśya śokinī",
  "SA07davyasou:86": "vilokya devapratimānanaśriyam /",
  "SA07davyasou:87": "Yaśo_18 vilokya devapratimānanaśriyam /",
  "SA07davyasou:88": "praṇamya bhaktyā pariśuddhacetasā",
  "SA07davyasou:89": "yayāca devān iti viprayogataḥ //",
  "SA07davyasou:90": "trilokasargasthitināśahetavaḥ",
  "SA07davyasou:91": "caturmukhaśrīpaticandraśekharāḥ /",
  "SA07davyasou:92": "Yaśo_19 caturmukhaśrīpaticandraśekharāḥ /",
  "SA07davyasou:93": "mahānubhāvā jagadīśvarāḥ surāḥ",
  "SA07davyasou:94": "suduḥkhitāyai śaraṇaṃ bhavantu me //",
  "SA07davyasou:95": "vidhāya yācñām iti devahastayor",
  "SA07davyasou:96": "upāyanīkṛtya babandha kārṣikau /",
  "SA07davyasou:97": "Yaśo_20 upāyanīkṛtya babandha kārṣikau /",
  "SA07davyasou:98": "upāharan miṣṭanavāmbupāyasaṃ",
  "SA07davyasou:99": "sitāṃśukaṃ svarṇamayīṃ ca putrikām //",
  "SA07davyasou:100": "pramodam āpādayituṃ sureśvarāṃś",
  "SA07davyasou:101": "cakāra ghaṇṭāninadaṃ ca bhūsurān /",
  "SA07davyasou:102": "Yaśo_21 cakāra ghaṇṭāninadaṃ ca bhūsurān /",
  "SA07davyasou:103": "ajījapad devagirā ca saṃstavān",
  "SA07davyasou:104": "nanāma bhūmāv asakṛt nipatya sā //",
  "SA07davyasou:105": "pure vane saṃvasathe 'pi vallabho",
  "SA07davyasou:106": "vased dhi kasminn iti divyācakṣuṣā /",
  "SA07davyasou:107": "Yaśo_22 vased dhi kasminn iti divyācakṣuṣā /",
  "SA07davyasou:108": "vilokya gehaṃ vinivartayantu taṃ",
  "SA07davyasou:109": "divaukasaḥ prārthayataiva sā punaḥ //",
  "SA07davyasou:110": "nirīkṣya jihmānimiṣeṇa cakṣuṣā",
  "SA07davyasou:111": "nihatya gāḍhaṃ caraṇaṃ bhuvas tale /",
  "SA07davyasou:112": "Yaśo_23 nihatya gāḍhaṃ caraṇaṃ bhuvas tale /",
  "SA07davyasou:113": "muhur muhur daṃśitakomalādharā",
  "SA07davyasou:114": "prakarṣam utkṣipya vikampya tarjanīm //",
  "SA07davyasou:115": "are-tvayāryasya gṛhābhiniṣkramaḥ",
  "SA07davyasou:116": "śruto na dṛṣṭaḥ vada kiṃ nirākulam /",
  "SA07davyasou:117": "Yaśo_24 śruto na dṛṣṭaḥ vada kiṃ nirākulam /",
  "SA07davyasou:118": "iti pratīhārabhaṭaṃ ruṣoccakai",
  "SA07davyasou:119": "raveṇa papraccha sabāṣpalocanā //",
  "SA07davyasou:120": "bhavān sa ārye nṛpavaṃśaśekharo",
  "SA07davyasou:121": "mayā na dṛṣṭaḥ pratihāram āgataḥ /",
  "SA07davyasou:122": "Yaśo_25 mayā na dṛṣṭaḥ pratihāram āgataḥ /",
  "SA07davyasou:123": "varāṅganābhiḥ saha kelikānane",
  "SA07davyasou:124": "gate niśīthe 'ramateti me śrutam //",
  "SA07davyasou:125": "avītarāgo nanu rājanandano",
  "SA07davyasou:126": "vavāñcha tasmān nanu nūtanapriyām /",
  "SA07davyasou:127": "Yaśo_26 vavāñcha tasmān nanu nūtanapriyām /",
  "SA07davyasou:128": "mithaḥ sa kāṃ cit pariṇīya nāṭikāṃ",
  "SA07davyasou:129": "bahir gato 'ddheti vitarkayāmy aham //",
  "SA07davyasou:130": "purā hi mandhātvabhidho mahāyaśāḥ",
  "SA07davyasou:131": "sa sārvabhaumo 'pi surāṅganāgaṇe /",
  "SA07davyasou:132": "Yaśo_27 sa sārvabhaumo 'pi surāṅganāgaṇe /",
  "SA07davyasou:133": "kṛtānurāgo parivarjya mānuṣīṃ",
  "SA07davyasou:134": "śriyaṃ prapede nu divaukasāṃ puram //",
  "SA07davyasou:135": "mano'bhirāmaṃ surasundarīgaṇaṃ",
  "SA07davyasou:136": "vihāya vindhyāvanatāpasāśrame /",
  "SA07davyasou:137": "Yaśo_28 vihāya vindhyāvanatāpasāśrame /",
  "SA07davyasou:138": "agastipatnīṃ parirabhya tāpasīṃ",
  "SA07davyasou:139": "lalāsa rantuṃ niśi pākaśāsanaḥ //",
  "SA07davyasou:140": "pratītalaṅkāvijite 'grabhūpatir",
  "SA07davyasou:141": "daśānano daṇḍakakānanaṃ gataḥ /",
  "SA07davyasou:142": "Yaśo_29 daśānano daṇḍakakānanaṃ gataḥ /",
  "SA07davyasou:143": "vigṛhya sītāṃ janakātmajām api",
  "SA07davyasou:144": "nijapriyāvad vyadadhāt tadātmasāt //",
  "SA07davyasou:145": "maheśvaraś cāpi purā tapaś carann",
  "SA07davyasou:146": "umāmahiṣyā saha kalikānane /",
  "SA07davyasou:147": "Yaśo_30 umāmahiṣyā saha kalikānane /",
  "SA07davyasou:148": "apāsya sātatyasamādhibhāvanaṃ",
  "SA07davyasou:149": "vyarīramac cājanayat sutau nanu //",
  "SA07davyasou:150": "Yaśo_31 śucākulāṃ sāntvayituṃ vadhūttamām",
  "SA07davyasou:151": "iti pratīhāra uraḥsthale 'ñjalim /",
  "SA07davyasou:152": "vidhāya valgudhvaninānukampayā",
  "SA07davyasou:153": "natena mūrdhnā vacasā vyajijñapat //",
  "SA07davyasou:154": "are-tvam etarhi bhavantam uttamaṃ",
  "SA07davyasou:155": "gṛhe bahir vopavane 'pi kutra cit /",
  "SA07davyasou:156": "Yaśo_32 gṛhe bahir vopavane 'pi kutra cit /",
  "SA07davyasou:157": "aharmukhe kiṃ niśi vā dadarśitha",
  "SA07davyasou:158": "vyapṛcchad itthaṃ nijasevikāṃ ruṣā //",
  "SA07davyasou:159": "nidhāya vāmetarajānumaṇḍalaṃ",
  "SA07davyasou:160": "kṣitau bhujau cāpi lalāṭamastake /",
  "SA07davyasou:161": "Yaśo_33 kṣitau bhujau cāpi lalāṭamastake /",
  "SA07davyasou:162": "natārdhakāyaṃ nijagāda sevikā",
  "SA07davyasou:163": "śrutaṃ ca dṛṣṭaṃ na na vedmi kiṃ cana //",
  "SA07davyasou:164": "ataḥ-paraṃ harmyam upeyuṣī satī",
  "SA07davyasou:165": "pramārjya nisyandajalaṃ vilocane /",
  "SA07davyasou:166": "Yaśo_34 pramārjya nisyandajalaṃ vilocane /",
  "SA07davyasou:167": "are-kim āryo 'tra samāgato na vā",
  "SA07davyasou:168": "vyapṛcchad antaḥpurapālapaṇḍakam //",
  "SA07davyasou:169": "praśasyarūpeṇa girā ca valgunā",
  "SA07davyasou:170": "śriyā 'pi dhṛtyā tava kakṣam āgatāḥ /",
  "SA07davyasou:171": "Yaśo_35 śriyā 'pi dhṛtyā tava kakṣam āgatāḥ /",
  "SA07davyasou:172": "mahottame sarvajanena mānitā",
  "SA07davyasou:173": "bhavanti tasya tritayāḥ priyāṅganāḥ //",
  "SA07davyasou:174": "mano'bhirāmā bhavatī ca bhāratī",
  "SA07davyasou:175": "tathaiva lakṣmīr iti tāḥ priyāṅganāḥ /",
  "SA07davyasou:176": "Yaśo_36 tathaiva lakṣmīr iti tāḥ priyāṅganāḥ /",
  "SA07davyasou:177": "sadaiva tābhī ramate kumārakaḥ",
  "SA07davyasou:178": "kadā cid anyatra mano 'sya no rajet //",
  "SA07davyasou:179": "jugupsite satpuruṣair manasvibhiḥ",
  "SA07davyasou:180": "sadoṣakāme vimukho nu te patiḥ /",
  "SA07davyasou:181": "Yaśo_37 sadoṣakāme vimukho nu te patiḥ /",
  "SA07davyasou:182": "tato 'sya śuddhāntagatiḥ kadā cana",
  "SA07davyasou:183": "śrutā na dṛṣṭā na divā vā niśi //",
  "SA07davyasou:184": "tapodhanānāṃ samatām upeyivān",
  "SA07davyasou:185": "ratiṃ na kuryāt sa bhavatraye kva cit /",
  "SA07davyasou:186": "Yaśo_38 ratiṃ na kuryāt sa bhavatraye kva cit /",
  "SA07davyasou:187": "amedhyagehena same 'varodhane",
  "SA07davyasou:188": "kathaṃ vidhatte ratim eṣa te patiḥ //",
  "SA07davyasou:189": "asādhyarogāyatane sukhetare",
  "SA07davyasou:190": "bubhukṣayā nityanipīḍitodare /",
  "SA07davyasou:191": "Yaśo_39 bubhukṣayā nityanipīḍitodare /",
  "SA07davyasou:192": "atarpaṇīye vividhānnapānakaiḥ",
  "SA07davyasou:193": "spṛhāṃ na dehe prakaroti paṇḍitaḥ //",
  "SA07davyasou:194": "vikīrṇagharmodakavipruṣi tvaci",
  "SA07davyasou:195": "vicitravarṇaṃ śamalena saṃskṛte /",
  "SA07davyasou:196": "Yaśo_40 vicitravarṇaṃ śamalena saṃskṛte /",
  "SA07davyasou:197": "śarīra ālokya jano vinaśyate",
  "SA07davyasou:198": "pataṅgavad dīpaśikhojjvalaprabhām //",
  "SA07davyasou:199": "tato nṛpāntaḥpurapālakodite",
  "SA07davyasou:200": "kṛtāvadhānā vacane yaśodharā /",
  "SA07davyasou:201": "Yaśo_41 kṛtāvadhānā vacane yaśodharā /",
  "SA07davyasou:202": "divaukasāṃ nandanakānanopamaṃ",
  "SA07davyasou:203": "jagāma rājopavanaṃ calātmanā //",
  "SA07davyasou:204": "athābhirāmopavanasya rakṣakaḥ",
  "SA07davyasou:205": "puraḥ pragamyorasi baddhapāṇinā /",
  "SA07davyasou:206": "Yaśo_42 puraḥ pragamyorasi baddhapāṇinā /",
  "SA07davyasou:207": "kimartham ārye 'tra samāgatis tava",
  "SA07davyasou:208": "bhavāmi jijñāsur iti nyavedayat //",
  "SA07davyasou:209": "sakhe mamāryo rahasāruṇodaye",
  "SA07davyasou:210": "samāgato vātra na kiṃ samāgataḥ /",
  "SA07davyasou:211": "Yaśo_43 samāgato vātra na kiṃ samāgataḥ /",
  "SA07davyasou:212": "kim atra dṛṣṭaḥ sa na vā śrutas tvayā",
  "SA07davyasou:213": "vicintya samyag vada māṃ yathātatham //",
  "SA07davyasou:214": "gataspṛhaḥ pañcasu kāmavastuṣu",
  "SA07davyasou:215": "sa āryaputro vijitendriyo bhavet /",
  "SA07davyasou:216": "Yaśo_44 sa āryaputro vijitendriyo bhavet /",
  "SA07davyasou:217": "tato 'bhirāmopavanaṃ samāgamo",
  "SA07davyasou:218": "na dṛṣṭa ārye na hi vā śruto mayā //",
  "SA07davyasou:219": "vidhitsamāno 'bhyudayaṃ jagattraye",
  "SA07davyasou:220": "divaukasānām adhigamya yācanam /",
  "SA07davyasou:221": "Yaśo_45 divaukasānām adhigamya yācanam /",
  "SA07davyasou:222": "divo 'vatīrṇo nanu puṇyatejasā",
  "SA07davyasou:223": "manuṣyajanmany ajaniṣṭa sa prabhuḥ //",
  "SA07davyasou:224": "mahāmahe saty asitarṣir īyivān",
  "SA07davyasou:225": "divaṃ kim ity asya vimṛśya kāraṇam /",
  "SA07davyasou:226": "Yaśo_46 divaṃ kim ity asya vimṛśya kāraṇam /",
  "SA07davyasou:227": "svamitraśuddhodanabhūpateḥ suto",
  "SA07davyasou:228": "'janīti harṣāt tam avekṣituṃ yayau //",
  "SA07davyasou:229": "tato 'sitarṣir nṛpasaudham āgataḥ",
  "SA07davyasou:230": "kumārakaṃ vīkṣya sapuṇyasampadam /",
  "SA07davyasou:231": "Yaśo_47 kumārakaṃ vīkṣya sapuṇyasampadam /",
  "SA07davyasou:232": "atīndriyajñānamayena cakṣuṣā",
  "SA07davyasou:233": "parīkṣamāṇo 'sya śarīralakṣaṇam //",
  "SA07davyasou:234": "jagāda vākyaṃ śiśur eṣa puṇyavān",
  "SA07davyasou:235": "bhaved dhi buddho hatamārabandhanaḥ /",
  "SA07davyasou:236": "Yaśo_48 bhaved dhi buddho hatamārabandhanaḥ /",
  "SA07davyasou:237": "pratārayet sarvanarāmarāsurān",
  "SA07davyasou:238": "apārasaṃsāramahārṇavād iti //",
  "SA07davyasou:239": "dvijeṣu pañcasv atha dehalakṣaṇaṃ",
  "SA07davyasou:240": "parīkṣya kauṇḍaṇyabudho 'bravīd iti /",
  "SA07davyasou:241": "Yaśo_49 parīkṣya kauṇḍaṇyabudho 'bravīd iti /",
  "SA07davyasou:242": "vinaśya tṛṣṇādimalāny ayaṃ śiśur",
  "SA07davyasou:243": "anuttaraṃ buddhapadaṃ gamiṣyati //",
  "SA07davyasou:244": "tadā prasūtakṣaṇa eva lumbinī-",
  "SA07davyasou:245": "vane sa saptāmbujamūrdhny abhivrajan /",
  "SA07davyasou:246": "Yaśo_50 vane sa saptāmbujamūrdhny abhivrajan /",
  "SA07davyasou:247": "abhītanādaṃ nyanadaj jagattraye",
  "SA07davyasou:248": "bhaveyam agras tv iti siṃhapotavat //",
  "SA07davyasou:249": "bhramaddvirephaśrutiramyanisvanaḥ",
  "SA07davyasou:250": "sugandhinaḥ puṣpaparāgarāśinā /",
  "SA07davyasou:251": "Yaśo_51 sugandhinaḥ puṣpaparāgarāśinā /",
  "SA07davyasou:252": "taraṃgiṇīśīkaramiśramāruto",
  "SA07davyasou:253": "vavau samantād vipine śanaiḥ tadā //",
  "SA07davyasou:254": "mahātmanas tasya parārdhyajanmani",
  "SA07davyasou:255": "vasuṃdharāyā nidhayaḥ samudgatāḥ /",
  "SA07davyasou:256": "Yaśo_52 vasuṃdharāyā nidhayaḥ samudgatāḥ /",
  "SA07davyasou:257": "dharāṅganā tarhy atimātramoditā",
  "SA07davyasou:258": "savismayāṃ siddhim adarśayad bhuvi //",
  "SA07davyasou:259": "tataḥ paraṃ lumbinikānanād gṛhaṃ",
  "SA07davyasou:260": "vicitrajāmbūnadabimbasaṃnibham /",
  "SA07davyasou:261": "Yaśo_53 vicitrajāmbūnadabimbasaṃnibham /",
  "SA07davyasou:262": "kumārakaṃ tau pitarau savatsalau",
  "SA07davyasou:263": "sabandhuvargaṃ parivārya ninyatuḥ //",
  "SA07davyasou:264": "gurupradiṣṭaṃ vidhivad gurupriyaḥ",
  "SA07davyasou:265": "samastavidyāsamayaṃ ca darśanam /",
  "SA07davyasou:266": "Yaśo_54 samastavidyāsamayaṃ ca darśanam /",
  "SA07davyasou:267": "bhave bhave saṃcitapuṇyatejasā",
  "SA07davyasou:268": "samadhyagīṣṭāciram eṣa buddhimā //",
  "SA07davyasou:269": "Yaśo_55 śrutena dhṛtyā navayauvanaśriyam",
  "SA07davyasou:270": "upeyuṣo 'syāryasutasya janmanaḥ /",
  "SA07davyasou:271": "saha tvayā ṣoḍaśame hi hāyane",
  "SA07davyasou:272": "vivāhadīkṣāṃ vidhivac cakāra nu //"
}