{
  "S05bsu027_u:0": "[Vaidya_117]",
  "S05bsu027_u:1": "Pratītyasamutpādādivibhaṅganirdeśasūtram /",
  "S05bsu027_u:2": "evaṃ mayā śrutam /",
  "S05bsu027_u:3": "ekasmin samaye bhagavān śrāvastyāṃ viharati sma jetavane anāthapiṇḍadasyārāme mahatā bhikṣusaṃghena sārdhamardhatrayodaśabhirbhikṣuśataiḥ /",
  "S05bsu027_u:4": "tatra bhagavānāmantrayate sma - pratītyasamutpādasya vo bhikṣavaḥ ādiṃ vo deśayiṣyāmi vibhaṅgaṃ ca /",
  "S05bsu027_u:5": "tacchṛṇuta,",
  "S05bsu027_u:6": "sādhu ca suṣṭhu ca manasikuruta, bhāṣiṣye -",
  "S05bsu027_u:7": "pratītyasamutpādasya ādiḥ katamaḥ?",
  "S05bsu027_u:8": "yaduta asmin sati idaṃ bhavati,",
  "S05bsu027_u:9": "asyotpādādidamutpadyate /",
  "S05bsu027_u:10": "yaduta avidyāpratyayāḥ saṃskārāḥ /",
  "S05bsu027_u:11": "saṃskārapratyayaṃ vijñānam /",
  "S05bsu027_u:12": "vijñānapratyayaṃ nāmarūpam /",
  "S05bsu027_u:13": "nāmarūpapratyayaṃ ṣaḍāyatanam /",
  "S05bsu027_u:14": "ṣaḍāyatanapratyayaḥ sparśaḥ /",
  "S05bsu027_u:15": "sparśapratyayā vedanā /",
  "S05bsu027_u:16": "vedanāpratyayā tṛṣṇā /",
  "S05bsu027_u:17": "tṛṣṇāpratyayamupādānam /",
  "S05bsu027_u:18": "upādānapratyayo bhavaḥ /",
  "S05bsu027_u:19": "bhavapratyayā jātiḥ /",
  "S05bsu027_u:20": "jātipratyayā jarāmaraṇaśokaparidevaduḥkhadaurmanasyopāyāsāḥ saṃbhavanti /",
  "S05bsu027_u:21": "evamasya kevalasya mahato duḥkhaskandhasya samudayo bhavati /",
  "S05bsu027_u:22": "ayamucyate pratītyasamutpādasyādiḥ //",
  "S05bsu027_u:23": "vibhaṅgaḥ katamaḥ?",
  "S05bsu027_u:24": "avidyāpratyayāḥ saṃskārāḥ ityavidyā katamā?",
  "S05bsu027_u:25": "yat pūrvānte 'jñānam,",
  "S05bsu027_u:26": "aparānte 'jñānam,",
  "S05bsu027_u:27": "pūrvāntāparānte 'jñānam,",
  "S05bsu027_u:28": "adhyātmajñānam,",
  "S05bsu027_u:29": "bahirdhājñānam,",
  "S05bsu027_u:30": "adhyātmabahirdhājñānam,",
  "S05bsu027_u:31": "karmaṇyajñānam,",
  "S05bsu027_u:32": "vipāke 'jñānam,",
  "S05bsu027_u:33": "karmavipāke 'jñānam,",
  "S05bsu027_u:34": "buddhe 'jñānam,",
  "S05bsu027_u:35": "dharme 'jñānam,",
  "S05bsu027_u:36": "saṃghe 'jñānam,",
  "S05bsu027_u:37": "duḥkhe 'jñānam,",
  "S05bsu027_u:38": "samudaye 'jñānam,",
  "S05bsu027_u:39": "nirodhe 'jñānam,",
  "S05bsu027_u:40": "mārge 'jñānam,",
  "S05bsu027_u:41": "hetāvajñānam,",
  "S05bsu027_u:42": "hetusamutpanneṣu dharmeṣvajñānam,",
  "S05bsu027_u:43": "kuśalākuśaleṣu sāvadyānavadyeṣu sevitavyāsevitavyeṣu hīnapraṇītakṛṣṇaśuklasapratibhāgapratītyasamutpanneṣu dharmeṣvajñānam,",
  "S05bsu027_u:44": "ṣaṭsu vā punaḥ sparśāyataneṣu yathābhūta(tā?)",
  "S05bsu027_u:45": "saṃprativedhaḥ iti /",
  "S05bsu027_u:46": "yadatra tatra yathābhūtasyājñānam,",
  "S05bsu027_u:47": "adarśanam,",
  "S05bsu027_u:48": "anabhisamayaḥ,",
  "S05bsu027_u:49": "tamaḥ,",
  "S05bsu027_u:50": "saṃmohaḥ,",
  "S05bsu027_u:51": "avidyāndhakāram,",
  "S05bsu027_u:52": "iyamucyate 'vidyā //",
  "S05bsu027_u:53": "avidyāpratyayāḥ saṃskārāḥ katame?",
  "S05bsu027_u:54": "trayaḥ saṃskārāḥ - kāyasaṃskārāḥ vāksaṃskārāḥ manaḥsaṃskārā iti //",
  "S05bsu027_u:55": "saṃskārapratyayaṃ vijñānamiti vijñānaṃ katamat?",
  "S05bsu027_u:56": "ṣaḍ vijñānakāyāḥ - cakṣurvijñānaṃ śrotraghrāṇajihvākāyamanovijñānam //",
  "S05bsu027_u:57": "vijñānapratyayaṃ nāmarūpamiti nāma katamat?",
  "S05bsu027_u:58": "catvāra arūpiṇaḥ skandhāḥ /",
  "S05bsu027_u:59": "katame catvāraḥ?",
  "S05bsu027_u:60": "vedanāskandhaḥ saṃjñāskandhaḥ saṃskāraskandhaḥ vijñānaskandhaḥ /",
  "S05bsu027_u:61": "rūpaṃ katamat?",
  "S05bsu027_u:62": "yat kiṃcidrūpam,",
  "S05bsu027_u:63": "sarvaṃ tat catvāri mahābhūtāni /",
  "S05bsu027_u:64": "catvāri ca mahābhūtānyupādāya itīdaṃ ca nāma,",
  "S05bsu027_u:65": "tadaikadhyamabhisaṃkṣipya nāmarūpamityucyate //",
  "S05bsu027_u:66": "nāmarūpapratyayaṃ ṣaḍāyatanamiti ṣaḍāyatanaṃ katamat?",
  "S05bsu027_u:67": "ṣaḍādhyātmikānyāyatanāni /",
  "S05bsu027_u:68": "cakṣurādhyātmikamāyatanaṃ śrotaghrāṇajihvākāyamanaādhyātmikamāyatanam //",
  "S05bsu027_u:69": "ṣaḍāyatanapratyayaḥ sparśa iti sparśaḥ katamaḥ?",
  "S05bsu027_u:70": "ṣaṭ sparśakāyāḥ /",
  "S05bsu027_u:71": "cakṣuḥsaṃsparśaḥ śrotraghrāṇajihvākāyamanaḥsaṃsparśaḥ //",
  "S05bsu027_u:72": "(Psvibh, Vaidya 118)",
  "S05bsu027_u:73": "sparśapratyayā vedaneti vedanā katamā?",
  "S05bsu027_u:74": "tisro vedanāḥ /",
  "S05bsu027_u:75": "sukhā duḥkhā aduḥkhāsukhā ca //",
  "S05bsu027_u:76": "vedanāpratyayā tṛṣṇeti tṛṣṇā katamā?",
  "S05bsu027_u:77": "tisrastṛṣṇāḥ /",
  "S05bsu027_u:78": "kāmatṛṣṇā rūpatṛṣṇā arūpyatṛṣṇā ca //",
  "S05bsu027_u:79": "tṛṣṇāpratyayamupādānamiti upādānaṃ katamat?",
  "S05bsu027_u:80": "catvāryupādānāni /",
  "S05bsu027_u:81": "kāmopādānaṃ dṛṣṭyupādānaṃ śīlavratopādānamātmavādopādānam //",
  "S05bsu027_u:82": "upādānapratyayo bhava iti bhavaḥ katamaḥ?",
  "S05bsu027_u:83": "trayo bhavāḥ /",
  "S05bsu027_u:84": "kāmabhavaḥ rūpabhavaḥ arūpyabhavaḥ //",
  "S05bsu027_u:85": "bhavapratyayā jātiriti jātiḥ katamā?",
  "S05bsu027_u:86_0": "yā teṣāṃ teṣāṃ sattvānāṃ tasmiṃstasmin sattvanikāye jātiḥ saṃjātiravakrāntirabhirvṛttiḥ",
  "S05bsu027_u:86_1": "prādurbhāvaḥ skandhapratilambho dhātupratilambhaḥ āyatanapratilambhaḥ skandhānāmabhinirvṛttiḥ",
  "S05bsu027_u:86_2": "jīvitendriyasya prādurbhāvaḥ //",
  "S05bsu027_u:87": "jātipratyayaṃ jarāmaraṇamiti jarā katamā?",
  "S05bsu027_u:88_0": "yattat khālatyaṃ pālityaṃ valīpracuratā jīrṇatā bhugnatā kubjagopānasīvaṅkatā tilakālakācitagātratā",
  "S05bsu027_u:88_1": "khuapraśvāsakāyatā purataḥ prāgbhārakāyatā daṇḍaviṣkambhaṇatā dhandhatvaṃ mandatvaḥ",
  "S05bsu027_u:88_2": "hāniḥ parihāṇiḥ indriyāṇāṃ paripākaḥ paribhedaḥ saṃskārāṇāṃ purāṇībhāvaḥ jarjarībhāvaḥ /",
  "S05bsu027_u:89": "iyamucyate jarā //",
  "S05bsu027_u:90": "maraṇaṃ karamat?",
  "S05bsu027_u:91": "yā teṣāṃ teṣāṃ sattvānāṃ tasmāttasmāt sattvanikāyāt cyutiḥ cyavanato bhedaḥ antarahāṇiḥ āyuṣo hāṇiḥ uṣmaṇo hāṇiḥ jīvitendriyasya nirodhaḥ skandhānāṃ nikṣepaḥ maraṇaṃ kālakriyā /",
  "S05bsu027_u:92": "idamucyate maraṇamiti /",
  "S05bsu027_u:93": "idaṃ ca maraṇa pūrvikā ca jarā,",
  "S05bsu027_u:94": "tadubhayamaikadhyamabhisaṃkṣipya jarāmaraṇamityucyate //",
  "S05bsu027_u:95": "ayamucyate pratītyasamutpādasya vibhaṅgaḥ /",
  "S05bsu027_u:96": "pratītyasamutpādasyādiṃ vo deśayiṣyāmi iti vibhaṅgaṃ ca iti vo yaduktam,",
  "S05bsu027_u:97": "tadetat pratyuktam //",
  "S05bsu027_u:98": "idamavocadbhagavān /",
  "S05bsu027_u:99": "āttamanasaste bhikṣavo bhagavato bhāṣitamabhyanandan //",
  "S05bsu027_u:100": "pratītyasamutpādādivibhaṅganirdeśasūtraṃ samāptam //"
}