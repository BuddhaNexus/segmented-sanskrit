{
  "SA05n836:0": "catuḥṣaṣṭisaṃvarastotram",
  "SA05n836:1": "śrīherukaṃ mahāvīraṃ viśuddhaṃ kuliśeśvaram |",
  "SA05n836:2": "namāmi sarvabhāvena ḍākinīgaṇabhūṣitam || 1 ||",
  "SA05n836:3": "saṃvarāya namastubhyaṃ dvayākārāya namo namaḥ |",
  "SA05n836:4": "cakrasthitāya devāya cakrasaṃvara te namaḥ || 2 ||",
  "SA05n836:5": "meṣavaktra namaste'stu śivaśaktisvarūpiṇe |",
  "SA05n836:6": "mahākrodhasvarūpāya meṣasaṃvara te namaḥ || 3 ||",
  "SA05n836:7": "aśvānanāya devāya ratikarmaratāya ca |",
  "SA05n836:8": "bhuktimuktipradātre ca aśvasaṃvara te namaḥ || 4 ||",
  "SA05n836:9": "vyāghrāsyāya namastubhyaṃ śaktiyuktāya vai namaḥ |",
  "SA05n836:10": "surāmāṃsarato nityaṃ vyāghrasaṃvara te namaḥ || 5 ||",
  "SA05n836:11": "kūrmāsyāya namastubhyaṃ surate saṃratāya ca |",
  "SA05n836:12": "namo devādhidevāya kūrmasaṃvara te namaḥ || 6 ||",
  "SA05n836:13": "namāmi matsyavaktrāya mānavānāṃ hitāya vai |",
  "SA05n836:14": "namo namaste deveśa matsyasaṃvara te namaḥ || 7 ||",
  "SA05n836:15": "makarākāravaktrāya mahāśaktidharāya ca |",
  "SA05n836:16": "manovāñchāpradātre ca makarasaṃvara te namaḥ || 8 ||",
  "SA05n836:17": "saṃvarāyoṣṭravaktrāya śaktikāliṅgaṇāya ca |",
  "SA05n836:18": "maithune tatparāyātha uṣṭrasaṃvara te namaḥ || 9 ||",
  "SA05n836:19": "namāmi gajavaktrāya yonau liṅgapradāya ca |",
  "SA05n836:20": "namo bhagavate tubhyaṃ gajasaṃvara te namaḥ || 10 ||",
  "SA05n836:21": "maṇḍūkānananāthāya trailokyeśāya te namaḥ |",
  "SA05n836:22": "namo devādhidevāya maṇḍūkasaṃvarāya ca || 11 ||",
  "SA05n836:23": "namāmi cāvivaktrāya nityaṃ ratiratāya ca |",
  "SA05n836:24": "namaste devadeveśa ahisaṃvara te namaḥ || 12 ||",
  "SA05n836:25": "śukānanāya devāya dvayākārākāraśobhine |",
  "SA05n836:26": "namo namaste bhīmāṅga śukasaṃvara te namaḥ || 13 ||",
  "SA05n836:27": "saṃvarāya namaste'stu śaktiyuktāya vai namaḥ |",
  "SA05n836:28": "siṃhānanāya vai nityaṃ siṃhasaṃvara te namaḥ || 14 ||",
  "SA05n836:29": "markaṭānanadevāya śakticumbanaratāya ca |",
  "SA05n836:30": "namaste'stu namaste'stu harisaṃvara te namaḥ || 15 ||",
  "SA05n836:31": "śvānavaktra namaste'stu śakticumbanakāriṇe |",
  "SA05n836:32": "namaste bhagavan deva śvānasaṃvara te namaḥ || 16 ||",
  "SA05n836:33": "varāhāsyavarevāna bhagakrīḍanakāraka |",
  "SA05n836:34": "namāmi śaktiyuktāya ghṛṣṭisaṃvara te namaḥ || 17 ||",
  "SA05n836:35": "jambukāsyāya devāya śaktikāliṅgaṇāya ca |",
  "SA05n836:36": "sarvapāpaharāyaiva śivāsaṃvara te namaḥ || 18 ||",
  "SA05n836:37": "namāmi gṛdhravaktrāya duḥkhanāśāya te namaḥ |",
  "SA05n836:38": "dvayākārasiddhidevāya gṛdhrasaṃvara te namaḥ || 19 ||",
  "SA05n836:39": "kākānanāya śuddhayakṣa śṛṅgārarūpadhāriṇe |",
  "SA05n836:40": "namo bhagavate tubhyaṃ kākasaṃvara te namaḥ || 20 ||",
  "SA05n836:41": "ulūkavaktriṇe tubhyaṃ mahāsaukhyapradāya ca |",
  "SA05n836:42": "yonimaithunakṛddevolūkasaṃvara te namaḥ || 21 ||",
  "SA05n836:43": "tārkṣyānanāya devāya tāriṇe bhavasāgarāt |",
  "SA05n836:44": "namastubhyaṃ namastubhyaṃ namo garuḍasaṃvara || 22 ||",
  "SA05n836:45": "govaktrāya namastubhyaṃ gohatyāpāpahāriṇe |",
  "SA05n836:46": "gotravṛddhipradātre ca dhenusaṃvara te namaḥ || 23 ||",
  "SA05n836:47": "gardabhākāravaktrāya gatāgatakṣayāya te |",
  "SA05n836:48": "gaṇeśvarāya devāya kharasaṃvara te namaḥ || 24 ||",
  "SA05n836:49": "mahiṣāsyāya devāya mahāpralayakāriṇe |",
  "SA05n836:50": "śaktiyuktāya devāya namao mahiṣasaṃvara || 25 ||",
  "SA05n836:51": "viḍālāsya namastubhyaṃ mūtrakūṇḍaprakrīḍine |",
  "SA05n836:52": "śreṣṭhāya parameśāya namo mārjārasaṃvara || 26 ||",
  "SA05n836:53": "namāmi śālvavaktrāya dvayaṅgamekeva śobhitam |",
  "SA05n836:54": "namāmi devadeveśa namaḥ śarabhasaṃvara || 27 ||",
  "SA05n836:55": "siddhāya siddharūpāya guṇāya guṇavartine |",
  "SA05n836:56": "krauñcāsyāya namastubhyaṃ krauñcasaṃvara te namaḥ || 28 ||",
  "SA05n836:57": "ulkānanāya śuddhāya uttamāya namo namaḥ |",
  "SA05n836:58": "ugrāya bhīmarūpāya ulkasaṃvara te namaḥ || 29 ||",
  "SA05n836:59": "haṃsavaktra namaste'stu haṃsasvarasvarūpiṇe |",
  "SA05n836:60": "haṃsaḥso'haṃsvarūpāya haṃsasaṃvara te namaḥ || 30 ||",
  "SA05n836:61": "mṛgavaktrāya devāya namāmi parameśvara |",
  "SA05n836:62": "ṛddhisiddhipradātre ca mṛgasaṃvara te namaḥ || 31 ||",
  "SA05n836:63": "śaktiyukta namaste'stu cakravākānanāya ca |",
  "SA05n836:64": "namaste cakavākākhyasaṃvarāya namo namaḥ || 32 ||",
  "SA05n836:65": "ajānanāya vīrāya avidyānāśine namaḥ |",
  "SA05n836:66": "apavargaphalāptyarthamajasaṃvara te namaḥ || 33 ||",
  "SA05n836:67": "kukkuṭāsyāya devāya kulavṛddhikarāya ca |",
  "SA05n836:68": "namaste kauṭukeśāya tubhyaṃ kukkuṭasaṃvara || 34 ||",
  "SA05n836:69": "kṛṣṇasārasavaktrāya namaste karmasambhava |",
  "SA05n836:70": "kālanāśāya devāya eṇasaṃvara te namaḥ || 35 ||",
  "SA05n836:71": "mūṣānanāya pūrṇāya jñānadāya namo namaḥ |",
  "SA05n836:72": "sarvadaityavināśaya mūṣasaṃvara te namaḥ || 36 ||",
  "SA05n836:73": "sālūkāsya namaste'stu mānādivaradāya ca |",
  "SA05n836:74": "siddhibuddhipradātre ca namaḥ sālūkasaṃvara || 37 ||",
  "SA05n836:75": "namaḥ kapolavaktrāya prajñopāyātmarūpiṇe |",
  "SA05n836:76": "namaste'stu mahāvīra kapotasaṃvarāya ca || 38 ||",
  "SA05n836:77": "namāmi grāhavaktrāya bhuktimuktipradāya ca |",
  "SA05n836:78": "namo'stu sarvabhūteśa grāhasaṃvara te namaḥ || 39 ||",
  "SA05n836:79": "namaste cihlavaktrāya namaste maithune rata |",
  "SA05n836:80": "namaste bhagavan deva cihlasaṃvara te namaḥ || 40 ||",
  "SA05n836:81": "caṭakāsyāya devāya namaste'stu jagadguro |",
  "SA05n836:82": "namaste'stu guṇādhīśa caṭakasaṃvara te namaḥ || 41 ||",
  "SA05n836:83": "sārasāsya namastubhyaṃ namaste guṇasāgara |",
  "SA05n836:84": "namo bhagavate tubhyaṃ sārasasaṃvarāya ca || 42 ||",
  "SA05n836:85": "khañjanāsyāya devāya mahādevāya te namaḥ |",
  "SA05n836:86": "nirvāṇapadadātre khañjarīṭasaṃvarāya ca || 43 ||",
  "SA05n836:87": "namaste kṣemakaryāsya bhuktimuktipradāya ca |",
  "SA05n836:88": "namo bhagavate deva kṣemakarisaṃvarāya te || 44 ||",
  "SA05n836:89": "śaśakāsya namastubhyaṃ namaste bhuvaneśvara |",
  "SA05n836:90": "karmapradāya te nityaṃ namaḥ śaśakasaṃvara || 45 ||",
  "SA05n836:91": "namo bhallūkavaktrāya ratikrīḍāparāya ca |",
  "SA05n836:92": "kāryasiddhipradātre ca namo bhallūkasaṃvara || 46 ||",
  "SA05n836:93": "pikāsyāya namastubhyaṃ mantrasiddhikarāya ca |",
  "SA05n836:94": "trilokeśāya sarvāya pikasaṃvara te namaḥ || 47 ||",
  "SA05n836:95": "namāmi vakavaktrāya śaktiyuktagaṇādhipa |",
  "SA05n836:96": "namāmi devadeveśa vakasaṃvara te namaḥ || 48 ||",
  "SA05n836:97": "khaṅgivaktrāya devāya sarvadā śirasā namaḥ |",
  "SA05n836:98": "namo'stu parameśāya khaṅgisaṃvara te namaḥ || 49 ||",
  "SA05n836:99": "karkaṭāsya namastubhyaṃ namaḥ saṃsārahetave |",
  "SA05n836:100": "pāpapuñjavināśāya namaḥ karkaṭasaṃvara || 50 ||",
  "SA05n836:101": "namaḥ śallakivaktrāya sarvadoṣanivāriṇe |",
  "SA05n836:102": "saṃsārapāśanāśāya namaḥ śallakisaṃvara || 51 ||",
  "SA05n836:103": "vṛścikāsya namastubhyaṃ balakalyāṇadāya ca |",
  "SA05n836:104": "namo namaste devāya namo vṛścikasaṃvara || 52 ||",
  "SA05n836:105": "alivaktrāya devāya alimodāya te namaḥ |",
  "SA05n836:106": "alilokavināśāya alisaṃvara te namaḥ || 53 ||",
  "SA05n836:107": "jāhakāsya namaste'stu janamohakṛtāya vai |",
  "SA05n836:108": "jayakalyāṇadātre ca namo jāhakasaṃvara || 54 ||",
  "SA05n836:109": "camarīvaktra te nityaṃ cāturyaphaladāyine |",
  "SA05n836:110": "caturvargapradātre ca namaścamarisaṃvara || 55 ||",
  "SA05n836:111": "sīmnivaktranamastubhyaṃ siddhitvaphaladāyine |",
  "SA05n836:112": "namo bhagavate nityaṃ sīmnisaṃvara te namaḥ || 56 ||",
  "SA05n836:113": "namāmi godhavaktrāya namaste bhīmavikrama |",
  "SA05n836:114": "namaste divyanetrāya godhāsaṃvara te namaḥ || 57 ||",
  "SA05n836:115": "cakorāsya namastubhyaṃ cāñcalyadoṣanāśaka |",
  "SA05n836:116": "namo bhagavate tubhyaṃ cakorasaṃvarāya ca || 58 ||",
  "SA05n836:117": "godhikāsyāya satataṃ sṛṣṭisaṃhārakariṇe |",
  "SA05n836:118": "namaste paramānanda godhikāsaṃvarāya ca || 59 ||",
  "SA05n836:119": "madhumākṣikavaktrāya namaste mānadāyaka |",
  "SA05n836:120": "sarvajñāya pareśāya namo mākṣikasaṃvara || 60 ||",
  "SA05n836:121": "pataṅgavaktra te nityaṃ parameśa namaḥ sadā |",
  "SA05n836:122": "divyāya divaseśāya namaḥ pataṅgasaṃvara || 61 ||",
  "SA05n836:123": "naravaktrāya devāya narakadhnāya te namaḥ |",
  "SA05n836:124": "nānārūpadadhānāya narasaṃvara te namaḥ || 62 ||",
  "SA05n836:125": "sṛṣṭirūpa jagaddhāma sṛjate sarvabhūtakam |",
  "SA05n836:126": "namo namaste satataṃ sṛṣṭisaṃvara te namaḥ || 63 ||",
  "SA05n836:127": "sthitirūpāya devāya guṇāya guṇavartine |",
  "SA05n836:128": "namaḥ sāravināśāya sthitisaṃvara te namaḥ || 64 ||",
  "SA05n836:129": "sarvalokasya saṃhārakartre te parameśvara |",
  "SA05n836:130": "tasmādahaṃ namasyāmi namaḥ pralayasaṃvara || 65 ||",
  "SA05n836:131": "jyotirvaktra paraṃ dhāma namaste jagadīśvara |",
  "SA05n836:132": "parātparataraṃ sūkṣma jyotiḥsaṃvara te namaḥ || 66 ||",
  "SA05n836:133": "itīdaṃ saṃvarastotraṃ triṣu lokeṣu durlabham |",
  "SA05n836:134": "parātparataraṃ stotraṃ catuḥṣaṣṭipramāṇakam || 67 ||",
  "SA05n836:135": "catuḥṣaṣṭipramāṇeṣu ādyamādyaṃ tu vaktritam |",
  "SA05n836:136": "bhūyo granthamayāccātra ādyamātraṃ pracoditam || 68 ||",
  "SA05n836:137": "kanyārthī labhate kanyāṃ dhanārthī labhate dhanam |",
  "SA05n836:138": "vidyārthī labhate vidyāṃ mokṣārthī mokṣamāpnuyāt || 69 ||",
  "SA05n836:139": "vaśīkaraṇamuccāṭaṃ māramohanastambhanam |",
  "SA05n836:140": "ākarṣaṇaṃ ca vidveṣaṃ dhātuvādaṃ rasāyanam || 70 ||",
  "SA05n836:141": "guṭikāṃ pādukāsiddhiṃ khaṅgasiddhiṃ tathaiva ca |",
  "SA05n836:142": "khecarīsiddhi vaidyāṅgaṃ mantrasiddhiṃ ca vākpaṭuḥ || 71 ||",
  "SA05n836:143": "parakāyapraveśaṃ ca dravyākarṣaṇameva ca |",
  "SA05n836:144": "labhate stotrarājena satyaṃ satyaṃ mayoditam || 72 ||",
  "SA05n836:145": "durbhikṣaṃ cāpadi pāṭhaṃ kurute śubhamāpnuyāt |",
  "SA05n836:146": "ativṛṣṭāvanāvṛṣṭau mahāmārīsamudbhave || 73 ||",
  "SA05n836:147": "rājyabhraṃśe jñānabhraṃśe strībhraṃśe ca dhanakṣaye |",
  "SA05n836:148": "kalahe ca vivāde ca paṭhate stotramuttamam || 74 ||",
  "SA05n836:149": "bhikṣukebhyaḥ sahasrebhyaḥ tulyaṃ dānakṛtaṃ phalam |",
  "SA05n836:150": "bahunātra kimuktena brahmāṇḍadānajaṃ phalam || 75 ||",
  "SA05n836:151": "śrīsaṃvarāgame mahātantre amitābhavairocanasaṃvāde",
  "SA05n836:152": "śrīcakrasaṃvarādicatuḥṣaṣṭisaṃvarastotraṃ samāptam |"
}