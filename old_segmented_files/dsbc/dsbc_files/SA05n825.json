{
  "SA05n825:0": "ratnamālāstotram",
  "SA05n825:1": "ācāryavanaratnapādaviracitam",
  "SA05n825:2": "lokeśvaraṃ vimalaśūnyakṛpārdracittaṃ",
  "SA05n825:3": "mārgajñatāprathitadeśanayārthavācam |",
  "SA05n825:4": "sarvajñatādiparipūrṇaviśuddhadehaṃ",
  "SA05n825:5": "jñānādhikāralalitaṃ śirasā namāmi || 1 ||",
  "SA05n825:6": "vaineyabhedavaśato bahudhāvabhāsai-",
  "SA05n825:7": "reko'pi pātragajaleṣu śaśīva yasmāt |",
  "SA05n825:8": "saṃlakṣase parahitānugataiva tasmād",
  "SA05n825:9": "buddhistvaho paramavismayanīyarūpā || 2 ||",
  "SA05n825:10": "saṃpūrṇacandravadane lalito lalāṭa-",
  "SA05n825:11": "deśādvinirgatamaheśvaradevaputraḥ |",
  "SA05n825:12": "vaineyaśāmbhavajanapratibodhanārthaṃ",
  "SA05n825:13": "devādhidevapratimānuja īśvarastvam || 3 ||",
  "SA05n825:14": "vaineyakomalabhavapratibodhanāya",
  "SA05n825:15": "kiṃ dhāma saṃbhṛta mahāśubhalakṣaṇaṃ te |",
  "SA05n825:16": "niryāta eva hi pitāmahadevapūjāṃ",
  "SA05n825:17": "lokeśvareśvaraparaṃ śirasā namāmi || 4 ||",
  "SA05n825:18": "vaineyavaiṣṇavajanapratibodhanāya",
  "SA05n825:19": "rājīvapāṇihṛdayāt pratiniḥsṛto'sau |",
  "SA05n825:20": "nārāyaṇo'pi bhuvaneśvara eva tasmāt",
  "SA05n825:21": "puṃsāṃ tvameva paramottama eva nānyaḥ || 5 ||",
  "SA05n825:22": "candrārkasādarabalāhitabhaktibhājāṃ",
  "SA05n825:23": "saṃdarśanārthamibhanīlasulocanābhyām |",
  "SA05n825:24": "yanniḥsṛtau śaśiravī bhuvi locanābhyāṃ",
  "SA05n825:25": "dhvastāntarālatamasaṃ tamahaṃ namāmi || 6 ||",
  "SA05n825:26": "sārasvatīvinayayojitabhaktibhājāṃ",
  "SA05n825:27": "bodhāya vai bhagavatīha sarasvatīyam |",
  "SA05n825:28": "dṛṣṭāgratastava jinātmajapaprasūtā",
  "SA05n825:29": "prajñābhilāṣiphaladaṃ tamahaṃ namāmi || 7 ||",
  "SA05n825:30": "vaineyavāyujanitākṣaramārgasiddhyai",
  "SA05n825:31": "yo lokanātha sugato'tha viniḥsṛto'sau |",
  "SA05n825:32": "devaḥ samīraṇavaro bhuvi janmabhājā-",
  "SA05n825:33": "mīryāpathārthaphaladaṃ tamahaṃ namāmi || 8 ||",
  "SA05n825:34": "vaineyavāruṇaśivāyanamīpsitānāṃ",
  "SA05n825:35": "saṃbodhanārthamudarātsugatātmajānām |",
  "SA05n825:36": "yanniḥsṛto varuṇadevavaro'pyakasmā-",
  "SA05n825:37": "daiśvaryasiddhi phaladaṃ tamahaṃ namāmi || 9 ||",
  "SA05n825:38": "vaineyasaṃmataphalādyabhilāṣiṇo vai",
  "SA05n825:39": "saṃsiddhaye pravaralakṣaṇapādapadme |",
  "SA05n825:40": "yanniḥsṛtā bhagavatī dharaṇī prasiddhā",
  "SA05n825:41": "trailokyanāthamasamaṃ satataṃ namāmi || 10 ||",
  "SA05n825:42": "saṃsāramuktamapi susthitameva tatra",
  "SA05n825:43": "kāruṇyataśca bhavacāriṇi sattvavarge |",
  "SA05n825:44": "bhūyāt sthitirmama sadāsthirasā bhavanta-",
  "SA05n825:45": "mevaṃ mahāśayavaraṃ paramaṃ namāmi || 11 ||",
  "SA05n825:46": "ekena pādatalakena bhavatsvakena",
  "SA05n825:47": "cakrāntasaṃvaramanantaralokadhātau |",
  "SA05n825:48": "kalpāntadagdhabhuvane jvalitogravahni-",
  "SA05n825:49": "rniḥśvāsavāyubalatastava nirvṛtaḥ syāt || 12 ||",
  "SA05n825:50": "svāṃ tarjanīṃ mukhadhṛto'hitatarjanena",
  "SA05n825:51": "saṃcālitāśca bahumerugaṇā nakhasya |",
  "SA05n825:52": "koṣoddhṛtaṃ jaladhitoyamaśeṣataḥ syāt",
  "SA05n825:53": "sāmarthyamīdṛśamaho bhavataḥ kuto'nyat || 13 ||",
  "SA05n825:54": "kvedaṃ ca śaiśavaparaṃ nanu cārurupaṃ",
  "SA05n825:55": "saṃdarśanīyavarakomalabālacandram |",
  "SA05n825:56": "durvāramāramathanaṃ ca mayaikasahyaṃ",
  "SA05n825:57": "vikrāntaduḥsahaparaṃ kva ca ceṣṭitaṃ te || 14 ||",
  "SA05n825:58": "eṣā batāñjananibhorujanāvalī sā",
  "SA05n825:59": "kauṭilyacāruvikaṭā svaśiroruhāgre |",
  "SA05n825:60": "kleśendhane jvalitavisphuritatvavahne-",
  "SA05n825:61": "rdhūmāvalīva vimalā nanu lakṣyate te || 15 ||",
  "SA05n825:62": "tvatkāntileśavimalā daśadikpratānaiḥ",
  "SA05n825:63": "pakṣāsitakṣayakṛśā sakalā suśobhā |",
  "SA05n825:64": "paryanta iṣṭaśaśino bhavaneṣu yatte",
  "SA05n825:65": "manye virāji nikhilaṃ tava kāntileśāt || 16 ||",
  "SA05n825:66": "bandhurhi ko mārgikasaṃmataṃ mataṃ naro narī sā sa ca satpathaṃ patham |",
  "SA05n825:67": "parārthasaṃpāditasaṃvaraṃ varaṃ namāmi bhūmīśvararājinaṃ jinam || 17 ||",
  "SA05n825:68": "anityanirvāṇapade sthitaṃ sthitaṃ prabhāsvarādhiṣṭhitasaṃhitaṃ hitam |",
  "SA05n825:69": "śamīkṛtāśeṣajanaṃ śivaṃ śivaṃ namāmi bhūmīśvararājinaṃ jinam || 18 ||",
  "SA05n825:70": "gabhastimālāmitasaṃkulaṃ kulaṃ tatra svapāṇau dhṛtapaṅkajaṃ kajam |",
  "SA05n825:71": "ratānugāśobhitasaṃrataṃ rataṃ namāmi bhūmīśvararājinaṃ jinam || 19 ||",
  "SA05n825:72": "svadharmadhātuṃ karuṇāparaṃ paraṃ śubhādisaṃbhārasusaṃbhṛtaṃ bhṛtam |",
  "SA05n825:73": "vikalpahīnaṃ dhvanideśakaṃ śakaṃ namāmi bhūmīśvarājinaṃ jinam || 20 ||",
  "SA05n825:74": "tathatātathatādvayaśātaśataṃ sadasatparipūritadharmakatham |",
  "SA05n825:75": "kathanīyavirājitasatyaparaṃ praṇame dharaṇīśvararājavaram || 21 ||",
  "SA05n825:76": "varavārijarūpi jagatprasaraṃ sarasīruhalocanacārutaram |",
  "SA05n825:77": "tarasāpi rasatvaviśuddhiparaṃ praṇame dharaṇīścarājavaram || 22 ||",
  "SA05n825:78": "varanirmitabhogaparārtharataṃ rataśūnyanirañjanadharmadharam |",
  "SA05n825:79": "dharaṇīndravibhūṣitasiddhiparaṃ praṇame dharaṇīśvararājavaram || 23 ||",
  "SA05n825:80": "varasatsahajodadhicandramukhaṃ sukhabhāṣitasattvavimuktipadam |",
  "SA05n825:81": "padabhūṣaṇalakṣaṇatānuparaṃ praṇame dharaṇīśvararājavaram || 24 ||",
  "SA05n825:82": "lokeśvareyaṃ (māṃ)",
  "SA05n825:83": "tava ratnamālāmacīkaracchrīvanaratnapādaḥ |",
  "SA05n825:84": "avāpi yattena śubhapraviṣṭaṃ tenaiva loko'stu samantabhadraḥ || 25 ||",
  "SA05n825:85": "śrīmadāryāvalokiteśvarabhaṭṭārakasya",
  "SA05n825:86": "ratnamālāstotraṃ samāptam |"
}