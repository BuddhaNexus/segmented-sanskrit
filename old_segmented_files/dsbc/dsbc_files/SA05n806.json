{
  "SA05n806:0": "(ārya) tārānamaskāraikaviṃśatistotram",
  "SA05n806:1": "om namo bhagavatyai āryaśrī-ekaviṃśatitārāyai",
  "SA05n806:2": "namastāre ture vīre kṣaṇadyutinibhekṣaṇe |",
  "SA05n806:3": "trailokyanāthavaktrābjavikasatkamalodbhave || 1 ||",
  "SA05n806:4": "namaḥ śataśaraccandrasaṃpūrṇeva varānane |",
  "SA05n806:5": "tārāsahasrakiraṇaiḥ prahasatkiraṇojjvale || 2 ||",
  "SA05n806:6": "namaḥ kanakanīlābja-pāṇipadmavibhūṣite |",
  "SA05n806:7": "dānavīryatapaḥśā(kṣā)",
  "SA05n806:8": "ntititikṣādhyānagocare || 3 ||",
  "SA05n806:9": "namastathāgatoṣṇīṣavijayānantacāriṇi |",
  "SA05n806:10": "śeṣapāramitāprāptajinaputraniṣevite || 4 ||",
  "SA05n806:11": "namastutārahuṃkārapūritāśādigantare |",
  "SA05n806:12": "saptalokakramākrā[nte] aśeṣakaruṇā(ṇe)kṣaṇe || 5 ||",
  "SA05n806:13": "namaḥ śakrānalabrahmamarudviśveśvarārcite |",
  "SA05n806:14": "bhūtavetālagandharvagaṇayakṣapuraskṛte || 6 ||",
  "SA05n806:15": "namaḥ straditi phaṭkāra parajatra(yantra)",
  "SA05n806:16": "pramardini |",
  "SA05n806:17": "pratyālīḍhapadanyāse śikhī(khi)",
  "SA05n806:18": "jvālākulojjvale || 7 ||",
  "SA05n806:19": "namasture mahāghore mālavīravināśini |",
  "SA05n806:20": "bhṛkuṭīkṛtavaktrābjasarvaśatrunisundanī(ṣūdini) || 8 ||",
  "SA05n806:21": "namaḥ strīratnamudrāṅkahṛdayāṅgulibhūṣite |",
  "SA05n806:22": "bhūṣitāśeṣadikcakranikarasvakarākule || 9 ||",
  "SA05n806:23": "namaḥ pramuditāśeṣamuktākṣīraprasāriṇi |",
  "SA05n806:24": "hasatprahasatuttāre māralolavaśaṅkari || 10 ||",
  "SA05n806:25": "namaḥ samantabhūpālapata(ṭa)",
  "SA05n806:26": "lākarṣaṇa(ṇe)kṣaṇe |",
  "SA05n806:27": "carabhṛkuṭihūṃkārasarvāpadavimocanī(cini) || 11 ||",
  "SA05n806:28": "namaḥ śrīkhaṇḍakhaṇḍendu[su]muktābharaṇa(ṇo)jjvale |",
  "SA05n806:29": "amitābhajitābhārabhāsure kiraṇoddhruve(ddhure) || 12 ||",
  "SA05n806:30": "namaḥ kalpāntahutabhugajvālāmālāntare(ra)sthite |",
  "SA05n806:31": "ālīḍhamudi(dri)",
  "SA05n806:32": "tābaddharipucakravināśinī(ni) || 13 ||",
  "SA05n806:33": "namaḥ karatarā(lā)",
  "SA05n806:34": "ghāṭa(ta)",
  "SA05n806:35": "caraṇāhatabhūtale |",
  "SA05n806:36": "bhṛkuṭīkṛtahukārasaptapātālabhedinī(ni) || 14 ||",
  "SA05n806:37": "namaḥ śive śubhe śānte śāntanirvāṇagocare |",
  "SA05n806:38": "svāhāpraṇavasaṃyukte mahāpātakanāśanī(śini) || 15 ||",
  "SA05n806:39": "namaḥ pramuditābaddharigātraprabhedini |",
  "SA05n806:40": "daśākṣarapadanyāse vidyāhuṃkāradīpite || 16 ||",
  "SA05n806:41": "nama[stāre] ture pādaghātahuṃkāravījite |",
  "SA05n806:42": "merumaṇḍalakailāśabhuvanatrayacāriṇī(ṇi) || 17 ||",
  "SA05n806:43": "namaḥ sure sa(śa)",
  "SA05n806:44": "rākārahariṇāṅkakare(ra)sthite |",
  "SA05n806:45": "haridviruktaphaṭkāra(re)",
  "SA05n806:46": "aśeṣaviṣanāśiṇī(ni) || 18 ||",
  "SA05n806:47": "namaḥ surāsuragaṇayakṣakinnarasevite |",
  "SA05n806:48": "abuddhamuditābhogakarī(ri)",
  "SA05n806:49": "duḥsvapnanāśinī(ni) || 19 ||",
  "SA05n806:50": "namaścandrārkasampūrṇanayanadyutibhāsvare |",
  "SA05n806:51": "tārādviruktatuttāre viṣamajvala(ra)nāśini || 20 ||",
  "SA05n806:52": "namaḥ strītattvavinyāse śivaśaktisamanvite |",
  "SA05n806:53": "grahavetāra(la)",
  "SA05n806:54": "yakṣoṣmanāśini pravare ture || 21 ||",
  "SA05n806:55": "mantramūlamidaṃ stotraṃ namaskāraikaviṃśatiḥ(ti) |",
  "SA05n806:56": "yaḥ paṭhetprātaḥ (paṭhet prayataḥ)",
  "SA05n806:57": "dhīmān devyābhaktisamanvite(taḥ) ||22 ||",
  "SA05n806:58": "sāyaṃ vā prātarutthāya smaret sarvābhayapradam |",
  "SA05n806:59": "sarvapāpapraśamanaṃ sarvadurgatināśanam || 23 ||",
  "SA05n806:60": "abhiṣikto bhavet tūrṇaṃ saptabhirjinakoṭibhiḥ |",
  "SA05n806:61": "māsamātreṇa caivāsau sukhaṃ bauddhapadaṃ vrajet || 24 ||",
  "SA05n806:62": "viṣaṃ tasya mahāghoraṃ sthāvaraṃ cātha jaṅgamam |",
  "SA05n806:63": "smaraṇānna padaṃ yāti khāditaṃ pi(pī)tameva vā || 25 ||",
  "SA05n806:64": "grahajo(jā)laviṣārtānāṃ parastrīviṣanāśanam |",
  "SA05n806:65": "anyeṣāṃ caiva sattvānāṃ dvisaptamabhivartitam || 26 ||",
  "SA05n806:66": "putrakāmo labhet putraṃ ghanakāmo labheddhanam |",
  "SA05n806:67": "sarvakāmānavāpnoti na vighnaiḥ pratihanyate || 27 ||",
  "SA05n806:68": "iti śrīsamyaksaṃbuddhavailo(ro)",
  "SA05n806:69": "canabhāṣitaṃ bhagavatyāryatārādevyā",
  "SA05n806:70": "namaskāraikaviṃśatināmāṣṭottaraśatakaṃ buddhabhāṣitaṃ parisamāptam"
}