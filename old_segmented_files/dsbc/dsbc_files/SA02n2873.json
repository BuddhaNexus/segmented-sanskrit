{
  "SA02n2873:3": "Vāṅmaṇḍalanamaskāraślokāḥ",
  "SA02n2873:10": "ekānanā vedakarāstrinetrāḥ cacryādidevyaḥ",
  "SA02n2873:11": "suviśākhapādāḥ |",
  "SA02n2873:15": "nābhi ( bhī )",
  "SA02n2873:16": "sarojasthitayāmanāḍīśuddhāḥ",
  "SA02n2873:17": "suratnānvitamauliyukttāḥ ||1 ||",
  "SA02n2873:24": "pūrve hakatryudbhavakṛṣṇacācrci vande",
  "SA02n2873:25": "śavasthāruṇapadhmasaṃsthām |",
  "SA02n2873:29": "tāṃ kartiśūle ca bhujeṣu śakttiḥ khaṭvāṅga",
  "SA02n2873:30": "indro'ṅkagataśca yasyāḥ || 2",
  "SA02n2873:37": "padhmeṣu carcikādīnāṃ daṇḍanāḍayātmikāḥ kramāt|",
  "SA02n2873:42": "saṃmukhāḍḍakṣiṇāvartte dalasthā lalitāṅghrayaḥ  || 3 ||",
  "SA02n2873:50": "bhīmogrā kāladaṃṣṭrā jvaladanalamukhā vāyuvegā",
  "SA02n2873:51": "pracaṇḍā",
  "SA02n2873:55": "raudrākṣī sthūlanāsā kamalavasudale",
  "SA02n2873:56": "carcikādadyāḥ svadikṣu |",
  "SA02n2873:60": "aṣṭau śrīnāyikābhā hi-bhava-ya-yi-yṛ-hī-jātakartyudbhavāyā-",
  "SA02n2873:64": "stābhyaḥ śrīyoginībhyo nama iha ihu-yḻṛ-yaṃ-jātakartyudbhavāśca  ||4 ||",
  "SA02n2873:72": "vārāhikārkakamale mahiṣāruṇābhā",
  "SA02n2873:76": "haḥkāradaṇḍajanitā sitaśaṅkarāṅkā |",
  "SA02n2873:80": "daṇḍaḥ kareṣvasivaro varaśṛṅkhalā ca",
  "SA02n2873:84": "kheṭaṃ yame padayugaṃ praṇamāmi tasyāḥ ||5 ||",
  "SA02n2873:91": "kaṅkālī kālarātrī ( triḥ )",
  "SA02n2873:92": "prakupitavadanā kāladaṃṣṭrā ( jihvā ) karālī",
  "SA02n2873:96": "kālī ghorā virūpā kamalavasudale śūkarī",
  "SA02n2873:97": "yantradevī |",
  "SA02n2873:101": "aṣṭau śrīnāyikābhā hṛbhava-ra-ri-rṛ-jātadaṇḍodbhavāyāḥ",
  "SA02n2873:105": "tābhyaḥ śrīyoginībhyo nama iha ru-rḻṛ-raṃ jātadaṇḍodbhavāśca  ||6 ||",
  "SA02n2873:113": "śuklottare vṛṣabhalohitapadmasaṃsthāṃ",
  "SA02n2873:117": "raudrīṃ ca haṃjanitaśūlabhavāṃ namāmi |",
  "SA02n2873:121": "yasyāstriśūlaḍamarū kramato bhujeṣu",
  "SA02n2873:125": "khaṭvāṅgaśuklabhujagā varuṇo yamo'ṅke ||7 ||",
  "SA02n2873:132": "gaurī gaṅgā ca nityā svaparamantreritā tro ( to )",
  "SA02n2873:133": "talā lakṣmaṇā ca",
  "SA02n2873:137": "piṅgā kṛṣṇā tathāṣṭau kamalavasudale nāyikā",
  "SA02n2873:138": "yatra raudrī |",
  "SA02n2873:142": "śrīmadraudrīsamānā hubhava-va-vi-vṛ-ha-jātaśūlena",
  "SA02n2873:143": "jātāḥ",
  "SA02n2873:147": "tābhyaḥ śrīyoginībhyo nama iha vu-vḷ-vaṃ-jātaśūlodbhavāyāḥ  ||8 ||",
  "SA02n2873:155": "airāvatāruṇapayojagahājavajra-",
  "SA02n2873:159": "jātā svavarṇasadṛśī danubhūmitāṅkā |",
  "SA02n2873:163": "aindrī sthitā ca varuṇe pavivāṇaghaṇṭā-",
  "SA02n2873:167": "kodaṇḍapāṇikamalā praṇamāmi tāṃ ca ||9 ||",
  "SA02n2873:174": "vajrābhā vajragātrā varakanakavatī corvaśī",
  "SA02n2873:175": "citralekhā",
  "SA02n2873:179": "rambhā'halyā sutārā kamalavasudale",
  "SA02n2873:180": "vajrahastādhideve |",
  "SA02n2873:184": "aṣṭau śrīnāyikābhā hṛbhava-la-li-ḷ-hṛ-jātavajrodbhavāyā-",
  "SA02n2873:188": "stābhyaḥ śrīyoginībhyo nama iha ḷ lḹ laṃ",
  "SA02n2873:189": "jātavajrodbhavābhyaḥ ||10 ||",
  "SA02n2873:196": "brāhmī tu haṃsagasitābjagatārkanetrā",
  "SA02n2873:200": "kṣmājātasūcitacaturmukhapītavarṇā |",
  "SA02n2873:204": "vāyau tu tāṃ dhṛtapayobhavadaṇḍakuṇḍī-",
  "SA02n2873:208": "pātrāṃ namāmi ghanavarṇahariryadaṅke ||11 ||",
  "SA02n2873:215": "sāvitrī padmanetrā khalu jalajavatī",
  "SA02n2873:216": "buddhivāgīśvararddhi-",
  "SA02n2873:220": "rgāyatrī vidyudeva smṛtirapi kamale",
  "SA02n2873:221": "vedavaktrādhidaiva |",
  "SA02n2873:225": "tābhyo brāhmīnibhābhyo nama iha satataṃ",
  "SA02n2873:226": "yoginībhyaḥ kṣṛ-lā-lī-",
  "SA02n2873:230": "ḷ-kṣṝ-lu-ḹ-laḥ ebhiḥ",
  "SA02n2873:231": "kramaparinihitairjātaśūnyodbhavābhyaḥ ||2 ||",
  "SA02n2873:238": "kṣaṃ jātapadmajanitā śaśikāntilakṣmi",
  "SA02n2873:242": "siṃhasthaśuklakamale giriśe namāmi |",
  "SA02n2873:246": "padmākṣasūktrakamalāni maṇiśca pāṇau",
  "SA02n2873:250": "kroḍe sthito rasamukhaścaraṇaṃ hi tasyāḥ ||13 ||",
  "SA02n2873:257": "śrīścetā candralekhā śaśadharavadanā haṃsavarṇā",
  "SA02n2873:258": "dhṛtiśca",
  "SA02n2873:262": "padmeśā tāranetrā vimalaśaśadharā īśapadme",
  "SA02n2873:263": "sacihnā |",
  "SA02n2873:267": "tābhyo lakṣmīnibhābhyo nama iha satataṃ",
  "SA02n2873:268": "yoginībhyaṃ kṣu-rā-rī-",
  "SA02n2873:272": "rṝ-kṣṝ-rū-rḻīī-raḥ-ebhīḥ",
  "SA02n2873:273": "kramaparivihitairjātapadmodbhavābhyaḥ ||14 ||",
  "SA02n2873:280": "kṣaṃ-śakticihnajanitāṃ ravisannibhāṃ tāṃ",
  "SA02n2873:284": "kekisthaśuklakamale gaṇanāyakāṅkām |",
  "SA02n2873:288": "kaumārikāṃ danudiśi praṇamāmi yasyāḥ",
  "SA02n2873:292": "śaktyaṅkṛśau ca maṇipāśavarau kareṣu ||5 ||",
  "SA02n2873:299": "padmānaṅgā kumārī mṛgapatinayanā ratnamālā",
  "SA02n2873:300": "sunetrā",
  "SA02n2873:304": "klīnā bhadrā payoje varaśikhigamanā nāyikā",
  "SA02n2873:305": "yatra rājan |",
  "SA02n2873:309": "tābhyo devīsamābhyo nama iha satataṃ yoginībhyaḥ",
  "SA02n2873:310": "kṣu-rā-rī-",
  "SA02n2873:314": "rṝ-kṣṝ-rū-rḹ-raḥ-ebhiḥ kramaparinihitairjāta [ ratnod ? ]",
  "SA02n2873:315": " bhavābhyaḥ ||16 ||",
  "SA02n2873:322": "vahnau kṣacakrajanitāsitapadmasaṃsthā",
  "SA02n2873:326": "śrīvaiṣṇavī [ ca ] garuḍā caturānanāṅkā |",
  "SA02n2873:330": "atyantakṛṣṇatanucakragadāpayoja-",
  "SA02n2873:334": "śaṅkhottamāṅkitakarā ca namostu tasyai ||17 ||",
  "SA02n2873:341": "śrīmāyākīrtilakṣmyau svaparamavijayā śrījayā",
  "SA02n2873:342": "śrījayantī",
  "SA02n2873:346": "śrīcakrī cāṣṭamī vai kamalasudale vaiṣṇavī dikṣu",
  "SA02n2873:347": "deśe |",
  "SA02n2873:351": "tābhyo devīnibhābhyo nama iha satataṃ",
  "SA02n2873:352": "yoginībhyaḥ kṣi-yā-yī-",
  "SA02n2873:356": "yṛ-kṣī-yḷ-yaḥ-ebhiḥ",
  "SA02n2873:357": "kramavidhinihitairjātacakrodbhavābhyaḥ ||18 ||",
  "SA02n2873:364": "savyadvārvāmavedyāṃ khalu ka-kha-ga-gha-ṅa-vyañjairbinduyugma-",
  "SA02n2873:368": "yuktairbījaiḥ",
  "SA02n2873:369": "prajātairdvibhujaśaśimukhecchādikāḥ pañca jātāḥ |",
  "SA02n2873:373": "pūrvādeḥ savyavedyāmapi ca-ṭa-pa-ta-vargākṣarebhyaḥ",
  "SA02n2873:374": "prajātai-",
  "SA02n2873:378": "ste varṇāḥ pañca pañca pratidiśi nihitā",
  "SA02n2873:379": "bījadhātusvacihnaiḥ ||19 ||",
  "SA02n2873:386": "tadvatsavargajairvāmavedyāṃ pūrvahavargajaiḥ |",
  "SA02n2873:390": "vāmotthekāpare candre kṣaḥkārajena tāstathā  ||20 || [ ||",
  "SA02n2873:398": "iti vāṅmaṇḍalanamaskāraślokāḥ || ]"
}