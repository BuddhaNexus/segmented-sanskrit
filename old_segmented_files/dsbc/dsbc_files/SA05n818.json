{
  "SA05n818:0": "avalokiteśvarāṣṭottaraśatanāmastotram",
  "SA05n818:1": "om namo'valokiteśvarāya",
  "SA05n818:2": "padmasattva mahāpadma lokeśvara maheśvara |",
  "SA05n818:3": "avalokiteśa dhīrāgrya vajradharma namo'stu te || 1 ||",
  "SA05n818:4": "dharmarāja mahāśuddha sattvarāja mahāmate |",
  "SA05n818:5": "padmātmaka mahāpadma padmanātha namo'stu te || 2 ||",
  "SA05n818:6": "padmodbhava supadmābha padmaśuddha suśodhaka |",
  "SA05n818:7": "vajrapadma supadmāṅka padmapadma namo'stu te || 3 ||",
  "SA05n818:8": "mahāviśva mahāloka mahākāya mahopama |",
  "SA05n818:9": "mahādhīra mahāvīra mahāśaure namo'stu te || 4 ||",
  "SA05n818:10": "sattvāśaya mahāyāna mahāyoga pitāmaha |",
  "SA05n818:11": "śambhu śaṅkara śuddhārtha buddhapadma namo'stu te || 5 ||",
  "SA05n818:12": "dharmatattvārtha saddharma śuddhaddharmaṃ sudharmakṛt |",
  "SA05n818:13": "mahādharma sudharmāgrya dharmacakra namo'stu te || 6 ||",
  "SA05n818:14": "buddhasattva susattvāgrya dharmasattva susattvadhṛk |",
  "SA05n818:15": "sattvottama susattvajña sattvasattva namo'stu te || 7 ||",
  "SA05n818:16": "avalokiteśa nāthāgrya mahānātha vilokita |",
  "SA05n818:17": "ālokaloka lokārtha lokanātha namo'stu te || 8 ||",
  "SA05n818:18": "lokākṣarākṣara mahā akṣarāgryākṣaropama |",
  "SA05n818:19": "akṣarākṣara sarvākṣa cakrākṣara namo'stu te || 9 ||",
  "SA05n818:20": "padmahasta mahāhasta samāśvāsaka dāyaka |",
  "SA05n818:21": "buddhadharma mahābuddha buddhātmaka namo'stu te || 10 ||",
  "SA05n818:22": "buddharūpa mahārūpa vajrarūpa surupavit |",
  "SA05n818:23": "dharmāloka sutejāgrya lokāloka namo'stu te || 11 ||",
  "SA05n818:24": "padmaśrīnātha nāthāgrya dharmaśrīnātha nāthavān |",
  "SA05n818:25": "brahyanātha mahābrahma brahmaputra namo'stu te || 12 ||",
  "SA05n818:26": "dīpa dīpāgrya dīpogra dīpāloka sudīpaka |",
  "SA05n818:27": "dīpanātha mahādipa buddhadīpa namo'stu te || 13 ||",
  "SA05n818:28": "buddhābhiṣikta buddhāgrya buddhaputra mahābudha |",
  "SA05n818:29": "buddhābhiṣekamurddhāgrya buddhabuddha namo'stu te || 14 ||",
  "SA05n818:30": "buddhacakṣo mahācakṣo dharmacakṣo mahekṣaṇa |",
  "SA05n818:31": "samādhijñānasarvasva vajranetra namo'stu te || 15 ||",
  "SA05n818:32": "evaṃ sarvātmanā gauṇaṃ nāmnāmaṣṭaśataṃ tava |",
  "SA05n818:33": "bhāvayet stunuyādvāpi lokaiśvaryamavāpnuyāt || 16 ||",
  "SA05n818:34": "āryāvalokiteśvaranāmāṣṭottaraśatādhyeṣaṇāstotraṃ samāptam |"
}