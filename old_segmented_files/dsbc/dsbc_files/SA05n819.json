{
  "SA05n819:0": "lokanāthastotram",
  "SA05n819:1": "kalpādike bhavasi ko hi mamāgabhāva",
  "SA05n819:2": "sarvasvasāra karuṇāmaya viśvamūrte |",
  "SA05n819:3": "kāryādike praṇamatīti samantakaṃ tvāṃ",
  "SA05n819:4": "śrīlokanātha tava pādayugaṃ name'ham || 1 ||",
  "SA05n819:5": "ākṛṣṇakena rajasā vinivartamāna-",
  "SA05n819:6": "ścāyāsi saumya sakalaḥ prativāsare ca |",
  "SA05n819:7": "hemasvarūparathakena samujjvalena",
  "SA05n819:8": "śrīlokanātha tava pādayugaṃ name'ham || 2 ||",
  "SA05n819:9": "brahmā tvameva hi sa viprakulaprasiddho",
  "SA05n819:10": "viṣṇuśca vaiṣṇavamate varadharmaketuḥ |",
  "SA05n819:11": "sarvajñako'si vimate prabhavo'vyayaśca",
  "SA05n819:12": "śrīlokanātha tava pādayugaṃ name'ham || 3 ||",
  "SA05n819:13": "bauddhānvaye bhavasi vajrakasūryarūpo",
  "SA05n819:14": "yogeśvaro hi śubhayogakamārgakeṣu |",
  "SA05n819:15": "gaṅgādharo bhavabhayasya vināśakāri",
  "SA05n819:16": "śrīlokanātha tava pādayugaṃ name'ham || 4 ||",
  "SA05n819:17": "kāruṇyabhāvahṛdayaḥ sahajaḥ saroci-",
  "SA05n819:18": "rvicchinnakalmaṣacayo guṇasāgaraśca |",
  "SA05n819:19": "cintāmaṇistvamasi lokaguruḥ kṛpeśa",
  "SA05n819:20": "śrīlokanātha tava pādayugaṃ name'ham || 5 ||",
  "SA05n819:21": "bandhūkavarṇa bahurūpa viśālanetra",
  "SA05n819:22": "sarvaprasūtikṛtaniṣkṛtikaḥ sudanta |",
  "SA05n819:23": "tvaṃ padmapāṇi vimalottama mitrarūpaḥ",
  "SA05n819:24": "śrīlokanātha tava pādayugaṃ name'ham || 6 ||",
  "SA05n819:25": "tava bahulacaritraṃ kaḥ samartho'sti vaktuṃ",
  "SA05n819:26": "tadapi mukharabhāvaiḥ stūyase tvaṃ mayātra |",
  "SA05n819:27": "yadapi padamaśuddhaṃ sarvametat kṣamasva",
  "SA05n819:28": "stutiriti kusumasrak bhaktimātrārcanaṃ syāt || 7 ||",
  "SA05n819:29": "śrīmadāryāvalokiteśvarabhaṭṭārakasya stotraṃ samāptam |"
}