{
  "SA05n889:0": "mañjuvajrastotram",
  "SA05n889:1": "mañjugarbhakṛtam",
  "SA05n889:2": "namaḥ śrīmañjuvajrāya",
  "SA05n889:3": "śaśadharamiva śubhraṃ khaḍgapustākapāṇiṃ",
  "SA05n889:4": "suruciramaliśāntaṃ pañcacīraṃ kumāram |",
  "SA05n889:5": "pṛthutaravaramokṣaṃ padmapatrāyatākṣaṃ",
  "SA05n889:6": "kumatidahanadakṣaṃ mañjughoṣaṃ namāmi || 1 ||",
  "SA05n889:7": "kṛtamṛgaripuyānaṃ dattabhaktapradānaṃ",
  "SA05n889:8": "suradanujanṛyānaṃ bodhisattvapradhānam |",
  "SA05n889:9": "akhilaguṇanidhānaṃ sarvavidyāvitānaṃ",
  "SA05n889:10": "karasarasijabāṇaṃ mañjughoṣaṃ namāmi || 2 ||",
  "SA05n889:11": "vibhṛtasakalakoṣaṃ kṣālitājñānadoṣaṃ",
  "SA05n889:12": "smaraṇabhajanatoṣaṃ dūrarāgādidoṣam |",
  "SA05n889:13": "vihitasasurapoṣaṃ siddhidākhyānayoṣaṃ",
  "SA05n889:14": "kṛtajaḍapariśoṣaṃ mañjughoṣaṃ namāmi || 3 ||",
  "SA05n889:15": "gaṇapatiśarajanmaśrīmahākālasiṃhaiḥ",
  "SA05n889:16": "parivṛtamiva candrābhābhamindīvarākṣam |",
  "SA05n889:17": "asi-śara-japamālā-pustakaṃ saṃvahantam",
  "SA05n889:18": "urasi lalitamālaṃ mañjughoṣaṃ namāmi || 4 ||",
  "SA05n889:19": "surapatiśamanāyāpyeṣa mitrāgnirakṣaḥ-",
  "SA05n889:20": "pavanapramathapālaiḥ saṃvṛtaṃ smeravaktram |",
  "SA05n889:21": "khagapatirathagātraṃ brahmavandyaṃ ramomā-",
  "SA05n889:22": "vihitacaraṇabhaktaṃ mañjughoṣaṃ namāmi || 5 ||",
  "SA05n889:23": "yadasikaṭhinadhārācchedamārgābhivāhā (haḥ)",
  "SA05n889:24": "sakalasaliladhārāpātitāṅgāṅgamantraḥ |",
  "SA05n889:25": "bahuparicayasthalyāṃ bhūminadyāpi reje",
  "SA05n889:26": "bahutaramahimānaṃ mañjughoṣaṃ namāmi || 6 ||",
  "SA05n889:27": "bhavadabhinavanutyā toṣitā guhyadevī",
  "SA05n889:28": "nikhilanigamasārā suprakāśā'tireje |",
  "SA05n889:29": "bhavajalanidhipāraṃ dānakalpadrumāgraṃ",
  "SA05n889:30": "galitabahumahograṃ mañjughoṣaṃ namāmi || 7 ||",
  "SA05n889:31": "vibhajati janaloko dharmadhātuṃ maheśaṃ",
  "SA05n889:32": "daśaśatadalapadme saṃsthitaṃ jyotiraiśam |",
  "SA05n889:33": "tadapi tava praśastaṃ devamāhātmyamīśaṃ",
  "SA05n889:34": "vibhajati bhujageśaṃ mañjughoṣaṃ namāmi || 8 ||",
  "SA05n889:35": "paṭhati yadidamiṣṭaṃ mālinīpadyabandhaṃ",
  "SA05n889:36": "sa bhavati kavirājo vādisiṃhāsanasthaḥ |",
  "SA05n889:37": "sakalavisabhāsu projjvaladvāksudhāraḥ",
  "SA05n889:38": "kalitasakalavidyo bhūṣaṇo bhavyadakṣaḥ || 9 ||",
  "SA05n889:39": "svayambhūpurāṇoddhṛtaṃ mañjugarbhaviracittaṃ",
  "SA05n889:40": "mañjuvajrastotraṃ samāptam |"
}