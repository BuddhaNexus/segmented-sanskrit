{
  "SA05n820:0": "lokeśvaraśatakam",
  "SA05n820:1": "vajradattācāryaviracitam",
  "SA05n820:2": "om namo lokanāthāya |",
  "SA05n820:3": "bhāsvanmāṇikyabhāso mukuṭabhṛti namannākanāthottamāṅge",
  "SA05n820:4": "bhaktiprahve sarojāsanaśirasi hasanmālatīmālikābhaḥ |",
  "SA05n820:5": "maulau mīlanmṛgāṅgāmakṛśakapiśatāṃ śāmbhave śātayantyo",
  "SA05n820:6": "loke lokeśapādāmalanakhaśaśabhṛtkāntayaḥ santu śāntyai || 1 ||",
  "SA05n820:7": "nirdhūtā dhūrjaṭīndorna khalu paṭu jaṭāpiṅgamāsaṅgaśāraiḥ",
  "SA05n820:8": "sārairārānmayūkhairna ca harimukuṭāmandamāṇikyabhābhiḥ |",
  "SA05n820:9": "kālimno nāpi līnā vibudhagaṇalalatkuntalālīnalīnā-",
  "SA05n820:10": "llokeśvaryo'nivāryāścaraṇanakharucaḥ santu vo dhvāntaśāntyai || 2 ||",
  "SA05n820:11": "paryāptodārakoṣasphuṭatarakamalābhogasaṃpattihetu -",
  "SA05n820:12": "rdūrībhūtapriyāṇāmapahṛtaviṣamātaṅkaśaṅkā janānām |",
  "SA05n820:13": "na kṣiptālaṅghanairapyupaśamitatamodurgrahodbhūtabhīti-",
  "SA05n820:14": "rlokeśāṅghryorapūrvā nakhaśiśirarūcāṃ candrikā vaḥ punātu || 3 ||",
  "SA05n820:15": "niḥśeṣaṃ kleśarāśīndhanadahanamahāpāvakoccaiḥ śikhā vo",
  "SA05n820:16": "līlālokāstrilokyāmapahṛtagahanābaddhamohāndhakārāḥ |",
  "SA05n820:17": "vahniskandheṣvabandhyā narakabhuvi sudhāvārivistāradhārāḥ",
  "SA05n820:18": "saṃsārīṃ saṃharantāṃ nakhanivaharucaḥ padmabhṛtpādajātāḥ || 4 ||",
  "SA05n820:19": "svacchandacchedivāñcchāvitaraṇacaturācintyacintāmaṇīnām",
  "SA05n820:20": "udgāḍhātaṅkaśaṅkodgamaśamanamanohāriṇī hārabhāsām |",
  "SA05n820:21": "spaṣṭāvirbhūtanānāguṇanivahadhṛtādarśabimbacchavīnāṃ",
  "SA05n820:22": "chāyā vaḥ pātu lokeśvaracaraṇabhuvāmunmayūukhā nakhānām || 5 ||",
  "SA05n820:23": "nāthasyodañcaduccāmitarucirucibhī rocamānorucūḍā-",
  "SA05n820:24": "rociṣṇoruccakāsaccaraṇanakharucāṃ saṃcayo'sau ciraṃ vaḥ |",
  "SA05n820:25": "atyuccīyāt tadarcācaturasuraśiraścāruratnoccayoccai -",
  "SA05n820:26": "rnānāniścārirociścayaracitaśacīrucyacāpopacāraḥ || 6 ||",
  "SA05n820:27": "śrīmadbhogonnatīnāmabhimataviṣayaprāptidānādahīnāṃ",
  "SA05n820:28": "sevābhājāṃ samantādavicalitarucaḥ prītimutpādayantaḥ |",
  "SA05n820:29": "vaimalyātulyabimbopahasitaśaśinaḥ śātitadhvāntadoṣā -",
  "SA05n820:30": "stoṣaṃ māṇikyadīpā iva dadatu nakhāḥ padmapāṇeḥ padorvaḥ || 7 ||",
  "SA05n820:31": "rājadrājīvapāṇernakhanivaharucāṃ pādapadmodbhavānām",
  "SA05n820:32": "udbhedo bhedako'sau bhavatu bhavabhiyāṃ nirbharāṇāṃ bharaṃ vaḥ |",
  "SA05n820:33": "jātastaddehabhūmecaviratakaruṇāvāridattopakāro",
  "SA05n820:34": "yo'nalpaḥ kalpavṛkṣāṅkuranikara ivopāttavijñānabījaḥ || 8 ||",
  "SA05n820:35": "lokeśasyāṅghripadmaprabhavanakharuco dattamārārisenā-",
  "SA05n820:36": "saṃtrāsāḥ śoṇatūṇonmukhanihitaphalāmoghabāṇāṃśuśobhāḥ |",
  "SA05n820:37": "śaśvatsaṃsāraghorāvaṭapatitajanottāraṇāsajjarajju-",
  "SA05n820:38": "prārohā rohayantāṃ duradhigamamahāsaṃpaduccaiḥ padaṃ vaḥ || 9 ||",
  "SA05n820:39": "bhāsvantaḥ kleśakarmaśramavivaśajagattāpavicchedaśūrāṃ",
  "SA05n820:40": "dūrāddoṣopalabdheḥ paṭubudhaguravastārakā ye janasya |",
  "SA05n820:41": "sāndrānandaṃ dadānāḥ sakalaśaśadharaśrībhṛto niṣkalaṅkā-",
  "SA05n820:42": "ste vaścintāmacintyāḥ kamalakaranakhāḥ pādajātā jayantu || 10 ||",
  "SA05n820:43": "bhāsvatkhaṇḍendukhaṇḍairapacitiracanā kiṃ kṛtā śambhuneyaṃ",
  "SA05n820:44": "nyastā ratnāvalī vā kimu niratiśayotkaṇṭhayā bodhilakṣmyā |",
  "SA05n820:45": "devairdivyādbhutānāmasamasumanasāṃ lambitā mālikā nu",
  "SA05n820:46": "prīyātpaṅktirnakhānāmiti janitamatirlokanāthāṅghrijā vaḥ || 11 ||",
  "SA05n820:47": "kvāyaṃ rājīvajanmā namadamaraśirastuṅgamāṇikyaśayyā -",
  "SA05n820:48": "śaśvatsuptāṅghrireṇuḥ kva ca dharaṇitalālambamaulipraṇāmaḥ |",
  "SA05n820:49": "itthaṃ saṃjātahāsā iva rucinicayairdanturā ye durantā",
  "SA05n820:50": "dhvāntacchedāya lokeśvaracaraṇabhavāste nakhā vo bhavantu || 12 ||",
  "SA05n820:51": "śūrāstāpāpahāre śiśiratarasudhāśīkarāsārakārāḥ",
  "SA05n820:52": "prākārāḥ krūradūraprasaraduruśarāsāramārāpakāre |",
  "SA05n820:53": "saṃsārākārakārāgṛhabṛhadudarodāraduḥkhaprakāraṃ",
  "SA05n820:54": "ghoraṃ vaḥ saṃharantāṃ caraṇanakharucaḥ śrīkarāḥ padmapāṇeḥ || 13 ||",
  "SA05n820:55": "bhāso lokeśapādaprabhavanakhabhuvāṃ dūradurvāramāra-",
  "SA05n820:56": "vyāmuktavyāpibāṇāvaraṇajavanikābhrāntimutpādayantyaḥ |",
  "SA05n820:57": "saṃkleśānīkanāśasphuṭapiśunamahāketusaṃghātakalpā -",
  "SA05n820:58": "strailokyāśakyaśaktitribhavajayabṛhadvaijayantyo jayanti || 14 ||",
  "SA05n820:59": "samyaksaṃbodhicetaḥ śaśina iva samudbhāsibhāsāṃ samūho",
  "SA05n820:60": "nirdagdhakleśabhūtipracaya iva bṛhanmuktimārgānilāstaḥ |",
  "SA05n820:61": "vinyastādabhraśubhropalaphalaka iva krūramārairabhedyaḥ",
  "SA05n820:62": "pāyādutprekṣito vo nakharucinikaraḥ pādajaḥ padmapāṇeḥ || 15 ||",
  "SA05n820:63": "vailakṣyeṇekṣaṇīyāḥ kṣaṇamasamatamaḥkṣepadakṣāḥ kṣapāṇāṃ",
  "SA05n820:64": "nāthenākṣuṇṇapakṣakṣatinijavipadā niṣkalaṅkākṣayā ye |",
  "SA05n820:65": "akṣīṇārātipakṣakṣapaṇapaṭujagadrakṣaṇākṣuṇṇadīkṣā-",
  "SA05n820:66": "rakṣā rakṣantu lokeśvaracaraṇanakhāste'kṣaṇakṣepato vaḥ || 16 ||",
  "SA05n820:67": "satsaṃpatsādhanasya pravarakamalabhṛtpādadurgāśrayasya",
  "SA05n820:68": "trailokyaśrījigīṣornakharucinivahasyātulo'bhyuccayo vaḥ |",
  "SA05n820:69": "mitreṇāptodayena sphuṭamanuvalatālambhitātyantavṛddheḥ",
  "SA05n820:70": "pāyāt sāpāyasarvavyasanaripumahāyānasaṃsiddhihetuḥ || 17 ||",
  "SA05n820:71": "avrīḍaścūḍayā'sau vahati paśupatiḥ kāmamardhendubhūṣāṃ",
  "SA05n820:72": "śaureḥ śobhābhilāṣaḥ kathamapi kṛtinaḥ kaustubhenāstameti |",
  "SA05n820:73": "dṛṣṭe'smin so'pi mogho maghavamaṇimahāmaulirityāptacittai-",
  "SA05n820:74": "ryatkāntyācinti lokaiścaraṇanakhagaṇaḥ padmapāṇeḥ sa jīyāt || 18 ||",
  "SA05n820:75": "ārāduccairudañcatkṛtavitatiradhastārako nārakāṇām |",
  "SA05n820:76": "udyannīhārarociḥ śucirarivadanaśyāmatāpātahetuḥ |",
  "SA05n820:77": "durvijñeyānubhāvo nikhilajagadatispaṣṭadṛṣṭiprakāśaḥ",
  "SA05n820:78": "pādāmbhojāvalambī nakhakiraṇacayaḥ pātu vo buddhamauleḥ || 19 ||",
  "SA05n820:79": "dṛṣṭo hṛṣṭāmareśārcanacaturavadhūmuktakarpūrapāṃśu-",
  "SA05n820:80": "prodbhāso bhaktibhārapravaṇaharajaṭābhūtivibhrāntabhūmiḥ |",
  "SA05n820:81": "pūjāvikṣiptalakṣmīkarakamalagalatkeśarāgrāṇureṇu-",
  "SA05n820:82": "cchāyaḥ pāyādapāyānnakhakaravisaraḥ padmahastāṅghrijo vaḥ || 20 ||",
  "SA05n820:83": "merau niṣpītapītadyutiramarakarisphārasindūradhūlī-",
  "SA05n820:84": "śoṇaśrīśāntaśūro haritaharihayāhīnahārityahārī |",
  "SA05n820:85": "mīlannīlāvabhāso nakharucivisaraḥ padmabhṛtpādapadmād",
  "SA05n820:86": "udyaddugdhābdhivṛddhitrasadamaravadhūvīkṣito rakṣatād vaḥ || 21 ||",
  "SA05n820:87": "śobhā saṃbhāvyate'smin himakaradhavalairnāmaraiścāmarairvā",
  "SA05n820:88": "bhaktyārambheṇa rambhe parikirasi mudhā kiṃ nu karpūradhūlīḥ |",
  "SA05n820:89": "kiṃ te puṣpaiḥ prakīrṇaiḥ śaciśucibhiriti svāminā svarbhuvāṃ vaḥ",
  "SA05n820:90": "pūjāyāmabjapāṇernakharucinivahaḥ pādajo varṇito'vyāt || 22 ||",
  "SA05n820:91": "kailāsodbhāsivindhye kavalitabalijitkāyakālimni kālī-",
  "SA05n820:92": "līlālāvaṇyalepe vihataharitimaśvetapūṣāśvaraśmau |",
  "SA05n820:93": "lokeśāṅghrernakhānāmurukaranikare kiṃ mayetīva śīrṇaṃ",
  "SA05n820:94": "śītāṃśoḥ pātu pūjācaturasurajanākīrṇakuṇḍacchalād vaḥ || 23 ||",
  "SA05n820:95": "pādāḥ pādodbhavānāmativitatibhṛtāṃ buddhamaulernakhānāṃ",
  "SA05n820:96": "pāre saṃsārayādaḥpatigamanamahāsetubandhāyamānāḥ |",
  "SA05n820:97": "udyaddurvāraduḥkhānalaśamanamahāpuṇyasaṃbhāravāri-",
  "SA05n820:98": "svacchandācchacchaṭābhā jhaṭiti vighaṭanaṃ kurvatāṃ vo bhavasya || 24 ||",
  "SA05n820:99": "kālīkāntendukānteḥ paribhavavidhṛtirbhūrikailāsabhūbhṛd-",
  "SA05n820:100": "bhāsāṃ hāsāvagītirhatirapi himavadgauratāgauravasya |",
  "SA05n820:101": "kṣīrākūpārapūraplutirapi kakubhāṃ nākanāgīyadanta-",
  "SA05n820:102": "cchedacchāyākṣatirvo nakharucivitatiḥ pātu lokeśvarāṅghryoḥ || 25 ||",
  "SA05n820:103": "yatpūjāpārijāte valitamalikulaṃ mīlati śrīsaroje",
  "SA05n820:104": "sevāsaktaḥ svayaṃbhūrmukulitanalinād duḥsthito yatra tasthau |",
  "SA05n820:105": "yenāśyāmā dinaśrīḥ śamitasuravadhūklāntidoṣā niśā'bhūt",
  "SA05n820:106": "pādo'sau padmapāṇernakhavidhuvilasaccandrikaḥ pātu yuṣmān || 26 ||",
  "SA05n820:107": "kānto vibhrāntakāntāmarayuvatijanairarcito dṛṣṭipātaiḥ",
  "SA05n820:108": "śubhraṃ vibhrāṇmṛṇālīnikaramiva nakhodyotanodbhūtaśobhaḥ |",
  "SA05n820:109": "kāmīvānalpakāmaprasaraparavaśānekacaryāpravṛttaḥ",
  "SA05n820:110": "pādo vaḥ padmapāṇeravatu surapurīcandanāmandacarcaḥ || 27 ||",
  "SA05n820:111": "ānandāmandabhārollasadamarakaraiḥ pārijāte vikīrṇe",
  "SA05n820:112": "kālīyaḥ kālakūṭotkaṭapaṭala iva prollasadbhṛṅgasārthe |",
  "SA05n820:113": "śreyo vaḥ śrīrvidhattāmalamamalanakhadyotadugdhābdhimadhyād",
  "SA05n820:114": "udyantī padmapāṇeścaraṇasarasijasyācyutaprītihetoḥ || 28 ||",
  "SA05n820:115": "vāñchāvicchittikhedocchaladanalaśikhāchedakacchocchryo vaḥ",
  "SA05n820:116": "padmacchāyānukāracchalitamadhukarācchādanācchāditaśrīḥ |",
  "SA05n820:117": "svacchandānacchamūrchacchucinakhakiraṇocchūnagucchairatuccho",
  "SA05n820:118": "deyāt pādo'bjakānticchuritakararūco'cchinnamicchāmakṛcchrāt || 29 ||",
  "SA05n820:119": "udbhūtodbhāsicakradyutiramalanakhairadvitīyoditaśrī-",
  "SA05n820:120": "rbibhrāṇo lakṣaṇānāṃ gaṇamatulaguṇāgaṇyapuṇyopanītam |",
  "SA05n820:121": "niḥśeṣadvīpadīpaprabhavadatimahadvaibhavo buddhamauleḥ",
  "SA05n820:122": "pāyāt pādo namasyadbhuvanapatiśiraścakravartī ciraṃ vaḥ || 30 ||",
  "SA05n820:123": "kīrṇairākāśagaṅgākanakakamalinīkuḍmalaiḥ ko guṇo'smin",
  "SA05n820:124": "kiṃ gītairgītameva sphuṭasarasirūhabhrāntibhṛṅgāvalībhiḥ |",
  "SA05n820:125": "śakyaṃ māṇikyadāmnāmiha rūciranakhe śobhituṃ netyavādīd",
  "SA05n820:126": "yatpūjāyāṃ sureśaḥ sa jayati caraṇo vārijavyagrapāṇeḥ || 31 ||",
  "SA05n820:127": "yo nāthasyaiva nāsīdapi khalu sugateḥ kāraṇaṃ janmabhājāṃ",
  "SA05n820:128": "yasmin padmābhilāṣī na madhupanivahaḥ pakṣapātī jano'pi |",
  "SA05n820:129": "yenorvī nātigurvī natimatiguruṇā lambhitāpi trilokī",
  "SA05n820:130": "lokaṃ pādaḥ sa pāyāt sugataśaśadharadyotavidyotamauleḥ || 32 ||",
  "SA05n820:131": "yo'līnāṃ pānadānādakṛta dhṛtamudaṃ saṃhatiṃ sevakānāṃ",
  "SA05n820:132": "yasminnālaṃ vihīnaṃ na khalu haritatātyantaśobhopadhānam |",
  "SA05n820:133": "deyādvo laukanāthiściramamaraśiraḥśāyireṇūtkaro'sau",
  "SA05n820:134": "śreyaḥ śrīvāsabhūmirnakhakiraṇahasatkeśaraḥ pādapadmaḥ || 33 ||",
  "SA05n820:135": "dehadrohāvahogrāhitahativihito dehināṃ hetirīhā-",
  "SA05n820:136": "siddherāhūtirāho hṛtiratimahato bṛṃhatāṃhomahimnaḥ |",
  "SA05n820:137": "kleśavyūhāhitehāhutivihitabṛhaddrohiduḥkhāgnidāha-",
  "SA05n820:138": "vyāpohā vāhinī stādvahuvihitahitā padmahastāṅaghribhāvaḥ || 34 ||",
  "SA05n820:139": "buddhālaṃkāramauleravikalakamalabhrāntibhṛdbhṛṅgamālā",
  "SA05n820:140": "vācālā nirvicārā racayatu caraṇotsarpiṇī saṃpadaṃ vaḥ |",
  "SA05n820:141": "sattvatrāṇaikakāryā sthiratarasucirāvasthitisthāpanārthaṃ",
  "SA05n820:142": "vinyastā śṛṅkhaleva pracurakaruṇayā sthūlakālāyasasya || 35 ||",
  "SA05n820:143": "rājīvāntaḥpalāśāvaliruciramṛdurvajrasāro visāro",
  "SA05n820:144": "krudhyadvairipramuktapraharaṇanivahasyodbhavadbhaṅgabhūmiḥ |",
  "SA05n820:145": "rakto rāgopaśānteratiśayabalavatkāraṇaṃ tāraṇāyāṃ",
  "SA05n820:146": "saṃsārāmbhonidhervaḥ sugataśaśibhṛto'pāstakhedo'stu pādaḥ || 36 ||",
  "SA05n820:147": "jambhārerjṛmbhitāmbhoruharuciracaye cāpalenāpyalīnā",
  "SA05n820:148": "nālaṃ lolāpi lolā harakaravihitānandakundotkare'pi |",
  "SA05n820:149": "bhrājiṣṇau viṣṇukīrṇe parimalini puraḥ pārijāte'pyajāta-",
  "SA05n820:150": "prītiryatrālimālā ramati jinabhṛtaḥ so'ṅaghripadmo'vatād vaḥ || 37 ||",
  "SA05n820:151": "bhūyādudbhūtabhūridyutinivahalasatkeśarodbhāsuro vo",
  "SA05n820:152": "raktacchāyānuviddhonmukhanakhadaśanodbhāsikāntyā karālaḥ |",
  "SA05n820:153": "trasyaddurvṛttavairidvipataralacalallocanālokanīyo",
  "SA05n820:154": "lokeśasyāṅaghrisiṃhaḥ kamalabhavajaṭāraṇyaśāyī śivāya || 38 ||",
  "SA05n820:155": "acchīyānapyanaccho laghurapi na laghurlaṅghane diṅmukhānāṃ",
  "SA05n820:156": "trailokyānandano'pi prabalaparapuravyāpisantāpahetuḥ |",
  "SA05n820:157": "lāvaṇyālepalipto'pyatiśayamadhuro vaidhurādherniroddhā",
  "SA05n820:158": "pādoddyotaḥ kriyād vo jinarūcivikasanmālikāśekharasya || 39 ||",
  "SA05n820:159": "rājīvai rājarājo harirapi haritairhāribhiḥ pārijātaiḥ",
  "SA05n820:160": "kundaiḥ sānandamindurbahuvasuvisarairvāsavo bhāsamānaiḥ |",
  "SA05n820:161": "prītaḥ pāśī palāśairiti vibudhajanaḥ prājyapūjāviśeṣaṃ",
  "SA05n820:162": "yatrānuprāsakāvyāyitamatata sa vaḥ pātu pādo'bjapāṇeḥ || 40 ||",
  "SA05n820:163": "bhānurbhāsāṃ vikāśe nutimakṛta nakheṣūnmukho nākanāthaḥ",
  "SA05n820:164": "śarvaḥ śākhāgraparvaṇyavicalanayano'pyantakaḥ prāntakāntau |",
  "SA05n820:165": "yasyāsāmānyaśobhāviṣayamatiśayaṃ draṣṭumāsīdanīśo",
  "SA05n820:166": "niḥśeṣaṃ divyalokaḥ sa jayati caraṇo buddhabimbāṅkamauleḥ || 41 ||",
  "SA05n820:167": "samyaksaṃbuddhabhānusphuṭavikaṭajaṭāpuñjakuñjorumūrteḥ",
  "SA05n820:168": "saṃsārāmbhodhimajjadgurutarajanatāmedinīstambhanasya |",
  "SA05n820:169": "bhūyād bhadrāya pādaḥ kamaladharagireḥ sevitaḥ siddhasārthaiḥ",
  "SA05n820:170": "paryantodvāntakāntisravadaruṇamahādhātumannirjharo vaḥ || 42 ||",
  "SA05n820:171": "saṃsārādhvaprabandhaśramasakalajagatklāntisaṃśāntihetu-",
  "SA05n820:172": "cchāyasyecchāphalasya sphuṭanakhakusumodbhāsiśākhābhṛto vaḥ |",
  "SA05n820:173": "lokeśāṅaghridrumasya praṇatasurajanairmaulimālālavāle",
  "SA05n820:174": "mūle saṃpādito'vyānmaṇikalaśabhṛtaḥ svacchadhāmāmbusekaḥ || 43 ||",
  "SA05n820:175": "saṃmūlakleśajālaprabalaripubalonmūlanasthūlalābhā-",
  "SA05n820:176": "llabdhollāso vilāsī balavijayilasanmaulilīlālayo vaḥ |",
  "SA05n820:177": "pāda pāyādatulyāmalakamalabhṛto'līkaphullābjalobha-",
  "SA05n820:178": "vyālolānalpalāpollasadalipaṭalālluptasaṃgītilolaḥ || 44 ||",
  "SA05n820:179": "trailokyaiśvaryalakṣmīcapalakarivadhūsaṃyamālānadaṇḍaḥ",
  "SA05n820:180": "kaṣṭakleśāhidaṣṭaskhaladakhilajagatpālane dakṣatarkṣaḥ |",
  "SA05n820:181": "durvārāntaḥpraveśākṛśanarakapure dvāragāḍhārgalo vo",
  "SA05n820:182": "bhūtyai lokeśapādo bhavajaladhisamullaṅghanaikaplavo'stu || 45 ||",
  "SA05n820:183": "iti pādukādeśanā",
  "SA05n820:184": "vṛtto nṛttaprakāraḥ sapadi vighaṭitā vādyavidyānavadyā",
  "SA05n820:185": "no gītaṃ nāvagītaṃ kṛtarasaracanairnaiva bhāvairabhāvi |",
  "SA05n820:186": "ityantaḥ smeraśākre sadasi na śakitā yatra pūjāpsarobhiḥ",
  "SA05n820:187": "kartuṃ bhāvāturābhiḥ sa jayati janitātṛptirūpaḥ sarojī || 46 ||",
  "SA05n820:188": "uddāmasthāmavāmakramaviṣamamilanmāramānapramāthī",
  "SA05n820:189": "madhyādunmāthimohodgamamamitamahomaulirāyāmabhīmam |",
  "SA05n820:190": "śrīmanniḥsīmabhūmāsamamahimamahākāmadhāmātibhūmau",
  "SA05n820:191": "sattvapremaprakāmaprathitamṛdumanā nirvirāmāśrayo vaḥ || 47 ||",
  "SA05n820:192": "sāraprākāraghorāvaraṇanivaraṇo bandhanakrūradūra-",
  "SA05n820:193": "sphārāvārātiraudre narakanagarikākāradhāriṇyarīṇām |",
  "SA05n820:194": "kārāgārodare yaḥ smaraṇaśaraṇatāṃ kāraṇākātarāṇāṃ",
  "SA05n820:195": "yātaḥ satkārakārī gurutarakaruṇaḥ pātu lokeśvaro vaḥ || 48 ||",
  "SA05n820:196": "kalpāntollāsihelācaladanilacalollolakallolamālā-",
  "SA05n820:197": "vācāle nakrajālākulakalilajale vāridhāvullalantaḥ |",
  "SA05n820:198": "ālambe yasya nīlotpalavimalamahākuṭṭimālīnalīlām",
  "SA05n820:199": "ālīyante ladhīyaḥ sa jagati kamalī pālanāyāla mastu || 49 ||",
  "SA05n820:200": "dhūmaughobandhabandhīkṛtavidhuravidhubradhnanirbandhadhāmā",
  "SA05n820:201": "nīrodhodbhedabādhābudhavibudhavadhūdhīratoddhāradhuryaḥ |",
  "SA05n820:202": "yannāmādhītidhārādharavidhṛtikṛtāmeti dhūmadhvajo'pi",
  "SA05n820:203": "pradhvaṃsaṃ sādhu dheyānniravadhi sa vidhiṃ buddhadhāro dhṛtervaḥ || 50 ||",
  "SA05n820:204": "krodhādutkṣiptakālāyudhakaranikaraḥ krūrasūtkārakārī",
  "SA05n820:205": "dānaklidyatkapolākulamadhupakulākāṇḍakālorukāyaḥ |",
  "SA05n820:206": "krānto yannāmakaṣṭāṅkuśahatikṛpaṇaḥ kātareṇa karīndro",
  "SA05n820:207": "lokeśaḥ sa prakāmopakaraṇakaruṇaḥ kāmakṛtyaṃ kriyād vaḥ || 51 ||",
  "SA05n820:208": "sūtkāriśvāsapoṣākṛśaviṣavamathuploṣiroṣāśrayāśā",
  "SA05n820:209": "daṃśāśādarśitāsyā bhṛśatataśiraso nāśane dandaśūkāḥ |",
  "SA05n820:210": "yannāmāśīviṣeśadviṣi viśati viṣāṃ dhīṣu naśyantyanīśā",
  "SA05n820:211": "lokeśaḥ so'stu viśvaśrayadativiṣamakleśarāśeḥ śamāya || 52 ||",
  "SA05n820:212": "gandhodgārorugarvagrahaṇagajagaṇagrāsagītogravegaḥ",
  "SA05n820:213": "sāvegodagragāmī galagahanaguhāgāḍhagambhīragarjaḥ |",
  "SA05n820:214": "yogairyugyopayogānagami mṛgapatiryadgaṇodgītiyogād",
  "SA05n820:215": "duryogaṃ vaḥ sa yogī sthagayatu sugatāsaṅgituṅgottamāṅgaḥ || 53 ||",
  "SA05n820:216": "bhūyo'pāyānumeyaḥ kṣatadayahṛdayo bhīvidhāyī vihāya",
  "SA05n820:217": "stheyān śailopameyaḥ śrayadaligavayacchāyakāyo nikāyaḥ |",
  "SA05n820:218": "sadyogopāyamāyāmaya iva vilayaṃ yātavān yātavīryo",
  "SA05n820:219": "yatpādadhyāyineyaḥ sa sugatanilayo jāyatāṃ vo jayāya || 54 ||",
  "SA05n820:220": "saṃtrāsāvāsabhūmiṃ kalivisarasaradyādasaṃ bhāsvarāsi-",
  "SA05n820:221": "prāsaprollāsibhaṅgaṃ prasṛtasitalasatketusatphenahāsyām |",
  "SA05n820:222": "yatsevāsādhunāvā dviṣadasamasamitsārasenāsravantīṃ",
  "SA05n820:223": "sotsāhāḥ saṃcarante sukhamasukhamasau saṃhriyād vaḥ sarojī || 55 ||",
  "SA05n820:224": "bhūbhṛtsaṃbhārabhedaprabhukaravibhavo bhūribhūbhogabhīmaṃ",
  "SA05n820:225": "bibhrāṇaścitrabhānuṃ bhavadatibharabhīsaṃbhramodbhrāntibhūtaḥ |",
  "SA05n820:226": "dambholirbhītibhājāmabhavadabhipatannāśunābhāvabhūmi-",
  "SA05n820:227": "rbhakteryasya prabhāvāt sa bhavatu bhavabhidvo bhṛtāmbhojaśobhī || 56 ||",
  "SA05n820:228": "ābādhādhūtadhairyaścyutividhisavidho durvidhakrodhavedho",
  "SA05n820:229": "bādhāduḥsādharodhoddhurabahuvividhavyādhisaṃbādhadehaḥ |",
  "SA05n820:230": "yasya dhyānāvadhānādadhikadhṛtisudhādhānasaṃdhāritāsu-",
  "SA05n820:231": "rdhīmānnādhervidheyo dhiyamavatu sa vo bodhaye buddhamūrdhā || 57 ||",
  "SA05n820:232": "ārādāhūtiyātṛkṣitipatiratulārātiśātipratāpa-",
  "SA05n820:233": "prodgītirdaṇḍanītiprathitapitṛpatikhyātijiddaṇḍanītiḥ |",
  "SA05n820:234": "yatpādādhītiśakteḥ kupitamatiratiprītimāyāti bhūte-",
  "SA05n820:235": "rvyāghātodbhūtiheturjinavṛṣavasatiḥ saṃhriyād vaḥ sa bhītim || 58 ||",
  "SA05n820:236": "brahmā jihmāyito'bhūdagururapi guruḥ khaṇḍitākhaṇḍalokti-",
  "SA05n820:237": "rviṣṇustūṣṇīmadhṛṣṭo vacanaviracane vītagarvo'pi śarvaḥ |",
  "SA05n820:238": "tuṣṭāstuṣṭūṣavo'pi sphuṭamiti vibudhā no budhā yadguṇoktau",
  "SA05n820:239": "sa śrīmānabjapāṇirjayati jinamanogocarāntarguṇaughaḥ || 59 ||",
  "SA05n820:240": "dhātrā citraṃ caritraṃ suraripuripuṇā satrinetreṇa netre",
  "SA05n820:241": "gotrāmitreṇa gotraṃ gatirapi guruṇā rātripeṇāpi gātram |",
  "SA05n820:242": "maitraṃ mitreṇa putrīyitasakalajagatprema putreṇa cātre-",
  "SA05n820:243": "ryatredaṃ stotrapātraṃ stutamavatu sa vo'mātrasatto'bjapāṇiḥ || 60 ||",
  "SA05n820:244": "yasmin brahmā bahutvaṃ bahubahumatavānānanānāṃ nijānāṃ",
  "SA05n820:245": "skando'pyānandagarbhaṃ nutiṣu natikṛtau nāganātho'pi mūrdhnaḥ |",
  "SA05n820:246": "śakraḥ ślāghyāmanaiṣīnnayanadaśaśatāṃ yaṃ vilokya trilokī-",
  "SA05n820:247": "lokeśo nirvirāmānatinayanaratistotrapātraṃ sa pāyāt || 61 ||",
  "SA05n820:248": "khedī khe dīptaraśmiḥ kila viphalamasāvadhvanīnodhvanīno",
  "SA05n820:249": "lokālokārthamāste nanu kamalabhṛto dīptatāpaḥ pratāpaḥ |",
  "SA05n820:250": "dhīrairdhīrairakāri stutiriti vihitollāsabhāsāṃ sabhāsāṃ",
  "SA05n820:251": "yasyāryasyāstu tasmājjagati kṛtaripukṣiprasādaḥ prasādaḥ || 62 ||",
  "SA05n820:252": "nyastā yasminnamasye natirapi nitarāmunnatiḥ puṇyadhāmnāṃ",
  "SA05n820:253": "niḥsāmānyaikamānye'pacitirupacitirbhūyasī bhaktibhājām |",
  "SA05n820:254": "dhyānasthānasya yasya smṛtirapi sahasā vismṛtirbhūtabhīteḥ",
  "SA05n820:255": "so'nantācintyaloke vihitahitapatho lokanātho'vatādvaḥ || 63 ||",
  "SA05n820:256": "dambho dambholiraindraḥ kvacidakṛta surārātiśāto'tiśāto",
  "SA05n820:257": "hārī hārīraṇāsau balavati viphalābhīṣu rājīṣu rājī |",
  "SA05n820:258": "cakre cakreṇa nārthaṃ haririti janatā nūnamāheti heti-",
  "SA05n820:259": "vyāsavyāsaktimuktān jayati ripujanollāsarojī sarojī || 64 ||",
  "SA05n820:260": "lokātotaṃ dadhānaḥ sukhamapi jagatāṃ tīvraduḥkhena duḥkhī",
  "SA05n820:261": "nityaṃ nityānurakto'pyaśaraṇakṛpaṇaprāṇabhṛdyogayuktaḥ |",
  "SA05n820:262": "trailokyasyaikanātho'pyasamarucijanārādhanābandhuro yaḥ",
  "SA05n820:263": "so'vyāt saṃbuddhamaulirvihitavisadṛśācintyacaryaściraṃ vaḥ || 65 ||",
  "SA05n820:264": "nirvicchedatrilokīnihitanirupamasnehayogānuyogā-",
  "SA05n820:265": "nnirvāṇo na prakampyo balavadalaghubhistīrthikonmattavātaiḥ |",
  "SA05n820:266": "jīyāllokeśadīpaḥ sa bhuvanabhavanodbhūtamohāndhakāra-",
  "SA05n820:267": "dhvaṃso'vidhvaṃsadhāmā parahitakaraṇodyogasaṃvṛttavarttiḥ || 66 ||",
  "SA05n820:268": "kvāsau sarvatra maitrī kva viṣamabahalakleśavidveṣidāhaḥ",
  "SA05n820:269": "kva prauḍhā muktiśaktiḥ kva ca dṛḍhakaruṇāpāśaniṣpandabandhaḥ |",
  "SA05n820:270": "kvopekṣāpakṣapātaḥ kvaparahitakṛtivyagratā tadvicitraṃ",
  "SA05n820:271": "citraṃ rājīvapāṇeścaritamatijagajjāyatāṃ jyotijidvaḥ || 67 ||",
  "SA05n820:272": "sarvāgraḥ sarvarūpaprathanapṛthunayaḥ sarvadā sarvanāthaḥ",
  "SA05n820:273": "sarveṣāṃ sarvathā yo vinayavidhimahāsarvagurvarthasiddhyai |",
  "SA05n820:274": "sarvaiḥ sātotagarvairgurumahimaguṇaiḥ kharvayan sarvagarvān",
  "SA05n820:275": "sarvaḥ sarvaprado vaḥ saphalayatu rūcīḥ sarvavinmaulirurvīḥ || 68 ||",
  "SA05n820:276": "īśaḥ svāmī prajānāṃ patiramaragururlokapālo mahendro",
  "SA05n820:277": "bhāsvān dattāridaṇḍaḥ parajayabalijid vittado jaitrapāśaḥ |",
  "SA05n820:278": "ityantarhāsagarbhaṃ bhaṇati parijane'nvarthanāmnāṃ nijānāṃ",
  "SA05n820:279": "pātre yatrānulajjairnatamamaragaṇairabjapāṇiḥ sa jīyāt || 69 ||",
  "SA05n820:280": "varyāryāṇāṃ vareṇyo varaparavidhurotsāraṇā saṃvaro vo",
  "SA05n820:281": "durvāraiḥ sāramārairamitakaradharo durdharo vairivīraiḥ |",
  "SA05n820:282": "vīro vīrārivārī pracurataravarodārasaṃbhāravāri-",
  "SA05n820:283": "sphārāsārorudhārāvisaravitaraṇādhīradhārādharaḥ syāt || 70 ||",
  "SA05n820:284": "nityodyukte'tiśakte prabhavati nalinodbhāsi haste samasto",
  "SA05n820:285": "nāthī saṃsārayādaḥpatipathapathako loka ityākalayya |",
  "SA05n820:286": "yasmin vinyasya bhāraṃ laghuralaghukṛpo viśvadīpaṃ prapede",
  "SA05n820:287": "buddho'vandhyapratijñākṛtadhṛtividhṛtirnirvṛtiṃ so'vatādvaḥ || 71 ||",
  "SA05n820:288": "uccairūḍho garīyān sugata iva jagatkāryasaṃbhārabhāro",
  "SA05n820:289": "nyastā haste praśastā nija iva kamalālaṃkṛtirbhaktibhājām |",
  "SA05n820:290": "nirvāṇaṃ nārakāgnernicaya iva ciraṃ prāpitaḥ sattvasārthaḥ",
  "SA05n820:291": "tīvrakleśaprabandho jana iva śamito yena pāyāt sa yuṣmān || 72 ||",
  "SA05n820:292": "yo nānānantarūpaprakaṭanapaṭimakhyātamāyo'pyamāyaḥ",
  "SA05n820:293": "saṃśāntāśeṣabhīrapyatikaruṇatayā kātarācārakārī |",
  "SA05n820:294": "vītakrodho'pi duṣṭāśayadamanabudhakrodhanityānubandhaḥ",
  "SA05n820:295": "saṃbuddhodbhāsimauliḥ sa jayati mahatāṃ cintanīyo'pyacintyaḥ || 73 ||",
  "SA05n820:296": "dhatte naivottamāṅgaṃ paramapitu vapuryo'mitābhābhirāmaṃ",
  "SA05n820:297": "sannālaṃ nāravindaṃ gurubhayavidhurā vairisenāpi yasya |",
  "SA05n820:298": "yenābaddhā jaṭā no jagadahitahativyāpṛtenāpi kakṣā",
  "SA05n820:299": "dakṣo'sau rakṣatādvaḥ kupitayamamukhālokanāllokanāthaḥ || 74 ||",
  "SA05n820:300": "śeṣāśaṅkī cakampe bhavasalilanidhiḥ kleśasārthaiḥ pralīnaṃ",
  "SA05n820:301": "viśrāntaṃ bodhisattvairmunirapi śuśubhe ślāghyanirvāṇalīlaḥ |",
  "SA05n820:302": "yasminnābaddhakakṣe prasarati parito'śeṣasattvārthakārya-",
  "SA05n820:303": "vyāyāme sa prakāmaṃ śamayatu sugatāvāsamaulirmalaṃ vaḥ || 75 ||",
  "SA05n820:304": "iti bhagavadvarṇanā |",
  "SA05n820:305": "pratyūhavyūhabādhāvighaṭanaviṣame mānasasyāpyabhūmau",
  "SA05n820:306": "karmaṇyekāntaśarmacchidi jagadahitocchittaye padmapāṇiḥ |",
  "SA05n820:307": "nighnaḥ krīto'tha bhīto balavadiva yayā nirvikāro niyuktaḥ",
  "SA05n820:308": "sā nāthasyātigurvī prabhuravatu kṛpā niṣkṛpādantakādvaḥ || 76 ||",
  "SA05n820:309": "bhūmirnaivābhibhūtermṛdutaramapi yallokadhātūnanantān",
  "SA05n820:310": "atyantāvīryavīryaṃ yugapadapi bhṛśaṃ bhāsayaṃstrāsaśāntyai |",
  "SA05n820:311": "khadyotodyotaleśānukṛtivilisitāśeṣatejasviteja-",
  "SA05n820:312": "stejo lokeśvaraṃ vo haratu hṛdi bhavadbhūrimohāndhakāram || 77 ||",
  "SA05n820:313": "niḥśeṣākāśadhāturjana iva janitāpūritāśaḥ samantāt",
  "SA05n820:314": "prārabdhaḥ sarvabhāsāmiva niratiśayāpāyarāśervilopaḥ |",
  "SA05n820:315": "sārdhaṃ sāndrāndhakāraiḥ śamamagami mahānṛddhimānmānimāro",
  "SA05n820:316": "yenodbhāsaḥ sa bhūyāt sarasirūhabhṛto bhūtaye jāyatāṃ vaḥ || 78 ||",
  "SA05n820:317": "īśo'nyadyotanāśo balabhiditarabhāduḥśamadhvāntarāśeḥ",
  "SA05n820:318": "sattvadrohāśayānāmanupamatapano yo'cyutaścitrabhānuḥ |",
  "SA05n820:319": "nānānantācalādivyavahitaviṣayodbhāsane śaktidhārī",
  "SA05n820:320": "lokeśodbhāsa ekopyamaragaṇanibho bhībhide vaḥ sa bhūyāt || 79 ||",
  "SA05n820:321": "saṃpannāśeṣasattvapracurajalacarecchāsukhaṃ yatra dūraṃ",
  "SA05n820:322": "durlaṅghaye yāntyadhastād giraya iva janānarthadustīrthyasārthāḥ |",
  "SA05n820:323": "durvāro'sau samantāt pṛthubhavabhuvanābhāvasaṃrambhajṛmbhī",
  "SA05n820:324": "bhīdāvāgnipraśāntyai karakamalavibhāmbhonidhirjṛmbhatādvaḥ || 80 ||",
  "SA05n820:325": "nirvāṇo nārakāgniḥ kimiti yadi vineyeṣu dharmāmṛtaugho-",
  "SA05n820:326": "rāśirbaddho jaṭānāṃ yadi gatirahitaḥ kiṃ samaho ripūṇām |",
  "SA05n820:327": "mardhāgāre garīyān yadi vasati jino bandhurā kiṃ trilokī",
  "SA05n820:328": "nāthasyetthaṃ vicitrā vyavahṛtirahitaṃ hantu vo lokabandhoḥ || 81 ||",
  "SA05n820:329": "mūrcchatyekāpi sattvāśayavaśavihitākārabhedābhirāmā",
  "SA05n820:330": "durbhedābhūtakalpā caladacalamahārambhadambholikoṭiḥ |",
  "SA05n820:331": "laukeśī sanmanīṣākumudaśaśirucistīrthikānarthakoṭi-",
  "SA05n820:332": "dhvāntāntardhānabhānurbhavatu bhavabhide deśanā śāsanī vaḥ || 82 ||",
  "SA05n820:333": "durvādonmādanādipracuramadakaṇādākṣapādādivādi-",
  "SA05n820:334": "prodyannānāvivādāspadamadasadasāṃ sāpavādo vivādaḥ |",
  "SA05n820:335": "utsādaḥ sapramādonmadajanavipadāṃ kovidānāṃ prasādo",
  "SA05n820:336": "bhāvānutpādavādo jayati jinavapuḥ pādasaṃpādamauleḥ || 83 ||",
  "SA05n820:337": "trastavyastāriśastraṃ trayi jagati cakāsti stuto yadgabhasti-",
  "SA05n820:338": "stomo vistīrṇatoyaṃ marunilayasadāṃ vistaradhvastatāpaḥ",
  "SA05n820:339": "sa stādastāhitāstro balivalikuliśaḥ padmahastasya hasto -",
  "SA05n820:340": "'nāpāstyaiḥ vaṃ samastatribhuvanavikasatsādhvasāpāstiśastaḥ || 84 ||",
  "SA05n820:341": "saṃvartodvṛttavātavyatikaraviṣayottuṅgaśailadrumālī-",
  "SA05n820:342": "nirmūlonmūlanāya prabhurarikariṇāṃ śātitānalpadarpaḥ |",
  "SA05n820:343": "bhadro lokeśvarasya praṇayimadhukarākarṣadānaughavarṣī",
  "SA05n820:344": "hastastamberamo vo bhavaripunagarībhañjanāyālamastu || 85 ||",
  "SA05n820:345": "satkoṣaṃ sannidhānaṃ ghanarucijinatāvāñchitācchedasiddhau",
  "SA05n820:346": "yadyātaṃ śātakoṭimanukṛtimahitatrāsisatpatrakoṭi |",
  "SA05n820:347": "samyaksaṃbodhilakṣmīkṛtavasatiguṇaślāghayaivāvimuktaṃ",
  "SA05n820:348": "muktestatkāraṇaṃ vaḥ karakamalamalaṃ lokanāthasya bhūyāt || 86 ||",
  "SA05n820:349": "śaśvadvardhiṣṇutṛṣṇāvikalagaladarīvihvalapretarāśe-",
  "SA05n820:350": "rāhlādotpādi satyaṃ kamalamadhivasadbodhilakṣmīkamabjam |",
  "SA05n820:351": "satpadmaṃ bhūtivāñcchāvinihitamanasāṃ sādhusevāpadaṃ yat",
  "SA05n820:352": "tallokeśasya yuṣmān paṭurucipaṭalīrājirājīvamavyāt || 87 ||",
  "SA05n820:353": "nānāduḥkhapratānāṃ tanukiraṇaghanottāpanakleśabhānu-",
  "SA05n820:354": "mlānaṃ dīnānanaṃ yajjanamanujanayatyātapatropamānam |",
  "SA05n820:355": "glāniṃ mā gādahīnaṃ tadakhilabhuvanānyūnabhānūdvitānaṃ",
  "SA05n820:356": "līnaṃ lokeśapāṇau nalinamamalinaṃ mlānimeno nayedvaḥ || 88 ||",
  "SA05n820:357": "sthāmnaḥ sthānaṃ mahimno mahadudayapadaṃ dhāmadhāmnāṃ prathimna-",
  "SA05n820:358": "strāsāvāso ripūṇāṃ durabhibhavabhavādbhībhidodbhūtibhūmiḥ |",
  "SA05n820:359": "kāntaṃ śobhāniśāntaṃ vasatiratijagadvīryavistārirāśe-",
  "SA05n820:360": "rlokeśasyāstu bāhurbahujagadahitocchedanācaṇḍadaṇḍaḥ || 89 ||",
  "SA05n820:361": "yallāvaṇyāmṛtaughaṃ kṣaradapi janitātaṅkamevāhitānāṃ",
  "SA05n820:362": "tṛptiṃ naiva prayacchatyatha punaratanuprīṇanaṃ locanānām |",
  "SA05n820:363": "śobhāgāraṃ viśobhīkṛtasakalajagadbhūṣaṇaṃ bhūṣaṇānāṃ",
  "SA05n820:364": "tadbhuyādekacīraṃ sukhamasukhanude bhāsvadambhojino vaḥ || 90 ||",
  "SA05n820:365": "mlānaṃ rūpābhimānairjagati saphalatāṃ locanaiḥ puṇyabhājāṃ",
  "SA05n820:366": "yātaṃ ghātaṃ tu tṛṣṇātatitaralatarairyatra lāvaṇyasindhau |",
  "SA05n820:367": "manye'saṃkhyaiḥ śaśāṅkaprabhṛtibhiratulaṃ kāntimadbhiḥ kṛtaṃ syād",
  "SA05n820:368": "ekaṃ yadyāsyamasyāpyatijayi jayatāṃ tanmukhaṃ padmapāṇeḥ || 91 ||",
  "SA05n820:369": "yasmin vidveṣabhājāmaviratavilasatkāntitoyaughameghaiḥ",
  "SA05n820:370": "saṃvṛttaṃ dāhadāyi sphuṭataramasakṛddurdinaṃ dehināṃ tu |",
  "SA05n820:371": "dhautā dhvastānubandhā bahulamalamaṣīpaṅkalepaprabandhāḥ",
  "SA05n820:372": "tadbuddhāgāramūrddhāmukhamatisukhadaṃ stādvicitrakriyaṃ vaṃ || 92 ||",
  "SA05n820:373": "iṣṭābjākliṣṭapāṇeḥ sphuṭavikaṭakuṭīkuṭṭimāntopaviṣṭa-",
  "SA05n820:374": "spaṣṭaśliṣṭāmitābhadyutipaṭupaṭalāpāṭalābhāpaṭimnaḥ |",
  "SA05n820:375": "śobhāviṣṭairadṛṣṭoparighanaghaṭanasyotkaṭāṭopabandhaḥ",
  "SA05n820:376": "kūṭasyāvyājjaṭānāṃ kaṭurakaṭasuhṛtsaṃkaṭātkaṅkaṭo vaḥ || 93 ||",
  "SA05n820:377": "atyantāhlādahetoraviratavistṛtasyāmitābhaprabhāmbhaḥ",
  "SA05n820:378": "saṃbhārasyaiva sekān niratiśayamṛjāsundaro labdhavṛddhiḥ |",
  "SA05n820:379": "uddāmāmodidivyādbhutakusumacayairarcitaścintitārtha-",
  "SA05n820:380": "prāpteḥ saṃpattaye stānnalinadharajaṭākalpavallīcayo vaḥ || 94 ||",
  "SA05n820:381": "yāsāṃ baddho vimuktiṃ gamayati niyatāmujjhitānyādhivāso",
  "SA05n820:382": "niḥsāmānyāṃ vibhūṣāṃ janayati vapuṣā yāsu nātho'mitābhaḥ |",
  "SA05n820:383": "sarvāmodacchido yā nirupamavahalāmodaliptākhilāśā",
  "SA05n820:384": "laukeśyo'vaśyalabhyāṃ mṛtimativikaṭāstā jaṭā vo harantu || 95 ||",
  "SA05n820:385": "saṃghāto no jaṭānāmakhilajanamanobandhane pāśarāśi-",
  "SA05n820:386": "rlāvaṇyaṃ nāpi duḥkhānalavikalajagajjīvanīyo'mṛtaughaḥ |",
  "SA05n820:387": "nāmbhojaṃ duṣṭadamyāntakaturagakaśākleśadoṣātimoṣaḥ",
  "SA05n820:388": "pratyāśā poṣasiddhiṃ diśatu jinaśaśiśleṣikeśaḥ śriyo vaḥ || 96 ||",
  "SA05n820:389": "uddāmāpiṅgatejaḥprasaraviracitāśeṣadigdāhamoha-",
  "SA05n820:390": "trastātrāṇātrilokīkavalanarabhasollāsikālāgnikalpam |",
  "SA05n820:391": "daṣṭauṣṭhaṃ duṣṭadṛṣṭijvalitamanuhayagrīvamābaddhalakṣmyā",
  "SA05n820:392": "jātā lokeśvarī vaḥ kṣaṇamatulakṛpākātarā dṛṣṭiravyāt || 97 ||",
  "SA05n820:393": "lopaṃ lokaḥ prayāti sphuṭamakhilamahābhūbhṛtāmadya tūrṇaṃ",
  "SA05n820:394": "cūrṇībhāvo dharāṇāmapi jalanidhayaḥ śoṣamāyāntyanantāḥ |",
  "SA05n820:395": "itthaṃ yasyāntakopabhrukuṭibharabhavadbhaṅgabhīmāllalāṭā-",
  "SA05n820:396": "nniryāntīṃ vīkṣya devīmatulabhujabalaistrastamabjī sa jīyāt || 98 ||",
  "SA05n820:397": "stutyaiḥ stutyā gurūṇāmapi jagati gururvanditā vandanīyai-",
  "SA05n820:398": "rmūrtevānalpakalpārjitasakalajagattrāṇanirvyājaśaktiḥ |",
  "SA05n820:399": "lokasyārticchidā yaiḥ svayamatulakṛpaivābjino nirgatā stāt",
  "SA05n820:400": "tārā saṃsārakārodaraguruvilasatkāraṇāhāriṇī vaḥ || 99 ||",
  "SA05n820:401": "gīrvāṇagrāmagīto gurugaṇanaguṇo gīṣpateragragābhi-",
  "SA05n820:402": "rgrāhyonudgāḍhavargasphuṭagatigahano haṃsagāmyugragābhiḥ |",
  "SA05n820:403": "gambhīrodgāriṇībhirnigaditagarimāgeyapūgālpabhāgaḥ",
  "SA05n820:404": "samyaggamyaḥ samagro'vatu sugatagirāmabjino vo guṇaughaḥ || 100 ||",
  "SA05n820:405": "kavirapi janmani janmani bhaktaścaraṇe'valokiteśvarasya |",
  "SA05n820:406": "prakṛtiśaraṇagottaradhīḥ parahitagurukāryakāryaḥ syām || 101 ||",
  "SA05n820:407": "mahākṣapaṭalikaśrīvajradattakaviviracitaṃ",
  "SA05n820:408": "śrīlokeśvaraśatakaṃ samāptam |"
}