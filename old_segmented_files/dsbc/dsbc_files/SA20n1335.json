{
  "SA20n1335:0": "ārya maitreyapraṇītā",
  "SA20n1335:1": "madhyāntavibhāgakārikā",
  "SA20n1335:2": "lakṣaṇaparicchedaḥ prathamaḥ",
  "SA20n1335:3": "lakṣaṇaṃ hyāvṛtistattvaṃ pratipakṣasya bhāvanā |",
  "SA20n1335:4": "tatrā'vasthā phalaprāptiryānā ''nuttaryameva ca ||1 ||",
  "SA20n1335:5": "abhūtaparikalpo'sti dvayaṃ tatra na vidyate |",
  "SA20n1335:6": "śūnyatā vidyate tvatra tasyāmapi sa vidyate ||2 ||",
  "SA20n1335:7": "na śūnyaṃ nā'pi cā'śūnyaṃ tasmāt sarvaṃ vidhīyate |",
  "SA20n1335:8": "sattvādasattvāt sattvācca madhyamā pratipacca sā ||3 ||",
  "SA20n1335:9": "arthasattvātmavijñaptipratibhāsaṃ prajāyate |",
  "SA20n1335:10": "vijñānaṃ nāsti cāsyārthastadabhāvāttadapyasat ||4 ||",
  "SA20n1335:11": "abhūtaparikalpatvaṃ siddhamasya bhavatyataḥ |",
  "SA20n1335:12": "na tathā sarvathā'bhāvāt tatkṣayānmuktiriṣyate ||5 ||",
  "SA20n1335:13": "kalpitaḥ paratantraśca pariniṣpanna eva ca |",
  "SA20n1335:14": "arthādabhūtakalpācca dvayā'bhāvācca deśitaḥ ||6 ||",
  "SA20n1335:15": "upalabdhiṃ samāśritya nopalabdhiḥ prajāyate |",
  "SA20n1335:16": "nopalabdhiṃ samāśritya nopalabdhiḥ prajāyate ||7 ||",
  "SA20n1335:17": "upalabdhestataḥ siddhā nopalabdhisvabhāvatā |",
  "SA20n1335:18": "tasmācca samatā jñeyā nopalambhopalambhayoḥ ||8 ||",
  "SA20n1335:19": "abhūtaparikalpaśca cittacaittāstridhātukāḥ |",
  "SA20n1335:20": "tatrārthadṛṣṭirvijñānaṃ tadviśeṣe tu caitasāḥ ||9 ||",
  "SA20n1335:21": "evaṃ pratyayavijñānaṃ dvitīyaṃ caupabhogikam |",
  "SA20n1335:22": "upabhogaparicchedaprerakāstatra caitasāḥ ||10 ||",
  "SA20n1335:23": "chādanādropaṇāccaiva nayanātsaṃparigrahāt |",
  "SA20n1335:24": "pūraṇāt triparicchedādupabhogācca karṣaṇāt ||11 ||",
  "SA20n1335:25": "nibandhanādābhimakhyād duḥkhanāt kliśyate jagat |",
  "SA20n1335:26": "tredhā dvedhā ca saṃkleśaḥ saptadhā'bhūtakalpanāt ||12 ||",
  "SA20n1335:27": "lakṣaṇaṃ cā'tha paryāyastadartho bheda eva ca |",
  "SA20n1335:28": "sādhanañceti vijñeyaṃ śūnyatāyāḥ samāsataḥ ||13 ||",
  "SA20n1335:29": "dvayā'bhāvo hyabhāvasya bhāvaḥ śūnyasya lakṣaṇam |",
  "SA20n1335:30": "na bhāvo nā'pi cā'bhāvao na pṛthaktvaikalakṣaṇam ||14 ||",
  "SA20n1335:31": "tathatā bhūtakoṭiścā'nimittaṃ paramārthatā |",
  "SA20n1335:32": "dharmadhātuśca paryāyāḥ śūnyatāyāḥ samāsataḥ ||15 ||",
  "SA20n1335:33": "ananyathā'viparyāsatannirodhāryagocaraiḥ |",
  "SA20n1335:34": "hetutvāccāryadharmaṇāṃ paryāyārtho yathākramam ||16 ||",
  "SA20n1335:35": "saṃkliṣṭā ca viśuddhā ca samalā nirmalā ca sā |",
  "SA20n1335:36": "abdhātukanakākāśaśuddhivacchuddhiriṣyate ||17 ||",
  "SA20n1335:37": "bhoktṛbhojanataddhehapratiṣṭhāvastuśūnyatā |",
  "SA20n1335:38": "tacca yena yathā dṛṣṭaṃ yadarthaṃ tasya śūnyatā ||18 ||",
  "SA20n1335:39": "śubhadvayasya prāptyartha sadā sattvahitāya ca |",
  "SA20n1335:40": "saṃsārā'tyajanārthaṃ ca kuśalasyā'kṣayāya ca ||19 ||",
  "SA20n1335:41": "gotrasya ca viśuddhayarthaṃ lakṣaṇavyañjanāptaye |",
  "SA20n1335:42": "śuddhaye bu[ddha]dharmāṇāṃ bodhisattvaḥ prapadyate ||20 ||",
  "SA20n1335:43": "pudgalasyā'tha dharmāṇāmabhāvaḥ śūnyatā'tra hi |",
  "SA20n1335:44": "tadabhāvasya sadbhāvastasmin sā śūnyatā'parā ||21 ||",
  "SA20n1335:45": "saṃkliṣṭā cedbhavennāsau muktāssyuḥ sarvadehinaḥ |",
  "SA20n1335:46": "viśuddhā cedbhavennā'sau vyāyāmo niṣphalo bhavet ||22 ||",
  "SA20n1335:47": "na kliṣṭā nā'pi va'ākliṣṭā śuddhā'śuddhā na caiva sā |",
  "SA20n1335:48": "prabhāsvaratvāccittasya kleśasyāgantukatvataḥ ||23 ||",
  "SA20n1335:49": " ||iti lakṣaṇaparicchedaḥ prathamaḥ ||"
}