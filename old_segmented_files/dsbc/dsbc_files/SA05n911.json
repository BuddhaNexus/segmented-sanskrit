{
  "SA05n911:0": "vasudhārānāmadhāraṇīstotram",
  "SA05n911:1": "om namo bhagavatyai āryaśrīvasudhārāyai",
  "SA05n911:2": "divyarūpī surūpī ca saumyarūpī balapradā |",
  "SA05n911:3": "vasudharī vasudhāraṇī vasuśrī śrīkarī varā || 1 ||",
  "SA05n911:4": "dharaṇī dhāraṇī dhātā śaraṇyā bhaktavatsalā |",
  "SA05n911:5": "prajñāpāramitā devī prajñā śrībuddhivardhinī || 2 ||",
  "SA05n911:6": "vidyādharī śivā sūkṣmā śāstā sarvatra mātṛkā |",
  "SA05n911:7": "taruṇī tāru(ra)",
  "SA05n911:8": "ṇī devī vidyādāneśvareśvarī || 3 ||",
  "SA05n911:9": "bhūṣitā bhūtamātā ca sarvābharaṇabhūṣitā |",
  "SA05n911:10": "durdāntatrāsanī bhītā ugrā ugraparākramā || 4 ||",
  "SA05n911:11": "dānapāramitā devī varṣaṇī divyarūpiṇī |",
  "SA05n911:12": "nidhānaṃ sarvamāṅgalyā kīrtirlakṣmīryaśaḥśubhā || 5 ||",
  "SA05n911:13": "dahanī mālinī caṇḍī śabarī sarvamātrikā |",
  "SA05n911:14": "kṛtāntaśāsanī raudrī kaumārī viśvarūpiṇī || 6 ||",
  "SA05n911:15": "vīryapāramitā devī jagadānandarocanī |",
  "SA05n911:16": "tāpasī ugrarūpī ca ṛddhisiddhibalapradā || 7 ||",
  "SA05n911:17": "dhanyā puṇyā mahābhāgā ajitā jitavikramā |",
  "SA05n911:18": "jagadekahitā vidyā saṃgrāme tāraṇī śubhā || 8 ||",
  "SA05n911:19": "kṣāntipāramitā devī śīlinī dhyānadhyāyino |",
  "SA05n911:20": "padminī padmadhārī ca padmapriyā padmāsanī || 9 ||",
  "SA05n911:21": "śuddharūpī mahātejā hemavarṇā prabhākarī |",
  "SA05n911:22": "cintāmaṇimahādevī prajñāpustakadhāriṇī || 10 ||",
  "SA05n911:23": "nidhānaṃ kūṭimāruḍhidhanyāgāradhanapriyā |",
  "SA05n911:24": "traidhātukaṃ mahā ādi divyābharaṇabhūṣiṇī || 11 ||",
  "SA05n911:25": "mātarī sarvabuddhānāṃ ratnadhāte(tvī)",
  "SA05n911:26": "śvareśvarī |",
  "SA05n911:27": "śūnyatā bhāvanī devī bhāvābhāvavivarjitā || 12 ||",
  "SA05n911:28": "vainye(ne)ya kiṃ na vinyastā divyakleśanichedanī |",
  "SA05n911:29": "bhī(bhe) dinī sarvamārāṇāṃ saptapātālakṣobhinī (ṇī) || 13 ||",
  "SA05n911:30": "brahmāṇī vedamātā ca guhyā ca guhyavāsinī |",
  "SA05n911:31": "sarasvatī viśālākṣī caturbrahmavihāriṇī || 14 ||",
  "SA05n911:32": "tāthāgatī mahāramyā vajriṇī dharmadhāriṇī |",
  "SA05n911:33": "karmadhāteśvarī vidyā viśvajvālābhamaṇḍalī || 15 ||",
  "SA05n911:34": "bodha(dhi)nī sarvasattvānāṃ bodhyaṅgakṛtaśekharī |",
  "SA05n911:35": "dhyānā dhīrmuktisaṃpannā advayadvayabhāvinī || 16 ||",
  "SA05n911:36": "sarvārthasādhanī bhadrā strīrūpāmitavikramā |",
  "SA05n911:37": "darśinī buddhamārgāṇāṃ naṣṭamārgapradarśinī || 17 ||",
  "SA05n911:38": "vāgīśvarī mahāśāntirgoptrī dhātrī dhanapradā |",
  "SA05n911:39": "strīrūpadhāriṇī siddhā yoginī yogajeśvarī || 18 ||",
  "SA05n911:40": "manoharī mahākrāntiḥ saubhāgyapriyadarśinī |",
  "SA05n911:41": "sārthavāhakṛpādṛṣṭiḥ sarvatāthāgatātmakī || 19 ||",
  "SA05n911:42": "namaste'stu mahādevī sarvasattvārthadāyinī |",
  "SA05n911:43": "namaste divyarūpī ca vasudhārā namo'stu te || 20 ||",
  "SA05n911:44": "aṣṭottaraśataṃ nāma trikālaṃ yaṃ paṭhet pumān |",
  "SA05n911:45": "prāpnoti niyataṃ siddhimīpsitārthamanorathān || 21 ||",
  "SA05n911:46": "yadajñānakṛtaṃ pāpam ānantaryasudāruṇam |",
  "SA05n911:47": "tatsarvaṃ kṣapayatyāśu smaraṇāt sa(rva)bhadrakam || 22 ||",
  "SA05n911:48": "athavā śīlasaṃpannaḥ saptajātismaro bhavet |",
  "SA05n911:49": "priyaścādeyavākyena rūpavān priyadarśanaḥ || 23 ||",
  "SA05n911:50": "viprakṣatriyakuleṣu ādeyamupajāyate |",
  "SA05n911:51": "ante bhūmīśvaraṃ prāptaḥ paścāt prāpta(ḥ)",
  "SA05n911:52": "sukhāvatīm || 24 ||",
  "SA05n911:53": "śrīvasudhārānāmadhāraṇīstotraṃ",
  "SA05n911:54": "samyaksaṃbuddhabhāṣitaṃ samāptam |"
}