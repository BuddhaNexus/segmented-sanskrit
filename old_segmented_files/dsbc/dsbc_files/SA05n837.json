{
  "SA05n837:0": "pīṭhastavaḥ 1",
  "SA05n837:2": "brahmāṇī tattvarūpā vividhaghanaravā ghoracaṇḍī ca raudrī",
  "SA05n837:3": "kaumārī kīrtikāmā pibati madhumadaṃ vaiṣṇavī gāyamānā |",
  "SA05n837:4": "vārāhī vādayantī paṭutarapaṭahān nṛtyamānā tathaindrī",
  "SA05n837:5": "cāmuṇḍā cāpi lakṣmīrharagaṇasahitā mātaro vaḥ punantu || 1 || 2",
  "SA05n837:7": "gaṇapatiṃ ca herambaṃ vighnarājaṃ vināyakam |",
  "SA05n837:8": "devaputraṃ mahāvīryaṃ mahābalaparākramam || 2 ||",
  "SA05n837:9": "mahodaraṃ mahākāyamekadantaṃ gajānanam |",
  "SA05n837:10": "śvetavarṇaṃ mahādīptiṃ trinetraṃ gaṇanāyakam || 3 ||",
  "SA05n837:11": "akṣamālā (ca)",
  "SA05n837:12": "varadaṃ dakṣiṇakarasaṃsthitam |",
  "SA05n837:13": "paraśurmuṇḍapātraṃ ca vāmahastavidhāraṇam || 4 ||",
  "SA05n837:14": "nānāpuṣparataṃ devaṃ nānāgandhāvalepanam |",
  "SA05n837:15": "nāgayajñopavītāṅgaṃ nānāvighnavināśanam || 5 ||",
  "SA05n837:16": "devāsuramanuṣyaiśca siddhagandharvakinnaraiḥ |",
  "SA05n837:17": "pūjitaṃ vighnahantāraṃ mūṣakāṭaṃ namāmyaham || 6 || 3",
  "SA05n837:19": "brahmāṇī brahmasāvitrī brahmatattvavivedinī |",
  "SA05n837:20": "caturbhujā caturvaktrā caturdevaparāyaṇā || 7 ||",
  "SA05n837:21": "caturdeśe ciraṃ vyāpī caturyugopakāriṇī |",
  "SA05n837:22": "haṃsayuktavimānasthā saumyarūpā pitāmahī || 8 ||",
  "SA05n837:23": "pustakaṃ puṣpamālāṃ ca varadābhayadhāriṇī |",
  "SA05n837:24": "pītapuṣparatā devī pītāṅgā pītasannibhā || 9 ||",
  "SA05n837:25": "pītopahārasaṃyuktā pītagandhānulepinī |",
  "SA05n837:26": "udumbaradrumasthā ca prayāgakṣetravāsinī || 10 ||",
  "SA05n837:27": "pūrvapīṭhe sthitā nityaṃ brahmaśaktirnamo'stu te | 4",
  "SA05n837:29": "māheśvarī mahādevī mohamāyāniraṃja(kṛṃta) nī || 11 ||",
  "SA05n837:30": "mahāvṛṣasamārūḍhā mahāhuṃkāranādinī |",
  "SA05n837:31": "hemamukta(ktā)",
  "SA05n837:32": "nibhadehā kapālaśaśiśekharā || 12 ||",
  "SA05n837:33": "trilocanā triśulī ca akṣasūtrakamaṇḍalū |",
  "SA05n837:34": "nānālaṅkārasarvāṅgī dakṣiṇena varapradā || 13 ||",
  "SA05n837:35": "sṛṣṭisthitivināśānāṃ sarvathāpīśvareśvarī |",
  "SA05n837:36": "śvetapuṣparatā devī śvetagandhānulepinī || 14 ||",
  "SA05n837:37": "śvetavastraparīdhānā mudrābharaṇabhūṣitā |",
  "SA05n837:38": "vārāṇasyāṃ mahākṣetre tāra(la)",
  "SA05n837:39": "vṛkṣanivāsinī || 15 ||",
  "SA05n837:40": "uttare saṃsthitā pīṭhe māheśvarī namo'stu te | 5",
  "SA05n837:42": "bālabhāve mahāraudrī kaumārī raktalocanī || 16 ||",
  "SA05n837:43": "raktavarṇadharā devī sindūrārūṇavigrahā |",
  "SA05n837:44": "caturbhujā śaktisūtrasiddhapātradharā śubhā || 17 ||",
  "SA05n837:45": "kaumārī paramā śaktirmayūravaravāhinī |",
  "SA05n837:46": "raktavastraparīdhānā raktamāṃsāvasāyinī || 18 ||",
  "SA05n837:47": "kolāpuramahākṣetre vaṭavṛkṣanivāsinī |",
  "SA05n837:48": "agnipīṭhasthitā nityaṃ kaumārī te namo namaḥ || 19 || 6",
  "SA05n837:50": "vaiṣṇavī viṣṇumāyā ca daityadurdāntanāśinī |",
  "SA05n837:51": "haritaśyāmavarṇāṅgī garuḍopari saṃsthitā || 20 ||",
  "SA05n837:52": "śaṅkhacakragadāhastā caturbāhuvibhūṣitā |",
  "SA05n837:53": "raktajvālā mahākrīḍā nānābharaṇabhūṣitā || 21 ||",
  "SA05n837:54": "haritapuṣpagandhā ca haritavastradhāriṇī |",
  "SA05n837:55": "aṭṭahāse mahākṣetre kadambavṛkṣavāsinī || 22 |",
  "SA05n837:56": "svarge martye ca pātāle sthitirūpeṇa saṃsthitā |",
  "SA05n837:57": "tiṣṭhantī pīṭhanairṝtye nārāyaṇī namo'stu te || 23 || 7",
  "SA05n837:59": "vārāhī ghoraraktāṅgī raktakeśī mahodarī |",
  "SA05n837:60": "daṃṣṭrāvādinī gambhīrā raktanetrā trilocanī || 24 ||",
  "SA05n837:61": "kapālamālikā mālā vicitrapuṣpaśobhitā |",
  "SA05n837:62": "mahāśūkasamārūḍhā jvalitāgnisamaprabhā || 25 ||",
  "SA05n837:63": "aṅkuśakartikādaṇḍahāḍābharaṇabhūṣitā |",
  "SA05n837:64": "trinetrā jvalitadehā ca duṣṭadarpavināśinī || 26 ||",
  "SA05n837:65": "jayantīkṣetrasaṃsthānā nimbavṛkṣasamāśritā |",
  "SA05n837:66": "yāmyapīṭhe sthitā nityaṃ kolarūpī namo'stu te || 27 || 8",
  "SA05n837:68": "śakreśvarī sahasrākṣo kuṅkumāruṇavigrahā |",
  "SA05n837:69": "sureśvarī devadevī sarvālaṅkārabhūṣitā || 28 ||",
  "SA05n837:70": "caturbhujā viśālākṣī chatraghaṇṭāvidhāraṇī |",
  "SA05n837:71": "mahāvajradharā devī sthitā cairāvate gaje || 29 ||",
  "SA05n837:72": "nānāpuṣparatā devī nānāratnavibhūṣiṇī |",
  "SA05n837:73": "nānāgandhaviliptāṅgī nānāvastravirājinī || 30 ||",
  "SA05n837:74": "cīra(na)kṣetre mahākṣetre karañjavṛkṣasaṃsthitā |",
  "SA05n837:75": "nāgapīṭhasthitā nityaṃ śakreśvari namo'stu te || 31 || 9",
  "SA05n837:77": "cāmuṇḍā caṇḍikā caṇḍī pracaṇḍasurasundarī |",
  "SA05n837:78": "caṇḍāṭṭahāsā caṇḍākṣī pracaṇḍacaṇḍanāśinī || 32 ||",
  "SA05n837:79": "daṃṣṭrākarālā raktāṅgī kapilakeśī mahodarī |",
  "SA05n837:80": "kṛṣā(pā)ṇī bhīṣaṇī raudrī jihvālalanabhīṣaṇā || 33 ||",
  "SA05n837:81": "mahāpretāsanārūḍhā bhujāṣṭakasuśobhinī |",
  "SA05n837:82": "asicarmayutā hastaiḥ ḍamarukhaṭvāṅgadhāriṇī || 34 ||",
  "SA05n837:83": "kartikapālahastā ca varadābhayabhūṣitā |",
  "SA05n837:84": "hāḍālaṅkārasarvāṅgī aripratyasudā priyā || 35 ||",
  "SA05n837:85": "sahasrasūryasaṃkāśā ropakūpaṃ prati prati |",
  "SA05n837:86": "jvālāmālākulā dehe (sūrya)",
  "SA05n837:87": "koṭisamaprabhā || 36 ||",
  "SA05n837:88": "bhūtavetālaḍākinyaḥ parivārāśca rākṣasāḥ |",
  "SA05n837:89": "ekamakṣamahākṣetre aśvatthavṛkṣavāsinī |",
  "SA05n837:90": "pīṭhe marutasaṃsthāne cāmuṇḍāyai namo'stu te || 37 || 10",
  "SA05n837:92": "mahālakṣmīrmahādevī bhogāḍhyā guṇasundarī |",
  "SA05n837:93": "vaidūryapādukārūḍhā siṃhāsanasthitā sudhīḥ || 38 ||",
  "SA05n837:94": "caturbhujā viśālākṣī khaḍgakheṭakadhāriṇo |",
  "SA05n837:95": "pātrabindudharā devī hārdhayuk(hārārdhayuga)kuṇḍalī || 39 ||",
  "SA05n837:96": "ratnakhacitasarvāṅgī cūḍāmaṇivibhūṣitā |",
  "SA05n837:97": "vicitrapuṣparatnā ca vastragandhānulepanā || 40 ||",
  "SA05n837:98": "trailokyavyāpinī devī sarvasthā sacarācarā |",
  "SA05n837:99": "siddhagandharvanamitā vidyādharasurārcitā || 41 ||",
  "SA05n837:100": "devīkoṭamahākṣetre plakṣasaṃsthā varā balā |",
  "SA05n837:101": "īśānapīṭhasaṃsthānā mahālakṣmīḥ namo'stu te || 42 || 11",
  "SA05n837:103": "jananī sarbabuddhānāṃ sarvasantoṣakāriṇī |",
  "SA05n837:104": "tvameva sarvarūpā ca tvameva viśvarūpiṇī || 43 ||",
  "SA05n837:105": "bhairavaṃ bhīṣaṇaṃ raudraṃ ghoragambhorarūpiṇam |",
  "SA05n837:106": "nirañjananibhaṃ dehaṃ sarvakāmamahotsavam || 44 ||",
  "SA05n837:107": "nānābhujasamākīrṇā nānāvastradharā śubhā |",
  "SA05n837:108": "babhrurardhaśirorūhā dāvāgnisamatejasā || 45 ||",
  "SA05n837:109": "trilocanā mahātejā agnisūryasamaprabhā |",
  "SA05n837:110": "triśūlaṃ muṇḍaṃ khaṭvāṅgaṃ ḍamaruṃ tarjanīṃ dhvajam || 46 || 12",
  "SA05n837:112": "prajñāṃ tatpustakaṃ caiva khaḍgacarmadharā śubhā |",
  "SA05n837:113": "pāśāṅkuśadharaṃ devaṃ vajrasūcīmahādharam || 47 ||",
  "SA05n837:114": "kapālakartikaṃ cakraṃ gajacarmāvaguṇṭhitam |",
  "SA05n837:115": "daṃṣṭrākarālavadanaṃ vyāghracarmakaṭīvṛtam || 48 ||",
  "SA05n837:116": "sālaṅkāreṇa sarvāṅgaṃ narāsthipuṣpaśobhitam |",
  "SA05n837:117": "śīrṣamālādharā devī kāpālikoṭaraṃ śubham || 49 ||",
  "SA05n837:118": "cūḍāmaṇiṃ mahātejaṃ kapālaṃ candrabhūṣitam |",
  "SA05n837:119": "mahāpretāsanaṃ nityaṃ nīyamānā sadā priyā || 50 || 13",
  "SA05n837:121": "sahasrasūryasaṃkāśā chatrabindusamanvitā |",
  "SA05n837:122": "catuṣpīṭhasthitā nityam aṣṭakṣetranivāsinī || 51 ||",
  "SA05n837:123": "aṣṭamūrtisthitā devī aṣṭakayoginīpriyā |",
  "SA05n837:124": "bhadrapīṭhe sthitā nityaṃ bhadrakālīsamāvṛtā || 52 || 14",
  "SA05n837:126": "bhadrakāraṇakartā tvaṃ vīrabhadra namo'stu te |",
  "SA05n837:127": "asitāṅgo ruruścaiva caṇḍo'tha krodhabhairavaḥ || 53 ||",
  "SA05n837:128": "unmattabhairavaścaiva kapālī bhīṣaṇastathā |",
  "SA05n837:129": "saṃhārabhairavaścāṣṭabhairavāya namo'stu te || 54 || 15",
  "SA05n837:131": "svasthānā svādhikārā ca svasvarūpā svavīrakā |",
  "SA05n837:132": "svasvacchavṛkṣatānaṃ na divyākṣaṃ caiva bhūmigāḥ || 55 ||",
  "SA05n837:133": "daśadikkṣetrapālaṃ ca kṣetrāṇi ca caturdaśa |",
  "SA05n837:134": "pañcāśakṣetrapālaṃ ca kṣetrapāla namo'stu te || 56 || 16",
  "SA05n837:136": "nāthanātha mahānātha ādinātha mahāmate |",
  "SA05n837:137": "śrīnātha siddhanātha mīnanātha namo'stu te || 57 ||",
  "SA05n837:138": "kṣetranātha pīṭhanātha dvīpanātha mahātmane |",
  "SA05n837:139": "pretanātha bhūtanātha baḍhanātha namo'stu te || 58 ||",
  "SA05n837:140": "trināthaṃ navanāthaṃ ca ṣoḍaśanāthamuttamam |",
  "SA05n837:141": "saptaviṃśatipañcāśacaturaśīti namo'stu te || 59 ||",
  "SA05n837:142": "sarveṣāṃ nāthasiddhānāṃ mataṃ cakṣustvamavyayam |",
  "SA05n837:143": "potasadma tathā vīrastataḥ sarva namo'stu te || 60 || 17",
  "SA05n837:145": "ekavāraṃ dvivāraṃ ca trivāraṃ yaḥ paṭhennaraḥ |",
  "SA05n837:146": "śatamāvartayed yena prāpnoti phalamuttamam || 61 ||",
  "SA05n837:147": "nāśayecchokacintādi nāśayed vighnamaṇḍalam |",
  "SA05n837:148": "nāśeyed rogakalahān nāśayed duḥkhaduṣkaram || 62 ||",
  "SA05n837:149": "nāśayed bhayadāridrayaṃ nāśayed ripujaṃ bhayam |",
  "SA05n837:150": "nāśayedagnicaurādi nāśayed rājakopajam || 63 ||",
  "SA05n837:151": "nāśayed vigrahaṃ ghoraṃ parakṛtābhicārakam |",
  "SA05n837:152": "nāśayed dveṣarāgādi nāśayet sarvapātakam || 64 ||",
  "SA05n837:153": "āyurārogyamaiśvaryadhanadhānyapravardhanam |",
  "SA05n837:154": "dharmārthakāmamokṣāṇāṃ yaśaḥsaubhāgyavardhanam || 65 ||",
  "SA05n837:155": "ṛddhiṃ siddhiṃ śriyaṃ lakṣmīṃ vidyāṃ jñānaṃ sutādi ca |",
  "SA05n837:156": "buddhiṃ prajāṃ sumitrāṇi vardhayecca dine dine || 66 ||",
  "SA05n837:157": "nākāle maraṇaṃ caiva utpātaṃ nāśayet sadā |",
  "SA05n837:158": "sarve rogāḥ praśāmyanti dīrghamāyuravāpyate || 67 ||",
  "SA05n837:159": "śrīpīṭhastavastotraṃ samāptam |"
}