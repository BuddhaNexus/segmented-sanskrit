{
  "SA05n907:0": "vajravilāsinīstotram",
  "SA05n907:1": "mahāpaṇḍitavibhūticandrapādakṛtam",
  "SA05n907:2": "om namaḥ śrīvajrayoginyai",
  "SA05n907:3": "devāsuranaravanditacaraṇe bhavamagnoddharaṇārpitacaraṇe |",
  "SA05n907:4": "tvayi rodimi nutisavyapadeśaḥ kiṃ paśyasi punaraniśamaśeṣam || 1 ||",
  "SA05n907:5": "mātardevi kirasi kaṇamuṣṭiṃ janayad yajñasutaḥ kimatuṣṭim |",
  "SA05n907:6": "vajravārāhi narāhisurāṇāṃ tvaṃ śaraṇaṃ tava nāmaparāṇām || 2 ||",
  "SA05n907:7": "harikara(ri)",
  "SA05n907:8": "śikhiphaṇitaskarabhītistvatparacitte naiva sameti |",
  "SA05n907:9": "mātardevi nibhālaya mahyaṃ kiṃ sahase mama duḥkhamasahyam || 3 ||",
  "SA05n907:10": "tarjanavajrakaroṭa(vi)",
  "SA05n907:11": "dhāriṇi duṣṭaṃ tuṣṭaya māranivāriṇi |",
  "SA05n907:12": "abhinavakomalakaracaraṇāṅgi trivalisamucchadasaptavibhaṅgi || 4 ||",
  "SA05n907:13": "grāhyagrāhakabhāvaviśuddhe pādakarāṅgulijālapinaddhe |",
  "SA05n907:14": "āyatapārśvāṅgulikaracaraṇe dhvastavyādhijarājanimaraṇe || 5 ||",
  "SA05n907:15": "udgatareṇurūrdhvagataromā vajrayogavivṛtāmṛtadhāmā |",
  "SA05n907:16": "bālamarīcivilokanarakte jagatāṃ duḥkhanirākṛtiśakte || 6 ||",
  "SA05n907:17": "subhru tanūdari madhyakṛśāṅgi karṇāntāyatalocanabhṛṅgi(bhaṅgi) |",
  "SA05n907:18": "ḍākinye (va hi)",
  "SA05n907:19": "viśvamaśeṣaṃ loko vetti na te bahuveṣam || 7 ||",
  "SA05n907:20": "yatnamṛte jagadarthasamṛddhiścintitamātrajanepsitasiddhiḥ |",
  "SA05n907:21": "mūḍho vetti na duṣkṛtakarma prāvṛṣīva na śilāṅkurajanma || 8 ||",
  "SA05n907:22": "andha ivārkaśaśāṅkādarśī pāpajano hyanubhāvasparśī |",
  "SA05n907:23": "cintāmaṇimajñā na vidanti prajñe tvasya phalāni phalanti || 9 ||",
  "SA05n907:24": "cakrāṅkitakarapādamanojñe jñānānalabhasmīkṛtasaṅge |",
  "SA05n907:25": "kāyavacanamanasā kṛtaduritaṃ rāgadveṣamohaparikari(li)tam || 10 ||",
  "SA05n907:26": "taddiśāmi tava devi samakṣaṃ yāvadyāmi na durgatipakṣam |",
  "SA05n907:27": "manaḥ paramārthaṃ tanustava vedī sa bhavati naiva duritaparikhedī || 11 ||",
  "SA05n907:28": "lakṣaṇanikhilālakṣitagātre sakalānuvyañjanagatapātre |",
  "SA05n907:29": "navayauvanamadamantharapiṇḍe calakuṇḍalayugamaṇḍitagaṇḍe || 12 ||",
  "SA05n907:30": "tuṅganitambaghanastanabhāre galakālambiniraṃśuka(ṅkuśa)hāre |",
  "SA05n907:31": "saṃvaramadhupavicumbimukhābje tadbhujayugaparirabdhahṛdabje || 13 ||",
  "SA05n907:32": "nirbharasuratasukhāvikalākṣi muktaśiroruhavasananirapekṣe |",
  "SA05n907:33": "herukarāhudaṣṭamukhacandre sasmitaracita hū hū kṛtamantre || 14 ||",
  "SA05n907:34": "patimaulisthitavidhumamṛṣantī kimu candrārkau vapu(ra)muṣantī |",
  "SA05n907:35": "mātaste jagadanupamarūpaṃ tadvicāraya paramārtha sa(sva)rūpam || 15 ||",
  "SA05n907:36": "kleśadāhiṃ(ha)",
  "SA05n907:37": "śamanāmṛtavacanaṃ tadvineyajanaśuddhiviracanam |",
  "SA05n907:38": "padminīti nalināmalagandhaḥ karuṇāparajagadarthanibandhaḥ || 16 ||",
  "SA05n907:39": "divyasudhādhara(gata)",
  "SA05n907:40": "rasapānaṃ tajjagadadvayabodhinidhānam |",
  "SA05n907:41": "pratyaṅgasparśo'pyanimittaṃ sahajāmbudhiviplāvitacittam || 17 ||",
  "SA05n907:42": "kiṃ tvaṃ mātaḥ kariṣyasi tābhiḥ ṣaṭtriṃśacchatakoṭyabalābhiḥ |",
  "SA05n907:43": "yadyanyāṃ manute bahutātastadvihāya jhagiti dehi nu mātaḥ || 18 ||",
  "SA05n907:44": "nāḍīcakranirodha --------------------------------- mahāsukhasaṃvara |",
  "SA05n907:45": "devi tvaṃ śatabhāvavikalpā śūnyasamādhirapākṛtatulyā (talpā) || 19 ||",
  "SA05n907:46": "śūnyakṛpe sahajādvayamuktaṃ vajrāya(jrayā)",
  "SA05n907:47": "namidamavadhi(vi)bhaktam |",
  "SA05n907:48": "bhāvābhāvasamastākāśavyāpī bhavati sa tattvavikāśaḥ || 20 ||",
  "SA05n907:49": "udbhavanti tata eva vimokṣā bodhipakṣanikhilapratipakṣāḥ |",
  "SA05n907:50": "buddhānāmāveṇikadharmaḥ sattvarāśiparipācitakarmaḥ || 21 ||",
  "SA05n907:51": "yadvā yānatrayaniryāṇaṃ dhūmādikamapi cāparimāṇam |",
  "SA05n907:52": "tvadbhaktyā yadalambhi śubhaṃ me bhavatu mano bhavatīparamaṃ me |",
  "SA05n907:53": "sakalakaluṣarahitaṃ guṇasindhustvatparamaṃ hyata eva na bandhuḥ || 22 ||",
  "SA05n907:54": "guhyasamayasādhanatantre śrīvajravilāsinīstotraṃ samāptam |",
  "SA05n907:55": "kṛtiriyaṃ mahāpaṇḍitavibhūticandrapādānām |"
}