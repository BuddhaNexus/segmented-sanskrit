{
  "SA05n881:0": "suprabhātastotram",
  "SA05n881:1": "śrīharṣadevabhūpativiracitam",
  "SA05n881:2": "stutamapi surasaṅghaiḥ siddhagandharvayakṣai-",
  "SA05n881:3": "rdivi bhuvi suvicitraiḥ stotravāgbhiryatīśaiḥ |",
  "SA05n881:4": "ahamapi kṛtaśaktirnaumi saṃbuddhamāryaṃ",
  "SA05n881:5": "nabhasi garuḍayāte kiṃ na yānti dvirephāḥ || 1 ||",
  "SA05n881:6": "kṣapitaduritapakṣaḥ kṣīṇaniḥśeṣadoṣo",
  "SA05n881:7": "dravitakanakavarṇaḥ phullapadmāyatākṣaḥ |",
  "SA05n881:8": "surucirapariveṣaḥ suprabhāmaṇḍalaśrīḥ",
  "SA05n881:9": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 2 ||",
  "SA05n881:10": "madanabalavijetuḥ kāpathocchedakartu-",
  "SA05n881:11": "stribhuvanahitakartuḥ strīlatājālahartuḥ |",
  "SA05n881:12": "samasukhaphaladāturbhetturajñānaśailaṃ",
  "SA05n881:13": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 3 ||",
  "SA05n881:14": "asurasuranarāṇāṃ yo'grajanmāgradevaḥ",
  "SA05n881:15": "sakalabhuvanadhātau lokasṛṣṭyaikaśabdaḥ |",
  "SA05n881:16": "svapiti manujadhātā abjayoniḥ svayambhū-",
  "SA05n881:17": "rdaśabala tava nityaṃ suprabhātaṃ prabhātam || 4 ||",
  "SA05n881:18": "udayagiritaṭastho vidrumacchedatāmra-",
  "SA05n881:19": "stimiranikarahantā cakṣurekaṃ prajānām |",
  "SA05n881:20": "ravirapi madalolaḥ sarvathā so'pi supto",
  "SA05n881:21": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 5 ||",
  "SA05n881:22": "dviradadaśanapāṇḍuḥ śītaraśmiḥ śaśāṅka-",
  "SA05n881:23": "stilaka iva rajanyāḥ sarvacūḍāmaṇiryaḥ |",
  "SA05n881:24": "avigatamadarāgaḥ sarvathā so'pi supto",
  "SA05n881:25": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 6 ||",
  "SA05n881:26": "prabalabhujacatuṣkaḥ ṣoḍaśārdhārdhavaktro",
  "SA05n881:27": "japaniyamavidhijñaḥ sāmavedapravaktā |",
  "SA05n881:28": "amalakamalayoniḥ so'pi brahmā prasupto",
  "SA05n881:29": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 7 ||",
  "SA05n881:30": "himagiriśikharasthaḥ sarpayajñopavītī",
  "SA05n881:31": "tripuradahanadakṣo vyāghracarmottarīyaḥ |",
  "SA05n881:32": "saha girivaraputryā so'pi suptastriśūlī",
  "SA05n881:33": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 8 ||",
  "SA05n881:34": "jvalitakuliśapāṇirdurjayo dānavānāṃ",
  "SA05n881:35": "surapatirapi śacyā vibhrame mūḍhacetāḥ |",
  "SA05n881:36": "aniśiniśiprasuptaḥ kāmapaṅke nimagno",
  "SA05n881:37": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 9 ||",
  "SA05n881:38": "kuvalayadalanīlaḥ puṇḍarīkāyatākṣaḥ",
  "SA05n881:39": "suraripubalahantā viśvakṛdviśvarūpī |",
  "SA05n881:40": "harirapi cirasupto garbhavāsairamukto",
  "SA05n881:41": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 10 ||",
  "SA05n881:42": "kapilajaṭakalāpo raktatāmrāruṇākṣaḥ",
  "SA05n881:43": "paśupatiratikāle saṅgabhaṅgaikadakṣaḥ |",
  "SA05n881:44": "smaraśaradalitāṅgaḥ so'pi supto hutāśo",
  "SA05n881:45": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 11 ||",
  "SA05n881:46": "humaśaśikumudābho madyapānāruṇākṣo",
  "SA05n881:47": "dṛḍhakaṭhinabhujāṅgo lāṅgalī śaktihastaḥ |",
  "SA05n881:48": "bala iha śayito'sau revatīkaṇṭhalagno",
  "SA05n881:49": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 12 ||",
  "SA05n881:50": "gajamukhadaśanaikaḥ sarvato vighnahantā",
  "SA05n881:51": "vigalitamadavāriḥ ṣaṭpadākīrṇagaṇḍaḥ |",
  "SA05n881:52": "gaṇapatirapi supto vāruṇīpānamatto",
  "SA05n881:53": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 13 ||",
  "SA05n881:54": "atasikusumanīlo yasya śaktiḥ karāgre",
  "SA05n881:55": "navakamalavapuṣmān ṣaṇmukhaḥ krauñcahantā |",
  "SA05n881:56": "trinayanatanayo'sau nityasuptaḥ kumāro",
  "SA05n881:57": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 14 ||",
  "SA05n881:58": "aśanavasanahīnā bhāvyamānā virūpā",
  "SA05n881:59": "alamakhilavighātaiḥ pretavaddagdhadehāḥ |",
  "SA05n881:60": "ubhayagativihīnāste'pi nagnāḥ prasuptā",
  "SA05n881:61": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 15 ||",
  "SA05n881:62": "ṛṣaya iha mahānto vatsabhṛgvaṅgirādyāḥ",
  "SA05n881:63": "kratupulahavasiṣṭhā vyāsavālmīkigargāḥ |",
  "SA05n881:64": "parayuvativilāsairmohitāste'pi suptāḥ",
  "SA05n881:65": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 16 ||",
  "SA05n881:66": "yamavaruṇakuberā yakṣadaityoragendrāḥ",
  "SA05n881:67": "divi bhuvi gagane vā lokapālāstathānye |",
  "SA05n881:68": "yuvatimadakaṭākṣairvīṃkṣitāste'pi suptā",
  "SA05n881:69": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 17 ||",
  "SA05n881:70": "bhavajalanidhimagnā mohajālāvṛtāṅgā",
  "SA05n881:71": "manukapilakaṇādā bhrāmitā mūḍhacittāḥ |",
  "SA05n881:72": "samasukhaparihīnā vāliśāste'pi suptā",
  "SA05n881:73": "daśabala tava nityaṃ suprabhātaṃ prabhātam || 18 ||",
  "SA05n881:74": "ajñānanidrarajanītamasi prasuptā",
  "SA05n881:75": "tṛṣṇāviśālaśayane viṣayopadhāne |",
  "SA05n881:76": "kāle śubhāśubhaphalaṃ parikīryamāṇe",
  "SA05n881:77": "jāgarti yaḥ satatameva namo'stu tasmai || 19 ||",
  "SA05n881:78": "tīrtheṣu gokulaśatāni pibanti toyaṃ",
  "SA05n881:79": "tṛptiṃ vrajanti na ca tatkṣayamabhyupaiti |",
  "SA05n881:80": "tadvanmune kaviśatairapi saṃstutasya",
  "SA05n881:81": "na kṣīyate guṇanidhirguṇasāgarasya || 20 ||",
  "SA05n881:82": "suprabhātaṃ tavaikasya jñānonmīlitacakṣuṣaḥ |",
  "SA05n881:83": "ajñānatimirāndhānāṃ nityamastamito raviḥ || 21 ||",
  "SA05n881:84": "punaḥ prabhātaṃ punarutthito raviḥ punaḥ śaśāṅkaḥ punareva śarvarī |",
  "SA05n881:85": "mṛtyurjarā janma tathaiva he mune gatāgataṃ mūḍhajano na buddhyate || 22 ||",
  "SA05n881:86": "suprabhātaṃ sunakṣatraṃ śriyā pratyabhinanditam |",
  "SA05n881:87": "buddhaṃ dharmaṃ ca saṅghaṃ va praṇamāmi dine dine || 23 ||",
  "SA05n881:88": "stutvā lokaguruṃ mahāmunivaraṃ saddharmapuṇyodgamaṃ",
  "SA05n881:89": "nirdvandvaṃ hatarāgadveṣatimiraṃ śāntendriyaṃ nispṛham |",
  "SA05n881:90": "yatpuṇyaṃ samupārjitaṃ khalu mayā tenaiva loko'khilaṃ",
  "SA05n881:91": "pratyūṣastutiharṣito daśabale śraddhāṃ parāṃ vindatām || 24 ||",
  "SA05n881:92": "śrīharṣadevabhūpativiracitaṃ daśabalasya suprabhātastotraṃ samāptam |"
}