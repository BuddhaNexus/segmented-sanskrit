{
  "SA05n828:0": "saptākṣarastotram",
  "SA05n828:1": "om namo lokanāthāya matsyendrāya viṣṇave",
  "SA05n828:2": "namāmi jinanāthāya nityānityavidhāyine |",
  "SA05n828:3": "nirambaravilāsāya nirvikalpāya te namaḥ || 1 ||",
  "SA05n828:4": "mokṣadātā kṛtamokṣo mokṣasaukhyapradāyakaḥ |",
  "SA05n828:5": "merumaṇḍalamadhyasthamokṣadāya namo'stu te || 2 ||",
  "SA05n828:6": "lolakandarpapāśāya lokeśāya mahātmane |",
  "SA05n828:7": "lokarakṣāya devāya lokanāthāya te namaḥ || 3 ||",
  "SA05n828:8": "kanakācalamadhyasthakinnarairupapadyate |",
  "SA05n828:9": "kīrtihetoḥ samādhyeṣā kāntidaṃ tvāmupāsmahe || 4 ||",
  "SA05n828:10": "nāthamantrasamujjvālanāgābharaṇadhāriṇe |",
  "SA05n828:11": "niravadyāya buddhāya bodhidāya namo'stu te || 5 ||",
  "SA05n828:12": "sthānarakṣākṛtā dhīra sthānabhraṣṭaprapālaka |",
  "SA05n828:13": "locanā-māmakī-tārā-pāṇḍarābhyo namo namaḥ || 6 ||",
  "SA05n828:14": "yakṣarākṣasavetālabhujaṅgabhayahāriṇe |",
  "SA05n828:15": "amitābhakirīṭāya siddhidāya namo'stu te || 7 ||",
  "SA05n828:16": "bhikṣuromasahasrāṇi bhikṣurakṣāya te namaḥ |",
  "SA05n828:17": "ślokāṣṭakamidaṃ puṇyaṃ kīrtitaṃ te samādhipa || 8 ||",
  "SA05n828:18": "śrīmadāryāvalokiteśvarasya saptākṣarastotraṃ samāptam |"
}