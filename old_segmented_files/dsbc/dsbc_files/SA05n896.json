{
  "SA05n896:0": "dharmadhātuvāgīśvaramaṇḍalastotram",
  "SA05n896:1": "mañjuśriyaṃ mahāvīraṃ sarvamāravināśanam |",
  "SA05n896:2": "sarvasiddhīśvaraṃ nāthaṃ vāgīśvaraṃ namāmyaham || 1 ||",
  "SA05n896:3": "mañjughoṣaṃ mahāvīraṃ sarvamāravināśanam |",
  "SA05n896:4": "sarvākārapradātāraṃ dharmadhātuṃ namāmyaham || 2 ||",
  "SA05n896:5": "mahoṣṇīṣaṃ sitacchatraṃ tejorāśiṃ jayoditam |",
  "SA05n896:6": "vikiraṇodgataṃ caiva mahogratejasaṃ name || 3 ||",
  "SA05n896:7": "akṣobhyaṃ ca mahābodhiṃ vajrasattvaṃ namāmyaham |",
  "SA05n896:8": "vajrarājaṃ vajrarāgaṃ vajrasādhuṃ namāmyaham || 4 ||",
  "SA05n896:9": "śrīratnasambhavaṃ nāthaṃ vajraratnaṃ namāmyaham |",
  "SA05n896:10": "vajrasūryaṃ vajraketuṃ vajrahāsaṃ name sadā || 5 ||",
  "SA05n896:11": "amitābhaṃ mahārājaṃ vajradharmaṃ namāmyaham |",
  "SA05n896:12": "vajratīkṣṇaṃ mahāketuṃ vajrabhāṣaṃ name name || 6 ||",
  "SA05n896:13": "amoghasiddhiṃ siddheśaṃ vajrakarma namāmyaham |",
  "SA05n896:14": "vajrarakṣaṃ vajrayakṣaṃ vajrasandhiṃ name sadā || 7 ||",
  "SA05n896:15": "locanākhyāṃ mahādevīṃ pāṇḍarākhyāṃ namāmyaham |",
  "SA05n896:16": "māmakīṃ caiva devīṃ tāṃ tārādevīṃ name name || 8 ||",
  "SA05n896:17": "vajrāṅkuśaṃ mahāvīraṃ vajrapāśaṃ mahatprabham |",
  "SA05n896:18": "vajrasphoṭaḥ mahānāthaṃ vajrāveśaṃ name name || 9 ||",
  "SA05n896:19": "adhimuktīśvarīṃ bhūmiṃ pramuditāṃ namāmyaham |",
  "SA05n896:20": "vimalākhyāṃ mahābhūmiṃ prabhākarīṃ name name || 10 ||",
  "SA05n896:21": "arciṣmatīṃ sudurjayāṃ cābhimukhīṃ namāmyaham |",
  "SA05n896:22": "dūraṅgamāmacalākhyāṃ sādhumatīśvarīṃ name || 11 ||",
  "SA05n896:23": "dharmameghāṃ mahāmeghāṃ samatākhyāṃ prabhāṃ name |",
  "SA05n896:24": "ratnapāramitāṃ devīṃ dānapāramitāṃ name || 12 ||",
  "SA05n896:25": "śīlapāramitāṃ devīṃ kṣāntipāramitāṃ name |",
  "SA05n896:26": "vīryapāramitāṃ devīṃ dhyānapāramitāṃ śraye || 13 ||",
  "SA05n896:27": "prajñāpāramitāṃ devīm upāyākhyaṃ namāmyaham |",
  "SA05n896:28": "praṇidhānabalaṃ caiva jñānapāramitāṃ sadā || 14 ||",
  "SA05n896:29": "karmapāramitāṃ devīṃ namāmi satataṃ tathā |",
  "SA05n896:30": "āyuścittapariṣkāraṃ karmopapattikāṃ name || 15 ||",
  "SA05n896:31": "ṛddhyākhyāmadhimuktiṃ ca praṇidhānāṃ namāmyaham |",
  "SA05n896:32": "jñānākhyavaśitāṃ devīṃ dharmākhyavaśitāṃ name || 16 ||",
  "SA05n896:33": "tathatāṃ ca mahādevīṃ buddhabodhiṃ namāmyaham |",
  "SA05n896:34": "vasumatīṃ mahālakṣmīṃ ratnajvālāṃ name name || 17 ||",
  "SA05n896:35": "uṣṇīṣavijayāṃ devīṃ mārīcīṃ parṇaśābarīm |",
  "SA05n896:36": "jāṅguliṃ dhāraṇīṃ vande anantamukhadhāraṇīm || 18 ||",
  "SA05n896:37": "cundāṃ prajñāṃ ca padmāṃ ca sarvāvaraṇaśodhanīm |",
  "SA05n896:38": "akṣayajñānakāraṇḍaṃ dharmakāyavatīṃ name || 19 ||",
  "SA05n896:39": "dharmapratividaṃ devamarthaṃ ca pratisaṃvidam |",
  "SA05n896:40": "niruktisaṃvidaṃ devaṃ pratibhānākhyasaṃvidam || 20 ||",
  "SA05n896:41": "vajralāsyāṃ vajramālāṃ vajragītāṃ name sadā |",
  "SA05n896:42": "vajranṛtyāṃ mahādevīṃ namāmi satataṃ mudā || 21 ||",
  "SA05n896:43": "samantabhadraṃ bodhīśamakṣayatāṃ matiṃ name |",
  "SA05n896:44": "kṣitigarbhaṃ khagarbhaṃ ca bodhisattvaṃ namāmyaham || 22 ||",
  "SA05n896:45": "gagaṇagañjanātheśaṃ ratnapāṇiṃ name name |",
  "SA05n896:46": "sāgaramatiṃ bodhīśaṃ vajragarbhaṃ namāmyaham || 23 ||",
  "SA05n896:47": "lokeśvaraṃ mahāsatvaṃ sthānaprāptaṃ namāmyaham |",
  "SA05n896:48": "candraprabhaṃ mahātejaṃ vande'haṃ jālinīprabham || 24 ||",
  "SA05n896:49": "amitaprabhabodhośaṃ śrīpatiṃ bhānukūṭakam |",
  "SA05n896:50": "sarvaśokatamodghāṭaviṣkambhinaṃ namāmyaham || 25 ||",
  "SA05n896:51": "yamāntakaṃ mahāvīraṃ prajñāntakaṃ name name |",
  "SA05n896:52": "padmāntakaṃ mahāvīraṃ vighnāntakaṃ namāmyaham || 26 ||",
  "SA05n896:53": "trailokyavijayaṃ vīraṃ vajrajvālāṃ namāmyaham |",
  "SA05n896:54": "herukaṃ vajravīreśaṃ parameśaṃ namāmyaham || 27 ||",
  "SA05n896:55": "cakravarttīśvaraṃ vande sumbharājaṃ namāmyaham |",
  "SA05n896:56": "puṣpāṃ dhūpāṃ mahādīpāṃ gandhāṃ devīṃ namāmyaham || 28 ||",
  "SA05n896:57": "vajrarūpāṃ vajraśabdāṃ rasavajrāṃ namāmyaham |",
  "SA05n896:58": "vajrasparśāṃ mahādevīṃ viśvavarṇāṃ name sadā || 29 ||",
  "SA05n896:59": "indrayamajaleśāṃśca kuberamīśvaraṃ namaḥ |",
  "SA05n896:60": "agniṃ nairṛtyanāthaṃ ca vāyurājaṃ namāmyaham || 30 ||",
  "SA05n896:61": "brahmāṇaṃ viṣṇudevaṃ ca maheśvaraṃ kumārakam |",
  "SA05n896:62": "brahmāṇīṃ ca mahādevīṃ rudrāṇīṃ vaiṣṇavīṃ name || 31 ||",
  "SA05n896:63": "kaumārīṃ raktavarṇāṃ ca mahendrāṇīṃ namāmyaham |",
  "SA05n896:64": "vārāhīṃ kālikāṃ caṇḍīṃ bhṛṅgiṇaṃ gaṇanāyakam || 32 ||",
  "SA05n896:65": "mahākālaṃ mahābhīmaṃ nandikeśaṃ raviṃ name |",
  "SA05n896:66": "candraṃ bhaumaṃ budhaṃ vande guruṃ śukraṃ śanaiścaram || 33 ||",
  "SA05n896:67": "rāhuṃ ketuṃ balabhadraṃ jayakaraṃ name sadā |",
  "SA05n896:68": "madhukaraṃ vasantaṃ ca anantaṃ vāsukiṃ name || 34 ||",
  "SA05n896:69": "takṣaṃ karkoṭakaṃ padmaṃ mahāpadmaṃ namāmyaham |",
  "SA05n896:70": "śaṅkhapālaṃ ca kulikaṃ vemacitraṃ valiṃ name || 35 ||",
  "SA05n896:71": "prahlādaṃ ca mahādaityaṃ vairocanaṃ name sadā |",
  "SA05n896:72": "guruḍendra sumbharājaṃ paṃcaśikhaṃ namāmyaham || 36 ||",
  "SA05n896:73": "sarvārthasiddhiṃ vighneśaṃ pūrṇabhadraṃ namāmyaham |",
  "SA05n896:74": "maṇibhadraṃ mahāyakṣaṃ dhanadaṃ ca maheśvaram || 37 ||",
  "SA05n896:75": "vaiśravaṇaṃ mahāvīraṃ civikuṇḍalinaṃ namāmyaham |",
  "SA05n896:76": "kelimāliṃ sukhendraṃ ca calendraṃ ca namāmyaham || 38 ||",
  "SA05n896:77": "hāratīṃ yakṣiṇīṃ devīṃ bahuputravatīṃ name |",
  "SA05n896:78": "aśvinīṃ bharaṇīṃ tārāṃ kṛttikāṃ rohaṇīṃ tathā || 39 ||",
  "SA05n896:79": "mṛgaśīrṣāṃ tathaivārdrāṃ punarvasuṃ namāmyaham |",
  "SA05n896:80": "puṣyamāśleṣakātārāṃ maghāṃ ca pūrvāphālgunīm || 40 ||",
  "SA05n896:81": "uttarāphālgunīṃ hastaṃ citrāṃ svātiṃ viśākhakām |",
  "SA05n896:82": "anurādhāṃ tathā jyeṣṭhāṃ mūlatārāṃ namāmyaham || 41 ||",
  "SA05n896:83": "pūrvāṣāḍhottarāṣāḍhāṃ śravaṇāṃ ca namāmyaham |",
  "SA05n896:84": "dhaniṣṭhāṃ śatabhiṣāṃ ca pūrvottarābhādrapadām || 42 ||",
  "SA05n896:85": "revatīṃ ca mahātārām abhijitaṃ namāmyaham |",
  "SA05n896:86": "vajrāṅkuśaṃ mahāvīraṃ vajrapāśaṃ namāmyaham || 43 ||",
  "SA05n896:87": "vajrasphoṭaṃ mahābhīmaṃ vajrāṃśaṃ vai name name |",
  "SA05n896:88": "vāgīśvaraṃ mahābodhiṃ sarvavighnavināśakam || 44 ||",
  "SA05n896:89": "sarvajñaṃ jñānadātāraṃ dharmadhātuṃ namāmyaham |",
  "SA05n896:90": "mañjuśriyaṃ mahājñānaṃ sarvavidyāpradeśvaram |",
  "SA05n896:91": "sarvākārasvarūpaṃ ca vādirājaṃ namāmyaham || 45 ||",
  "SA05n896:92": "śrī dharmadhātuvāgīśvaramaṇḍalastotraṃ samāptam |"
}