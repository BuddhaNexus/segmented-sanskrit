{
  "SA05n856:0": "nairātmāṣṭakastotram",
  "SA05n856:1": "nirālamba-nirākāra-nirvikalpasvarūpiṇī |",
  "SA05n856:2": "nairātmā varadā devī nityameva namo'stu te || 1 ||",
  "SA05n856:3": "ekavaktrāṃ trinetrāṃ ca ūrdhvapiṅgalamūrdhajām |",
  "SA05n856:4": "kare kartikapālāṃ ca namāmi viśvamātarīm || 2 ||",
  "SA05n856:5": "navanṛtyabhūṣitāṃ ca sahasramukharūpiṇīm |",
  "SA05n856:6": "yogeśvarīṃ yogagamyāṃ namāmi jñānarūpiṇīm || 3 ||",
  "SA05n856:7": "śavārūḍhāṃ mahātejāṃ sarvaduṣṭanikṛntanīm |",
  "SA05n856:8": "sarvapāpaharāṃ devīṃ namāmi jagadīśvarīm || 4 ||",
  "SA05n856:9": "gaurīṃ caurīṃ ca vetālīṃ ghasmarīṃ pukkasīṃ tathā |",
  "SA05n856:10": "śabarīṃ cāṇḍālīṃ ḍombīṃ namāmi cāṣṭayoginīm || 5 ||",
  "SA05n856:11": "brahmāviṣṇuśivaśakracaturmāravināśinīm |",
  "SA05n856:12": "trailokyamohinīṃ devīṃ namāmi surasundarīm || 6 ||",
  "SA05n856:13": "akṣamālavibhūṣāṅgīm akārabījasambhavām |",
  "SA05n856:14": "nityaṃ dhyānaratāṃ devīṃ namāmi viśvamātarīm || 7 ||",
  "SA05n856:15": "sarvabhūtapriyāṃ devīṃ sarvamantraprabodhinīm |",
  "SA05n856:16": "sarvamantramayīṃ mātarnairātmāṃ tvāṃ namāmyaham || 8 ||",
  "SA05n856:17": "nairātmāṣṭakastotraṃ samāptam |"
}