{
  "SA05n811:0": "aṣṭamātṛkāstotram",
  "SA05n811:1": "om namaḥ śrīvighneśvarāṣṭamātṛkābhyaḥ |",
  "SA05n811:2": "vighneśvara mahāvīra siddhirūpāya vṛddhaye |",
  "SA05n811:3": "vighnanāśāya devāya gaṇeśāya namāmyaham || 1 ||",
  "SA05n811:4": "pūrve brahmāṇī devī haṃsamāruhya saṃsthitā |",
  "SA05n811:5": "pītavarṇaprabhā devī śrībrahmāṇi namo'stu te || 2 ||",
  "SA05n811:6": "dakṣiṇe śrīvārāhī mahiṣāsanasaṃsthitā |",
  "SA05n811:7": "raktavarṇā'ṅkuśahastā śrīvārāhi namo'stu te || 3 ||",
  "SA05n811:8": "paścime cendrāṇī devī gajamāruhya saṃsthitā |",
  "SA05n811:9": "kuṅkumābhā vajrahastā śrīindrāṇi namo'stu te || 4 ||",
  "SA05n811:10": "uttare māheśvarī devī vṛṣamāruhya saṃsthitā |",
  "SA05n811:11": "śvetavarṇaprabhā devī māheśvari namo'stu te || 5 ||",
  "SA05n811:12": "āgneye bālakaumārī mayūrakāntipūraṇī |",
  "SA05n811:13": "raktavarṇā śaktihastā śrīkaumāri namo'stu te || 6 ||",
  "SA05n811:14": "nairṛtye vaiṣṇavī devī śyāmābhā garuḍāsanā |",
  "SA05n811:15": "śaṅkhacakradharā devī nārāyaṇi namo'stu te || 7 ||",
  "SA05n811:16": "vāyavye cāmuṇḍā devī siṃhamāruhya saṃsthitā |",
  "SA05n811:17": "kakāramūrtidhārī ca khaḍgahastāṃ namāmyaham || 8 ||",
  "SA05n811:18": "īśāne caṇḍikā devī dhūmravarṇā prajvālinī |",
  "SA05n811:19": "kartimuṇḍadharā devī mahālakṣmi namo'stu te || 9 ||",
  "SA05n811:20": "aṣṭapīṭhasthitā devīraṣṭavṛkṣanivāsinīḥ |",
  "SA05n811:21": "aṣṭabhairavasaṃyuktā aṣṭamātṛkā namāmyaham || 10 ||",
  "SA05n811:22": "sūryaḥ somo mahīputro budho devāsuragurū |",
  "SA05n811:23": "śaniśca rāhuḥ ketuśca yamāya ca namo namaḥ || 11 ||",
  "SA05n811:24": "śrī aṣṭamātṛkāstotraṃ samāptam ||"
}