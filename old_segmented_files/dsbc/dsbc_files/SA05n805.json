{
  "SA05n805:0": "ākāśagarbhanāmāṣṭottaraśatastotram",
  "SA05n805:1": "om namo buddhāya",
  "SA05n805:2": "ākāśagarbha sattvārtha mahāsattva mahādyute |",
  "SA05n805:3": "mahāratna suratnāgrya vajraratna namo'stu te || 1 ||",
  "SA05n805:4": "abhiṣekamahāratna mahāśuddha mahāśubha |",
  "SA05n805:5": "buddharatna viśuddhāṅga ratnaratna namo'stu te || 2 ||",
  "SA05n805:6": "ākāśākāśasaṃbhūta sarvākāśa mahānabha |",
  "SA05n805:7": "ākāśadhātusarvāśa sarvāśāgrya namo'stu te || 3 ||",
  "SA05n805:8": "ratnasaṃbhava ratnorṇa buddhorṇa sutathāgata |",
  "SA05n805:9": "sarvaratna susarvāgrya ratnakārya namo'stu te || 4 ||",
  "SA05n805:10": "ratnaratnāgrya ratnogra ratnasarva tathāgata |",
  "SA05n805:11": "ratnottama mahākāśa samākāśa namo'stu te || 5 ||",
  "SA05n805:12": "alaṅkāramahāśobha śobhākara suśobhaka |",
  "SA05n805:13": "śuddhasarvārtha śuddhārtha dānacarya namo'stu te || 6 ||",
  "SA05n805:14": "dharmaratna viśuddhāgrya saṅgharatna tathāgata |",
  "SA05n805:15": "mahābhiṣeka lokārtha pramodārtha namo'stu te || 7 ||",
  "SA05n805:16": "dāna pradāna dānāgrya tyāga tyāgāgrya dāyaka |",
  "SA05n805:17": "sarvasattvārtha tattvārtha mahārthārtha namo'stu te || 8 ||",
  "SA05n805:18": "cintārāja mahāteja dānapāramitānaya |",
  "SA05n805:19": "tathāgata mahāsattva sarvabuddha namo'stu te || 9 ||",
  "SA05n805:20": "tathāgata mahāratna tathāgata mahāprabha |",
  "SA05n805:21": "tathāgata mahāketo mahāhāsa namo'stu te || 10 ||",
  "SA05n805:22": "tathāgatābhiṣekājña mahābhiṣeka mahāvibho |",
  "SA05n805:23": "lokanātha trilokāgrya lokasūrya namo'stu te || 11 ||",
  "SA05n805:24": "ratnādhikādhikatara ratnabhūṣaṇa ratnadhṛk |",
  "SA05n805:25": "ratnāloka mahāloka ratnakīrte namo'stu te || 12 ||",
  "SA05n805:26": "ratnotkara suratnottha maṇe vajramaṇe guṇa |",
  "SA05n805:27": "ratnākara sudīptāṅga sarvaratna namo'stu te || 13 ||",
  "SA05n805:28": "mahātmayaṣṭi ratneśa sarvāśāparipūraka |",
  "SA05n805:29": "sarvābhiprāyasaṃprāptiratnarāśi namo'stu te || 14 ||",
  "SA05n805:30": "abhvagrya vyāpi sarvātma varaprada mahāvara |",
  "SA05n805:31": "vibhūte sarvasaṃpatte vajragarbhaṃ namo'stu te || 15 ||",
  "SA05n805:32": "yaḥ kaścid dhārayen nāmnāmidante'ṣṭaśataṃ śivam |",
  "SA05n805:33": "sarvabuddhābhiṣekaṃ tu sa prāpnotyanaghaḥ kṣaṇāt || 16 ||",
  "SA05n805:34": "śrī ākāśagarbhanāmāṣṭaśatādhyeṣaṇāstotraṃ saṃpūrṇam |"
}