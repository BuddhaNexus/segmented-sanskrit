{
  "SA20n1339:0": "yānā'nuttaryaparicchedaḥ pañcamaḥ",
  "SA20n1339:1": "ānuttaryaṃprapattau hi punarālambane matam |",
  "SA20n1339:2": "samudāgama uddiṣṭaṃ pratipattistu ṣaḍ vidhā ||1 ||",
  "SA20n1339:3": "paramā'tha manaskāre anudharme'ntavarjane |",
  "SA20n1339:4": "viśiṣṭā cāviśiṣṭā ca paramā dvādaśātmikā ||2 ||",
  "SA20n1339:5": "audāryamāyatatvañca adhikaro'kṣayātmatā |",
  "SA20n1339:6": "nairantaryamakṛcchratvaṃ vittatvañca parigrahaḥ ||3 ||",
  "SA20n1339:7": "ārambhaprāptiniṣyandaniṣpattiḥ paramā matā |",
  "SA20n1339:8": "tataśca paramārthena daśa pāramitā matāḥ ||4 ||",
  "SA20n1339:9": "dānaṃ śīlaṃ kṣamā vīryaṃ dhyānaṃ prajñā upāyatā |",
  "SA20n1339:10": "praṇidhānaṃ balaṃ jñānametāḥ pāramitā daśa ||5 ||",
  "SA20n1339:11": "anugraho'vighātaśca karma tasya ca marṣaṇam |",
  "SA20n1339:12": "guṇavṛddhiśca sāmarthyamavatāravimocane ||6 ||",
  "SA20n1339:13": "akṣayatvaṃ sadā vṛttirniyataṃ bhogapācane |",
  "SA20n1339:14": "yathāprajñaptito dharmamahāyānamanaskriyā ||7 ||",
  "SA20n1339:15": "bodhisattvasya satataṃ prajñayā triprakārayā |",
  "SA20n1339:16": "dhātupuṣṭayaipraveśāya cārthasiddhyai bhavatyasau ||8 ||",
  "SA20n1339:17": "saṃyuktā dharmacaritaiḥ sā jñeyā daśabhiḥ punaḥ |",
  "SA20n1339:18": "lekhanā pūjanā dānaṃ śravaṇaṃ vācanod grahaḥ ||9 ||",
  "SA20n1339:19": "prakāśanā'tha svādhyāyaścintanā bhāvanā ca tat |",
  "SA20n1339:20": "ameyapuṇyaskandhaṃ hi caritaṃ tad daśātmakam ||10 ||",
  "SA20n1339:21": "viśeṣādakṣayatvācca parānugrahato'śamāt |",
  "SA20n1339:22": "avikṣiptā'viparyāsapraṇatā cā'nudhārmikī ||11 ||",
  "SA20n1339:23": "vyutthānaṃ viṣaye sārastathāsvādalayoddhavaḥ |",
  "SA20n1339:24": "sambhāvanā'bhisandhiśca manaskāre'pyahaṃkṛtiḥ ||12 ||",
  "SA20n1339:25": "hīnacittañca vikṣepaḥ parijñeyo hi dhīmatā |",
  "SA20n1339:26": "vyañjanā'rthamanaskāre'visāre lakṣaṇadvaye ||13 ||",
  "SA20n1339:27": "aśuddhaśuddhāvāgantukatve'trāsitā'nunnatau |",
  "SA20n1339:28": "saṃyogāt saṃstavāccaiva viyogādapyasaṃstavāt ||14 ||",
  "SA20n1339:29": "arthasattvamasattvañca vyañjane so'viparyayaḥ |",
  "SA20n1339:30": "dvayena pratibhāsatvaṃ tathā cā'vidyamānatā ||15 ||",
  "SA20n1339:31": "arthe sa cā'viparyāsaḥ sadasattvena varjitaḥ |",
  "SA20n1339:32": "tajjalpabhāvito jalpamanaskārastadāśrayaḥ ||16 ||",
  "SA20n1339:33": "manaskāre'viparyāso dvayaprakhyānakāraṇe |",
  "SA20n1339:34": "māyādivadasattvañca sattvañcā'rthasya tanmatam ||17 ||",
  "SA20n1339:35": "so'visāre'viparyāso bhāvā'bhāvā'visārataḥ |",
  "SA20n1339:36": "sarvasya nāmamātratvaṃ sarvakalpā'pravṛttaye ||18 ||",
  "SA20n1339:37": "svalakṣaṇe'viparyāsaḥ paramārthe svalakṣaṇe |",
  "SA20n1339:38": "dharmadhātuvinirmukto yasmād dharmo na vidyate ||19 ||",
  "SA20n1339:39": "sāmānyalakṣaṇantasmāt sa ca tatrā'viparyayaḥ |",
  "SA20n1339:40": "viparyastamanaskārā'vihāniparihāṇitaḥ ||20 ||",
  "SA20n1339:41": "tadaśuddhirviśuddhiśca sa ca tatrā'viparyayaḥ |",
  "SA20n1339:42": "dharmadhātorviśuddhatvāt prakṛtyā vyomavatpunaḥ ||21 ||",
  "SA20n1339:43": "dvayasyāgantukatvaṃ hi sa ca tatrā'viparyayaḥ |",
  "SA20n1339:44": "saṃkleśaśca viśuddhiśca dharmapudgalayorna hi ||22 ||",
  "SA20n1339:45": "asattvāt trāsatāmānau nā'taḥ so'trā'viparyayaḥ |",
  "SA20n1339:46": "pṛthaktvaikatvamantaśca tīrthyaśrāvakayorapi ||23 ||",
  "SA20n1339:47": "samāropā'pavādā'nto dvidhā pudgaladharmayoḥ |",
  "SA20n1339:48": "vipakṣapratipakṣā'ntaḥ śāśvatocchedasaṃjñitaḥ ||24 ||",
  "SA20n1339:49": "grāhyagrāhakasaṃkleśavyavadāne dvidhā tridhā |",
  "SA20n1339:50": "vikalpadvayatā'ntaśca sa ca saptavidho mataḥ ||25 ||",
  "SA20n1339:51": "bhāvā'bhāve praśāmye'tha śamane trāsyatadbhaye |",
  "SA20n1339:52": "grāhyagrāhe'tha samyaktvamithyātve vyāpṛtau na ca ||26 ||",
  "SA20n1339:53": "ajanmasamakālatve sa vikalpadvayā'ntatā |",
  "SA20n1339:54": "viśiṣṭā cā'viśiṣṭā ca jñeyā daśasu bhūmiṣu ||27 ||",
  "SA20n1339:55": "vyavasthānantato dhātuḥ sādhyasādhanadhāraṇā |",
  "SA20n1339:56": "avadhārapradhārā ca prativedhaḥ pratānatā ||28 ||",
  "SA20n1339:57": "pragamaḥ praśaṭhatvañca prakarṣālambanammatam |",
  "SA20n1339:58": "avaikalyā'pratikṣepo'vikṣepaśca prapūraṇā ||29 ||",
  "SA20n1339:59": "samutpādo nirūḍhiśca karmaṇyatvā'pratiṣṭhitā |",
  "SA20n1339:60": "nirāvaraṇatā tasyā'praśrabdhisamudāgamaḥ ||30 ||",
  "SA20n1339:61": "śāstraṃ madhyavibhāgaṃ hi gūḍhasārārthameva ca |",
  "SA20n1339:62": "mahārthañcaiva sarvārthaṃ sarvā'narthapraṇodanam ||31 ||",
  "SA20n1339:63": " ||iti yānānuttarya paricchedaḥ pañcamaḥ ||",
  "SA20n1339:64": "samāptā madhyāntavibhāgakārikāḥ"
}