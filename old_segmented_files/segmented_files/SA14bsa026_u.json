{
  "SA14bsa026_u:0": "Abhisamayālaṅkāra (Asa)",
  "SA14bsa026_u:1": "namo mañjuśriye kumārabhūtāya",
  "SA14bsa026_u:2": "āryamaitreyanāthaviracitam abhisamayālaṅkāro nāma prajñāpāramitopadeśaśāstram",
  "SA14bsa026_u:3": "sarvākārajñātādhikāraḥ prathamaḥ",
  "SA14bsa026_u:4": "maṅgālācaraṇam",
  "SA14bsa026_u:5": "yā sarvajñatayā nayatyupaśamaṃ śāntaiṣiṇaḥ śrāvakān",
  "SA14bsa026_u:6": "yā mārgajñatayā jagaddhitakṛtāṃ lokārthasampādikā /",
  "SA14bsa026_u:7": "sarvākāramidaṃ vadanti munayo viśvaṃ yayā saṃgatās",
  "SA14bsa026_u:8": "tasyai śrāvakabodhisattvagaṇino buddhasya mātre namaḥ // Asa_1.1 //",
  "SA14bsa026_u:9": "grānthārambhaprayojanam sarvākārajñatāmārgaḥ śāsitrā yo 'tra deśitaḥ /",
  "SA14bsa026_u:10": "dhīmanto vīkṣiṣiraṃstamanālīḍhaṃ parairiti // Asa_1.2 //",
  "SA14bsa026_u:11": "smṛtau cādhāya sūtrārthaṃ dharmacaryāṃ daśātmikām /",
  "SA14bsa026_u:12": "sukhena pratipatsīrannityārambhaprayojanam // Asa_1.3 //",
  "SA14bsa026_u:13": "prajñāpāramitāyāḥ kāyikavyavasthāpanam",
  "SA14bsa026_u:14": "prajñāpāramitāṣṭābhiḥ padārthaiḥ samudīritā /",
  "SA14bsa026_u:15": "sarvākārajñatā mārgajñatā sarvajñatā tataḥ // Asa_1.4 //",
  "SA14bsa026_u:16": "sarvākārābhisambodho mūrdhaprāpto 'nupūrvikaḥ /",
  "SA14bsa026_u:17": "ekakṣaṇābhisambodho dharmakāyaśca te 'ṣṭadhā // Asa_1.5 //",
  "SA14bsa026_u:18": "cittotpādo 'vavādaśca nirvedhāṅgaṃ caturvidham /",
  "SA14bsa026_u:19": "ādhāraḥ pratipatteśca dharmadhātusvabhāvakaḥ // Asa_1.6 //",
  "SA14bsa026_u:20": "ālambanaṃ samuddeśaḥ sannāhaprasthitikriye /",
  "SA14bsa026_u:21": "sambhārāśca saniryāṇāḥ sarvākārajñatā muneḥ // Asa_1.7 //",
  "SA14bsa026_u:22": "dhyāmīkaraṇatādīni śiṣyakhaḍgapathau ca yau /",
  "SA14bsa026_u:23": "mahānuśaṃso dṛṅmārga ehikāmutrikairguṇaiḥ // Asa_1.8 //",
  "SA14bsa026_u:24": "kāritramadhimuktiśca stutastobhitaśaṃsitāḥ /",
  "SA14bsa026_u:25": "pariṇāme 'numode ca manaskārāvanuttamau // Asa_1.9 //",
  "SA14bsa026_u:26": "nirhāraḥ śuddhiratyantamityayaṃ bhāvanāpathaḥ /",
  "SA14bsa026_u:27": "vijñānāṃ bodhisattvānāmiti mārgajñatoditā // Asa_1.10 //",
  "SA14bsa026_u:28": "prajñayā na bhave sthānaṃ kṛpayā na śame sthitiḥ /",
  "SA14bsa026_u:29": "anupāyena dūratvam upāyenāvidūratā // Asa_1.11 //",
  "SA14bsa026_u:30": "vipakṣapratipakṣau ca prayogaḥ samatāsya ca /",
  "SA14bsa026_u:31": "dṛṅmārgaḥ śrāvakādīnāmiti sarvajñateṣyate // Asa_1.12 //",
  "SA14bsa026_u:32": "ākārāḥ saprayogāśca guṇā doṣāḥ salakṣaṇāḥ /",
  "SA14bsa026_u:33": "mokṣanirvedhabhāgīye śaikṣo 'vaivartiko gaṇaḥ // Asa_1.13 //",
  "SA14bsa026_u:34": "samatā bhavaśāntyośca kṣetraśuddhiranuttarā /",
  "SA14bsa026_u:35": "sarvākārābhisambodha eṣa sopāyakauśalaḥ // Asa_1.14 //",
  "SA14bsa026_u:36": "liṅgaṃ tasya vivṛddhiśca nirūḍhiścittasaṃsthitiḥ /",
  "SA14bsa026_u:37": "caturdhā ca vikalpasya pratipakṣaścaturvidhaḥ // Asa_1.15 //",
  "SA14bsa026_u:38": "pratyekaṃ darśanākhye ca bhāvanākhye ca vartmani /",
  "SA14bsa026_u:39": "ānantaryasamādhiśca saha vipratipattibhiḥ // Asa_1.16 //",
  "SA14bsa026_u:40": "mūrdhābhisamayastredhā daśadhā cānupūrvikaḥ /",
  "SA14bsa026_u:41": "ekakṣaṇābhisambodho lakṣaṇena caturvidhaḥ // Asa_1.17 //",
  "SA14bsa026_u:42": "svābhāvikaḥ sasāmbhogo nairmāṇiko 'parastathā /",
  "SA14bsa026_u:43": "dharmakāyaḥ sakāritraścaturdhā samudīritaḥ // Asa_1.18 //",
  "SA14bsa026_u:44": "sarvākārajñatā",
  "SA14bsa026_u:45": "1 - cittotpādaḥ",
  "SA14bsa026_u:46": "cittotpādaḥ parārthāya samyaksambodhikāmatā /",
  "SA14bsa026_u:47": "samāsavyāsataḥ sā ca yathāsūtraṃ sa cocyate // Asa_1.19 //",
  "SA14bsa026_u:48": "bhūhemacandrajvalanairnidhiratnākarārṇavaiḥ /",
  "SA14bsa026_u:49": "vajrācalauṣadhīmitraiścintāmaṇyarkagītibhiḥ // Asa_1.20 //",
  "SA14bsa026_u:50": "nṛpagañjamahāmārgayānaprasravaṇodakaiḥ /",
  "SA14bsa026_u:51": "ānandoktinadīmevairdvāviṃśatividhaḥ sa ca // Asa_1.21 //",
  "SA14bsa026_u:52": "2 - avavādaḥ",
  "SA14bsa026_u:53": "pratipattau ca satyeṣu buddharatnādiṣu triṣu /",
  "SA14bsa026_u:54": "asaktāvapariśrānto pratipatsamparigrahe // Asa_1.22 //",
  "SA14bsa026_u:55": "cakṣuḥṣu pañcasu jñeyaḥ ṣaṭsvabhijñāguṇeṣu ca /",
  "SA14bsa026_u:56": "dṛṅmārge bhāvanākhye cetyavavādo daśātmakaḥ // Asa_1.23 //",
  "SA14bsa026_u:57": "mṛdutīkṣṇendriyau śraddhādṛṣṭiprāptau kulaṅkalau /",
  "SA14bsa026_u:58": "ekavīcyantarotpadya kārākārākaniṣṭhagāḥ // Asa_1.24 //",
  "SA14bsa026_u:59": "plutāstrayo bhavasyāgraparamo rūparāgahā /",
  "SA14bsa026_u:60": "dṛṣṭadharmaśamaḥ kāyasākṣī khaḍgaśca viṃśatiḥ // Asa_1.25 //",
  "SA14bsa026_u:61": "3 - nirvedhāṅgam",
  "SA14bsa026_u:62": "ālambanata ākārāddhetutvāt samparigrahāt /",
  "SA14bsa026_u:63": "caturvikalpasaṃyogaṃ yathāsvaṃ bhajatāṃ satām // Asa_1.26 //",
  "SA14bsa026_u:64": "śrāvakebhyaḥ sakhaḍgebhyo bodhisattvasya tāyinaḥ /",
  "SA14bsa026_u:65": "mṛdumadhyādhimātrāṇāmūṣmādīnāṃ viśiṣṭatā // Asa_1.27 //",
  "SA14bsa026_u:66": "ālambanamanityādi satyādhāraṃ tadākṛtiḥ /",
  "SA14bsa026_u:67": "niṣedho 'bhiniveśāderheturyānatrayāptaye // Asa_1.28 //",
  "SA14bsa026_u:68": "rūpādyāyavyayau viṣṭhāsthiti prajñaptyavācyate /",
  "SA14bsa026_u:69": "rūpāvādavasthitisteṣāṃ tadbhāvenāsvabhāvatā // Asa_1.29 //",
  "SA14bsa026_u:70": "tayormithaḥ svabhāvatvaṃ tadanityādyasaṃsthitiḥ /",
  "SA14bsa026_u:71": "tāsāṃ tadbhāvaśūnyatvaṃ mithaḥ svābhāvyametayoḥ // Asa_1.30 //",
  "SA14bsa026_u:72": "anudgraho yo dharmāṇāṃ tannimittāsamīkṣaṇam /",
  "SA14bsa026_u:73": "parīkṣaṇaṃ ca prajñāyāḥ sarvasyānupalambhataḥ // Asa_1.31 //",
  "SA14bsa026_u:74": "rūpāderasvabhāvatvaṃ tadabhāvasvabhāvatā /",
  "SA14bsa026_u:75": "tadajātiraniryāṇaṃ śuddhistadanimittatā // Asa_1.32 //",
  "SA14bsa026_u:76": "tannimittānadhiṣṭhānānadhimuktirasaṃjñatā /",
  "SA14bsa026_u:77": "samādhistasya kāritraṃ vyākṛtirmananākṣayaḥ // Asa_1.33 //",
  "SA14bsa026_u:78": "mithastrikasya svābhāvyaṃ samādheravikalpanā /",
  "SA14bsa026_u:79": "iti nirvedhabhāgīyaṃ mṛdumadhyādhimātrataḥ // Asa_1.34 //",
  "SA14bsa026_u:80": "dvaividhyaṃ grāhyakalpasya vastutatpratipakṣataḥ /",
  "SA14bsa026_u:81": "moharāśyādibhedena pratyekaṃ navadhā tu saḥ // Asa_1.35 //",
  "SA14bsa026_u:82": "dravyaprajñaptyadhiṣṭhāno dvividho grāhako mataḥ /",
  "SA14bsa026_u:83": "svatantrātmādirūpeṇa skandhādyāśrayatastathā // Asa_1.36 //",
  "SA14bsa026_u:84": "cittānavalīnatvādinaiḥsvābhāvyādideśakaḥ /",
  "SA14bsa026_u:85": "tadvipakṣaparityāgaḥ sarvathā samparigrahaḥ // Asa_1.37 //",
  "SA14bsa026_u:86": "4 - pratipatterādhāraḥ",
  "SA14bsa026_u:87": "ṣoḍhādhigamadharmasya pratipakṣaprahāṇayoḥ /",
  "SA14bsa026_u:88": "tayoḥ paryupayogasya prajñāyāḥ kṛpayā saha // Asa_1.38 //",
  "SA14bsa026_u:89": "śiṣyāsādhāraṇatvasya parārthānukramasya ca /",
  "SA14bsa026_u:90": "jñānasyāyatnavṛtteśca pratiṣṭhā gotramucyate // Asa_1.39 //",
  "SA14bsa026_u:91": "dharmadhātorasambhedād gotrabhedo na yujyate /",
  "SA14bsa026_u:92": "ādheyadharmabhedāttu tadbhedaḥ parigīyate // Asa_1.40 //",
  "SA14bsa026_u:93": "5 - ālambanam",
  "SA14bsa026_u:94": "ālambanaṃ sarvadharmāste punaḥ kuśalādayaḥ /",
  "SA14bsa026_u:95": "laukikādhigamākhyāśca ye ca lokottarā matāḥ // Asa_1.41 //",
  "SA14bsa026_u:96": "sāsravānāsravā dharmāḥ saṃskṛtāsaṃskṛtāśca ye /",
  "SA14bsa026_u:97": "śiṣyasādhāraṇā dharmā ye cāsādhāraṇā muneḥ // Asa_1.42 //",
  "SA14bsa026_u:98": "6 - samuddeśaḥ",
  "SA14bsa026_u:99": "sarvasattvāgratācittaprahāṇādhigamatraye /",
  "SA14bsa026_u:100": "tribhirmahattvairuddeśo vijñeyo 'yaṃ svayambhuvām // Asa_1.43 //",
  "SA14bsa026_u:101": "7 - sannāhapratipattiḥ",
  "SA14bsa026_u:102": "dānādau ṣaḍvidhe teṣāṃ pratyekaṃ saṃgraheṇa yā /",
  "SA14bsa026_u:103": "sannāhapratipattiḥ sā ṣaḍbhiḥ ṣaṭkairyathoditā // Asa_1.44 //",
  "SA14bsa026_u:104": "8 - prasthānapratipattiḥ",
  "SA14bsa026_u:105": "dhyānārūpyeṣu dānādau mārge maitryādikeṣu ca /",
  "SA14bsa026_u:106": "gatopalambhayoge ca trimaṇḍalaviśuddhiṣu // Asa_1.45 //",
  "SA14bsa026_u:107": "uddeśe ṣaṭsvabhijñāsu sarvākārajñatānaye /",
  "SA14bsa026_u:108": "prasthānapratipajjñeyā mahāyānādhirohiṇī // Asa_1.46 //",
  "SA14bsa026_u:109": "9 - sambhārapratipattiḥ",
  "SA14bsa026_u:110": "dayā dānādikaṃ ṣaṭkaṃ śamathaḥ savidarśanaḥ /",
  "SA14bsa026_u:111": "yuganaddhaśca yo mārga upāye yacca kauśalam // Asa_1.47 //",
  "SA14bsa026_u:112": "jñānaṃ puṇyaṃ ca mārgaśca dhāraṇī bhūmayo daśa /",
  "SA14bsa026_u:113": "pratipakṣaśca vijñeyaḥ sambhārapratipatkramaḥ // Asa_1.48 // 1.",
  "SA14bsa026_u:115": "pramuditā bhūmiḥ",
  "SA14bsa026_u:116": "labhyate prathamā bhūmirdaśadhā parikarmaṇā",
  "SA14bsa026_u:117": "āśayo hitavastutvaṃ sattveṣu samacittatā // Asa_1.49 //",
  "SA14bsa026_u:118": "tyāgaḥ sevā ca mitrāṇāṃ saddharmālambanaiṣaṇā /",
  "SA14bsa026_u:119": "sadā naiṣkramyacittatvaṃ buddhakāyagatā spṛhā // Asa_1.50 //",
  "SA14bsa026_u:120": "dharmasya deśanā satyaṃ daśamaṃ vākyamiṣyate /",
  "SA14bsa026_u:121": "jñeyaṃ ca parikarmaiṣāṃ svabhāvānupalambhataḥ // Asa_1.51 // 2.",
  "SA14bsa026_u:123": "vimalā bhūmiḥ",
  "SA14bsa026_u:124": "śīlaṃ kṛtajñatā kṣāntiḥ prāmodyaṃ mahatī kṛpā /",
  "SA14bsa026_u:125": "gauravaṃ guruśuśrūṣā vīryaṃ dānādike 'ṣṭamam // Asa_1.52 // 3.",
  "SA14bsa026_u:127": "prabhākarī bhūmiḥ",
  "SA14bsa026_u:128": "atṛptatā śrute dānaṃ dharmasya ca nirāmiṣam /",
  "SA14bsa026_u:129": "buddhakṣetrasya saṃśuddhiḥ saṃsārāparikheditā // Asa_1.53 //",
  "SA14bsa026_u:130": "hrīrapatrāpyamityetat pañcadhā mananātmakam / 4.",
  "SA14bsa026_u:132": "arciṣmatī bhūmiḥ",
  "SA14bsa026_u:133": "vanāśālpecchatā tuṣṭirdhūtasaṃlekhasevanam // Asa_1.54 //",
  "SA14bsa026_u:134": "śikṣāyā aparityāgaḥ kāmānāṃ vijugupsanam /",
  "SA14bsa026_u:135": "nirvitsarvāstisantyāgo 'navalīnānapekṣate // Asa_1.55 // 5.",
  "SA14bsa026_u:137": "sudurjayā bhūmiḥ",
  "SA14bsa026_u:138": "saṃstavaṃ kulamātsaryaṃ sthānaṃ saṅgaṇikāvaham /",
  "SA14bsa026_u:139": "ātmotkarṣaparāvajñe",
  "SA14bsa026_u:140": "karmamārgān daśāśubhān // Asa_1.56 //",
  "SA14bsa026_u:141": "mānaṃ stambhaṃ viparyāsaṃ vimatiṃ kleśamarṣaṇam /",
  "SA14bsa026_u:142": "vivarjayan samāpnoti daśaitān pañcamī bhuvam // Asa_1.57 // 6.",
  "SA14bsa026_u:144": "abhimukhī bhūmiḥ",
  "SA14bsa026_u:145": "dānaśīlakṣamāvīryadhyānaprajñāprapūraṇāt /",
  "SA14bsa026_u:146": "śiṣyakhaḍgaspṛhātrāsacetasāṃ parivarjakaḥ // Asa_1.58 //",
  "SA14bsa026_u:147": "yācito 'navalīnaśca sarvatyāge 'pyadurmanāḥ /",
  "SA14bsa026_u:148": "kṛśo 'pi nārthināṃ kṣeptā ṣaṣṭhīṃ bhūmiṃ samaśnute // Asa_1.59 // 7.",
  "SA14bsa026_u:150": "dūraṅgamā bhūmiḥ",
  "SA14bsa026_u:151": "ātmasattvagraho jīvapudgalocchedaśāśvataḥ /",
  "SA14bsa026_u:152": "nimittahetvoḥ skandheṣu dhātuṣvāyataneṣu ca // Asa_1.60 //",
  "SA14bsa026_u:153": "traidhātuke pratiṣṭhānaṃ saktirālīnacittatā /",
  "SA14bsa026_u:154": "ratnatritayaśīleṣu taddṛṣṭayabhiniveśitā // Asa_1.61 //",
  "SA14bsa026_u:155": "śūnyatāyāṃ vivādaśca tadvirodhaśca viṃśatiḥ /",
  "SA14bsa026_u:156": "kalaṅkā yasya vicchinnāḥ saptamīmetyasau bhuvam // Asa_1.62 //",
  "SA14bsa026_u:157": "trivimokṣamukhajñānaṃ trimaṇḍalaviśuddhatā /",
  "SA14bsa026_u:158": "karuṇāmananā dharmasamataikanayajñatā // Asa_1.63 //",
  "SA14bsa026_u:159": "anutpādakṣamājñānaṃ dharmāṇāmekadheraṇā /",
  "SA14bsa026_u:160": "kalpanāyāḥ samudghātaḥ saṃjñādṛkkleśavarjanam // Asa_1.64 //",
  "SA14bsa026_u:161": "śamathasya ca nidhyaptiḥ kauśalaṃ ca vidarśane /",
  "SA14bsa026_u:162": "cittasya dāntatā jñānaṃ sarvatrapratighāti ca // Asa_1.65 //",
  "SA14bsa026_u:163": "sakterabhūmiryatrecchaṃ kṣetrāntaragatiḥ samam /",
  "SA14bsa026_u:164": "sarvatra svātmabhāvasya darśanaṃ ceti viṃśatiḥ // Asa_1.66 // 8.",
  "SA14bsa026_u:166": "acalā bhūmiḥ",
  "SA14bsa026_u:167": "sarvasattvamanojñānamabhijñākrīḍanaṃ śubhā /",
  "SA14bsa026_u:168": "buddhakṣetrasya niṣpattirbuddhasevāparīkṣaṇe // Asa_1.67 //",
  "SA14bsa026_u:169": "akṣajñānaṃ jinakṣetraśuddhirmāyopamā sthitiḥ /",
  "SA14bsa026_u:170": "saṃcintya ca bhavādānamidaṃ karmāṣṭadhoditam // Asa_1.68 // 9.",
  "SA14bsa026_u:172": "sādhumato bhūmiḥ",
  "SA14bsa026_u:173": "praṇidhānānyanantāni devādīnāṃ rūtajñatā /",
  "SA14bsa026_u:174": "nadīva pratibhānānāṃ garbhāvakrāntiruttamā // Asa_1.69 //",
  "SA14bsa026_u:175": "kulajātyośca gotraśca parivārasya janmanaḥ /",
  "SA14bsa026_u:176": "naiṣkramyabodhivṛkṣāṇāṃ guṇapūre svasampadaḥ // Asa_1.70 // 10.",
  "SA14bsa026_u:178": "dharmamedhā bhūmiḥ",
  "SA14bsa026_u:179": "navabhūmīratikramya buddhabhūmau pratiṣṭhate /",
  "SA14bsa026_u:180": "yena jñānena sā jñeyā daśamī bodhisattvabhūḥ // Asa_1.71 //",
  "SA14bsa026_u:181": "[pratipakṣasambhāraḥ] pratipakṣo 'ṣṭadhā jñeyo darśanābhyāsamārgayoḥ /",
  "SA14bsa026_u:182": "grāhyagrāhakavikalpānāmaṣṭānāmupaśāntaye // Asa_1.72 //",
  "SA14bsa026_u:183": "20 - niryāṇapratipattiḥ",
  "SA14bsa026_u:184": "uddeśe samatāyāṃ ca sattvārthe yatnavarjane /",
  "SA14bsa026_u:185": "atyantāya ca niryāṇaṃ niryāṇaṃ prāptilakṣaṇam // Asa_1.73 //",
  "SA14bsa026_u:186": "sarvākārajñatāyāṃ ca niryāṇaṃ mārgagocaram /",
  "SA14bsa026_u:187": "niryāṇapratipajjñeyā seyamaṣṭavidhātmikā // Asa_1.74 //",
  "SA14bsa026_u:188": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:189": "prajñāpāramitopadeśaśāstre prathamādhikāraḥ /",
  "SA14bsa026_u:190": "2 Mārgajñatādhikāraḥ dvitīyaḥ",
  "SA14bsa026_u:191": "1 - dhyāmīkaraṇatādīni",
  "SA14bsa026_u:192": "dhyāmīkaraṇatā bhābhirdevānāṃ yogyatāṃ prati /",
  "SA14bsa026_u:193": "viṣayo niyato vyāptiḥ svabhāvastasya karma ca // Asa_2.1 //",
  "SA14bsa026_u:194": "2 - śrāvakamārgaḥ",
  "SA14bsa026_u:195": "caturṇāmāryasatyānāmākārānupalambhataḥ /",
  "SA14bsa026_u:196": "śrāvakāṇāmayaṃ mārgo jñeyo mārgajñatānaye // Asa_2.2 //",
  "SA14bsa026_u:197": "rūpādiskandhaśūnyatvācchūnyatānāmabhedataḥ /",
  "SA14bsa026_u:198": "ūsmāṇau 'nupalambhena teṣāṃ mūrdhagataṃ matam // Asa_2.3 //",
  "SA14bsa026_u:199": "kṣāntayasteṣu nityādiyogasthānaniṣedhataḥ /",
  "SA14bsa026_u:200": "daśa bhūmīḥ samārabhya vistarāsthānadeśanāt // Asa_2.4 //",
  "SA14bsa026_u:201": "agradharmagataṃ proktamāryaśrāvakartmani /",
  "SA14bsa026_u:202": "tatkasya hetorbuddhena buddhvā dharmāsamīkṣaṇāt // Asa_2.5 //",
  "SA14bsa026_u:203": "3 - pratyekabuddhamārgaḥ",
  "SA14bsa026_u:204": "paropadeśavaiyarthyaṃ svayambodhāt svayambhuvām /",
  "SA14bsa026_u:205": "gambhīratā ca jñānasya khaḍgānāmabhidhīyate // Asa_2.6 //",
  "SA14bsa026_u:206": "śuśrūṣā yasyā yasyārthe yatra yatra yathā yathā /",
  "SA14bsa026_u:207": "sa so 'rthaḥ khyātyaśabdo 'pi tasya tasya tathā tathā // Asa_2.7 //",
  "SA14bsa026_u:208": "grāhyārthakalpanāhānād grāhakasyāprahāṇataḥ /",
  "SA14bsa026_u:209": "ādhārataśca vijñeyaḥ khaḍgamārgasya saṃgrahaḥ // Asa_2.8 //",
  "SA14bsa026_u:210": "prajñapteravirodhena dharmatāsūcanākṛtiḥ /",
  "SA14bsa026_u:211": "ūṣmagaṃ mūrdhagaṃ rūpādyahānādiprabhāvitam // Asa_2.9 //",
  "SA14bsa026_u:212": "adhyātmaśūnyatādyābhī rūpāderaparigrāhāt /",
  "SA14bsa026_u:213": "kṣāntī rūpādyanutpādādyākārairagradharmatā // Asa_2.10 //",
  "SA14bsa026_u:214": "4 - bodhisattvamārgaḥ",
  "SA14bsa026_u:215": "kṣāntijñānakṣaṇaiḥ satyaṃ satyaṃ prati caturvidhaiḥ /",
  "SA14bsa026_u:216": "mārgajñatāyāṃ dṛṅmārgaḥ sānuśaṃso 'yamucyate // Asa_2.11 //",
  "SA14bsa026_u:217": "ādhārādheyatābhāvāttathatābuddhayormithaḥ /",
  "SA14bsa026_u:218": "paryāyeṇānanujñānaṃ mahattā sāpramāṇatā // Asa_2.12 //",
  "SA14bsa026_u:219": "parimāṇāntatābhāvo rūpāderavadhāraṇam /",
  "SA14bsa026_u:220": "tasyāṃ sthitasya buddhatve 'nudgrahātyāgatādayaḥ // Asa_2.13 //",
  "SA14bsa026_u:221": "maitryādi śūnyatā prāptirbuddhatvasya parigrahaḥ /",
  "SA14bsa026_u:222": "sarvasya vyavadānasya sarvādhivyādhiśātanam // Asa_2.14 //",
  "SA14bsa026_u:223": "nirvāṇagrāhaśāntatvaṃ buddhebhyo rakṣaṇādikam /",
  "SA14bsa026_u:224": "aprāṇivadhamārabhya sarvākārajñatānaye // Asa_2.15 //",
  "SA14bsa026_u:225": "svayaṃ sthitasya sattvānāṃ sthāpanaṃ pariṇāmanam /",
  "SA14bsa026_u:226": "dānādīnāṃ ca sambodhāviti mārgajñatākṣaṇāḥ // Asa_2.16 //",
  "SA14bsa026_u:227": "5 - bhāvanāmārgakāritram",
  "SA14bsa026_u:228": "sarvato damanaṃ nāma sarvataḥ kleśanirjayaḥ /",
  "SA14bsa026_u:229": "upakramāviṣahyatvaṃ bodhirādhārapūjyatā // Asa_2.17 //",
  "SA14bsa026_u:230": "sāsravo bhāvanāmārgaḥ",
  "SA14bsa026_u:231": "6 - bhāvanāmārgādhimuktiḥ",
  "SA14bsa026_u:232": "adhimuktistridhā jñeyā svārthā ca svaparārthikā /",
  "SA14bsa026_u:233": "parārthikaivetyeṣā ca pratyekaṃ trividheṣyate // Asa_2.18 //",
  "SA14bsa026_u:234": "mṛdvī madhyādhimātrā ca mṛdumṛdvādibhedataḥ /",
  "SA14bsa026_u:235": "sā punastribidhetyevaṃ saptaviṃśatidhā matā // Asa_2.19 //",
  "SA14bsa026_u:236": "7 - bhāvanāmārgādhimuktasya stutiḥ stobhaḥ praśaṃsā ca",
  "SA14bsa026_u:237": "stutiḥ stobhaḥ praśaṃsā ca prajñāpāramitāṃ prati /",
  "SA14bsa026_u:238": "adhimokṣasya mātrāṇāṃ navakaistribhiriṣyate // Asa_2.20 //",
  "SA14bsa026_u:239": "8 - pariṇāmanā",
  "SA14bsa026_u:240": "viśeṣaḥ pariṇāmastu tasya kāritramuttamam /",
  "SA14bsa026_u:241": "nopalambhākṛtiścāsāvaviparyāsalakṣaṇaḥ // Asa_2.21 //",
  "SA14bsa026_u:242": "vivikto buddhapuṇyaughasvabhāvasmṛtigocaraḥ /",
  "SA14bsa026_u:243": "sopāyaścānimittaśca buddhairabhyanumoditaḥ // Asa_2.22 //",
  "SA14bsa026_u:244": "traidhātukāprapannaśca pariṇāmo 'parastridhā /",
  "SA14bsa026_u:245": "mṛdurmadhyo 'dhimātraśca mahāpuṇyodayātmakaḥ // Asa_2.23 //",
  "SA14bsa026_u:246": "9 - anumodanā",
  "SA14bsa026_u:247": "upāyānupalambhābhyāṃ śubhamūlānumodanā /",
  "SA14bsa026_u:248": "anumode manaskārabhāvaneha vidhīyate // Asa_2.24 //",
  "SA14bsa026_u:249": "anāsravo bhāvanāmārgaḥ",
  "SA14bsa026_u:250": "10 - abhinirhāraḥ",
  "SA14bsa026_u:251": "svabhāvaḥ śreṣṭhatā tasya sarvasyānabhisaṃskṛtiḥ /",
  "SA14bsa026_u:252": "nopalambhena dharmāṇāmarpaṇā ca mahārthatā // Asa_2.25 //",
  "SA14bsa026_u:253": "11 - atyantaviśuddhiḥ",
  "SA14bsa026_u:254": "buddhasevā ca dānādirupāye yacca kauśalam /",
  "SA14bsa026_u:255": "hetavo 'trādhimokṣasya dharmavyasanahetavaḥ // Asa_2.26 //",
  "SA14bsa026_u:256": "mārādhiṣṭhānagambhīradharmatānadhimuktate /",
  "SA14bsa026_u:257": "skandhādyabhiniveśaśca pāpamitraparigrahaḥ // Asa_2.27 //",
  "SA14bsa026_u:258": "phalaśuddhiśca rūpādiśuddhireva tayordvayoḥ /",
  "SA14bsa026_u:259": "abhinnācchinnatā yasmāditi śuddhirudīritā // Asa_2.28 //",
  "SA14bsa026_u:260": "kleśajñeyatrimārgasya śiṣyakhaḍgajinaurasām /",
  "SA14bsa026_u:261": "hānād viśuddhirātyantikī tu buddhasya sarvathā // Asa_2.29 //",
  "SA14bsa026_u:262": "mṛdumṛdvādiko mārgaḥ śuddhirnavasu bhūmiṣu /",
  "SA14bsa026_u:263": "adhimātrādhimātrādermalasya pratipakṣataḥ // Asa_2.30 //",
  "SA14bsa026_u:264": "tridhātupratipakṣatvaṃ samatā mānameyayoḥ /",
  "SA14bsa026_u:265": "mārgasya ceṣyate tasya codyasya parihārataḥ // Asa_2.31 //",
  "SA14bsa026_u:266": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:267": "prajñāpāramitopadeśaśāstre dvitīyādhikāraḥ /",
  "SA14bsa026_u:268": "3 Sarvajñatādhikāraḥ tṛtīyaḥ",
  "SA14bsa026_u:269": "1 - prajñayā na bhave sthānam",
  "SA14bsa026_u:270": "2 - kṛpayā na śame sthitiḥ",
  "SA14bsa026_u:271": "nāpare na pare tīre nāntarāle tayoḥ sthitā /",
  "SA14bsa026_u:272": "adhvanāṃ samatājñānāt prajñāpāramitā matā // Asa_3.1 //",
  "SA14bsa026_u:273": "3 - anupāyena dūratvam",
  "SA14bsa026_u:274": "4 - upāyenāvidūratā",
  "SA14bsa026_u:275": "anupāyena dūraṃ sā sanimittopalambhataḥ /",
  "SA14bsa026_u:276": "upāyakauśalenāsyāḥ samyagāsannatoditā // Asa_3.2 //",
  "SA14bsa026_u:277": "5 - vipakṣaḥ",
  "SA14bsa026_u:278": "rupādiskandhaśūnyatve dharmeṣu tryadhvageṣu ca /",
  "SA14bsa026_u:279": "dānādau bodhipakṣeṣu caryāsaṃjñā vipakṣatā // Asa_3.3 //",
  "SA14bsa026_u:280": "6 - pratipakṣaḥ",
  "SA14bsa026_u:281": "dānādiṣvanahaṃkāraḥ pareṣāṃ tanniyojanam /",
  "SA14bsa026_u:282": "saṅgakoṭiniṣedho 'yaṃ sūkṣmaḥ saṅgo jinādiṣu // Asa_3.4 //",
  "SA14bsa026_u:283": "tadgāmbhīrya prakṛtyaiva vivekād dharmapaddhateḥ /",
  "SA14bsa026_u:284": "ekaprakṛtikaṃ jñānaṃ dharmāṇāṃ saṅgavarjanam // Asa_3.5 //",
  "SA14bsa026_u:285": "dṛṣṭādipratiṣedhena tasyā durbodhatoditā /",
  "SA14bsa026_u:286": "rūpādibhiravijñānāt tadacintyatvamiṣyate // Asa_3.6 //",
  "SA14bsa026_u:287": "evaṃ kṛtvā yathokto vai jñeyaḥ sarvajñatānaye /",
  "SA14bsa026_u:288": "ayaṃ vibhāgo niḥśeṣo vipakṣapratipakṣayoḥ // Asa_3.7 //",
  "SA14bsa026_u:289": "7 - prayogaḥ",
  "SA14bsa026_u:290": "rupādau tadanityādau tadapūriprapūrayoḥ /",
  "SA14bsa026_u:291": "tadasaṅgatve caryāyāḥ prayogaḥ pratiṣedhataḥ // Asa_3.8 //",
  "SA14bsa026_u:292": "avikāro na karttā ca prayogo duṣkarastridhā /",
  "SA14bsa026_u:293": "yathābhavya phalaprāpteravandhyo 'bhimataśca saḥ // Asa_3.9 //",
  "SA14bsa026_u:294": "aparapratyayo yaśca saptadhā khyātivedakaḥ /",
  "SA14bsa026_u:295": "8 - samatā",
  "SA14bsa026_u:296": "caturdhāmananā tasya rūpādau samatā matā // Asa_3.10 //",
  "SA14bsa026_u:297": "9 - darśanamārgaḥ",
  "SA14bsa026_u:298": "dharmajñānānvayajñānakṣāntijñānakṣaṇātmakaḥ /",
  "SA14bsa026_u:299": "duḥkhādisatye dṛṅmārga eṣa sarvajñatānaya // Asa_3.11 //",
  "SA14bsa026_u:300": "rūpaṃ na nityaṃ nānityamatītāntaṃ viśuddhakam /",
  "SA14bsa026_u:301": "anutpannāniruddhādi vyomābhaṃ lepavarjitam // Asa_3.12 //",
  "SA14bsa026_u:302": "parigraheṇa nirmuktamavyāhāraṃ svabhāvataḥ /",
  "SA14bsa026_u:303": "pravyāhāreṇa nāsyārthaḥ pareṣu prāpyate yataḥ // Asa_3.13 //",
  "SA14bsa026_u:304": "nopalambhakṛdatyantaviśuddhirvyādhyasambhavaḥ /",
  "SA14bsa026_u:305": "apāyocchittyakalpatve phalasākṣātkriyāṃ prati // Asa_3.14 //",
  "SA14bsa026_u:306": "asaṃsargo nimittaiśca vastuni vyañjane dvaye /",
  "SA14bsa026_u:307": "jñānasya yā cānutpattiriti sarvajñatākṣaṇāḥ // Asa_3.15 //",
  "SA14bsa026_u:308": "iti seyaṃ punaḥ seyaṃ seyaṃ khalu punastridhā /",
  "SA14bsa026_u:309": "adhikāratrayasyaiṣā samāptiḥ paridīpitā // Asa_3.16 //",
  "SA14bsa026_u:310": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre tṛtīyādhikāraḥ /",
  "SA14bsa026_u:311": "4 Sarvākārābhisambodhādhikāraḥ caturthaḥ",
  "SA14bsa026_u:312": "1 - ākārāḥ",
  "SA14bsa026_u:313": "vastujñānaprakārāṇāmākārā iti lakṣaṇam /",
  "SA14bsa026_u:314": "sarvajñatānāṃ traividhyāt trividhā eva te matāḥ // Asa_4.1 //",
  "SA14bsa026_u:315": "asadākāramārabhya yāvanniścalatākṛtiḥ /",
  "SA14bsa026_u:316": "catvāraḥ pratisatyaṃ te mārge pañcadaśa smṛtāḥ // Asa_4.2 //",
  "SA14bsa026_u:317": "hetau mārge ca duḥkhe ca nirodhe ca yathākramam /",
  "SA14bsa026_u:318": "aṣṭau te sapta pañceti ṣoḍaśeti ca kīrtitāḥ // Asa_4.3 //",
  "SA14bsa026_u:319": "smṛtyupasthānamārabhya buddhatvākārapaścimāḥ /",
  "SA14bsa026_u:320": "śiṣyāṇāṃ bodhisattvānāṃ buddhānāṃ ca yathākramam // Asa_4.4 //",
  "SA14bsa026_u:321": "saptatriṃśaccatustriṃśattriṃśannava ca te matāḥ /",
  "SA14bsa026_u:322": "trisarvajñatvabhedena mārgasatyānurodhataḥ // Asa_4.5 //",
  "SA14bsa026_u:323": "kṛtādhikārā buddheṣu teṣūptaśubhamūlakāḥ /",
  "SA14bsa026_u:324": "mitraiḥ sanāthāḥ kalyāṇairasyāḥ śravaṇabhājanam // Asa_4.6 //",
  "SA14bsa026_u:325": "buddhopāsanasampraśnadānaśīlādicaryayā /",
  "SA14bsa026_u:326": "udgrahadhāraṇādīnāṃ bhājanatvaṃ satāṃ matam // Asa_4.7 //",
  "SA14bsa026_u:327": "2 - prayogāḥ",
  "SA14bsa026_u:328": "rūpādiṣvanavasthānāt teṣu yoganiṣedhataḥ /",
  "SA14bsa026_u:329": "tattathatāgambhīratvāt teṣāṃ duravagāhataḥ // Asa_4.8 //",
  "SA14bsa026_u:330": "tadaprāmāṇyataḥ kṛcchrāccireṇa pratibodhataḥ /",
  "SA14bsa026_u:331": "vyākṛtāvavivartyatve niryāṇe sanirantare // Asa_4.9 //",
  "SA14bsa026_u:332": "āsannabodhe kṣipraṃ ca parārthe 'vṛddhayahānitaḥ /",
  "SA14bsa026_u:333": "dharmādharmādyadṛṣṭau ca rūpācintyādyadarśane // Asa_4.10 //",
  "SA14bsa026_u:334": "rūpādestannimittasya tadbhāvasyāvikalpakaḥ /",
  "SA14bsa026_u:335": "phalaratnapradātā ca śuddhakaḥ sāvadhiśca saḥ // Asa_4.11 //",
  "SA14bsa026_u:336": "3 - guṇāḥ",
  "SA14bsa026_u:337": "mārāṇāṃ śaktihānyādiścaturdaśavidho guṇaḥ /",
  "SA14bsa026_u:338": "4 - doṣāḥ",
  "SA14bsa026_u:339": "doṣāśca ṣaḍ viboddhavyāścaturbhirdaśakaiḥ saha // Asa_4.12 //",
  "SA14bsa026_u:340": "5 - lakṣaṇāni",
  "SA14bsa026_u:341": "lakṣyate yena tajjñeyaṃ lakṣaṇaṃ trividhaṃ ca tat /",
  "SA14bsa026_u:342": "jñānaṃ viśeṣaḥ kāritraṃ svabhāvo yaśca lakṣyate // Asa_4.13 //",
  "SA14bsa026_u:343": "tathāgatasya nirvṛttau loke cālujyanātmake /",
  "SA14bsa026_u:344": "sattvānāṃ cittacaryāsu tatsaṃkṣepe bahirgatau // Asa_4.14 //",
  "SA14bsa026_u:345": "akṣayākāratāyāṃ ca sarāgādau pravistṛte /",
  "SA14bsa026_u:346": "mahadgate 'pramāṇe ca vijñāne cānidarśane // Asa_4.15 //",
  "SA14bsa026_u:347": "adṛśyacittajñāne ca tadunmiñjādisaṃjñakam /",
  "SA14bsa026_u:348": "punastathatākāreṇa teṣāṃ jñānamataḥ param // Asa_4.16 //",
  "SA14bsa026_u:349": "tathatāyāṃ munerbodhatatparākhyānamityayam /",
  "SA14bsa026_u:350": "sarvajñatādhikāreṇa jñānalakṣaṇasaṃgrahaḥ // Asa_4.17 //",
  "SA14bsa026_u:351": "śūnyatve sānimitte ca praṇīdhānavivarjite /",
  "SA14bsa026_u:352": "anutpādānirodhādau dharmatāyā akopane // Asa_4.18 //",
  "SA14bsa026_u:353": "asaṃskāre 'vikalpe ca prabhedālakṣaṇatvayoḥ /",
  "SA14bsa026_u:354": "mārgajñatādhikāreṇa jñānalakṣaṇamiṣyate // Asa_4.19 //",
  "SA14bsa026_u:355": "svadharmamupaniśritya vihāre tasya satkṛtau /",
  "SA14bsa026_u:356": "gurutve mānanāyāṃ ca tatpūjākṛtakatvayoḥ // Asa_4.20 //",
  "SA14bsa026_u:357": "sarvatra vṛttimajjñānamadṛṣṭasya ca darśakam /",
  "SA14bsa026_u:358": "lokasya śūnyatākāraśūcakajñāpakākṣagam // Asa_4.21 //",
  "SA14bsa026_u:359": "acintyaśāntatādarśi lokasaṃjñanirodhi ca /",
  "SA14bsa026_u:360": "jñānalakṣaṇamityuktaṃ sarvākārajñatānaye // Asa_4.22 //",
  "SA14bsa026_u:361": "acintyādiviśeṣeṇa viśiṣṭaiḥ satyagocaraiḥ /",
  "SA14bsa026_u:362": "viśeṣalakṣaṇaṃ ṣaḍbhirdaśabhiścoditaṃ kṣaṇaiḥ // Asa_4.23 //",
  "SA14bsa026_u:363": "acintyātulyate meyasaṃkhyayoḥ samatikramau /",
  "SA14bsa026_u:364": "sarvāryasaṃgraho vijñavedyāsādhāraṇajñate // Asa_4.24 //",
  "SA14bsa026_u:365": "kṣiprajñānyūnapūrṇatve pratipatsamudāgamau /",
  "SA14bsa026_u:366": "ālambanaṃ ca sādhāraṃ sākalyaṃ samparigrahaḥ // Asa_4.25 //",
  "SA14bsa026_u:367": "anāsvādaśca vijñeyo viśeṣaḥ ṣoḍaśātmakaḥ /",
  "SA14bsa026_u:368": "viśeṣamārgo mārgebhyo yenānyebhyo viśiṣyate // Asa_4.26 //",
  "SA14bsa026_u:369": "hitaṃ sukhaṃ ca trāṇaṃ ca śaraṇaṃ layanaṃ nṛṇām /",
  "SA14bsa026_u:370": "parāyaṇaṃ ca dvīpaṃ ca pariṇāyakasaṃjñakam // Asa_4.27 //",
  "SA14bsa026_u:371": "anābhogaṃ tribhiryānaiḥ phalāsākṣātkriyātmakam /",
  "SA14bsa026_u:372": "paścimaṃ gatikāritramidaṃ kāritralakṣaṇam // Asa_4.28 //",
  "SA14bsa026_u:373": "kleśaliṅganimittānāṃ vipakṣapratipakṣayoḥ /",
  "SA14bsa026_u:374": "viveko duṣkaraikāntāvuddeśo 'nupalambhakaḥ // Asa_4.29 //",
  "SA14bsa026_u:375": "niṣiddhābhiniveśaśca yaścālambanasaṃjñakaḥ /",
  "SA14bsa026_u:376": "vipratyayo 'vighātī ca so 'padāgatyajātikaḥ // Asa_4.30 //",
  "SA14bsa026_u:377": "tathatānupalambhaśca svabhāvaḥ ṣoḍaśātmakaḥ /",
  "SA14bsa026_u:378": "lakṣīva lakṣyate ceti caturthaṃ lakṣaṇaṃ matam // Asa_4.31 //",
  "SA14bsa026_u:379": "6 - mokṣabhāgīyam",
  "SA14bsa026_u:380": "animittapradānādisamudāgamakauśalam /",
  "SA14bsa026_u:381": "sarvākārāvabodhe 'smin mokṣabhāgīyamiṣyate // Asa_4.32 //",
  "SA14bsa026_u:382": "buddhādyālambanā śraddhā vīryaṃ dānādigocaram /",
  "SA14bsa026_u:383": "smṛtirāśayasampattiḥ samādhiravikalpanā // Asa_4.33 //",
  "SA14bsa026_u:384": "dharmeṣu sarvairākārairjñānaṃ prajñeti pañcadhā /",
  "SA14bsa026_u:385": "tīkṣṇaiḥ subodhā sambodhirdurbodhā mṛdubhirmatā // Asa_4.34 //",
  "SA14bsa026_u:386": "7 - virvedhabhāgīyam",
  "SA14bsa026_u:387": "ālambanaṃ sarvasattvā ūṣmaṇāmiha śasyate /",
  "SA14bsa026_u:388": "samacittādirākārasteṣveva daśadhoditaḥ // Asa_4.35 //",
  "SA14bsa026_u:389": "svayaṃ pāpānnivṛttasya dānādyeṣu sthitasya ca /",
  "SA14bsa026_u:390": "tayorniyojanānyeṣāṃ varṇavādānukūlate // Asa_4.36 //",
  "SA14bsa026_u:391": "mūrdhagaṃ svaparādhāraṃ satyajñānaṃ tathā kṣamā /",
  "SA14bsa026_u:392": "tathāgradharmā vijñeyāḥ sattvānāṃ pācanādibhiḥ // Asa_4.37 //",
  "SA14bsa026_u:393": "8 - avaivartiko gaṇaḥ",
  "SA14bsa026_u:394": "nirvedhāṅgānyupādāya darśanābhyāsamārgayoḥ /",
  "SA14bsa026_u:395": "ye bodhisattvā vartante so 'trāvaivartiko gaṇaḥ // Asa_4.38 //",
  "SA14bsa026_u:396": "rūpādibhyo nivṛttyādyairliṅgairviśatidheritaiḥ /",
  "SA14bsa026_u:397": "nirvedhāṅgaśitasyedamakvairtikalakṣaṇam // Asa_4.39 //",
  "SA14bsa026_u:398": "rūpādibhyo nivṛttiśca vicikitsākṣaṇakṣayo /",
  "SA14bsa026_u:399": "ātmanaḥ kuśalasthasya pareṣāṃ tanniyojanam // Asa_4.40 //",
  "SA14bsa026_u:400": "parādhāraṃ ca dānādi gambhīre 'rthe 'pyakāṃkṣaṇam /",
  "SA14bsa026_u:401": "maitraṃ kāyādyasaṃvāsaḥ paṃcadhāvaraṇena ca // Asa_4.41 //",
  "SA14bsa026_u:402": "sarvānuśayahānaṃ ca smṛtisamprajñatā śuci /",
  "SA14bsa026_u:403": "cīvarādi śarīre ca kṛmīṇāmasamudbhavaḥ // Asa_4.42 //",
  "SA14bsa026_u:404": "cittākauṭilyamādānaṃ dhūtasyāmatsarāditā /",
  "SA14bsa026_u:405": "dharmatāyuktagāmitvaṃ lokārthaṃ narakaiṣaṇā // Asa_4.43 //",
  "SA14bsa026_u:406": "parairaneyatā mārasyānyamārgopadeśinaḥ /",
  "SA14bsa026_u:407": "māra ityeva bodhaśca caryā buddhānumoditā // Asa_4.44 //",
  "SA14bsa026_u:408": "ūṣmamūrdhasu sakṣāntiṣvagradharmeṣvavasthitaḥ /",
  "SA14bsa026_u:409": "liṅgairamībhirviśatyā sambodherna nivartatte // Asa_4.45 //",
  "SA14bsa026_u:410": "kṣāntijñānakṣaṇāḥ ṣaṭ ca pañca pañca ca dṛkpathe /",
  "SA14bsa026_u:411": "bodhisattvasya vijñeyamavaivartikalakṣaṇam // Asa_4.46 //",
  "SA14bsa026_u:412": "rūpādisaṃjñāvyāvṛttirdāḍharyaṃ cittasya hīnayoḥ /",
  "SA14bsa026_u:413": "yānayorvinivṛttiśca dhyānādyaṅgaparikṣayaḥ // Asa_4.47 //",
  "SA14bsa026_u:414": "kāyacetolaghutvaṃ ca kāmasevābhyupāyikī /",
  "SA14bsa026_u:415": "sadaiva brahmacāritvamājīvasya viśuddhatā // Asa_4.48 //",
  "SA14bsa026_u:416": "skandhādāvantarāyeṣu sambhāre sendriyādike /",
  "SA14bsa026_u:417": "samare matsarādau ca neti yogānuyogayoḥ // Asa_4.49 //",
  "SA14bsa026_u:418": "vihārapratiṣedhaśca dharmasyāṇoralabdhatā /",
  "SA14bsa026_u:419": "niścitattvaṃ svabhūmau ca bhūmitritayasaṃsthitiḥ // Asa_4.50 //",
  "SA14bsa026_u:420": "dharmārthaḥ jīvitatyāga ityamī ṣoḍaśa kṣaṇāḥ /",
  "SA14bsa026_u:421": "avaivartikaliṅgāni dṛṅmārgasthasya dhīmataḥ // Asa_4.51 //",
  "SA14bsa026_u:422": "gambhīro bhāvanāmārgo gāmbhīryaṃ śūnyatādikam /",
  "SA14bsa026_u:423": "samāropāpavādāntamuktatā sā gabhīratā // Asa_4.52 //",
  "SA14bsa026_u:424": "cintātulananidhyānānyabhīkṣṇaṃ bhāvanāpathaḥ /",
  "SA14bsa026_u:425": "nirvedhāṅgeṣu dṛṅmārge bhāvanāmārga eva ca // Asa_4.53 //",
  "SA14bsa026_u:426": "prābandhikatvādiṣṭo 'sau navadhā ca prakārataḥ /",
  "SA14bsa026_u:427": "mṛdumadhyādhimātrāṇāṃ punarmṛddhādibhedataḥ // Asa_4.54 //",
  "SA14bsa026_u:428": "asaṃkhyeyādinirdeśāḥ paramārthena na kṣamāḥ /",
  "SA14bsa026_u:429": "kṛpāniṣyandabhūtāste saṃvṛtyābhimatā muneḥ // Asa_4.55 //",
  "SA14bsa026_u:430": "hānivṛddhī na yujyete nirālāpasya vastunaḥ /",
  "SA14bsa026_u:431": "bhāvanākhyena kiṃ hīnaṃ vartmanā kimupadāgatam // Asa_4.56 //",
  "SA14bsa026_u:432": "yathā bodhistathaivāsāviṣṭasyārthasya sādhakaḥ /",
  "SA14bsa026_u:433": "tathatālakṣaṇā bodhiḥ so 'pi tallakṣaṇo mataḥ // Asa_4.57 //",
  "SA14bsa026_u:434": "pūrveṇa bodhirno yuktā manasā paścimena vā /",
  "SA14bsa026_u:435": "dīpadṛṣṭāntayogena gambhīrā dharmatāṣṭadhā // Asa_4.58 //",
  "SA14bsa026_u:436": "utpāde ca nirodhe ca tathatāyāṃ gabhīratā /",
  "SA14bsa026_u:437": "jñeye jñāne ca caryāyāmadvayopāyakauśale // Asa_4.59 //",
  "SA14bsa026_u:438": "9 - bhavaśāntyoḥ samatā",
  "SA14bsa026_u:439": "svapnopamatvād dharmāṇāṃ bhavaśāntyorakalpanā /",
  "SA14bsa026_u:440": "karmābhāvādicodyānāṃ parihārā yathoditāḥ // Asa_4.60 //",
  "SA14bsa026_u:441": "10 - anuttarā kṣetraśuddhiḥ",
  "SA14bsa026_u:442": "sattvalokasya yāśuddhistasyāḥ śuddhyupahārataḥ /",
  "SA14bsa026_u:443": "tathā bhājanalokasya buddhakṣetrasya śuddhatā // Asa_4.61 //",
  "SA14bsa026_u:444": "11 - upāyakauśalam",
  "SA14bsa026_u:445": "viṣayo 'sya prayogasya śātravāṇāmatikramaḥ /",
  "SA14bsa026_u:446": "apratiṣṭho yathāvedhamasādhāraṇalakṣaṇaḥ // Asa_4.62 //",
  "SA14bsa026_u:447": "asakto 'nupalambhaśca nimittapranidhikṣataḥ /",
  "SA14bsa026_u:448": "talliṅgaṃ cāpramāṇaṃ ca daśadhopāyakauśalam // Asa_4.63 //",
  "SA14bsa026_u:449": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre caturthādhikāraḥ //",
  "SA14bsa026_u:450": "Mūrdhābhisamayādhikāraḥ pañcamaḥ",
  "SA14bsa026_u:451": "1 - liṅgama(ūṣmā murdhaprayogaḥ)",
  "SA14bsa026_u:452": "svapnāntare 'pi svapnābhasarvadharmekṣaṇādikam /",
  "SA14bsa026_u:453": "mūrdhaprāptasya yogasya liṅgaṃ dvādaśadhā matam // Asa_5.1 //",
  "SA14bsa026_u:454": "2 - vivṛddhiḥ(mūrdhā mūrdhaprayogaḥ)",
  "SA14bsa026_u:455": "jambudvipajaneyattābuddhapūjāśubhādikām /",
  "SA14bsa026_u:456": "upamāṃ bahudhā kṛtvā vivṛddhiḥ ṣoḍaśātmikā // Asa_5.2 //",
  "SA14bsa026_u:457": "3 - nirudhiḥ(kṣāntiḥ mūrdhaprayogaḥ)",
  "SA14bsa026_u:458": "trisarvajñatvadharmāṇāṃ paripuriranuttarā /",
  "SA14bsa026_u:459": "aparityaktasattvārthā nirūḍhirabhidhīyate // Asa_5.3 //",
  "SA14bsa026_u:460": "4 - cittasaṃsthitiḥ(agradharmākhyaḥ mūrdhaprayogaḥ)",
  "SA14bsa026_u:461": "caturdvīpakasāhasradvitrisāhasrakopamaḥ /",
  "SA14bsa026_u:462": "kṛtvā puṇyabahutvena samādhiḥ parikīrtitaḥ // Asa_5.4 //",
  "SA14bsa026_u:463": "5 - darśanamārgaḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:464": "pravṛttau ca nivṛttau ca pratyekaṃ tau navātmakau /",
  "SA14bsa026_u:465": "grāhyau vikalpau vijñeyāvayathāviṣayātmakau // Asa_5.5 //",
  "SA14bsa026_u:466": "dravyaprajñaptisatsattvavikalpau grāhakau matau /",
  "SA14bsa026_u:467": "pṛthagjanāryabhedena pratyekaṃ tau navātmakau // Asa_5.6 //",
  "SA14bsa026_u:468": "grāhyau cenna tathā sto 'rthau kasya tau grāhakau matau /",
  "SA14bsa026_u:469": "iti grāhakabhāvena śūnyatālakṣaṇaṃ tayoḥ // Asa_5.7 //",
  "SA14bsa026_u:470": "eṣa svabhāve gotre ca pratipatsamudāgame /",
  "SA14bsa026_u:471": "jñānasyālambanābhrāntau pratipakṣavipakṣayoḥ // Asa_5.8 //",
  "SA14bsa026_u:472": "svasminnadhigame kartṛtatkāritrakriyāphale /",
  "SA14bsa026_u:473": "pravṛttipakṣādhiṣṭhāno vikalpo navadhā mataḥ // Asa_5.9 //",
  "SA14bsa026_u:474": "bhavaśāntiprapātitvānnyūnatve 'dhigamasya ca /",
  "SA14bsa026_u:475": "parigrahasyābhāve ca vaikalye pratipadgate // Asa_5.10 //",
  "SA14bsa026_u:476": "parapratyagāmitve samuddeśanivartane /",
  "SA14bsa026_u:477": "prādeśikatve nānātve sthānaprasthānamohayoḥ // Asa_5.11 //",
  "SA14bsa026_u:478": "pṛṣṭhato gamane ceti vikalpo 'yaṃ navātmakaḥ /",
  "SA14bsa026_u:479": "nivṛttipakṣādhiṣṭhānaḥ śrāvakādimanobhavaḥ // Asa_5.12 //",
  "SA14bsa026_u:480": "grāhakaḥ prathamo jñeyo grahaṇapratimokṣaṇe /",
  "SA14bsa026_u:481": "manaskriyāyāṃ dhātūnāmupaśleṣe trayasya ca // Asa_5.13 //",
  "SA14bsa026_u:482": "sthāne cābhiniveśe ca prajñaptau dharmavastunaḥ /",
  "SA14bsa026_u:483": "saktau ca pratipakṣe ca yathecchaṃ ca gatikṣatau // Asa_5.14 //",
  "SA14bsa026_u:484": "yathoddeśamaniryāṇe mārgāmārgāvadhāraṇe /",
  "SA14bsa026_u:485": "sanirodhe samutpāde vastuyogaviyogayoḥ // Asa_5.15 //",
  "SA14bsa026_u:486": "sthāne gotrasya nāśe ca prārthanāhetvabhāvayoḥ /",
  "SA14bsa026_u:487": "pratyarthikopalambhe ca vikalpo grāhako 'paraḥ // Asa_5.16 //",
  "SA14bsa026_u:488": "bodhau sandarśanānyeṣāṃ taddhetośca parīndanā /",
  "SA14bsa026_u:489": "tatprāptyanantaro hetuḥ puṇyabāhulyalakṣaṇaḥ // Asa_5.17 //",
  "SA14bsa026_u:490": "kṣayānutpādayorjñāne malānāṃ bodhirucyate /",
  "SA14bsa026_u:491": "kṣayābhāvādanutpādātte hi jñeye yathākramam // Asa_5.18 //",
  "SA14bsa026_u:492": "prakṛtāvaniruddhāyāṃ darśanākhyena vartmanā /",
  "SA14bsa026_u:493": "vikalpajātaṃ ki kṣīṇaṃ kiṃ vānatpattimāgatam // Asa_5.19 //",
  "SA14bsa026_u:494": "sattā ca nāma dharmāṇāṃ jñeye cāvaraṇakṣayaḥ /",
  "SA14bsa026_u:495": "kathyate yatparaiḥ śāsturatra vismīyate mayā // Asa_5.20 //",
  "SA14bsa026_u:496": "nāpaneyamataḥ kiñcit prakṣeptavyaṃ na kiṃcana /",
  "SA14bsa026_u:497": "draṣṭavyaṃ bhūtato bhūtaṃ bhūtadarśī vimucyate // Asa_5.21 //",
  "SA14bsa026_u:498": "ekaikasyaiva dānādau teṣāṃ yaḥ saṃgraho mithaḥ /",
  "SA14bsa026_u:499": "sa ekakṣaṇikaḥ kṣāntisaṃgṛhīto 'tra dṛkpathaḥ // Asa_5.22 //",
  "SA14bsa026_u:500": "sa samādhiṃ samāpadya tataḥ siṃhavijṛmbhitam /",
  "SA14bsa026_u:501": "anulomaṃ vilomaṃ ca pratītyotpādamīkṣate // Asa_5.23 //",
  "SA14bsa026_u:502": "6 - bhāvanāmārgaḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:503": "kāmāptamabadhīkṛtya vijñānamasamāhitam /",
  "SA14bsa026_u:504": "sanirodhāḥ samapattīrgattvāgamya nava dvidhā // Asa_5.24 //",
  "SA14bsa026_u:505": "ekadvitricatuḥpañcaṣaṭsaptāṣṭavyatikramāt /",
  "SA14bsa026_u:506": "avaskandasamāpattiranirodhamatulyatā // Asa_5.25 //",
  "SA14bsa026_u:507": "saṃkṣepe vistare buddhaiḥ sānāthyenāparigrahe /",
  "SA14bsa026_u:508": "traikālike guṇābhāve śreyasastrividhe pathi // Asa_5.26 //",
  "SA14bsa026_u:509": "eko grāhyavikalpo 'yaṃ prayogākāragocaraḥ /",
  "SA14bsa026_u:510": "dvitīyaścittacaittānāṃ pravṛttiviṣayo mataḥ // Asa_5.27 //",
  "SA14bsa026_u:511": "anutpādastu cittasya bodhimaṇḍāmanaskriyā /",
  "SA14bsa026_u:512": "hīnayānamanaskārau sambodheramanaskṛtiḥ // Asa_5.28 //",
  "SA14bsa026_u:513": "bhāvane 'bhāvane caiva tadviparyaya eva ca /",
  "SA14bsa026_u:514": "ayathārthaśca vijñeyo vikalpo bhāvanāpathe // Asa_5.29 //",
  "SA14bsa026_u:515": "grāhakaḥ prathamo jñeyaḥ sattvaprajñaptigocaraḥ /",
  "SA14bsa026_u:516": "dharmapratyaśūnyatvasaktipravicayātmakaḥ // Asa_5.30 //",
  "SA14bsa026_u:517": "kṛte ca vastuno yānatritaye ca sa kīrttitaḥ /",
  "SA14bsa026_u:518": "dakṣiṇāyā aśuddhau vā caryāyāśca vikopane // Asa_5.31 //",
  "SA14bsa026_u:519": "sattvaprajñaptitaddhetuviṣayo navadhāparaḥ /",
  "SA14bsa026_u:520": "bhāvanāmārgasambaddho vipakṣastad vighātataḥ // Asa_5.32 //",
  "SA14bsa026_u:521": "sarvajñatānāṃ tisṛṇāṃ yathāsvaṃ trividhāvṛttau /",
  "SA14bsa026_u:522": "śāntimārgatathatādisamprayogaviyogayoḥ // Asa_5.33 //",
  "SA14bsa026_u:523": "asamatve ca duḥkhādau kleśānāṃ prakṛtāvapi /",
  "SA14bsa026_u:524": "dvayābhāve ca saṃmohe vikalpaḥ paścimo mataḥ // Asa_5.34 //",
  "SA14bsa026_u:525": "āsāṃ kṣaye satītīnāṃ cirāyocchvasitā iva /",
  "SA14bsa026_u:526": "sarvākārajagatsaukhyasādhanā guṇasampadaḥ // Asa_5.35 //",
  "SA14bsa026_u:527": "sarvāḥ sarvābhisāreṇa nikāmaphalaśālinam /",
  "SA14bsa026_u:528": "bhajante taṃ mahāsattvaṃ mahodadhimivāpagāḥ // Asa_5.36 //",
  "SA14bsa026_u:529": "7 - ānantaryasamādhiḥ(mūrdhaprayogaḥ)",
  "SA14bsa026_u:530": "trisāhasrajanaṃ śiṣyakhaḍgādhigamasampadi /",
  "SA14bsa026_u:531": "bodhisattvasya ca nyāme pratiṣṭhāpya śubhopamāḥ // Asa_5.37 //",
  "SA14bsa026_u:532": "kṛtvā puṇyabahutvena buddhatvāpteranantaraḥ /",
  "SA14bsa026_u:533": "ānantaryasamādhiḥ sa sarvākārajñatā ca tat // Asa_5.38 //",
  "SA14bsa026_u:534": "ālambanamabhāvo 'sya smṛtiścādhipatirmataḥ /",
  "SA14bsa026_u:535": "ākāraḥ śāntatā cātra jalpājalpipravādinām // Asa_5.39 //",
  "SA14bsa026_u:536": "ālambanopapattau ca tatsvabhāvāvadhāraṇe /",
  "SA14bsa026_u:537": "sarvākarajñatājñāne paramārthe sasaṃvṛttau // Asa_5.40 //",
  "SA14bsa026_u:538": "prayoge triṣu ratneṣu sopāye samaye muneḥ /",
  "SA14bsa026_u:539": "viparyāse samārge ca pratipakṣavipakṣayoḥ // Asa_5.41 //",
  "SA14bsa026_u:540": "lakṣaṇe bhāvanāyāṃ ca matā vipratipattayaḥ /",
  "SA14bsa026_u:541": "sarvākārajñatādhārā ṣoḍhā daśa ca vādinām // Asa_5.42 //",
  "SA14bsa026_u:542": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre paṃcamādhikāraḥ /",
  "SA14bsa026_u:543": "Ānupūrvikādhikāraḥ ṣaṣṭhaḥ",
  "SA14bsa026_u:544": "dānena prajñayā yāvad buddhādau smṛtibhiśca sā /",
  "SA14bsa026_u:545": "dharmābhāvasvabhāvenetyanupūrvakriyā matā // Asa_6.1 //",
  "SA14bsa026_u:546": "ityabhisamāyālaṅkāre nāma prajñāpāramitopadeśaśāstre ṣaṣṭhādhikāraḥ /",
  "SA14bsa026_u:547": "Ekakṣaṇābhisambodhādhikāraḥ saptamaḥ",
  "SA14bsa026_u:548": "1 - avipākalalakṣaṇaḥ",
  "SA14bsa026_u:549": "anāsravāṇāṃ sarveṣāmekaikenāpi saṃgrahāt /",
  "SA14bsa026_u:550": "ekakṣaṇāvabodho 'yaṃ jñeyo dānādinā muneḥ // Asa_7.1 //",
  "SA14bsa026_u:551": "araghaṭṭaṃ yathaikāpi padikā puruṣeritā /",
  "SA14bsa026_u:552": "sakṛt sarvaṃ cālayati jñānamekakṣaṇe tathā // Asa_7.2 //",
  "SA14bsa026_u:553": "2 - vipākalakṣaṇaḥ",
  "SA14bsa026_u:554": "vipākadharmatāvasthā sarvaśuklamayī yadā /",
  "SA14bsa026_u:555": "prajñāpāramitā jātā jñānamekakṣaṇe tadā // Asa_7.3 //",
  "SA14bsa026_u:556": "3 - alakṣaṇalakṣaṇaḥ",
  "SA14bsa026_u:557": "svapnopameṣu dharmeṣu sthitvā dānādicaryayā /",
  "SA14bsa026_u:558": "alakṣaṇatvaṃ dharmāṇāṃ kṣaṇenaikena vindati // Asa_7.4 //",
  "SA14bsa026_u:559": "4 - advayalakṣaṇaḥ",
  "SA14bsa026_u:560": "svapnaṃ taddarśinaṃ caiva dvayayogena nekṣate /",
  "SA14bsa026_u:561": "dharmāṇāmadvayaṃ tattvaṃ kṣaṇenaikena paśyati // Asa_7.5 //",
  "SA14bsa026_u:562": "ityabhisamayālaṅkāre nāma",
  "SA14bsa026_u:563": "prajñāpāramitopadeśaśāstre saptamādhikāraḥ /",
  "SA14bsa026_u:564": "Dharmakāyādhikāraḥ aṣṭamaḥ",
  "SA14bsa026_u:565": "1 - svabhāvakāyaḥ",
  "SA14bsa026_u:566": "sarvākārāṃ viśuddhiṃ ye dharmāḥ prāptā nirāsravāḥ /",
  "SA14bsa026_u:567": "svābhāviko muneḥ kāyasteṣāṃ prakṛtilakṣaṇaḥ // Asa_8.1 //",
  "SA14bsa026_u:568": "2 - jñānadharmakāyaḥ",
  "SA14bsa026_u:569": "bodhipakṣāpramāṇāni vimokṣā anupūrvaśaḥ /",
  "SA14bsa026_u:570": "navātmikā samāpattiḥ kṛtsnaṃ daśavidhātmakam // Asa_8.2 //",
  "SA14bsa026_u:571": "abhibhvāyatanānyaṣṭaprakārāṇi prabhedataḥ /",
  "SA14bsa026_u:572": "araṇā praṇidhijñānamabhijñāḥ pratisaṃvidaḥ // Asa_8.3 //",
  "SA14bsa026_u:573": "sarvākārāścatastro 'tha śuddhayo vaśitā daśa /",
  "SA14bsa026_u:574": "balāni daśa catvāri vaiśāradyānyarakṣaṇam // Asa_8.4 //",
  "SA14bsa026_u:575": "trividhaṃ smutyupasthānaṃ tridhāsaṃmoṣadharmatā /",
  "SA14bsa026_u:576": "vāsanāyāḥ samudghāto mahatī karuṇā jane // Asa_8.5 //",
  "SA14bsa026_u:577": "āveṇikā munereva dharmā ye 'ṣṭādaśeritāḥ /",
  "SA14bsa026_u:578": "sarvākārajñatā ceti dharmakāyo 'bhidhīyate // Asa_8.6 //",
  "SA14bsa026_u:579": "śrāvakasyāraṇādṛṣṭernṛkleśaparihāritā /",
  "SA14bsa026_u:580": "tatkleśastrotaucchittyai grāmādiṣu jināraṇā // Asa_8.7 //",
  "SA14bsa026_u:581": "anābhogamanāsaṅgamavyāghātaṃ sadā sthitam /",
  "SA14bsa026_u:582": "sarvapraśnāpanud bauddhaṃ praṇidhijñānamiṣyate // Asa_8.8 //",
  "SA14bsa026_u:583": "paripākaṃ gate hetau yasya yasya yadā yadā /",
  "SA14bsa026_u:584": "hitaṃ bhavati karttavyaṃ prathate tasya tasya saḥ // Asa_8.9 //",
  "SA14bsa026_u:585": "varṣatyapi hi parjanye naiva bījaṃ prarohati /",
  "SA14bsa026_u:586": "samutpāde 'pi buddhānāṃ nābhavyo bhadramaśnute // Asa_8.10 //",
  "SA14bsa026_u:587": "iti kāritravaipulyād buddho vyāpī nirucyate /",
  "SA14bsa026_u:588": "akṣayatvācca tasyaiva nitya ityapi kathyate // Asa_8.11 //",
  "SA14bsa026_u:589": "3 - sambhogakāyaḥ",
  "SA14bsa026_u:590": "dvātriṃśallakṣaṇāśītivyañjanātmā munerayam /",
  "SA14bsa026_u:591": "sāmbhogiko mataḥ kāyo mahāyānopabhogataḥ // Asa_8.12 //",
  "SA14bsa026_u:592": "cakrāṅkahastaḥ kramakūrmapādo jālāvanaddhāṅgulipāṇipādaḥ /",
  "SA14bsa026_u:593": "karau sapādau taruṇau mṛdū ca samutsadai saptabhirāśrayo 'sya // Asa_8.13 //",
  "SA14bsa026_u:594": "dīrghāṅgulirvyāyatapārṣṇigātraṃ prājyaṃ tvṛjūcchaṅkhapadordhvaromā /",
  "SA14bsa026_u:595": "eṇeyajaṅghaśca paṭūrubāhuḥ kośāvanaddhottamabastiguhyaḥ // Asa_8.14 //",
  "SA14bsa026_u:596": "suvarṇavarṇaḥ pratanūcchaviśca pradakṣiṇaikaikasujātaromā /",
  "SA14bsa026_u:597": "ūrṇāṅkitāsyo haripūrvakāyaḥ skandhau vṛtāvasya citāntarāṃsaḥ // Asa_8.15 //",
  "SA14bsa026_u:598": "hīno rasaḥ khyāti rasottamo 'sya nyagrodhavanmaṇḍalatulyamūrtiḥ /",
  "SA14bsa026_u:599": "uṣṇīṣamūrdhā pṛthucārujivho brahmasvaraḥ siṃhahanuḥ suśuklāḥ // Asa_8.16 //",
  "SA14bsa026_u:600": "tulyāḥ pramāṇe 'viralāśca dantā anyūnasaṃkhyā daśikāścatasraḥ /",
  "SA14bsa026_u:601": "nīlekṣaṇo govṛṣapakṣmanetro dvātriṃśadetāni hi lakṣaṇāni // Asa_8.17 //",
  "SA14bsa026_u:602": "yasya yasyātra yo heturlakṣaṇasya prasādhakaḥ /",
  "SA14bsa026_u:603": "tasya tasya prapūryāyaṃ samudāgamalakṣaṇaḥ // Asa_8.18 //",
  "SA14bsa026_u:604": "gurūṇāmanuyānādirdṛḍhatā saṃvaraṃ prati /",
  "SA14bsa026_u:605": "saṃgrahāsevanaṃ dānaṃ praṇītasya ca vastunaḥ // Asa_8.19 //",
  "SA14bsa026_u:606": "vadhyamokṣasamādānaṃ vivṛddhiḥ kuśalasya ca /",
  "SA14bsa026_u:607": "ityādiko yathāsūtraṃ heturlakṣaṇasādhakaḥ // Asa_8.20 //",
  "SA14bsa026_u:608": "tāmrāḥ snigdhāśca tuṅgāśca nakhā aṅgulayo muneḥ /",
  "SA14bsa026_u:609": "vṛttāścitānupūrvāśca gūḍhā nirgranthayaḥ śirāḥ // Asa_8.21 //",
  "SA14bsa026_u:610": "gūḍhau gulphau samau pādau siṃhebhadvijagopateḥ /",
  "SA14bsa026_u:611": "vikrāntaṃ dakṣiṇaṃ cārugamanamṛjuvṛttatā // Asa_8.22 //",
  "SA14bsa026_u:612": "muṣṭānupūrvate medhyamṛdutve śuddhagātratā /",
  "SA14bsa026_u:613": "pūrvavyañjanatā cārupṛthumaṇḍalagātratā // Asa_8.23 //",
  "SA14bsa026_u:614": "samakramatvaṃ śuddhatvaṃ netrayoḥ sukumāratā /",
  "SA14bsa026_u:615": "adīnotsadagātratve susaṃhatanagātratā // Asa_8.24 //",
  "SA14bsa026_u:616": "suvibhaktāṅgatā dhvāntapradhvastālokaśuddhatā /",
  "SA14bsa026_u:617": "vṛttamṛṣṭākṣatākṣāmakukṣitāśca gabhīratā // Asa_8.25 //",
  "SA14bsa026_u:618": "dakṣiṇāvartatā nābheḥ samantād darśanīyatā /",
  "SA14bsa026_u:619": "samācāraḥ śuciḥ kālatilakāpagatā tanuḥ // Asa_8.26 //",
  "SA14bsa026_u:620": "karau tulamṛdū snigdhagambhīrāyatalekhatā /",
  "SA14bsa026_u:621": "nātyāyataṃ vaco bimbapratibimbaupamauṣṭhatā // Asa_8.27 //",
  "SA14bsa026_u:622": "mṛdvī tanvī ca raktā ca jivhā jīmūtaghoṣatā /",
  "SA14bsa026_u:623": "cārūmañjusvaro daṃṣṭrā vṛttāstīkṣṇāḥ sitāḥ samāḥ // Asa_8.28 //",
  "SA14bsa026_u:624": "anupūrvī gatāstuṅgā nāsikā paramaṃ śuciḥ /",
  "SA14bsa026_u:625": "viśāle nayane pakṣmacitaṃ padmadalākṣitā // Asa_8.29 //",
  "SA14bsa026_u:626": "āyataślakṣṇasusnigdhasamaromnau bhruvau bhujau /",
  "SA14bsa026_u:627": "pīnāyatau samau karṇāvupaghātavivarjitau // Asa_8.30 //",
  "SA14bsa026_u:628": "lalāṭamaparimlānaṃ pṛthupūrṇottamāṅgatā /",
  "SA14bsa026_u:629": "bhrabharābhāścitāḥ ślakṣṇā asaṃlulitamurtayaḥ // Asa_8.31 //",
  "SA14bsa026_u:630": "keśā aparuṣāḥ pusāṃ saurabhyādapahāriṇaḥ /",
  "SA14bsa026_u:631": "śrīvatsaḥ svastikaṃ ceti buddhānuvyañjanaṃ matam // Asa_8.32 //",
  "SA14bsa026_u:632": "4 - nairmāṇikakāyaḥ",
  "SA14bsa026_u:633": "karoti yena citrāṇi hitāni jagataḥ samam /",
  "SA14bsa026_u:634": "ābhavāt so 'nupacchinnaḥ kāyo nairmāṇiko muneḥ // Asa_8.33 //",
  "SA14bsa026_u:635": "buddhakāritrāṇi tathā karmāpyanucchinnamasyāsaṃsāramiṣyate /",
  "SA14bsa026_u:636": "gatīnāṃ śamanaṃ karma saṃgrahe ca caturvidhe // Asa_8.34 //",
  "SA14bsa026_u:637": "niveśanaṃ sasaṃkleśe vyavadānāvabodhane /",
  "SA14bsa026_u:638": "sattvānāmarthayāthātmye ṣaṭsu pāramitāsu ca // Asa_8.35 //",
  "SA14bsa026_u:639": "buddhamārge prakṛtyaiva śūnyatāyāṃ dvayakṣaye /",
  "SA14bsa026_u:640": "saṃkete 'nupalambhe ca paripāke ca dehinām // Asa_8.36 //",
  "SA14bsa026_u:641": "bodhisattvasya mārge 'bhiniveśasya nivāraṇe /",
  "SA14bsa026_u:642": "bodhiprāptau jinakṣetraviśuddhau niyatiṃ prati // Asa_8.37 //",
  "SA14bsa026_u:643": "aprameye ca sattvārthe buddhasevādike guṇe /",
  "SA14bsa026_u:644": "bodheraṅgeṣvanāśe ca karmaṇāṃ satyadarśane // Asa_8.38 //",
  "SA14bsa026_u:645": "viparyāsaprahāṇe ca tadavastukatānaye /",
  "SA14bsa026_u:646": "vyavadāne sasambhāre saṃskṛtāsaṃskṛte prati // Asa_8.39 //",
  "SA14bsa026_u:647": "vyatibhedāparijñāne nirvāṇe ca niveśanam /",
  "SA14bsa026_u:648": "dharmakāyasya karmedaṃ saptaviṃśatidhā matam // Asa_8.40 //",
  "SA14bsa026_u:649": "ityabhisamayālaṅkāre nāma prajñāpāramitopadeśaśāstre aṣṭamādhikāraḥ /"
}