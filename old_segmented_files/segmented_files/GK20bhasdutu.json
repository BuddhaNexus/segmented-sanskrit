{
  "GK20bhasdutu:0": "Stage director",
  "GK20bhasdutu:1": "Chamberlain:",
  "GK20bhasdutu:2": "Badarayana, a servant of Duryodhana",
  "GK20bhasdutu:3": "Duryodhana: eldest son of Dhrtarastra",
  "GK20bhasdutu:4": "Vasudeva: Krsna,",
  "GK20bhasdutu:5": "incarnation of Visnu",
  "GK20bhasdutu:6": "Sudarsana: Vasudeva's discus",
  "GK20bhasdutu:7": "Dhrtarastra:",
  "GK20bhasdutu:8": "king of Hastinapura",
  "GK20bhasdutu:9": "dūtavākyam/",
  "GK20bhasdutu:10": "nāndyante tataḥ praviśati sūtradhāraḥ/",
  "GK20bhasdutu:11": "sūtradhāraḥ",
  "GK20bhasdutu:12": "pādaḥ pāyādupendrasya sarvalokotsavaḥ sa vaḥ/",
  "GK20bhasdutu:13": "vyāviddho namuciryena tanutāmranakhena khe// 1//",
  "GK20bhasdutu:14": "evamāryamiśrān vijñāpayāmi/",
  "GK20bhasdutu:15": "aye kiṃ nu khalu mayi vijñāpanavyagre śabda iva",
  "GK20bhasdutu:16": "śrūyate/ aṅga",
  "GK20bhasdutu:17": "paśyāmi/",
  "GK20bhasdutu:18": "nepathye/",
  "GK20bhasdutu:19": "bho bhoḥ pratihārādhikṛtāḥ mahārājo duryodhanaḥ samājñāpayati/",
  "GK20bhasdutu:20": "sūtradhāraḥ",
  "GK20bhasdutu:21": "bhavatu vijñātam/",
  "GK20bhasdutu:22": "utpanne dhārtarāṣṭrāṇāṃ virodhe pāṇḍavaiḥ saha/",
  "GK20bhasdutu:23": "mantraśālāṃ racayati bhṛtyo duryodhanājñayā// 2//",
  "GK20bhasdutu:24": "niṣkrāntaḥ/",
  "GK20bhasdutu:25": "sthāpanā/",
  "GK20bhasdutu:26": "tataḥ praviśati kāñcukīyaḥ/",
  "GK20bhasdutu:27": "kāñcukīyaḥ",
  "GK20bhasdutu:28": "bho bhoḥ pratihārādhikṛtāḥ mahārājo duryodhanaḥ samājñāpayati/ adya",
  "GK20bhasdutu:29": "sarvapārthivaiḥ saha",
  "GK20bhasdutu:30": "mantrayitumicchāmi/",
  "GK20bhasdutu:31": "tadāhūyantāṃ sarve rājāna iti/",
  "GK20bhasdutu:32": "parikramyāvalokya/aye ayaṃ",
  "GK20bhasdutu:33": "mahārājo",
  "GK20bhasdutu:34": "duryodhana ita evābhivartate/ ya eṣaḥ",
  "GK20bhasdutu:35": "śyāmo yuvā sitadukūlakṛtottarīyaḥ sacchattracāmaravaro racitāṅgarāgaḥ/",
  "GK20bhasdutu:36": "śrīmānvibhūṣaṇamaṇidyutirañjitāṅgo nakṣatramadhya iva parvagataḥ śaśāṅkaḥ// 3//",
  "GK20bhasdutu:37": "tataḥ praviśati yathānirdiṣṭo duryodhanaḥ/",
  "GK20bhasdutu:38": "duryodhanaḥ uddhūtaroṣamiva me",
  "GK20bhasdutu:39": "hṛdayaṃ saharṣaṃ prāptaṃ",
  "GK20bhasdutu:40": "raṇotsavamimaṃ sahasā vicintya/",
  "GK20bhasdutu:41": "icchāmi pāṇḍavabale varavāraṇānā-",
  "GK20bhasdutu:42": "mutkṛttadantamusalāni",
  "GK20bhasdutu:43": "mukhāni kartum// 4//",
  "GK20bhasdutu:45": "kāñcukīyaḥ",
  "GK20bhasdutu:46": "jayatu mahārājaḥ/",
  "GK20bhasdutu:47": "mahārājaśāsanātsamānītaṃ sarvarājamaṇḍalam/",
  "GK20bhasdutu:48": "duryodhanaḥ",
  "GK20bhasdutu:49": "samyakkṛtam/",
  "GK20bhasdutu:50": "praviśa tvamavarodhanam/",
  "GK20bhasdutu:51": "kāñcukīyaḥ",
  "GK20bhasdutu:52": "yadājñāpayati mahārājaḥ/",
  "GK20bhasdutu:53": "niṣkrāntaḥ/",
  "GK20bhasdutu:54": "duryodhanaḥ",
  "GK20bhasdutu:55": "āryau vaikarṇavarṣadevau ucyatām/",
  "GK20bhasdutu:56": "asti mamaikādaśākṣauhiṇībalasamudayaḥ/ asya kaḥ",
  "GK20bhasdutu:58": "senāpatirbhavitumarhatīti/",
  "GK20bhasdutu:59": "kimāhaturbhavantau/",
  "GK20bhasdutu:60": "mahānkhalvayamarthaḥ/",
  "GK20bhasdutu:61": "mantrayitvā vaktavyamiti/",
  "GK20bhasdutu:62": "sadṛśametat/",
  "GK20bhasdutu:63": "tadāgamyatāṃ mantraśālāmeva praviśāmaḥ/",
  "GK20bhasdutu:64": "ācārya abhivādaye/",
  "GK20bhasdutu:65": "praviśatu bhavānmantraśālām/",
  "GK20bhasdutu:66": "pitāmaha abhivādaye/ praviśatu",
  "GK20bhasdutu:67": "bhavānmantraśālām/",
  "GK20bhasdutu:68": "mātula abhivādaye/",
  "GK20bhasdutu:69": "praviśatu bhavānmantraśālām/ āryau",
  "GK20bhasdutu:70": "vaikarṇavarṣadevau praviśatāṃ bhavantau/",
  "GK20bhasdutu:71": "bho bhoḥ sarvakṣatriyāḥ svairaṃ",
  "GK20bhasdutu:72": "praviśantu bhavantaḥ/",
  "GK20bhasdutu:73": "vayasya karṇa praviśāvastāvat/",
  "GK20bhasdutu:74": "praviśya/ācārya etatkūrmāsanam/",
  "GK20bhasdutu:75": "āsyatām/",
  "GK20bhasdutu:76": "pitāmaha etatsiṃhāsanam/",
  "GK20bhasdutu:77": "āsyatām/ mātula",
  "GK20bhasdutu:78": "etaccarmāsanam/",
  "GK20bhasdutu:79": "āsyatām/",
  "GK20bhasdutu:80": "āryau vaikarṇavarṣadevau āsātāṃ bhavantau/ bho bhoḥ",
  "GK20bhasdutu:81": "sarvakṣatriyāḥ",
  "GK20bhasdutu:82": "svairamāsatāṃ bhavantaḥ/",
  "GK20bhasdutu:83": "kimiti kimiti/",
  "GK20bhasdutu:84": "mahārājo nāsta iti/",
  "GK20bhasdutu:85": "aho sevādharmaḥ/",
  "GK20bhasdutu:86": "nanvayamahamāse/",
  "GK20bhasdutu:87": "vayasya karṇa tvamapyāssva/",
  "GK20bhasdutu:88": "upaviśya/āryau vaikarṇavarṣadevau ucyatām/ asti",
  "GK20bhasdutu:89": "mamaikādaśākṣauhiṇībalasamudayaḥ/ asya kaḥ",
  "GK20bhasdutu:90": "senāpatirbhavitumarhatīti/",
  "GK20bhasdutu:91": "kimāhaturbhavantau/",
  "GK20bhasdutu:92": "atrabhavāngāndhārarājo vakṣyatīti/ bhavatu",
  "GK20bhasdutu:93": "mātulenābhidhīyatām/",
  "GK20bhasdutu:94": "kimāha mātulaḥ/",
  "GK20bhasdutu:95": "atrabhavati gāṅgeye sthite ko.anyaḥ senāpatirbhavitumarhatīti/",
  "GK20bhasdutu:96": "samyagāha mātulaḥ/",
  "GK20bhasdutu:97": "bhavatu bhavatu pitāmaha eva bhavatu/",
  "GK20bhasdutu:98": "vayamapyetadabhilaṣāmaḥ/",
  "GK20bhasdutu:99": "senāninādapaṭahasvanaśaṅkhanādai- ścaṇḍānilāhatamahodadhinādakalpaiḥ/",
  "GK20bhasdutu:100": "gāṅgeyamūrdhni",
  "GK20bhasdutu:101": "patitairabhiṣekatoyaiḥ sārdhaṃ patantu hṛdayāni narādhipānām// 5// praviśya/",
  "GK20bhasdutu:102": "kāñcukīyaḥ",
  "GK20bhasdutu:103": "jayatu mahārājaḥ/",
  "GK20bhasdutu:104": "eṣa khalu pāṇḍavaskandhāvārāddautyenāgataḥ puruṣottamo",
  "GK20bhasdutu:105": "nārāyaṇaḥ/",
  "GK20bhasdutu:106": "duryodhanaḥ",
  "GK20bhasdutu:107": "mā tāvat/ bho bādarāyaṇa kiṃ kiṃ kaṃsabhṛtyo dāmodarastava puruṣottamaḥ/ sa",
  "GK20bhasdutu:108": "gopālakastava",
  "GK20bhasdutu:109": "puruṣottamaḥ/",
  "GK20bhasdutu:110": "bārhadrathāpahṛtaviṣayakīrtibhogastava puruṣottamaḥ/ aho",
  "GK20bhasdutu:111": "pārthivāsannamāśritya",
  "GK20bhasdutu:112": "bhṛtyajanasya samudācāraḥ/",
  "GK20bhasdutu:113": "sagarvaṃ khalvasya vacanam/",
  "GK20bhasdutu:114": "ā apadhvaṃsa/",
  "GK20bhasdutu:115": "kāñcukīyaḥ",
  "GK20bhasdutu:116": "prasīdatu mahārājaḥ/",
  "GK20bhasdutu:117": "saṃbhrameṇa samudācāro vismṛtaḥ/",
  "GK20bhasdutu:118": "pādayoḥ patati/",
  "GK20bhasdutu:119": "duryodhanaḥ",
  "GK20bhasdutu:120": "saṃbhrama iti/",
  "GK20bhasdutu:121": "ā manuṣyāṇāmastyeva saṃbhramaḥ/",
  "GK20bhasdutu:122": "uttiṣṭhotiṣṭha/",
  "GK20bhasdutu:123": "kāñcukīyaḥ",
  "GK20bhasdutu:124": "anugṛhīto.asmi/",
  "GK20bhasdutu:125": "duryodhanaḥ",
  "GK20bhasdutu:126": "idānīṃ prasanno.asmi/",
  "GK20bhasdutu:127": "ka eṣa dūtaḥ prāptaḥ/",
  "GK20bhasdutu:128": "kāñcukīyaḥ",
  "GK20bhasdutu:129": "dūtaḥ prāptaḥ keśavaḥ/",
  "GK20bhasdutu:130": "duryodhanaḥ",
  "GK20bhasdutu:131": "keśava iti/ evameṣṭavyam/",
  "GK20bhasdutu:132": "ayameva samudācāraḥ/",
  "GK20bhasdutu:133": "bho bho rājānaḥ dautyenāgatasya",
  "GK20bhasdutu:134": "keśavasya kiṃ",
  "GK20bhasdutu:135": "yuktam/",
  "GK20bhasdutu:136": "kimāhurbhavantaḥ/",
  "GK20bhasdutu:137": "arghyapradānena pūjayitavyaḥ keśava iti/",
  "GK20bhasdutu:138": "na me rocate/",
  "GK20bhasdutu:139": "grahaṇamasyātra hitaṃ",
  "GK20bhasdutu:140": "paśyāmi/",
  "GK20bhasdutu:141": "grahaṇamupagate tu vāsubhadre hṛtanayanā iva pāṇḍavā bhaveyuḥ/",
  "GK20bhasdutu:142": "gatimatirahiteṣu",
  "GK20bhasdutu:143": "pāṇḍaveṣu",
  "GK20bhasdutu:144": "kṣitirakhilāpi bhavenmamāsapatnā// 6//",
  "GK20bhasdutu:145": "api ca yo.atra keśavasya pratyutthāsyati sa mayā dvādaśasuvarṇabhāreṇa daṇḍyaḥ/",
  "GK20bhasdutu:146": "tadapramattā",
  "GK20bhasdutu:147": "bhavantu bhavantaḥ/",
  "GK20bhasdutu:148": "ātmagatam/",
  "GK20bhasdutu:149": "ko nu khalvidānīṃ mamāpratyutthānasyopāyaḥ/ hanta",
  "GK20bhasdutu:150": "dṛṣṭa",
  "GK20bhasdutu:151": "upāyaḥ/prakāśam/",
  "GK20bhasdutu:152": "bādarāyaṇa ānīyatāṃ sa citrapaṭo nanu yatra",
  "GK20bhasdutu:153": "draupadīkeśāmbarāvakarṣaṇamālikhitam/",
  "GK20bhasdutu:154": "ātmagatam/",
  "GK20bhasdutu:155": "tasmindṛṣṭivinyāsaṃ",
  "GK20bhasdutu:156": "kurvannotthāsyāmi",
  "GK20bhasdutu:157": "keśavasya/",
  "GK20bhasdutu:158": "kāñcukīyaḥ",
  "GK20bhasdutu:159": "yadājñāpayati mahārājaḥ/",
  "GK20bhasdutu:160": "niṣkramya praviśya/",
  "GK20bhasdutu:161": "jayatu mahārājaḥ/",
  "GK20bhasdutu:162": "ayaṃ sa citrapaṭaḥ/",
  "GK20bhasdutu:163": "duryodhanaḥ",
  "GK20bhasdutu:164": "mamāgrataḥ prasāraya/",
  "GK20bhasdutu:165": "kāñcukīyaḥ",
  "GK20bhasdutu:166": "yadājñāpayati mahārājaḥ/",
  "GK20bhasdutu:167": "prasārayati/",
  "GK20bhasdutu:168": "duryodhanaḥ",
  "GK20bhasdutu:169": "aho darśanīyo.ayaṃ citrapaṭaḥ/",
  "GK20bhasdutu:170": "eṣa duḥśāsano draupadīṃ keśahaste gṛhītavān/ eṣā",
  "GK20bhasdutu:171": "khalu",
  "GK20bhasdutu:172": "draupadī/",
  "GK20bhasdutu:173": "duḥśāsanaparāmṛṣṭā saṃbhramotphullalocanā/",
  "GK20bhasdutu:174": "rāhuvaktrāntaragatā candralekheva",
  "GK20bhasdutu:175": "śobhate// 7//",
  "GK20bhasdutu:176": "eṣa durātmā bhīmaḥ sarvarājasamakṣamavamānitāṃ draupadīṃ dṛṣṭvā",
  "GK20bhasdutu:177": "pravṛddhāmarṣaḥ",
  "GK20bhasdutu:178": "sabhāstambhaṃ tulayati/",
  "GK20bhasdutu:179": "eṣa yudhiṣṭhiraḥ",
  "GK20bhasdutu:180": "satyadharmaghṛṇāyukto dyūtavibhraṣṭacetanaḥ/",
  "GK20bhasdutu:181": "karotyapāṅgavikṣepaiḥ śāntāmarṣaṃ",
  "GK20bhasdutu:182": "vṛkodaram// 8//",
  "GK20bhasdutu:183": "eṣa idānīmarjunaḥ/",
  "GK20bhasdutu:184": "roṣākulākṣaḥ sphuritādharoṣṭhastṛṇāya matvā ripumaṇḍalaṃ tat/",
  "GK20bhasdutu:185": "utsādayiṣyanniva",
  "GK20bhasdutu:186": "sarvarājñaḥ",
  "GK20bhasdutu:187": "śanaiḥ samākarṣati gāṇḍivajyām// 9//",
  "GK20bhasdutu:188": "eṣa yudhiṣṭhiro.arjunaṃ nivārayati/",
  "GK20bhasdutu:189": "etau nakulasahadevau",
  "GK20bhasdutu:190": "kṛtaparikarabandhau carmanistriṃśahastau paruṣitamukharāgau",
  "GK20bhasdutu:191": "spaṣṭadaṣṭādharoṣṭhau/",
  "GK20bhasdutu:192": "vigatamaraṇaśaṅkau satvaraṃ bhrātaraṃ me harimiva mṛgapotau tejasābhiprayātau// 10//",
  "GK20bhasdutu:194": "eṣa yudhiṣṭhiraḥ kumārāvupetya nivārayati/",
  "GK20bhasdutu:195": "nīco.ahameva viparītamatiḥ kathaṃ vā roṣaṃ parityajatamadya nayānayajñau/",
  "GK20bhasdutu:196": "dyūtādhikāramavamānamamṛṣyamāṇāḥ sattvādhikeṣu vacanīyaparākramāḥ syuḥ// 11//",
  "GK20bhasdutu:197": "iti/ eṣa gāndhārarājaḥ",
  "GK20bhasdutu:198": "akṣānkṣipansa kitavaḥ prahasansagarvaṃ saṃkocayanniva mudaṃ dviṣatāṃ",
  "GK20bhasdutu:199": "svakīrtyā/ svairāsano",
  "GK20bhasdutu:200": "drupadarājasutāṃ rudantīṃ kākṣeṇa paśyati likhatyapi gāṃ nayajñaḥ// 12//",
  "GK20bhasdutu:201": "etāvācāryapitāmahau tāṃ dṛṣṭvā lajjāyamānau paṭāntāntarhitamukhau sthitau/ aho asya",
  "GK20bhasdutu:203": "varṇāḍhyatā/",
  "GK20bhasdutu:204": "aho bhāvopapannatā/",
  "GK20bhasdutu:205": "aho yuktalekhatā/",
  "GK20bhasdutu:206": "suvyaktamālikhito.ayaṃ",
  "GK20bhasdutu:207": "citrapaṭaḥ/",
  "GK20bhasdutu:208": "prīto.asmi/ ko.atra/",
  "GK20bhasdutu:209": "kāñcukīyaḥ",
  "GK20bhasdutu:210": "jayatu mahārājaḥ/",
  "GK20bhasdutu:211": "duryodhanaḥ",
  "GK20bhasdutu:212": "bādarāyaṇa ānīyatāṃ sa vihagavāhanamātravismito dūtaḥ/",
  "GK20bhasdutu:213": "kāñcukīyaḥ",
  "GK20bhasdutu:214": "yadājñāpayati mahārājaḥ/",
  "GK20bhasdutu:215": "niṣkrāntaḥ/",
  "GK20bhasdutu:216": "duryodhanaḥ",
  "GK20bhasdutu:217": "vayasya karṇa",
  "GK20bhasdutu:218": "prāptaḥ kilādya vacanādiha pāṇḍavānāṃ dautyena bhṛtya iva kṛṣṇamatiḥ sa kṛṣṇaḥ/",
  "GK20bhasdutu:219": "śrotuṃ",
  "GK20bhasdutu:220": "sakhe tvamapi sajjaya karṇa karṇau nārīmṛdūni vacanāni yudhiṣṭhirasya// 13// tataḥ",
  "GK20bhasdutu:221": "praviśati",
  "GK20bhasdutu:222": "vāsudevaḥ kāñcukīyaśca/ vāsudevaḥ",
  "GK20bhasdutu:223": "adya khalu dharmarājavacanāddhanaṃjayākṛtrimamitratayā",
  "GK20bhasdutu:224": "cāhavadarpamanuktagrāhiṇaṃ",
  "GK20bhasdutu:225": "suyodhanaṃ prati mayāpyanucitadautyasamayo.anuṣṭhitaḥ/ atha ca",
  "GK20bhasdutu:226": "kṛṣṇāparābhavabhuvā ripuvāhinībha- kumbhasthalīdalanatīkṣṇagadādharasya/",
  "GK20bhasdutu:227": "bhīmasya",
  "GK20bhasdutu:228": "kopaśikhinā yudhi pārthapattri- caṇḍānilaiśca kuruvaṃśavanaṃ vinaṣṭam// 14//",
  "GK20bhasdutu:229": "idaṃ suyodhanaśibiram/ iha hi",
  "GK20bhasdutu:230": "āvāsāḥ pārthivānāṃ surapurasadṛśāḥ svacchandavihitā vistīrṇāḥ śastraśālā",
  "GK20bhasdutu:231": "bahuvidhakaraṇaiḥ śastrairupacitāḥ/",
  "GK20bhasdutu:232": "heṣante mandurāsthāsturagavaraghaṭā bṛṃhanti",
  "GK20bhasdutu:233": "kariṇa",
  "GK20bhasdutu:234": "aiśvaryaṃ sphītametatsvajanaparibhavādāsannavilayam// 15// bhoḥ",
  "GK20bhasdutu:236": "duṣṭavādī guṇadveṣī śaṭhaḥ svajananirdayaḥ/",
  "GK20bhasdutu:237": "suyodhano hi māṃ dṛṣṭvā naiva",
  "GK20bhasdutu:238": "kāryaṃ kariṣyati// 16//",
  "GK20bhasdutu:240": "bho bādarāyaṇa kiṃ praveṣṭavyam/",
  "GK20bhasdutu:241": "kāñcukīyaḥ",
  "GK20bhasdutu:242": "atha kimatha kim/",
  "GK20bhasdutu:243": "praveṣṭumarhati padmanābhaḥ/",
  "GK20bhasdutu:244": "vāsudevaḥ",
  "GK20bhasdutu:245": "praviśya/ kathaṃ kathaṃ māṃ dṛṣṭvā saṃbhrāntāḥ sarvakṣatriyāḥ/ alamalaṃ",
  "GK20bhasdutu:246": "saṃbhrameṇa/",
  "GK20bhasdutu:247": "svairamāsatāṃ bhavantaḥ/",
  "GK20bhasdutu:248": "duryodhanaḥ",
  "GK20bhasdutu:249": "kathaṃ kathaṃ keśavaṃ dṛṣṭvā saṃbhrāntāḥ sarvakṣatriyāḥ/",
  "GK20bhasdutu:250": "alamalaṃ saṃbhrameṇa/",
  "GK20bhasdutu:251": "smaraṇīyaḥ pūrvamāśrāvito daṇḍaḥ/",
  "GK20bhasdutu:252": "nanvahamājñaptā/",
  "GK20bhasdutu:253": "vāsudevaḥ",
  "GK20bhasdutu:254": "upagamya/bhoḥ suyodhana kimāsse/",
  "GK20bhasdutu:255": "duryodhanaḥ",
  "GK20bhasdutu:256": "āsanātpatitvātmagatam/",
  "GK20bhasdutu:257": "suvyaktaṃ prāpta eva keśavaḥ/",
  "GK20bhasdutu:258": "utsāhena matiṃ kṛtvāpyāsīno.asmi samāhitaḥ/",
  "GK20bhasdutu:259": "keśavasya prabhāvena",
  "GK20bhasdutu:260": "calito.asmyāsanādaham// 17//",
  "GK20bhasdutu:261": "aho bahumāyo.ayaṃ dūtaḥ/",
  "GK20bhasdutu:262": "prakāśam/",
  "GK20bhasdutu:263": "bho dūta etadāsanamāsyatām/",
  "GK20bhasdutu:264": "vāsudevaḥ",
  "GK20bhasdutu:265": "ācārya āsyatāṃ/",
  "GK20bhasdutu:266": "gāṅgeyapramukhā rājānaḥ svairamāsatāṃ bhavantaḥ/",
  "GK20bhasdutu:267": "vayamapyupaviśāmaḥ/",
  "GK20bhasdutu:268": "upaviśya/",
  "GK20bhasdutu:269": "aho darśanīyo.ayaṃ citrapaṭaḥ/ mā tāvat/",
  "GK20bhasdutu:270": "draupadīkeśāmbarākarṣaṇamatrālikhitam/",
  "GK20bhasdutu:271": "aho nu khalu",
  "GK20bhasdutu:272": "suyodhano.ayaṃ svajanāvamānaṃ parākramaṃ paśyati bāliśatvāt/",
  "GK20bhasdutu:273": "ko nāma loke",
  "GK20bhasdutu:274": "svayamātmadoṣamudghāṭayennaṣṭaghṛṇaḥ sabhāsu// 18//",
  "GK20bhasdutu:275": "āḥ apanīyatāmeṣa citrapaṭaḥ/",
  "GK20bhasdutu:276": "duryodhanaḥ",
  "GK20bhasdutu:277": "bādarāyaṇa apanīyatāṃ kila citrapaṭaḥ/",
  "GK20bhasdutu:278": "kāñcukīyaḥ",
  "GK20bhasdutu:279": "yadājñāpayati maharājaḥ/apanayati/",
  "GK20bhasdutu:280": "duryodhanaḥ",
  "GK20bhasdutu:281": "bho dūta",
  "GK20bhasdutu:282": "dharmātmajo vāyusutaśca bhīmo bhrātārjuno me tridaśendrasūnuḥ/ yamau ca",
  "GK20bhasdutu:283": "tāvaśvisutau vinītau",
  "GK20bhasdutu:284": "sarve sabhṛtyāḥ kuśalopapannāḥ// 19// vāsudevaḥ",
  "GK20bhasdutu:285": "sadṛśametadgāndhārīputrasya/",
  "GK20bhasdutu:286": "atha kimatha kim/",
  "GK20bhasdutu:287": "kuśalinaḥ sarve/",
  "GK20bhasdutu:288": "bhavato rājye",
  "GK20bhasdutu:289": "śarīre",
  "GK20bhasdutu:290": "bāhyābhyantare ca kuśalamanāmayaṃ ca pṛṣṭvā vijñāpayanti yudhiṣṭhirādayaḥ",
  "GK20bhasdutu:291": "pāṇḍavāḥ/",
  "GK20bhasdutu:292": "anubhūtaṃ mahadduḥkhaṃ saṃpūrṇaḥ samayaḥ sa ca/",
  "GK20bhasdutu:293": "asmākamapi dharmyaṃ",
  "GK20bhasdutu:294": "yaddāyādyaṃ",
  "GK20bhasdutu:295": "tadvibhajyatām// 20// iti/",
  "GK20bhasdutu:297": "duryodhanaḥ",
  "GK20bhasdutu:298": "kathaṃ kathaṃ dāyādyamiti/ bhoḥ",
  "GK20bhasdutu:299": "vane pitṛvyo mṛgayāprasaṃgataḥ kṛtāparādho muniśāpamāptavān/",
  "GK20bhasdutu:300": "tadāprabhṛtyeva sa",
  "GK20bhasdutu:301": "dāraniḥspṛhaḥ parātmajānāṃ pitṛtāṃ kathaṃ vrajet// 21// vāsudevaḥ",
  "GK20bhasdutu:302": "purāvidaṃ bhavantaṃ pṛcchāmi/",
  "GK20bhasdutu:303": "vicitravīryo viṣayī vipattiṃ kṣayeṇa yātaḥ punarambikāyām/",
  "GK20bhasdutu:304": "vyāsena jāto",
  "GK20bhasdutu:305": "dhṛtarāṣṭra eva labheta",
  "GK20bhasdutu:306": "rājyaṃ janakaḥ kathaṃ te// 22//",
  "GK20bhasdutu:307": "mā mā bhavān/",
  "GK20bhasdutu:308": "evaṃ parasparavirodhavivardhanena śīghraṃ bhavetkurukulaṃ nṛpa nāmaśeṣam/",
  "GK20bhasdutu:309": "tatkartumarhati",
  "GK20bhasdutu:310": "bhavānapakṛtya roṣaṃ yattvāṃ yudhiṣṭhiramukhāḥ praṇayādbruvanti// 23//",
  "GK20bhasdutu:311": "duryodhanaḥ",
  "GK20bhasdutu:312": "bho dūta na jānāti bhavānrājyavyavahāram/",
  "GK20bhasdutu:313": "rājyaṃ nāma nṛpātmajaiḥ sahṛdairjitvā ripūnbhujyate talloke na tu yācyate na tu",
  "GK20bhasdutu:314": "punardīnāya vā",
  "GK20bhasdutu:315": "dīyate/ kāṅkṣā cennṛpatitvamāptumacirātkurvantu te sāhasaṃ svairaṃ vā praviśantu",
  "GK20bhasdutu:316": "śāntamatibhirjuṣṭaṃ śamāyāśramam// 24// vāsudevaḥ",
  "GK20bhasdutu:317": "bhoḥ suyodhana alaṃ bandhujane paruṣamabhidhātum/",
  "GK20bhasdutu:318": "puṇyasaṃcayaprāptāmadhigamya nṛpaśriyam/",
  "GK20bhasdutu:319": "vañcayedyaḥ suhṛdbandhūnsa",
  "GK20bhasdutu:320": "bhavedviphalaśramaḥ// 25//",
  "GK20bhasdutu:322": "duryodhanaḥ",
  "GK20bhasdutu:323": "bho dūta",
  "GK20bhasdutu:324": "syālaṃ tava gurorbhūpaṃ kaṃsaṃ na te dayā/",
  "GK20bhasdutu:325": "kathamasmākamevaṃ syātteṣu",
  "GK20bhasdutu:326": "nityāpakāriṣu// 26//",
  "GK20bhasdutu:327": "vāsudevaḥ",
  "GK20bhasdutu:328": "alaṃ tanmaddoṣato jñātum/",
  "GK20bhasdutu:329": "kṛtvā putraviyogārtāṃ bahuśo jananīṃ mama/",
  "GK20bhasdutu:330": "vṛddhaṃ svapitaraṃ baddhvā hato.ayaṃ",
  "GK20bhasdutu:331": "mṛtyunā",
  "GK20bhasdutu:332": "svayam// 27// duryodhanaḥ",
  "GK20bhasdutu:333": "sarvathā vañcitastvayā kaṃsaḥ/",
  "GK20bhasdutu:334": "alamātmastavena/",
  "GK20bhasdutu:335": "na śauryametat/ paśya/",
  "GK20bhasdutu:336": "jāmātṛnāśavyasanābhitapte roṣābhibhūte magadheśvare.atha/",
  "GK20bhasdutu:337": "palāyamānasya",
  "GK20bhasdutu:338": "bhayāturasya",
  "GK20bhasdutu:339": "śauryaṃ tadetatkva gataṃ tavāsīt// 28// vāsudevaḥ",
  "GK20bhasdutu:340": "bhoḥ suyodhana deśakālāvasthāpekṣitaṃ khalu śauryaṃ nayānugāminām/ iha",
  "GK20bhasdutu:341": "tiṣṭhatu",
  "GK20bhasdutu:342": "tāvadasmadgataḥ parihāsaḥ/",
  "GK20bhasdutu:343": "svakāryamanuṣṭhīyatām/",
  "GK20bhasdutu:344": "kartavyo bhrātṛṣu sneho vismartavyā guṇetarāḥ/",
  "GK20bhasdutu:345": "saṃbandho bandhubhiḥ",
  "GK20bhasdutu:346": "śreyānlokayorubhayorapi//",
  "GK20bhasdutu:347": "29// duryodhanaḥ devātmajairmanuṣyāṇāṃ kathaṃ vā bandhutā bhavet/",
  "GK20bhasdutu:348": "piṣṭapeṣaṇametāvatparyāptaṃ chidyatāṃ kathā// 30// vāsudevaḥ",
  "GK20bhasdutu:349": "ātmagatam/",
  "GK20bhasdutu:350": "prasādyamānaḥ sāmnāyaṃ na svabhāvaṃ vimuñcati/",
  "GK20bhasdutu:351": "hanta saṃkṣobhayāmyenaṃ",
  "GK20bhasdutu:352": "vacobhiḥ",
  "GK20bhasdutu:353": "paruṣākṣaraiḥ// 31//",
  "GK20bhasdutu:354": "prakāśam/bhoḥ suyodhana kiṃ na jānīṣe.arjunasya balaparākramam/",
  "GK20bhasdutu:355": "duryodhanaḥ",
  "GK20bhasdutu:356": "na jāne/",
  "GK20bhasdutu:357": "vāsudevaḥ",
  "GK20bhasdutu:358": "bhoḥ śrūyatām/",
  "GK20bhasdutu:359": "kairātaṃ vapurāsthitaḥ paśupatiryuddhena saṃtoṣito vahneḥ khāṇḍavamaśnataḥ",
  "GK20bhasdutu:360": "sumahatī vṛṣṭiḥ",
  "GK20bhasdutu:361": "śaraiśchāditā/",
  "GK20bhasdutu:362": "devendrārtikarā nivātakavacā nītāḥ kṣayaṃ līlayā nanvekena tadā",
  "GK20bhasdutu:363": "virāṭanagare",
  "GK20bhasdutu:364": "bhīṣmādayo nirjitāḥ// 32//",
  "GK20bhasdutu:365": "api ca tavāpi pratyakṣamaparaṃ kathayāmi/",
  "GK20bhasdutu:366": "nanu tvaṃ citrasenena nīyamāno nabhastalam/",
  "GK20bhasdutu:367": "vikrośanghoṣayātrāyāṃ phalgunenaiva",
  "GK20bhasdutu:368": "mokṣitaḥ// 33//",
  "GK20bhasdutu:369": "kiṃ bahunā/",
  "GK20bhasdutu:370": "dātumarhasi madvākyādrājyārdhaṃ dhṛtarāṣṭraja/",
  "GK20bhasdutu:371": "anyathā sāgarāntāṃ gāṃ hariṣyanti hi",
  "GK20bhasdutu:373": "pāṇḍavāḥ// 34// duryodhanaḥ",
  "GK20bhasdutu:374": "kathaṃ kathaṃ/",
  "GK20bhasdutu:375": "hariṣyanti hi pāṇḍavāḥ/",
  "GK20bhasdutu:376": "praharati yadi yuddhe māruto bhīmarūpī praharati yadi sākṣātpārtharūpeṇa śakraḥ/",
  "GK20bhasdutu:377": "paruṣavacanadakṣa tvadvacobhirna dāsye tṛṇamapi pitṛbhukte vīryagupte svarājye//",
  "GK20bhasdutu:378": "35// vāsudevaḥ",
  "GK20bhasdutu:379": "bhoḥ kurukulakalaṅkabhūta ayaśolubdha vayaṃ kila tṛṇāntarābhibhāṣakāḥ/",
  "GK20bhasdutu:380": "duryodhanaḥ",
  "GK20bhasdutu:381": "bho gopālaka tṛṇānyabhibhāṣyo bhavān/",
  "GK20bhasdutu:382": "avadhyāṃ pramadāṃ hatvā hayaṃ govṛṣameva ca/",
  "GK20bhasdutu:383": "mallānapi sunirlajjo vaktumicchasi",
  "GK20bhasdutu:384": "sādhubhiḥ//",
  "GK20bhasdutu:385": "36// vāsudevaḥ",
  "GK20bhasdutu:386": "bhoḥ suyodhana nanu kṣipasi mām/",
  "GK20bhasdutu:387": "duryodhanaḥ",
  "GK20bhasdutu:388": "nanu satyamevaitat/",
  "GK20bhasdutu:389": "vāsudevaḥ",
  "GK20bhasdutu:390": "gacchāmi tāvat/",
  "GK20bhasdutu:391": "duryodhanaḥ",
  "GK20bhasdutu:392": "gaccha gaccha paśukhuroddhatareṇurūpitāṅgo vrajameva/",
  "GK20bhasdutu:393": "viphalīkṛtaḥ kālaḥ/",
  "GK20bhasdutu:394": "vāsudevaḥ",
  "GK20bhasdutu:395": "evamastu/ na vayamanuktasaṃdeśā gantumicchāmaḥ/",
  "GK20bhasdutu:396": "tadākarṇyatāṃ yudhiṣṭhirasya",
  "GK20bhasdutu:397": "saṃdeśaḥ/",
  "GK20bhasdutu:398": "duryodhanaḥ",
  "GK20bhasdutu:399": "āḥ abhāṣyastvam/",
  "GK20bhasdutu:400": "ahamavadhṛtapāṇḍarātapatro dvijavarahastadhṛtāmbusiktamūrdhā/",
  "GK20bhasdutu:401": "avanatanṛpamaṇḍalānuyātraiḥ",
  "GK20bhasdutu:402": "saha kathayāmi bhavadvidhairna bhāṣe// 37// vāsudevaḥ",
  "GK20bhasdutu:403": "na vyāharati kila māṃ suyodhanaḥ/ bhoḥ",
  "GK20bhasdutu:404": "śaṭha bāndhavaniḥsneha kāka kekara piṅgala/",
  "GK20bhasdutu:405": "tvadarthātkuruvaṃśo.ayamacirānnāśameṣyati// 38//",
  "GK20bhasdutu:406": "bho bho rājānaḥ gacchāmastāvat/",
  "GK20bhasdutu:407": "duryodhanaḥ",
  "GK20bhasdutu:408": "kathaṃ yāsyati kila keśavaḥ/",
  "GK20bhasdutu:409": "duḥśāsana durmarṣaṇa durmukha durbuddhe",
  "GK20bhasdutu:410": "duṣṭeśvara",
  "GK20bhasdutu:411": "dūtasamudācāramatikrāntaḥ keśavo badhyatām/",
  "GK20bhasdutu:412": "kathamaśaktāḥ/",
  "GK20bhasdutu:413": "duḥśāsana na",
  "GK20bhasdutu:414": "samarthaḥ",
  "GK20bhasdutu:415": "khalvasi/",
  "GK20bhasdutu:416": "karituraganihantā kaṃsahantā sa kṛṣṇaḥ paśupakulanivāsādānujīvyānabhijñaḥ/",
  "GK20bhasdutu:417": "hṛtabhujabalavīryaḥ pārthivānāṃ samakṣaṃ svavacanakṛtadoṣo badhyatāmeṣa",
  "GK20bhasdutu:418": "śīghram// 39//",
  "GK20bhasdutu:419": "ayamaśaktaḥ/",
  "GK20bhasdutu:420": "mātula badhyatāmayaṃ keśavaḥ/",
  "GK20bhasdutu:421": "kathaṃ parāṅmukhaḥ patati/ bhavatu/",
  "GK20bhasdutu:422": "ahameva",
  "GK20bhasdutu:423": "pāśairbadhnāmi/",
  "GK20bhasdutu:424": "pāśamudyamyopasarpati/",
  "GK20bhasdutu:425": "vāsudevaḥ",
  "GK20bhasdutu:426": "kathaṃ baddhukāmo māṃ kila suyodhanaḥ/",
  "GK20bhasdutu:427": "bhavatu/",
  "GK20bhasdutu:428": "suyodhanasya sāmarthyaṃ",
  "GK20bhasdutu:429": "paśyāmi/viśvarūpamāsthitaḥ/",
  "GK20bhasdutu:430": "duryodhanaḥ",
  "GK20bhasdutu:431": "bho dūta",
  "GK20bhasdutu:432": "sṛjasi yadi samantāddevamāyāḥ svamāyāḥ praharasi yadi vā tvaṃ durnivāraiḥ",
  "GK20bhasdutu:433": "surāstraiḥ/",
  "GK20bhasdutu:434": "hayagajavṛṣabhāṇāṃ pātanājjātadarpo narapatigaṇamadhye badhyase tvaṃ mayādya// 40//",
  "GK20bhasdutu:436": "āḥ tiṣṭhedānīm/",
  "GK20bhasdutu:437": "kathaṃ na dṛṣṭaḥ keśavaḥ/",
  "GK20bhasdutu:438": "ayaṃ keśavaḥ/",
  "GK20bhasdutu:439": "aho hrasvatvaṃ keśavasya/ āḥ",
  "GK20bhasdutu:441": "tiṣṭhedānīm/",
  "GK20bhasdutu:442": "kathaṃ na dṛṣṭaḥ keśavaḥ/",
  "GK20bhasdutu:443": "ayaṃ keśavaḥ/",
  "GK20bhasdutu:444": "aho dīrghatvaṃ keśavasya/",
  "GK20bhasdutu:445": "kathaṃ na",
  "GK20bhasdutu:446": "dṛṣṭaḥ keśavaḥ/",
  "GK20bhasdutu:447": "ayaṃ keśavaḥ/",
  "GK20bhasdutu:448": "sarvatra mantraśālāyāṃ keśavā bhavanti/ kimidānīṃ",
  "GK20bhasdutu:449": "kariṣye/",
  "GK20bhasdutu:450": "bhavatu dṛṣṭam/",
  "GK20bhasdutu:451": "bho bho rājānaḥ ekenaikaḥ keśavo badhyatām/",
  "GK20bhasdutu:452": "kathaṃ svayameva",
  "GK20bhasdutu:453": "pāśairbaddhāḥ",
  "GK20bhasdutu:454": "patanti rājānaḥ/",
  "GK20bhasdutu:455": "sādhu bho jambhaka sādhu/",
  "GK20bhasdutu:456": "matkārmukodaraviniḥsṛtabāṇajālai- rviddhaṃ kṣaratkṣatajarañjitasarvagātram/",
  "GK20bhasdutu:457": "paśyantu",
  "GK20bhasdutu:458": "pāṇḍutanayāḥ śibiropanītaṃ tvāṃ bāṣparuddhanayanāḥ pariniśvasantaḥ// 41//",
  "GK20bhasdutu:459": "niṣkrāntaḥ/",
  "GK20bhasdutu:460": "vāsudevaḥ",
  "GK20bhasdutu:461": "bhavatu pāṇḍavānāṃ kāryamahameva sādhayāmi/",
  "GK20bhasdutu:462": "bhoḥ sudarśana itastāvat/",
  "GK20bhasdutu:463": "tataḥ praviśati sudarśanaḥ/",
  "GK20bhasdutu:464": "sudarśanaḥ",
  "GK20bhasdutu:465": "eṣa bhoḥ/",
  "GK20bhasdutu:466": "śrutvā giraṃ bhagavato vipulaprasādā- nnirdhāvito.asmi parivāritatoyadaughaḥ/",
  "GK20bhasdutu:467": "kasminkhalu",
  "GK20bhasdutu:468": "prakupitaḥ kamalāyatākṣaḥ kasyādya mūrdhani mayā pravijṛmbhitavyam// 42//",
  "GK20bhasdutu:469": "kva nu khalu bhagavānnārāyaṇaḥ/",
  "GK20bhasdutu:470": "avyaktādiracintyātmā lokasaṃrakṣaṇodyataḥ/",
  "GK20bhasdutu:471": "eko.anekavapuḥ",
  "GK20bhasdutu:472": "śrīmāndviṣadbalaniṣūdanaḥ// 43//",
  "GK20bhasdutu:473": "vilokya/aye ayaṃ bhagavānhastinapuradvāre dūtasamudācāreṇopasthitaḥ/ kutaḥ",
  "GK20bhasdutu:474": "khalvāpaḥ kutaḥ",
  "GK20bhasdutu:475": "khalvāpaḥ/ bhagavati ākāśagaṅge āpastāvat/",
  "GK20bhasdutu:476": "hanta sravati/",
  "GK20bhasdutu:477": "ācamyopasṛtya/jayatu",
  "GK20bhasdutu:478": "bhagavānnārāyaṇaḥ/praṇamati/",
  "GK20bhasdutu:479": "vāsudevaḥ",
  "GK20bhasdutu:480": "sudarśana apratihataparākramo bhava/",
  "GK20bhasdutu:481": "sudarśanaḥ",
  "GK20bhasdutu:482": "anugṛhīto.asmi/",
  "GK20bhasdutu:483": "vāsudevaḥ",
  "GK20bhasdutu:484": "diṣṭyā bhavānkarmakāle prāptaḥ/",
  "GK20bhasdutu:485": "sudarśanaḥ",
  "GK20bhasdutu:486": "kathaṃ kathaṃ karmakāla iti/",
  "GK20bhasdutu:487": "ājñāpayatu bhagavānājñāpayatu/",
  "GK20bhasdutu:488": "kiṃ merumandarakulaṃ parivartayāmi saṃkṣobhayāmi sakalaṃ makarālayaṃ vā/",
  "GK20bhasdutu:489": "nakṣatravaṃśamakhilaṃ bhuvi pātayāmi nāśakyamasti mama deva tava prasādāt// 44//",
  "GK20bhasdutu:490": "vāsudevaḥ",
  "GK20bhasdutu:491": "bhoḥ sudarśana itastāvat/",
  "GK20bhasdutu:492": "bhoḥ suyodhana",
  "GK20bhasdutu:493": "yadi lavaṇajalaṃ vā kandaraṃ vā girīṇāṃ grahagaṇacaritaṃ vā vāyumārgaṃ prayāsi/ mama",
  "GK20bhasdutu:495": "bhujabalayogaprāptasaṃjātavegaṃ bhavatu capala cakraṃ kālacakraṃ tavādya// 45//",
  "GK20bhasdutu:496": "sudarśanaḥ",
  "GK20bhasdutu:497": "bhoḥ suyodhanahataka tiṣṭha tiṣṭha/",
  "GK20bhasdutu:498": "punarvicārya/",
  "GK20bhasdutu:499": "prasīdatu bhagavānnārāyṇaḥ/",
  "GK20bhasdutu:500": "mahībhārāpanayanaṃ kartuṃ jātasya bhūtale/",
  "GK20bhasdutu:501": "asminnevaṃ gate deva nanu",
  "GK20bhasdutu:502": "syādviphalaśramaḥ// 46//",
  "GK20bhasdutu:503": "vāsudevaḥ",
  "GK20bhasdutu:504": "sudarśana roṣātsamudācāro nāvekṣitaḥ/",
  "GK20bhasdutu:505": "gamyatāṃ svanilayameva/",
  "GK20bhasdutu:506": "sudarśanaḥ",
  "GK20bhasdutu:507": "yadājñāpayati bhagavānnārāyaṇaḥ/",
  "GK20bhasdutu:508": "kathaṃ kathaṃ gopālaka iti/",
  "GK20bhasdutu:509": "tricaraṇātikrāntatriloko",
  "GK20bhasdutu:510": "nārāyaṇaḥ khalvatra bhagavān/",
  "GK20bhasdutu:511": "śaraṇaṃ vrajantu bhavantaḥ/",
  "GK20bhasdutu:512": "yāvadgacchāmi/ aye",
  "GK20bhasdutu:513": "etadbhagavadāyudhavaraṃ śārṅgaṃ prāptam/",
  "GK20bhasdutu:514": "tanumṛdulalitāṅgaṃ strīsvabhāvopapannaṃ harikaradhṛtamadhyaṃ",
  "GK20bhasdutu:515": "śatrusaṃghaikakālaḥ/",
  "GK20bhasdutu:516": "kanakakhacitapṛṣṭhaṃ bhāti kṛṣṇasya pārśve navasaliladapārśve cāruvidyullekheva// 47//",
  "GK20bhasdutu:518": "bho bhoḥ śārṅga praśāntaroṣo bhagavānnārāyaṇaḥ/",
  "GK20bhasdutu:519": "gamyatāṃ svanilayameva/ hanta",
  "GK20bhasdutu:520": "nivṛttaḥ/",
  "GK20bhasdutu:521": "yāvadgacchāmi/",
  "GK20bhasdutu:522": "aye iyaṃ kaumodakī prāptā/",
  "GK20bhasdutu:523": "maṇikanakavicitrā citramālottarīyā suraripugaṇagātradhvaṃsane jātatṛṣṇā/",
  "GK20bhasdutu:524": "girivarataṭarūpā",
  "GK20bhasdutu:525": "durnivārātivīryā vrajati nabhasi śīghraṃ meghavṛndānuyātrā// 48//",
  "GK20bhasdutu:526": "he kaumodaki praśāntaroṣo bhagavānnārāyaṇaḥ/",
  "GK20bhasdutu:527": "hanta nivṛttā/",
  "GK20bhasdutu:528": "yāvadgacchāmi/ aye ayaṃ",
  "GK20bhasdutu:530": "pāñcajanyaḥ prāptaḥ/",
  "GK20bhasdutu:531": "pūrṇendukundakumudodarahāragauro nārāyaṇānanasarojakṛtaprasādaḥ/",
  "GK20bhasdutu:532": "yasya svanaṃ",
  "GK20bhasdutu:533": "pralayasāgaraghoṣatulyaṃ garbhā niśamya nipatantyasurāṅganānām// 49//",
  "GK20bhasdutu:534": "he pāñcajanya praśāntaroṣo bhagavān/",
  "GK20bhasdutu:535": "gamyatāṃ svanilayameva/",
  "GK20bhasdutu:536": "hanta nivṛttaḥ/ aye",
  "GK20bhasdutu:537": "nandakāsiḥ",
  "GK20bhasdutu:538": "prāptaḥ/",
  "GK20bhasdutu:539": "vanitāvigraho yuddhe mahāsurabhayaṃkaraḥ/",
  "GK20bhasdutu:540": "prayāti gagane śīghraṃ maholkeva",
  "GK20bhasdutu:541": "vibhātyayam// 50//",
  "GK20bhasdutu:542": "he nandaka praśāntaroṣo bhagavān/",
  "GK20bhasdutu:543": "gamyatāṃ svanilayameva/",
  "GK20bhasdutu:544": "hanta nivṛttaḥ/",
  "GK20bhasdutu:545": "yāvadgacchāmi/ aye",
  "GK20bhasdutu:546": "etāni bhagavadāyudhavarāṇi/",
  "GK20bhasdutu:547": "so.ayaṃ khaḍgaḥ kharāṃśorapahasitatanuḥ svaiḥ karairnandakākhyaḥ seyaṃ",
  "GK20bhasdutu:548": "kaumodakī yā",
  "GK20bhasdutu:549": "suraripukaṭhinoraḥsthalakṣodadakṣā/",
  "GK20bhasdutu:550": "saiṣā śārṅgābhidhānā pralayaghanaravajyāravā",
  "GK20bhasdutu:551": "cāpayaṣṭiḥ so.ayaṃ gambhīraghoṣaḥ śaśikaraviśadaḥ śaṅkharāṭpāñcajanyaḥ// 51// he",
  "GK20bhasdutu:552": "śārṅga kaumodaki pāñcajanya daityāntakṛnnandaka śatruvahne/",
  "GK20bhasdutu:553": "praśāntaroṣo",
  "GK20bhasdutu:554": "bhagavānmurāriḥ",
  "GK20bhasdutu:555": "svasthānamevātra hi gaccha tāvat// 52//",
  "GK20bhasdutu:556": "hanta nivṛttāḥ/",
  "GK20bhasdutu:557": "yāvadgacchāmi/",
  "GK20bhasdutu:558": "aye atyuddhūto vāyuḥ/",
  "GK20bhasdutu:559": "atitapatyādityaḥ/ calitāḥ",
  "GK20bhasdutu:560": "parvatāḥ/",
  "GK20bhasdutu:561": "kṣubdhāḥ sāgarāḥ/",
  "GK20bhasdutu:562": "patitā vṛkṣāḥ/",
  "GK20bhasdutu:563": "bhrāntā meghāḥ/",
  "GK20bhasdutu:564": "pralīnā vāsukiprabhṛtayo",
  "GK20bhasdutu:565": "bhujaṃgeśvarāḥ/",
  "GK20bhasdutu:566": "kiṃ nu khalvidam/",
  "GK20bhasdutu:567": "aye ayaṃ bhagavato vāhanaḥ garuḍaḥ prāptaḥ/",
  "GK20bhasdutu:568": "surāsurāṇāṃ parikhedalabdhaṃ yenāmṛtaṃ mātṛvimokṣaṇārthaṃ/",
  "GK20bhasdutu:569": "ācchinnamāsīddviṣato",
  "GK20bhasdutu:570": "murārestvāmudvahāmīti varo.api dattaḥ// 53//",
  "GK20bhasdutu:571": "he kāśyapapriyasuta garuḍa praśāntaroṣo bhagavāndevadeveśaḥ/ gamyatāṃ",
  "GK20bhasdutu:572": "svanilayameva/ hanta",
  "GK20bhasdutu:573": "nivṛttaḥ/ yāvadgacchāmi/",
  "GK20bhasdutu:574": "ete sthitā viyati kiṃnarayakṣasiddhāḥ devāśca saṃbhramacalanmukuṭottamāṅgāḥ/",
  "GK20bhasdutu:575": "ruṣṭe.acyute",
  "GK20bhasdutu:576": "vigatakāntiguṇāḥ praśāntaṃ śrutvā śrayanti sadanāni nivṛttatāpāḥ// 54//",
  "GK20bhasdutu:577": "yāvadahamapi kāntāṃ meruguhāmeva yāsyāmi/",
  "GK20bhasdutu:578": "niṣkrāntaḥ/ vāsudevaḥ",
  "GK20bhasdutu:579": "yāvadahamapi pāṇḍavaśibirameva yāsyāmi/",
  "GK20bhasdutu:580": "nepathye/",
  "GK20bhasdutu:581": "na khalu na khalu gantavyam/",
  "GK20bhasdutu:582": "vāsudevaḥ",
  "GK20bhasdutu:583": "aye vṛddharājasvara iva/",
  "GK20bhasdutu:584": "bho rājan eṣa sthito.asmi/",
  "GK20bhasdutu:585": "tataḥ praviśati dhṛtarāṣṭraḥ/",
  "GK20bhasdutu:586": "dhṛtarāṣṭraḥ",
  "GK20bhasdutu:587": "kva nu khalu bhagavānnārāyaṇaḥ/",
  "GK20bhasdutu:588": "kva nu khalu bhagavānpāṇḍavaśreyaskaraḥ/ kva nu",
  "GK20bhasdutu:589": "khalu",
  "GK20bhasdutu:590": "bhagavānviprapriyaḥ/",
  "GK20bhasdutu:591": "kva nu khalu bhagavāndevakīnandanaḥ/",
  "GK20bhasdutu:592": "mama putrāparādhāttu śārṅgapāṇe tavādhunā/",
  "GK20bhasdutu:593": "etanme tridaśādhyākṣa pādayoḥ",
  "GK20bhasdutu:594": "patitaṃ",
  "GK20bhasdutu:595": "śiraḥ// 55// patati/ vāsudevaḥ",
  "GK20bhasdutu:596": "hā dhik/ patito.atrabhavān/",
  "GK20bhasdutu:597": "uttiṣṭhottiṣṭha/",
  "GK20bhasdutu:598": "dhṛtarāṣṭraḥ",
  "GK20bhasdutu:599": "anugṛhīto.asmi/",
  "GK20bhasdutu:600": "bhagavan idamarghyaṃ pādyaṃ ca pratigṛhyatām/",
  "GK20bhasdutu:601": "vāsudevaḥ",
  "GK20bhasdutu:602": "sarvaṃ gṛhṇāmi/",
  "GK20bhasdutu:603": "kiṃ te bhūyaḥ priyamupaharāmi/",
  "GK20bhasdutu:604": "dhṛtarāṣṭraḥ",
  "GK20bhasdutu:605": "yadi me bhagavānprasannaḥ kimataḥ paramicchāmi/",
  "GK20bhasdutu:606": "vāsudevaḥ",
  "GK20bhasdutu:607": "gacchatu bhavānpunardarśanāya/",
  "GK20bhasdutu:608": "dhṛtarāṣṭraḥ",
  "GK20bhasdutu:609": "yadājñāpayati bhagavannārāyaṇaḥ/",
  "GK20bhasdutu:610": "niṣkrāntaḥ/",
  "GK20bhasdutu:611": "bharatavākyam/",
  "GK20bhasdutu:612": "imāṃ sāgaraparyantāṃ himavadvindhyakuṇḍalām/",
  "GK20bhasdutu:613": "mahīmekātapatrāṅkāṃ rājasiṃhaḥ praśāstu naḥ// 56//",
  "GK20bhasdutu:615": "niṣkrāntāḥ sarve/",
  "GK20bhasdutu:616": "dūtavākyaṃ",
  "GK20bhasdutu:617": "samāptam/",
  "GK20bhasdutu:618": "ka iti/ tricaraṇātikrāntatriloko",
  "GK20bhasdutu:619": "nārāyaṇaḥ khalvatra bhagavān/",
  "GK20bhasdutu:620": "śaraṇaṃ vrajantu bhavantaḥ/ yāvadga"
}