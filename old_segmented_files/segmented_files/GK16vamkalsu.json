{
  "GK16vamkalsu:0": "pāṇḍitavaraśrīvāmanaviracitāni",
  "GK16vamkalsu:1": "kāvyālaṅkārasūtrāṇi",
  "GK16vamkalsu:2": "svavṛttisametāni @#--------------------",
  "GK16vamkalsu:4": "śārīraṃ nāma prathamādhikaraṇam /",
  "GK16vamkalsu:5": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:6": "praṇamya paramaṃ jyotirvāmanena kavipriyā /",
  "GK16vamkalsu:7": "kāvyālaṅkārasūtrāṇāṃ teṣāṃ vṛttirvidhīyate //",
  "GK16vamkalsu:8": "kāvyaṃ grāhyamalaṅkārāt // VKal_1,1.1 //",
  "GK16vamkalsu:9": "saundaryamalaṅkāraḥ // VKal_1,1.2 //",
  "GK16vamkalsu:10": "sa doṣaguṇālaṅkārahānādānābhyām // VKal_1,1.3 //",
  "GK16vamkalsu:11": "śāstrataste // VKal_1,1.4 //",
  "GK16vamkalsu:12": "kāvyaṃ saddṛṣṭādṛṣṭārtham,",
  "GK16vamkalsu:13": "prītikīrtihetutvāt // VKal_1,1.5 //* @#--------------------",
  "GK16vamkalsu:15": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:16": "arocakinaḥ satṛṇābhyavahāriṇaśca kavayaḥ // VKal_1,2.1 //",
  "GK16vamkalsu:17": "pūrve śiṣyāḥ,",
  "GK16vamkalsu:18": "vivekitvāt // VKal_1,2.2 //*",
  "GK16vamkalsu:19": "netare, tadviparyayāt // VKal_1,2.3 //*",
  "GK16vamkalsu:20": "na śāstramadravyeṣvarthavat // VKal_1,2.4 //",
  "GK16vamkalsu:21": "na katakaṃ paṅkaprasādanāya // VKal_1,2.5 //",
  "GK16vamkalsu:22": "rītirātmā kāvyasya // VKal_1,2.6 //",
  "GK16vamkalsu:23": "viśiṣṭā padaracanā rītiḥ // VKal_1,2.7 //",
  "GK16vamkalsu:24": "viśeṣo guṇātmā // VKal_1,2.8 //",
  "GK16vamkalsu:25": "sā tridhā --- vaidarbhī gauḍīyā pāñcālī ceti // VKal_1,2.9 //",
  "GK16vamkalsu:26": "vidarbhādiṣu dṛṣṭatvāttatsamākhyā // VKal_1,2.10 //",
  "GK16vamkalsu:27": "samagraguṇopetā vaidarbhī // VKal_1,2.11 //",
  "GK16vamkalsu:28": "ojaḥkāntimatī gauḍīyā // VKal_1,2.12 //",
  "GK16vamkalsu:29": "mādhuryasaukumāryopapannā pāñcālī // VKal_1,2.13 //",
  "GK16vamkalsu:30": "tāsāṃ pūrvā grāhyā,",
  "GK16vamkalsu:31": "guṇasākalyāt // VKal_1,2.14 //*",
  "GK16vamkalsu:32": "na punaritare,",
  "GK16vamkalsu:33": "stokaguṇatvāt // VKal_1,2.15 //*",
  "GK16vamkalsu:34": "tadārohaṇārthamitarābhyāsa ityeke // VKal_1,2.16 //",
  "GK16vamkalsu:35": "tattu na; atattvaśīlasya tattvāniṣpatteḥ // VKal_1,2.17 //*",
  "GK16vamkalsu:36": "na śaṇasūtravānābhyāse trasarasūtravānavaicitryalābhaḥ // VKal_1,2.18 //",
  "GK16vamkalsu:37": "sāpi samāsābhāve śuddhavaidarbhī // VKal_1,2.19 //",
  "GK16vamkalsu:38": "tasyāmarthaguṇasaṃpadāsvādyā // VKal_1,2.20 //",
  "GK16vamkalsu:39": "tadupārohādarthaguṇaleśo 'pi // VKal_1,2.21 //",
  "GK16vamkalsu:40": "sāpi vaidarbhī,",
  "GK16vamkalsu:41": "tātsthyāt // VKal_1,2.22 //* @#--------------------",
  "GK16vamkalsu:43": "tṛtīyo 'dhyāyaḥ /",
  "GK16vamkalsu:44": "loko vidyā prakīrṇa ca kāvyāṅgāni // VKal_1,3.1 //",
  "GK16vamkalsu:45": "lokavṛttaṃ lokaḥ // VKal_1,3.2 //",
  "GK16vamkalsu:46": "śabdasmṛtyabhidhānakośacchandovicitikalākāmaśāstradaṇḍanītipūrvā vidyāḥ // VKal_1,3.3 //",
  "GK16vamkalsu:47": "śabdasmṛteḥ śabdaśuddhiḥ // VKal_1,3.4 //",
  "GK16vamkalsu:48": "abhidhānakośāt padārthaniścayaḥ // VKal_1,3.5 //",
  "GK16vamkalsu:49": "chandovicitervṛttasaṃśayacchedaḥ // VKal_1,3.6 //",
  "GK16vamkalsu:50": "kalāśāstrebhyaḥ kalātattvasya saṃvit // VKal_1,3.7 //",
  "GK16vamkalsu:51": "kāmaśāstrataḥ kāmopacārasya // VKal_1,3.8 //",
  "GK16vamkalsu:52": "daṇḍanīternayāpanayayoḥ // VKal_1,3.9 //",
  "GK16vamkalsu:53": "itivṛttakuṭilatvaṃ ca tataḥ // VKal_1,3.10 //",
  "GK16vamkalsu:54": "lakṣyajñatvamabhiyogo vṛddhasevāvekṣaṇaṃ pratibhānamavadhānaṃ ca prakīrṇam // VKal_1,3.11 //",
  "GK16vamkalsu:55": "tatra kāvyaparicayo lakṣyajñatvam // VKal_1,3.12 //",
  "GK16vamkalsu:56": "kāvyabandhodyamo 'bhiyogaḥ // VKal_1,3.13 //",
  "GK16vamkalsu:57": "kāvyopadeśaguruśuśrūṣaṇaṃ vṛddhasevā // VKal_1,3.14 //",
  "GK16vamkalsu:58": "padādhānoddharaṇamavekṣaṇam // VKal_1,3.15 //",
  "GK16vamkalsu:59": "kavitvabījaṃ pratibhānam // VKal_1,3.16 //",
  "GK16vamkalsu:60": "cittaikāgryamavadhānam // VKal_1,3.17 //",
  "GK16vamkalsu:61": "taddeśakālābhyām // VKal_1,3.18 //",
  "GK16vamkalsu:62": "vivikto deśaḥ // VKal_1,3.19 //",
  "GK16vamkalsu:63": "rātriyāmasturīyaḥ kālaḥ // VKal_1,3.20 //",
  "GK16vamkalsu:64": "kāvyaṃ gadyaṃ padyaṃ ca // VKal_1,3.21 //",
  "GK16vamkalsu:65": "gadyaṃ vṛttagandhi cūrṇamutkalikāprāyaṃ ca // VKal_1,3.22 //",
  "GK16vamkalsu:66": "padyabhāgavadvṛttagandhi // VKal_1,3.23 //",
  "GK16vamkalsu:67": "anāviddhalalitapadaṃ cūrṇam // VKal_1,3.24 //",
  "GK16vamkalsu:68": "viparītamutkalikāprāyam // VKal_1,3.25 //",
  "GK16vamkalsu:69": "padyamanekabhedam // VKal_1,3.26 //",
  "GK16vamkalsu:70": "tadanibaddhaṃ nibaddhaṃ ca // VKal_1,3.27 //",
  "GK16vamkalsu:71": "kramasiddhistayoḥ sraguttaṃsavat // VKal_1,3.28 //",
  "GK16vamkalsu:72": "nānibaddhaṃ cakāstyekatejaḥparamāṇuvat // VKal_1,3.29 //",
  "GK16vamkalsu:73": "saṃdarbheṣu daśarūpakaṃ śreyaḥ // VKal_1,3.30 //",
  "GK16vamkalsu:74": "taddhi citraṃ citrapaṭavadviśeṣasākalyāt // VKal_1,3.31 //",
  "GK16vamkalsu:75": "tato 'nyabhedakḷptiḥ // VKal_1,3.32 //",
  "GK16vamkalsu:76": "samāptaṃ cedaṃ śārīraṃ prathamamadhikaraṇam / @#-------------------------------------------------------",
  "GK16vamkalsu:78": "doṣadarśanaṃ nāma dvitīyamadhikaraṇam /",
  "GK16vamkalsu:79": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:80": "guṇaviparyayātmāno doṣāḥ // VKal_2,1.1 //",
  "GK16vamkalsu:81": "arthatastadavagamaḥ // VKal_2,1.2 //",
  "GK16vamkalsu:82": "saukaryāya prapañcaḥ // VKal_2,1.3 //",
  "GK16vamkalsu:83": "duṣṭaṃ padamasādhu kaṣṭaṃ grāmyamapratītamanarthakaṃ ca // VKal_2,1.4 //",
  "GK16vamkalsu:84": "śabdasmṛtiviruddhamasādhu // VKal_2,1.5 //",
  "GK16vamkalsu:85": "śrutivirasaṃ kaṣṭam // VKal_2,1.6 //",
  "GK16vamkalsu:86": "lokamātraprayuktaṃ grāmyam // VKal_2,1.7 //",
  "GK16vamkalsu:87": "śāstramātraprayuktamapratītam // VKal_2,1.8 //",
  "GK16vamkalsu:88": "pūraṇārthamanarthakam // VKal_2,1.9 //",
  "GK16vamkalsu:89": "na vākyālaṅkārārtham // VKal_2,1.10 //",
  "GK16vamkalsu:90": "anyārthaneyagūḍhārthāślīlakliṣṭāni ca // VKal_2,1.11 //",
  "GK16vamkalsu:91": "rūḍhicyutamanyārtham // VKal_2,1.12 //",
  "GK16vamkalsu:92": "kalpitārthaṃ neyārtham // VKal_2,1.13 //",
  "GK16vamkalsu:93": "aprasiddhārthaprayuktaṃ gūḍhārtham // VKal_2,1.14 //",
  "GK16vamkalsu:94": "asamyārthāntaramasamyasmṛtihetuścāślīlam // VKal_2,1.15 //",
  "GK16vamkalsu:95": "na guptalakṣitasaṃvṛtāni // VKal_2,1.16 //",
  "GK16vamkalsu:96": "aprasiddhāsabhyaṃ guptam // VKal_2,1.17 //",
  "GK16vamkalsu:97": "lākṣaṇikāsabhyānvitaṃ lakṣitam // VKal_2,1.18 //",
  "GK16vamkalsu:98": "lokasaṃvītaṃ saṃvṛtam // VKal_2,1.19 //",
  "GK16vamkalsu:99": "tantraividhyam,",
  "GK16vamkalsu:100": "vrīḍājugupsāmaṅgalātaṅkadāyibhedāt // VKal_2,1.20 //*",
  "GK16vamkalsu:101": "vyavahitārthapratyayaṃ kliṣṭam // VKal_2,1.21 //",
  "GK16vamkalsu:102": "antyābhyāṃ vākyaṃ vyākhyātam // VKal_2,1.22 // @#--------------------",
  "GK16vamkalsu:104": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:105": "bhinnavṛttayatibhraṣṭavisaṃdhīni vākyāni // VKal_2,2.1 //",
  "GK16vamkalsu:106": "svalakṣaṇacyutavṛttaṃ bhinnavṛttam // VKal_2,2.2 //",
  "GK16vamkalsu:107": "virasavirāmaṃ yatibhraṣṭam // VKal_2,2.3 //",
  "GK16vamkalsu:108": "taddhātunāmabhāgabhede svarasaṃdhyakṛte prāyeṇa // VKal_2,2.4 //",
  "GK16vamkalsu:109": "na vṛttadoṣātpṛthagyatidpṣaḥ,",
  "GK16vamkalsu:110": "vṛttasya yatyātmakatvāt // VKal_2,2.5 //*",
  "GK16vamkalsu:111": "na, lakṣaṇaḥ pṛthaktvāt // VKal_2,2.6 //*",
  "GK16vamkalsu:112": "virūpapadasaṃdhi visaṃdhiḥ // VKal_2,2.7 //",
  "GK16vamkalsu:113": "padasaṃdhervairūpyaṃ viśleṣo 'ślīlatvaṃ kaṣṭatvaṃ ca // VKal_2,2.8 //",
  "GK16vamkalsu:114": "vyarthaikārthasaṃdigdhāyuktāpakramalokavidyā viruddhāni ca // VKal_2,2.9 //",
  "GK16vamkalsu:115": "vyāhatapūrvottarārthaṃ vyartham // VKal_2,2.10 //",
  "GK16vamkalsu:116": "uktārthapadamekārtham // VKal_2,2.11 //",
  "GK16vamkalsu:117": "na, viśeṣaścedekārthaṃ duṣṭam // VKal_2,2.12 //*",
  "GK16vamkalsu:118": "dhanurjyādhvanau dhanuḥśrutirārūḍheḥ pratipattyai // VKal_2,2.13 //",
  "GK16vamkalsu:119": "karṇāvataṃsaśravaṇakuṇḍalaśiraḥśekhareṣu karṇādinirdeśaḥ saṃnidheḥ // VKal_2,2.14 //",
  "GK16vamkalsu:120": "muktāhāraśabde muktāśabdaḥ śuddheḥ // VKal_2,2.15 //",
  "GK16vamkalsu:121": "puṣpamālāśabde puṣpapadamutkarṣasya // VKal_2,2.16 //",
  "GK16vamkalsu:122": "karikalabhaśabde kariśabdastādrūpyasya // VKal_2,2.17 //",
  "GK16vamkalsu:123": "viśeṣaṇasya ca // VKal_2,2.18 //",
  "GK16vamkalsu:124": "tadidaṃ prayukteṣu // VKal_2,2.19 //",
  "GK16vamkalsu:125": "saṃśayakṛtsaṃdigdham // VKal_2,2.20 //",
  "GK16vamkalsu:126": "māyādivikalpitārthamprayuktam // VKal_2,2.21 //",
  "GK16vamkalsu:127": "kramahīnārthamapakramam // VKal_2,2.22 //",
  "GK16vamkalsu:128": "deśakālasvabhāvaviruddhārthāni lokaviruddhāni // VKal_2,2.23 //",
  "GK16vamkalsu:129": "kalācaturvargaśāstraviruddhārthāni vidyāviruddhāni // VKal_2,2.24 //",
  "GK16vamkalsu:130": "samāotaṃ cedaṃ doṣadarśanam dvitīyamadhikaraṇam / @#-----------------------------------------------",
  "GK16vamkalsu:132": "guṇavivecanaṃ nāma tṛtīyamadhikaraṇam /",
  "GK16vamkalsu:133": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:134": "kāvyaśobhāyāḥ kartāro dharmā guṇāḥ // VKal_3,1.1 //",
  "GK16vamkalsu:135": "tadatiśayahetavastvalaṅkārāḥ // VKal_3,1.2 //",
  "GK16vamkalsu:136": "pūrve nityāḥ // VKal_3,1.3 //",
  "GK16vamkalsu:137": "ojaḥprasādaśleṣasamatāsamādhimādhuryasaukumāryodāratārthavyaktikāntayo bandhaguṇāḥ // VKal_3,1.4 //",
  "GK16vamkalsu:138": "gāḍhabandhatvamojaḥ // VKal_3,1.5 //",
  "GK16vamkalsu:139": "śaithilyaṃ prasādaḥ // VKal_3,1.6 //",
  "GK16vamkalsu:140": "guṇaḥ saṃplavāt // VKal_3,1.7 //",
  "GK16vamkalsu:141": "sa tvanubhavasiddhaḥ // VKal_3,1.8 //",
  "GK16vamkalsu:142": "sāmyotkarṣau ca // VKal_3,1.9 //",
  "GK16vamkalsu:143": "masṛṇatvaṃ śleṣaḥ // VKal_3,1.10 //",
  "GK16vamkalsu:144": "mārgāvhedaḥ samatā // VKal_3,1.11 //",
  "GK16vamkalsu:145": "ārohāvarohakramaḥ samādhiḥ // VKal_3,1.12 //",
  "GK16vamkalsu:146": "na pṛthak, ārohāvarohayorojaḥprasādarūpatvāt // VKal_3,1.13 //*",
  "GK16vamkalsu:147": "na, asaṃpṛktatvāt // VKal_3,1.14 //*",
  "GK16vamkalsu:148": "anaikāntyācca // VKal_3,1.15 //",
  "GK16vamkalsu:149": "ojaḥprasādayoḥ tīvrāvasthā,",
  "GK16vamkalsu:150": "tāviti cedabhyupagamaḥ // VKal_3,1.16 //*",
  "GK16vamkalsu:151": "viśeṣāpekṣitvāttayoḥ // VKal_3,1.17 //",
  "GK16vamkalsu:152": "ārohāvarohanimittaṃ samādhirākhyāyate // VKal_3,1.18 //",
  "GK16vamkalsu:153": "kramavidhānārthaṃ vā // VKal_3,1.19 //",
  "GK16vamkalsu:154": "pṛthakpadatvaṃ mādhuryam // VKal_3,1.20 //",
  "GK16vamkalsu:155": "ajaraṭhatvaṃ saukumāryam // VKal_3,1.21 //",
  "GK16vamkalsu:156": "vikaṭatvamudāratā // VKal_3,1.22 //",
  "GK16vamkalsu:157": "arthavyaktihetutvamarthavyaktiḥ // VKal_3,1.23 //",
  "GK16vamkalsu:158": "aujjvalyaṃ kāntiḥ // VKal_3,1.24 //",
  "GK16vamkalsu:159": "nāsantaḥ, sadvedyatvāt // VKal_3,1.25 //*",
  "GK16vamkalsu:160": "na bhrāntāḥ,",
  "GK16vamkalsu:161": "niṣkampatvāt // VKal_3,1.26 //*",
  "GK16vamkalsu:162": "na pāṭhadharmāḥ,",
  "GK16vamkalsu:163": "sarvatrādṛṣṭeḥ // VKal_3,1.27 //* @#--------------------",
  "GK16vamkalsu:165": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:166": "ta evārthaguṇāḥ // VKal_3,2.1 //",
  "GK16vamkalsu:167": "arthasya prauḍhirojaḥ // VKal_3,2.2 //",
  "GK16vamkalsu:168": "arthavaimalyaṃ prasādaḥ // VKal_3,2.3 //",
  "GK16vamkalsu:169": "ghaṭanā śleṣaḥ // VKal_3,2.4 //",
  "GK16vamkalsu:170": "avaiṣamyaṃ samatā // VKal_3,2.5 //",
  "GK16vamkalsu:171": "arthadṛṣṭiḥ samādhiḥ // VKal_3,2.6 //",
  "GK16vamkalsu:172": "artho dvividhaḥ,",
  "GK16vamkalsu:173": "ayoniranyacchāyāyonirvā // VKal_3,2.7 //*",
  "GK16vamkalsu:174": "artho vyaktaḥ sūkṣmaśca // VKal_3,2.8 //",
  "GK16vamkalsu:175": "sūkṣmo bhāvyo vāsanīyaśca // VKal_3,2.9 //",
  "GK16vamkalsu:176": "uktivaicitryaṃ mādhuryam // VKal_3,2.10 //",
  "GK16vamkalsu:177": "apāruṣyaṃ saukumāryam // VKal_3,2.11 //",
  "GK16vamkalsu:178": "agrāmyatvamudāratā // VKal_3,2.12 //",
  "GK16vamkalsu:179": "vastusvabhāvasphuṭatvamarthavyaktiḥ // VKal_3,2.13 //",
  "GK16vamkalsu:180": "dīptarasatvaṃ kāntiḥ // VKal_3,2.14 //",
  "GK16vamkalsu:181": "samāotaṃ cedaṃ guṇavivecanaṃ tṛtīyamadhikaraṇam / @#-----------------------------------------------",
  "GK16vamkalsu:183": "ālaṅkārikaṃ nāma caturthamadhikaraṇam /",
  "GK16vamkalsu:184": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:185": "padamanaikārthamakṣaraṃ cāvṛttaṃ sthānaniyame yamakam // VKal_4,1.1 //",
  "GK16vamkalsu:186": "pādāḥ pādasyaikasyānekasya cādimadhyāntabhāgāḥ sthānāni // VKal_4,1.2 //",
  "GK16vamkalsu:187": "bhaṅgādutkarṣaḥ // VKal_4,1.3 //",
  "GK16vamkalsu:188": "śṛṅkhalā parivartakaścūrṇamiti bhaṅgamārgaḥ // VKal_4,1.4 //",
  "GK16vamkalsu:189": "varṇavicchedacalanaṃ śṛṅkhalā // VKal_4,1.5 //",
  "GK16vamkalsu:190": "saṅgavinivṛttau svarūpāpattiḥ parivartakaḥ // VKal_4,1.6 //",
  "GK16vamkalsu:191": "piṇḍākṣarabhede svarūpalopaścūrṇam // VKal_4,1.7 //",
  "GK16vamkalsu:192": "śeṣaḥ sarūpo 'nuprāsaḥ // VKal_4,1.8 //",
  "GK16vamkalsu:193": "anulbaṇo varṇānuprāsaḥ śreyān // VKal_4,1.9 //",
  "GK16vamkalsu:194": "pādānuprāsaḥ pādayamakavat // VKal_4,1.10 // @#--------------------",
  "GK16vamkalsu:196": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:197": "upamānenopameyasya guṇaleśataḥ sāmyamupamā // VKal_4,2.1 //",
  "GK16vamkalsu:198": "guṇabāhulyataśca kalpitā // VKal_4,2.2 //",
  "GK16vamkalsu:199": "taddvaividhyam,",
  "GK16vamkalsu:200": "padavākyārthavṛttibhedāt // VKal_4,2.3 //*",
  "GK16vamkalsu:201": "sā pūṇā luptā ca // VKal_4,2.4 //",
  "GK16vamkalsu:202": "guṇadyotakopamānopameyaśabdānāṃ sāmagrye pūrṇā // VKal_4,2.5 //",
  "GK16vamkalsu:203": "lope luptā // VKal_4,2.6 //",
  "GK16vamkalsu:204": "stutinindātattvākhyāneṣu // VKal_4,2.7 //",
  "GK16vamkalsu:205": "hīnatvādhikatvaliṅgavacanabhedāsādṛśyāsaṃbhavāstaddoṣāḥ // VKal_4,2.8 //",
  "GK16vamkalsu:206": "jātipramāṇadharmanyūnatopamānasya hīnatvam // VKal_4,2.9 //",
  "GK16vamkalsu:207": "dharmayorekanirdeśe 'nyasya saṃvit,",
  "GK16vamkalsu:208": "sāhacaryāt // VKal_4,2.10 //*",
  "GK16vamkalsu:209": "tenādhikarvaṃ vyākhyātam // VKal_4,2.11 //",
  "GK16vamkalsu:210": "upamānopameyayorliṅgavyatyāso liṅgabhedaḥ // VKal_4,2.12 //",
  "GK16vamkalsu:211": "iṣṭaḥ puṃpuṃsakayoḥ prāyeṇa // VKal_4,2.13 //",
  "GK16vamkalsu:212": "laukikyāṃ samāsābhihitāyāmupamāprapañce ca // VKal_4,2.14 //",
  "GK16vamkalsu:213": "tena vacanabhedo vyākhyātaḥ // VKal_4,2.15 //",
  "GK16vamkalsu:214": "apratītaguṇasādṛśyamasādṛśyam // VKal_4,2.16 //",
  "GK16vamkalsu:215": "asādṛśyahatā hyupamā,",
  "GK16vamkalsu:216": "tanniṣṭhāśca kavayaḥ // VKal_4,2.17 //*",
  "GK16vamkalsu:217": "upamānādhikyāttadapoha ityeke // VKal_4,2.18 //",
  "GK16vamkalsu:218": "na, apuṣṭārthatvāt // VKal_4,2.19 //*",
  "GK16vamkalsu:219": "anupapattirasaṃbhavaḥ // VKal_4,2.20 //",
  "GK16vamkalsu:220": "na viruddho 'tiśayaḥ // VKal_4,2.21 // @#--------------------",
  "GK16vamkalsu:222": "tṛtīyo 'dhyāyaḥ /",
  "GK16vamkalsu:223": "prativastuprabhṛtirupamāprapañcaḥ // VKal_4,3.1 //",
  "GK16vamkalsu:224": "upameyasyoktau samānavastunyāsaḥ prativastu // VKal_4,3.2 //",
  "GK16vamkalsu:225": "anuktau samāsoktiḥ // VKal_4,3.3 //",
  "GK16vamkalsu:226": "kiñciduktāvaprastutapraśaṃsā // VKal_4,3.4 //",
  "GK16vamkalsu:227": "samena vastunānyāpalāpo 'pahnutiḥ // VKal_4,3.5 //",
  "GK16vamkalsu:228": "upamānenopameyasya guṇasāmyāttattvāropo rūpakam // VKal_4,3.6 //",
  "GK16vamkalsu:229": "sa dharmeṣu tantraprayoge śleṣaḥ // VKal_4,3.7 //",
  "GK16vamkalsu:230": "sādṛśyāllakṣaṇā vakroktiḥ // VKal_4,3.8 //",
  "GK16vamkalsu:231": "atadrūpasyānyathādhyavasānamatiśayārthamutprekṣā // VKal_4,3.9 //",
  "GK16vamkalsu:232": "saṃbhāvyadharmatadutkarṣakalpanātiśayoktiḥ // VKal_4,3.10 //",
  "GK16vamkalsu:233": "upamānopameyasaṃśayaḥ saṃdehaḥ // VKal_4,3.11 //",
  "GK16vamkalsu:234": "viruddhābhāsatvaṃ virodhaḥ // VKal_4,3.12 //",
  "GK16vamkalsu:235": "kriyāpratiṣedhe prasiddhatatphalavyaktirvibhāvanā // VKal_4,3.13 //",
  "GK16vamkalsu:236": "ekasyopameyatvopamānatve 'nanvayaḥ // VKal_4,3.14 //",
  "GK16vamkalsu:237": "krameṇopameyopamā // VKal_4,3.15 //",
  "GK16vamkalsu:238": "samavisadṛśābhyāṃ parivartanaṃ parivṛttiḥ // VKal_4,3.16 //",
  "GK16vamkalsu:239": "upameyopamānānāṃ kramasaṃbandhaḥ kramaḥ // VKal_4,3.17 //",
  "GK16vamkalsu:240": "upamānopameyavākyeṣvekā kriyā dīpakam // VKal_4,3.18 //",
  "GK16vamkalsu:241": "tantrividham /",
  "GK16vamkalsu:242": "ādimadhyāntavākyavṛttibhedāt // VKal_4,3.19 //*",
  "GK16vamkalsu:243": "kriyayaiva svatadarthānvayakhyāpanaṃ nidarśanam // VKal_4,3.20 //",
  "GK16vamkalsu:244": "uktisiddhyai vastuno 'rthāntarasyaiva nyasanamarthāntaranyāsaḥ // VKal_4,3.21 //",
  "GK16vamkalsu:245": "upameyasya guṇātirekatvaṃ vyatirekaḥ // VKal_4,3.22 //",
  "GK16vamkalsu:246": "ekaguṇahānikalpanāyāṃ sāmyadārḍhyaṃ viśeṣoktiḥ // VKal_4,3.23 //",
  "GK16vamkalsu:247": "saṃbhāvyaviśiṣṭakarmākaraṇānnindāstotrārthā vyājastutiḥ // VKal_4,3.24 //",
  "GK16vamkalsu:248": "vyājasya satyasārūpyaṃ vyājoktiḥ // VKal_4,3.25 //",
  "GK16vamkalsu:249": "viśiṣṭena sāmyārthamekakālakriyāyogastulyayogitā // VKal_4,3.26 //",
  "GK16vamkalsu:250": "upamānākṣepaścākṣepaḥ // VKal_4,3.27 //",
  "GK16vamkalsu:251": "vastudvayakriyayostulyakālayorekapadābhidhānaṃ sahoktiḥ // VKal_4,3.28 //",
  "GK16vamkalsu:252": "yatsādṛśyaṃ tatsaṃpattiḥ samāhitam // VKal_4,3.29 //",
  "GK16vamkalsu:253": "alaṅkārasyālaṅkārayonitvaṃ saṃsṛṣṭiḥ // VKal_4,3.30 //",
  "GK16vamkalsu:254": "tadbhedāvupamārūpakotprekṣāvayavau // VKal_4,3.31 //",
  "GK16vamkalsu:255": "upamājanyaṃ rūpakamupamārūpakam // VKal_4,3.32 //",
  "GK16vamkalsu:256": "utprekṣāheturutprekṣāvayavaḥ // VKal_4,3.33 //",
  "GK16vamkalsu:257": "samāotaṃ cedamālaṅkārikaṃ caturthamadhikaraṇam / @#-----------------------------------------------",
  "GK16vamkalsu:259": "prāyogikaṃ nāma pañcamamadhikaraṇam /",
  "GK16vamkalsu:260": "prathamo 'dhyāyaḥ /",
  "GK16vamkalsu:261": "naikaṃ padaṃ dviḥ prayojyaṃ prāyeṇa // VKal_5,1.1 //",
  "GK16vamkalsu:262": "nityā saṃhitaikapadavatpādeṣvardhāntavarjam // VKal_5,1.2 //",
  "GK16vamkalsu:263": "na pādāntalaghorgurutvaṃ ca sarvatra // VKal_5,1.3 //",
  "GK16vamkalsu:264": "na gādye samāptaprāyaṃ vṛttam,",
  "GK16vamkalsu:265": "anyatrodgatādibhyaḥ saṃvādāt // VKal_5,1.4 //*",
  "GK16vamkalsu:266": "na pādādau khalvādayaḥ // VKal_5,1.5 //",
  "GK16vamkalsu:267": "nārdhe kiñcitsamāptaṃ vākyam // VKal_5,1.6 //",
  "GK16vamkalsu:268": "na karmadhārayo bahuvrīhipratipattikaraḥ // VKal_5,1.7 //",
  "GK16vamkalsu:269": "tena viparyayo vyākhyātaḥ // VKal_5,1.8 //",
  "GK16vamkalsu:270": "saṃbhāvyaniṣedhanivartane dvau pratiṣedhau // VKal_5,1.9 //",
  "GK16vamkalsu:271": "viśeṣaṇamātraprayogo viśeṣyapratipattau // VKal_5,1.10 //",
  "GK16vamkalsu:272": "sarvanāmnānusaṃdhirvṛtticchannasya // VKal_5,1.11 //",
  "GK16vamkalsu:273": "saṃbandhasaṃbandhe 'pi ṣaṣṭhī kvacit // VKal_5,1.12 //",
  "GK16vamkalsu:274": "atiprayuktaṃ deśabhāṣāpadam // VKal_5,1.13 //",
  "GK16vamkalsu:275": "liṅgādhyāhārau // VKal_5,1.14 //",
  "GK16vamkalsu:276": "lakṣaṇāśabdāśca // VKal_5,1.15 //",
  "GK16vamkalsu:277": "na tadbāhulyamekatra // VKal_5,1.16 //",
  "GK16vamkalsu:278": "stanādīnāṃ dvitvāviṣṭā jātiḥ prāyeṇa // VKal_5,1.17 // @#--------------------",
  "GK16vamkalsu:280": "dvitīyo 'dhyāyaḥ /",
  "GK16vamkalsu:281": "rudrāvityekaśeṣo 'nveṣyaḥ // VKal_5,2.1 //",
  "GK16vamkalsu:282": "miliklabikṣapiprabhṛtīnāṃ dhātutvam,",
  "GK16vamkalsu:283": "dhātuguṇasyāsamāpteḥ // VKal_5,2.2 //*",
  "GK16vamkalsu:284": "valerātmanepadamanityam,",
  "GK16vamkalsu:285": "jñāpakāt // VKal_5,2.3 //*",
  "GK16vamkalsu:286": "kṣīyata iti karmakartari // VKal_5,2.4 //",
  "GK16vamkalsu:287": "khidyata iti ca // VKal_5,2.5 //",
  "GK16vamkalsu:288": "mārgerātmaepadamalakṣma // VKal_5,2.6 //",
  "GK16vamkalsu:289": "lolamānādayaścānaśi // VKal_5,2.7 //",
  "GK16vamkalsu:290": "labhergatyarthatvāṇṇicyaṇau kartuḥ karmatvākarmatve // VKal_5,2.8 //",
  "GK16vamkalsu:291": "te-me-śabdau nipāteṣu // VKal_5,2.9 //*",
  "GK16vamkalsu:292": "tiraskṛta iti paribhūte,",
  "GK16vamkalsu:293": "antardhyupacārāt // VKal_5,2.10 //*",
  "GK16vamkalsu:294": "naikaśabdaḥ,",
  "GK16vamkalsu:295": "supsupeti samāsāt // VKal_5,2.11 //*",
  "GK16vamkalsu:296": "madhupipāsuprabhṛtīnāṃ samāso gamigāmyādiṣu pāṭhāt // VKal_5,2.12 //",
  "GK16vamkalsu:297": "trivalīśabdaḥ siddhaḥ saṃjñā cet // VKal_5,2.13 //",
  "GK16vamkalsu:298": "bimbādhara iti vṛttau madhyamapadalopinyām // VKal_5,2.14 //",
  "GK16vamkalsu:299": "āmūlalolādiṣu vṛttirvispaṣṭapaṭuvat // VKal_5,2.15 //",
  "GK16vamkalsu:300": "na dhānyaṣaṣṭhādiṣu ṣaṣṭhīsamāsapratiṣedhaḥ,",
  "GK16vamkalsu:301": "pūraṇena taddhitāntatvāt // VKal_5,2.16 //*",
  "GK16vamkalsu:302": "patrapītimādiṣu guṇavacanena // VKal_5,2.17 //",
  "GK16vamkalsu:303": "avarjyo bahuvrīhirvyadhikaraṇo janmādyuttarapadaḥ // VKal_5,2.18 //",
  "GK16vamkalsu:304": "hastāgrāgrahastādayo guṇaguṇinorbhedābhedāt // VKal_5,2.19 //",
  "GK16vamkalsu:305": "pūrvanipāte 'pabhraṃśo rakṣyaḥ // VKal_5,2.20 //",
  "GK16vamkalsu:306": "nipātenāpyabhihite karmaṇi na karmavibhaktiḥ,",
  "GK16vamkalsu:307": "parigaṇanasya prāyikatvāt // VKal_5,2.21 //*",
  "GK16vamkalsu:308": "śakyamiti rūpaṃ karmābhidhāyāṃ liṅgavacanasyāpi sāmānyopakramāt // VKal_5,2.22 //",
  "GK16vamkalsu:309": "hānivadādhikyamapyaṅgānāṃ vikāraḥ // VKal_5,2.23 //",
  "GK16vamkalsu:310": "na kṛmikīṭānāmiti,",
  "GK16vamkalsu:311": "ekavadbhāvaprasaṅgāt // VKal_5,2.24 //*",
  "GK16vamkalsu:312": "na kharoṣṭrau,",
  "GK16vamkalsu:313": "uṣṭrakharamiti pāṭhāt // VKal_5,2.25 //*",
  "GK16vamkalsu:314": "āsetyasateḥ // VKal_5,2.26 //",
  "GK16vamkalsu:315": "yudhyediti yudhaḥ kyaci // VKal_5,2.27 //",
  "GK16vamkalsu:316": "viralāyamānādiṣu kyaṅ nirūpyaḥ // VKal_5,2.28 //",
  "GK16vamkalsu:317": "ahetau hanterṇic,",
  "GK16vamkalsu:318": "curādipāṭhāt // VKal_5,2.29 //*",
  "GK16vamkalsu:319": "anucarīti careṣṭittvāt // VKal_5,2.30 //",
  "GK16vamkalsu:320": "kesarālamityalateraṇi // VKal_5,2.31 //",
  "GK16vamkalsu:321": "patralamiti lāteḥ ke // VKal_5,2.32 //",
  "GK16vamkalsu:322": "mahīdhrādayo mūlavibhujādidarśanāt // VKal_5,2.33 //",
  "GK16vamkalsu:323": "brahmādiṣu hanterniyamādarihādyasiddhiḥ // VKal_5,2.34 //",
  "GK16vamkalsu:324": "brahmavidādayaḥ kṛdantavṛttyā // VKal_5,2.35 //",
  "GK16vamkalsu:325": "tairmahīdharādayo vyākhyātāḥ // VKal_5,2.36 //",
  "GK16vamkalsu:326": "bhidurādayaḥ karmakartari kartari ca // VKal_5,2.37 //",
  "GK16vamkalsu:327": "guṇavistarādayaścintyāḥ // VKal_5,2.38 //",
  "GK16vamkalsu:328": "avatarāpacāyaśabdayordīrghahrasvatvavyatyāso bālānām // VKal_5,2.39 //",
  "GK16vamkalsu:329": "śobheti nipātanāt // VKal_5,2.40 //",
  "GK16vamkalsu:330": "avidhau guroḥ striyāṃ bahulaṃ vivakṣā // VKal_5,2.41 //",
  "GK16vamkalsu:331": "vyavasitādiṣu ktaḥ kartari, cakārāt // VKal_5,2.42 //*",
  "GK16vamkalsu:332": "āheti bhūte 'nyaṇalantabhramād bruvo laṭi // VKal_5,2.43 //",
  "GK16vamkalsu:333": "śabalādibhyaḥ striyāṃ ṭāpo 'prāptiḥ // VKal_5,2.44 //",
  "GK16vamkalsu:334": "prāṇini nīleti cintyam // VKal_5,2.45 //",
  "GK16vamkalsu:335": "manuṣyajātervivakṣāvivakṣe // VKal_5,2.46 //",
  "GK16vamkalsu:336": "ūkārāntādapyūṅ,",
  "GK16vamkalsu:337": "pravṛtteḥ // VKal_5,2.47 //*",
  "GK16vamkalsu:338": "kārtikīya iti ṭhañ durdharaḥ // VKal_5,2.48 //",
  "GK16vamkalsu:339": "śārvaramiti ca // VKal_5,2.49 //",
  "GK16vamkalsu:340": "śāśvatamiti prayukteḥ // VKal_5,2.50 //",
  "GK16vamkalsu:341": "rājavaṃśyādayaḥ sādhvarthe ye bhavanti // VKal_5,2.51 //",
  "GK16vamkalsu:342": "dāravaśabdo duṣprayuktaḥ // VKal_5,2.52 //",
  "GK16vamkalsu:343": "mugdhimādiṣvivamanij mṛgyaḥ // VKal_5,2.53 //",
  "GK16vamkalsu:344": "aupamyādayaścāturvarṇyavat // VKal_5,2.54 //",
  "GK16vamkalsu:345": "ṣyañaḥ ṣitkaraṇādīkāro bahulam // VKal_5,2.55 //",
  "GK16vamkalsu:346": "dhanvīti vrīhyādiṣu pāṭhāt // VKal_5,2.56 //",
  "GK16vamkalsu:347": "caturasraśobhīti ṇinau // VKal_5,2.57 //",
  "GK16vamkalsu:348": "kañcukīyā iti kyaci // VKal_5,2.58 //",
  "GK16vamkalsu:349": "bauddhapratiyogyapekṣāyāmapyātiśāyanikāḥ // VKal_5,2.59 //",
  "GK16vamkalsu:350": "kauśilādaya ilaci varṇalopāt // VKal_5,2.60 //",
  "GK16vamkalsu:351": "mauktikamiti vinayādipāṭhāt // VKal_5,2.61 //",
  "GK16vamkalsu:352": "prātibhādayaḥ prajñādiṣu // VKal_5,2.62 //",
  "GK16vamkalsu:353": "na sarajasamityanavyayībhāve // VKal_5,2.63 //",
  "GK16vamkalsu:354": "dhṛtadhanuṣītyasaṃjñāyām // VKal_5,2.64 //",
  "GK16vamkalsu:355": "durgandhipada iddurlabhaḥ // VKal_5,2.65 //",
  "GK16vamkalsu:356": "sudatyādayaḥ pratividheyāḥ // VKal_5,2.66 //",
  "GK16vamkalsu:357": "kṣatadṛḍhorsa iti na kap,",
  "GK16vamkalsu:358": "tadantavidhipratiṣedhāt // VKal_5,2.67 //*",
  "GK16vamkalsu:359": "avaihīti vṛddhiravadyā // VKal_5,2.68 //",
  "GK16vamkalsu:360": "apāṅganetreti lugalabhyaḥ // VKal_5,2.69 //",
  "GK16vamkalsu:361": "neṣṭhāḥ śliṣṭapriyādayaḥ,",
  "GK16vamkalsu:362": "puṃvadbhāvapratiṣedhāt // VKal_5,2.70 //*",
  "GK16vamkalsu:363": "dṛḍhabhaktiriti sarvatreti // VKal_5,2.71 //",
  "GK16vamkalsu:364": "jambulatādayo hrasvavidheḥ // VKal_5,2.72 //",
  "GK16vamkalsu:365": "tilakādayo 'jirādiṣu // VKal_5,2.73 //",
  "GK16vamkalsu:366": "niśamya-niśamayyaśabdau prakṛtibhedāt // VKal_5,2.74 //",
  "GK16vamkalsu:367": "saṃyamyaniyamyaśabdau,",
  "GK16vamkalsu:368": "aṇijantatvāt // VKal_5,2.75 //*",
  "GK16vamkalsu:369": "prapīyeti pīṅaḥ // VKal_5,2.76 //",
  "GK16vamkalsu:370": "dūrayatīti bahulagrahaṇāt // VKal_5,2.77 //",
  "GK16vamkalsu:371": "gacchatīprabhṛtiṣvaniṣedhyo num // VKal_5,2.78 //",
  "GK16vamkalsu:372": "mitreṇa goptreti puṃvadbhāvāt // VKal_5,2.79 //",
  "GK16vamkalsu:373": "vetsyasīti padabhaṅgāt // VKal_5,2.80 //",
  "GK16vamkalsu:374": "kāmayānaśabdaḥ siddho 'nādiścet // VKal_5,2.81 //",
  "GK16vamkalsu:375": "sauhṛd-daurhṛdaśabdāvaṇi hṛdbhāvāt // VKal_5,2.82 //",
  "GK16vamkalsu:376": "virama iti nipātanāt // VKal_5,2.83 //",
  "GK16vamkalsu:377": "uparyādiṣu sāmīpye dvirukteṣu dvitīyā // VKal_5,2.84 //",
  "GK16vamkalsu:378": "mandaṃ mandamityaprakārārthatve // VKal_5,2.85 //",
  "GK16vamkalsu:379": "na nidrādrugiti,",
  "GK16vamkalsu:380": "bhaṣbhāvaprāpteḥ // VKal_5,2.86 //*",
  "GK16vamkalsu:381": "niṣpanda iti patvaṃ cintyam // VKal_5,2.87 //",
  "GK16vamkalsu:382": "nāṅgulisaṅga iti mūrdhanyavidheḥ // VKal_5,2.88 //",
  "GK16vamkalsu:383": "tenāvantisenādayaḥ pratyuktāḥ // VKal_5,2.89 //",
  "GK16vamkalsu:384": "nendravāhane ṇatvam;",
  "GK16vamkalsu:385": "āhitatvasyāvivakṣitatvāt // VKal_5,2.90 //*",
  "GK16vamkalsu:386": "samāptaṃ cedaṃ prāyogikaṃ pañcamadhikaraṇam /",
  "GK16vamkalsu:387": "samāpto 'yaṃ granthaḥ //"
