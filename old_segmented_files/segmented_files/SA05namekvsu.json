{
  "SA05namekvsu:0": "Ekaviṃśatistotra",
  "SA05namekvsu:1": "namas tāre ture vīre kṣaṇadyutinibhekṣaṇe /",
  "SA05namekvsu:2": "trailokyanāthavaktrābjavikasatkesarodbhave // Nevs_1",
  "SA05namekvsu:3": "namas śāntasaraccandrasaṃpūrṇapaṭalānane /",
  "SA05namekvsu:4": "tāre sahasravikalpaprahasatkiraṇojjvale // Nevs_2",
  "SA05namekvsu:5": "namaḥ kanakanīlābjapāṇipadmavibhūṣite /",
  "SA05namekvsu:6": "dānavīryatapaḥkṣāntititikṣādhyānagocare // Nevs_3",
  "SA05namekvsu:7": "namas tathāgatoṣṇīṣavijayānantacāriṇi /",
  "SA05namekvsu:8": "aśeṣapāramitāprāptajinaputraniṣevite // Nevs_4",
  "SA05namekvsu:9": "namas tuttārahūṃkārapūritāśādigantare /",
  "SA05namekvsu:10": "saptalokakramākrāntā aśeṣākarṣaṇakṣaṇe // Nevs_5",
  "SA05namekvsu:11": "namaḥ śakranarabrahmamarudviśveśvarārcite /",
  "SA05namekvsu:12": "bhūtavetālagandharvagaṇapuraskṛte // Nevs_6",
  "SA05namekvsu:13": "namas tratritriphaṭkāre paramantrapramardani /",
  "SA05namekvsu:14": "pratyālīḍhapādanyāse śikhijvālākulojjvale // Nevs_7",
  "SA05namekvsu:15": "namas ture mahāghore māravīravināśani /",
  "SA05namekvsu:16": "bhṛkuṭīkṛtavaktrābjasarvaduṣṭanisūdani // Nevs_8",
  "SA05namekvsu:17": "namas triratnamudrāṅke hṛdyāṅgulivibhūṣite /",
  "SA05namekvsu:18": "bhūṣitāśeṣadikcakranikare sukulākule // Nevs_9",
  "SA05namekvsu:19": "namaḥ pramuditāṭopamukuṭākṣiptasāriṇi /",
  "SA05namekvsu:20": "hasatprahasattuttāre māralokabhayaṃkari // Nevs_10",
  "SA05namekvsu:21": "namaḥ samantabhūpālāpātālākarṣaṇakṣaṇe /",
  "SA05namekvsu:22": "bhṛkuṭikṛtahūṃkāre sarvāpadavimocani // Nevs_11",
  "SA05namekvsu:23": "namaḥ śikhaṇḍakhaṇḍendumukuṭābharaṇojjvale /",
  "SA05namekvsu:24": "amitābhatathābhāre bhāsvare kiraṇadhruve // Nevs_12",
  "SA05namekvsu:25": "namaḥ karatalāghātacaraṇāhatabhūtale /",
  "SA05namekvsu:26": "bhṛkuṭikṛtahūṃkārasaptapātālanāśini // Nevs_13",
  "SA05namekvsu:27": "namaḥ kalpāntahutabhugjvālāmālāntare sthite /",
  "SA05namekvsu:28": "ālīḍhamuditābaddharipucakravināśini // Nevs_14",
  "SA05namekvsu:29": "namaḥ śive śubhe śānte śāntanirvāṇagocare /",
  "SA05namekvsu:30": "svāhā praṇamya saṃyukte mahāpātakanāśini // Nevs_15",
  "SA05namekvsu:31": "namaḥ pramuditābaddharipugātraprabhedani /",
  "SA05namekvsu:32": "daśākṣarapādanyāse vidyāhuṃkāradīpite // Nevs_16",
  "SA05namekvsu:33": "namas ture pādāghāte huṃkārakārajīvite /",
  "SA05namekvsu:34": "merumaṇḍalakailāśabhūvanatrayacāraṇi // Nevs_17",
  "SA05namekvsu:35": "namaḥ surāsarākārahariṇīkakare sthite /",
  "SA05namekvsu:36": "haradviruktaphaṭkāra aśeṣaviśanāśini // Nevs_18",
  "SA05namekvsu:37": "namaḥ suragaṇayakṣāsurakinnarasevite /",
  "SA05namekvsu:38": "ābaddhamuditābhogakari duḥsvapnanāśini // Nevs_19",
  "SA05namekvsu:39": "namaś candrārkasaṃpūrṇa nayanadyutisvabhāsvare /",
  "SA05namekvsu:40": "tāra dviruktottāre viṣamajvalanāśini // Nevs_20",
  "SA05namekvsu:41": "namas tritalavinyāse śivaśaktisamanvite /",
  "SA05namekvsu:42": "grahavetālayakṣādyanāśani pravare ture // Nevs_21",
  "SA05namekvsu:43": "mantramūlam idaṃ stotraṃ namaskāraikaviṃśati /",
  "SA05namekvsu:44": "yaḥ paṭhet prāyaśo dhīmān devyā bhaktisamanvitaḥ // 1",
  "SA05namekvsu:45": "so 'yaṃ vā prātar utthāya smaret sarvābhayapradaṃ /",
  "SA05namekvsu:46": "sarvapāpapraśamanaṃ sarvadurgatināśanam // 2",
  "SA05namekvsu:47": "abhiṣiktobhaya tūrṇaṃ asmin mahattām āsādya /",
  "SA05namekvsu:48": "viṣaṃ tasya mahāghoraṃ smaraṇāt pralayaṃ yānti // 3",
  "SA05namekvsu:49": "grahajvalaviṣārtānām anyeṣāṃ caiva satvānām /",
  "SA05namekvsu:50": "putrakāmo labhet putraṃ sarvakāmān avāpnoti // 4",
  "SA05namekvsu:51": "saptābhir jinakoṭibhiḥ so 'nte bauddhapadaṃ vrajet /",
  "SA05namekvsu:52": "sthāvaraṃ vātha jaṅgamaṃ sāḍ idaṃ pīḍam eva ca // 5",
  "SA05namekvsu:53": "paramārtivināśanaṃ dvitrisaptābhivartinām /",
  "SA05namekvsu:54": "dhanakāmo labhed dhanaṃ na vighnaiḥ pratihanyate // 6",
  "SA05namekvsu:55": "iti śrīsaṃyaksaṃbuddhavairocanabhāṣitaṃ bhagavatyāryatāradevyā namaskāraikaviṃśatistotraṃ saṃpūrṇaṃ samāptaṃ // śubham //"
}