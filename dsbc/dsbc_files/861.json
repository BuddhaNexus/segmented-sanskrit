{
    "861:0": "pañcarakṣādevīstotrāṇi",
    "861:1": "1 mahāpratisarāstotram",
    "861:2": "om namaḥ śrīmahāpratisarāyai",
    "861:3": "tathāgatādyāstathatāṃ tattvamāpurmahattaram |",
    "861:4": "dhāraṇīdhāraṇādyasyāḥ pratisarāṃ namāmi tām || 1 ||",
    "861:5": "raṇe śakro'jayaddaityān dhāraṇīdhvajadhṛg bahūn |",
    "861:6": "saṃgrāmajayadāṃ bhīmāṃ pratisarāṃ namāmi tām || 2 ||",
    "861:7": "yatprabhāvād brahmadatto'labhad rājyamakaṇṭakam |",
    "861:8": "sārvabhaumapradāṃ devīṃ pratisarāṃ namāmi tām || 3 ||",
    "861:9": "bahvaparādho'pi yadbhakto rājyādhikāramāptavān |",
    "861:10": "śastrādibhītisaṃhatrīṃ pratisarāṃ namāmi tām || 4 ||",
    "861:11": "ratnānyavāpurvaṇijo yāṃ smṛtvodadhinirgatāḥ |",
    "861:12": "sarvabādhāpraśamanīṃ pratisarāṃ namāmi tām || 5 ||",
    "861:13": "śrīmahāpratisarārakṣādevīstotraṃ samāptam |",
    "861:14": "2 mahāmantrānusāriṇīstotram",
    "861:15": "om namaḥ śrīmahāmantrānusāriṇyai",
    "861:16": "buddhādhiṣṭhānato buddhābhayadāṃ bhayanāśinīm |",
    "861:17": "bhavāmbudhinimagnānāṃ namo mantrānusāriṇīm || 1 ||",
    "861:18": "yanmantroccāraṇādeva ṣaḍītayaḥ sudāruṇāḥ |",
    "861:19": "nāśaṃ prayānti varadāṃ namo mantrāmusāriṇīm || 2 ||",
    "861:20": "mantrānusāriṇo lokān nānye mantrādayo grahāḥ |",
    "861:21": "pīḍayanti priyāṃścāpi namo mantrānusāriṇīm || 3 ||",
    "861:22": "buddho'bhyabhāṣad gāthāstā yanmantrakathanāntaram |",
    "861:23": "yābhiḥ sarvatra svasti syānnamo mantrānusāriṇīm || 4 ||",
    "861:24": "kalau buddhavihīne'smin lokānāṃ hitamācaret |",
    "861:25": "pāpotpātapraśamanīṃ namo mantrānusāriṇīm || 5 ||",
    "861:26": "śrīmahāmantrānusāriṇīstotraṃ samāptam |",
    "861:27": "3 mahāmāyūrīstotram",
    "861:28": "om namaḥ śrīmahāmāyūrīryai",
    "861:29": "duṣṭaṃ kṛṣṇabhujaṅgaṃ ca naraḥ svāntikaṃ pālayet |",
    "861:30": "yasyā mantrānubhāvena māyūrīṃ praṇamāmi tām || 1 ||",
    "861:31": "brahmādayo lokapālā yaddhāraṇyā samāpnuvan |",
    "861:32": "svāni svānyadhikārāṇi māyūrīṃ praṇamāmi tām || 2 ||",
    "861:33": "svarṇāvabhāsaṃ śikhinaṃ nālabhajjapinaṃ kudhīḥ |",
    "861:34": "amoghenāpi pāśena māyūrīṃ praṇamāmi tām || 3 ||",
    "861:35": "yanmantrajapato jīvāḥ prājīvañchuṣkapādapāḥ |",
    "861:36": "mṛtasaṃjīvinīṃ devīṃ māyūrīṃ praṇamāmi tām || 4 ||",
    "861:37": "yanmantrisaṅgāt pavano mahopadravaśāntikṛt |",
    "861:38": "buddhānāṃ bodhidāṃ nityaṃ māyūrīṃ praṇamāmi tām || 5 ||",
    "861:39": "śrīmahāmāyūrīrakṣādevīstotraṃ samāptam |",
    "861:40": "4 mahāśītavatīrakṣādevīstotram",
    "861:41": "om namo mahāśītavatyai",
    "861:42": "yaddhāraṇīmanujapan rāhulo bhadramāptavān |",
    "861:43": "viheṭhito grahaiḥ sarvaiḥ śītavatīṃ namāmyaham || 1 ||",
    "861:44": "pāpatāpe śītakarīṃ śītalādyupasargataḥ |",
    "861:45": "śītoṣṇaduḥkhaśamanīṃ śītavatīṃ namāmyaham || 2 ||",
    "861:46": "mantragranthitasūtrāṇāṃ dhāraṇāllakṣayojanam |",
    "861:47": "pathikānāṃ pālayitrīṃ śītavatīṃ namāmyaham || 3 ||",
    "861:48": "śmaśānasthena muninā yā samuccāritā purā |",
    "861:49": "grahopadravaśāntyarthaṃ śītavatīṃ namāmyaham || 4 ||",
    "861:50": "grahābhibhūtavātānāṃ granthipadavidhāriṇām |",
    "861:51": "grahabhītipraśamanīṃ śītavatīṃ namāmyaham || 5 ||",
    "861:52": "śrī mahāśītavatīrakṣādevīstotraṃ samāptam |",
    "861:53": "5 mahāsāhasrapramardinīstotram",
    "861:54": "om namaḥ śrīmahāsāhasrapramardinye",
    "861:55": "mahāsāhasrike loke sāhasrahitakāriṇām |",
    "861:56": "sahasrasattvajananīṃ naumi sāhasramardinīm || 1 ||",
    "861:57": "sopadravāyāṃ vaiśālyāṃ mahotsavo yataḥ sadā |",
    "861:58": "mahopasargaśamanīṃ naumi sāhasramardinīm || 2 ||",
    "861:59": "yakṣarākṣasabhūtānāṃ damanīṃ duṣṭacetasām |",
    "861:60": "duritopadravahatāṃ naumi sāhasramardinīm || 3 ||",
    "861:61": "yaddhāraṇīpaṭhanato rakṣitaḥ śākyakeśarī |",
    "861:62": "viṣato viṣadigdhāṃ tāṃ naumi sāhasramardinīm || 4 ||",
    "861:63": "madhumiśritabhaiṣajyaṃ sarvaroganivāraṇam |",
    "861:64": "mṛtasañjīvanaṃ loke naumi sāhasramardinīm || 5 ||",
    "861:65": "śrīmahāsāhasramardinīrakṣādevīstotraṃ samāptam |"
}