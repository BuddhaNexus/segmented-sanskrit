{
  "SA05n830:0": "śāradāṣṭakastotram",
  "SA05n830:1": "śaṅkhendukundahimasannibhacārudehāṃ",
  "SA05n830:2": "haṃsasthitāṃ kamalapatrasurocanīyām |",
  "SA05n830:3": "divyāmbarābharaṇabhūṣitasaumyarūpāṃ",
  "SA05n830:4": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 1 ||",
  "SA05n830:5": "saṃsārasāgaramahodadhimagnasattva-",
  "SA05n830:6": "santāriṇīṃ suranarārcitapādapadmām |",
  "SA05n830:7": "hārārddhahāramaṇikuṇḍalamaṇḍitāṅgīṃ",
  "SA05n830:8": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 2 ||",
  "SA05n830:9": "yā bhāratīti kathitā jananī ca loke",
  "SA05n830:10": "mohāndhakārabharabhagnakṛtāṃ janānām |",
  "SA05n830:11": "saṃkīrtitā munibhirastasamastadoṣaiḥ",
  "SA05n830:12": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 3 ||",
  "SA05n830:13": "vīṇānuvādanaratāṃ sphaṭikākṣamālā-",
  "SA05n830:14": "saṃdhāriṇīṃ kanakapustakadhāriṇīṃ ca |",
  "SA05n830:15": "ratnaiḥ śubhaiḥ surucirāṃ kṛtahastapadmāṃ",
  "SA05n830:16": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 4 ||",
  "SA05n830:17": "pūjyā sadaiva jananī parivandanīyā",
  "SA05n830:18": "devyā gaṇā manasi saṃparimodanīyāḥ |",
  "SA05n830:19": "jīvārthinaḥ phalabhṛto guṇavarṇanā ca",
  "SA05n830:20": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 5 ||",
  "SA05n830:21": "natvā bhajejjagati yo jananīṃ prasiddhāṃ",
  "SA05n830:22": "kuryātsadā bhagavatī kilapañcasaukhyam |",
  "SA05n830:23": "saundaryarūpaguṇavittasubhogabhājaṃ",
  "SA05n830:24": "śrī śāradāṃ bhagavatīṃ satataṃ namāmi || 6 ||",
  "SA05n830:25": "pūjopahāravalibhiḥ paripūjanīyā",
  "SA05n830:26": "devyā gaṇādikamanojñasuvaktrapadmāḥ |",
  "SA05n830:27": "yaiḥ pūjitā bhagavatī kila jāḍyahantrī",
  "SA05n830:28": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 7 ||",
  "SA05n830:29": "yasyāḥ prasādamavagamya sukhapradāyā-",
  "SA05n830:30": "strailokyanāthauditārkasamaprabhaḥ syāt |",
  "SA05n830:31": "dharmārthakāmaphaladāmatha mokṣadāṃ ca",
  "SA05n830:32": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 8 ||",
  "SA05n830:33": "śrīśāradāṣṭakastotraṃ samāptam |"
}