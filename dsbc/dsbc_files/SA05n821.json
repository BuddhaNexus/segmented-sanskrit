{
  "SA05n821:0": "lokeśvarastotram",
  "SA05n821:1": "nānākṛtiṃ sukṛtinaṃ jagataḥ śaraṇyaṃ",
  "SA05n821:2": "lokeśakeśavaharātmadharairavaśyam |",
  "SA05n821:3": "saṃsārasāgarataraṃ kamalāyatākṣaṃ",
  "SA05n821:4": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi ||1 ||",
  "SA05n821:5": "rakṣākaraṃ sakalabhītivatāṃ janānāṃ",
  "SA05n821:6": "vāmānanaṃ kanakakuṇḍalacārukarṇam |",
  "SA05n821:7": "graiveyakādimaṇikābharaṇena śobhaṃ",
  "SA05n821:8": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 2 ||",
  "SA05n821:9": "bhaktyā natasya manujasya vipadvirāmaṃ",
  "SA05n821:10": "svargāpavargaphaladaṃ varadaṃ dayālum |",
  "SA05n821:11": "devādhidevamamaraṃ jinasaṃghavantaṃ",
  "SA05n821:12": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi ||3 ||",
  "SA05n821:13": "santaptarūpatilakaṃ tilakaṃ surāṇāṃ",
  "SA05n821:14": "ratnākaraṃ kanakakuntakaśobhahastam |",
  "SA05n821:15": "bhrājiṣṇumābharaṇadhāraṇadikpradaṃ taṃ",
  "SA05n821:16": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 4 ||",
  "SA05n821:17": "durgandhadurgatiharaṃ duritāpahāraṃ",
  "SA05n821:18": "durbhikṣanāśakaraṇaṃ karuṇākaraṃ tam |",
  "SA05n821:19": "vidyāpradaṃ guṇanidhiṃ śubhaśuddhadehaṃ",
  "SA05n821:20": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 5 ||",
  "SA05n821:21": "itthaṃ varaṃ sakalabhūtagaṇādhināthaṃ",
  "SA05n821:22": "trailokyanāthamamarairapi vandyamānam |",
  "SA05n821:23": "evaṃ kvacid rathavare sukhasaṃpraviṣṭaṃ",
  "SA05n821:24": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 6 ||",
  "SA05n821:25": "yakṣādikinnaranarairmunibhiśca nāgai-",
  "SA05n821:26": "rvidyādharaiḥ suragaṇairdanujaiḥ piśācaiḥ |",
  "SA05n821:27": "sarvopakāranamitaṃ puṭitā grahastaiḥ",
  "SA05n821:28": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 7 ||",
  "SA05n821:29": "gandhādicandanayutaṃ mṛganābhibhiśca",
  "SA05n821:30": "karpūrakuṅkumavarairharicandanaiśca |",
  "SA05n821:31": "tasyānulepanakṛtena suśobhitāṅgaṃ",
  "SA05n821:32": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 8 ||",
  "SA05n821:33": "rogādināśanakaraṃ bhajatāṃ sunāma",
  "SA05n821:34": "śokādiduḥkhaharaṇaṃ sukhacintanīyam |",
  "SA05n821:35": "pīyūṣatulyavacanaṃ mṛdujātagātraṃ",
  "SA05n821:36": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 9 ||",
  "SA05n821:37": "ājānulambitakaraṃ gajarājamadhyaṃ",
  "SA05n821:38": "saundaryakuṇḍaśikharaṃ sphaṭikābhadantam |",
  "SA05n821:39": "atyantasundaratanuṃ śubhalakṣaṇāṅgaṃ",
  "SA05n821:40": "śrīlokanāthavibudhaṃ śubhadaṃ bhajāmi || 10 ||",
  "SA05n821:41": "ye lokanāthasya sadā surabhyaṃ stotraṃ paṭhiṣyanti janā sabhāvāḥ |",
  "SA05n821:42": "śrīlokanātho'pi tadīpsitārthaṃ svahastagrastastvaritaṃ dadāti || 11 ||",
  "SA05n821:43": "śrīlokeśvarastotraṃ samāptam |"
}