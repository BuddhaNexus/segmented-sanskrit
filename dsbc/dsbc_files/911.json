{
    "911:0": "vasudhārānāmadhāraṇīstotram",
    "911:1": "om namo bhagavatyai āryaśrīvasudhārāyai",
    "911:2": "divyarūpī surūpī ca saumyarūpī balapradā |",
    "911:3": "vasudharī vasudhāraṇī vasuśrī śrīkarī varā || 1 ||",
    "911:4": "dharaṇī dhāraṇī dhātā śaraṇyā bhaktavatsalā |",
    "911:5": "prajñāpāramitā devī prajñā śrībuddhivardhinī || 2 ||",
    "911:6": "vidyādharī śivā sūkṣmā śāstā sarvatra mātṛkā |",
    "911:7": "taruṇī tāru(ra)",
    "911:8": "ṇī devī vidyādāneśvareśvarī || 3 ||",
    "911:9": "bhūṣitā bhūtamātā ca sarvābharaṇabhūṣitā |",
    "911:10": "durdāntatrāsanī bhītā ugrā ugraparākramā || 4 ||",
    "911:11": "dānapāramitā devī varṣaṇī divyarūpiṇī |",
    "911:12": "nidhānaṃ sarvamāṅgalyā kīrtirlakṣmīryaśaḥśubhā || 5 ||",
    "911:13": "dahanī mālinī caṇḍī śabarī sarvamātrikā |",
    "911:14": "kṛtāntaśāsanī raudrī kaumārī viśvarūpiṇī || 6 ||",
    "911:15": "vīryapāramitā devī jagadānandarocanī |",
    "911:16": "tāpasī ugrarūpī ca ṛddhisiddhibalapradā || 7 ||",
    "911:17": "dhanyā puṇyā mahābhāgā ajitā jitavikramā |",
    "911:18": "jagadekahitā vidyā saṃgrāme tāraṇī śubhā || 8 ||",
    "911:19": "kṣāntipāramitā devī śīlinī dhyānadhyāyino |",
    "911:20": "padminī padmadhārī ca padmapriyā padmāsanī || 9 ||",
    "911:21": "śuddharūpī mahātejā hemavarṇā prabhākarī |",
    "911:22": "cintāmaṇimahādevī prajñāpustakadhāriṇī || 10 ||",
    "911:23": "nidhānaṃ kūṭimāruḍhidhanyāgāradhanapriyā |",
    "911:24": "traidhātukaṃ mahā ādi divyābharaṇabhūṣiṇī || 11 ||",
    "911:25": "mātarī sarvabuddhānāṃ ratnadhāte(tvī)",
    "911:26": "śvareśvarī |",
    "911:27": "śūnyatā bhāvanī devī bhāvābhāvavivarjitā || 12 ||",
    "911:28": "vainye(ne)ya kiṃ na vinyastā divyakleśanichedanī |",
    "911:29": "bhī(bhe) dinī sarvamārāṇāṃ saptapātālakṣobhinī (ṇī) || 13 ||",
    "911:30": "brahmāṇī vedamātā ca guhyā ca guhyavāsinī |",
    "911:31": "sarasvatī viśālākṣī caturbrahmavihāriṇī || 14 ||",
    "911:32": "tāthāgatī mahāramyā vajriṇī dharmadhāriṇī |",
    "911:33": "karmadhāteśvarī vidyā viśvajvālābhamaṇḍalī || 15 ||",
    "911:34": "bodha(dhi)nī sarvasattvānāṃ bodhyaṅgakṛtaśekharī |",
    "911:35": "dhyānā dhīrmuktisaṃpannā advayadvayabhāvinī || 16 ||",
    "911:36": "sarvārthasādhanī bhadrā strīrūpāmitavikramā |",
    "911:37": "darśinī buddhamārgāṇāṃ naṣṭamārgapradarśinī || 17 ||",
    "911:38": "vāgīśvarī mahāśāntirgoptrī dhātrī dhanapradā |",
    "911:39": "strīrūpadhāriṇī siddhā yoginī yogajeśvarī || 18 ||",
    "911:40": "manoharī mahākrāntiḥ saubhāgyapriyadarśinī |",
    "911:41": "sārthavāhakṛpādṛṣṭiḥ sarvatāthāgatātmakī || 19 ||",
    "911:42": "namaste'stu mahādevī sarvasattvārthadāyinī |",
    "911:43": "namaste divyarūpī ca vasudhārā namo'stu te || 20 ||",
    "911:44": "aṣṭottaraśataṃ nāma trikālaṃ yaṃ paṭhet pumān |",
    "911:45": "prāpnoti niyataṃ siddhimīpsitārthamanorathān || 21 ||",
    "911:46": "yadajñānakṛtaṃ pāpam ānantaryasudāruṇam |",
    "911:47": "tatsarvaṃ kṣapayatyāśu smaraṇāt sa(rva)bhadrakam || 22 ||",
    "911:48": "athavā śīlasaṃpannaḥ saptajātismaro bhavet |",
    "911:49": "priyaścādeyavākyena rūpavān priyadarśanaḥ || 23 ||",
    "911:50": "viprakṣatriyakuleṣu ādeyamupajāyate |",
    "911:51": "ante bhūmīśvaraṃ prāptaḥ paścāt prāpta(ḥ)",
    "911:52": "sukhāvatīm || 24 ||",
    "911:53": "śrīvasudhārānāmadhāraṇīstotraṃ",
    "911:54": "samyaksaṃbuddhabhāṣitaṃ samāptam |"
}