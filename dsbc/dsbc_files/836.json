{
    "836:0": "catuḥṣaṣṭisaṃvarastotram",
    "836:1": "śrīherukaṃ mahāvīraṃ viśuddhaṃ kuliśeśvaram|",
    "836:2": "namāmi sarvabhāvena ḍākinīgaṇabhūṣitam|| 1||",
    "836:3": "saṃvarāya namastubhyaṃ dvayākārāya namo namaḥ|",
    "836:4": "cakrasthitāya devāya cakrasaṃvara te namaḥ|| 2||",
    "836:5": "meṣavaktra namaste'stu śivaśaktisvarūpiṇe|",
    "836:6": "mahākrodhasvarūpāya meṣasaṃvara te namaḥ|| 3||",
    "836:7": "aśvānanāya devāya ratikarmaratāya ca|",
    "836:8": "bhuktimuktipradātre ca aśvasaṃvara te namaḥ|| 4||",
    "836:9": "vyāghrāsyāya namastubhyaṃ śaktiyuktāya vai namaḥ|",
    "836:10": "surāmāṃsarato nityaṃ vyāghrasaṃvara te namaḥ|| 5||",
    "836:11": "kūrmāsyāya namastubhyaṃ surate saṃratāya ca|",
    "836:12": "namo devādhidevāya kūrmasaṃvara te namaḥ|| 6||",
    "836:13": "namāmi matsyavaktrāya mānavānāṃ hitāya vai|",
    "836:14": "namo namaste deveśa matsyasaṃvara te namaḥ|| 7||",
    "836:15": "makarākāravaktrāya mahāśaktidharāya ca|",
    "836:16": "manovāñchāpradātre ca makarasaṃvara te namaḥ|| 8||",
    "836:17": "saṃvarāyoṣṭravaktrāya śaktikāliṅgaṇāya ca|",
    "836:18": "maithune tatparāyātha uṣṭrasaṃvara te namaḥ|| 9||",
    "836:19": "namāmi gajavaktrāya yonau liṅgapradāya ca|",
    "836:20": "namo bhagavate tubhyaṃ gajasaṃvara te namaḥ|| 10||",
    "836:21": "maṇḍūkānananāthāya trailokyeśāya te namaḥ|",
    "836:22": "namo devādhidevāya maṇḍūkasaṃvarāya ca|| 11||",
    "836:23": "namāmi cāvivaktrāya nityaṃ ratiratāya ca|",
    "836:24": "namaste devadeveśa ahisaṃvara te namaḥ|| 12||",
    "836:25": "śukānanāya devāya dvayākārākāraśobhine|",
    "836:26": "namo namaste bhīmāṅga śukasaṃvara te namaḥ|| 13||",
    "836:27": "saṃvarāya namaste'stu śaktiyuktāya vai namaḥ|",
    "836:28": "siṃhānanāya vai nityaṃ siṃhasaṃvara te namaḥ|| 14||",
    "836:29": "markaṭānanadevāya śakticumbanaratāya ca|",
    "836:30": "namaste'stu namaste'stu harisaṃvara te namaḥ|| 15||",
    "836:31": "śvānavaktra namaste'stu śakticumbanakāriṇe|",
    "836:32": "namaste bhagavan deva śvānasaṃvara te namaḥ|| 16||",
    "836:33": "varāhāsyavarevāna bhagakrīḍanakāraka|",
    "836:34": "namāmi śaktiyuktāya ghṛṣṭisaṃvara te namaḥ|| 17||",
    "836:35": "jambukāsyāya devāya śaktikāliṅgaṇāya ca|",
    "836:36": "sarvapāpaharāyaiva śivāsaṃvara te namaḥ|| 18||",
    "836:37": "namāmi gṛdhravaktrāya duḥkhanāśāya te namaḥ|",
    "836:38": "dvayākārasiddhidevāya gṛdhrasaṃvara te namaḥ|| 19||",
    "836:39": "kākānanāya śuddhayakṣa śṛṅgārarūpadhāriṇe|",
    "836:40": "namo bhagavate tubhyaṃ kākasaṃvara te namaḥ|| 20||",
    "836:41": "ulūkavaktriṇe tubhyaṃ mahāsaukhyapradāya ca|",
    "836:42": "yonimaithunakṛddevolūkasaṃvara te namaḥ|| 21||",
    "836:43": "tārkṣyānanāya devāya tāriṇe bhavasāgarāt|",
    "836:44": "namastubhyaṃ namastubhyaṃ namo garuḍasaṃvara|| 22||",
    "836:45": "govaktrāya namastubhyaṃ gohatyāpāpahāriṇe|",
    "836:46": "gotravṛddhipradātre ca dhenusaṃvara te namaḥ|| 23||",
    "836:47": "gardabhākāravaktrāya gatāgatakṣayāya te|",
    "836:48": "gaṇeśvarāya devāya kharasaṃvara te namaḥ|| 24||",
    "836:49": "mahiṣāsyāya devāya mahāpralayakāriṇe|",
    "836:50": "śaktiyuktāya devāya namao mahiṣasaṃvara|| 25||",
    "836:51": "viḍālāsya namastubhyaṃ mūtrakūṇḍaprakrīḍine|",
    "836:52": "śreṣṭhāya parameśāya namo mārjārasaṃvara|| 26||",
    "836:53": "namāmi śālvavaktrāya dvayaṅgamekeva śobhitam|",
    "836:54": "namāmi devadeveśa namaḥ śarabhasaṃvara|| 27||",
    "836:55": "siddhāya siddharūpāya guṇāya guṇavartine|",
    "836:56": "krauñcāsyāya namastubhyaṃ krauñcasaṃvara te namaḥ|| 28||",
    "836:57": "ulkānanāya śuddhāya uttamāya namo namaḥ|",
    "836:58": "ugrāya bhīmarūpāya ulkasaṃvara te namaḥ|| 29||",
    "836:59": "haṃsavaktra namaste'stu haṃsasvarasvarūpiṇe|",
    "836:60": "haṃsaḥso'haṃsvarūpāya haṃsasaṃvara te namaḥ|| 30||",
    "836:61": "mṛgavaktrāya devāya namāmi parameśvara|",
    "836:62": "ṛddhisiddhipradātre ca mṛgasaṃvara te namaḥ|| 31||",
    "836:63": "śaktiyukta namaste'stu cakravākānanāya ca|",
    "836:64": "namaste cakavākākhyasaṃvarāya namo namaḥ|| 32||",
    "836:65": "ajānanāya vīrāya avidyānāśine namaḥ|",
    "836:66": "apavargaphalāptyarthamajasaṃvara te namaḥ|| 33||",
    "836:67": "kukkuṭāsyāya devāya kulavṛddhikarāya ca|",
    "836:68": "namaste kauṭukeśāya tubhyaṃ kukkuṭasaṃvara|| 34||",
    "836:69": "kṛṣṇasārasavaktrāya namaste karmasambhava|",
    "836:70": "kālanāśāya devāya eṇasaṃvara te namaḥ|| 35||",
    "836:71": "mūṣānanāya pūrṇāya jñānadāya namo namaḥ|",
    "836:72": "sarvadaityavināśaya mūṣasaṃvara te namaḥ|| 36||",
    "836:73": "sālūkāsya namaste'stu mānādivaradāya ca|",
    "836:74": "siddhibuddhipradātre ca namaḥ sālūkasaṃvara|| 37||",
    "836:75": "namaḥ kapolavaktrāya prajñopāyātmarūpiṇe|",
    "836:76": "namaste'stu mahāvīra kapotasaṃvarāya ca|| 38||",
    "836:77": "namāmi grāhavaktrāya bhuktimuktipradāya ca|",
    "836:78": "namo'stu sarvabhūteśa grāhasaṃvara te namaḥ|| 39||",
    "836:79": "namaste cihlavaktrāya namaste maithune rata|",
    "836:80": "namaste bhagavan deva cihlasaṃvara te namaḥ|| 40||",
    "836:81": "caṭakāsyāya devāya namaste'stu jagadguro|",
    "836:82": "namaste'stu guṇādhīśa caṭakasaṃvara te namaḥ|| 41||",
    "836:83": "sārasāsya namastubhyaṃ namaste guṇasāgara|",
    "836:84": "namo bhagavate tubhyaṃ sārasasaṃvarāya ca|| 42||",
    "836:85": "khañjanāsyāya devāya mahādevāya te namaḥ|",
    "836:86": "nirvāṇapadadātre khañjarīṭasaṃvarāya ca || 43||",
    "836:87": "namaste kṣemakaryāsya bhuktimuktipradāya ca|",
    "836:88": "namo bhagavate deva kṣemakarisaṃvarāya te|| 44||",
    "836:89": "śaśakāsya namastubhyaṃ namaste bhuvaneśvara|",
    "836:90": "karmapradāya te nityaṃ namaḥ śaśakasaṃvara|| 45||",
    "836:91": "namo bhallūkavaktrāya ratikrīḍāparāya ca|",
    "836:92": "kāryasiddhipradātre ca namo bhallūkasaṃvara|| 46||",
    "836:93": "pikāsyāya namastubhyaṃ mantrasiddhikarāya ca|",
    "836:94": "trilokeśāya sarvāya pikasaṃvara te namaḥ|| 47||",
    "836:95": "namāmi vakavaktrāya śaktiyuktagaṇādhipa|",
    "836:96": "namāmi devadeveśa vakasaṃvara te namaḥ|| 48||",
    "836:97": "khaṅgivaktrāya devāya sarvadā śirasā namaḥ|",
    "836:98": "namo'stu parameśāya khaṅgisaṃvara te namaḥ|| 49||",
    "836:99": "karkaṭāsya namastubhyaṃ namaḥ saṃsārahetave|",
    "836:100": "pāpapuñjavināśāya namaḥ karkaṭasaṃvara|| 50||",
    "836:101": "namaḥ śallakivaktrāya sarvadoṣanivāriṇe|",
    "836:102": "saṃsārapāśanāśāya namaḥ śallakisaṃvara|| 51||",
    "836:103": "vṛścikāsya namastubhyaṃ balakalyāṇadāya ca|",
    "836:104": "namo namaste devāya namo vṛścikasaṃvara|| 52||",
    "836:105": "alivaktrāya devāya alimodāya te namaḥ|",
    "836:106": "alilokavināśāya alisaṃvara te namaḥ|| 53||",
    "836:107": "jāhakāsya namaste'stu janamohakṛtāya vai|",
    "836:108": "jayakalyāṇadātre ca namo jāhakasaṃvara|| 54||",
    "836:109": "camarīvaktra te nityaṃ cāturyaphaladāyine|",
    "836:110": "caturvargapradātre ca namaścamarisaṃvara|| 55||",
    "836:111": "sīmnivaktranamastubhyaṃ siddhitvaphaladāyine|",
    "836:112": "namo bhagavate nityaṃ sīmnisaṃvara te namaḥ|| 56||",
    "836:113": "namāmi godhavaktrāya namaste bhīmavikrama|",
    "836:114": "namaste divyanetrāya godhāsaṃvara te namaḥ|| 57||",
    "836:115": "cakorāsya namastubhyaṃ cāñcalyadoṣanāśaka|",
    "836:116": "namo bhagavate tubhyaṃ cakorasaṃvarāya ca|| 58||",
    "836:117": "godhikāsyāya satataṃ sṛṣṭisaṃhārakariṇe|",
    "836:118": "namaste paramānanda godhikāsaṃvarāya ca|| 59||",
    "836:119": "madhumākṣikavaktrāya namaste mānadāyaka|",
    "836:120": "sarvajñāya pareśāya namo mākṣikasaṃvara|| 60||",
    "836:121": "pataṅgavaktra te nityaṃ parameśa namaḥ sadā|",
    "836:122": "divyāya divaseśāya namaḥ pataṅgasaṃvara|| 61||",
    "836:123": "naravaktrāya devāya narakadhnāya te namaḥ|",
    "836:124": "nānārūpadadhānāya narasaṃvara te namaḥ|| 62||",
    "836:125": "sṛṣṭirūpa jagaddhāma sṛjate sarvabhūtakam|",
    "836:126": "namo namaste satataṃ sṛṣṭisaṃvara te namaḥ|| 63||",
    "836:127": "sthitirūpāya devāya guṇāya guṇavartine|",
    "836:128": "namaḥ sāravināśāya sthitisaṃvara te namaḥ|| 64||",
    "836:129": "sarvalokasya saṃhārakartre te parameśvara|",
    "836:130": "tasmādahaṃ namasyāmi namaḥ pralayasaṃvara|| 65||",
    "836:131": "jyotirvaktra paraṃ dhāma namaste jagadīśvara|",
    "836:132": "parātparataraṃ sūkṣma jyotiḥsaṃvara te namaḥ|| 66||",
    "836:133": "itīdaṃ saṃvarastotraṃ triṣu lokeṣu durlabham|",
    "836:134": "parātparataraṃ stotraṃ catuḥṣaṣṭipramāṇakam|| 67||",
    "836:135": "catuḥṣaṣṭipramāṇeṣu ādyamādyaṃ tu vaktritam|",
    "836:136": "bhūyo granthamayāccātra ādyamātraṃ pracoditam|| 68||",
    "836:137": "kanyārthī labhate kanyāṃ dhanārthī labhate dhanam|",
    "836:138": "vidyārthī labhate vidyāṃ mokṣārthī mokṣamāpnuyāt|| 69||",
    "836:139": "vaśīkaraṇamuccāṭaṃ māramohanastambhanam|",
    "836:140": "ākarṣaṇaṃ ca vidveṣaṃ dhātuvādaṃ rasāyanam|| 70||",
    "836:141": "guṭikāṃ pādukāsiddhiṃ khaṅgasiddhiṃ tathaiva ca|",
    "836:142": "khecarīsiddhi vaidyāṅgaṃ mantrasiddhiṃ ca vākpaṭuḥ|| 71||",
    "836:143": "parakāyapraveśaṃ ca dravyākarṣaṇameva ca|",
    "836:144": "labhate stotrarājena satyaṃ satyaṃ mayoditam|| 72||",
    "836:145": "durbhikṣaṃ cāpadi pāṭhaṃ kurute śubhamāpnuyāt|",
    "836:146": "ativṛṣṭāvanāvṛṣṭau mahāmārīsamudbhave|| 73||",
    "836:147": "rājyabhraṃśe jñānabhraṃśe strībhraṃśe ca dhanakṣaye|",
    "836:148": "kalahe ca vivāde ca paṭhate stotramuttamam|| 74||",
    "836:149": "bhikṣukebhyaḥ sahasrebhyaḥ tulyaṃ dānakṛtaṃ phalam|",
    "836:150": "bahunātra kimuktena brahmāṇḍadānajaṃ phalam|| 75||",
    "836:151": "śrīsaṃvarāgame mahātantre amitābhavairocanasaṃvāde",
    "836:152": "śrīcakrasaṃvarādicatuḥṣaṣṭisaṃvarastotraṃ samāptam|"
}