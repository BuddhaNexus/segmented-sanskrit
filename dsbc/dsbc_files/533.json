{
    "533:0": "|| kamalākarasarvatathāgatastavaparivartaḥ ||",
    "533:1": "atha khalu bhagavāṃstāṃ bodhisattvasamuccayāṃ kuladevatāmetadavocat |",
    "533:2": "tena khalu punaḥ kuladevate kālena tena samayena rājā suvarṇabhujendro nāmāsīt |",
    "533:3": "etena kamalākareṇa sarvatathāgatastavenātītānāgatapratyutpannān buddhān bhagavato'bhyastāvīt ||",
    "533:4": "ye jina pūrvaka ye ca bhavanti",
    "533:5": "ye ca dhriyanti daśodiśi loke |",
    "533:6": "teṣa jināna karomi praṇāmaṃ",
    "533:7": "taṃ jinasaṃghamahaṃ praśayiṣye || 1 ||",
    "533:8": "śāntapraśāntaviśuddhamunīndraṃ",
    "533:9": "suvarṇavarṇaprabhāsitagātram |",
    "533:10": "sarvasurāsurasusvarabuddhaṃ",
    "533:11": "brahmarute svaragarjitaghoṣam || 2 ||",
    "533:12": "ṣaṭpadamaulamahīruhakeśaṃ",
    "533:13": "nīlasukuñcitakāśanikāśam |",
    "533:14": "śaṅkhatuṣārasupāṇḍaladantaṃ",
    "533:15": "hemavirājitabhāsitanābham || 3 ||",
    "533:16": "nīlaviśālaviśuddhasunetraṃ",
    "533:17": "nīlamivitpalapracyutibhāsam |",
    "533:18": "padmasuvarṇaviśālasujihvaṃ",
    "533:19": "padmaprabhāsitapadmamukhābham || 4 ||",
    "533:20": "śaṅkhamṛṇālanibhāmukhatorṇaṃ",
    "533:21": "dakṣiṇavartitaverulivarṇam |",
    "533:22": "sūkṣmaniśākarakṣīṇaśaśīva",
    "533:23": "gātra munermramarājvalanābham || 5 ||",
    "533:24": "kāñcanakoṭi suvarṇamṛduraṃ",
    "533:25": "nāsamukhonnata pīvaraghrāṇam |",
    "533:26": "agradharāgraviśiṣṭasunāsaṃ",
    "533:27": "mṛduka sarvajināṃśa satatam || 6 ||",
    "533:28": "ekasame cittaromamukhāgraṃ",
    "533:29": "vālasuromapradakṣiṇavartam |",
    "533:30": "nīlanibhā jvalakuṇḍalajātaṃ",
    "533:31": "nīlavirājitamaulisugrīvam || 7 ||",
    "533:32": "jātasamānaprabhāsitagātraṃ",
    "533:33": "pūjitasarvi deśodiśi loke |",
    "533:34": "duḥkhamanantapraśāntatriloke",
    "533:35": "sarvasukhena ca tarpitasattvam || 8 ||",
    "533:36": "narakagātiṣvatha tiryaggatīṣu",
    "533:37": "pretasurāsuramanuṣyagatīṣu |",
    "533:38": "teṣu ca sarvasukhārpitasattvaṃ",
    "533:39": "sarvapraśānta apāyagatīṣu || 9 ||",
    "533:40": "varṇasuvarṇakanākanibhāsaṃ",
    "533:41": "kāñcanataptaprabhāsitagātram |",
    "533:42": "saumyaśaśāṅkasuvimalavakraṃ",
    "533:43": "vikāsitarājitasuvimalavadanam || 10 ||",
    "533:44": "taruṇatanūruhakomalagātraṃ",
    "533:45": "siṃhamivākramavikramanāgam |",
    "533:46": "lambitahasta pralambitabāhuṃ",
    "533:47": "mārutapreritaśālalateva || 11 ||",
    "533:48": "vyomaprabhājvalamuñcitaraśmiṃ",
    "533:49": "sūryasahasramiva pratapantam |",
    "533:50": "nirmalagātravarebhi munīndraṃ",
    "533:51": "sarvaprabhāsita kṣetramanantam || 12 ||",
    "533:52": "candraniśākarabhāskarajālaṃ",
    "533:53": "kṣetramanantasahasraśateṣu |",
    "533:54": "te'pi ca niṣṭhita sarvamabhūṣi",
    "533:55": "buddhaprabhāsavirocanatāyai || 13 ||",
    "533:56": "buddhadivākaralokapradipaṃ",
    "533:57": "buddhadivākaraśatasahasram |",
    "533:58": "kṣetramanantasahasraśateṣu",
    "533:59": "paśyatu sattva tathāgatasūryam || 14 ||",
    "533:60": "puṇyasahasraśatāccitakāyaṃ",
    "533:61": "sarvaguṇebhiralaṅkṛtagātram |",
    "533:62": "śauṇḍagajendranibhaṃ jinabāhuṃ",
    "533:63": "vimalasulakṣaṇamaṇḍitahastam || 15 ||",
    "533:64": "bhūmitalopamarajasamatulyaṃ",
    "533:65": "sūkṣmarajopama ye gatabuddhāḥ ||",
    "533:66": "sūkṣmarajopama ye ca bhavanti",
    "533:67": "sūkṣmarajopama ye ca sthihanti || 16 ||",
    "533:68": "teṣa jināna karomi praṇāmaṃ",
    "533:69": "kāyatu vācamanena prasannaḥ |",
    "533:70": "puṣpapradānasugandhapradānai -",
    "533:71": "rvarṇaśatena sucetasi cāpi || 17 ||",
    "533:72": "jihvaśatairapi buddhaguṇāni",
    "533:73": "kalpasahasraśate na hi vaktum |",
    "533:74": "ye guṇasādhunivṛtti jinānāṃ",
    "533:75": "sā ca varāgravicitra anekaiḥ || 18 ||",
    "533:76": "ekajinasya guṇā na hi śakyā",
    "533:77": "jihvaśatairapi bhāṣitu kaścit |",
    "533:78": "kāmamaśakti hi sarvajinānāṃ",
    "533:79": "ekaguṇasya hi vistara vaktum || 19 ||",
    "533:80": "sarvasadevakuloktasamūhaḥ",
    "533:81": "sarvabhavāgrabhave jalapūrṇā |",
    "533:82": "keśagrahaṇena tu śakya pramātuṃ",
    "533:83": "naiva ca ekaguṇā sugatānām || 20 ||",
    "533:84": "varṇita saṃstuta me jinasarvaṃ",
    "533:85": "kāyatu vāca prasannamanena |",
    "533:86": "yanmama sañcitapuṇyaphalāgraṃ",
    "533:87": "tena ca sattva prabhotu jinatvam || 21 ||",
    "533:88": "evaṃ stavitva narapati buddhaṃ",
    "533:89": "evaṃ karoti nṛpaḥ praṇidhānam |",
    "533:90": "yatra ca kutraci mahya bhaveta",
    "533:91": "jāti anāgatakalpamanantā || 22 ||",
    "533:92": "īdṛśi bheri mi paśyami svapne",
    "533:93": "īdṛśa deśana tatra śṛṇomi |",
    "533:94": "īdṛśa jīnastutikamalākara",
    "533:95": "jātiṣu tatra labhe smaraṇatvam || 23 ||",
    "533:96": "buddhaguṇāni anantamatulyā",
    "533:97": "ye'pi ca durlabha kalpasahasraiḥ |",
    "533:98": "amu śruṇeya ca svapnagato'pi",
    "533:99": "teṣu ca deśayi divasagato'pi || 24 ||",
    "533:100": "duḥkhasamudra vimocayi sattvā",
    "533:101": "pūrayi ṣaḍabhi pi pāramitābhiḥ |",
    "533:102": "bodhimanuttara paśca labheyaṃ",
    "533:103": "kṣetra bhaveta mamā asamarthyam || 25 ||",
    "533:104": "bheripradānavipākaphalena",
    "533:105": "sarvajināna ca saṃstutihetoḥ |",
    "533:106": "saṃmukha paśyami śākyamunīndraṃ",
    "533:107": "vyākaraṇaṃ hyahu tatra labheyam || 26 ||",
    "533:108": "ye ima dāraka dvau mama putrau",
    "533:109": "kanakendra kanakaprabhāsvaraḥ |",
    "533:110": "te ubhi dāraka tatra labheyaṃ",
    "533:111": "bodhimanuttara vyākaraṇaṃ ca || 27 ||",
    "533:112": "ye'pi ca sattva arakṣa atrāṇā",
    "533:113": "śaraṇavihīna viyāsagatāśca |",
    "533:114": "teṣu bhaveya anāgata sarva",
    "533:115": "trāṇaparāyaṇaśaraṇebhiśca || 28 ||",
    "533:116": "duḥkhasamudbhavasaṃkṣayakartā",
    "533:117": "sarvasukhasya ca ākarabhūtaḥ |",
    "533:118": "kalpa anāgata bodhi careyaṃ",
    "533:119": "yattaka pūrvakakoṭigatāśca || 29 ||",
    "533:120": "svarṇaprabhottamadeśanatāya",
    "533:121": "pāpasamudra śoṣatu mahyam |",
    "533:122": "karmasamudra vikīryatu mahyaṃ",
    "533:123": "kleśasamudra vicchidyatu mahyam || 30 ||",
    "533:124": "puṇyasamudra prapūryatu mahyaṃ",
    "533:125": "jñānasamudra viśodhyatu mahyam |",
    "533:126": "vimalajñānaprabhāsabalena",
    "533:127": "sarvaguṇāna samudra bhaveyā || 31 ||",
    "533:128": "bodhiguṇairguṇaratna prapūrṇā",
    "533:129": "deśanasvarṇa prabhāsabalena |",
    "533:130": "puṇyaprabhāsa bhaviṣyati mahyaṃ",
    "533:131": "bodhiprabhāsa viśodhyatu mahyam || 32 ||",
    "533:132": "kāyaprabhāsa bhaviṣyati mahyaṃ",
    "533:133": "puṇyaprabhāsa virocanatā ca |",
    "533:134": "sarvatriloki viśiṣṭa bhaveyaṃ",
    "533:135": "pūṇyabalena samanvita nityam || 33 ||",
    "533:136": "duḥkhasamudra uttārayitā",
    "533:137": "sarvasukhasya ca sāgarakalpya |",
    "533:138": "kalpamanāgata bodhi careyaṃ",
    "533:139": "yattakapūrvakakoṭigatāśca || 34 ||",
    "533:140": "yādṛśakṣetraviśiṣṭa triloke",
    "533:141": "sarvajinānamananta guṇena |",
    "533:142": "tādṛśakṣetramanantaguṇaṃ",
    "533:143": "bheṣyati mahyamanāgatasarvam || 35 ||",
    "533:144": "iti śrīsuvarṇaprabhāsottamasūtrendrarāje kamalākara -",
    "533:145": "sarvatathāgatastavaparivarto nāma pañcamaḥ ||"
}