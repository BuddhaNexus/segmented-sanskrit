{
  "SA07n919:0": "CANTO V",
  "SA07n919:1": "sa tathā viṣayairvilobhyamānaḥ |",
  "SA07n919:2": "paramārhairapi śākyarājasūnuḥ |",
  "SA07n919:3": "na jagāma dhṛtiṃ na śarma lebhe",
  "SA07n919:4": "hṛdaye siṃha ivātidigdhaviddhaḥ ||1 ||",
  "SA07n919:5": "atha mantisutaiḥ kṣamaiḥ kadāci-",
  "SA07n919:6": "tsakhibhiścitrakathaiḥ kṛtānuyātraḥ |",
  "SA07n919:7": "vanabhūmididṛkṣayā śamepsu-",
  "SA07n919:8": "rnaradevānumato bahiḥ pratasthe ||2 ||",
  "SA07n919:9": "navarukmakhalīnakiṅkiṇīkaṃ",
  "SA07n919:10": "pracalaccāramaracāruhemabhāṇḍam |",
  "SA07n919:11": "abhiruhya sa kanthakaṃ sadaśvaṃ",
  "SA07n919:12": "prayayau ketumiva drumābjaketuḥ ||3 ||",
  "SA07n919:13": "sa vikṛṣṭatarāṃ vanāntabhūmiṃ",
  "SA07n919:14": "vanalobhācca yayau mahīguṇācca |",
  "SA07n919:15": "salilormivikārasīramārgāṃ",
  "SA07n919:16": "vasudhāṃ caiva dadarśa kṛṣyamāṇām ||4 ||",
  "SA07n919:17": "halabhinnavikīrṇaśaṣpadarbhā",
  "SA07n919:18": "hatasūkṣmakrimikīṭajantukīrṇām |",
  "SA07n919:19": "samavekṣya rasāṃ tathāvidhāṃ tāṃ",
  "SA07n919:20": "svajanasyeva vadhe bhṛśaṃ śuśoca ||5 ||",
  "SA07n919:21": "kṛṣataḥ puruṣāṃśca vīkṣamāṇaḥ",
  "SA07n919:22": "pavanārkāśurajovibhinnavarṇān |",
  "SA07n919:23": "vahanaklamaviklavāṃśca dhuryān",
  "SA07n919:24": "paramāryaḥ paramāṃ kṛpāṃ cakāra ||6 ||",
  "SA07n919:25": "avatīrya tatasturaṅgapṛṣṭhā-",
  "SA07n919:26": "cchanakairgā vyacaracchucā parītaḥ |",
  "SA07n919:27": "jagato jananavyayaṃ vicinvan",
  "SA07n919:28": "kṛpaṇaṃ khalvidamityuvāca cārtaḥ ||7 ||",
  "SA07n919:29": "manasā ca viviktatāmabhīpsuḥ",
  "SA07n919:30": "suhṛdastānanuyāyino nivārya |",
  "SA07n919:31": "abhitaścalacāruparṇavatyā",
  "SA07n919:32": "vijane mūlamupeyivān sa jambvāḥ ||8 ||",
  "SA07n919:33": "niṣasāda sa yatra śaucavatyāṃ",
  "SA07n919:34": "bhuvi vaidūryanikāśaśādvalāyām |",
  "SA07n919:35": "jagataḥ prabhavavyayau vicinva-",
  "SA07n919:36": "nmanasaśca sthitimārgamālalambe ||9 ||",
  "SA07n919:37": "samavāptamanaḥsthitiśca sadyo",
  "SA07n919:38": "viṣayecchādibhirādhibhiśca muktaḥ |",
  "SA07n919:39": "savitarkavicāramāpa śāntaṃ",
  "SA07n919:40": "prathamaṃ dhyānamanāsravaprakāram ||10 ||",
  "SA07n919:41": "adhigamya tato vivekajaṃ tu",
  "SA07n919:42": "paramaprītisukhaṃ manaḥsamādhim |",
  "SA07n919:43": "idameva tataḥ paraṃ pradadhyau",
  "SA07n919:44": "manasā lokagatiṃ niśāmya samyak ||11 ||",
  "SA07n919:45": "kṛpaṇaṃ bata yajjanaḥ svayaṃ sa-",
  "SA07n919:46": "nnavaśo vyādhijarāvināśadharmā |",
  "SA07n919:47": "jarayārditamāturaṃ mṛtaṃ vā",
  "SA07n919:48": "paramajño vijugupsate madāndhaḥ ||12 ||",
  "SA07n919:49": "iha cedahamīdṛśaḥ svayaṃ sa-",
  "SA07n919:50": "nvijugupseya paraṃ tathāsvabhāvam |",
  "SA07n919:51": "na bhavetsadṛśaṃ hi tatkṣamaṃ vā",
  "SA07n919:52": "paramaṃ dharmamimaṃ vijānato me ||13 ||",
  "SA07n919:53": "iti tasya vipaśyato yathāva-",
  "SA07n919:54": "jjagato vyādhijarāvipattidoṣān |",
  "SA07n919:55": "balayauvanajīvitapravṛtto",
  "SA07n919:56": "vijagāmātmagato madaḥ kṣaṇena ||14 ||",
  "SA07n919:57": "na jaharṣa na cāpi cānutepe",
  "SA07n919:58": "vicikitsāṃ na yayau na tandrinidre |",
  "SA07n919:59": "na ca kāmaguṇeṣu saṃrarañje",
  "SA07n919:60": "na vididveṣa paraṃ na cāvamene ||15 ||",
  "SA07n919:61": "iti buddhiriyaṃ ca nīrajaskā",
  "SA07n919:62": "vavṛdhe tasya mahātmano viśuddhā |",
  "SA07n919:63": "puruṣairaparairadṛśyamānaḥ",
  "SA07n919:64": "puruṣaścopasasarpa bhikṣuveṣaḥ ||16 ||",
  "SA07n919:65": "naradevasutastamabhyapṛccha-",
  "SA07n919:66": "dvada ko'sīti śaśaṃsa so'tha tasmai |",
  "SA07n919:67": "narapuṃgava janmamṛtyubhītaḥ",
  "SA07n919:68": "śramaṇaḥ pravrajito'smi mokṣahetoḥ ||17 ||",
  "SA07n919:69": "jagati kṣayadharmake mumukṣa-",
  "SA07n919:70": "rmṛgaye'haṃ śivamakṣayaṃ padaṃ tat |",
  "SA07n919:71": "svajane'nyajane ca tulyabuddhi-",
  "SA07n919:72": "rviṣayebhyo vinivṛttarāgadoṣaḥ ||18 ||",
  "SA07n919:73": "nivasan kvacideva vṛkṣamūle",
  "SA07n919:74": "vijane vāyatane girau vane vā |",
  "SA07n919:75": "vicarāmyaparigraho nirāśaḥ",
  "SA07n919:76": "paramārthāya yathopapannabhaikṣaḥ ||19 ||",
  "SA07n919:77": "iti paśyata eva rājasūno-",
  "SA07n919:78": "ridamuktvā sa nabhaḥ samutpapāta |",
  "SA07n919:79": "sa hi tadvapuranyabuddhadarśī",
  "SA07n919:80": "smṛtaye tasya sameyivāndivaukāḥ ||20 ||",
  "SA07n919:81": "gaganaṃ khagavadgate ca tasmi-",
  "SA07n919:82": "nṛvaraḥ saṃjahṛṣe visismiye ca |",
  "SA07n919:83": "upalabhya tataśca dharmasaṃjñā-",
  "SA07n919:84": "mabhiniryāṇavidhau matiṃ cakāra ||21 ||",
  "SA07n919:85": "tata indrasamo jitendriyāśvaḥ",
  "SA07n919:86": "pravivikṣuḥ puramaśvamāruroha |",
  "SA07n919:87": "parivārajanaṃ tvavekṣamāṇa-",
  "SA07n919:88": "stata evābhimataṃ vanaṃ na bheje ||22 ||",
  "SA07n919:89": "sa jarāmaraṇakṣayaṃ cikīrṣu-",
  "SA07n919:90": "rvanavāsāya matiṃ smṛtau nidhāya |",
  "SA07n919:91": "praviveśa punaḥ puraṃ na kāmā-",
  "SA07n919:92": "dvanabhūmeriva maṇḍalaṃ dvipendraḥ ||23 ||",
  "SA07n919:93": "sukhitā bata nirvṛtā ca sā strī",
  "SA07n919:94": "patirīdṛkṣa ihāyatākṣa yasyāḥ |",
  "SA07n919:95": "iti taṃ samudīkṣya rājakanyā",
  "SA07n919:96": "praviśantaṃ pathi sāñjalirjagāda ||24 ||",
  "SA07n919:97": "atha ghoṣamimaṃ mahābhraghoṣaḥ",
  "SA07n919:98": "pariśuśrāva śamaṃ paraṃ ca lebhe |",
  "SA07n919:99": "śrutavānsa hi nirvṛteti śabdaṃ",
  "SA07n919:100": "parinirvāṇavidhau matiṃ cakāra ||25 ||",
  "SA07n919:101": "atha kāñcanaśailaśṛṅgavarṣmā",
  "SA07n919:102": "gajamegharṣabhabāhunisvanākṣaḥ |",
  "SA07n919:103": "kṣayamakṣayadharmajātarāgaḥ",
  "SA07n919:104": "śaśisiṃhānanavikramaḥ prapede ||26 ||",
  "SA07n919:105": "mṛgarājagatistato'bhyagaccha-",
  "SA07n919:106": "nnṛpatiṃ mantrigaṇairupāsyamānam |",
  "SA07n919:107": "samitau marutāmiva jvalantaṃ",
  "SA07n919:108": "maghavantaṃ tridive sanatkumāraḥ ||27 ||",
  "SA07n919:109": "praṇipatya ca sāñjalirbabhāṣe",
  "SA07n919:110": "diśa mahyaṃ naradeva sādhvanujñām |",
  "SA07n919:111": "parivivrajiṣāmi mokṣaheto-",
  "SA07n919:112": "rniyato hyasya janasya viprayogaḥ ||28 ||",
  "SA07n919:113": "iti tasya vaco niśamya rājā",
  "SA07n919:114": "kariṇevābhihato drumaścacāla |",
  "SA07n919:115": "kamalapratime'ñjalau gṛhītvā",
  "SA07n919:116": "vacanaṃ cedamuvāca bāṣpakaṇṭhaḥ ||29 ||",
  "SA07n919:117": "pratisaṃhara tāt buddhimetāṃ",
  "SA07n919:118": "na hi kālastava dharmasaṃśrayasya |",
  "SA07n919:119": "vayasi prathame matau calāyāṃ",
  "SA07n919:120": "bahudoṣāṃ hi vadanti dharmacaryām ||30 ||",
  "SA07n919:121": "viṣayeṣu kutūhalendriyasya",
  "SA07n919:122": "vratakhedeṣvasamarthīniścayasya |",
  "SA07n919:123": "taruṇasya manaścalatyaraṇyā-",
  "SA07n919:124": "danabhijñaśca viśeṣato viveke ||31 ||",
  "SA07n919:125": "mama tu priyadharma dharmakāla-",
  "SA07n919:126": "stvayi lakṣmīmavasṛjya lakṣmabhūte |",
  "SA07n919:127": "sthiravikrama vikrameṇa dharma-",
  "SA07n919:128": "stava hitvā tu guruṃ bhavedadharmaḥ ||32 ||",
  "SA07n919:129": "tadimaṃ vyavasāyamutsṛja tvaṃ",
  "SA07n919:130": "bhava tāvannirato gṛhasthadharme |",
  "SA07n919:131": "puruṣasya vayaḥsukhāni bhuktvā",
  "SA07n919:132": "ramaṇīyo hi tapovanapraveśaḥ ||33 ||",
  "SA07n919:133": "iti vākyamidaṃ niśamya rājñaḥ",
  "SA07n919:134": "kalaviṅkasvara uttaraṃ babhāṣe |",
  "SA07n919:135": "yadi me pratibhūtaścaturṣu rājan",
  "SA07n919:136": "bhavasi tvaṃ na tapovanaṃ śrayiṣye ||34 ||",
  "SA07n919:137": "na bhavenmaraṇāya jīvitaṃ me",
  "SA07n919:138": "viharetsvāsthyamidaṃ ca me na rogaḥ |",
  "SA07n919:139": "na ca yauvanamākṣipejjarā me",
  "SA07n919:140": "na ca saṃpattimimāṃ haredvipattiḥ ||35 ||",
  "SA07n919:141": "iti durlabhamarthamūcivāṃsaṃ",
  "SA07n919:142": "tanayaṃ vākyamuvāca śākyarājaḥ |",
  "SA07n919:143": "tyaja buddhimimāmatipravṛttā-",
  "SA07n919:144": "mavahāsyo'timanoratho'kramaśca ||36 ||",
  "SA07n919:145": "atha merugururguruṃ babhāṣe",
  "SA07n919:146": "yadi nāsti krama eṣa nāsmi vāryaḥ |",
  "SA07n919:147": "śaraṇājjvalanena dahyamānā-",
  "SA07n919:148": "nna hi niścikramiṣuḥ kṣamaṃ grahītum ||37 ||",
  "SA07n919:149": "jagataśca yadā dhruvo viyogo",
  "SA07n919:150": "nanu dharmāya varaṃ svayaviyogaḥ |",
  "SA07n919:151": "avaśaṃ nanu viprayojayenmā-",
  "SA07n919:152": "makṛtasvārthamatṛptameva mṛtyuḥ ||38 ||",
  "SA07n919:153": "iti bhūmipatirniśamya tasya",
  "SA07n919:154": "vyavasāyaṃ tanayasya nirmumukṣoḥ |",
  "SA07n919:155": "abhidhāya na yāsyatīti bhūyo",
  "SA07n919:156": "vidadhe rakṣaṇamuttamāṃśca kāmān ||39 ||",
  "SA07n919:157": "sacivaistu nidarśito yathāvad",
  "SA07n919:158": "bahumānātpraṇayācca śāstrapūrvam |",
  "SA07n919:159": "guruṇā ca nivārito'śrupātaiḥ",
  "SA07n919:160": "praviveśāvasathaṃ tataḥ sa śocan ||40 ||",
  "SA07n919:161": "calakuṇḍalacumbitānanābhi-",
  "SA07n919:162": "rghananiśvāsavikampitastanībhiḥ |",
  "SA07n919:163": "vanitābhiradhīralocanābhi-",
  "SA07n919:164": "mṛrgaśāvābhirivābhyudīkṣyamāṇaḥ ||41 ||",
  "SA07n919:165": "sa hi kāñcanaparvatāvadāto",
  "SA07n919:166": "hṛdayenmādakaro varāṅganānām |",
  "SA07n919:167": "śravaṇāṅgavilocanātmabhāvā-",
  "SA07n919:168": "nvacanasparśavapurguṇairjahāra ||42 ||",
  "SA07n919:169": "vigate divase tato vimānaṃ",
  "SA07n919:170": "vapuṣā sūrya iva pradīpyamānaḥ |",
  "SA07n919:171": "timiraṃ vijighāṃsurātmabhāsā",
  "SA07n919:172": "ravirudyanniva merumāruroha ||43 ||",
  "SA07n919:173": "kanakojjvaladīptadīpavṛkṣaṃ",
  "SA07n919:174": "varakālāgurudhūpapūrṇagarbham |",
  "SA07n919:175": "adhiruhya sa vajrabhakticitraṃ",
  "SA07n919:176": "pravaraṃ kāñcanamāsanaṃ siṣeve ||44 ||",
  "SA07n919:177": "tata uttamamuttamāṅganāstaṃ",
  "SA07n919:178": "niśi tūryairupatasthurindrakalpam |",
  "SA07n919:179": "himavacchirasīva candragaure",
  "SA07n919:180": "draviṇendrātmajamapsarogaṇaughāḥ ||45 ||",
  "SA07n919:181": "paramairapi divyatūryakalpaiḥ",
  "SA07n919:182": "sa tu tairnaiva ratiṃ yayau na harṣam |",
  "SA07n919:183": "paramārthasukhāya tasya sādho-",
  "SA07n919:184": "rabhiniścikramiṣā yato na reme ||46 ||",
  "SA07n919:185": "atha tatra suraistapovariṣṭhai-",
  "SA07n919:186": "rakaniṣṭhairvyavasāyamasya buddhvā |",
  "SA07n919:187": "yugapatpramadājanasya nidrā",
  "SA07n919:188": "vihitāsīdvikṛtāśca gātraceṣṭāḥ ||47 ||",
  "SA07n919:189": "abhavacchayitā hi tatra kāci-",
  "SA07n919:190": "dviniveśya pracale kare kapolam |",
  "SA07n919:191": "dayitāmapi rukmapattracitrāṃ",
  "SA07n919:192": "kupitevāṅkagatāṃ vihāya vīṇām ||48 ||",
  "SA07n919:193": "vibabhau karalagnaveṇuranyā",
  "SA07n919:194": "stanavisrastasitāṃśukā śayānā |",
  "SA07n919:195": "ṛjuṣaṭpadapaṅktijuṣṭapadmā",
  "SA07n919:196": "jalaphenaprahasattaṭā nadīva ||49 ||",
  "SA07n919:197": "navapuṣkaragarbhakomalābhyāṃ",
  "SA07n919:198": "tapanīyojjvalasaṃgatāṅgadābhyām |",
  "SA07n919:199": "svapiti sma tathāparā bhujābhyāṃ",
  "SA07n919:200": "parirabhya priyavanmṛdaṅgameva ||50 ||",
  "SA07n919:201": "navahāṭakabhūṣaṇāstathānyā",
  "SA07n919:202": "vasanaṃ pītamanuttamaṃ vasānāḥ |",
  "SA07n919:203": "avaśā ghananidrayā nipetu-",
  "SA07n919:204": "rgajabhagnā iva karṇikāraśākhāḥ ||51 ||",
  "SA07n919:205": "avalambya gavākṣapārśvamanyā",
  "SA07n919:206": "śayitā cāpavibhugnagātrayaṣṭiḥ |",
  "SA07n919:207": "virarāja vilambicāruhārā",
  "SA07n919:208": "racitā toraṇāśālabhañjikeva ||52 ||",
  "SA07n919:209": "maṇikuṇḍaladaṣṭapattralekhaṃ",
  "SA07n919:210": "mukhapadmaṃ vinataṃ tathāparasyāḥ |",
  "SA07n919:211": "śatapattramivārdhavakranāḍaṃ",
  "SA07n919:212": "sthitakāraṇḍavaghaṭṭitaṃ cakāśe ||53 ||",
  "SA07n919:213": "aparāḥ śayitā yathopaviṣṭāḥ",
  "SA07n919:214": "stanabhārairavanamyamānagātrāḥ |",
  "SA07n919:215": "upaguhya parasparaṃ vireju-",
  "SA07n919:216": "rbhujapāśaistapanīyaparihāryaiḥ ||54 ||",
  "SA07n919:217": "mahatī parivādinī ca kāci-",
  "SA07n919:218": "dvanitāliṅgya sakhīmiva prasuptā |",
  "SA07n919:219": "vijughūrṇa calatsuvarṇasūtrā",
  "SA07n919:220": "vadanenākulakarṇikojjvalena ||55 ||",
  "SA07n919:221": "paṇavaṃ yuvatirbhujāṃsadeśā-",
  "SA07n919:222": "davavisraṃsitacārūpāśamanyā |",
  "SA07n919:223": "savilāsaratāntatāntamūrvo-",
  "SA07n919:224": "rvivare kāntamivābhinīya śiśye ||56 ||",
  "SA07n919:225": "aparā babhūvurnimīlitākṣyo",
  "SA07n919:226": "vipulākṣyo'pi śubhabhruvo'pi satyaḥ |",
  "SA07n919:227": "pratisaṃkucitāravindakośāḥ",
  "SA07n919:228": "savitaryastamite yathā nalinyaḥ ||57 ||",
  "SA07n919:229": "śithilākulamūrdhajā tathānyā",
  "SA07n919:230": "jaghanasrastavibhūṣaṇāṃśukāntā |",
  "SA07n919:231": "aśayiṣṭa vikīrṇakaṇṭhasūtrā",
  "SA07n919:232": "gajabhagnā pratiyātanāṅganeva ||58 ||",
  "SA07n919:233": "aparāstvavaśā hriyā viyuktā",
  "SA07n919:234": "dhṛtimatyo'pi vapurguṇairupetāḥ |",
  "SA07n919:235": "viniśaśvasurulbaṇaṃ śayānā",
  "SA07n919:236": "vikṛtāḥ kṣiptabhūjā jajṛmbhire ca ||59 ||",
  "SA07n919:237": "vyapaviddhavibhūṣaṇasrajo'nyā",
  "SA07n919:238": "visṛtāgranthanavāsaso visaṃjñāḥ |",
  "SA07n919:239": "animīlitaśuklaniścalākṣyo",
  "SA07n919:240": "na virejuḥ śayitā gatāsukalpāḥ ||60 ||",
  "SA07n919:241": "vivṛtāsyapuṭā vivṛddhagātrī",
  "SA07n919:242": "prapatadvaktrajalā prakāśaguhyā |",
  "SA07n919:243": "aparā madaghūrṇiteva śiśye",
  "SA07n919:244": "na babhāse vikṛtaṃ vapuḥ pupoṣa ||61 ||",
  "SA07n919:245": "iti sattvakulānvayānurūpaṃ",
  "SA07n919:246": "vividhaṃ sa pramadājanaḥ śayānaḥ |",
  "SA07n919:247": "sarasaḥ sadṛśaṃ babhāra rūpaṃ",
  "SA07n919:248": "pavanāvarjitarugnapuṣkarasya ||62 ||",
  "SA07n919:249": "samavekṣya tathā tathā śayānā",
  "SA07n919:250": "vikṛtāstā yuvatīradhīraceṣṭāḥ |",
  "SA07n919:251": "guṇavadvapuṣo'pi valgubhāṣā",
  "SA07n919:252": "nṛpasūnuḥ sa vigarhayāṃbabhūva ||63 ||",
  "SA07n919:253": "aśucirvikṛtaśca jīvaloke",
  "SA07n919:254": "vanitānāmayamīdṛśaḥ svabhāvaḥ |",
  "SA07n919:255": "vasanābharaṇaistu vañcyamānaḥ",
  "SA07n919:256": "puruṣaḥ strīviṣayeṣu rāgameti ||64 ||",
  "SA07n919:257": "vimṛśedyadi yoṣitāṃ manuṣyaḥ",
  "SA07n919:258": "prakṛtiṃ svapnavikāramīdṛśaṃ ca |",
  "SA07n919:259": "dhruvamatra na vardhayetpramādaṃ",
  "SA07n919:260": "guṇasaṃkalpahatastu rāgameti ||65 ||",
  "SA07n919:261": "iti tasya tadantaraṃ viditvā",
  "SA07n919:262": "niśi niścikramiṣā samudbabhūva |",
  "SA07n919:263": "avagamya manastato'sya devai-",
  "SA07n919:264": "rbhavanadvāramapāvṛtaṃ babhūva ||66 ||",
  "SA07n919:265": "atha so'vatatāra harmyapṛṣṭhā-",
  "SA07n919:266": "dyuvatīstāḥ śayitā vigarhamāṇaḥ |",
  "SA07n919:267": "avatīrya tataśca nirviśaṅko",
  "SA07n919:268": "gṛhakakṣyāṃ prathamāṃ vinirjagāma ||67 ||",
  "SA07n919:269": "turagāvacaraṃ sa bodhayitvā",
  "SA07n919:270": "javinaṃ chandakamitthamityuvāca |",
  "SA07n919:271": "hayamānaya kanthakaṃ tvarāvā-",
  "SA07n919:272": "namṛtaṃ prāptumito'dya me yiyāsā ||68 ||",
  "SA07n919:273": "hṛdi yā mama tuṣṭiradya jātā",
  "SA07n919:274": "vyavasāyaśca yathā matau niviṣṭaḥ |",
  "SA07n919:275": "vijane'pi ca nāthavānivāsmi",
  "SA07n919:276": "dhruvamartho'bhimukhaḥ sameta iṣṭaḥ ||69 ||",
  "SA07n919:277": "hriyameva ca saṃnatiṃ ca hitvā",
  "SA07n919:278": "śayitā matpramukhe yathā yuvatyaḥ |",
  "SA07n919:279": "vivṛte ca yathā svayaṃ kapāṭe",
  "SA07n919:280": "niyataṃ yātumato mamādya kālaḥ ||70 ||",
  "SA07n919:281": "pratigṛhya tataḥ sa bharturājñāṃ",
  "SA07n919:282": "viditārtho'pi narendraśāsanasya |",
  "SA07n919:283": "manasīva pareṇa codyamāna-",
  "SA07n919:284": "sturagasyānayane matiṃ cakāra ||71 ||",
  "SA07n919:285": "atha hemakhalīnapūrṇavaktraṃ",
  "SA07n919:286": "laghuśayyāstaraṇonapūrṇavaktraṃ",
  "SA07n919:287": "laghuśayyāstaraṇopagūḍhapṛṣṭham |",
  "SA07n919:288": "balasattvajavānvayopapannaṃ",
  "SA07n919:289": "sa varāśvaṃ tamupānināya bhartre ||72 ||",
  "SA07n919:290": "pratatrikapucchamūlapārṣṇi",
  "SA07n919:291": "nibhṛtahṛsvatanūjapucchakarṇam |",
  "SA07n919:292": "vinatonnatapṛṣṭhakukṣipārśva",
  "SA07n919:293": "vipulaprothalalāṭakaṭyuraskam ||73 ||",
  "SA07n919:294": "upaguhya sa taṃ viśālavakṣāḥ",
  "SA07n919:295": "kamalābhena ca sāntvayan kareṇa |",
  "SA07n919:296": "madhurākṣarayā girā śaśāsa",
  "SA07n919:297": "dhvajinīmadhyamiva praveṣṭukāmaḥ ||74 ||",
  "SA07n919:298": "bahuśaḥ kila śatravo nirastāḥ",
  "SA07n919:299": "samare tvāmadhiruhya pārthivena |",
  "SA07n919:300": "ahamapyamṛtaṃ padaṃ yathāva-",
  "SA07n919:301": "tturagaśreṣṭha labheya tatkuruṣva ||75 ||",
  "SA07n919:302": "sulabhāḥ khalu saṃyuge sahāyā",
  "SA07n919:303": "viṣayāvāptasukhe dhanārjane vā |",
  "SA07n919:304": "puruṣasya tu durlabhāḥ sahāyāḥ",
  "SA07n919:305": "patitasyāpadi dharmasaṃśraye vā ||76 ||",
  "SA07n919:306": "iha caiva bhavanti ye sahāyaḥ",
  "SA07n919:307": "kaluṣe karmaṇi dharmasaṃśraye vā |",
  "SA07n919:308": "avagacchait me yathāntarātmā",
  "SA07n919:309": "niyataṃ te'pi janāstadaṃśabhājaḥ ||77 ||",
  "SA07n919:310": "tadidaṃ parigamya dharmayuktaṃ",
  "SA07n919:311": "mama niryāṇamito jagaddhitāya |",
  "SA07n919:312": "turagottama vegavikramābhyāṃ",
  "SA07n919:313": "prayatasvātmahite jagaddhite ca ||78 ||",
  "SA07n919:314": "iti suhṛdamivānuśiṣya kṛtye",
  "SA07n919:315": "turagavaraṃ nṛvaro vanaṃ yiyāsuḥ |",
  "SA07n919:316": "sitamasitagatidyutirvapuṣmān",
  "SA07n919:317": "raviriva śāradamabhramāruroha ||79 ||",
  "SA07n919:318": "atha sa pariharanniśīthacaṇḍaṃ",
  "SA07n919:319": "parijanabodhakaraṃ dhvanīṃ sadaśvaḥ |",
  "SA07n919:320": "vigatahanuravaḥ praśāntaheṣa-",
  "SA07n919:321": "ścakitavimuktapadakramo jagāma ||80 ||",
  "SA07n919:322": "kanakavalayabhūṣitaprakoṣṭhaiḥ",
  "SA07n919:323": "kamalanibhaiḥ kamalāniva pravidhya |",
  "SA07n919:324": "avanatatanavastato'sya yakṣā-",
  "SA07n919:325": "ścakitagatairdīdhare khurān karāgraiḥ ||81 ||",
  "SA07n919:326": "guruparighakapāṭasaṃvṛtā yā",
  "SA07n919:327": "na sukhamapi dviradairapāvriyante |",
  "SA07n919:328": "vrajati nṛpasute gatasvanāstāḥ",
  "SA07n919:329": "svayamabhavanvivṛtāḥ puraḥ pratolyaḥ ||82 ||",
  "SA07n919:330": "pitaramabhimukhaṃ sutaṃ ca bālaṃ",
  "SA07n919:331": "janamanuraktamanuttamāṃ ca lakṣmīm |",
  "SA07n919:332": "kṛtamatirapahāya nirvyapekṣaḥ",
  "SA07n919:333": "pitṛnagarātsa tato vinirjagāma ||83 ||",
  "SA07n919:334": "atha sa vimalapaṅkajāyatākṣaḥ",
  "SA07n919:335": "puramavalokya nanāda siṃhanādam |",
  "SA07n919:336": "jananamaraṇayoradṛṣṭapāro",
  "SA07n919:337": "na puramahaṃ kapilāvhayaṃ praveṣṭā ||84 ||",
  "SA07n919:338": "iti vacanamidaṃ niśamya tasya",
  "SA07n919:339": "draviṇapateḥ pariṣadgaṇā nananduḥ |",
  "SA07n919:340": "pramuditamanasaśca devasaṅghā",
  "SA07n919:341": "vyavasitapāraṇamāśaśaṃsire'smai ||85 ||",
  "SA07n919:342": "hutavahavapuṣoḥ divaukaso'nye",
  "SA07n919:343": "vyavasitamasya suduṣkaraṃ viditvā |",
  "SA07n919:344": "akṛṣata tuhine pathi prakāśaṃ",
  "SA07n919:345": "ghanavivaraprasṛtā ivendupādāḥ ||86 ||",
  "SA07n919:346": "harituragaturaṅgavatturaṅgaḥ",
  "SA07n919:347": "sa tu vicaranmanasīva codyamānaḥ |",
  "SA07n919:348": "aruṇaparuṣatāramantarikṣaṃ",
  "SA07n919:349": "sa ca subahūni jagāma yojanāni ||87 ||",
  "SA07n919:350": "iti buddhacarite mahākāvye'bhiniṣkramaṇo",
  "SA07n919:351": "nāma pañcamaḥ sargaḥ ||5 ||"
}