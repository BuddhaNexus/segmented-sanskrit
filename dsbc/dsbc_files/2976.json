{
    "2976:0": "yaśodharāviṣaye ḍakṭaraśvīyatīndravimala-caturdhurīṇena viracitā saṅgītamālikā tatra prathamaṁ saṅgītam śrīśrīyaśodharā-stutiḥ 1 janani jagadādhāra-bhaktacitta-prasādike |",
    "2976:1": "avaguṇṭhanahārike putrasannyāsakārike |",
    "2976:2": "tubhyaṁ namaḥ ||",
    "2976:3": "1|",
    "2976:4": "116||",
    "2976:5": "2 janani yaśodhare suyaśoviśāle pāpatāpahāriṇi kāñcanaprojjvale sugatamohini prasūnasukomale dehi te padaṁ mātayatipatibhāle ||",
    "2976:6": "2|",
    "2976:7": "117||",
    "2976:8": "viphale śrutiphale prāgatā bhūtale premapathavihāriṇi puṇyavimale kavitvasumadhure śāstrasamujjvale yatirnauti punastava padakamale ||",
    "2976:9": "3|",
    "2976:10": "118||",
    "2976:11": "3 bimbasundari gope'mba bhaddakacce(1)",
    "2976:12": "subhaddake |",
    "2976:13": "namastubhyaṁ dayāsāre phimpe(2)",
    "2976:14": "rāhulamātṛke ||",
    "2976:15": "tubhyaṁ namaḥ ||",
    "2976:16": "4|",
    "2976:17": "119||",
    "2976:18": "dvitīya-saṅgītam yaśodharā-janma “namo bhagavatorahato sammā saṁbuddhasya“ kapilavāstunagare candrakaradhārāsāre samāgato yadā tathāgataḥ |",
    "2976:19": "vaiśākhapūrṇimātithau pāpatāpau ciradhautau devagīti “rjāto raṇañjahaḥ“ ||",
    "2976:20": "tadā tvamapi janani bhavaduḥkhanivāriṇi gopā viṣamavirahabhītā |",
    "2976:21": "yaśodharā yaśobhāgā viśvajana-manolobhā saṁjātā manoramā-duhitā ||",
    "2976:22": "[āsīstvaṁ rāmāvatāre jagajjananī sītā vasundharāsutā gopārūpā punarapi tvamāgatā yuge yuge pāpaharaṇa-mānasā mātā |",
    "2976:23": "] chandakaścirasārathiḥ jāto hayo manogatiḥ kaṇṭako janmāntarānucaraḥ |",
    "2976:24": "anye līlāsahacarā dharitrīpavitrīkarā (aho)",
    "2976:25": "mahollāsaściraśubhakaraḥ ||",
    "2976:26": "samameva āgatāḥ sarve te priyaparikarāḥ ||",
    "2976:27": "(sadyaḥ) līlāprakīrtane mātarāśīrvādān dadasva naḥ |",
    "2976:28": "tathāgataṁ tvayā sākaṁ yatirnauti punaḥ punaḥ ||",
    "2976:29": "tṛtīyaṁ saṅgītam vivāhavāsare yaśodharā śrībuddhaika-jīvite janani nama'stu te |",
    "2976:30": "vivāhavāsarasajjite susmite namo'stu te ||",
    "2976:31": "1||",
    "2976:32": "124||",
    "2976:33": "vidhātuḥ paramā sṛṣṭirbuddhaikaśaraṇā parā |",
    "2976:34": "“byalaṁkaromi māraṁ na tvāmiti“ bhāṣaṇatatparā ||",
    "2976:35": "2||",
    "2976:36": "125||",
    "2976:37": "hatāśa-sundarānanda-devadattavimānike |",
    "2976:38": "baddhavaṁśamahābhūtirmaitreyāgamasūcike ||",
    "2976:39": "3||",
    "2976:40": "126||",
    "2976:41": "dharmasaṅgha-mahāśakti-mahonmeṣaṇākārike |",
    "2976:42": "daṇḍapāṇi-mahāmāyā-paramānanda-dāyike ||",
    "2976:43": "4||",
    "2976:44": "127||",
    "2976:45": "yantraniṣpeṣite martye hiṁsādbeṣaprapūrite |",
    "2976:46": "sadbuddhiṁ dehi putrebhyo mātargope dharāhite ||",
    "2976:47": "5||",
    "2976:48": "128||",
    "2976:49": "kaliyugapāvani mahājanani namastubhyaṁ namo'stu te |",
    "2976:50": "sutādhamasya yatīndrasya sugopāyai namo'stu te ||",
    "2976:51": "6||",
    "2976:52": "129||",
    "2976:53": "caturthaṁ saṅgītam sukhado jananī-niyata-vikāśaḥ |",
    "2976:54": "kleṣṭe bhuvane satya-prakāśaḥ ||",
    "2976:55": "1||",
    "2976:56": "130||",
    "2976:57": "aho dharitryā guru-santrāsaḥ ||",
    "2976:58": "2||",
    "2976:59": "232||",
    "2976:60": "āṇavakacūrṇahāso dānaveyasamullāso rusa-sphuṭaniko(1)",
    "2976:61": "'ntaka vilāsaḥ ||",
    "2976:62": "3||",
    "2976:63": "132||",
    "2976:64": "jananyā bhavatu paramollāsaḥ |",
    "2976:65": "kaṣāya-bhuvane hitaprakāśaḥ ||",
    "2976:66": "4||",
    "2976:67": "133||",
    "2976:68": "prapañcita-pañcaśīlaṁ jagajjanapuṇyamūlaṁ vardhayatu diśi varasaubhrātram |",
    "2976:69": "pañcaskandhanivārakaṁ caturāryasatyādikaṁ karotu viśvaṁ satatapavitram ||",
    "2976:70": "5||",
    "2976:71": "134||",
    "2976:72": "siṁhalabrahmaśyāma - cīnakambojanīpan - bhūmipuñjaṁ satyadharmajitam |",
    "2976:73": "vilasatu mātṛgānaiḥ putrajāgaraṇatānaiḥ bhavatu bhārataṁ duhitṛ-nanditam ||",
    "2976:74": "6||",
    "2976:75": "135||",
    "2976:76": "prasaratu puṇyaṁ dhyānabṛṁhitam |",
    "2976:77": "karmabhuvi lasatu puṇyamaviratam ||",
    "2976:78": "136||",
    "2976:79": "varade'yi jananīmaṇe deśe mama puṇyadhane ghaṭatāṁ bhrātṛdrohavināśaḥ |",
    "2976:80": "mātarviśvasya parāśvāsaḥ ||8||137||"
}