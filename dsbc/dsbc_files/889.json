{
    "889:0": "mañjuvajrastotram",
    "889:1": "mañjugarbhakṛtam",
    "889:2": "namaḥ śrīmañjuvajrāya",
    "889:3": "śaśadharamiva śubhraṃ khaḍgapustākapāṇiṃ",
    "889:4": "suruciramaliśāntaṃ pañcacīraṃ kumāram |",
    "889:5": "pṛthutaravaramokṣaṃ padmapatrāyatākṣaṃ",
    "889:6": "kumatidahanadakṣaṃ mañjughoṣaṃ namāmi || 1 ||",
    "889:7": "kṛtamṛgaripuyānaṃ dattabhaktapradānaṃ",
    "889:8": "suradanujanṛyānaṃ bodhisattvapradhānam |",
    "889:9": "akhilaguṇanidhānaṃ sarvavidyāvitānaṃ",
    "889:10": "karasarasijabāṇaṃ mañjughoṣaṃ namāmi || 2 ||",
    "889:11": "vibhṛtasakalakoṣaṃ kṣālitājñānadoṣaṃ",
    "889:12": "smaraṇabhajanatoṣaṃ dūrarāgādidoṣam |",
    "889:13": "vihitasasurapoṣaṃ siddhidākhyānayoṣaṃ",
    "889:14": "kṛtajaḍapariśoṣaṃ mañjughoṣaṃ namāmi || 3 ||",
    "889:15": "gaṇapatiśarajanmaśrīmahākālasiṃhaiḥ",
    "889:16": "parivṛtamiva candrābhābhamindīvarākṣam |",
    "889:17": "asi-śara-japamālā-pustakaṃ saṃvahantam",
    "889:18": "urasi lalitamālaṃ mañjughoṣaṃ namāmi || 4 ||",
    "889:19": "surapatiśamanāyāpyeṣa mitrāgnirakṣaḥ-",
    "889:20": "pavanapramathapālaiḥ saṃvṛtaṃ smeravaktram |",
    "889:21": "khagapatirathagātraṃ brahmavandyaṃ ramomā-",
    "889:22": "vihitacaraṇabhaktaṃ mañjughoṣaṃ namāmi || 5 ||",
    "889:23": "yadasikaṭhinadhārācchedamārgābhivāhā (haḥ)",
    "889:24": "sakalasaliladhārāpātitāṅgāṅgamantraḥ |",
    "889:25": "bahuparicayasthalyāṃ bhūminadyāpi reje",
    "889:26": "bahutaramahimānaṃ mañjughoṣaṃ namāmi || 6 ||",
    "889:27": "bhavadabhinavanutyā toṣitā guhyadevī",
    "889:28": "nikhilanigamasārā suprakāśā'tireje |",
    "889:29": "bhavajalanidhipāraṃ dānakalpadrumāgraṃ",
    "889:30": "galitabahumahograṃ mañjughoṣaṃ namāmi || 7 ||",
    "889:31": "vibhajati janaloko dharmadhātuṃ maheśaṃ",
    "889:32": "daśaśatadalapadme saṃsthitaṃ jyotiraiśam |",
    "889:33": "tadapi tava praśastaṃ devamāhātmyamīśaṃ",
    "889:34": "vibhajati bhujageśaṃ mañjughoṣaṃ namāmi || 8 ||",
    "889:35": "paṭhati yadidamiṣṭaṃ mālinīpadyabandhaṃ",
    "889:36": "sa bhavati kavirājo vādisiṃhāsanasthaḥ |",
    "889:37": "sakalavisabhāsu projjvaladvāksudhāraḥ",
    "889:38": "kalitasakalavidyo bhūṣaṇo bhavyadakṣaḥ || 9 ||",
    "889:39": "svayambhūpurāṇoddhṛtaṃ mañjugarbhaviracittaṃ",
    "889:40": "mañjuvajrastotraṃ samāptam |"
}