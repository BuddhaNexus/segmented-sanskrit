{
  "SA05n910:0": "vajrayoginyāḥ stutipraṇidhānam",
  "SA05n910:1": "siddhācārya-virūpādaviracitam",
  "SA05n910:2": "om namaḥ śrī vajrayoginyai,",
  "SA05n910:3": "namobuddhadharmasaṃghebhyaḥ,",
  "SA05n910:4": "namo gurubuddhabodhisattvebhyaḥ,",
  "SA05n910:5": "namo locanādidaśavajravilāsinībhyaḥ,",
  "SA05n910:6": "namo yamāntakādidaśakrodhavīrebhyaḥ saprajñebhyaḥ |",
  "SA05n910:7": "om o ā hūṃ iti kāyavākacita indriyavijñānāni,",
  "SA05n910:8": "vākacittāni manovijñānāni,",
  "SA05n910:9": "paracittajñānābhijñā,",
  "SA05n910:10": "parakāyapraveśābhijñā puṇḍarīkeṣu nābhisthe raktasūrya maṇḍalaḥ || 1 ||",
  "SA05n910:11": "dharmodayā padmamadhye sindūrāruṇarūpiṇī |",
  "SA05n910:12": "triśṛṅgā hi trikoṇā vā catuḥpīṭhasamanvitā || 2 ||",
  "SA05n910:13": "hrīṃkāraṃ madhyabhāge'syāḥ pītavarṇaḥ prakīrtitaḥ |",
  "SA05n910:14": "tadbhavā pītavarṇā ca avadhūtyā (tī)",
  "SA05n910:15": "svayaṃ sthitā || 3 ||",
  "SA05n910:16": "lalanāyāṃ tu suśyāmā rasanāyāṃ ca gaurikā |",
  "SA05n910:17": "pratyālīḍhapadā nāmnā madhye pītā manoramā || 4 ||",
  "SA05n910:18": "trimārge saṃsthitā devī trikāyavajrayoginī |",
  "SA05n910:19": "seyaṃ nāmnā bhavedekā sarvasambuddhaḍākinī || 5 ||",
  "SA05n910:20": "trailokye divyarūpā ca sarvāśāparipūrikā |",
  "SA05n910:21": "svakartinā kartitaṃ ca vāmahaste svamastakam || 6 ||",
  "SA05n910:22": "dakṣiṇe'syā sthitā kartirjagato duḥkhacchedinī |",
  "SA05n910:23": "asyā vāme sthitā śyāmā nāmnā sā varavarṇinī || 7 ||",
  "SA05n910:24": "karparaḥ savyahaste ca kartirvāme ca saṃsthitā |",
  "SA05n910:25": "dakṣiṇe caiva pītā vai vajravairocanī sthitā || 8 ||",
  "SA05n910:26": "kartikā dakṣiṇe haste vāme kapāladhāriṇī |",
  "SA05n910:27": "pratyālīḍhapadā devī nagnikā tu viśeṣataḥ || 9 ||",
  "SA05n910:28": "karau ca dvau tayoḥ pādau viparītau tathā sthitau |",
  "SA05n910:29": "buddhānāṃ jananī madhyā samyaksambuddharūpiṇī || 10 ||",
  "SA05n910:30": "akṣayā subhagā nityaṃ hantrī ca mṛtyujanmanoḥ |",
  "SA05n910:31": "dharmasaṃbhoganirmāṇamahāsukhasvarūpiṇī || 11 ||",
  "SA05n910:32": "seyaṃ tribhuvane devī rājaye candravatsadā |",
  "SA05n910:33": "kabandhādavadhūtyāśca praviśetsvamukheṣvasṛk || 12 ||",
  "SA05n910:34": "lalanārasanābhyāṃ ca niḥsṛtya śirasi tathā |",
  "SA05n910:35": "karūṇāraktarūpeṇa pibantī sarvaśāntaye || 13 ||",
  "SA05n910:36": "duḥkhaṃ saṃprekṣya lokānāṃ caturmāraniṣūdinī |",
  "SA05n910:37": "daurmanasyādi kartyā ca kartantī sarvameva tat || 14 ||",
  "SA05n910:38": "stutā hi jinamātastvaṃ suprasīda bhavārṇave |",
  "SA05n910:39": "svārthaṃ kartuṃ parārthaṃ ca bhavāmyahaṃ yathā kṣamaḥ || 15 ||",
  "SA05n910:40": "tattvayā devī kartavyamanugrahaṃ mayi sṛja |",
  "SA05n910:41": "bālo'hamajñarūpī ca kāyavākacittadoṣataḥ || 16 ||",
  "SA05n910:42": "gurubhaktiṃ na jāne'haṃ tvadbhaktiṃ ca viśeṣataḥ |",
  "SA05n910:43": "ityākalaya lokeśi kṛpā mayi vidhīyatām || 17 ||",
  "SA05n910:44": "kāyavākacetasāṃ sāmyaṃ kuru me tvaṃ prasādataḥ |",
  "SA05n910:45": "yena muñcāmi saṃsāraṃ ṣaḍgatiṃ na viśāmyaham || 18 ||",
  "SA05n910:46": "yathā ca vajravaddeho yāvanmukto bhavāmyaham |",
  "SA05n910:47": "māraiḥ sarvaiḥ parityakto yathā kāyo bhavenmama || 19 ||",
  "SA05n910:48": "līlayā siddhibhāgāḥ syurnirvighnaḥ sarvasiddhibhāk |",
  "SA05n910:49": "sarvasādhanasiddhi syāt kāyaścaivājarāmaraḥ || 20 ||",
  "SA05n910:50": "trāsanaṃ sarvamārāṇāṃ sarvāśāparipūraṇam |",
  "SA05n910:51": "dehi me tvaṃ varāṃ siddhiṃ hitvā caiva gamāgamam || 21 ||",
  "SA05n910:52": "mano vilīyatāṃ devi pañcendriyānusaṃyutam |",
  "SA05n910:53": "siddhyecca vajrasattvāyuryauvanārogyasatsukham || 22 ||",
  "SA05n910:54": "tvatprasādānmahāmudrā sarveṣāmeva saṃbhavet |",
  "SA05n910:55": "devāsuramanuṣyāśca yatra yatra ca saṃsthitāḥ |",
  "SA05n910:56": "sukhinaste ca sarve syurbuddhatvaphalalābhinaḥ || 23 ||",
  "SA05n910:57": "śrīguhyasamayatantre trikāyavajrayoginyāḥ stutipraṇidhānaṃ samāptam |",
  "SA05n910:58": "kṛtiriyaṃ siddhācārya śrīvirūpādānām |"
}