{
  "SA20n1380:0": "ācārya vasubandhuviracitam",
  "SA20n1380:1": "pañcaskandhaprakaraṇam",
  "SA20n1380:2": "prathamādhikāraḥ - skandhoddeśanirdeśakaḥ",
  "SA20n1380:3": "pañcaskandhāḥ",
  "SA20n1380:4": "rupaskandhaḥ vedanāskandhaḥ saṃjñāskandhaḥ saṃskāraskandhaḥ vijñānaskandhaśca ||1 ||",
  "SA20n1380:5": "rupasya dvau bhedau",
  "SA20n1380:6": "rupaṃ katamat |",
  "SA20n1380:7": "yat kiñcid rupaṃ sarvaṃ taccatvāri mahābhūtāni catvāri ca mahābhūtānyupādāya ||2 ||",
  "SA20n1380:8": "mahābhūtanāmāni",
  "SA20n1380:9": "katamāni catvāri mahābhūtāni |",
  "SA20n1380:10": "pṛthivī dhātuḥ abdhātuḥ tejodhātuḥvāyudhātuśca ||3 ||",
  "SA20n1380:11": "mahābhūtalakṣaṇāni",
  "SA20n1380:12": "tatra pṛthivīdhātuḥ katamaḥ | kaṭhinatā ||4 ||",
  "SA20n1380:13": "abdhātuḥ katamaḥ |",
  "SA20n1380:14": "niṣyandatā ||5 ||",
  "SA20n1380:15": "tejodhātuḥ katamaḥ uṣṇatā ||6 |",
  "SA20n1380:16": "vāyudhātuḥ katamaḥ |",
  "SA20n1380:17": "paunaḥ punyakampanatā ||7 ||",
  "SA20n1380:18": "upādāyarupanāmāni",
  "SA20n1380:19": "upādayarupaṃ katamat |",
  "SA20n1380:20": "cakṣurindriyam,",
  "SA20n1380:21": "śrotrendriyam,",
  "SA20n1380:22": "ghraṇendriyam,",
  "SA20n1380:23": "jihvendriyam,",
  "SA20n1380:24": "kāyendriyam,",
  "SA20n1380:25": "rupam,",
  "SA20n1380:26": "śabdaḥ,",
  "SA20n1380:27": "gandhaḥ,",
  "SA20n1380:28": "rasaḥ,",
  "SA20n1380:29": "spraṣṭavyaikadeśaḥ,",
  "SA20n1380:30": "abijñaptiśca ||8 ||",
  "SA20n1380:31": "upādāyarupalakṣaṇāni",
  "SA20n1380:32": "cakṣurindrayaṃ katamat |",
  "SA20n1380:33": "varṇabiṣayo rupaprasādaḥ ||9 ||",
  "SA20n1380:34": "śrotrendrayaṃ katamat |",
  "SA20n1380:35": "śabdaviṣayo rupaprasādaḥ ||10 ||",
  "SA20n1380:36": "ghrāṇendriyam katamat |",
  "SA20n1380:37": "gandhaviṣayo rupaprasādaḥ ||11 ||",
  "SA20n1380:38": "jihvendriyam katamat |",
  "SA20n1380:39": "rasaviṣayo rupaprasādaḥ ||12 ||",
  "SA20n1380:40": "kāyendriyam katamat |",
  "SA20n1380:41": "spraṣṭavyaviṣayo rupaprasādaḥ ||13 ||",
  "SA20n1380:42": "rupaṃ katamat |",
  "SA20n1380:43": "cakṣurgocaraḥ |",
  "SA20n1380:44": "varṇaḥ saṃsthānam bijñaptiśca ||14 ||",
  "SA20n1380:45": "śabdaḥ katamaḥ |",
  "SA20n1380:46": "śrotragocaraḥ |",
  "SA20n1380:47": "niṣpannḥ aniṣpannaḥ ubhayaḥ |",
  "SA20n1380:48": "upāttacatumarhābhutahetukaḥ anupāttacaturmahābhūtahetukaḥ ubhayacaturmahābhūtahetukaśca ||15 ||",
  "SA20n1380:49": "gandhaḥ katamaḥ |",
  "SA20n1380:50": "ghrāṇagocaraḥ |",
  "SA20n1380:51": "surabhiḥ asurabhiḥ tadanyaśca ||16 ||",
  "SA20n1380:52": "rasaḥ katamaḥjihvāgocaraḥ |",
  "SA20n1380:53": "madhuraḥ amalaḥ lavaṇaḥ kaṭuḥ tiktaḥ kaṣāyaśc ||17 ||",
  "SA20n1380:54": "spraṣṭavyaḥ katamaḥ |",
  "SA20n1380:55": "kāyagocaraḥ |",
  "SA20n1380:56": "mahābhūtānyupādāya ślakṣṇatvam karkasatbam gurutvam laghutvam śītatvam jighatsā pipāsā ca ||18 ||",
  "SA20n1380:57": "abijñaptirupaṃ katamat |",
  "SA20n1380:58": "vijñaptisamādhisaṃbhūtaṃ rupam anidarśanam apratigham ||19 ||",
  "SA20n1380:59": "rupaskandhaḥ soniddeśaḥ pariniṣṭhitaḥ |",
  "SA20n1380:60": "atha vedanā salakṣaṇaprabhedā",
  "SA20n1380:61": "vedanā katamā |",
  "SA20n1380:62": "trividho'nubhavaḥ |",
  "SA20n1380:63": "sukhaḥ duḥkhaḥ aduḥkhāsukhaśca ||20 ||",
  "SA20n1380:64": "sukho yasmin nirudhde saṃyogecchā jāyate ||21 ||",
  "SA20n1380:65": "duḥkho yasminnutpanne viyogecchā jāyate ||22 ||",
  "SA20n1380:66": "aduḥkhāsukho yasminnutpanne nobhayecchā jāyane ||23 ||",
  "SA20n1380:67": "vedanaskandhaḥ sonideśanirdeśaḥ parinisṭhataḥ |",
  "SA20n1380:68": "atha saṃjñā lakṣaṇa prabhedā",
  "SA20n1380:69": "saṃjñā katamā |",
  "SA20n1380:70": "viṣayanimittod grahaṇam ||24 ||",
  "SA20n1380:71": "sā tridhā parittā mahadgatā apramāṇā ca ||25 ||",
  "SA20n1380:72": "saṃjñā skandhaḥ soddeśanirdeśaḥ parinisṭhitaḥ |",
  "SA20n1380:73": "saṃskārāṇāṃ dvaubhedau",
  "SA20n1380:74": "saṃskārāḥ katame |",
  "SA20n1380:75": "vedanāsaṃjñābhyāmanye caitāḥ cittaviprayuktāśca ||26 ||",
  "SA20n1380:76": "tatra caitadharmāḥ katame |",
  "SA20n1380:77": "ye dharmārścittena saṃprayuktāḥ ||27 ||",
  "SA20n1380:78": "caitaparisaṃkhyānam",
  "SA20n1380:79": "te katame | sparśaḥ manaskāraḥ vedanā saṃjñā cetanā chandaḥ adhimokṣaḥ smṛtiḥ samādhiḥ prajñā śradhdā hrīḥ apatrapā alobhaḥ kuśalamūlam adveṣaḥkuśalamūlam amohaḥkuśalamūlam vīryam praśrabdhiḥ apramādaḥ upekṣā avihiṃsā rāga pratighaḥ mānaḥ avidyā dṛṣṭiḥ vicikitsā krodha upanāhaḥ mrakṣaḥ pradāśaḥ īrṣyā mātsaryarm māyā śāṭhyam madaḥ ahiṃsā āhrikyam anapatrāpyam styānam,",
  "SA20n1380:80": "auddhatyam,",
  "SA20n1380:81": "āśraddhyam,",
  "SA20n1380:82": "kauśīddhyam pramādaḥ muṣitāsmṛti,",
  "SA20n1380:83": "bikṣepaḥ,",
  "SA20n1380:84": "aprasajanyam,",
  "SA20n1380:85": "kaukrityam,",
  "SA20n1380:86": "middham,",
  "SA20n1380:87": "vitarkaḥ, vicāraśca ||28 ||",
  "SA20n1380:88": "eteśāṃ paśca sarvatragāḥ |",
  "SA20n1380:89": "pañcapratiniyataviṣayā |",
  "SA20n1380:90": "ekādaśakuśalāḥ |",
  "SA20n1380:91": "ṣaṭkleśāḥ |",
  "SA20n1380:92": "śiṣṭā upakleśāḥ |",
  "SA20n1380:93": "catvāro'nyatrāpi bhavantītyaniyatāḥ ||29 ||",
  "SA20n1380:94": "caittalakṣaṇāni",
  "SA20n1380:95": "sparśaḥkatamaḥ |",
  "SA20n1380:96": "trikasaṃnipāte indriyavikāraparicchedaḥ ||30 ||",
  "SA20n1380:97": "manaskāraḥ katamaḥ |",
  "SA20n1380:98": "cetasa ābhogaḥ ||31 ||",
  "SA20n1380:99": "cetanā katamā |",
  "SA20n1380:100": "guṇadoṣānubhayeṣu cittābhisaṃskāro manaskarma ||32 ||",
  "SA20n1380:101": "chandaḥ katamaḥ |",
  "SA20n1380:102": "abhiprete vastunyabhilāṣaḥ ||33 ||",
  "SA20n1380:103": "adhimokṣaḥ katamaḥ |",
  "SA20n1380:104": "niścite vastuni tathaivāvadhāraṇam ||34 ||",
  "SA20n1380:105": "smṛtiḥ katamā |",
  "SA20n1380:106": "saṃstute vastunyasaṃpramoṣaḥ ||35 ||",
  "SA20n1380:107": "samādhiḥ katamaḥ |",
  "SA20n1380:108": "upaparīkṣye vastuni cittasyaikāgratā ||36 ||",
  "SA20n1380:109": "prajñā katamā |",
  "SA20n1380:110": "tatraiva pravicayo yogāyogavihito'nyathā ca ||37 ||",
  "SA20n1380:111": "śraddā katamā |",
  "SA20n1380:112": "karmafalāsatyaratneṣvabhisaṃpratyayaḥ |",
  "SA20n1380:113": "cetaso'bhilāṣaḥ cetasaḥ prasādaḥ ||38 ||",
  "SA20n1380:114": "hrīḥ | katamā ātmānaṃ dharmaṃ vādhipatiṃ kṛtvā svayamavayena lajjā ||39 ||",
  "SA20n1380:115": "apatrapā katamā |",
  "SA20n1380:116": "lokamadhipatiṃ kṛtvā parato'vadhena lajjā ||40 ||",
  "SA20n1380:117": "alobhaḥ katamaḥ |",
  "SA20n1380:118": "lobha pratipakṣaḥ |",
  "SA20n1380:119": "cetaso dānakṣetrāparigrahaḥ ||41 ||",
  "SA20n1380:120": "adveṣaḥ katamaḥ |",
  "SA20n1380:121": "dveṣapratipakṣaḥ | maitri ||42 ||",
  "SA20n1380:122": "amohaḥ katamaḥ |",
  "SA20n1380:123": "mohapratipakṣaḥ |",
  "SA20n1380:124": "yathābhūtasaṃpratipattiḥ ||43 ||",
  "SA20n1380:125": "vīryaṃ katamat kausīddyapratipakṣaḥ kuśale cetaso'bhyutsāhaḥ ||44 ||",
  "SA20n1380:126": "praśrabdhiḥ katamā |",
  "SA20n1380:127": "dauṣṭhulyapratipakṣaḥ kāyacittakarmaṇyatā ||45 ||",
  "SA20n1380:128": "apramādaḥ katamaḥ |",
  "SA20n1380:129": "pramādapratipakṣaḥ |",
  "SA20n1380:130": "alobhādīn vīryāntān niśritya akuśalānāṃ dharmāṇāṃ prahāṇam |",
  "SA20n1380:131": "tatpratipakṣāṇāṃ kuśalānāṃ dharmāṇāṃ bhāvanā ||46 ||",
  "SA20n1380:132": "upekṣā katamā |",
  "SA20n1380:133": "alobhādīn vīryantān niśritya cittasamatā |",
  "SA20n1380:134": "cittapraśastā |",
  "SA20n1380:135": "cittānābhogatā |",
  "SA20n1380:136": "yayākliṣṭadharmānavakāśenā'kliṣṭeṣu sthitiḥ ||47 ||",
  "SA20n1380:137": "avihiṃsā katamā |",
  "SA20n1380:138": "vihiṃsāpratipakṣaḥ karuṇā ||48 ||",
  "SA20n1380:139": "rāgaḥ katamaḥ |",
  "SA20n1380:140": "upādānaskandheṣvabhyarthanā |",
  "SA20n1380:141": "adhyavasānam ||49",
  "SA20n1380:142": "pratighaḥ katamaḥ |",
  "SA20n1380:143": "satveṣvāghātacittatā ||50 ||",
  "SA20n1380:144": "mānaḥ katamaḥ |",
  "SA20n1380:145": "mānaḥ saptavidhaḥ |",
  "SA20n1380:146": "atimānaḥ , mānātimānaḥ asmimānaḥ ,",
  "SA20n1380:147": "abhimānaḥ ,",
  "SA20n1380:148": "ūnamānaḥ ,",
  "SA20n1380:149": "mithyāmānaśca ||51 ||",
  "SA20n1380:150": "atimānaḥ katamaḥ |",
  "SA20n1380:151": "hīnāt śreyānahamasmi sadṛśena vā sadṛśo'smīti cintayato yā cittasyonnatiḥ ||53 ||",
  "SA20n1380:152": "mānātimānaḥ katamaḥ |",
  "SA20n1380:153": "śreyaso'hameva śreyāniti cintayato yā cittasyonnatiḥ ||54 ||",
  "SA20n1380:154": "asmimānaḥ katamaḥ |",
  "SA20n1380:155": "pañcasūpādānaskandheṣvātmānamātmīyaṃ vā paśyato yā cittasyonnati ||55 ||",
  "SA20n1380:156": "abhimānaḥ katamaḥ |",
  "SA20n1380:157": "aprāpta uttare viśeṣādhigame prāpto mayeti cintayato yā cittasyonnatiḥ ||56 ||",
  "SA20n1380:158": "ūnamānaḥ katamaḥ |",
  "SA20n1380:159": "bahṛvantaraviśiṣṭādalpāntarahino'smīti cintayato yā cittasyonnatiḥ ||57 ||",
  "SA20n1380:160": "mithyāmānaḥ katamaḥ |",
  "SA20n1380:161": "aguṇavato guṇavānahamasmīti cintayato yā cittasyonnatiḥ ||58 ||",
  "SA20n1380:162": "aviddhyā katamā |",
  "SA20n1380:163": "karmafalasatyaratnānāmajñānam |",
  "SA20n1380:164": "sā punaḥ sahajā parikalpitā ca ||59 ||",
  "SA20n1380:165": "rāgacaritasya rāgapratighayoḥ rāgacaritasyāviddyāyāścetyakuśalānāṃ trīṇī mūlāni |",
  "SA20n1380:166": "lobho'kuśalamūlaṃ ddheṣo'kuśalamūlaṃ ,",
  "SA20n1380:167": "moho'kuśalamūlaṃ ca ||60 ||",
  "SA20n1380:168": "dṛṣṭiḥ katamā pañcadṛṣṭyaḥ - satkāyadṛṣṭiḥ,",
  "SA20n1380:169": "antargrāhyadṛṣṭiḥ,",
  "SA20n1380:170": "mithyādṛṣṭiḥ ,",
  "SA20n1380:171": "dṛṣṭiparāmarśaḥ,",
  "SA20n1380:172": "śīlavrataparāmarśaśca ||61 ||",
  "SA20n1380:173": "satkāyadṛṣṭiḥ katamā |",
  "SA20n1380:174": "pañcopādāna - skandhānātmata ātmīyato vā paśyato yā kliṣṭā prajñā ||62 ||",
  "SA20n1380:175": "antargrāhyadṛṣṭiḥ katamā |",
  "SA20n1380:176": "tānevādhipatiṃ kṛtvā śāśvatata ucchedato vā paśyato yā kliṣṭā prajñā ||63 ||",
  "SA20n1380:177": "mithyādṛṣṭiḥ katamā |",
  "SA20n1380:178": "hetuṃ vā falaṃ vā kriyāṃ vāpavadataḥ sadāvastu vināśayato yā kliṣṭā prajñā ||64 ||",
  "SA20n1380:179": "dṛṣṭiparāmarśaḥ katamaḥ |",
  "SA20n1380:180": "tā eva tikhro dṛṣṭistadāśrayāṃśca skandhān śreṣṭhataḥ paramataśca paśyato yā kliṣṭā prajñā ||65 ||",
  "SA20n1380:181": "śīlabrataparāmarśaḥ katamaḥ |",
  "SA20n1380:182": "śīlaṃ vrataṃ tadāśrayāṃśca skandhānaśuddhito muktito naiyāṇikataśca paśyato yā kliṣṭā prajñā ||66 ||",
  "SA20n1380:183": "vicikitsā katamā |",
  "SA20n1380:184": "satyādiṣu yā dvikoṭikā matiḥ ||67 ||",
  "SA20n1380:185": "eteṣāṃ kleśānāṃ paścimāstistro dṛṣṭayaḥ vīcikitsā ca parikalpitāḥ |",
  "SA20n1380:186": "śiṣṭāḥ sahajāḥ parikalpitāśca ||68 ||",
  "SA20n1380:187": "krodhaḥ katamaḥ |",
  "SA20n1380:188": "vartaṃmānamapakāramāgamya yā cetasa āghātacetanā ||69 ||",
  "SA20n1380:189": "upanāhaḥ katamaḥ |",
  "SA20n1380:190": "vairānubandha ||70 ||",
  "SA20n1380:191": "mrakṣaḥ katamaḥ ātmano'vaddhapracchādanā ||71 ||",
  "SA20n1380:192": "pradāśaḥ katamaḥ caṇḍavacodāśitā ||72 ||",
  "SA20n1380:193": "īrṣyā katamā parasaṃpattau cetaso vyāropa ||73 ||",
  "SA20n1380:194": "mātsarya katamat |",
  "SA20n1380:195": "dānavirodhī cetasa āgrahaḥ ||74 ||",
  "SA20n1380:196": "māyā katamā |",
  "SA20n1380:197": "paravaścanābhūtārthasadarśanatā ||75 ||",
  "SA20n1380:198": "śāṭhyaṃ katamat |",
  "SA20n1380:199": "svadoṣapracchādanopāyasaṃgṛhitaṃ cetasaḥ kauṭilyam ||76 ||",
  "SA20n1380:200": "madaḥ katamaḥ |",
  "SA20n1380:201": "svasaṃpattau raktasyoddharṣaḥ |",
  "SA20n1380:202": "cetasaḥ paryādānam ||77 ||",
  "SA20n1380:203": "vihiṃsā katamā |",
  "SA20n1380:204": "satveṣu viheṭhanā ||78 ||",
  "SA20n1380:205": "āhrīkyaṃ katamat |",
  "SA20n1380:206": "avadyena svayamalajjā ||79 ||",
  "SA20n1380:207": "anapatrāp katamat |",
  "SA20n1380:208": "avadyena parato'lajjā ||80",
  "SA20n1380:209": "satyānaṃ katamat |",
  "SA20n1380:210": "cittākarmaṇyatā |",
  "SA20n1380:211": "staimityam ||81 ||",
  "SA20n1380:212": "auddhatyaṃ katamat |",
  "SA20n1380:213": "cittasyāvyupasamaḥ ||82 ||",
  "SA20n1380:214": "āśraddhyaṃ katamat |",
  "SA20n1380:215": "karmaphalasatyaratneṣvanabhisaṃpratyayaḥ |",
  "SA20n1380:216": "śraddhāvipakṣaścetaso'prasādaḥ ||83 ||",
  "SA20n1380:217": "kauśiddyaṃ katamatṃḥ kuśale cetaso'nabhyutsāho vīryavipakṣa ||84 ||",
  "SA20n1380:218": "pramādaḥ katamaḥ |",
  "SA20n1380:219": "yā lobhadveṣamohekauśīddyaiścitasyānārakṣā kuśalasyābhāvanā ||85 ||",
  "SA20n1380:220": "muṣitāsmṛtiḥ katamā |",
  "SA20n1380:221": "kliṣṭāsmṛtiḥ |",
  "SA20n1380:222": "kuśalāpratipatiḥ ||86 ||",
  "SA20n1380:223": "vikṣepaḥ katamaḥ |",
  "SA20n1380:224": "rāgadveṣamohāṃśikaḥ pañcakāmaguṇeṣu cittasya visāraḥ ||87 ||",
  "SA20n1380:225": "aprasajanyaṃ katamat kleśasaṃprayuktā prajñā |",
  "SA20n1380:226": "tayā kāyavākcittacaryā asaṃviditeva pravartate ||88 ||",
  "SA20n1380:227": "kaukṛtyaṃ katamat |",
  "SA20n1380:228": "cetaso vipratisāraḥ ||89 ||",
  "SA20n1380:229": "middhaṃ katamat |",
  "SA20n1380:230": "asvataṃtravṛtticetaso'bhisaṃkṣepaḥ ||90 ||",
  "SA20n1380:231": "vitarkaḥ katamaḥ paryeṣako manojalpaḥ cetanāprajñāviśeṣaḥ yā cittasyaudārikatā ||91 ||",
  "SA20n1380:232": "vicāraḥ katamaḥ |",
  "SA20n1380:233": "paryeṣako manojalpaḥ tathaiva yā cittasya sūkṣmatā ||92 ||",
  "SA20n1380:234": " |ittaviprayuktaparisaṃkhyānam ||",
  "SA20n1380:235": "cittaviprayuttasaṃskārāḥ katame |",
  "SA20n1380:236": "ye rupacitacaittadhikāre prajñaptāḥ ,",
  "SA20n1380:237": "ta evānyatrāprajñaptāḥ ||93 ||",
  "SA20n1380:238": "te katame | prāpti,",
  "SA20n1380:239": "asaṃjñisamāpattiḥ ,",
  "SA20n1380:240": "nirodhasamāpattiḥ ,",
  "SA20n1380:241": "āsaṃjñikam ,",
  "SA20n1380:242": "jīvitendriyam,",
  "SA20n1380:243": "nikāyasabhāgatā,",
  "SA20n1380:244": "jāti ,",
  "SA20n1380:245": "jarā,",
  "SA20n1380:246": "sthitiḥ,",
  "SA20n1380:247": "anityatā,",
  "SA20n1380:248": "nāmakāyaḥ,",
  "SA20n1380:249": "padakāyaḥ,",
  "SA20n1380:250": "vyañjanakāyaḥ,",
  "SA20n1380:251": "pṛthagjanatvamityevamādibhedasamādānāḥ ||94 ||",
  "SA20n1380:252": " |ittaviprayuktalakṣaṇāni ||",
  "SA20n1380:253": "tatra prāptiḥ katamā |",
  "SA20n1380:254": "pratilambhaḥ samanvāgamaśca |",
  "SA20n1380:255": "bījam , vaśitā,",
  "SA20n1380:256": "abhimukhībhāvaśca yathāyogam ||95 ||",
  "SA20n1380:257": "asaṃjñisamāpattiḥ katamṃ |",
  "SA20n1380:258": "śubhakṛtsnavītarāgasyoparyavītarāgasya niḥsaraṇasaṃjñāpūrvakeṇa manasikāreṇāvasthāvarāṇāṃ cittacaittadharmāṇāṃ yo nirodhaḥ ||96 ||",
  "SA20n1380:259": "nirodhasamāpattiḥ katamā |",
  "SA20n1380:260": "ākiñcanyāyatanabītarāgasya bhavāgratāduccalitasya śāntavihāra - saṃjñāpūrvakeṇa manasikāreṇāsthāvarāṇāṃ cittacaittadharmāṇāṃ tadekatyānāṃ ca sthāvarāṇāṃ yo nirodhaḥ ||97 ||",
  "SA20n1380:261": "āsaṃjñikaṃ katamat |",
  "SA20n1380:262": "asaṃjñisatvanikāyadeveṣupapannasyāsthāvarāṇāṃ cittacaittadharmāṇāṃ nirodhaḥ ||98 ||",
  "SA20n1380:263": "jīvitendriyam katamat |",
  "SA20n1380:264": "nikāyasabhāgeṣu pūrvakarmānuviddhyeṣu yaḥ saṃskārāṇāṃ sthitikālaniyamaḥ ||99 ||",
  "SA20n1380:265": "nikāyasabhāgaḥ katamaḥ |",
  "SA20n1380:266": "yā satvānāmātmabhāvasadṛśatā ||100 ||",
  "SA20n1380:267": "jātiḥkatamā |",
  "SA20n1380:268": "nikāyasabhāge saṃskārāṇāmabhūtvā yo bhāṣaḥ ||101 ||",
  "SA20n1380:269": "jarā katamā |",
  "SA20n1380:270": "tathā teṣāṃ prabandhānyathātvam ||102 ||",
  "SA20n1380:271": "sthiti katamā |",
  "SA20n1380:272": "tathā teṣāṃ prabandhanvayaḥ ||103 ||",
  "SA20n1380:273": "anityatā katamā |",
  "SA20n1380:274": "tathā teṣāṃ prabandhavināśaḥ ||104 ||",
  "SA20n1380:275": "nāmakāyaḥ katamaḥ |",
  "SA20n1380:276": "dharmāṇāṃ svabhāvādhivacanam ||105 ||",
  "SA20n1380:277": "pādakāyaḥ katamaḥ |",
  "SA20n1380:278": "dharmāṇāṃ viśeṣādhivacanam ||106 ||",
  "SA20n1380:279": "vyañjanakāyaḥ katamaḥ |",
  "SA20n1380:280": "akṣarāṇi |",
  "SA20n1380:281": "tadubhayābhivyañjanatāmupādāya ||107 ||",
  "SA20n1380:282": "vākyāni tāni |",
  "SA20n1380:283": "nāmapadāśreyeṇārthavacanatāmupādāya ||108 ||",
  "SA20n1380:284": "akṣaraṃ punaḥ paryāyākṣaraṇatāmupādāya ||109 ||",
  "SA20n1380:285": "pṛthagjanatvaṃ katamat |",
  "SA20n1380:286": "āryadharmāṇāmapratilambhaḥ ||110 ||",
  "SA20n1380:287": "iti saṃskāraskandho nāma ||",
  "SA20n1380:288": "saṃskāraskandhaḥ soddyeśanirdeśaḥ pariniṣṭhitaḥ ||111 ||",
  "SA20n1380:289": " ||vijñānalakṣaṇam ||",
  "SA20n1380:290": "vijñānaṃ katamat |",
  "SA20n1380:291": "ālambanaṃbijñaptiḥ ||112 ||",
  "SA20n1380:292": "cittamanasī vijñānaparyāyau sanirvacano taccitaṃ mano'pi citrīkāratā manoniśrayatāṃ ca upādāya ||113 ||",
  "SA20n1380:293": " ||salakṣaṇacittanirdeśaḥ ||",
  "SA20n1380:294": "mūlacittamālayavijñānaṃ tadyathedaṃ sarvasaṃskārāṇāṃ saṃcitaṃ bījam ||114 ||",
  "SA20n1380:295": "tatpunarālambanamapyuparichinnamekasantānavarti |",
  "SA20n1380:296": "yathā nirodhasamāpatyasaṃjñisamāpatyā - saṃjñikebhyovyutthitasyāsmād viṣayavijñaptikaṃ nāma pravṛtti vijñānaṃ jāyate |",
  "SA20n1380:297": "ālambanapratyayamapekṣya bhinnākāreṣu vṛttitāmucchiddya punarjātatāṃ saṃsāre pravṛttinivṛtitāṃ copādāya tadālambanaṃ nāma vijñānam ||115 ||",
  "SA20n1380:298": "ālayavijñānaṃ hi sarvabījālayatāmātmabhāvālayahetutāmātmabhāve sthititāṃ copādāya ||116 ||",
  "SA20n1380:299": "ādānavijñānamapi tat |",
  "SA20n1380:300": "ātmabhāvādānatāmupādāya ||117 ||",
  "SA20n1380:301": " ||salakṣaṇamanonirdeśaḥ ||",
  "SA20n1380:302": "mūlamana ālayavijñānamāśritya nityamātmamohātmadṛṣṭyātmamānātmasnehaiḥ saṃprayuktaṃ vijñānamekajātīyasantānavarti arhatvāryamārganirodhasamāpattikāle vyāvartate ||118 ||",
  "SA20n1380:303": "vijñānaskandhaḥ soniddeśanirdeśaḥ pariniṣṭhitaḥ ||",
  "SA20n1380:304": " ||skandhanirvacanam ||",
  "SA20n1380:305": "kimarthaḥ skandhaḥ rāśyarthaḥ |",
  "SA20n1380:306": "kālagotrākāragativiṣayabhinnānāṃ rupādīnāmabhisaṃkṣepatāmupādāya ||119 ||",
  "SA20n1380:307": "skandhoddyeśanirdeśakaḥ prathamādhikāraḥ pariniṣṭhitaḥ ||",
  "SA20n1380:308": "dvitīyādhikāraḥ",
  "SA20n1380:309": "āyatanoddyeśanirdeśakaḥ",
  "SA20n1380:310": "dvādaśāyatanāni |",
  "SA20n1380:311": "cakṣurāyatanam ,",
  "SA20n1380:312": "rupāyatanam ,",
  "SA20n1380:313": "śrotrāyatanam,",
  "SA20n1380:314": "śabdāyatanam ,",
  "SA20n1380:315": "ghrāṇāyatanam ,",
  "SA20n1380:316": "gandhāyatanam ,",
  "SA20n1380:317": "jihvāyatanam ,",
  "SA20n1380:318": "rasāyatanam ,",
  "SA20n1380:319": "kāyāyatanam ,",
  "SA20n1380:320": "spraṣṭavyāyatanam,",
  "SA20n1380:321": "mana āyatanam,",
  "SA20n1380:322": "dharmāyatanaṃ ca ||120 ||",
  "SA20n1380:323": " |'ayatanānāṃ salakṣaṇaprapañcaḥ ||",
  "SA20n1380:324": "cakṣurādīni rupaśabdagandharasāyatanāni yathoktapūrvāṇi ||121 ||",
  "SA20n1380:325": "spraṣṭavyāyatanaṃ catvāri mahābhūtāni uktaśca yaḥ spraṣṭabyaikadeśaḥ ||122 ||",
  "SA20n1380:326": "mana āyatanaṃ yo vijñānaskandhaḥ ||123 ||",
  "SA20n1380:327": "dharmāyatanaṃ vedanā ,",
  "SA20n1380:328": "saṃjñā ,",
  "SA20n1380:329": "saṃskārāḥ ,",
  "SA20n1380:330": "avijñaptiḥ ,",
  "SA20n1380:331": "asaṃskṛtāśca ||124 ||",
  "SA20n1380:332": " |'saṃskṛtāḥ salakṣaṇaprabhedāḥ ||",
  "SA20n1380:333": "asaṃskṛtā katame |",
  "SA20n1380:334": "ākāśam , apratisaṃkhyānirodhaḥ ,",
  "SA20n1380:335": "pratisaṃkhyānirodhaḥ tathatā ca ||125 ||",
  "SA20n1380:336": "tatrākāśaṃ katamat |",
  "SA20n1380:337": "rupāvakāśaḥ ||126 ||",
  "SA20n1380:338": "apratisaṃkhyānirodhaḥ katamaḥ |",
  "SA20n1380:339": "yo nirodho na visaṃyogaḥ ||127 ||",
  "SA20n1380:340": "sakleśā'pratipakṣeṇa skandhānāmātyantiko nirodhaḥ ||128 ||",
  "SA20n1380:341": "pratisaṃkhyānirodhaḥ katamaḥ |",
  "SA20n1380:342": "yo nirodho visaṃyogaḥ ||129 ||",
  "SA20n1380:343": "sakleśapratipakṣeṇa skandhānāmātyantiko nirodhaḥ ||130 ||",
  "SA20n1380:344": "tathatā katamā |",
  "SA20n1380:345": "yā dharmāṇāṃ dharmatā |",
  "SA20n1380:346": "dharmanairātmyam ||131 ||",
  "SA20n1380:347": " |'ayatananirvacanam ||",
  "SA20n1380:348": "kimupādāyāyatanamiti |",
  "SA20n1380:349": "vijñānotpādadvāratāmupādāya ||132 ||",
  "SA20n1380:350": "āyatanoddyeśanirdeśako dvitīyādhikāraḥ pariniṣṭhitaḥ |",
  "SA20n1380:351": "tṛtīyādhikāraḥ",
  "SA20n1380:352": "dhātūddeśanirdeśakaḥ",
  "SA20n1380:353": " ||dhātunāmāni ||",
  "SA20n1380:354": "aṣṭadaśa dhātavaḥ |",
  "SA20n1380:355": "cakṣurdhātuḥ,",
  "SA20n1380:356": "rupadhātuḥ,",
  "SA20n1380:357": "cakṣurvijñānadhātu ,",
  "SA20n1380:358": "śrotradhātuḥ,",
  "SA20n1380:359": "śabdadhātuḥ,",
  "SA20n1380:360": "śrotravijñānadhātuḥ,",
  "SA20n1380:361": "ghrāṇa-dhātuḥ,",
  "SA20n1380:362": "gandhadhātuḥ,",
  "SA20n1380:363": "ghrāṇavijñānadhātuḥ,",
  "SA20n1380:364": "jihvādhātuḥ,",
  "SA20n1380:365": "rasadhātuḥ,",
  "SA20n1380:366": "jihvāvijñānadhātuḥ ,",
  "SA20n1380:367": "kāyadhātuḥ ,",
  "SA20n1380:368": "spraṣṭavyadhātuḥ ,",
  "SA20n1380:369": "kāyabijñānadhātuḥ ,",
  "SA20n1380:370": "manodhātuḥ ,",
  "SA20n1380:371": "dharmadhātuḥ ,",
  "SA20n1380:372": "manovijñānadhātuśca ||133 ||",
  "SA20n1380:373": " ||dhātuprapaścaḥ ||",
  "SA20n1380:374": "cakṣurādayo dhātavo rupādayo dhātavaśca yathāyatanāni ||134 ||",
  "SA20n1380:375": "ṣaḍvijñānadhātavaścakṣurāddyāśrayā rupādyālambanāvijñaptayaḥ ||135 ||",
  "SA20n1380:376": "manodhātusteṣāmeva samanantaranirudhyaṃ ṣaḍvijñānadeśanāśrayatāmupādāya ||136 ||",
  "SA20n1380:377": " ||dhātuvyavasthanam ||",
  "SA20n1380:378": "ebamaṣṭādaśadhātuvyavasthānam |",
  "SA20n1380:379": "yo rupaskandhastāni daśāyatanadhātavaḥ ||137 ||",
  "SA20n1380:380": "dharmāyatanaṃ dharmadhātvekadeśaḥ ||138 ||",
  "SA20n1380:381": "yo vijñānaskandhastanmana āyatanam ,",
  "SA20n1380:382": "saptama manodhātavaśca ||139 ||",
  "SA20n1380:383": "anye ye trayaḥ skandhā rupadhātvekadeśaśca ,",
  "SA20n1380:384": "te'saṃskṛtai sahitā dharmāyatanaṃ dharmadhātuśca ||140 ||",
  "SA20n1380:385": " ||dhātunirvacanam ||",
  "SA20n1380:386": "kimupādāya dhātava iti |",
  "SA20n1380:387": "ākārakātmalakṣaṇadhāraṇatāmupādāya ||141 ||",
  "SA20n1380:388": " ||skandhāyatanadhātukrameṇa dharmadeśanāyāḥ prayojanam ||",
  "SA20n1380:389": "kimartha skandhādikrameṇa deśanā |",
  "SA20n1380:390": "trividhātmagrāhapratipakṣeṇāyaṃ kramaḥ ||142 ||",
  "SA20n1380:391": "ātmagrāhastrividhaḥ ekātmagrāhaḥ ,",
  "SA20n1380:392": "vedakātmagrāhaḥ,",
  "SA20n1380:393": "kārakātmagrāhaśca ||143 ||",
  "SA20n1380:394": " |'ṣṭādaśadhātuvikalpāḥ ||",
  "SA20n1380:395": "aṣṭādaśadhātuṣu kati rupiṇaḥ |",
  "SA20n1380:396": "ye rūpaskandhasvabhāvāḥ ||144 ||",
  "SA20n1380:397": "katyarupiṇaḥ | śiṣṭāḥ ||145 ||",
  "SA20n1380:398": "kati sanidarśanāḥ |",
  "SA20n1380:399": "rupadhāturekaḥ sanidarśanaviṣayaḥ ||146 ||",
  "SA20n1380:400": "katyanidarśanāḥ | śiṣṭāḥ ||147 ||",
  "SA20n1380:401": "kati sapratidhāḥ |",
  "SA20n1380:402": "daśa rupiṇaḥ |",
  "SA20n1380:403": "yatra yatpratidhātastatra tat sapratigham ||148 ||",
  "SA20n1380:404": "katyapratighāḥ | śiṣṭāḥ ||149 ||",
  "SA20n1380:405": "kati sāsravāḥ |",
  "SA20n1380:406": "pañcadaśa paścimānāṃ trayāṇāmekadeśaśca |",
  "SA20n1380:407": "te kleśotpādapratyakṣacaryāviṣayatāmupādāyaṃ ||150 ||",
  "SA20n1380:408": "katyayanāstravāḥ |",
  "SA20n1380:409": "paścimānāṃ trayāṇāmekadeśaḥ ||151 ||",
  "SA20n1380:410": "kati kāmapratisaṃyuktāḥ |sarva ||152 ||",
  "SA20n1380:411": "kati rupapratisaṃyuktāḥ |",
  "SA20n1380:412": "caturdaśa sthāpayitvā gandharasaghrāṇajihvāvijñānadhātūn ||153 ||",
  "SA20n1380:413": "katyārupyasaṃprayuktāḥ |",
  "SA20n1380:414": "paścimāśrayaḥ ||154 ||",
  "SA20n1380:415": "katyapratisaṃyuktāḥ |",
  "SA20n1380:416": "trayāṇāmekadeśaḥ ||155 ||",
  "SA20n1380:417": "kati skandhasaṃgrahītāḥ |",
  "SA20n1380:418": "asaṃskṛtān sthāpayitvā ||156 ||",
  "SA20n1380:419": "katyupādānaskandhasaṃgṛhītāḥ ye sāsravāḥ ||157 ||",
  "SA20n1380:420": "kati kuśalākuśalāvyākṛtāḥ |",
  "SA20n1380:421": "daśa trividhāḥ |",
  "SA20n1380:422": "sapta manodhātavaḥ,",
  "SA20n1380:423": "rupaśabdadharmadhātavaśca |",
  "SA20n1380:424": "śiṣṭā avyākṛtāḥ ||158 ||",
  "SA20n1380:425": "katyādhyātmikāḥ |",
  "SA20n1380:426": "dvādaśa |",
  "SA20n1380:427": "sthāpayitvā rupaśabda gandharasaspraṣṭāvyadharmadhātūn ||159 ||",
  "SA20n1380:428": "kati bāhyāḥ |",
  "SA20n1380:429": "ṣaṭ |",
  "SA20n1380:430": "ye sthāpitāḥ ||160 ||",
  "SA20n1380:431": "kati sālambanāḥ |",
  "SA20n1380:432": "saptacittadhātavaḥ dharmadhātvekadeśo'pi yaścaittaḥ ||161 ||",
  "SA20n1380:433": "katyanālambanāḥ |",
  "SA20n1380:434": "śiṣṭā daśa dharmadhātvekadeśaśca ||162 ||",
  "SA20n1380:435": "kati savitarkāḥ |",
  "SA20n1380:436": "manodhātuḥ manovijñānadhātuḥ dharmadhātvekadeśaśca ||163 ||",
  "SA20n1380:437": "kati niṣpannāḥ |",
  "SA20n1380:438": "pañcādhyātmikāḥ |",
  "SA20n1380:439": "caturṇāmekadeśaḥ ||164 ||",
  "SA20n1380:440": "katyaniṣpannāḥ śiṣṭā daśa,",
  "SA20n1380:441": "caturṇāmekadeśaśca ||165 ||",
  "SA20n1380:442": "kati sabhāgāḥ |",
  "SA20n1380:443": "ādhyātmikāḥ pañca rupiṇaḥ |",
  "SA20n1380:444": "svavijñānasahitaviṣayasadṛśatāmupādāya ||166 ||",
  "SA20n1380:445": "kati tatsabhāgāḥ |",
  "SA20n1380:446": "ta eva svavijñānavirahitasvānvayasadṛśatāmupādāya ||167 ||",
  "SA20n1380:447": "dhātūddyeśanirdeśakastṛtīyādhikaraḥ pariniṣṭitaḥ |",
  "SA20n1380:448": "ācāryavasubandhuviracit pañcaskandhaprakaraṇaṃ samāptam |",
  "SA20n1380:449": "pañcaskandhaprakaraṇaṃ śāstriṇā śāntibhikṣuṇā |",
  "SA20n1380:450": "bhoṭānuvādamāgamya saṃskṛte punaruddhṛtam",
  "SA20n1380:451": "yadatra sukṛtaṃ sarva tadācāryasya dhīmataḥ |",
  "SA20n1380:452": "yat kukṛtaṃ tadāgo me kṣantavyaṃ sādhusūribhiḥ ||"
}