{
    "856:0": "nairātmāṣṭakastotram",
    "856:1": "nirālamba-nirākāra-nirvikalpasvarūpiṇī |",
    "856:2": "nairātmā varadā devī nityameva namo'stu te || 1 ||",
    "856:3": "ekavaktrāṃ trinetrāṃ ca ūrdhvapiṅgalamūrdhajām |",
    "856:4": "kare kartikapālāṃ ca namāmi viśvamātarīm || 2 ||",
    "856:5": "navanṛtyabhūṣitāṃ ca sahasramukharūpiṇīm |",
    "856:6": "yogeśvarīṃ yogagamyāṃ namāmi jñānarūpiṇīm || 3 ||",
    "856:7": "śavārūḍhāṃ mahātejāṃ sarvaduṣṭanikṛntanīm |",
    "856:8": "sarvapāpaharāṃ devīṃ namāmi jagadīśvarīm || 4 ||",
    "856:9": "gaurīṃ caurīṃ ca vetālīṃ ghasmarīṃ pukkasīṃ tathā |",
    "856:10": "śabarīṃ cāṇḍālīṃ ḍombīṃ namāmi cāṣṭayoginīm || 5 ||",
    "856:11": "brahmāviṣṇuśivaśakracaturmāravināśinīm |",
    "856:12": "trailokyamohinīṃ devīṃ namāmi surasundarīm || 6 ||",
    "856:13": "akṣamālavibhūṣāṅgīm akārabījasambhavām |",
    "856:14": "nityaṃ dhyānaratāṃ devīṃ namāmi viśvamātarīm || 7 ||",
    "856:15": "sarvabhūtapriyāṃ devīṃ sarvamantraprabodhinīm |",
    "856:16": "sarvamantramayīṃ mātarnairātmāṃ tvāṃ namāmyaham || 8 ||",
    "856:17": "nairātmāṣṭakastotraṃ samāptam |"
}