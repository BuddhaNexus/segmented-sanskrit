{
  "SA05n862:0": "pratisarāstotram",
  "SA05n862:1": "om namaḥ śrīpratisarāyai",
  "SA05n862:2": "pratisarai ! amaraughaiḥ pūjitāṃ tvāṃ nato'smi",
  "SA05n862:3": "pratipadajanarakṣākāriṇīṃ sarvakālam |",
  "SA05n862:4": "bhṛgudahanasamudrāpātaduḥkhe surakṣāṃ",
  "SA05n862:5": "ripuviṣabhayahatrīṃ rājyabhogādikartrīm || 1 ||",
  "SA05n862:6": "parivṛtanijavargān bhaktadattāpavargān",
  "SA05n862:7": "kalitaduritavargān mokṣalābhaikamārgān |",
  "SA05n862:8": "kṛtanarasurasargān kāritānaṅgabhaṅgān",
  "SA05n862:9": "stutikṛtamunigargān sarvarakṣaikasaṅgān || 2 ||",
  "SA05n862:10": "prahṛtasakalavighne sarvalokaikamānye",
  "SA05n862:11": "daśabalakṛtadhanye pañcadevīvareṇye |",
  "SA05n862:12": "viṣamapadaśaraṇye sarvadā tvāṃ namasye",
  "SA05n862:13": "vasita iva araṇye dāvadagdhe virasye || 3 ||",
  "SA05n862:14": "sakalajanasuvāñchāpūraṇe kāmadhenu-",
  "SA05n862:15": "mabhilaṣitaphalāptyai kalpavṛkṣāgravallīm |",
  "SA05n862:16": "asurasuranarādyairvanditāṅghryabjayugmā-",
  "SA05n862:17": "murasi lalitānānāratnamālādiyuktām || 4 ||",
  "SA05n862:18": "uḍupatiśatadīptāṃ śaṅkhakundāvadātāṃ",
  "SA05n862:19": "grahabhayapariśāntāṃ vajrasattvātmikāṃ tām |",
  "SA05n862:20": "śravaṇalalitalole kuṇḍale saṃvahantīṃ",
  "SA05n862:21": "mukuṭamaṇiśikhābhiḥ kāśitāṃ tāṃ namāmi || 5 ||",
  "SA05n862:22": "janani sakaladuḥkhāttāraṇī tvaṃ prasīda",
  "SA05n862:23": "sapadi vigatarakṣāṃ rakṣa me mātaraṃ ca |",
  "SA05n862:24": "sakaruṇaruditaṃ māṃ tvaddayāsvasthabhūtaṃ",
  "SA05n862:25": "kṛtasarasinipātāṃ tvaṃ gatistvaṃ gatirnau || 6 ||",
  "SA05n862:26": "paṭhati pratisarāyāḥ stotrametatsadā yo",
  "SA05n862:27": "jvalanajalaviṣāṇāṃ coraśārdūlakānām |",
  "SA05n862:28": "sagadanidhanakānāṃ bhītayā nāśayantī",
  "SA05n862:29": "pratipadamalabhagnaṃ kāmunā dhairyalābham || 7 ||",
  "SA05n862:30": "nijajanapariyuktā dharmakāmārthavṛddhā",
  "SA05n862:31": "ripugaṇaparimuktā saukhyameva prabhuktā |",
  "SA05n862:32": "dadati vipulabhogyaṃ kāmadā sarvadā'sau",
  "SA05n862:33": "nihatasakalapāpaṃ mokṣamāyānti cānte || 8 ||",
  "SA05n862:34": "vigalitanayanāmbuḥ sāñjaliḥ saṃvilāpī",
  "SA05n862:35": "stutimiti pracakāra traibhavānāṃ jananyāḥ |",
  "SA05n862:36": "bhavatu mama tu saṃpat sarvabhāvā kṛśāno",
  "SA05n862:37": "tanu kuśalasurakṣā itthamevaṃ sucittaḥ || 9 ||",
  "SA05n862:38": "vipacatu mayi sarvaṃ mādṛśaṃ karma bhogyaṃ",
  "SA05n862:39": "sasuranaracayānāṃ bhrātṛmātṛdruhāṇām |",
  "SA05n862:40": "kuruta kuruta dhairyaṃ yena mokṣaṃ sulabhyaṃ",
  "SA05n862:41": "jaṭharanilayavāsaṃ māstu pāpaṃ kadācit || 10 ||",
  "SA05n862:42": "udaranarakavāsaṃ ye tadā sāvatāraṃ",
  "SA05n862:43": "satataśubhasucetāḥ sādhayiṣyāmi bodhim |",
  "SA05n862:44": "kṛtajagati surakṣāṃ yena kaivalyaprāptiḥ",
  "SA05n862:45": "iti muditasucetāḥ karmaṇā digdhacittaḥ || 11 ||",
  "SA05n862:46": "pratisara avatāryāṃ sāmbikāṃ mā vilamba",
  "SA05n862:47": "aniśamitivadan so nindayan vai svakarma |",
  "SA05n862:48": "pratibhayaviṣadigdhastasthivānarbhakaḥ san",
  "SA05n862:49": "muhuriha hi śaraṇyāṃ tāṃ smaran sarvarakṣām || 12 ||",
  "SA05n862:50": "aho sudhanyā jagadekamātā prakrāntadaivā ripuvargabhītā |",
  "SA05n862:51": "yasyāḥ prabhāvādvihitātyapāpāt prayāti vahnirjalaśītalatvam || 13 ||",
  "SA05n862:52": "prabhāvatāho punareva tasyāḥ",
  "SA05n862:53": "vahniṃ jalaṃ cāpi giriṃ sthalaṃ vai |",
  "SA05n862:54": "yasyāḥ smṛtenaiva savismatīyān",
  "SA05n862:55": "vaiśvānaraḥ prāpa dvidhākṛtiṃ tām || 14 ||",
  "SA05n862:56": "gopāśarīraṃ hradapātaśītaṃ śaitaṃ tvanāśārthamivādhikoṣṇām |",
  "SA05n862:57": "daivādidattābhijane samantānnitāntadūrādatipāpadattām || 15 ||",
  "SA05n862:58": "śrībhadrakalpāvadānoddhṛtaṃ yaśodharāgarbhasthabālakṛtaṃ",
  "SA05n862:59": "pratisarāstotraṃ samāptam |"
}