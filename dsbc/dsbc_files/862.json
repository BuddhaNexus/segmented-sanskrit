{
    "862:0": "pratisarāstotram",
    "862:1": "om namaḥ śrīpratisarāyai",
    "862:2": "pratisarai ! amaraughaiḥ pūjitāṃ tvāṃ nato'smi",
    "862:3": "pratipadajanarakṣākāriṇīṃ sarvakālam |",
    "862:4": "bhṛgudahanasamudrāpātaduḥkhe surakṣāṃ",
    "862:5": "ripuviṣabhayahatrīṃ rājyabhogādikartrīm || 1 ||",
    "862:6": "parivṛtanijavargān bhaktadattāpavargān",
    "862:7": "kalitaduritavargān mokṣalābhaikamārgān |",
    "862:8": "kṛtanarasurasargān kāritānaṅgabhaṅgān",
    "862:9": "stutikṛtamunigargān sarvarakṣaikasaṅgān || 2 ||",
    "862:10": "prahṛtasakalavighne sarvalokaikamānye",
    "862:11": "daśabalakṛtadhanye pañcadevīvareṇye |",
    "862:12": "viṣamapadaśaraṇye sarvadā tvāṃ namasye",
    "862:13": "vasita iva araṇye dāvadagdhe virasye || 3 ||",
    "862:14": "sakalajanasuvāñchāpūraṇe kāmadhenu-",
    "862:15": "mabhilaṣitaphalāptyai kalpavṛkṣāgravallīm |",
    "862:16": "asurasuranarādyairvanditāṅghryabjayugmā-",
    "862:17": "murasi lalitānānāratnamālādiyuktām || 4 ||",
    "862:18": "uḍupatiśatadīptāṃ śaṅkhakundāvadātāṃ",
    "862:19": "grahabhayapariśāntāṃ vajrasattvātmikāṃ tām |",
    "862:20": "śravaṇalalitalole kuṇḍale saṃvahantīṃ",
    "862:21": "mukuṭamaṇiśikhābhiḥ kāśitāṃ tāṃ namāmi || 5 ||",
    "862:22": "janani sakaladuḥkhāttāraṇī tvaṃ prasīda",
    "862:23": "sapadi vigatarakṣāṃ rakṣa me mātaraṃ ca |",
    "862:24": "sakaruṇaruditaṃ māṃ tvaddayāsvasthabhūtaṃ",
    "862:25": "kṛtasarasinipātāṃ tvaṃ gatistvaṃ gatirnau || 6 ||",
    "862:26": "paṭhati pratisarāyāḥ stotrametatsadā yo",
    "862:27": "jvalanajalaviṣāṇāṃ coraśārdūlakānām |",
    "862:28": "sagadanidhanakānāṃ bhītayā nāśayantī",
    "862:29": "pratipadamalabhagnaṃ kāmunā dhairyalābham || 7 ||",
    "862:30": "nijajanapariyuktā dharmakāmārthavṛddhā",
    "862:31": "ripugaṇaparimuktā saukhyameva prabhuktā |",
    "862:32": "dadati vipulabhogyaṃ kāmadā sarvadā'sau",
    "862:33": "nihatasakalapāpaṃ mokṣamāyānti cānte || 8 ||",
    "862:34": "vigalitanayanāmbuḥ sāñjaliḥ saṃvilāpī",
    "862:35": "stutimiti pracakāra traibhavānāṃ jananyāḥ |",
    "862:36": "bhavatu mama tu saṃpat sarvabhāvā kṛśāno",
    "862:37": "tanu kuśalasurakṣā itthamevaṃ sucittaḥ || 9 ||",
    "862:38": "vipacatu mayi sarvaṃ mādṛśaṃ karma bhogyaṃ",
    "862:39": "sasuranaracayānāṃ bhrātṛmātṛdruhāṇām |",
    "862:40": "kuruta kuruta dhairyaṃ yena mokṣaṃ sulabhyaṃ",
    "862:41": "jaṭharanilayavāsaṃ māstu pāpaṃ kadācit || 10 ||",
    "862:42": "udaranarakavāsaṃ ye tadā sāvatāraṃ",
    "862:43": "satataśubhasucetāḥ sādhayiṣyāmi bodhim |",
    "862:44": "kṛtajagati surakṣāṃ yena kaivalyaprāptiḥ",
    "862:45": "iti muditasucetāḥ karmaṇā digdhacittaḥ || 11 ||",
    "862:46": "pratisara avatāryāṃ sāmbikāṃ mā vilamba",
    "862:47": "aniśamitivadan so nindayan vai svakarma |",
    "862:48": "pratibhayaviṣadigdhastasthivānarbhakaḥ san",
    "862:49": "muhuriha hi śaraṇyāṃ tāṃ smaran sarvarakṣām || 12 ||",
    "862:50": "aho sudhanyā jagadekamātā prakrāntadaivā ripuvargabhītā |",
    "862:51": "yasyāḥ prabhāvādvihitātyapāpāt prayāti vahnirjalaśītalatvam || 13 ||",
    "862:52": "prabhāvatāho punareva tasyāḥ",
    "862:53": "vahniṃ jalaṃ cāpi giriṃ sthalaṃ vai |",
    "862:54": "yasyāḥ smṛtenaiva savismatīyān",
    "862:55": "vaiśvānaraḥ prāpa dvidhākṛtiṃ tām || 14 ||",
    "862:56": "gopāśarīraṃ hradapātaśītaṃ śaitaṃ tvanāśārthamivādhikoṣṇām |",
    "862:57": "daivādidattābhijane samantānnitāntadūrādatipāpadattām || 15 ||",
    "862:58": "śrībhadrakalpāvadānoddhṛtaṃ yaśodharāgarbhasthabālakṛtaṃ",
    "862:59": "pratisarāstotraṃ samāptam |"
}