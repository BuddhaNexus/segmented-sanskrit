{
    "919:0": "CANTO V",
    "919:1": "sa tathā viṣayairvilobhyamānaḥ|",
    "919:2": "paramārhairapi śākyarājasūnuḥ|",
    "919:3": "na jagāma dhṛtiṃ na śarma lebhe",
    "919:4": "hṛdaye siṃha ivātidigdhaviddhaḥ||1||",
    "919:5": "atha mantisutaiḥ kṣamaiḥ kadāci-",
    "919:6": "tsakhibhiścitrakathaiḥ kṛtānuyātraḥ|",
    "919:7": "vanabhūmididṛkṣayā śamepsu-",
    "919:8": "rnaradevānumato bahiḥ pratasthe||2||",
    "919:9": "navarukmakhalīnakiṅkiṇīkaṃ",
    "919:10": "pracalaccāramaracāruhemabhāṇḍam|",
    "919:11": "abhiruhya sa kanthakaṃ sadaśvaṃ",
    "919:12": "prayayau ketumiva drumābjaketuḥ||3||",
    "919:13": "sa vikṛṣṭatarāṃ vanāntabhūmiṃ",
    "919:14": "vanalobhācca yayau mahīguṇācca|",
    "919:15": "salilormivikārasīramārgāṃ",
    "919:16": "vasudhāṃ caiva dadarśa kṛṣyamāṇām||4||",
    "919:17": "halabhinnavikīrṇaśaṣpadarbhā",
    "919:18": "hatasūkṣmakrimikīṭajantukīrṇām|",
    "919:19": "samavekṣya rasāṃ tathāvidhāṃ tāṃ",
    "919:20": "svajanasyeva vadhe bhṛśaṃ śuśoca||5||",
    "919:21": "kṛṣataḥ puruṣāṃśca vīkṣamāṇaḥ",
    "919:22": "pavanārkāśurajovibhinnavarṇān|",
    "919:23": "vahanaklamaviklavāṃśca dhuryān",
    "919:24": "paramāryaḥ paramāṃ kṛpāṃ cakāra||6||",
    "919:25": "avatīrya tatasturaṅgapṛṣṭhā-",
    "919:26": "cchanakairgā vyacaracchucā parītaḥ|",
    "919:27": "jagato jananavyayaṃ vicinvan",
    "919:28": "kṛpaṇaṃ khalvidamityuvāca cārtaḥ||7||",
    "919:29": "manasā ca viviktatāmabhīpsuḥ",
    "919:30": "suhṛdastānanuyāyino nivārya|",
    "919:31": "abhitaścalacāruparṇavatyā",
    "919:32": "vijane mūlamupeyivān sa jambvāḥ||8||",
    "919:33": "niṣasāda sa yatra śaucavatyāṃ",
    "919:34": "bhuvi vaidūryanikāśaśādvalāyām|",
    "919:35": "jagataḥ prabhavavyayau vicinva-",
    "919:36": "nmanasaśca sthitimārgamālalambe||9||",
    "919:37": "samavāptamanaḥsthitiśca sadyo",
    "919:38": "viṣayecchādibhirādhibhiśca muktaḥ|",
    "919:39": "savitarkavicāramāpa śāntaṃ",
    "919:40": "prathamaṃ dhyānamanāsravaprakāram||10||",
    "919:41": "adhigamya tato vivekajaṃ tu",
    "919:42": "paramaprītisukhaṃ manaḥsamādhim|",
    "919:43": "idameva tataḥ paraṃ pradadhyau",
    "919:44": "manasā lokagatiṃ niśāmya samyak||11||",
    "919:45": "kṛpaṇaṃ bata yajjanaḥ svayaṃ sa-",
    "919:46": "nnavaśo vyādhijarāvināśadharmā|",
    "919:47": "jarayārditamāturaṃ mṛtaṃ vā",
    "919:48": "paramajño vijugupsate madāndhaḥ||12||",
    "919:49": "iha cedahamīdṛśaḥ svayaṃ sa-",
    "919:50": "nvijugupseya paraṃ tathāsvabhāvam|",
    "919:51": "na bhavetsadṛśaṃ hi tatkṣamaṃ vā",
    "919:52": "paramaṃ dharmamimaṃ vijānato me||13||",
    "919:53": "iti tasya vipaśyato yathāva-",
    "919:54": "jjagato vyādhijarāvipattidoṣān|",
    "919:55": "balayauvanajīvitapravṛtto",
    "919:56": "vijagāmātmagato madaḥ kṣaṇena||14||",
    "919:57": "na jaharṣa na cāpi cānutepe",
    "919:58": "vicikitsāṃ na yayau na tandrinidre|",
    "919:59": "na ca kāmaguṇeṣu saṃrarañje",
    "919:60": "na vididveṣa paraṃ na cāvamene||15||",
    "919:61": "iti buddhiriyaṃ ca nīrajaskā",
    "919:62": "vavṛdhe tasya mahātmano viśuddhā|",
    "919:63": "puruṣairaparairadṛśyamānaḥ",
    "919:64": "puruṣaścopasasarpa bhikṣuveṣaḥ||16||",
    "919:65": "naradevasutastamabhyapṛccha-",
    "919:66": "dvada ko'sīti śaśaṃsa so'tha tasmai|",
    "919:67": "narapuṃgava janmamṛtyubhītaḥ",
    "919:68": "śramaṇaḥ pravrajito'smi mokṣahetoḥ||17||",
    "919:69": "jagati kṣayadharmake mumukṣa-",
    "919:70": "rmṛgaye'haṃ śivamakṣayaṃ padaṃ tat|",
    "919:71": "svajane'nyajane ca tulyabuddhi-",
    "919:72": "rviṣayebhyo vinivṛttarāgadoṣaḥ||18||",
    "919:73": "nivasan kvacideva vṛkṣamūle",
    "919:74": "vijane vāyatane girau vane vā|",
    "919:75": "vicarāmyaparigraho nirāśaḥ",
    "919:76": "paramārthāya yathopapannabhaikṣaḥ||19||",
    "919:77": "iti paśyata eva rājasūno-",
    "919:78": "ridamuktvā sa nabhaḥ samutpapāta|",
    "919:79": "sa hi tadvapuranyabuddhadarśī",
    "919:80": "smṛtaye tasya sameyivāndivaukāḥ||20||",
    "919:81": "gaganaṃ khagavadgate ca tasmi-",
    "919:82": "nṛvaraḥ saṃjahṛṣe visismiye ca|",
    "919:83": "upalabhya tataśca dharmasaṃjñā-",
    "919:84": "mabhiniryāṇavidhau matiṃ cakāra||21||",
    "919:85": "tata indrasamo jitendriyāśvaḥ",
    "919:86": "pravivikṣuḥ puramaśvamāruroha|",
    "919:87": "parivārajanaṃ tvavekṣamāṇa-",
    "919:88": "stata evābhimataṃ vanaṃ na bheje||22||",
    "919:89": "sa jarāmaraṇakṣayaṃ cikīrṣu-",
    "919:90": "rvanavāsāya matiṃ smṛtau nidhāya|",
    "919:91": "praviveśa punaḥ puraṃ na kāmā-",
    "919:92": "dvanabhūmeriva maṇḍalaṃ dvipendraḥ||23||",
    "919:93": "sukhitā bata nirvṛtā ca sā strī",
    "919:94": "patirīdṛkṣa ihāyatākṣa yasyāḥ|",
    "919:95": "iti taṃ samudīkṣya rājakanyā",
    "919:96": "praviśantaṃ pathi sāñjalirjagāda||24||",
    "919:97": "atha ghoṣamimaṃ mahābhraghoṣaḥ",
    "919:98": "pariśuśrāva śamaṃ paraṃ ca lebhe|",
    "919:99": "śrutavānsa hi nirvṛteti śabdaṃ",
    "919:100": "parinirvāṇavidhau matiṃ cakāra||25||",
    "919:101": "atha kāñcanaśailaśṛṅgavarṣmā",
    "919:102": "gajamegharṣabhabāhunisvanākṣaḥ|",
    "919:103": "kṣayamakṣayadharmajātarāgaḥ",
    "919:104": "śaśisiṃhānanavikramaḥ prapede||26||",
    "919:105": "mṛgarājagatistato'bhyagaccha-",
    "919:106": "nnṛpatiṃ mantrigaṇairupāsyamānam|",
    "919:107": "samitau marutāmiva jvalantaṃ",
    "919:108": "maghavantaṃ tridive sanatkumāraḥ||27||",
    "919:109": "praṇipatya ca sāñjalirbabhāṣe",
    "919:110": "diśa mahyaṃ naradeva sādhvanujñām|",
    "919:111": "parivivrajiṣāmi mokṣaheto-",
    "919:112": "rniyato hyasya janasya viprayogaḥ||28||",
    "919:113": "iti tasya vaco niśamya rājā",
    "919:114": "kariṇevābhihato drumaścacāla|",
    "919:115": "kamalapratime'ñjalau gṛhītvā",
    "919:116": "vacanaṃ cedamuvāca bāṣpakaṇṭhaḥ||29||",
    "919:117": "pratisaṃhara tāt buddhimetāṃ",
    "919:118": "na hi kālastava dharmasaṃśrayasya|",
    "919:119": "vayasi prathame matau calāyāṃ",
    "919:120": "bahudoṣāṃ hi vadanti dharmacaryām||30||",
    "919:121": "viṣayeṣu kutūhalendriyasya",
    "919:122": "vratakhedeṣvasamarthīniścayasya|",
    "919:123": "taruṇasya manaścalatyaraṇyā-",
    "919:124": "danabhijñaśca viśeṣato viveke||31||",
    "919:125": "mama tu priyadharma dharmakāla-",
    "919:126": "stvayi lakṣmīmavasṛjya lakṣmabhūte|",
    "919:127": "sthiravikrama vikrameṇa dharma-",
    "919:128": "stava hitvā tu guruṃ bhavedadharmaḥ||32||",
    "919:129": "tadimaṃ vyavasāyamutsṛja tvaṃ",
    "919:130": "bhava tāvannirato gṛhasthadharme|",
    "919:131": "puruṣasya vayaḥsukhāni bhuktvā",
    "919:132": "ramaṇīyo hi tapovanapraveśaḥ||33||",
    "919:133": "iti vākyamidaṃ niśamya rājñaḥ",
    "919:134": "kalaviṅkasvara uttaraṃ babhāṣe|",
    "919:135": "yadi me pratibhūtaścaturṣu rājan",
    "919:136": "bhavasi tvaṃ na tapovanaṃ śrayiṣye||34||",
    "919:137": "na bhavenmaraṇāya jīvitaṃ me",
    "919:138": "viharetsvāsthyamidaṃ ca me na rogaḥ|",
    "919:139": "na ca yauvanamākṣipejjarā me",
    "919:140": "na ca saṃpattimimāṃ haredvipattiḥ||35||",
    "919:141": "iti durlabhamarthamūcivāṃsaṃ",
    "919:142": "tanayaṃ vākyamuvāca śākyarājaḥ|",
    "919:143": "tyaja buddhimimāmatipravṛttā-",
    "919:144": "mavahāsyo'timanoratho'kramaśca||36||",
    "919:145": "atha merugururguruṃ babhāṣe",
    "919:146": "yadi nāsti krama eṣa nāsmi vāryaḥ|",
    "919:147": "śaraṇājjvalanena dahyamānā-",
    "919:148": "nna hi niścikramiṣuḥ kṣamaṃ grahītum||37||",
    "919:149": "jagataśca yadā dhruvo viyogo",
    "919:150": "nanu dharmāya varaṃ svayaviyogaḥ|",
    "919:151": "avaśaṃ nanu viprayojayenmā-",
    "919:152": "makṛtasvārthamatṛptameva mṛtyuḥ||38||",
    "919:153": "iti bhūmipatirniśamya tasya",
    "919:154": "vyavasāyaṃ tanayasya nirmumukṣoḥ|",
    "919:155": "abhidhāya na yāsyatīti bhūyo",
    "919:156": "vidadhe rakṣaṇamuttamāṃśca kāmān||39||",
    "919:157": "sacivaistu nidarśito yathāvad",
    "919:158": "bahumānātpraṇayācca śāstrapūrvam|",
    "919:159": "guruṇā ca nivārito'śrupātaiḥ",
    "919:160": "praviveśāvasathaṃ tataḥ sa śocan||40||",
    "919:161": "calakuṇḍalacumbitānanābhi-",
    "919:162": "rghananiśvāsavikampitastanībhiḥ|",
    "919:163": "vanitābhiradhīralocanābhi-",
    "919:164": "mṛrgaśāvābhirivābhyudīkṣyamāṇaḥ||41||",
    "919:165": "sa hi kāñcanaparvatāvadāto",
    "919:166": "hṛdayenmādakaro varāṅganānām|",
    "919:167": "śravaṇāṅgavilocanātmabhāvā-",
    "919:168": "nvacanasparśavapurguṇairjahāra||42||",
    "919:169": "vigate divase tato vimānaṃ",
    "919:170": "vapuṣā sūrya iva pradīpyamānaḥ|",
    "919:171": "timiraṃ vijighāṃsurātmabhāsā",
    "919:172": "ravirudyanniva merumāruroha||43||",
    "919:173": "kanakojjvaladīptadīpavṛkṣaṃ",
    "919:174": "varakālāgurudhūpapūrṇagarbham|",
    "919:175": "adhiruhya sa vajrabhakticitraṃ",
    "919:176": "pravaraṃ kāñcanamāsanaṃ siṣeve||44||",
    "919:177": "tata uttamamuttamāṅganāstaṃ",
    "919:178": "niśi tūryairupatasthurindrakalpam|",
    "919:179": "himavacchirasīva candragaure",
    "919:180": "draviṇendrātmajamapsarogaṇaughāḥ||45||",
    "919:181": "paramairapi divyatūryakalpaiḥ",
    "919:182": "sa tu tairnaiva ratiṃ yayau na harṣam|",
    "919:183": "paramārthasukhāya tasya sādho-",
    "919:184": "rabhiniścikramiṣā yato na reme||46||",
    "919:185": "atha tatra suraistapovariṣṭhai-",
    "919:186": "rakaniṣṭhairvyavasāyamasya buddhvā|",
    "919:187": "yugapatpramadājanasya nidrā",
    "919:188": "vihitāsīdvikṛtāśca gātraceṣṭāḥ||47||",
    "919:189": "abhavacchayitā hi tatra kāci-",
    "919:190": "dviniveśya pracale kare kapolam|",
    "919:191": "dayitāmapi rukmapattracitrāṃ",
    "919:192": "kupitevāṅkagatāṃ vihāya vīṇām||48||",
    "919:193": "vibabhau karalagnaveṇuranyā",
    "919:194": "stanavisrastasitāṃśukā śayānā|",
    "919:195": "ṛjuṣaṭpadapaṅktijuṣṭapadmā",
    "919:196": "jalaphenaprahasattaṭā nadīva||49||",
    "919:197": "navapuṣkaragarbhakomalābhyāṃ",
    "919:198": "tapanīyojjvalasaṃgatāṅgadābhyām|",
    "919:199": "svapiti sma tathāparā bhujābhyāṃ",
    "919:200": "parirabhya priyavanmṛdaṅgameva||50||",
    "919:201": "navahāṭakabhūṣaṇāstathānyā",
    "919:202": "vasanaṃ pītamanuttamaṃ vasānāḥ|",
    "919:203": "avaśā ghananidrayā nipetu-",
    "919:204": "rgajabhagnā iva karṇikāraśākhāḥ||51||",
    "919:205": "avalambya gavākṣapārśvamanyā",
    "919:206": "śayitā cāpavibhugnagātrayaṣṭiḥ|",
    "919:207": "virarāja vilambicāruhārā",
    "919:208": "racitā toraṇāśālabhañjikeva||52||",
    "919:209": "maṇikuṇḍaladaṣṭapattralekhaṃ",
    "919:210": "mukhapadmaṃ vinataṃ tathāparasyāḥ|",
    "919:211": "śatapattramivārdhavakranāḍaṃ",
    "919:212": "sthitakāraṇḍavaghaṭṭitaṃ cakāśe||53||",
    "919:213": "aparāḥ śayitā yathopaviṣṭāḥ",
    "919:214": "stanabhārairavanamyamānagātrāḥ|",
    "919:215": "upaguhya parasparaṃ vireju-",
    "919:216": "rbhujapāśaistapanīyaparihāryaiḥ||54||",
    "919:217": "mahatī parivādinī ca kāci-",
    "919:218": "dvanitāliṅgya sakhīmiva prasuptā|",
    "919:219": "vijughūrṇa calatsuvarṇasūtrā",
    "919:220": "vadanenākulakarṇikojjvalena||55||",
    "919:221": "paṇavaṃ yuvatirbhujāṃsadeśā-",
    "919:222": "davavisraṃsitacārūpāśamanyā|",
    "919:223": "savilāsaratāntatāntamūrvo-",
    "919:224": "rvivare kāntamivābhinīya śiśye||56||",
    "919:225": "aparā babhūvurnimīlitākṣyo",
    "919:226": "vipulākṣyo'pi śubhabhruvo'pi satyaḥ|",
    "919:227": "pratisaṃkucitāravindakośāḥ",
    "919:228": "savitaryastamite yathā nalinyaḥ||57||",
    "919:229": "śithilākulamūrdhajā tathānyā",
    "919:230": "jaghanasrastavibhūṣaṇāṃśukāntā|",
    "919:231": "aśayiṣṭa vikīrṇakaṇṭhasūtrā",
    "919:232": "gajabhagnā pratiyātanāṅganeva||58||",
    "919:233": "aparāstvavaśā hriyā viyuktā",
    "919:234": "dhṛtimatyo'pi vapurguṇairupetāḥ|",
    "919:235": "viniśaśvasurulbaṇaṃ śayānā",
    "919:236": "vikṛtāḥ kṣiptabhūjā jajṛmbhire ca||59||",
    "919:237": "vyapaviddhavibhūṣaṇasrajo'nyā",
    "919:238": "visṛtāgranthanavāsaso visaṃjñāḥ|",
    "919:239": "animīlitaśuklaniścalākṣyo",
    "919:240": "na virejuḥ śayitā gatāsukalpāḥ||60||",
    "919:241": "vivṛtāsyapuṭā vivṛddhagātrī",
    "919:242": "prapatadvaktrajalā prakāśaguhyā|",
    "919:243": "aparā madaghūrṇiteva śiśye",
    "919:244": "na babhāse vikṛtaṃ vapuḥ pupoṣa||61||",
    "919:245": "iti sattvakulānvayānurūpaṃ",
    "919:246": "vividhaṃ sa pramadājanaḥ śayānaḥ|",
    "919:247": "sarasaḥ sadṛśaṃ babhāra rūpaṃ",
    "919:248": "pavanāvarjitarugnapuṣkarasya||62||",
    "919:249": "samavekṣya tathā tathā śayānā",
    "919:250": "vikṛtāstā yuvatīradhīraceṣṭāḥ|",
    "919:251": "guṇavadvapuṣo'pi valgubhāṣā",
    "919:252": "nṛpasūnuḥ sa vigarhayāṃbabhūva||63||",
    "919:253": "aśucirvikṛtaśca jīvaloke",
    "919:254": "vanitānāmayamīdṛśaḥ svabhāvaḥ|",
    "919:255": "vasanābharaṇaistu vañcyamānaḥ",
    "919:256": "puruṣaḥ strīviṣayeṣu rāgameti||64||",
    "919:257": "vimṛśedyadi yoṣitāṃ manuṣyaḥ",
    "919:258": "prakṛtiṃ svapnavikāramīdṛśaṃ ca|",
    "919:259": "dhruvamatra na vardhayetpramādaṃ",
    "919:260": "guṇasaṃkalpahatastu rāgameti||65||",
    "919:261": "iti tasya tadantaraṃ viditvā",
    "919:262": "niśi niścikramiṣā samudbabhūva|",
    "919:263": "avagamya manastato'sya devai-",
    "919:264": "rbhavanadvāramapāvṛtaṃ babhūva||66||",
    "919:265": "atha so'vatatāra harmyapṛṣṭhā-",
    "919:266": "dyuvatīstāḥ śayitā vigarhamāṇaḥ|",
    "919:267": "avatīrya tataśca nirviśaṅko",
    "919:268": "gṛhakakṣyāṃ prathamāṃ vinirjagāma||67||",
    "919:269": "turagāvacaraṃ sa bodhayitvā",
    "919:270": "javinaṃ chandakamitthamityuvāca|",
    "919:271": "hayamānaya kanthakaṃ tvarāvā-",
    "919:272": "namṛtaṃ prāptumito'dya me yiyāsā||68||",
    "919:273": "hṛdi yā mama tuṣṭiradya jātā",
    "919:274": "vyavasāyaśca yathā matau niviṣṭaḥ|",
    "919:275": "vijane'pi ca nāthavānivāsmi",
    "919:276": "dhruvamartho'bhimukhaḥ sameta iṣṭaḥ||69||",
    "919:277": "hriyameva ca saṃnatiṃ ca hitvā",
    "919:278": "śayitā matpramukhe yathā yuvatyaḥ|",
    "919:279": "vivṛte ca yathā svayaṃ kapāṭe",
    "919:280": "niyataṃ yātumato mamādya kālaḥ||70||",
    "919:281": "pratigṛhya tataḥ sa bharturājñāṃ",
    "919:282": "viditārtho'pi narendraśāsanasya|",
    "919:283": "manasīva pareṇa codyamāna-",
    "919:284": "sturagasyānayane matiṃ cakāra||71||",
    "919:285": "atha hemakhalīnapūrṇavaktraṃ",
    "919:286": "laghuśayyāstaraṇonapūrṇavaktraṃ",
    "919:287": "laghuśayyāstaraṇopagūḍhapṛṣṭham|",
    "919:288": "balasattvajavānvayopapannaṃ",
    "919:289": "sa varāśvaṃ tamupānināya bhartre||72||",
    "919:290": "pratatrikapucchamūlapārṣṇi",
    "919:291": "nibhṛtahṛsvatanūjapucchakarṇam|",
    "919:292": "vinatonnatapṛṣṭhakukṣipārśva",
    "919:293": "vipulaprothalalāṭakaṭyuraskam||73||",
    "919:294": "upaguhya sa taṃ viśālavakṣāḥ",
    "919:295": "kamalābhena ca sāntvayan kareṇa|",
    "919:296": "madhurākṣarayā girā śaśāsa",
    "919:297": "dhvajinīmadhyamiva praveṣṭukāmaḥ||74||",
    "919:298": "bahuśaḥ kila śatravo nirastāḥ",
    "919:299": "samare tvāmadhiruhya pārthivena|",
    "919:300": "ahamapyamṛtaṃ padaṃ yathāva-",
    "919:301": "tturagaśreṣṭha labheya tatkuruṣva||75||",
    "919:302": "sulabhāḥ khalu saṃyuge sahāyā",
    "919:303": "viṣayāvāptasukhe dhanārjane vā|",
    "919:304": "puruṣasya tu durlabhāḥ sahāyāḥ",
    "919:305": "patitasyāpadi dharmasaṃśraye vā||76||",
    "919:306": "iha caiva bhavanti ye sahāyaḥ",
    "919:307": "kaluṣe karmaṇi dharmasaṃśraye vā|",
    "919:308": "avagacchait me yathāntarātmā",
    "919:309": "niyataṃ te'pi janāstadaṃśabhājaḥ||77||",
    "919:310": "tadidaṃ parigamya dharmayuktaṃ",
    "919:311": "mama niryāṇamito jagaddhitāya|",
    "919:312": "turagottama vegavikramābhyāṃ",
    "919:313": "prayatasvātmahite jagaddhite ca||78||",
    "919:314": "iti suhṛdamivānuśiṣya kṛtye",
    "919:315": "turagavaraṃ nṛvaro vanaṃ yiyāsuḥ|",
    "919:316": "sitamasitagatidyutirvapuṣmān",
    "919:317": "raviriva śāradamabhramāruroha||79||",
    "919:318": "atha sa pariharanniśīthacaṇḍaṃ",
    "919:319": "parijanabodhakaraṃ dhvanīṃ sadaśvaḥ|",
    "919:320": "vigatahanuravaḥ praśāntaheṣa-",
    "919:321": "ścakitavimuktapadakramo jagāma||80||",
    "919:322": "kanakavalayabhūṣitaprakoṣṭhaiḥ",
    "919:323": "kamalanibhaiḥ kamalāniva pravidhya|",
    "919:324": "avanatatanavastato'sya yakṣā-",
    "919:325": "ścakitagatairdīdhare khurān karāgraiḥ||81||",
    "919:326": "guruparighakapāṭasaṃvṛtā yā",
    "919:327": "na sukhamapi dviradairapāvriyante|",
    "919:328": "vrajati nṛpasute gatasvanāstāḥ",
    "919:329": "svayamabhavanvivṛtāḥ puraḥ pratolyaḥ||82||",
    "919:330": "pitaramabhimukhaṃ sutaṃ ca bālaṃ",
    "919:331": "janamanuraktamanuttamāṃ ca lakṣmīm|",
    "919:332": "kṛtamatirapahāya nirvyapekṣaḥ",
    "919:333": "pitṛnagarātsa tato vinirjagāma||83||",
    "919:334": "atha sa vimalapaṅkajāyatākṣaḥ",
    "919:335": "puramavalokya nanāda siṃhanādam|",
    "919:336": "jananamaraṇayoradṛṣṭapāro",
    "919:337": "na puramahaṃ kapilāvhayaṃ praveṣṭā||84||",
    "919:338": "iti vacanamidaṃ niśamya tasya",
    "919:339": "draviṇapateḥ pariṣadgaṇā nananduḥ|",
    "919:340": "pramuditamanasaśca devasaṅghā",
    "919:341": "vyavasitapāraṇamāśaśaṃsire'smai||85||",
    "919:342": "hutavahavapuṣoḥ divaukaso'nye",
    "919:343": "vyavasitamasya suduṣkaraṃ viditvā|",
    "919:344": "akṛṣata tuhine pathi prakāśaṃ",
    "919:345": "ghanavivaraprasṛtā ivendupādāḥ||86||",
    "919:346": "harituragaturaṅgavatturaṅgaḥ",
    "919:347": "sa tu vicaranmanasīva codyamānaḥ|",
    "919:348": "aruṇaparuṣatāramantarikṣaṃ",
    "919:349": "sa ca subahūni jagāma yojanāni||87||",
    "919:350": "iti buddhacarite mahākāvye'bhiniṣkramaṇo",
    "919:351": "nāma pañcamaḥ sargaḥ||5||"
}