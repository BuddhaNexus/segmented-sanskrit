{
    "811:0": "aṣṭamātṛkāstotram",
    "811:1": "om namaḥ śrīvighneśvarāṣṭamātṛkābhyaḥ |",
    "811:2": "vighneśvara mahāvīra siddhirūpāya vṛddhaye |",
    "811:3": "vighnanāśāya devāya gaṇeśāya namāmyaham || 1 ||",
    "811:4": "pūrve brahmāṇī devī haṃsamāruhya saṃsthitā |",
    "811:5": "pītavarṇaprabhā devī śrībrahmāṇi namo'stu te || 2 ||",
    "811:6": "dakṣiṇe śrīvārāhī mahiṣāsanasaṃsthitā |",
    "811:7": "raktavarṇā'ṅkuśahastā śrīvārāhi namo'stu te || 3 ||",
    "811:8": "paścime cendrāṇī devī gajamāruhya saṃsthitā |",
    "811:9": "kuṅkumābhā vajrahastā śrīindrāṇi namo'stu te || 4 ||",
    "811:10": "uttare māheśvarī devī vṛṣamāruhya saṃsthitā |",
    "811:11": "śvetavarṇaprabhā devī māheśvari namo'stu te || 5 ||",
    "811:12": "āgneye bālakaumārī mayūrakāntipūraṇī |",
    "811:13": "raktavarṇā śaktihastā śrīkaumāri namo'stu te || 6 ||",
    "811:14": "nairṛtye vaiṣṇavī devī śyāmābhā garuḍāsanā |",
    "811:15": "śaṅkhacakradharā devī nārāyaṇi namo'stu te || 7 ||",
    "811:16": "vāyavye cāmuṇḍā devī siṃhamāruhya saṃsthitā |",
    "811:17": "kakāramūrtidhārī ca khaḍgahastāṃ namāmyaham || 8 ||",
    "811:18": "īśāne caṇḍikā devī dhūmravarṇā prajvālinī |",
    "811:19": "kartimuṇḍadharā devī mahālakṣmi namo'stu te || 9 ||",
    "811:20": "aṣṭapīṭhasthitā devīraṣṭavṛkṣanivāsinīḥ |",
    "811:21": "aṣṭabhairavasaṃyuktā aṣṭamātṛkā namāmyaham || 10 ||",
    "811:22": "sūryaḥ somo mahīputro budho devāsuragurū |",
    "811:23": "śaniśca rāhuḥ ketuśca yamāya ca namo namaḥ || 11 ||",
    "811:24": "śrī aṣṭamātṛkāstotraṃ samāptam ||"
}