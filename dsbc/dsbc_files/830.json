{
    "830:0": "śāradāṣṭakastotram",
    "830:1": "śaṅkhendukundahimasannibhacārudehāṃ",
    "830:2": "haṃsasthitāṃ kamalapatrasurocanīyām |",
    "830:3": "divyāmbarābharaṇabhūṣitasaumyarūpāṃ",
    "830:4": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 1 ||",
    "830:5": "saṃsārasāgaramahodadhimagnasattva-",
    "830:6": "santāriṇīṃ suranarārcitapādapadmām |",
    "830:7": "hārārddhahāramaṇikuṇḍalamaṇḍitāṅgīṃ",
    "830:8": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 2 ||",
    "830:9": "yā bhāratīti kathitā jananī ca loke",
    "830:10": "mohāndhakārabharabhagnakṛtāṃ janānām |",
    "830:11": "saṃkīrtitā munibhirastasamastadoṣaiḥ",
    "830:12": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 3 ||",
    "830:13": "vīṇānuvādanaratāṃ sphaṭikākṣamālā-",
    "830:14": "saṃdhāriṇīṃ kanakapustakadhāriṇīṃ ca |",
    "830:15": "ratnaiḥ śubhaiḥ surucirāṃ kṛtahastapadmāṃ",
    "830:16": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 4 ||",
    "830:17": "pūjyā sadaiva jananī parivandanīyā",
    "830:18": "devyā gaṇā manasi saṃparimodanīyāḥ |",
    "830:19": "jīvārthinaḥ phalabhṛto guṇavarṇanā ca",
    "830:20": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 5 ||",
    "830:21": "natvā bhajejjagati yo jananīṃ prasiddhāṃ",
    "830:22": "kuryātsadā bhagavatī kilapañcasaukhyam |",
    "830:23": "saundaryarūpaguṇavittasubhogabhājaṃ",
    "830:24": "śrī śāradāṃ bhagavatīṃ satataṃ namāmi || 6 ||",
    "830:25": "pūjopahāravalibhiḥ paripūjanīyā",
    "830:26": "devyā gaṇādikamanojñasuvaktrapadmāḥ |",
    "830:27": "yaiḥ pūjitā bhagavatī kila jāḍyahantrī",
    "830:28": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 7 ||",
    "830:29": "yasyāḥ prasādamavagamya sukhapradāyā-",
    "830:30": "strailokyanāthauditārkasamaprabhaḥ syāt |",
    "830:31": "dharmārthakāmaphaladāmatha mokṣadāṃ ca",
    "830:32": "śrīśāradāṃ bhagavatīṃ satataṃ namāmi || 8 ||",
    "830:33": "śrīśāradāṣṭakastotraṃ samāptam |"
}