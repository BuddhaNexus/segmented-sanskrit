{
    "806:0": "(ārya) tārānamaskāraikaviṃśatistotram",
    "806:1": "om namo bhagavatyai āryaśrī-ekaviṃśatitārāyai",
    "806:2": "namastāre ture vīre kṣaṇadyutinibhekṣaṇe|",
    "806:3": "trailokyanāthavaktrābjavikasatkamalodbhave|| 1||",
    "806:4": "namaḥ śataśaraccandrasaṃpūrṇeva varānane|",
    "806:5": "tārāsahasrakiraṇaiḥ prahasatkiraṇojjvale|| 2||",
    "806:6": "namaḥ kanakanīlābja-pāṇipadmavibhūṣite|",
    "806:7": "dānavīryatapaḥśā(kṣā)",
    "806:8": "ntititikṣādhyānagocare|| 3||",
    "806:9": "namastathāgatoṣṇīṣavijayānantacāriṇi|",
    "806:10": "śeṣapāramitāprāptajinaputraniṣevite|| 4||",
    "806:11": "namastutārahuṃkārapūritāśādigantare|",
    "806:12": "saptalokakramākrā[nte] aśeṣakaruṇā(ṇe)kṣaṇe|| 5||",
    "806:13": "namaḥ śakrānalabrahmamarudviśveśvarārcite|",
    "806:14": "bhūtavetālagandharvagaṇayakṣapuraskṛte || 6||",
    "806:15": "namaḥ straditi phaṭkāra parajatra(yantra)",
    "806:16": "pramardini|",
    "806:17": "pratyālīḍhapadanyāse śikhī(khi)",
    "806:18": "jvālākulojjvale|| 7||",
    "806:19": "namasture mahāghore mālavīravināśini|",
    "806:20": "bhṛkuṭīkṛtavaktrābjasarvaśatrunisundanī(ṣūdini)|| 8||",
    "806:21": "namaḥ strīratnamudrāṅkahṛdayāṅgulibhūṣite|",
    "806:22": "bhūṣitāśeṣadikcakranikarasvakarākule|| 9||",
    "806:23": "namaḥ pramuditāśeṣamuktākṣīraprasāriṇi|",
    "806:24": "hasatprahasatuttāre māralolavaśaṅkari|| 10||",
    "806:25": "namaḥ samantabhūpālapata(ṭa)",
    "806:26": "lākarṣaṇa(ṇe)kṣaṇe|",
    "806:27": "carabhṛkuṭihūṃkārasarvāpadavimocanī(cini) || 11||",
    "806:28": "namaḥ śrīkhaṇḍakhaṇḍendu[su]muktābharaṇa(ṇo)jjvale|",
    "806:29": "amitābhajitābhārabhāsure kiraṇoddhruve(ddhure) || 12||",
    "806:30": "namaḥ kalpāntahutabhugajvālāmālāntare(ra)sthite|",
    "806:31": "ālīḍhamudi(dri)",
    "806:32": "tābaddharipucakravināśinī(ni)|| 13||",
    "806:33": "namaḥ karatarā(lā)",
    "806:34": "ghāṭa(ta)",
    "806:35": "caraṇāhatabhūtale|",
    "806:36": "bhṛkuṭīkṛtahukārasaptapātālabhedinī(ni)|| 14||",
    "806:37": "namaḥ śive śubhe śānte śāntanirvāṇagocare|",
    "806:38": "svāhāpraṇavasaṃyukte mahāpātakanāśanī(śini)|| 15||",
    "806:39": "namaḥ pramuditābaddharigātraprabhedini|",
    "806:40": "daśākṣarapadanyāse vidyāhuṃkāradīpite|| 16||",
    "806:41": "nama[stāre] ture pādaghātahuṃkāravījite|",
    "806:42": "merumaṇḍalakailāśabhuvanatrayacāriṇī(ṇi) || 17||",
    "806:43": "namaḥ sure sa(śa)",
    "806:44": "rākārahariṇāṅkakare(ra)sthite|",
    "806:45": "haridviruktaphaṭkāra(re)",
    "806:46": "aśeṣaviṣanāśiṇī(ni)|| 18||",
    "806:47": "namaḥ surāsuragaṇayakṣakinnarasevite |",
    "806:48": "abuddhamuditābhogakarī(ri)",
    "806:49": "duḥsvapnanāśinī(ni) || 19 ||",
    "806:50": "namaścandrārkasampūrṇanayanadyutibhāsvare|",
    "806:51": "tārādviruktatuttāre viṣamajvala(ra)nāśini|| 20||",
    "806:52": "namaḥ strītattvavinyāse śivaśaktisamanvite|",
    "806:53": "grahavetāra(la)",
    "806:54": "yakṣoṣmanāśini pravare ture|| 21||",
    "806:55": "mantramūlamidaṃ stotraṃ namaskāraikaviṃśatiḥ(ti)|",
    "806:56": "yaḥ paṭhetprātaḥ (paṭhet prayataḥ)",
    "806:57": "dhīmān devyābhaktisamanvite(taḥ)||22||",
    "806:58": "sāyaṃ vā prātarutthāya smaret sarvābhayapradam|",
    "806:59": "sarvapāpapraśamanaṃ sarvadurgatināśanam|| 23||",
    "806:60": "abhiṣikto bhavet tūrṇaṃ saptabhirjinakoṭibhiḥ|",
    "806:61": "māsamātreṇa caivāsau sukhaṃ bauddhapadaṃ vrajet|| 24||",
    "806:62": "viṣaṃ tasya mahāghoraṃ sthāvaraṃ cātha jaṅgamam|",
    "806:63": "smaraṇānna padaṃ yāti khāditaṃ pi(pī)tameva vā|| 25||",
    "806:64": "grahajo(jā)laviṣārtānāṃ parastrīviṣanāśanam|",
    "806:65": "anyeṣāṃ caiva sattvānāṃ dvisaptamabhivartitam|| 26||",
    "806:66": "putrakāmo labhet putraṃ ghanakāmo labheddhanam|",
    "806:67": "sarvakāmānavāpnoti na vighnaiḥ pratihanyate|| 27||",
    "806:68": "iti śrīsamyaksaṃbuddhavailo(ro)",
    "806:69": "canabhāṣitaṃ bhagavatyāryatārādevyā",
    "806:70": "namaskāraikaviṃśatināmāṣṭottaraśatakaṃ buddhabhāṣitaṃ parisamāptam"
}