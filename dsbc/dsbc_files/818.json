{
    "818:0": "avalokiteśvarāṣṭottaraśatanāmastotram",
    "818:1": "om namo'valokiteśvarāya",
    "818:2": "padmasattva mahāpadma lokeśvara maheśvara|",
    "818:3": "avalokiteśa dhīrāgrya vajradharma namo'stu te|| 1||",
    "818:4": "dharmarāja mahāśuddha sattvarāja mahāmate|",
    "818:5": "padmātmaka mahāpadma padmanātha namo'stu te|| 2||",
    "818:6": "padmodbhava supadmābha padmaśuddha suśodhaka|",
    "818:7": "vajrapadma supadmāṅka padmapadma namo'stu te|| 3||",
    "818:8": "mahāviśva mahāloka mahākāya mahopama|",
    "818:9": "mahādhīra mahāvīra mahāśaure namo'stu te|| 4||",
    "818:10": "sattvāśaya mahāyāna mahāyoga pitāmaha|",
    "818:11": "śambhu śaṅkara śuddhārtha buddhapadma namo'stu te|| 5||",
    "818:12": "dharmatattvārtha saddharma śuddhaddharmaṃ sudharmakṛt|",
    "818:13": "mahādharma sudharmāgrya dharmacakra namo'stu te|| 6||",
    "818:14": "buddhasattva susattvāgrya dharmasattva susattvadhṛk|",
    "818:15": "sattvottama susattvajña sattvasattva namo'stu te|| 7||",
    "818:16": "avalokiteśa nāthāgrya mahānātha vilokita|",
    "818:17": "ālokaloka lokārtha lokanātha namo'stu te|| 8||",
    "818:18": "lokākṣarākṣara mahā akṣarāgryākṣaropama|",
    "818:19": "akṣarākṣara sarvākṣa cakrākṣara namo'stu te|| 9||",
    "818:20": "padmahasta mahāhasta samāśvāsaka dāyaka|",
    "818:21": "buddhadharma mahābuddha buddhātmaka namo'stu te|| 10||",
    "818:22": "buddharūpa mahārūpa vajrarūpa surupavit|",
    "818:23": "dharmāloka sutejāgrya lokāloka namo'stu te|| 11||",
    "818:24": "padmaśrīnātha nāthāgrya dharmaśrīnātha nāthavān|",
    "818:25": "brahyanātha mahābrahma brahmaputra namo'stu te|| 12||",
    "818:26": "dīpa dīpāgrya dīpogra dīpāloka sudīpaka|",
    "818:27": "dīpanātha mahādipa buddhadīpa namo'stu te|| 13||",
    "818:28": "buddhābhiṣikta buddhāgrya buddhaputra mahābudha|",
    "818:29": "buddhābhiṣekamurddhāgrya buddhabuddha namo'stu te|| 14||",
    "818:30": "buddhacakṣo mahācakṣo dharmacakṣo mahekṣaṇa|",
    "818:31": "samādhijñānasarvasva vajranetra namo'stu te|| 15||",
    "818:32": "evaṃ sarvātmanā gauṇaṃ nāmnāmaṣṭaśataṃ tava|",
    "818:33": "bhāvayet stunuyādvāpi lokaiśvaryamavāpnuyāt|| 16||",
    "818:34": "āryāvalokiteśvaranāmāṣṭottaraśatādhyeṣaṇāstotraṃ samāptam|"
}