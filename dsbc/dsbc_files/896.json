{
    "896:0": "dharmadhātuvāgīśvaramaṇḍalastotram",
    "896:1": "mañjuśriyaṃ mahāvīraṃ sarvamāravināśanam |",
    "896:2": "sarvasiddhīśvaraṃ nāthaṃ vāgīśvaraṃ namāmyaham || 1 ||",
    "896:3": "mañjughoṣaṃ mahāvīraṃ sarvamāravināśanam |",
    "896:4": "sarvākārapradātāraṃ dharmadhātuṃ namāmyaham || 2 ||",
    "896:5": "mahoṣṇīṣaṃ sitacchatraṃ tejorāśiṃ jayoditam |",
    "896:6": "vikiraṇodgataṃ caiva mahogratejasaṃ name || 3 ||",
    "896:7": "akṣobhyaṃ ca mahābodhiṃ vajrasattvaṃ namāmyaham |",
    "896:8": "vajrarājaṃ vajrarāgaṃ vajrasādhuṃ namāmyaham || 4 ||",
    "896:9": "śrīratnasambhavaṃ nāthaṃ vajraratnaṃ namāmyaham |",
    "896:10": "vajrasūryaṃ vajraketuṃ vajrahāsaṃ name sadā || 5 ||",
    "896:11": "amitābhaṃ mahārājaṃ vajradharmaṃ namāmyaham |",
    "896:12": "vajratīkṣṇaṃ mahāketuṃ vajrabhāṣaṃ name name || 6 ||",
    "896:13": "amoghasiddhiṃ siddheśaṃ vajrakarma namāmyaham |",
    "896:14": "vajrarakṣaṃ vajrayakṣaṃ vajrasandhiṃ name sadā || 7 ||",
    "896:15": "locanākhyāṃ mahādevīṃ pāṇḍarākhyāṃ namāmyaham |",
    "896:16": "māmakīṃ caiva devīṃ tāṃ tārādevīṃ name name || 8 ||",
    "896:17": "vajrāṅkuśaṃ mahāvīraṃ vajrapāśaṃ mahatprabham |",
    "896:18": "vajrasphoṭaḥ mahānāthaṃ vajrāveśaṃ name name || 9 ||",
    "896:19": "adhimuktīśvarīṃ bhūmiṃ pramuditāṃ namāmyaham |",
    "896:20": "vimalākhyāṃ mahābhūmiṃ prabhākarīṃ name name || 10 ||",
    "896:21": "arciṣmatīṃ sudurjayāṃ cābhimukhīṃ namāmyaham |",
    "896:22": "dūraṅgamāmacalākhyāṃ sādhumatīśvarīṃ name || 11 ||",
    "896:23": "dharmameghāṃ mahāmeghāṃ samatākhyāṃ prabhāṃ name |",
    "896:24": "ratnapāramitāṃ devīṃ dānapāramitāṃ name || 12 ||",
    "896:25": "śīlapāramitāṃ devīṃ kṣāntipāramitāṃ name |",
    "896:26": "vīryapāramitāṃ devīṃ dhyānapāramitāṃ śraye || 13 ||",
    "896:27": "prajñāpāramitāṃ devīm upāyākhyaṃ namāmyaham |",
    "896:28": "praṇidhānabalaṃ caiva jñānapāramitāṃ sadā || 14 ||",
    "896:29": "karmapāramitāṃ devīṃ namāmi satataṃ tathā |",
    "896:30": "āyuścittapariṣkāraṃ karmopapattikāṃ name || 15 ||",
    "896:31": "ṛddhyākhyāmadhimuktiṃ ca praṇidhānāṃ namāmyaham |",
    "896:32": "jñānākhyavaśitāṃ devīṃ dharmākhyavaśitāṃ name || 16 ||",
    "896:33": "tathatāṃ ca mahādevīṃ buddhabodhiṃ namāmyaham |",
    "896:34": "vasumatīṃ mahālakṣmīṃ ratnajvālāṃ name name || 17 ||",
    "896:35": "uṣṇīṣavijayāṃ devīṃ mārīcīṃ parṇaśābarīm |",
    "896:36": "jāṅguliṃ dhāraṇīṃ vande anantamukhadhāraṇīm || 18 ||",
    "896:37": "cundāṃ prajñāṃ ca padmāṃ ca sarvāvaraṇaśodhanīm |",
    "896:38": "akṣayajñānakāraṇḍaṃ dharmakāyavatīṃ name || 19 ||",
    "896:39": "dharmapratividaṃ devamarthaṃ ca pratisaṃvidam |",
    "896:40": "niruktisaṃvidaṃ devaṃ pratibhānākhyasaṃvidam || 20 ||",
    "896:41": "vajralāsyāṃ vajramālāṃ vajragītāṃ name sadā |",
    "896:42": "vajranṛtyāṃ mahādevīṃ namāmi satataṃ mudā || 21 ||",
    "896:43": "samantabhadraṃ bodhīśamakṣayatāṃ matiṃ name |",
    "896:44": "kṣitigarbhaṃ khagarbhaṃ ca bodhisattvaṃ namāmyaham || 22 ||",
    "896:45": "gagaṇagañjanātheśaṃ ratnapāṇiṃ name name |",
    "896:46": "sāgaramatiṃ bodhīśaṃ vajragarbhaṃ namāmyaham || 23 ||",
    "896:47": "lokeśvaraṃ mahāsatvaṃ sthānaprāptaṃ namāmyaham |",
    "896:48": "candraprabhaṃ mahātejaṃ vande'haṃ jālinīprabham || 24 ||",
    "896:49": "amitaprabhabodhośaṃ śrīpatiṃ bhānukūṭakam |",
    "896:50": "sarvaśokatamodghāṭaviṣkambhinaṃ namāmyaham || 25 ||",
    "896:51": "yamāntakaṃ mahāvīraṃ prajñāntakaṃ name name |",
    "896:52": "padmāntakaṃ mahāvīraṃ vighnāntakaṃ namāmyaham || 26 ||",
    "896:53": "trailokyavijayaṃ vīraṃ vajrajvālāṃ namāmyaham |",
    "896:54": "herukaṃ vajravīreśaṃ parameśaṃ namāmyaham || 27 ||",
    "896:55": "cakravarttīśvaraṃ vande sumbharājaṃ namāmyaham |",
    "896:56": "puṣpāṃ dhūpāṃ mahādīpāṃ gandhāṃ devīṃ namāmyaham || 28 ||",
    "896:57": "vajrarūpāṃ vajraśabdāṃ rasavajrāṃ namāmyaham |",
    "896:58": "vajrasparśāṃ mahādevīṃ viśvavarṇāṃ name sadā || 29 ||",
    "896:59": "indrayamajaleśāṃśca kuberamīśvaraṃ namaḥ |",
    "896:60": "agniṃ nairṛtyanāthaṃ ca vāyurājaṃ namāmyaham || 30 ||",
    "896:61": "brahmāṇaṃ viṣṇudevaṃ ca maheśvaraṃ kumārakam |",
    "896:62": "brahmāṇīṃ ca mahādevīṃ rudrāṇīṃ vaiṣṇavīṃ name || 31 ||",
    "896:63": "kaumārīṃ raktavarṇāṃ ca mahendrāṇīṃ namāmyaham |",
    "896:64": "vārāhīṃ kālikāṃ caṇḍīṃ bhṛṅgiṇaṃ gaṇanāyakam || 32 ||",
    "896:65": "mahākālaṃ mahābhīmaṃ nandikeśaṃ raviṃ name |",
    "896:66": "candraṃ bhaumaṃ budhaṃ vande guruṃ śukraṃ śanaiścaram || 33 ||",
    "896:67": "rāhuṃ ketuṃ balabhadraṃ jayakaraṃ name sadā |",
    "896:68": "madhukaraṃ vasantaṃ ca anantaṃ vāsukiṃ name || 34 ||",
    "896:69": "takṣaṃ karkoṭakaṃ padmaṃ mahāpadmaṃ namāmyaham |",
    "896:70": "śaṅkhapālaṃ ca kulikaṃ vemacitraṃ valiṃ name || 35 ||",
    "896:71": "prahlādaṃ ca mahādaityaṃ vairocanaṃ name sadā |",
    "896:72": "guruḍendra sumbharājaṃ paṃcaśikhaṃ namāmyaham || 36 ||",
    "896:73": "sarvārthasiddhiṃ vighneśaṃ pūrṇabhadraṃ namāmyaham |",
    "896:74": "maṇibhadraṃ mahāyakṣaṃ dhanadaṃ ca maheśvaram || 37 ||",
    "896:75": "vaiśravaṇaṃ mahāvīraṃ civikuṇḍalinaṃ namāmyaham |",
    "896:76": "kelimāliṃ sukhendraṃ ca calendraṃ ca namāmyaham || 38 ||",
    "896:77": "hāratīṃ yakṣiṇīṃ devīṃ bahuputravatīṃ name |",
    "896:78": "aśvinīṃ bharaṇīṃ tārāṃ kṛttikāṃ rohaṇīṃ tathā || 39 ||",
    "896:79": "mṛgaśīrṣāṃ tathaivārdrāṃ punarvasuṃ namāmyaham |",
    "896:80": "puṣyamāśleṣakātārāṃ maghāṃ ca pūrvāphālgunīm || 40 ||",
    "896:81": "uttarāphālgunīṃ hastaṃ citrāṃ svātiṃ viśākhakām |",
    "896:82": "anurādhāṃ tathā jyeṣṭhāṃ mūlatārāṃ namāmyaham || 41 ||",
    "896:83": "pūrvāṣāḍhottarāṣāḍhāṃ śravaṇāṃ ca namāmyaham |",
    "896:84": "dhaniṣṭhāṃ śatabhiṣāṃ ca pūrvottarābhādrapadām || 42 ||",
    "896:85": "revatīṃ ca mahātārām abhijitaṃ namāmyaham |",
    "896:86": "vajrāṅkuśaṃ mahāvīraṃ vajrapāśaṃ namāmyaham || 43 ||",
    "896:87": "vajrasphoṭaṃ mahābhīmaṃ vajrāṃśaṃ vai name name |",
    "896:88": "vāgīśvaraṃ mahābodhiṃ sarvavighnavināśakam || 44 ||",
    "896:89": "sarvajñaṃ jñānadātāraṃ dharmadhātuṃ namāmyaham |",
    "896:90": "mañjuśriyaṃ mahājñānaṃ sarvavidyāpradeśvaram |",
    "896:91": "sarvākārasvarūpaṃ ca vādirājaṃ namāmyaham || 45 ||",
    "896:92": "śrī dharmadhātuvāgīśvaramaṇḍalastotraṃ samāptam |"
}