{
    "825:0": "ratnamālāstotram",
    "825:1": "ācāryavanaratnapādaviracitam",
    "825:2": "lokeśvaraṃ vimalaśūnyakṛpārdracittaṃ",
    "825:3": "mārgajñatāprathitadeśanayārthavācam |",
    "825:4": "sarvajñatādiparipūrṇaviśuddhadehaṃ",
    "825:5": "jñānādhikāralalitaṃ śirasā namāmi || 1 ||",
    "825:6": "vaineyabhedavaśato bahudhāvabhāsai-",
    "825:7": "reko'pi pātragajaleṣu śaśīva yasmāt |",
    "825:8": "saṃlakṣase parahitānugataiva tasmād",
    "825:9": "buddhistvaho paramavismayanīyarūpā || 2 ||",
    "825:10": "saṃpūrṇacandravadane lalito lalāṭa-",
    "825:11": "deśādvinirgatamaheśvaradevaputraḥ |",
    "825:12": "vaineyaśāmbhavajanapratibodhanārthaṃ",
    "825:13": "devādhidevapratimānuja īśvarastvam || 3 ||",
    "825:14": "vaineyakomalabhavapratibodhanāya",
    "825:15": "kiṃ dhāma saṃbhṛta mahāśubhalakṣaṇaṃ te |",
    "825:16": "niryāta eva hi pitāmahadevapūjāṃ",
    "825:17": "lokeśvareśvaraparaṃ śirasā namāmi || 4 ||",
    "825:18": "vaineyavaiṣṇavajanapratibodhanāya",
    "825:19": "rājīvapāṇihṛdayāt pratiniḥsṛto'sau |",
    "825:20": "nārāyaṇo'pi bhuvaneśvara eva tasmāt",
    "825:21": "puṃsāṃ tvameva paramottama eva nānyaḥ || 5 ||",
    "825:22": "candrārkasādarabalāhitabhaktibhājāṃ",
    "825:23": "saṃdarśanārthamibhanīlasulocanābhyām |",
    "825:24": "yanniḥsṛtau śaśiravī bhuvi locanābhyāṃ",
    "825:25": "dhvastāntarālatamasaṃ tamahaṃ namāmi || 6 ||",
    "825:26": "sārasvatīvinayayojitabhaktibhājāṃ",
    "825:27": "bodhāya vai bhagavatīha sarasvatīyam |",
    "825:28": "dṛṣṭāgratastava jinātmajapaprasūtā",
    "825:29": "prajñābhilāṣiphaladaṃ tamahaṃ namāmi || 7 ||",
    "825:30": "vaineyavāyujanitākṣaramārgasiddhyai",
    "825:31": "yo lokanātha sugato'tha viniḥsṛto'sau |",
    "825:32": "devaḥ samīraṇavaro bhuvi janmabhājā-",
    "825:33": "mīryāpathārthaphaladaṃ tamahaṃ namāmi || 8 ||",
    "825:34": "vaineyavāruṇaśivāyanamīpsitānāṃ",
    "825:35": "saṃbodhanārthamudarātsugatātmajānām |",
    "825:36": "yanniḥsṛto varuṇadevavaro'pyakasmā-",
    "825:37": "daiśvaryasiddhi phaladaṃ tamahaṃ namāmi || 9 ||",
    "825:38": "vaineyasaṃmataphalādyabhilāṣiṇo vai",
    "825:39": "saṃsiddhaye pravaralakṣaṇapādapadme |",
    "825:40": "yanniḥsṛtā bhagavatī dharaṇī prasiddhā",
    "825:41": "trailokyanāthamasamaṃ satataṃ namāmi || 10 ||",
    "825:42": "saṃsāramuktamapi susthitameva tatra",
    "825:43": "kāruṇyataśca bhavacāriṇi sattvavarge |",
    "825:44": "bhūyāt sthitirmama sadāsthirasā bhavanta-",
    "825:45": "mevaṃ mahāśayavaraṃ paramaṃ namāmi || 11 ||",
    "825:46": "ekena pādatalakena bhavatsvakena",
    "825:47": "cakrāntasaṃvaramanantaralokadhātau |",
    "825:48": "kalpāntadagdhabhuvane jvalitogravahni-",
    "825:49": "rniḥśvāsavāyubalatastava nirvṛtaḥ syāt || 12 ||",
    "825:50": "svāṃ tarjanīṃ mukhadhṛto'hitatarjanena",
    "825:51": "saṃcālitāśca bahumerugaṇā nakhasya |",
    "825:52": "koṣoddhṛtaṃ jaladhitoyamaśeṣataḥ syāt",
    "825:53": "sāmarthyamīdṛśamaho bhavataḥ kuto'nyat || 13 ||",
    "825:54": "kvedaṃ ca śaiśavaparaṃ nanu cārurupaṃ",
    "825:55": "saṃdarśanīyavarakomalabālacandram |",
    "825:56": "durvāramāramathanaṃ ca mayaikasahyaṃ",
    "825:57": "vikrāntaduḥsahaparaṃ kva ca ceṣṭitaṃ te || 14 ||",
    "825:58": "eṣā batāñjananibhorujanāvalī sā",
    "825:59": "kauṭilyacāruvikaṭā svaśiroruhāgre |",
    "825:60": "kleśendhane jvalitavisphuritatvavahne-",
    "825:61": "rdhūmāvalīva vimalā nanu lakṣyate te || 15 ||",
    "825:62": "tvatkāntileśavimalā daśadikpratānaiḥ",
    "825:63": "pakṣāsitakṣayakṛśā sakalā suśobhā |",
    "825:64": "paryanta iṣṭaśaśino bhavaneṣu yatte",
    "825:65": "manye virāji nikhilaṃ tava kāntileśāt || 16 ||",
    "825:66": "bandhurhi ko mārgikasaṃmataṃ mataṃ naro narī sā sa ca satpathaṃ patham |",
    "825:67": "parārthasaṃpāditasaṃvaraṃ varaṃ namāmi bhūmīśvararājinaṃ jinam || 17 ||",
    "825:68": "anityanirvāṇapade sthitaṃ sthitaṃ prabhāsvarādhiṣṭhitasaṃhitaṃ hitam |",
    "825:69": "śamīkṛtāśeṣajanaṃ śivaṃ śivaṃ namāmi bhūmīśvararājinaṃ jinam || 18 ||",
    "825:70": "gabhastimālāmitasaṃkulaṃ kulaṃ tatra svapāṇau dhṛtapaṅkajaṃ kajam |",
    "825:71": "ratānugāśobhitasaṃrataṃ rataṃ namāmi bhūmīśvararājinaṃ jinam || 19 ||",
    "825:72": "svadharmadhātuṃ karuṇāparaṃ paraṃ śubhādisaṃbhārasusaṃbhṛtaṃ bhṛtam |",
    "825:73": "vikalpahīnaṃ dhvanideśakaṃ śakaṃ namāmi bhūmīśvarājinaṃ jinam || 20 ||",
    "825:74": "tathatātathatādvayaśātaśataṃ sadasatparipūritadharmakatham |",
    "825:75": "kathanīyavirājitasatyaparaṃ praṇame dharaṇīśvararājavaram || 21 ||",
    "825:76": "varavārijarūpi jagatprasaraṃ sarasīruhalocanacārutaram |",
    "825:77": "tarasāpi rasatvaviśuddhiparaṃ praṇame dharaṇīścarājavaram || 22 ||",
    "825:78": "varanirmitabhogaparārtharataṃ rataśūnyanirañjanadharmadharam |",
    "825:79": "dharaṇīndravibhūṣitasiddhiparaṃ praṇame dharaṇīśvararājavaram || 23 ||",
    "825:80": "varasatsahajodadhicandramukhaṃ sukhabhāṣitasattvavimuktipadam |",
    "825:81": "padabhūṣaṇalakṣaṇatānuparaṃ praṇame dharaṇīśvararājavaram || 24 ||",
    "825:82": "lokeśvareyaṃ (māṃ)",
    "825:83": "tava ratnamālāmacīkaracchrīvanaratnapādaḥ |",
    "825:84": "avāpi yattena śubhapraviṣṭaṃ tenaiva loko'stu samantabhadraḥ || 25 ||",
    "825:85": "śrīmadāryāvalokiteśvarabhaṭṭārakasya",
    "825:86": "ratnamālāstotraṃ samāptam |"
}