{
  "SA14n1225:0": "XXXII",
  "SA14n1225:1": "parīnandanāparivarto nāma dvātriṃśattamaḥ |",
  "SA14n1225:2": "sahapratilabdhānāmityādi |",
  "SA14n1225:3": "anena labdhānāṃ samādhīnāṃ tatkālajamanuśaṃsamāha |",
  "SA14n1225:4": "ata eva sahaśabdaḥ samānakālatārthaḥ |",
  "SA14n1225:5": "pratilabdhānāmiti pratilabdheṣu satsu |",
  "SA14n1225:6": "ebhireva nayeriti yathoktavastubhiraṣṭābhirabhisamayaiḥ |",
  "SA14n1225:7": "ebhireva nāmabhiriti sarvākārajñatādibhiḥ |",
  "SA14n1225:8": "sarvāsu ca jātiṣu na jātu buddhavirahito'bhūdityuddeśaḥ |",
  "SA14n1225:9": "asya nirdeśa uttaro granthaḥ svapnāntaragatopīti yāvat |",
  "SA14n1225:10": "tatra khalvityādirupodghātaḥ parīndanāyāḥ |",
  "SA14n1225:11": "tadanenāpi paryāyeṇaiti sadāpraruditavṛttāntenāpi |",
  "SA14n1225:12": "tasmāttarhītyādinā anuśāsanīśabdaparyantena prathamā parīndanā |",
  "SA14n1225:13": "atra ca tathāgatādhiṣṭhāneneti vacanaṃ kimartham ?",
  "SA14n1225:14": "mahārthatvābhdagavatī bavhantarāyā ca vinā buddhādhiṣṭhānena likhitumapi [na] śakyeti pradarśanārtham |",
  "SA14n1225:15": "iyamasmākamanuśāsanītīyamasmākaṃ parīndanā |",
  "SA14n1225:16": "yathā asyāḥ śravaṇādikaṃ na vicchidyate tathā tvayā kartavyamityarthaḥ |",
  "SA14n1225:17": "tatkasya hetoriti |",
  "SA14n1225:18": "tatparīndanaṃ kena prayojanenetyarthaḥ |",
  "SA14n1225:19": "ata āha |",
  "SA14n1225:20": "atra hītyādi |",
  "SA14n1225:21": "tatkathamityādinā dvītīyaparīndanāyā upodghātaḥ |",
  "SA14n1225:22": "asmin mama samucchaya iti mama śarīre |",
  "SA14n1225:23": "tathataditi |",
  "SA14n1225:24": "etatpremādikam |",
  "SA14n1225:25": "etenopādghātena dve parīndane |",
  "SA14n1225:26": "yata āha dvirapi trirapīti |",
  "SA14n1225:27": "parīndāmīti samarpayāmi |",
  "SA14n1225:28": "anuparīndamītyānukūlyaṃ samarpayāmi |",
  "SA14n1225:29": "kathaṃ parīndanetyata āha yatheyamityādi |",
  "SA14n1225:30": "anyaḥ puruṣa ityabhaktaḥ |",
  "SA14n1225:31": "kasmādasyāmetāvadgauravamityata āha |",
  "SA14n1225:32": "yāvadityādi bahuvidhāśca pūjābhiriti yāvat |",
  "SA14n1225:33": "bhagavatyāḥ parisamāptimudyotayan saṅgītikāra āha |",
  "SA14n1225:34": "idamavocadityādi |",
  "SA14n1225:35": "idamiti prajñāpāramitāsūtram |",
  "SA14n1225:36": "avocaditi bhāṣitavān |",
  "SA14n1225:37": "bhagavāniti śāstā |",
  "SA14n1225:38": "parārtharasikāstathāgatāḥ |",
  "SA14n1225:39": "parārtha ca parārthakriyā bhagavatyā deśanā |",
  "SA14n1225:40": "tata āha |",
  "SA14n1225:41": "āttamanā iti |",
  "SA14n1225:42": "niruttaradharmadeśanayā prītaḥ sannityarthaḥ |",
  "SA14n1225:43": "śrotāraḥ kimukurvannityāha |",
  "SA14n1225:44": "bhagavato bhāṣitamabhyanandanniti |",
  "SA14n1225:45": "abhipūrvo nandiḥ sakarmakaḥ |",
  "SA14n1225:46": "sa kadācidabhilāṣe vartate |",
  "SA14n1225:47": "kadācitprītau |",
  "SA14n1225:48": "yadā prītau tadā tadālambanamevāsya karma bhavati |",
  "SA14n1225:49": "tatpunaḥ karma bhagavato bhāṣitam |",
  "SA14n1225:50": "eṣaiva deśanā |",
  "SA14n1225:51": "ke punaste śrotāra ityāha |",
  "SA14n1225:52": "te cetyādi |",
  "SA14n1225:53": "lokaḥ sattvasamūhaḥ |",
  "SA14n1225:54": "sa ca śrotṝṇāṃ rājagṛhasya ca prakṛtatvānmanuṣyasamūha eva kevalo gamyeta |",
  "SA14n1225:55": "tathā mā bhūditi viśeṣaṇaṃ kriyate |",
  "SA14n1225:56": "sadevamānuṣāsuragandharvaśceti |",
  "SA14n1225:57": "te ca bodhisattvādayaḥ sadevādiśca loka iti samuccayaḥ ||",
  "SA14n1225:58": "parīndanābhidhāyī parivartaḥ parīndanāparivartaḥ ||",
  "SA14n1225:59": "nānāvibhramalāñchanavyapagamādagrāhyamagrāhakam |",
  "SA14n1225:60": "bhātyetattathatātmanā samarasaṃ yasyāmaśeṣaṃ jagat ||",
  "SA14n1225:61": "prajñāpāramitā vikalpataraṇī sā bodhisattvasya dhīḥ |",
  "SA14n1225:62": "dhīraiḥ saiva viśuddhipāragamane tāthāgatī kathyate ||",
  "SA14n1225:63": "sa(śa)satyaṣṭau yadabhisamayān yatsahasrāṇi cāṣṭau |",
  "SA14n1225:64": "sūtraṃ tatte bhagavati mayā yaśca labdho vibhajya ||",
  "SA14n1225:65": "puṇyaskandhaḥ phalatu sa yathā yuktimuktiṃ prajānām |",
  "SA14n1225:66": "niḥsīmānāṃ mama ca vaśitāṃ viśvakāryakriyāsu ||",
  "SA14n1225:67": "anupamaguṇamṛṣṭā nirmalāśeṣavarṇā |",
  "SA14n1225:68": "harati bhavaratiṃ vo bhāratī gautamasya |",
  "SA14n1225:69": "bhavati mahati(?)",
  "SA14n1225:70": "vartiryā dayā snehapūrṇe",
  "SA14n1225:71": "janamanasi tadantarjjyotiṣaḥ saṃkramāya ||",
  "SA14n1225:72": "prajñāpāramitāyāḥ pramitāyā daśaśatībhiraṣṭābhiḥ |",
  "SA14n1225:73": "sāratametyabhisamaye sphuṭā ghanā pañjikeyaṃ me ||",
  "SA14n1225:74": "ye dharmā hetuprabhavā hetuṃ teṣāṃ tathāgato hyavadat |",
  "SA14n1225:75": "teṣāṃ ca yo nirodha evaṃvādī mahāśramaṇaḥ ||",
  "SA14n1225:76": "?(?) samvat ā to rājñaḥ śrīharṣadevarājye śrīgaṇḍigulmaviṣaye |",
  "SA14n1225:77": "kulaputrakāyasthaḥ (? sthasya)",
  "SA14n1225:78": "paṇḍitaśrījīvandharaśiṃ(siṃ)",
  "SA14n1225:79": "hasya pustako(?",
  "SA14n1225:80": "stike)yamiti ||0 ||"
}