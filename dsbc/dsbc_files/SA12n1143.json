{
  "SA12n1143:0": "śrīsuvrahmaṇyadīkṣivena viracitaṃ nṛtyanāṭakama",
  "SA12n1143:1": "nṛtyaprasūtiḥ",
  "SA12n1143:2": "nāndī",
  "SA12n1143:3": "gaula śrādi",
  "SA12n1143:4": "pa mārajitaṃ bhaja manasā madhuragauṇaṃ sarvadā",
  "SA12n1143:5": "śra kāruṇam śrāśutoṣam kāmāriṃ karidamanakaram",
  "SA12n1143:6": "ca dāruṇābhavasāgaratāraṇaṃ taruṇāruṇacaraṇayugāanam",
  "SA12n1143:7": "yogināṃ cakravartinaṃ dhammacakrapravartinaṃ",
  "SA12n1143:8": "vṛkṣamūlāsīnaṃ śiṣyāvṛtaṃ jñānaprasūnam",
  "SA12n1143:9": "prathamaṃ dṛśyanam",
  "SA12n1143:10": "devarājo samarasanmnddho śrāyāti",
  "SA12n1143:11": "haṃsadhvaniḥ śrādi",
  "SA12n1143:12": "bhīkara vajrāyudhadharo",
  "SA12n1143:13": "śrīkarāśaśīpatirasau",
  "SA12n1143:14": "śravṛttaraśatapurabhāśotsuka vīro devarājo śrāthātyadhunā",
  "SA12n1143:15": "kiṃcitkālānantaraṃ vijayībhūtī indraḥ vijayanṛttaṃ nṛtyati",
  "SA12n1143:16": "vijayībhūto'ham vijayībhūto'ham",
  "SA12n1143:17": "tataḥ sahasaiva śrabaṃrāt tripurāsurāḥ vividhāyudharvarṣaiḥ devān trāsayanti |",
  "SA12n1143:18": "bhīvabhītāḥ",
  "SA12n1143:19": "devāḥ parameśvaraṃ prārthayanti |",
  "SA12n1143:20": "darbāra śrādi",
  "SA12n1143:21": "bhayaṃkarātripurarūpaiḥ bādhitā vayaṃ bhītyā svayaṃbhuvaṃ śaṃbhunāthaṃ śivaṃ param śraghoraṃ",
  "SA12n1143:22": "raktavantastīvrabhakatyā baddhāñlihastāḥ śrava bhava śiveti śrāhvaya prārthayāmaḥ",
  "SA12n1143:23": "nāṭayaraṃgī nimiṣamekam timirāyati,",
  "SA12n1143:24": "tataḥ devāḥ śivāgamanaṃ vīkṣya gāyanti |",
  "SA12n1143:25": "nāṭakaruṃji śrādi",
  "SA12n1143:26": "pa śrāyati maheśvaro śrabhitojasā bhāsvaro",
  "SA12n1143:27": "nirāyudhakarayugo samāvṛttamaragaṇo",
  "SA12n1143:28": "ca 1 sākrosaṃ ghoṣatmiḥ sarvaśatrunāśaṃ",
  "SA12n1143:29": "vākdevavaikuṣṭādibhiḥ prārthito ittham",
  "SA12n1143:30": "śīghram śrasmān śāstrīya deva deva",
  "SA12n1143:31": "virāgragaṇya sayojayādhunā",
  "SA12n1143:32": "sarvadaivāḥ gāyanti",
  "SA12n1143:33": "ca 2 samaravijayasannaddhāḥ śraniruddhāḥ śratisamarthāḥ śrāśritāstrayaṃbake vayam",
  "SA12n1143:34": "ca 3 karaghṛtāśugo'haṃ khaṇḍanacaturo'ham",
  "SA12n1143:35": "devaḥ 1 niratiśaya javasahito nirmūlakaracaturonirghṛṇartīkṣṇatamo'smyaham",
  "SA12n1143:36": "ca 4 nirupamaśakharṣaṇotsuko'smyaham",
  "SA12n1143:37": "śranupamanagajābrūnibhakuṃcito",
  "SA12n1143:38": "dhanuvaravinākanāmāhaṃ dhanvanāmadhirājabhṛto",
  "SA12n1143:39": "dānavabhīṣaṇakaraghoṣajyāmaṇḍitapracaṇḍo'ham",
  "SA12n1143:40": "ca 5 vajrasāraśulāyudho'haṃ vairipurnibhedanacaturo",
  "SA12n1143:41": "vācāmagocaravegeṇa vairivaśayaṃ kāraye",
  "SA12n1143:42": "tataḥ sarve devāḥ parameśvara pārvatī ca pradakṣiṇaṃ kurvanti “ samaravijayasammaddhāḥ“",
  "SA12n1143:43": "itigāyantaḥ",
  "SA12n1143:44": "pantuvarālī śrādi",
  "SA12n1143:45": "dṛṣṭā mamatābhṛtaṃ devagaṇa śratvā pratāpaprakaṭīkaraṇaṃ",
  "SA12n1143:46": "matvā mabnasi tripuradahanaṃ mamattāharaṇa yugapada devanām",
  "SA12n1143:47": "smitānanaṃ himavatsutāyāh jitandriyo śaṃbhukhalokya",
  "SA12n1143:48": "mandāhāsāṃkitaṃ suṃdarāsyaṃ candramaulikarvidārito",
  "SA12n1143:49": "śrānandaraudrarasāṃkita mahāhāsaṃ sahasākarot",
  "SA12n1143:50": "duṃdubhiśatakoṭisvanādiva (tripura)",
  "SA12n1143:51": "nāśam śrabhūta tatkṣaṇe |",
  "SA12n1143:52": "dvitīyaṃ dṛśyam",
  "SA12n1143:53": "rkicitkālānantara kailāse pradoṣasamaye,",
  "SA12n1143:54": "sarve devāḥ ḥ saṃgatāḥ parameśvarasya nṛttaṃ draṣṭum |",
  "SA12n1143:55": "śaṃbhu nṛttaṃ kartuṃ mārayati |",
  "SA12n1143:56": "nītimati śrādi",
  "SA12n1143:57": "pa pradoṣasamaye nṛttaparaṃ parameśvaraṃ stuvantyamarāḥ",
  "SA12n1143:58": "śra vrataṃ dhṛtaṃ natāvane vilaṃbite hate same",
  "SA12n1143:59": "ca tūṃburuvīṇānandimṛdaṃgavādanairutsāhapūrṇam",
  "SA12n1143:60": "śaṃbhumavalokya sasmitam śraṃbujānanā haimavatī",
  "SA12n1143:61": "kuṃbakucayugalāmṛduvācā yācate nārade munis",
  "SA12n1143:62": "biṃbāyitagītāmṛtātra gīyatāṃ tripurāntakathā",
  "SA12n1143:63": "nāradamunistadā tripurāntakathāṃ gāyati |",
  "SA12n1143:64": "kābodi",
  "SA12n1143:65": "bhayaṃkaravarakrūrāmaranāśakaraparatripurāsurendraiḥ bādhito bhītabhīto purandharo viṣṇubraṃhādibhiḥ sakaladevagaṇeḥ saha tryaṃbakaṃ śaraṇaṃgato pālaya kṛpālaya parameśvareti yācate,",
  "SA12n1143:66": "kṛpātaraṃgittāntaraṃgaḥ śaṃkaro'bhayaṃkaro nirāyadhastadaiva tripurāntakaparo bhavati |",
  "SA12n1143:67": "taṃ prārthayanti devāḥ śāstrīyatāmasmāniti |",
  "SA12n1143:68": "voḍī devaiko dhanurbhavāmīti śranyo bhavāmi sāyakamiti",
  "SA12n1143:69": "śranyataro bhavāmi triśūlamiti uccaiḥ ghoṣayantaḥ",
  "SA12n1143:70": "bhūtavijayina iva nṛtyanti |",
  "SA12n1143:71": "tān dṛṣṭavā",
  "SA12n1143:72": "karuṇārasāṃkitahāsyarasotpattyā codito",
  "SA12n1143:73": "maheśvaro uccaiḥ mahāhāsaṃ karoti,",
  "SA12n1143:74": "tatkṣaṇaiva",
  "SA12n1143:75": "bhūta tripuradahanaṃ vīkṣya vismayātpullalocanāḥ",
  "SA12n1143:76": "devāḥ hāhākāraṃ kurvanti prastuvanti ca |",
  "SA12n1143:77": "etatsarvamabhinayaiḥ darśayati śaṃbhuḥ",
  "SA12n1143:78": "gītasyānte śakaro,",
  "SA12n1143:79": "ghoṣati sotsāha",
  "SA12n1143:80": "sṛṣṭaṃ nava śāstramidamabhinayanāmāṃkitamiti |",
  "SA12n1143:81": "śaṃbhustadā taṇḍumāhvava gāyati nṛtyati ca",
  "SA12n1143:82": "navarasakannḍa śrādi",
  "SA12n1143:83": "pa śrṛṇu me mahāmuni sṛṣṭaṃ navaṃ śāstramidam",
  "SA12n1143:84": "śrabhinayaiti nāmāṃkitaṃ pracārayācāryaiḥ",
  "SA12n1143:85": "śra bhaṇitaṃ bhaveta nṛtyaṃ nṛttamabhinayākitaṃ",
  "SA12n1143:86": "locanadvayamivaidaṃ pālaya navarasāṃkitam |",
  "SA12n1143:87": "ca gaṇītaṃ pauruṣāṃkitaṃ tāṇḍavaṃ bhavet",
  "SA12n1143:88": "dharaṇyāṃ tattaṇḍunā śikṣāhaṃ bhavet",
  "SA12n1143:89": "bhaṇiva strībhāvanṛtyaṃ lāsyanāmāṃkitaṃ",
  "SA12n1143:90": "vanitābhyāṃ pārvatyā śrabhayāsito bhavet |",
  "SA12n1143:91": "tadā sarve nṛtyanti",
  "SA12n1143:92": "madhyamāvatī rūpakam",
  "SA12n1143:93": "pa dhurjaṭiṃ śaṃbhu nāyaṃ dūrikṛtakhiladuritasamūhaṃ",
  "SA12n1143:94": "śrarcāmḥ nṛttairabhinayaiḥ sānanda lāsya tāṇḍavābhyām",
  "SA12n1143:95": "śra sūlādisaptatālāṃṅkita kolāhalannṛttairiha",
  "SA12n1143:96": "śūlāyughadhaṭaśaṃbhu parvatabālāṃ ca",
  "SA12n1143:97": "kiṇkirṇāmālābhirārā dhayāmovayam",
  "SA12n1143:98": "ca 1 śrāśritāvane karuṇāṃma śratiśāntaṃ yogamejjana",
  "SA12n1143:99": "śrasuranāśane vīraraudrādibhāvān darśayanta",
  "SA12n1143:100": "śravanatāṃgīṃpūrṇa śrṛṃgāraṃ kāśayantīma",
  "SA12n1143:101": "bhavāliṃgita madhyamāvatīṃ bhavaṃcāpi vaṃdāmahe",
  "SA12n1143:102": "ca 2 gaṃgādharaṃ śaṃkābhṛtaurvācakainindantīṃ",
  "SA12n1143:103": "kalahāṃnvarita khaṃḍitādikānāyikānnaṭantīṃ",
  "SA12n1143:104": "tuṃgasvanabharacadanasikāṃ dvāre vāsakasajjikāṃ",
  "SA12n1143:105": "śaṃkaram unmādayantīṃ svādhīnapatikāṃ bhaje |"
}