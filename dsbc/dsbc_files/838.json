{
    "838:0": "ṣaṭtriṃśatsaṃvarastutiḥ",
    "838:1": "śrīsaṃvaraṃ mahāvīraṃ vārāhīṃ cāpi yoginīm |",
    "838:2": "namāmi sarvabhāvena yoginījālanāyakam || 1 ||",
    "838:3": "ḍākinīṃ ca tathā lāmāṃ khaṇḍarohāṃ ca rūpiṇīm |",
    "838:4": "caturo'mṛtabhāṇḍāṃśca bodhicittena pūritān || 2 ||",
    "838:5": "pullīramalayodbhūtaṃ khaṇḍakapālasaṃvaram |",
    "838:6": "pracaṇḍāliṅgaṇaṃ vīraṃ namāmi śirasi sthitam || 3 ||",
    "838:7": "jālandharātsamutpannaṃ mahākaṃkālasaṃvaram |",
    "838:8": "caṇḍālyāliṅgaṇaṃ vīraṃ śikhādeśe sthitaṃ name || 4 ||",
    "838:9": "oḍiyānātsamudbhūtaṃ kaṃkālanāma saṃvaram |",
    "838:10": "prabhāvatīsamāpannaṃ savyakarṇasthitaṃ name || 5 ||",
    "838:11": "arbudapīṭhamadhyasthaṃ saṃvaraṃ vikaṭadaṃṣṭriṇam |",
    "838:12": "mahānāsāsamāpannaṃ pṛṣṭhadeśe sthitaṃ name || 6 ||",
    "838:13": "godāvarīsamudbhūtaṃ surāvīriṇasaṃvaram |",
    "838:14": "saṃpuṭaṃ vīramatyākhyā vāmakarṇasthitaṃ name || 7 ||",
    "838:15": "rāmeśvarodbhavaṃ vīraṃ amitābhākhyasaṃvaram |",
    "838:16": "kharvarosaṃpuṭaṃ nāthaṃ bhruvormadhyasthitaṃ name || 8 ||",
    "838:17": "devīkoṭodbhavaṃ nāthaṃ vajraprabhākhyasaṃvaram |",
    "838:18": "laṅkeśvarīsamāpannaṃ namāmi netrakoṇagam || 9 ||",
    "838:19": "māravākhyātsamutpannaṃ vajradehākhyasaṃvaram |",
    "838:20": "drumacchāyāsamāpannaṃ namāmi skandhadeśagam || 10 ||",
    "838:21": "kāmarūpodbhavaṃ vīraṃ aṅgulikākhyasaṃvaram |",
    "838:22": "airāvatīsamāpannaṃ namāmi kakṣadeśagam || 11 ||",
    "838:23": "okārasthānasaṃjātaṃ vajrajaṭilasaṃvaram |",
    "838:24": "bhairavāliṅgaṇaṃ vīraṃ tanuyugodbhavaṃ name || 12 ||",
    "838:25": "triśakunyadbhavaṃ nāthaṃ mahāvīrākhyasaṃvaram |",
    "838:26": "vāyuvegāsamāpannaṃ nābhideśasthitaṃ name || 13 ||",
    "838:27": "kauśalāyāṃ samudbhūtaṃ vajrahūkārasaṃvaram |",
    "838:28": "surābhakṣīsamāpannaṃ namāmi nāsikāgrajam || 14 ||",
    "838:29": "kaliṅgapīṭhasaṃjātaṃ suprabhānāma saṃvaram |",
    "838:30": "śyāmādevī samāpannaṃ namāmi mukhabhāgajam || 15 ||",
    "838:31": "lampākasthānasaṃbhūtaṃ vajraprabhākhyasaṃvaram |",
    "838:32": "subhadrāliṅgaṇaṃ vīraṃ kaṇṭhadeśodbhavaṃ name || 16 ||",
    "838:33": "kāñcyākhyapīṭhasaṃjātaṃ mahābhairavasaṃvaram |",
    "838:34": "hayakarṇāsamāpannaṃ namāmi hṛdimadhyagam || 17 ||",
    "838:35": "himālayodbhavaṃ vīraṃ virūpākṣākhyasaṃvaram |",
    "838:36": "khagānanāsamāpannaṃ meḍhrasthānagataṃ name || 18 ||",
    "838:37": "pretapuryāṃ samudbhūtaṃ mahābalākhyasaṃvaram |",
    "838:38": "cakravegāsamāpannaṃ liṅgasthānagataṃ name || 19 ||",
    "838:39": "guhadeśātsamudbhūtaṃ ratnavajrākhyasaṃvaram |",
    "838:40": "khaṇḍarohāsamāpannaṃ namāmi gudamadhyagam || 20 ||",
    "838:41": "saurāṣṭradeśasaṃbhūtaṃ hayagrīvākhyasaṃvaram |",
    "838:42": "śauṇḍinyāliṅgaṇaṃ vīraṃ namāmi urumadhyagam || 21 ||",
    "838:43": "suvarṇadvīpasaṃjātam ākāśagarbhasaṃvaram |",
    "838:44": "cakravarmiṇīsamāpannaṃ jaṅghāmadhyagataṃ name || 22 ||",
    "838:45": "nagarapīṭhamadhyasthaṃ śrīherukākhyasaṃvaram |",
    "838:46": "suvīrāliṅgaṇaṃ vīraṃ namāmyaṅgulivartinam || 23 ||",
    "838:47": "sindhudeśasamudbhūtaṃ padmanṛtyākhyasaṃvaram |",
    "838:48": "mahābalāsamāpannaṃ pādapṛṣṭhagataṃ name || 24 ||",
    "838:49": "marutīpīṭhasaṃjātaṃ vairocanākhyasaṃvaram |",
    "838:50": "cakravartinīsamāpannaṃ namāmyaṅguṣṭhamadhyagam || 25 ||",
    "838:51": "kulatāyāḥ samudbhūtaṃ vajrasattvākhyasaṃvaram |",
    "838:52": "mahāvīryāsamāpannaṃ jānudvayagata name || 26 ||",
    "838:53": "pūrvadvāre sthitaṃ vīraṃ vajracaṇḍākhyasaṃvaram |",
    "838:54": "kākāsyāliṅgaṇaṃ vīraṃ namāmi suranāyakam || 27 ||",
    "838:55": "uttaradvāramadhyasthaṃ vajrānalākhyasaṃvaram |",
    "838:56": "ulūkāsyāsamāpannaṃ namāmi yakṣanāyakam || 28 ||",
    "838:57": "paścimadvāramadhyasthaṃ vajroṣṇīṣākhyasaṃvaram |",
    "838:58": "śvānāsyāsampuṭaṃ vīraṃ namāmi pannagādhipam || 29 ||",
    "838:59": "dakṣiṇadvāramadhyasthaṃ vajrakuṇḍalisaṃvaram |",
    "838:60": "śūkarāsyāsamāpannaṃ namāmi yamanāyakam || 30 ||",
    "838:61": "āgneyadigvimātrasthaṃ vajrayakṣākhyasaṃvaram |",
    "838:62": "yamadāḍhīsamāpannaṃ namāmi vahnināyakam || 31 ||",
    "838:63": "nairṛtyadigvibhāṣasthaṃ vajrakīlākhyasaṃvaram |",
    "838:64": "yamadūtīsamāpannaṃ namāmi rākṣasādhipam || 32 ||",
    "838:65": "vāyavyadigvibhāgasthaṃ vajramahābalāhvayam |",
    "838:66": "yamadaṃṣṭrīsamāpannaṃ namāmi pavanādhipam || 33 ||",
    "838:67": "īśānadigvibhāgasthaṃ vajrabhīṣaṇasaṃvaram |",
    "838:68": "yamamathanīsamāpannaṃ namāmi bhūtanāyakam || 34 ||",
    "838:69": "etāndevān namasyāmi digvidikṣu susaṃsthitān |",
    "838:70": "vīrān vīreśvarīḥ sarvā herukaṃ parameśvaram || 35 ||",
    "838:71": "sahajānandātmakaṃ devaṃ viśuddhaṃ tāṇḍavānvitam |",
    "838:72": "ḍākinījālamadhyasthaṃ pratyātmavedyagocaram || 36 ||",
    "838:73": "stutvedaṃ devatīcakraṃ yanmayopārjitaṃ śubham |",
    "838:74": "tena puṇyena loko'stu vajraḍākaparāyaṇaḥ || 37 ||",
    "838:75": "śrī ṣaṭtriṃśatsaṃvaragaṇacakramaṇḍalastutiḥ samāptāḥ ||"
}