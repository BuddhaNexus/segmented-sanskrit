{
  "SA20n1337:0": "tattvaparicchedastṛtīyaḥ",
  "SA20n1337:1": "mūlalakṣaṇatattvamaviparyāsalakṣaṇam |",
  "SA20n1337:2": "phalahetumayaṃ tattvaṃ sūkṣmaudārikameva ca ||1 ||",
  "SA20n1337:3": "prasiddhaṃ śuddhiviṣayaṃ saṅgrāhyaṃ bhedalakṣaṇam |",
  "SA20n1337:4": "kauśalyatattvaṃ daśadhā ātmadṛṣṭivipakṣataḥ ||2 ||",
  "SA20n1337:5": "svabhāvastrividho'sacca nityaṃ saccā'pyatattvataḥ |",
  "SA20n1337:6": "sadasattattvataśceti svabhāvatrayamiṣyate ||3 ||",
  "SA20n1337:7": "samāropā'pavādasya dharmapudgalayoriha |",
  "SA20n1337:8": "grāhyagrāhakayoścāpi bhāvā'bhāve ca darśanam ||4 ||",
  "SA20n1337:9": "yajjñānānna pravarteta taddhi tattvasya lakṣaṇam |",
  "SA20n1337:10": "asadartho hyanityārtha utpādadavyayalakṣaṇaḥ ||5 ||",
  "SA20n1337:11": "samalā'malabhāvena mūlatattve yathākramam |",
  "SA20n1337:12": "duḥkhamādānalakṣmākhyaṃ sambandhenā'paraṃ matam ||6 ||",
  "SA20n1337:13": "abhāvaścāpyatadbhāvaḥ prakṛtiḥ śūnyatā matā |",
  "SA20n1337:14": "alakṣaṇañca nairātmyaṃ tad-vilakṣaṇameva ca ||7 ||",
  "SA20n1337:15": "svalakṣaṇañca nirdiṣṭaṃ duḥkhasatyamato matam |",
  "SA20n1337:16": "vāsanā'tha samutthānamavisaṃyoga eva ca ||8 ||",
  "SA20n1337:17": "svabhāvadvayanotpattirmalaśāntidvayaṃ matam |",
  "SA20n1337:18": "parijñāyāṃ prahāṇe ca prāptisākṣātkṛtāvayam ||9 ||",
  "SA20n1337:19": "mārgasatyaṃ samākhyātaṃ prajñaptipratipattitaḥ |",
  "SA20n1337:20": "tathodbhāvanayaudāraṃ paramārthantu ekataḥ ||10 ||",
  "SA20n1337:21": "arthaprāptiprapattyā hi paramārthastridhā mataḥ |",
  "SA20n1337:22": "nirvikārā'viparyāsapariniṣpattito dvayam ||11 ||",
  "SA20n1337:23": "lokaprasiddhamekasmāt trayād yuktiprasiddhakam |",
  "SA20n1337:24": "viśuddhagocaraṃ dvedhā ekasmādeva kīrtitam ||12 ||",
  "SA20n1337:25": "nimittasya vikalpasya nāmnaśca dvayasaṅgrahaḥ |",
  "SA20n1337:26": "samyagjñānasatattvasya ekenaiva ca saṅgrahaḥ ||13 ||",
  "SA20n1337:27": "pravṛttitattvaṃ dvividhaṃ sanniveśakupannatā |",
  "SA20n1337:28": "ekaṃ lakṣaṇavijñaptiśuddhisamyakprapannatā ||14 ||",
  "SA20n1337:29": "ekahetutvabhoktṛtvakartṛtvavaśavartane |",
  "SA20n1337:30": "ādhipatyārthanityatve kleśaśuddhyāśraye'pi ca ||15 ||",
  "SA20n1337:31": "yogitvā'muktamuktatve ātmadarśanameṣu hi |",
  "SA20n1337:32": "parikalpavikalpārthadharmatārthena teṣu te ||16 ||",
  "SA20n1337:33": "anekatvā'bhisaṅkṣepaparicchedārtha āditaḥ |",
  "SA20n1337:34": "grāhakagrāhyatadgrāhabījārthaścā'paro mataḥ ||17 ||",
  "SA20n1337:35": "veditārthaparicchedabhogāyadvārato'param |",
  "SA20n1337:36": "punarhetuphalāyāsānāropā'napavādataḥ ||18 ||",
  "SA20n1337:37": "aniṣṭeṣṭaviśuddhīnāṃ samotpattyādhipatyayoḥ |",
  "SA20n1337:38": "samprāptisamudācārapāratantryārthato'param ||19 ||",
  "SA20n1337:39": "grahaṇasthānasandhānabhogaśuddhidvayārthataḥ |",
  "SA20n1337:40": "phalahetūpayogārthanopayogāttathā'param ||20 ||",
  "SA20n1337:41": "vedanāsanimittārthatannimittaprapattitaḥ |",
  "SA20n1337:42": "tacchamapratipakṣārthayogādaparamiṣyate ||21 ||",
  "SA20n1337:43": "guṇadoṣā'vikalpena jñānena parataḥ svayam |",
  "SA20n1337:44": "niryāṇādaparaṃ jñeyaṃ saprajñaptisahetukāt ||22 ||",
  "SA20n1337:45": "nimittāt praśamāt sārthāt paścimaṃ samudāhṛtam ||",
  "SA20n1337:46": " ||iti tattvaparicchedastṛtīyaḥ ||"
}