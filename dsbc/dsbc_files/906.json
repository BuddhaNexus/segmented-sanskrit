{
    "906:0": "vajravilāsinīsādhanāstavaḥ",
    "906:1": "ravivikasvarabhāsvarasundarī-",
    "906:2": "prabalakeśarasaṃvarakarṇikāt |",
    "906:3": "udayamastakaśṛṅgasamullasa-",
    "906:4": "nnavasupūrṇaśaśāṅkakarāṅkitām || 1 ||",
    "906:5": "vividhapuṣparasāsavavedyapi",
    "906:6": "drutavilambitamadhyanadaddhvaniḥ |",
    "906:7": "alirupetya niśārdhamahotsave",
    "906:8": "dhayati tadgatadhīriti vismayaḥ || 2 ||",
    "906:9": "udayate'stamupaiti niśākaraḥ",
    "906:10": "punarato'pyudayācalamaulitām |",
    "906:11": "ubhayakoṭikalāpinirīkṣaṇe",
    "906:12": "na ca śaśī na vibhāti divākaraḥ || 3 ||",
    "906:13": "ravirudeti sarojadalāntare",
    "906:14": "śikharato'pi vidhuḥ pravilīyate |",
    "906:15": "vrajati cordhvamasau vaḍavānalo",
    "906:16": "gilati rāhuradhaḥ śaśibhāskarau || 4 ||",
    "906:17": "kamalinīkamalāsanalocana-",
    "906:18": "prakaṭagocaragocaramīlanam |",
    "906:19": "prathamamaṅgamidaṃ kuliśāmbuja-",
    "906:20": "dvayanimīlanamatra tato'param || 5 ||",
    "906:21": "sapadi sādhakabījanigharṣaṇāt",
    "906:22": "tritayamambarameti caturthakam |",
    "906:23": "sahajamakṣayadhāmakalāvalī-",
    "906:24": "kalitakālavilāpanaleliham || 6 ||",
    "906:25": "yadi suśikṣitavajravilāsinīgurumukhādhigamo'dhigato bhavet |",
    "906:26": "bhavati yasya tadottamamadhyamādhamaviniścayameti sa tattvadṛk || 7 ||",
    "906:27": "śrīvajravilāsinīsādhanāstavaḥ samāptaḥ |"
}