{
    "1143:0": "śrīsuvrahmaṇyadīkṣivena viracitaṃ nṛtyanāṭakama",
    "1143:1": "nṛtyaprasūtiḥ",
    "1143:2": "nāndī",
    "1143:3": "gaula śrādi",
    "1143:4": "pa mārajitaṃ bhaja manasā madhuragauṇaṃ sarvadā",
    "1143:5": "śra kāruṇam śrāśutoṣam kāmāriṃ karidamanakaram",
    "1143:6": "ca dāruṇābhavasāgaratāraṇaṃ taruṇāruṇacaraṇayugāanam",
    "1143:7": "yogināṃ cakravartinaṃ dhammacakrapravartinaṃ",
    "1143:8": "vṛkṣamūlāsīnaṃ śiṣyāvṛtaṃ jñānaprasūnam",
    "1143:9": "prathamaṃ dṛśyanam",
    "1143:10": "devarājo samarasanmnddho śrāyāti",
    "1143:11": "haṃsadhvaniḥ śrādi",
    "1143:12": "bhīkara vajrāyudhadharo",
    "1143:13": "śrīkarāśaśīpatirasau",
    "1143:14": "śravṛttaraśatapurabhāśotsuka vīro devarājo śrāthātyadhunā",
    "1143:15": "kiṃcitkālānantaraṃ vijayībhūtī indraḥ vijayanṛttaṃ nṛtyati",
    "1143:16": "vijayībhūto'ham vijayībhūto'ham",
    "1143:17": "tataḥ sahasaiva śrabaṃrāt tripurāsurāḥ vividhāyudharvarṣaiḥ devān trāsayanti |",
    "1143:18": "bhīvabhītāḥ",
    "1143:19": "devāḥ parameśvaraṃ prārthayanti |",
    "1143:20": "darbāra śrādi",
    "1143:21": "bhayaṃkarātripurarūpaiḥ bādhitā vayaṃ bhītyā svayaṃbhuvaṃ śaṃbhunāthaṃ śivaṃ param śraghoraṃ",
    "1143:22": "raktavantastīvrabhakatyā baddhāñlihastāḥ śrava bhava śiveti śrāhvaya prārthayāmaḥ",
    "1143:23": "nāṭayaraṃgī nimiṣamekam timirāyati,",
    "1143:24": "tataḥ devāḥ śivāgamanaṃ vīkṣya gāyanti |",
    "1143:25": "nāṭakaruṃji śrādi",
    "1143:26": "pa śrāyati maheśvaro śrabhitojasā bhāsvaro",
    "1143:27": "nirāyudhakarayugo samāvṛttamaragaṇo",
    "1143:28": "ca 1 sākrosaṃ ghoṣatmiḥ sarvaśatrunāśaṃ",
    "1143:29": "vākdevavaikuṣṭādibhiḥ prārthito ittham",
    "1143:30": "śīghram śrasmān śāstrīya deva deva",
    "1143:31": "virāgragaṇya sayojayādhunā",
    "1143:32": "sarvadaivāḥ gāyanti",
    "1143:33": "ca 2 samaravijayasannaddhāḥ śraniruddhāḥ śratisamarthāḥ śrāśritāstrayaṃbake vayam",
    "1143:34": "ca 3 karaghṛtāśugo'haṃ khaṇḍanacaturo'ham",
    "1143:35": "devaḥ 1 niratiśaya javasahito nirmūlakaracaturonirghṛṇartīkṣṇatamo'smyaham",
    "1143:36": "ca 4 nirupamaśakharṣaṇotsuko'smyaham",
    "1143:37": "śranupamanagajābrūnibhakuṃcito",
    "1143:38": "dhanuvaravinākanāmāhaṃ dhanvanāmadhirājabhṛto",
    "1143:39": "dānavabhīṣaṇakaraghoṣajyāmaṇḍitapracaṇḍo'ham",
    "1143:40": "ca 5 vajrasāraśulāyudho'haṃ vairipurnibhedanacaturo",
    "1143:41": "vācāmagocaravegeṇa vairivaśayaṃ kāraye",
    "1143:42": "tataḥ sarve devāḥ parameśvara pārvatī ca pradakṣiṇaṃ kurvanti “ samaravijayasammaddhāḥ“",
    "1143:43": "itigāyantaḥ",
    "1143:44": "pantuvarālī śrādi",
    "1143:45": "dṛṣṭā mamatābhṛtaṃ devagaṇa śratvā pratāpaprakaṭīkaraṇaṃ",
    "1143:46": "matvā mabnasi tripuradahanaṃ mamattāharaṇa yugapada devanām",
    "1143:47": "smitānanaṃ himavatsutāyāh jitandriyo śaṃbhukhalokya",
    "1143:48": "mandāhāsāṃkitaṃ suṃdarāsyaṃ candramaulikarvidārito",
    "1143:49": "śrānandaraudrarasāṃkita mahāhāsaṃ sahasākarot",
    "1143:50": "duṃdubhiśatakoṭisvanādiva (tripura)",
    "1143:51": "nāśam śrabhūta tatkṣaṇe |",
    "1143:52": "dvitīyaṃ dṛśyam",
    "1143:53": "rkicitkālānantara kailāse pradoṣasamaye,",
    "1143:54": "sarve devāḥ ḥ saṃgatāḥ parameśvarasya nṛttaṃ draṣṭum |",
    "1143:55": "śaṃbhu nṛttaṃ kartuṃ mārayati |",
    "1143:56": "nītimati śrādi",
    "1143:57": "pa pradoṣasamaye nṛttaparaṃ parameśvaraṃ stuvantyamarāḥ",
    "1143:58": "śra vrataṃ dhṛtaṃ natāvane vilaṃbite hate same",
    "1143:59": "ca tūṃburuvīṇānandimṛdaṃgavādanairutsāhapūrṇam",
    "1143:60": "śaṃbhumavalokya sasmitam śraṃbujānanā haimavatī",
    "1143:61": "kuṃbakucayugalāmṛduvācā yācate nārade munis",
    "1143:62": "biṃbāyitagītāmṛtātra gīyatāṃ tripurāntakathā",
    "1143:63": "nāradamunistadā tripurāntakathāṃ gāyati |",
    "1143:64": "kābodi",
    "1143:65": "bhayaṃkaravarakrūrāmaranāśakaraparatripurāsurendraiḥ bādhito bhītabhīto purandharo viṣṇubraṃhādibhiḥ sakaladevagaṇeḥ saha tryaṃbakaṃ śaraṇaṃgato pālaya kṛpālaya parameśvareti yācate,",
    "1143:66": "kṛpātaraṃgittāntaraṃgaḥ śaṃkaro'bhayaṃkaro nirāyadhastadaiva tripurāntakaparo bhavati |",
    "1143:67": "taṃ prārthayanti devāḥ śāstrīyatāmasmāniti |",
    "1143:68": "voḍī devaiko dhanurbhavāmīti śranyo bhavāmi sāyakamiti",
    "1143:69": "śranyataro bhavāmi triśūlamiti uccaiḥ ghoṣayantaḥ",
    "1143:70": "bhūtavijayina iva nṛtyanti |",
    "1143:71": "tān dṛṣṭavā",
    "1143:72": "karuṇārasāṃkitahāsyarasotpattyā codito",
    "1143:73": "maheśvaro uccaiḥ mahāhāsaṃ karoti,",
    "1143:74": "tatkṣaṇaiva",
    "1143:75": "bhūta tripuradahanaṃ vīkṣya vismayātpullalocanāḥ",
    "1143:76": "devāḥ hāhākāraṃ kurvanti prastuvanti ca |",
    "1143:77": "etatsarvamabhinayaiḥ darśayati śaṃbhuḥ",
    "1143:78": "gītasyānte śakaro,",
    "1143:79": "ghoṣati sotsāha",
    "1143:80": "sṛṣṭaṃ nava śāstramidamabhinayanāmāṃkitamiti |",
    "1143:81": "śaṃbhustadā taṇḍumāhvava gāyati nṛtyati ca",
    "1143:82": "navarasakannḍa śrādi",
    "1143:83": "pa śrṛṇu me mahāmuni sṛṣṭaṃ navaṃ śāstramidam",
    "1143:84": "śrabhinayaiti nāmāṃkitaṃ pracārayācāryaiḥ",
    "1143:85": "śra bhaṇitaṃ bhaveta nṛtyaṃ nṛttamabhinayākitaṃ",
    "1143:86": "locanadvayamivaidaṃ pālaya navarasāṃkitam |",
    "1143:87": "ca gaṇītaṃ pauruṣāṃkitaṃ tāṇḍavaṃ bhavet",
    "1143:88": "dharaṇyāṃ tattaṇḍunā śikṣāhaṃ bhavet",
    "1143:89": "bhaṇiva strībhāvanṛtyaṃ lāsyanāmāṃkitaṃ",
    "1143:90": "vanitābhyāṃ pārvatyā śrabhayāsito bhavet |",
    "1143:91": "tadā sarve nṛtyanti",
    "1143:92": "madhyamāvatī rūpakam",
    "1143:93": "pa dhurjaṭiṃ śaṃbhu nāyaṃ dūrikṛtakhiladuritasamūhaṃ",
    "1143:94": "śrarcāmḥ nṛttairabhinayaiḥ sānanda lāsya tāṇḍavābhyām",
    "1143:95": "śra sūlādisaptatālāṃṅkita kolāhalannṛttairiha",
    "1143:96": "śūlāyughadhaṭaśaṃbhu parvatabālāṃ ca",
    "1143:97": "kiṇkirṇāmālābhirārā dhayāmovayam",
    "1143:98": "ca 1 śrāśritāvane karuṇāṃma śratiśāntaṃ yogamejjana",
    "1143:99": "śrasuranāśane vīraraudrādibhāvān darśayanta",
    "1143:100": "śravanatāṃgīṃpūrṇa śrṛṃgāraṃ kāśayantīma",
    "1143:101": "bhavāliṃgita madhyamāvatīṃ bhavaṃcāpi vaṃdāmahe",
    "1143:102": "ca 2 gaṃgādharaṃ śaṃkābhṛtaurvācakainindantīṃ",
    "1143:103": "kalahāṃnvarita khaṃḍitādikānāyikānnaṭantīṃ",
    "1143:104": "tuṃgasvanabharacadanasikāṃ dvāre vāsakasajjikāṃ",
    "1143:105": "śaṃkaram unmādayantīṃ svādhīnapatikāṃ bhaje |"
}