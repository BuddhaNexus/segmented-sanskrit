{
  "SA05n901:0": "māravijayastotram",
  "SA05n901:1": "jitamārakaliṃ jagadekaguruṃ",
  "SA05n901:2": "surayakṣamahoragadaityanatam |",
  "SA05n901:3": "catusatyasudeśitamārgamimaṃ",
  "SA05n901:4": "guṇameghamahaṃ praṇamāmi sadā || 1 ||",
  "SA05n901:5": "bhavabhīmamahodadhimadhyagataṃ",
  "SA05n901:6": "kṛpaṇārtaravaṃ samavekṣya janam |",
  "SA05n901:7": "parimocayituṃ ya iha praṇidhiṃ",
  "SA05n901:8": "pracakāra vibhuṃ tamahaṃ praṇataḥ || 2 ||",
  "SA05n901:9": "girirājanibhaṃ śaradindudhiyaṃ",
  "SA05n901:10": "sumukhaṃ subhujaṃ vararūpadharam |",
  "SA05n901:11": "sugataṃ gatamapratimaṃ sugataṃ",
  "SA05n901:12": "praṇato'smi sadā jagadarthakaram || 3 ||",
  "SA05n901:13": "kanakaprabhayā paripītatanuṃ",
  "SA05n901:14": "varadundubhitoyadavalgurutam |",
  "SA05n901:15": "gajahaṃsavilambitadhīragataṃ",
  "SA05n901:16": "śirasā'bhinato'smi guṇaikanidhim || 4 ||",
  "SA05n901:17": "taruṇārkasamairacalairnayanaiḥ",
  "SA05n901:18": "sphuradugramahāśanibhīmaravaiḥ |",
  "SA05n901:19": "namucipraharairasiśaktidharai-",
  "SA05n901:20": "ścalito'si vibho na hi tvaṃ vikṛtaiḥ || 5 ||",
  "SA05n901:21": "lavalīphalapāṇḍurakarṇapuṭāḥ",
  "SA05n901:22": "kucabhāravināmitagātralatāḥ |",
  "SA05n901:23": "nahi tvāmaśakan khalu nāgasutāḥ",
  "SA05n901:24": "śvasitaiḥ skhalitairapi kampayitum || 6 ||",
  "SA05n901:25": "taravaḥ kusumastabakābharaṇā",
  "SA05n901:26": "bahuratnasahasracitāśca nagāḥ |",
  "SA05n901:27": "na tathābhiratiṃ janayanti satāṃ",
  "SA05n901:28": "janayanti yathā tava vīraguṇāḥ || 7 ||",
  "SA05n901:29": "sphuṭacitrapadaṃ bahuyuktiyutaṃ",
  "SA05n901:30": "gamakaṃ vacanaṃ tava karṇasukham |",
  "SA05n901:31": "śubhamārgaphalaṃ prasamīkṣyajanā",
  "SA05n901:32": "na gṛṇanti punastvadṛte'nyavacaḥ || 8 ||",
  "SA05n901:33": "iti vaḥ śaraṇaṃ samavekṣya janā",
  "SA05n901:34": "na patantyapi kalpaśatairniraye |",
  "SA05n901:35": "vinihatya ca doṣaripūn bahulān",
  "SA05n901:36": "pariyanti śubhaṃ varamokṣapuram || 9 ||",
  "SA05n901:37": "tava saumyatayā'pyabhibhūtatamo",
  "SA05n901:38": "na virājati śītakaro gagane |",
  "SA05n901:39": "tava kāñcanakuṅkumasaprabhayā",
  "SA05n901:40": "prabhayā'bhihato na vibhāti raviḥ || 10 ||",
  "SA05n901:41": "tava nātha śubhe vadanāmburūhe",
  "SA05n901:42": "nayanabhramarā nipatanti nṛṇām |",
  "SA05n901:43": "pratibuddhadale kamale vimale",
  "SA05n901:44": "bhramarā iva puṣpaśatākulitāḥ || 11 ||",
  "SA05n901:45": "iti toṭakamantravarairatulaiḥ",
  "SA05n901:46": "parikīrtya mayā tava varṇalavān |",
  "SA05n901:47": "yadupārjitamadya śubhaṃ vipulaṃ",
  "SA05n901:48": "śivamastu tato bhuvi devanṛṇām || 12 ||",
  "SA05n901:49": "śrīmāravijayastotraṃ samāptam ||"
}