{
  "SA05n816:0": "avalokiteśvarastotram",
  "SA05n816:1": "vāsukināgarājakṛtam",
  "SA05n816:2": "om namo'valokiteśvarāya",
  "SA05n816:3": "jaṭādharaṃ saumyaviśālalocanaṃ",
  "SA05n816:4": "sadāprasannānanacandramaṇḍalam |",
  "SA05n816:5": "surāsurairvanditapādapaṅkajaṃ",
  "SA05n816:6": "namāmi nāthaṃ maṇipadmasaṃbhavam ||1 ||",
  "SA05n816:7": "sarojapatrāyatadivyalocanaṃ",
  "SA05n816:8": "kudṛṣṭisaṃśodhitaśuddhalocanam |",
  "SA05n816:9": "kṛpāmṛtārdraṃ jagadekalocanaṃ",
  "SA05n816:10": "namāmi nāthaṃ maṇipadmasaṃbhavam ||2 ||",
  "SA05n816:11": "hārenduhārārdhahimādhikojjavalaṃ",
  "SA05n816:12": "nighṛṣṭagaṇḍāmalalolakuṇḍalam |",
  "SA05n816:13": "gabhastimālākulakoṭisaṃkulaṃ",
  "SA05n816:14": "namāmi nāthaṃ maṇipadmasaṃbhavam ||3 ||",
  "SA05n816:15": "prabuddhadharmādhvani dharmadhātukaṃ",
  "SA05n816:16": "sahasrabāhuṃ dvicatuśca ṣaḍbhujam |",
  "SA05n816:17": "khadhātunā tulyamanantabāhukaṃ",
  "SA05n816:18": "namāmi nāthaṃ maṇipadmasaṃbhavam ||4 ||",
  "SA05n816:19": "upāyaprajñodadhimanthanodbhavaṃ",
  "SA05n816:20": "tridhātusaṃrakṣaṇahetusaṃbhavam |",
  "SA05n816:21": "jinendramauliṃ jinadhātusaṃbhavaṃ",
  "SA05n816:22": "namāmi nāthaṃ maṇipadmasaṃbhavam ||5 ||",
  "SA05n816:23": "padmopari gataṃ nāthaṃ padmahastaṃ jaṭādharam |",
  "SA05n816:24": "āryāvalokitaṃ vande sarvasattvānukampakam ||6 ||",
  "SA05n816:25": "śrīvāsukināgarājakṛtamāryāvalokiteśvarastotraṃ samāptam |"
}