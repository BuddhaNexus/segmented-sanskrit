{
    "302:0": "6 abhimukhī nāma ṣaṣṭhī bhūmiḥ |",
    "302:1": "upakramagāthāḥ |",
    "302:2": "caraṇavara śruṇitvā bhūmiśreṣṭhaṃ vidūnāṃ",
    "302:3": "gagani sugataputrā harṣitāḥ puṣpavarṣī |",
    "302:4": "maṇiratana udārā ābhayuktā viśuddhā",
    "302:5": "abhikira sugatasya sādhviti vyāharantaḥ || 1 ||",
    "302:6": "maruta śatasahasrā harṣitā antarīkṣe",
    "302:7": "diviya rucira citrā ratnacūrṇā udārāḥ |",
    "302:8": "abhikira sugatebhyo gandhamālyānulepān",
    "302:9": "chatradhvajāpatākāhāracandrārdhahārān || 2 ||",
    "302:10": "marupati vaśavartī sarvadevagaṇena",
    "302:11": "upari khaga paṭhitvā megha ratnāmayāni |",
    "302:12": "abhikiriṣu prasannaḥ pūjanārthaṃ jinasya",
    "302:13": "sādhu sugataputrā vyāharī hṛṣṭacittāḥ || 3 ||",
    "302:14": "amaravadhusahasrāṇyantarīkṣe sthitāni",
    "302:15": "gīta ruta manojñā vādyasaṃgītiyuktā |",
    "302:16": "sarvarutasvarebhyo eva śabdā ravante",
    "302:17": "jina kṛtu sumanojñaiḥ kleśatāpasya hantā || 4 ||",
    "302:18": "śūnya prakṛtiśāntā sarvadharmānimittāḥ",
    "302:19": "khagapathasamatulyā nirvikalpā viśuddhāḥ |",
    "302:20": "gatisthitivinirvṛttā niṣprapañcā aśeṣā",
    "302:21": "tathatasama tathatvāddharmatā nirvikalpā || 5 ||",
    "302:22": "yaiḥ punaranubuddhāḥ sarvadharmeva teṣāṃ",
    "302:23": "bhāvi tatha abhāve iñjanā nāsti kācit |",
    "302:24": "kṛpa karuṇa jage ca mocanārthaṃ prayuktā-",
    "302:25": "ste hi sugataputrā aurasā dharmajātāḥ || 6 ||",
    "302:26": "dānacari carante sarva hitvā nimittaṃ",
    "302:27": "śīlasudhṛtacittāṃ ādiśāntā praśāntāḥ |",
    "302:28": "jagati kṛta kṣamante akṣayā dharmajñānī",
    "302:29": "viriyabalaupetāḥ sarvadharmāviviktāḥ || 7 ||",
    "302:30": "dhyānanayapraviṣṭā jīrṇakleśā viśuddhāḥ",
    "302:31": "sarvaviditavastū ādiśūnyādhimuktāḥ |",
    "302:32": "jñānakriyabalāḍhyā nityayuktā jagārthaṃ",
    "302:33": "te hi sugataputrāḥ śāntapāpā mahātmāḥ || 8 ||",
    "302:34": "īdṛśā rutasahasra bhaṇitvā",
    "302:35": "khe sthitāḥ sumadhurā surakanyāḥ |",
    "302:36": "tūṣṇibhūta jinamīkṣi prasannā",
    "302:37": "dharmagauravaratā marukanyāḥ || 9 ||",
    "302:38": "vimukticandra abravīdvajragarbhaṃ viśāradam |",
    "302:39": "kīdṛśākāraniṣpattiḥ pañcamāyāmanantaram || 10 ||",
    "302:40": "upasaṃhāragāthāḥ |",
    "302:41": "paripūrṇamārgacaraṇā vidu pañcamāyāṃ",
    "302:42": "dharmānimittata alakṣaṇatā ajātā |",
    "302:43": "anutpāda ādipariśuddhyatiniṣprapañcā",
    "302:44": "bhāvetva jñānamati ṣaṣṭhi samākramanti || 11 ||",
    "302:45": "dharmā vivikta apratigraha nirvikalpā",
    "302:46": "māyāsvabhāva dvayabhāvatu viprayuktā |",
    "302:47": "anulomayanta avilomanta dharmanetrī",
    "302:48": "jñānānvitāḥ pravara ṣaṣṭhi samākramanti || 12 ||",
    "302:49": "tīkṣṇānulomasthita jñānabalopapetāḥ",
    "302:50": "samudāgamaṃ vibhavu prekṣiṣu sarvaloke |",
    "302:51": "mohāndhakāraprabhavaṃ jagasaṃbhavātmā",
    "302:52": "tasyaiva mohavigamena pravṛti nāsti || 13 ||",
    "302:53": "vicinanti pratyayakṛtiṃ paramārthaśūnyāṃ",
    "302:54": "kriya hetupratyayasamajña kriyāvirodhau |",
    "302:55": "yāthāvataḥ karakapetakriyāṃ viditvā",
    "302:56": "vicinanti saṃskṛta ghanābhrasamaṃ nirīham || 14 ||",
    "302:57": "satyeṣu'jñānu paramārthatu sā avidyā",
    "302:58": "karmā ca cetanabalena vibhāgaprāptam |",
    "302:59": "cittaṃ niśritya sahajaṃ puna nāmarūpam",
    "302:60": "evaṃmukhā bhavati yāva dukhasya skandhaḥ || 15 ||",
    "302:61": "te cittamātra ti traidhātukamotaranti",
    "302:62": "api cā bhavāṅga iti dvādaśa ekacitte |",
    "302:63": "saṃrāgu jātu api cittu prabhāvitastu",
    "302:64": "evaṃ ca saṃbhavakṣayaṃ puna cittabhāgam || 16 ||",
    "302:65": "kāryaṃ avidyadvaya kurvati mohabhāve",
    "302:66": "mohebhi hetu vahate puna cetanāyāḥ |",
    "302:67": "evaṃ ca yāva jaradhvaṃsanaskandhabhedam",
    "302:68": "anu sarva duḥkhaprabhavaṃ kṣayataḥ abhāvaḥ || 17 ||",
    "302:69": "ucchedu no bhavati pratyayatāmavidyā",
    "302:70": "nocchedyatāpi kara prahāya saṃnirodham |",
    "302:71": "moho teṣu ca upādānaṃ kleśavartma",
    "302:72": "karma bhavaṃ ca api cetana śeṣa duḥkhā || 18 ||",
    "302:73": "mohaṃ tu āyatana saṃskṛtaduḥkha teṣāṃ",
    "302:74": "sparśaṃ ca vedana sukhādukhatāya duḥkhā |",
    "302:75": "śeṣānamaṅganapariṇāmaduḥkhavṛddhiḥ",
    "302:76": "vyuccheda tasya duḥkhatā na hi ātmamasti || 19 ||",
    "302:77": "adhveṣu pūrvaṃ tamacetanasaṃskṛtasya",
    "302:78": "vijñāna vedana vivartati pratyutpannam",
    "302:79": "aparāntu teṣu prabhavo dukhasaṃbhaveyam",
    "302:80": "āpekṣa cchedu prasaraṃ ca nirīkṣayantaḥ || 20 ||",
    "302:81": "mohasya pratyayatu saṃbhavate vibandhā",
    "302:82": "vinibandhanavyayakṣaye sati pratyayānām |",
    "302:83": "hetośca mūlaprabhavaṃ na tu hetubhedaṃ",
    "302:84": "vyuparīkṣate ca jina jñāna svabhāvaśūnyam || 21 ||",
    "302:85": "anuloma mohaprabhavaṃ ca prabhāvataśca",
    "302:86": "pratilomahetu kṣayato bhava sarvacchedyam |",
    "302:87": "gambhīrapratyayatamasya sato'sataśca",
    "302:88": "vyuparīkṣate daśavidhaṃ aniketabuddhiḥ || 22 ||",
    "302:89": "saṃdhī bhavāṅgatu tathāpi ca karmasthānam",
    "302:90": "avibhāgatastrividhu vartmani pūrvataśca |",
    "302:91": "triyahetu duḥkhavibhavā udaya vyayaṃ ca",
    "302:92": "abhāvato'kṣayata pratyaya ānulomam || 23 ||",
    "302:93": "evaṃ pratītyasamutpāda samotaranti",
    "302:94": "māyopamaṃ vitatha vedakarmāpanītam |",
    "302:95": "svapnopamaṃ ca tathatā pratibhāsa caiva",
    "302:96": "bālāna mohana marīcisamasvabhāvam || 24 ||",
    "302:97": "yā eva bhāvana sa śūnyata paṇḍitānāṃ",
    "302:98": "rati pratyayāna bhavate idamānimittam |",
    "302:99": "jānitva jātu vitathaṃ praṇidhātu nāsti",
    "302:100": "anyatra sattvakṛpayā upapadyanti || 25 ||",
    "302:101": "evaṃ vimokṣamukha bhāvayi te mahātmā",
    "302:102": "kṛpabuddhi bhūya tatha buddhaguṇābhilāṣī |",
    "302:103": "saṃyogasaṃskṛtikṛta vyuparīkṣamāṇo",
    "302:104": "niyatāśayo bhavati naikaguṇopapetaḥ || 26 ||",
    "302:105": "pūrṇā sahasra daśa śūnyataye samādhī",
    "302:106": "tatha ānimittavaradaṃ ca vimokṣa tāyī || 27 ||",
    "302:107": "pralhādayanti jagadāśaya candraābhā",
    "302:108": "vahamānu vāta caturo asaṃhāryaprāptā |",
    "302:109": "atikramya mārapathamābha jinaurasānāṃ",
    "302:110": "praśamenti kleśaparitāpa dukhārditānām || 28 ||",
    "302:111": "iha bhūmideśupagatā marutādhipāste",
    "302:112": "bhontī sunirmita kṛtāvadhimānaghātī |",
    "302:113": "yaṃ caiva ārabhiṣu jñānapathopapetā",
    "302:114": "asaṃhārya śrāvakagatī atikrānta dhīrāḥ || 29 ||",
    "302:115": "ākāṅkṣamāṇu sugatātmaja vīryaprāptāḥ",
    "302:116": "koṭīśatasahasrapūrṇa samādhi labdhāḥ |",
    "302:117": "paśyanti ekakṣaṇi buddha daśaddiśāsu",
    "302:118": "pratapanti sūrya eva madhyagu grīṣmakāle || 30 ||",
    "302:119": "gambhīra durdṛśā sūkṣma durjñeyā jinaśrāvakaiḥ |",
    "302:120": "ṣaṣṭhī bhūmirmahātmānāmākhyātā sugatātmajāḥ || 31 ||"
}