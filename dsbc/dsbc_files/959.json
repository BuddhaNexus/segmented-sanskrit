    "959:1054": "135. trividhā vikalpāḥ|",
    "959:1055": "tadyathā-anusmaraṇavikalpaḥ,",
    "959:1056": "saṃtirana(tīraṇa)",
    "959:1057": "vikalpaḥ, sahajavikalpaśceti||",
    "959:1058": "136. catvāraḥ samādhayaḥ|",
    "959:1059": "tadyathā-śūraṃgamaḥ,",
    "959:1060": "gaganagañjaḥ vimalaprabhaḥ,",
    "959:1061": "siṃhavikrīḍitaśceti|",
    "959:1062": "137. caturdaśāvyākṛtavastūni|",
    "959:1063": "tadyathā-śāśvato lokaḥ,",
    "959:1064": "aśāśvato lokaḥ,",
    "959:1065": "śāśvataścāśāśvataśca,",
    "959:1066": "naiva śāśvato nāśāśvataśca|",
    "959:1067": "antavāllokaḥ,",
    "959:1068": "anantavāllokaḥ,",
    "959:1069": "antavāṃścānantavāllokaśca,",
    "959:1070": "naivāntavānnānantavāṃśca|",
    "959:1071": "bhavati tathāgataḥ paraṃ maraṇāt,",
    "959:1072": "na bhavati tathāgataḥ paraṃ maraṇāt,",
    "959:1073": "bhavati na ca bhavati ca tathāgataḥ paraṃ maraṇāt,",
    "959:1074": "naiva bhavati na bhavati tathāgataḥ paraṃ maraṇāt|",
    "959:1075": "sa jīvastaccharīram,",
    "959:1076": "anyo jīvo'nyaccharīraṃ ceti||",
    "959:1077": "138. trīṇi kuśalamūlāni|",
    "959:1078": "tadyathā-adveṣaḥ,",
    "959:1079": "alobhaḥ,",
    "959:1080": "amohaśceti||",
    "959:1081": "139. etadviparyayāntrīṇyakuśalamūlāni|",
    "959:1082": "tadyathā-lobhaḥ,",
    "959:1083": "mohaḥ,",
    "959:1084": "dveṣaśceti||",
    "959:1085": "140. tisraḥ śikṣāḥ|",
    "959:1086": "tadyathā-adhicittaśikṣā,",
    "959:1087": "adhiśīlaśikṣā,",
    "959:1088": "adhiprajñāśikṣā ceti||",
    "959:1089": "iti nāgārjunapādaviracito'yaṃ dharmasaṃgrahaḥ samāptaḥ||"
}