{
    "826:0": "rūpastavaḥ",
    "826:1": "sarvabhūtamaṇikampita tubhyaṃ",
    "826:2": "kṣāntimeva caritaṃ tava rūpam |",
    "826:3": "sarvarūpavaradivyasurūpaṃ",
    "826:4": "taṃ namāmi daśabalavararūpam || 1 ||",
    "826:5": "bhṛṅgarājaśikhimūrdhni sukeśaṃ",
    "826:6": "kokilābhaśikhidivyasukeśam |",
    "826:7": "snigdhanīlamṛdukuñcitakeśaṃ",
    "826:8": "taṃ namāmi daśabalavarakeśam || 2 ||",
    "826:9": "śaṅkhakundakumudaṃ vimaromaṃ",
    "826:10": "janmaduḥkhavigataṃ vimaromam |",
    "826:11": "rogaśokavimate vimaromaṃ",
    "826:12": "taṃ namāmi daśabalavararomam || 3 ||",
    "826:13": "pūrṇacandradyutiśobhitavaktraṃ",
    "826:14": "tuṅganāsamaṇibhāśubhavaktram |",
    "826:15": "buddhapaṅkajinabhāmaravaktraṃ",
    "826:16": "taṃ namāmi daśabalavaravaktram || 4 ||",
    "826:17": "raśmisahasravicitrasumūrdhni",
    "826:18": "puṣpavarṣaśatapūjitamūrdhni |",
    "826:19": "ratnavarṣaśatapūjitamūrdhni",
    "826:20": "taṃ namāmi daśabalavaramūrdhni || 5 ||",
    "826:21": "netrapatākavilambitaśīrṣaṃ",
    "826:22": "kāñcanachatravaropitaśīrṣam |",
    "826:23": "netravitānasuśobhitaśīrṣaṃ",
    "826:24": "taṃ namāmi daśabalavaraśīrṣam || 6 ||",
    "826:25": "divyāmbaramukuṭaṃ maṇimukuṭaṃ",
    "826:26": "candraprabhāmukuṭāmaṇimukuṭam |",
    "826:27": "āgataśīrṣamakhaṇḍitamukuṭaṃ",
    "826:28": "taṃ namāmi daśabalavaramukuṭam || 7 ||",
    "826:29": "cārudalāyatapaṅkajanetraṃ",
    "826:30": "divyajñānavipulāyatanetram |",
    "826:31": "dhyānamokṣaśubhasaṃskṛtanetraṃ",
    "826:32": "taṃ namāmi daśabalavaranetram || 8 ||",
    "826:33": "śuddhakarṇaśubhaśuddhasukarṇaṃ",
    "826:34": "śuddhaprajñavaraśobhitakarṇam |",
    "826:35": "śuddhajñānavaraśuddhasukarṇaṃ",
    "826:36": "taṃ namāmi daśabalavarakarṇam || 9 ||",
    "826:37": "śubhrakundaśaśipaṅkajadantaṃ",
    "826:38": "tīkṣṇasulakṣaṇanirmaladantam |",
    "826:39": "viśvapañcadaśapañcasudantaṃ",
    "826:40": "taṃ namāmi daśabalavaradantam || 10 ||",
    "826:41": "uccavākyasasurāsurajihvāṃ",
    "826:42": "dharmajñānakṛtapāpadajihvām |",
    "826:43": "gandhadhūpasasurāsurajihvāṃ",
    "826:44": "taṃ namāmi daśabalavarajihvām || 11 ||",
    "826:45": "meghasudundubhināditaghoṣaṃ",
    "826:46": "satyadharmanayasuviditaghoṣam |",
    "826:47": "divyavādiparavāditaghoṣaṃ",
    "826:48": "taṃ namāmi daśabalavaraghoṣam || 12 ||",
    "826:49": "grīvagrīvadaśagrīvasugrīvaṃ",
    "826:50": "kṣāntivīryaṃ tava grīvasugrīvam |",
    "826:51": "hemavarṇamaṇigrīvasugrīvaṃ",
    "826:52": "taṃ namāmi daśabalavaragrīvam || 13 ||",
    "826:53": "siṃhakāyahimakundasukāyaṃ",
    "826:54": "dhyānakāyaguṇasāgarakāyam |",
    "826:55": "aṣṭasulakṣaṇanirmalakāyaṃ",
    "826:56": "taṃ namāmi daśabalavarakāyam || 14 ||",
    "826:57": "śvetaraktaśubhaśobhitavastraṃ",
    "826:58": "nīlapītaharitāyitavastram |",
    "826:59": "hāradhavalaśubhaśobhitavastraṃ",
    "826:60": "taṃ namāmi daśabalavaravastram || 15 ||",
    "826:61": "hemanāgakarasaṃskṛtabāhū",
    "826:62": "puṣpadānakṛtaśobhitabāhū |",
    "826:63": "dhīravīryasumanoharabāhū",
    "826:64": "taṃ (tau) namāmi daśabalavarabāhū || 16 ||",
    "826:65": "cāmaracakrasuśobhitahastaṃ",
    "826:66": "vimalasukomalapaṅkajahastam |",
    "826:67": "bhūtapretaśaraṇāgatahastaṃ",
    "826:68": "taṃ namāmi daśabalavarahastam || 17 ||",
    "826:69": "rāgadveṣatanuvarjitacittaṃ",
    "826:70": "kalpakoṭisamabhāvitacittam |",
    "826:71": "śīlajñānasamabhāvitacittaṃ",
    "826:72": "taṃ namāmi daśabalavaracittam || 18 ||",
    "826:73": "padmanābhaśubhaśobhitanābhaṃ",
    "826:74": "padmayoniśubhaśobhitanābham |",
    "826:75": "brahmajyotisuśobhitanābhaṃ",
    "826:76": "taṃ namāmi daśabalavaranābham || 19 ||",
    "826:77": "cakrasulakṣaṇabhūṣitapādaṃ",
    "826:78": "aṅkuśaśaktivirājitapādam |",
    "826:79": "nāgayakṣagaṇavanditapādaṃ",
    "826:80": "taṃ namāmi daśabalavarapādam || 20 ||",
    "826:81": "bhāskaradyutirājitapādaṃ",
    "826:82": "devadānavagaṇavanditapādam |",
    "826:83": "sarvadevagaṇavanditapādaṃ",
    "826:84": "taṃ namāmi daśabalavarapādam || 21 ||",
    "826:85": "sarvadevagaṇapūjitaśīrṣaṃ",
    "826:86": "sūryakoṭisamabhāvitaśīrṣam |",
    "826:87": "divyajvālajvalitārpitaśīrṣaṃ",
    "826:88": "taṃ namāmi daśabalavaraśīrṣam || 22 ||",
    "826:89": "puṣpadhūpaśatapūjitaśīrṣaṃ",
    "826:90": "dīpagandhaśatapūjitaśīrṣam |",
    "826:91": "mālyavastraśatapūjitaśīrṣaṃ",
    "826:92": "taṃ namāmi daśabalavaraśīrṣam || 23 ||",
    "826:93": "tribhuvanadharmakalābhyuṣṇīṣa-",
    "826:94": "ratnachatramukuṭādipūjitam |",
    "826:95": "ye paṭhanti sugatastavamenaṃ",
    "826:96": "te labhanti śubhamokṣapathādhigamam || 24 ||",
    "826:97": "śrīmadāryāvalokiteśvarabhaṭṭārakasya rūpastavaṃ samāptam |"
}