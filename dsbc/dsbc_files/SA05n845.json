{
  "SA05n845:0": "hāratīstotram",
  "SA05n845:1": "śrī hāratīdevyaiḥ namaḥ",
  "SA05n845:2": "kundaprasūnasadṛśīṃ rucirāṅgarāgāṃ",
  "SA05n845:3": "nānāvibhūṣaṇavibhūṣitavajradehām |",
  "SA05n845:4": "savyena hastakamalena bhayārtihantrīṃ",
  "SA05n845:5": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 1 ||",
  "SA05n845:6": "āliṅgitāṃ nijamasavyabhujena putrān",
  "SA05n845:7": "netraistribhiśca pariśobhitacārudehām |",
  "SA05n845:8": "nānāvidhairmaṇigaṇai racitaṃ kirīṭaṃ",
  "SA05n845:9": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 2 ||",
  "SA05n845:10": "tāṃ vahnimaṇḍalagatāṃ vasupatrapadme",
  "SA05n845:11": "śrīratnapīṭha uparisthitapādapadmām |",
  "SA05n845:12": "yakṣādibhiḥ parivṛtāṃ lalitāsanasthāṃ",
  "SA05n845:13": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 3 ||",
  "SA05n845:14": "lokatrayārtiśamanīṃ śiśujīvadātrīṃ",
  "SA05n845:15": "śrīhāratīṃ karuṇayā nijabhaktadakṣām |",
  "SA05n845:16": "duḥkhāpahāṃ janamanoharabhogabhājāṃ",
  "SA05n845:17": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 4 ||",
  "SA05n845:18": "saṃtrāsinīṃ piśunavṛttivatāṃ janānāṃ",
  "SA05n845:19": "visphoṭakādirujarāśivināśakartrīm |",
  "SA05n845:20": "bhakteṣu dhātṛsadṛśīṃ ca suśītalāṅgīṃ",
  "SA05n845:21": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 5 ||",
  "SA05n845:22": "buddhānuśāsanaratāmatisūkṣmarūpāṃ",
  "SA05n845:23": "lokatrayārcitapadāṃ karuṇārcibhūtām |",
  "SA05n845:24": "gandharvakinnaragaṇaiḥ parisevyamānāṃ",
  "SA05n845:25": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 6 ||",
  "SA05n845:26": "keyūrakuṇḍalasukaṅkaṇanūpurāḍhyāṃ",
  "SA05n845:27": "nānāvicitravasanapradadhānaśīlām |",
  "SA05n845:28": "saṃrakṣaṇīṃ śiśugaṇān paripālayantīṃ",
  "SA05n845:29": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 7 ||",
  "SA05n845:30": "svarbhūrasātalamavasthitabhuktidāna-",
  "SA05n845:31": "saṃsaktapūrṇahṛdayāṃ triguṇātmarūpām |",
  "SA05n845:32": "tāṃ saṃsmarāmyaharahaḥ śaraṇaṃ prapadye",
  "SA05n845:33": "yakṣeśvarīṃ bahusutāṃ praṇamāmi bhaktyā || 8 ||",
  "SA05n845:34": "rakṣākarīṃ stutimimāṃ prapaṭhenmanuṣyaḥ",
  "SA05n845:35": "śrīhāratīṃ nijagṛhe śiśurakṣaṇāya |",
  "SA05n845:36": "vācaṃ śriyaṃ suvipulaṃ ca sukhādi sarvaṃ",
  "SA05n845:37": "dātrī trikaṃ paṭhati yaḥ prayataḥ prabhāte || 9 ||",
  "SA05n845:38": "śrībhavaratnavinirgataṃ bālarakṣākaraṃ hāratīstotraṃ samāptam |"
}