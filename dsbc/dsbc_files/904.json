{
    "904:0": "kalyāṇatriṃśatikāstotram",
    "904:1": "evaṃkārasamāpannā kāyavākacittabuddhitaḥ|",
    "904:2": "karomi satataṃ tasyā nutiṃ pūjāṃ pradakṣiṇām|| 1||",
    "904:3": "yā devī sarvasattvānāṃ sṛṣṭisaṃhārakāriṇī|",
    "904:4": "namastasyai namastasyai namastasyai namo namaḥ|| 2||",
    "904:5": "yā devī sarvabhūtānāṃ pratipāle pratiṣṭhitā|",
    "904:6": "namastasyai namastasyai namastasyai namo namaḥ|| 3||",
    "904:7": "yā devī devadevīnāṃ devatārūpiṇī sthitā|",
    "904:8": "namastasyai namastasyai namastasyai namo namaḥ|| 4||",
    "904:9": "yā devī daityadurdāntadāharūpā bhayānakā|",
    "904:10": "namastasyai namastasyai namastasyai namo namaḥ|| 5||",
    "904:11": "yā devī sarvanāgānāṃ sahasramukhanāginī|",
    "904:12": "namastasyai namastasyai namastasyai namo namaḥ|| 6||",
    "904:13": "yā devī saptapātāle śāntarūpeṇa saṃsthitā|",
    "904:14": "namastasyai namastasyai namastasyai namo namaḥ|| 7||",
    "904:15": "yā devī naranārīṇāṃ vedamātā cidambikā|",
    "904:16": "namastasyai namastasyai namastasyai namo namaḥ|| 8||",
    "904:17": "yā devī kṣatriṇīvaiśyāśūdrījātiprapūjitā|",
    "904:18": "namastasyai namastasyai namastasyai namo namaḥ|| 9||",
    "904:19": "yā devī pretalokānāṃ pālanāya maharddhikā|",
    "904:20": "namastasyai namastasyai namastasyai namo namaḥ|| 10||",
    "904:21": "yā devī sarvatiryakṣu tāriṇī tāpanāśinī|",
    "904:22": "namastasyai namastasyai namastasyai namo namaḥ|| 11||",
    "904:23": "yā devī nārakīyāṇāṃ duḥkhabhājāṃ ca mātṛkā|",
    "904:24": "namastasyai namastasyai namastasyai namo namaḥ|| 12||",
    "904:25": "yā devī ṣoḍaśasaṃkhyanarakāṇāṃ vināśinī|",
    "904:26": "namastasyai namastasyai namastasyai namo namaḥ|| 13||",
    "904:27": "yā devī dveṣirāgīṇāṃ mohināṃ durdurīkṛtā|",
    "904:28": "namastasyai namastasyai namastasyai namo namaḥ|| 14||",
    "904:29": "yā devī kāmakrodhābhyāṃ krodharūpeṇa saṃsthitā|",
    "904:30": "namastasyai namastasyai namastasyai namo namaḥ|| 15||",
    "904:31": "yā devī lobhalābhānāṃ laṅghanāya vilambitā|",
    "904:32": "namastasyai namastasyai namastasyai namo namaḥ|| 16||",
    "904:33": "yā devī māyāyā mātā mātṝṇāṃ vajrayogiṇī|",
    "904:34": "namastasyai namastasyai namastasyai namo namaḥ|| 17||",
    "904:35": "yā devī rājacaurāgnisiṃhaśatruvināśinī|",
    "904:36": "namastasyai namastasyai namastasyai namo namaḥ|| 18||",
    "904:37": "yā devī jalanāgāhidurbhikṣabhayatāriṇī|",
    "904:38": "namastasyai namastasyai namastasyai namo namaḥ|| 19||",
    "904:39": "yā devī pañcabhūtānāṃ candre sūrye pratiṣṭhitā|",
    "904:40": "namastasyai namastasyai namastasyai namo namaḥ|| 20||",
    "904:41": "yā devī bhautikī velā ālikālivicāriṇī|",
    "904:42": "namastasyai namastasyai namastasyai namo namaḥ|| 21||",
    "904:43": "yā devī sarvapīṭheśī kṣetrarūpopachandikā|",
    "904:44": "namastasyai namastasyai namastasyai namo namaḥ|| 22||",
    "904:45": "yā devī pīlavākhyātā upapīlavamīlitā|",
    "904:46": "namastasyai namastasyai namastasyai namo namaḥ|| 23||",
    "904:47": "yā devī dvādaśe cakre rāśilagnavibhūṣiṇī|",
    "904:48": "namastasyai namastasyai namastasyai namo namaḥ|| 24||",
    "904:49": "yā devī ṣaḍaṃge deśe prodbuddhamukhacandrikā|",
    "904:50": "namastasyai namastasyai namastasyai namo namaḥ|| 25||",
    "904:51": "yā devī caṇḍacakreṣu gāminī parameśvarī|",
    "904:52": "namastasyai namastasyai namastasyai namo namaḥ|| 26||",
    "904:53": "yā devī kāyavākacitte mantrarūpeṇa gāminī|",
    "904:54": "namastasyai namastasyai namastasyai namo namaḥ|| 27||",
    "904:55": "yā devī nirmalātmā śrīrṛddhisiddhibalapradā|",
    "904:56": "namastasyai namastasyai namastasyai namo namaḥ|| 28||",
    "904:57": "yā devo sarvajantūnāṃ sadā maṅgalakāriṇī|",
    "904:58": "namastasyai namastasyai namastasyai namo namaḥ|| 29||",
    "904:59": "ya idaṃ paṭhate dhīmān devyā bhaktisamanvitaḥ|",
    "904:60": "buddhatvaṃ labhate śīghraṃ kalyāṇaṃ maṅgalaṃ śivam|| 30||",
    "904:61": "śrī trikāyanivāsinīvajradevyāḥ kalyāṇatriṃśatikāstotraṃ samāptam|"
}