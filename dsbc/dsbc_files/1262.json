{
    "1262:0": "tarkaśāstram|",
    "1262:1": "atha prathamaṃ prakaraṇam|",
    "1262:2": "(śāstramāha)",
    "1262:3": "bhavān manyate'smadvacanamanyāyyamiti cedbhavato'pi vacanamanyāyyam|",
    "1262:4": "yadi bhavadvacanamanyāyyaṃ,",
    "1262:5": "tadāsmadvacanaṃ nyāyyam|",
    "1262:6": "yadi [bhavatocyate] bhavadvacanaṃ nyāyyaṃ,",
    "1262:7": "paramasmadvacanamanyāyyaṃ tanna yuktam|",
    "1262:8": "kiñca yadanyāyyaṃ tadetatsvato nyāyyaṃ,",
    "1262:9": "tasmād yadanyāyyaṃ tannāsti|",
    "1262:10": "yadi svato 'nyāyyaṃ tadetadanyāyyamasaccaiva,",
    "1262:11": "tasmādahamanyāyya[vādī] iti cedbhavatocyate tadayuktam|",
    "1262:12": "anyacca| mama vacanamanyāyyamiti cedbhavatocyate,",
    "1262:13": "tato bhavānajña iti spaṣṭam|",
    "1262:14": "kuta iti cet|",
    "1262:15": "yadanyāyyaṃ tannirābhāsam|",
    "1262:16": "vacanaṃ nyāyābhāvādbhinnamabhinnaṃ vā ?|",
    "1262:17": "abhinnaṃ cedvacanamapi nāstīti mama vacanamanyāyyamiti bhavatā kathamucyate|",
    "1262:18": "atha bhinnaṃ,",
    "1262:19": "tato vacanaṃ nyāyyamiti mama vacanamanyāyyamiti kathaṃ bhavatocyate|",
    "1262:20": "vacanasvalakṣaṇakhaṇḍanācca|",
    "1262:21": "bhavatkhaṇḍanavacanamasmadvacanasya samakālīnamasamakālīnaṃ vā ?",
    "1262:22": "samakālīnaṃ cedasmadvacanakhaṇḍane'samarthaṃ,",
    "1262:23": "yathā gośṛṅge'śvakarṇau vā parasparaṃ khaṇḍayituṃ na śaknuvataḥ|",
    "1262:24": "samakālīnatvāt|",
    "1262:25": "asamakālīnañced,",
    "1262:26": "bhavataḥ khaṇḍanaṃ pūrvamasmadvacanaṃ paścāt|",
    "1262:27": "athāsmadvacanānuccāraṇāt,",
    "1262:28": "kiṃ bhavatā khaṇḍyate ?|",
    "1262:29": "tasmābhdavataḥ khaṇḍanamasidvam|",
    "1262:30": "athāsmadvacanaṃ pūrvaṃ,",
    "1262:31": "bhavataḥ khaṇḍanaṃ tu paścād,",
    "1262:32": "evaṃ tarhyasmadvacanasidvatvāt kasya khaṇḍanaṃ bhaviṣyati ?|",
    "1262:33": "samakālīnañcet,",
    "1262:34": "tato'smadvacanaṃ bhavataśca khaṇḍanamityetayoretat khaṇḍanametacca khaṇḍanīyamiti viśeṣo na syāt|",
    "1262:35": "yathā nadīsamudravārisaṃyogakāle viśeṣāsambhavaḥ|",
    "1262:36": "aparañca| bhavataḥ khaṇḍanaṃ svakhaṇḍanārthamasvakhaṇḍanārthaṃ vā ?",
    "1262:37": "svakhaṇḍanārthañcet,",
    "1262:38": "tataḥ svataḥ svārthahīnaṃ bhavedasmadvacananu siddham|",
    "1262:39": "asvakhaṇḍanārthañced, asiddham|",
    "1262:40": "kuta iti cet,",
    "1262:41": "svārthataḥ khaṇḍanāsidveḥ|",
    "1262:42": "siddhañcet,",
    "1262:43": "svārthahāniḥ parārthasidviśca|",
    "1262:44": "kiñcāsmadvacanamanyāyyamiti ced,",
    "1262:45": "vacanameva na bhavet|",
    "1262:46": "vacanañcennaivānyāyyam|",
    "1262:47": "anyāyyamiti cet,",
    "1262:48": "tadvirudvam|",
    "1262:49": "yathā kumārī putravatīti|",
    "1262:50": "yato yadi kumārī putravatīti na sampadyate|",
    "1262:51": "yadi putravatī,",
    "1262:52": "tarhi naiva kumārī|",
    "1262:53": "kumārīti putravatīti cobhayaṃ virudvam|",
    "1262:54": "tasmānmama vacanamanyāyyamiti cedbhavatocyate tadayuktam|",
    "1262:55": "etatpunaḥ pratyakṣavirudvam|",
    "1262:56": "bhavānasmākaṃ vacanaṃ śrutvā'nyāyyaṃ manyate cet,",
    "1262:57": "tattarhi śrutatvātpratyakṣasiddham|",
    "1262:58": "pratyakṣaṃ hi balavattaraṃ tasmādbhavadvacanahāniḥ|",
    "1262:59": "yathā kaścinmanyeta śrotravijñānataḥ śabdasyānupalabdhiratha śrotravijñānataḥ śabdasyopalabdhiḥ pratyakṣasidveti,",
    "1262:60": "tadā pratyakṣasya balavattaratvāttadvacanahāniḥ|",
    "1262:61": "anumānaviruddhaṃ caitat|",
    "1262:62": "mama vacanamanumānenopalabhyate cettato nyāyyamiti spaṣṭam|",
    "1262:63": "anyāyyaṃ hi vacanaṃ naiva vidyate|",
    "1262:64": "yadi vacanamasti tadā nyāyyamiti jñāyate|",
    "1262:65": "yathā kaścinmanyeta śabdo'nityo hetumattvāt|",
    "1262:66": "yacca hetumattadanityaṃ,",
    "1262:67": "yathā ghaṭaḥ|",
    "1262:68": "sa hetumattvādanityaḥ|",
    "1262:69": "yadi hetumāṃstadā'nityaḥ śabdo,",
    "1262:70": "yadi nityo naiva hetumān|",
    "1262:71": "ityanityatānumānasidvā|",
    "1262:72": "anumānabalānnityatahāniḥ|",
    "1262:73": "nyāyyamiti,",
    "1262:74": "yadvacanaṃ tannyāyyam|",
    "1262:75": "yannyāyyamityanumānasiddhaṃ,",
    "1262:76": "tadanyāyyamiti pratiṣidvam|",
    "1262:77": "lokavirudvaṃ caitat|",
    "1262:78": "yadbhavatoktaṃ mama vacanamanyāyyamiti tallokavirudvam|",
    "1262:79": "kasmāditi, loke hi caturvidhanyāyasatvāt|",
    "1262:80": "[tathā hi] hetuphalanyāyaḥ sāpekṣanyāyaḥ sādhananyāyastathatānyāyaśca|",
    "1262:81": "hetuphalanyāyaḥ|",
    "1262:82": "yathā bījamaṅkuraśca|",
    "1262:83": "sāpekṣanyāyaḥ|",
    "1262:84": "yathā dīrghaṃ hrasvaṃ,",
    "1262:85": "pitā putraḥ|",
    "1262:86": "sādhananyāyaḥ|",
    "1262:87": "yathā pañcāvayavavākyaṃ sādhanārtham|",
    "1262:88": "tathatānyāyastrividhaḥ|",
    "1262:89": "yathā'nātmatathatā'nityatātathatā nirodhatathatā ca|",
    "1262:90": "iha loke vacanaṃ phalaṃ,",
    "1262:91": "nyāyo heturiti|",
    "1262:92": "iha loke yadā phalaṃ dṛṣṭaṃ tadā sahetukaṃ jñātam|",
    "1262:93": "yadā vacanamupalabdhaṃ,",
    "1262:94": "tadā nyāyyaṃ jñātam|",
    "1262:95": "yanmama vacanamanyāyyamiti bhavatoktaṃ tallokavirudvam|",
    "1262:96": "anyāyyaṃ vacanamityasya nāstyavakāśaḥ|",
    "1262:97": "yad bhavatoktaṃ mama vacanamanyahisaṃvāditvāditi tadidamidānīṃ bhavatā sārdhaṃ vicārya nirdhāryate|",
    "1262:98": "yadi kaścidanyadvadettadā tasya doṣaḥ syat|",
    "1262:99": "bhidyate bhavataḥ pratijñā'smatpratijñātaḥ|",
    "1262:100": "atha tadbhavataḥ svoktam|",
    "1262:101": "tadānyaduktam|",
    "1262:102": "tasmādbhavāneva doṣamāpadyate|",
    "1262:103": "yadi bhavadartho 'smaduktādanyastadānyatvadoṣo bhavata eva na tu mama|",
    "1262:104": "yadi nānyat,",
    "1262:105": "tarhi matpakṣatulyameva,",
    "1262:106": "tena nāstyanyatvam|",
    "1262:107": "athocyate mamānyaditi tanmithyā|",
    "1262:108": "anyaccānyasmānnānyadityananyatvam|",
    "1262:109": "yadyanyadanyasmādanyat,",
    "1262:110": "tato'nyanna bhavet|",
    "1262:111": "yathā manuṣyo goranyo na gaurbhavati,",
    "1262:112": "yadyanyadanyasmādanyat,",
    "1262:113": "tadā tadekaṃ bhavet|",
    "1262:114": "yadyekaṃ tato nānyat,",
    "1262:115": "tatkimucyate mamānyaditi|",
    "1262:116": "ataścaitanyāyyamiti|",
    "1262:117": "ahaṃ nyāyamavalambya bhavatā vivade|",
    "1262:118": "tasmādanyathāhaṃ vadāmi|",
    "1262:119": "yadyāvayorbheda eva na syānna tadā bhavatā vivādo'haṃ tu bhavadarthameva vadāmi|",
    "1262:120": "sarvamuktamanyaditi ced,",
    "1262:121": "bhavatāpi kiñciduktamiti bhavānapyanyadvadatīti doṣo bhavata eva|",
    "1262:122": "yadi bhavato vākyaṃ nānyat,",
    "1262:123": "tarhi mamāpi vacanaṃ nānyaditi yad bhavatoktamahamanyadvadāmīti tadayuktam|",
    "1262:124": "atha bhavadvacanaṃ mithyaiva|",
    "1262:125": "śeṣaṃ pūrvavat|",
    "1262:126": "yad (bhavato)",
    "1262:127": "ktaṃ mama vacanamasiddhamiti tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:128": "vicārya nirdhāryate|",
    "1262:129": "yadi vacanamasiddhaṃ,",
    "1262:130": "tadā tadeva vacanamasiddham|",
    "1262:131": "yadi vacanasyāsiddhatvaṃ,",
    "1262:132": "tadā vacanameva na prāptam|",
    "1262:133": "atha vacanaṃ na prāptaṃ,",
    "1262:134": "kathaṃ bhavatocyate mayā yaduktaṃ tadasiddhamiti|",
    "1262:135": "atha vacanaṃ prāptaṃ tatsidvameva syāt|",
    "1262:136": "yadi maduktamasiddhamiti bhavatocyate tadayuktam|",
    "1262:137": "yadi sarvaṃ vacanamasiddha,",
    "1262:138": "tarhi bhavaduktaṃ mama khaṇdanamapyasiddham|",
    "1262:139": "yadi bhavaduktaṃ khaṇḍanamasiddhaṃ na syāt,",
    "1262:140": "tarhi mama vacanamapi naivāsiddham|",
    "1262:141": "yadi (bhavato)",
    "1262:142": "cyate maduktamasiddhaṃ tadayuktam|",
    "1262:143": "yadasiddhaṃ tatsvata eva siddham|",
    "1262:144": "tasmānnāstyasiddham|",
    "1262:145": "yadyasiddha na svataḥ siddhaṃ,",
    "1262:146": "tadāsiddhaṃ na syāt|",
    "1262:147": "yadi siddhaṃ,",
    "1262:148": "nāstyasiddhaṃ,",
    "1262:149": "tato yad (bhavato)",
    "1262:150": "ktaṃ maduktamasiddhamiti tanniravakāśam|",
    "1262:151": "yadi bhavatocyate mama khaṇḍanamananubhāṣaṇameva tadā na mama matasyopalabdhi|",
    "1262:152": "atha nopalabhyate mama mataṃ,",
    "1262:153": "tato mama khaṇḍanaṃ kartuṃ na śakyate|",
    "1262:154": "tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:155": "vicārya nirdhāryate|",
    "1262:156": "yadi bhavān matkhaṇḍanaṃ nānubhāṣate,",
    "1262:157": "tadā bhavān khaṇḍanaṃ vaktuṃ na śaknoti|",
    "1262:158": "kiṃ punarmanyate bhavān khaṇḍanamananubhāṣya khaṇḍanaṃ śakyaṃ,",
    "1262:159": "athavā khaṇḍanamanubhāṣya khaṇḍanaṃ śakyam|",
    "1262:160": "yadi tāvad (bhavān)",
    "1262:161": "ananubhāṣya (khaṇḍanaṃ vaktuṃ śaknoti)",
    "1262:162": ", ahamapyananubhāṣya khaṇḍanaṃ vaktuṃ śaknuyām|",
    "1262:163": "athavā khaṇḍanamanubhāṣya khaṇḍanaṃ vaktuṃ śakyaṃ,",
    "1262:164": "tadā sadaiva khaṇḍanānubhāṣaṇam syāt|",
    "1262:165": "kutaḥ ? khaṇḍanātpunaḥ khaṇḍanasyotpannatvāt|",
    "1262:166": "tadā khaṇḍanānavasthā|",
    "1262:167": "na ca sa kālo vidyate yatra na khaṇḍanānubhāṣaṇam|",
    "1262:168": "[ataḥ] yatra khaṇḍanaṃ vaktuṃ śakyate sa kālo nāsti|",
    "1262:169": "aparañca khaṇḍanamiti khaṇḍanānnāma|",
    "1262:170": "yadi tatkhaṇḍanānubhāṣaṇānnāma khaṇḍanamiti vaktuṃ śakyate,",
    "1262:171": "ananubhāṣya tu khaṇḍanamiti vaktuṃ ja śakyate|",
    "1262:172": "tataḥ pūrvakhaṇḍanasya nāma paścādanubhāṣaṇaṃ prāptam|",
    "1262:173": "paravartikhaṇḍanaṃ nāma na tāvadanubhāṣaṇaṃ prāpnoti|",
    "1262:174": "tṛtīyantu dvitīyasya khaṇḍanasya nāmānubhāṣaṇaṃ prāptam|",
    "1262:175": "caturthaṃ tṛtīyasya khaṇḍanasya nāmānubhāṣaṇaṃ prāptam|",
    "1262:176": "iti sadānubhāṣaṇādanavasthā|",
    "1262:177": "ananubhāṣya khaṇḍanaṃ nāma vaktuṃ śakyamiti ced,",
    "1262:178": "ananubhāṣyāpi tataḥ prathamaṃ khaṇḍanaṃ nāma vaktuṃ śakyam|",
    "1262:179": "prathamaṃ khaṇḍanam nāmānanubhāṣya khaṇḍanaṃ nāma vaktuṃ śakyamiti cet,",
    "1262:180": "dvitīyamapi khaṇḍanaṃ nāmānanubhāṣya khaṇḍanaṃ nāma vaktuṃ śakyate|",
    "1262:181": "dvitīyaṃ khaṇḍanaṃ nāmānanubhāṣya khaṇḍanamiti vaktuṃ śakyata iti cet,",
    "1262:182": "prathamaṃ khaṇḍanaṃ nāmāpyananubhāṣya khaṇḍanaṃ nāma vaktuṃ śakyeta|",
    "1262:183": "kintu prathamaṃ khaṇḍanaṃ nāmāvaśyamanubhāṣyam|",
    "1262:184": "tasmātkhaṇḍanaṃ nāma vaktuṃ śakyate|",
    "1262:185": "atha dvitīyaṃ khaṇḍanaṃ nāmāpyavaśyamanubhāṣyaṃ,",
    "1262:186": "tadā khaṇḍanaṃ nāma vaktuaṃ śakyate natvananubhāṣya vaktavyam|",
    "1262:187": "yadi punaḥ khaṇḍanamananubhāṣya vadet,",
    "1262:188": "khaṇḍanasya nigrahasthānāpattiḥ|",
    "1262:189": "yadi bhavān svakhaṇḍanaṃ nānubhāṣate,",
    "1262:190": "tato bhavaduktakhaṇḍanasya nigrahasthānāpattiḥ|",
    "1262:191": "yadi bhavān khaṇḍanamananubhāṣya khaṇḍanaṃ vadet,",
    "1262:192": "khaṇḍanaṃ vadaṃśca nigrahasthāne na patet,",
    "1262:193": "tadāhamapi khaṇḍanamananubhāṣya khaṇḍanaṃ vadanna nigrāhyaḥ|",
    "1262:194": "kiñca yadā bhavadvacanaṃ mama[mataṃ] khaṇḍayati,",
    "1262:195": "tadā'hamanubhāṣe|",
    "1262:196": "yadā tvahaṃ bhavanmataṃ khaṇḍayāmi tadā bhavānapyanubhāṣate|",
    "1262:197": "atha parasparānubhāṣaṇaṃ,",
    "1262:198": "na tadā khaṇḍanapratiṣṭhāpanam|",
    "1262:199": "yadi parasparānubhāṣaṇaṃ,",
    "1262:200": "tadā samyagarthahāniḥ|",
    "1262:201": "yathā potāvanyonyasambadvau samudravelāsamaye parasparasaṃgharṣeṇa dolāyamānau|",
    "1262:202": "aparañca| sarve śabdā yadā mukhānnirgatāstadā naṣṭā eveti kathaṃ madvacanānubhāṣaṇam ?|",
    "1262:203": "atha śabdo vināśadharmā|",
    "1262:204": "apunarāgamanātpunarbhāṣaṇamaśakyam|",
    "1262:205": "atha śabdaḥ sthitiśīlastadānubhāṣaṇamaśakyaṃ, nityatvāt|",
    "1262:206": "naṣṭa iti cen,",
    "1262:207": "na kiñcidanubhāṣitavyaṃ tadabhāvāt|",
    "1262:208": "śabdo naṣṭa iti cet,",
    "1262:209": "tvadanubhāṣaṇāyaitanme vacanamiti yadbhavān bravīti sa kutarka eva|",
    "1262:210": "yad(bhavato)",
    "1262:211": "ktaṃ madvacanaṃ pūrva,",
    "1262:212": "[bhavata] khaṇḍanaṃ tu paścāditi tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:213": "vicārya nirdhāryate|",
    "1262:214": "yadi madvacanaṃ pūrvaṃ,",
    "1262:215": "khaṇḍanantu paścāditi tanyāyyam|",
    "1262:216": "kuta iti cen,",
    "1262:217": "madvacanaṃ pūrvaṃ bhavadvacanaṃ tu paścāditi|",
    "1262:218": "yadyasmadvacanaṃ paravartivacanaṃ khaṇḍayati,",
    "1262:219": "tato'smadartho viśiṣyate bhavadvacanasya tu hāniḥ|",
    "1262:220": "kiñca yadyucyate bhavatā sarvāṇi vacanāni pūrvavartīni khaṇḍanantu paravartīti,",
    "1262:221": "tadā bhavānapi pūrvameva vacanaṃ vadatīti paścātkhaṇḍanaṃ bhavet|",
    "1262:222": "yadi bhavadvacanasya pūrvavartitve'pi paścātkhaṇḍanaṃ nāsti,",
    "1262:223": "tarhi madvacanasya pūrvavartitve'pi,",
    "1262:224": "paścātkhaṇḍanaṃ na syāt|",
    "1262:225": "yacca khaṇḍanaṃ pūrvasya paravartīti svabhāvataḥ pūrvasya khaṇḍanaṃ na paścādasti|",
    "1262:226": "yadi svabhāvata eva pūrvasya khaṇḍanaṃ paścāt syāt,",
    "1262:227": "tadā pūrvaṃ paścādityubhe na syātām|",
    "1262:228": "tasmādyadbhavatoktaṃ pūrvasya khaṇḍanaṃ parabhāvīti tadayuktam|",
    "1262:229": "yadi svabhāvataḥ pūrvasya khaṇḍanaṃ na paścāt|",
    "1262:230": "hetvabhāvāt|",
    "1262:231": "tadā pūrvasyāpi khaṇḍanaṃ parabhāvi na bhavet|",
    "1262:232": "yadbhavatoktaṃ madvacanaṃ pūrvaṃ,",
    "1262:233": "khaṇḍanaṃ tu parabhāvi tanmithyā|",
    "1262:234": "yadabhihitaṃ bhavatā,",
    "1262:235": "mayā hetvantaramuktamiti tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:236": "vicārya nirdhāryate|",
    "1262:237": "yadi pūrvahetuṃ parityajya hetvantarapratiṣṭhāpanānnigrahasthānamāpadyate,",
    "1262:238": "tadā bhavān nigrahasthānamāpannaḥ|",
    "1262:239": "kathamiti ced,",
    "1262:240": "bhavatā pūrvahetuṃ parityajya hetvantarapratiṣṭhāpanāt|",
    "1262:241": "yadi hetvantarapratiṣṭhāpanādbhavato na nigrahasthānatvāpattistadā mamāpi tathā|",
    "1262:242": "kiñca maduktahetuto bhavaduktahetorbhedaḥ|",
    "1262:243": "yadyanyaṃ hetuṃ vadāmi tanmama nyāyyam|",
    "1262:244": "yadyanyaṃ hetuṃ na vadeyaṃ,",
    "1262:245": "tato bhavaddhetuṃ vadeyam|",
    "1262:246": "tato na pratipakṣatayā virodho'pi tvāvayostulyavacanataiva|",
    "1262:247": "yadi sadṛśa eva heturāvābhyāṃ pratiṣṭhāpitaḥ,",
    "1262:248": "tadā bhavānasmaddhetuṃ khaṇḍayatīti svahetumeva khaṇḍayati|",
    "1262:249": "api ca yadi sarvāṇi vacanāni hetvantarāṇi syustadābhavaduktāni vacanānyapi hetvantarāṇi bhaveyuḥ|",
    "1262:250": "tataśca bhavato nigrahasthānāpattiḥ|",
    "1262:251": "atha vacanānyuccārayannapi na bhavān nigrāhyastarhi yadbhavatoktaṃ hetuṃ pratiṣṭhāpayannahaṃ nigrāhya iti tadayuktam|",
    "1262:252": "yadbhavatoktamarthāntaraṃ vadāmīti tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:253": "vicāryaṃ nirdhāryate|",
    "1262:254": "anyā me pratijñā,",
    "1262:255": "anyā ca bhavata iti yattannyāyyameva|",
    "1262:256": "athāhaṃ bhavataḥ pratipakṣatayā virodhītyarthāntaraṃ bravīmi|",
    "1262:257": "yadi matamasmadartho bhavadarthādananyastadā'smadartho bhavadarthapratipakṣatayā na virudvo|",
    "1262:258": "yadi bhavānasmadarthaṃ khaṇḍayati,",
    "1262:259": "tarhi bhavataḥ svārthasyaiva khaṇḍanaṃ bhavet|",
    "1262:260": "yadarthāntaraṃ na tatsvayamarthāntarama|",
    "1262:261": "tasmānnāstyarthāntaram|",
    "1262:262": "yadi tvarthāntaraṃ svayamevārthāntaraṃ,",
    "1262:263": "na tadārthāntaram|",
    "1262:264": "tasmādyadbhavatoktamahamarthāntaraṃ vadāmīti na yuktam|",
    "1262:265": "aparañca yaduktaṃ sarvaṃ tadarthāntarañcettadā bhavaduktamapyarthāntaraṃ bhavet|",
    "1262:266": "yadi bhavaduktamarthāntaraṃ na bhavettadā yadbhavatā pratijñātaṃ sarvamarthāntaramiti tadayuktam|",
    "1262:267": "yad (bhavato)",
    "1262:268": "ktamahaṃ pūrvacanādananyadvacanaṃ vadāmīti tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:269": "vicārya nirdhāryate|",
    "1262:270": "asmatpratijñā bhavatpratijñāyāḥ pratipakṣabhāvena viruddhā|",
    "1262:271": "yadasmatpratijñā bhavatpratijñāyāḥ pratipakṣabhāvena virudvā,",
    "1262:272": "tannyāyyam|",
    "1262:273": "kuta iti cet|",
    "1262:274": "sarvatrāhaṃ bhavadarthakhaṇḍanārthaṃ vadāmi,",
    "1262:275": "tasmādasmadvacanamananyat|",
    "1262:276": "yadi mayārthāntaramuktaṃ,",
    "1262:277": "tarhi bhavatpratijñā'smadarthādanyā|",
    "1262:278": "yadyahamarthāntaraṃ vadāmi tadā bhavadarthaṃ vadāmi|",
    "1262:279": "evantāvannāhaṃ bhavadviruddo|",
    "1262:280": "tataśca yadi bhavān māṃ khaṇḍayati,",
    "1262:281": "tarhi svārthasyaiva khaṇḍanam|",
    "1262:282": "anyacca| yathā mayā pūrvamuktamanityaḥ śabda iti|",
    "1262:283": "etāni vacanāni vināśasvabhāvāni kṣayasvabhāvāni ca|",
    "1262:284": "idānīmanyadvacanamuccaryate|",
    "1262:285": "tataśca yadbhavatoktaṃ bhavān pūrvavacanaṃ vadatīti tanmithyā|",
    "1262:286": "aparañca| yadbhavatoktaṃ maduktamananyaditi|",
    "1262:287": "tatra yadyahamanyadvadāmi tadā tadanyayat|",
    "1262:288": "yadyahamananyadvadāmi tadananyat|",
    "1262:289": "yadyahaṃ tadvadaṃstanna sādhayituṃ śaknomi,",
    "1262:290": "tadā yadbhavatoktamananyaditi tadyuktam|",
    "1262:291": "yacca (bhavato)",
    "1262:292": "ktaṃ mayā sarvamuktaṃ nānujñāyata iti tadidānīṃ (bhavatā sārdhaṃ)",
    "1262:293": "vicārya nirdhāryate|",
    "1262:294": "sarvaṃ nānujñāyata iti yaduktaṃ bhavatā etadvacanaṃ sarvasminnantarbhavati na vā ?|",
    "1262:295": "yadi tāvatsarvasminnantarbhavati tadā bhavān svayaṃ svoktaṃ nānujānāti|",
    "1262:296": "yadi svayaṃ nānujānātyasmadarthaḥ svata eva siddho bhavedbhavavacanasya tu hāniḥ syāt|",
    "1262:297": "atha sarvasminnāntarbhavati tadā tasya sarvatvameva na syāt|",
    "1262:298": "yadi sarvatvameva na bhavet,",
    "1262:299": "tadā bhavatā yadananujñātaṃ tatsarvam|",
    "1262:300": "yadi sarvamananujñātaṃ tadā'smadartho bhavatā naivānanujñātaḥ|",
    "1262:301": "asmadarthaḥ siddho bhavatastu sarvasya pratiṣedhaḥ|",
    "1262:302": "iti prathamaṃ prakaraṇam|"
}