{
    "910:0": "vajrayoginyāḥ stutipraṇidhānam",
    "910:1": "siddhācārya-virūpādaviracitam",
    "910:2": "om namaḥ śrī vajrayoginyai,",
    "910:3": "namobuddhadharmasaṃghebhyaḥ,",
    "910:4": "namo gurubuddhabodhisattvebhyaḥ,",
    "910:5": "namo locanādidaśavajravilāsinībhyaḥ,",
    "910:6": "namo yamāntakādidaśakrodhavīrebhyaḥ saprajñebhyaḥ |",
    "910:7": "om o ā hūṃ iti kāyavākacita indriyavijñānāni,",
    "910:8": "vākacittāni manovijñānāni,",
    "910:9": "paracittajñānābhijñā,",
    "910:10": "parakāyapraveśābhijñā puṇḍarīkeṣu nābhisthe raktasūrya maṇḍalaḥ || 1 ||",
    "910:11": "dharmodayā padmamadhye sindūrāruṇarūpiṇī |",
    "910:12": "triśṛṅgā hi trikoṇā vā catuḥpīṭhasamanvitā || 2 ||",
    "910:13": "hrīṃkāraṃ madhyabhāge'syāḥ pītavarṇaḥ prakīrtitaḥ |",
    "910:14": "tadbhavā pītavarṇā ca avadhūtyā (tī)",
    "910:15": "svayaṃ sthitā || 3 ||",
    "910:16": "lalanāyāṃ tu suśyāmā rasanāyāṃ ca gaurikā |",
    "910:17": "pratyālīḍhapadā nāmnā madhye pītā manoramā || 4 ||",
    "910:18": "trimārge saṃsthitā devī trikāyavajrayoginī |",
    "910:19": "seyaṃ nāmnā bhavedekā sarvasambuddhaḍākinī || 5 ||",
    "910:20": "trailokye divyarūpā ca sarvāśāparipūrikā |",
    "910:21": "svakartinā kartitaṃ ca vāmahaste svamastakam || 6 ||",
    "910:22": "dakṣiṇe'syā sthitā kartirjagato duḥkhacchedinī |",
    "910:23": "asyā vāme sthitā śyāmā nāmnā sā varavarṇinī || 7 ||",
    "910:24": "karparaḥ savyahaste ca kartirvāme ca saṃsthitā |",
    "910:25": "dakṣiṇe caiva pītā vai vajravairocanī sthitā || 8 ||",
    "910:26": "kartikā dakṣiṇe haste vāme kapāladhāriṇī |",
    "910:27": "pratyālīḍhapadā devī nagnikā tu viśeṣataḥ || 9 ||",
    "910:28": "karau ca dvau tayoḥ pādau viparītau tathā sthitau |",
    "910:29": "buddhānāṃ jananī madhyā samyaksambuddharūpiṇī || 10 ||",
    "910:30": "akṣayā subhagā nityaṃ hantrī ca mṛtyujanmanoḥ |",
    "910:31": "dharmasaṃbhoganirmāṇamahāsukhasvarūpiṇī || 11 ||",
    "910:32": "seyaṃ tribhuvane devī rājaye candravatsadā |",
    "910:33": "kabandhādavadhūtyāśca praviśetsvamukheṣvasṛk || 12 ||",
    "910:34": "lalanārasanābhyāṃ ca niḥsṛtya śirasi tathā |",
    "910:35": "karūṇāraktarūpeṇa pibantī sarvaśāntaye || 13 ||",
    "910:36": "duḥkhaṃ saṃprekṣya lokānāṃ caturmāraniṣūdinī |",
    "910:37": "daurmanasyādi kartyā ca kartantī sarvameva tat || 14 ||",
    "910:38": "stutā hi jinamātastvaṃ suprasīda bhavārṇave |",
    "910:39": "svārthaṃ kartuṃ parārthaṃ ca bhavāmyahaṃ yathā kṣamaḥ || 15 ||",
    "910:40": "tattvayā devī kartavyamanugrahaṃ mayi sṛja |",
    "910:41": "bālo'hamajñarūpī ca kāyavākacittadoṣataḥ || 16 ||",
    "910:42": "gurubhaktiṃ na jāne'haṃ tvadbhaktiṃ ca viśeṣataḥ |",
    "910:43": "ityākalaya lokeśi kṛpā mayi vidhīyatām || 17 ||",
    "910:44": "kāyavākacetasāṃ sāmyaṃ kuru me tvaṃ prasādataḥ |",
    "910:45": "yena muñcāmi saṃsāraṃ ṣaḍgatiṃ na viśāmyaham || 18 ||",
    "910:46": "yathā ca vajravaddeho yāvanmukto bhavāmyaham |",
    "910:47": "māraiḥ sarvaiḥ parityakto yathā kāyo bhavenmama || 19 ||",
    "910:48": "līlayā siddhibhāgāḥ syurnirvighnaḥ sarvasiddhibhāk |",
    "910:49": "sarvasādhanasiddhi syāt kāyaścaivājarāmaraḥ || 20 ||",
    "910:50": "trāsanaṃ sarvamārāṇāṃ sarvāśāparipūraṇam |",
    "910:51": "dehi me tvaṃ varāṃ siddhiṃ hitvā caiva gamāgamam || 21 ||",
    "910:52": "mano vilīyatāṃ devi pañcendriyānusaṃyutam |",
    "910:53": "siddhyecca vajrasattvāyuryauvanārogyasatsukham || 22 ||",
    "910:54": "tvatprasādānmahāmudrā sarveṣāmeva saṃbhavet |",
    "910:55": "devāsuramanuṣyāśca yatra yatra ca saṃsthitāḥ |",
    "910:56": "sukhinaste ca sarve syurbuddhatvaphalalābhinaḥ || 23 ||",
    "910:57": "śrīguhyasamayatantre trikāyavajrayoginyāḥ stutipraṇidhānaṃ samāptam |",
    "910:58": "kṛtiriyaṃ siddhācārya śrīvirūpādānām |"
}