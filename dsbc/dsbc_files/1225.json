{
    "1225:0": "XXXII",
    "1225:1": "parīnandanāparivarto nāma dvātriṃśattamaḥ|",
    "1225:2": "sahapratilabdhānāmityādi|",
    "1225:3": "anena labdhānāṃ samādhīnāṃ tatkālajamanuśaṃsamāha|",
    "1225:4": "ata eva sahaśabdaḥ samānakālatārthaḥ|",
    "1225:5": "pratilabdhānāmiti pratilabdheṣu satsu|",
    "1225:6": "ebhireva nayeriti yathoktavastubhiraṣṭābhirabhisamayaiḥ|",
    "1225:7": "ebhireva nāmabhiriti sarvākārajñatādibhiḥ|",
    "1225:8": "sarvāsu ca jātiṣu na jātu buddhavirahito'bhūdityuddeśaḥ|",
    "1225:9": "asya nirdeśa uttaro granthaḥ svapnāntaragatopīti yāvat|",
    "1225:10": "tatra khalvityādirupodghātaḥ parīndanāyāḥ|",
    "1225:11": "tadanenāpi paryāyeṇaiti sadāpraruditavṛttāntenāpi|",
    "1225:12": "tasmāttarhītyādinā anuśāsanīśabdaparyantena prathamā parīndanā|",
    "1225:13": "atra ca tathāgatādhiṣṭhāneneti vacanaṃ kimartham ?",
    "1225:14": "mahārthatvābhdagavatī bavhantarāyā ca vinā buddhādhiṣṭhānena likhitumapi [na] śakyeti pradarśanārtham|",
    "1225:15": "iyamasmākamanuśāsanītīyamasmākaṃ parīndanā|",
    "1225:16": "yathā asyāḥ śravaṇādikaṃ na vicchidyate tathā tvayā kartavyamityarthaḥ|",
    "1225:17": "tatkasya hetoriti|",
    "1225:18": "tatparīndanaṃ kena prayojanenetyarthaḥ|",
    "1225:19": "ata āha|",
    "1225:20": "atra hītyādi|",
    "1225:21": "tatkathamityādinā dvītīyaparīndanāyā upodghātaḥ|",
    "1225:22": "asmin mama samucchaya iti mama śarīre|",
    "1225:23": "tathataditi|",
    "1225:24": "etatpremādikam|",
    "1225:25": "etenopādghātena dve parīndane|",
    "1225:26": "yata āha dvirapi trirapīti|",
    "1225:27": "parīndāmīti samarpayāmi|",
    "1225:28": "anuparīndamītyānukūlyaṃ samarpayāmi|",
    "1225:29": "kathaṃ parīndanetyata āha yatheyamityādi|",
    "1225:30": "anyaḥ puruṣa ityabhaktaḥ|",
    "1225:31": "kasmādasyāmetāvadgauravamityata āha|",
    "1225:32": "yāvadityādi bahuvidhāśca pūjābhiriti yāvat|",
    "1225:33": "bhagavatyāḥ parisamāptimudyotayan saṅgītikāra āha|",
    "1225:34": "idamavocadityādi|",
    "1225:35": "idamiti prajñāpāramitāsūtram|",
    "1225:36": "avocaditi bhāṣitavān|",
    "1225:37": "bhagavāniti śāstā|",
    "1225:38": "parārtharasikāstathāgatāḥ|",
    "1225:39": "parārtha ca parārthakriyā bhagavatyā deśanā|",
    "1225:40": "tata āha|",
    "1225:41": "āttamanā iti|",
    "1225:42": "niruttaradharmadeśanayā prītaḥ sannityarthaḥ|",
    "1225:43": "śrotāraḥ kimukurvannityāha|",
    "1225:44": "bhagavato bhāṣitamabhyanandanniti|",
    "1225:45": "abhipūrvo nandiḥ sakarmakaḥ|",
    "1225:46": "sa kadācidabhilāṣe vartate|",
    "1225:47": "kadācitprītau|",
    "1225:48": "yadā prītau tadā tadālambanamevāsya karma bhavati|",
    "1225:49": "tatpunaḥ karma bhagavato bhāṣitam|",
    "1225:50": "eṣaiva deśanā|",
    "1225:51": "ke punaste śrotāra ityāha|",
    "1225:52": "te cetyādi|",
    "1225:53": "lokaḥ sattvasamūhaḥ|",
    "1225:54": "sa ca śrotṝṇāṃ rājagṛhasya ca prakṛtatvānmanuṣyasamūha eva kevalo gamyeta|",
    "1225:55": "tathā mā bhūditi viśeṣaṇaṃ kriyate|",
    "1225:56": "sadevamānuṣāsuragandharvaśceti|",
    "1225:57": "te ca bodhisattvādayaḥ sadevādiśca loka iti samuccayaḥ||",
    "1225:58": "parīndanābhidhāyī parivartaḥ parīndanāparivartaḥ||",
    "1225:59": "nānāvibhramalāñchanavyapagamādagrāhyamagrāhakam|",
    "1225:60": "bhātyetattathatātmanā samarasaṃ yasyāmaśeṣaṃ jagat||",
    "1225:61": "prajñāpāramitā vikalpataraṇī sā bodhisattvasya dhīḥ|",
    "1225:62": "dhīraiḥ saiva viśuddhipāragamane tāthāgatī kathyate||",
    "1225:63": "sa(śa)satyaṣṭau yadabhisamayān yatsahasrāṇi cāṣṭau|",
    "1225:64": "sūtraṃ tatte bhagavati mayā yaśca labdho vibhajya||",
    "1225:65": "puṇyaskandhaḥ phalatu sa yathā yuktimuktiṃ prajānām|",
    "1225:66": "niḥsīmānāṃ mama ca vaśitāṃ viśvakāryakriyāsu||",
    "1225:67": "anupamaguṇamṛṣṭā nirmalāśeṣavarṇā|",
    "1225:68": "harati bhavaratiṃ vo bhāratī gautamasya|",
    "1225:69": "bhavati mahati(?)",
    "1225:70": "vartiryā dayā snehapūrṇe",
    "1225:71": "janamanasi tadantarjjyotiṣaḥ saṃkramāya||",
    "1225:72": "prajñāpāramitāyāḥ pramitāyā daśaśatībhiraṣṭābhiḥ|",
    "1225:73": "sāratametyabhisamaye sphuṭā ghanā pañjikeyaṃ me||",
    "1225:74": "ye dharmā hetuprabhavā hetuṃ teṣāṃ tathāgato hyavadat|",
    "1225:75": "teṣāṃ ca yo nirodha evaṃvādī mahāśramaṇaḥ||",
    "1225:76": "?(?) samvat ā to rājñaḥ śrīharṣadevarājye śrīgaṇḍigulmaviṣaye|",
    "1225:77": "kulaputrakāyasthaḥ (? sthasya)",
    "1225:78": "paṇḍitaśrījīvandharaśiṃ(siṃ)",
    "1225:79": "hasya pustako(?",
    "1225:80": "stike)yamiti||0||"
}