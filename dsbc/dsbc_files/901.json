{
    "901:0": "māravijayastotram",
    "901:1": "jitamārakaliṃ jagadekaguruṃ",
    "901:2": "surayakṣamahoragadaityanatam |",
    "901:3": "catusatyasudeśitamārgamimaṃ",
    "901:4": "guṇameghamahaṃ praṇamāmi sadā || 1 ||",
    "901:5": "bhavabhīmamahodadhimadhyagataṃ",
    "901:6": "kṛpaṇārtaravaṃ samavekṣya janam |",
    "901:7": "parimocayituṃ ya iha praṇidhiṃ",
    "901:8": "pracakāra vibhuṃ tamahaṃ praṇataḥ || 2 ||",
    "901:9": "girirājanibhaṃ śaradindudhiyaṃ",
    "901:10": "sumukhaṃ subhujaṃ vararūpadharam |",
    "901:11": "sugataṃ gatamapratimaṃ sugataṃ",
    "901:12": "praṇato'smi sadā jagadarthakaram || 3 ||",
    "901:13": "kanakaprabhayā paripītatanuṃ",
    "901:14": "varadundubhitoyadavalgurutam |",
    "901:15": "gajahaṃsavilambitadhīragataṃ",
    "901:16": "śirasā'bhinato'smi guṇaikanidhim || 4 ||",
    "901:17": "taruṇārkasamairacalairnayanaiḥ",
    "901:18": "sphuradugramahāśanibhīmaravaiḥ |",
    "901:19": "namucipraharairasiśaktidharai-",
    "901:20": "ścalito'si vibho na hi tvaṃ vikṛtaiḥ || 5 ||",
    "901:21": "lavalīphalapāṇḍurakarṇapuṭāḥ",
    "901:22": "kucabhāravināmitagātralatāḥ |",
    "901:23": "nahi tvāmaśakan khalu nāgasutāḥ",
    "901:24": "śvasitaiḥ skhalitairapi kampayitum || 6 ||",
    "901:25": "taravaḥ kusumastabakābharaṇā",
    "901:26": "bahuratnasahasracitāśca nagāḥ |",
    "901:27": "na tathābhiratiṃ janayanti satāṃ",
    "901:28": "janayanti yathā tava vīraguṇāḥ || 7 ||",
    "901:29": "sphuṭacitrapadaṃ bahuyuktiyutaṃ",
    "901:30": "gamakaṃ vacanaṃ tava karṇasukham |",
    "901:31": "śubhamārgaphalaṃ prasamīkṣyajanā",
    "901:32": "na gṛṇanti punastvadṛte'nyavacaḥ || 8 ||",
    "901:33": "iti vaḥ śaraṇaṃ samavekṣya janā",
    "901:34": "na patantyapi kalpaśatairniraye |",
    "901:35": "vinihatya ca doṣaripūn bahulān",
    "901:36": "pariyanti śubhaṃ varamokṣapuram || 9 ||",
    "901:37": "tava saumyatayā'pyabhibhūtatamo",
    "901:38": "na virājati śītakaro gagane |",
    "901:39": "tava kāñcanakuṅkumasaprabhayā",
    "901:40": "prabhayā'bhihato na vibhāti raviḥ || 10 ||",
    "901:41": "tava nātha śubhe vadanāmburūhe",
    "901:42": "nayanabhramarā nipatanti nṛṇām |",
    "901:43": "pratibuddhadale kamale vimale",
    "901:44": "bhramarā iva puṣpaśatākulitāḥ || 11 ||",
    "901:45": "iti toṭakamantravarairatulaiḥ",
    "901:46": "parikīrtya mayā tava varṇalavān |",
    "901:47": "yadupārjitamadya śubhaṃ vipulaṃ",
    "901:48": "śivamastu tato bhuvi devanṛṇām || 12 ||",
    "901:49": "śrīmāravijayastotraṃ samāptam ||"
}