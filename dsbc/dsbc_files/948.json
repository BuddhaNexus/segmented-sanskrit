{
    "948:0": "dvitīyaḥ sargaḥ",
    "948:1": "devānāṃ tuṣitapurīgamanam",
    "948:2": "tatrāntare jagati pūrvanimittamāsīd",
    "948:3": "dṛṣṭvā tadadbhutamamartyagaṇāḥ sametāḥ|",
    "948:4": "sarvajñatāvasara eṣa taveti vaktuṃ",
    "948:5": "jagmuḥ purīṃ suragurostuṣitāabhidhānam||1||",
    "948:6": "uttuṅganīlamaṇimandirajṛmbhamāṇa-",
    "948:7": "rociśchaṭācchuraṇaśādvalitāntarikṣām|",
    "948:8": "prakrīḍamānamṛgaśāvaviloladṛṣṭi-",
    "948:9": "cchāyāsamuccalanacandrakilopakaṇṭhām||2||",
    "948:10": "līlācakorarasanāñcalalihyamāna-",
    "948:11": "prāsādadantavalabhīkiraṇaprarohām|",
    "948:12": "tiryakpravṛttamaṇitoraṇadīrgharaśmi-",
    "948:13": "mālāvalīguṇitavandanamālikābhām||3||",
    "948:14": "śiñjānapañcaśaracakritakārmukajyā-",
    "948:15": "jhaṅkāravegacalitādhvagavāmanetrām|",
    "948:16": "darpāndhadiggajakapolamadapravāha-",
    "948:17": "kallolinīsalilakardamitapratolīm||4||",
    "948:18": "śampāsahasracaturasrasaroruhākṣī-",
    "948:19": "dehaprabhāpunarudīritadīpamālām|",
    "948:20": "saudhasthaloparisamucchritavaijayantī-",
    "948:21": "cīnāṃśukākalitadigvanitāvaguṇṭhām||5||",
    "948:22": "puṣpāvacāyavalamānapurandhrivarga-",
    "948:23": "pīnastanonnativikalpitakeliśailām|",
    "948:24": "mākandakorakagalanmakarandapūra-",
    "948:25": "dhārānubaddhapunaruktataṭākatoyām||6||",
    "948:26": "śrṛṅgāramaṇḍapaśironavaratnatejaḥ-",
    "948:27": "sañcārasañcitaśatakratucāpaśobhām|",
    "948:28": "mandārakalpaharicandanapārijāta-",
    "948:29": "santānasaṃhṛtadaridrakathāprasaṅgām||7||",
    "948:30": "tatra sthitaṃ suragaṇā dadṛśastamenaṃ",
    "948:31": "siṃhāsane vividharatnaśilānibaddhe|",
    "948:32": "vibhrājamānabahudhātuvicitravarṇe",
    "948:33": "merormṛgendramiva sānutaṭapradeśe||8||",
    "948:34": "māṇikyamaulivalabhīsavidhasthitena",
    "948:35": "mānyena maṅgalasitātapavāraṇena|",
    "948:36": "pūrvācalasya suṣamāṃ maṇituṅgaśrṛṅga-",
    "948:37": "saṃlakṣyapūrṇaśaśinaḥ pratipakṣayantam||9||",
    "948:38": "pratyagrahāṭakaśilāphalakāyatasya",
    "948:39": "phālasthalasya paritaḥ prasṛtairmakhaiḥ|",
    "948:40": "āśāviśālanayanānanamaṇḍanānā-",
    "948:41": "mākalpayantamiva kāntisudhāvibhāgam||10||",
    "948:42": "āyāmaśālibhiramandadayāsamudra-",
    "948:43": "velājaleṣu viharadbhirapāṅgapātaiḥ|",
    "948:44": "āpādayantamamarādhiparājyalakṣmyāḥ",
    "948:45": "krīḍāsaroruhatatīriva diṅmukheṣu||11||",
    "948:46": "ākāśakandaradarīṣu vitāyamānai-",
    "948:47": "rānandamandahasitairadhikaprasannaiḥ",
    "948:48": "sandhukṣaṇāya nijakīrttipayaḥpayodheḥ",
    "948:49": "sampādayantamiva śāśvatamindulokam||12||",
    "948:50": "abhyarṇavarttibhirakṛtrimabhaktiśobhai-",
    "948:51": "rātmīyabimbasadṛśaiḥ saha mitravargaiḥ|",
    "948:52": "ābhāṣaṇeṣvadharavidrumarāgalakṣyā-",
    "948:53": "dantaḥsphurantamanurāgamivodgirantam||13||",
    "948:54": "ānandavāṣpajalajarjaradṛṣṭipāta-",
    "948:55": "mabhyullasatpulakabhūṣitagaṇḍarekham|",
    "948:56": "ākarṇayantamabhijātanijāpadāna-",
    "948:57": "magre kuśīlavagaṇairabhigīyamānam||14||",
    "948:58": "kalpadrumaprasavakalpitakarṇapūra-",
    "948:59": "riccholikāvigalitairmakarandapūraiḥ|",
    "948:60": "bāhudvayasya mahanīyaparākramasya",
    "948:61": "vīrābhiṣekamahimānamivācarantam||15||",
    "948:62": "uttuṅgabāhuyugalodayaśailajāta-",
    "948:63": "tejodivākarayaśohimaraśmiśaṅkām|",
    "948:64": "ātanvatāruṇasitopalanirmitena",
    "948:65": "maṅgalyakuṇḍalayugena manojñagaṇḍam||16||",
    "948:66": "mandārapuṣpakalikāparikalpitena",
    "948:67": "mālyena mānyabhujamadhyavilambitena|",
    "948:68": "kaṇṭhapraṇālimukhagatvararaktadhāra-",
    "948:69": "mādarśayantamiva maitrabalāvatāram||17||",
    "948:70": "abhyudgatairaruṇarāgamano'bhirāmai-",
    "948:71": "rāmuktaratnavalayāṃkuraraśmijālaiḥ|",
    "948:72": "nirbhidyamānanijaśauryamahaḥpravāla-",
    "948:73": "sañchāditāviva bhujau viṭapau dadhānam||18||",
    "948:74": "aṅgairamandaharicandanapaṅkaliptai-",
    "948:75": "rabhyantareṣu kutakuṃkumapatralekhaiḥ|",
    "948:76": "pakṣīndracañcupuṭapāṭanajarjarāṅgāṃ",
    "948:77": "jīmūtavāhanadaśāmiva darśayantam||19||",
    "948:78": "nānāvidhābharaṇaratnamarīcidaṇḍai-",
    "948:79": "diṅmaṇḍaleṣu paritaḥ parijṛmbhamāṇaiḥ|",
    "948:80": "āgāmibodhipadavaibhavacihnabhūtā-",
    "948:81": "mṛddhipadarśanadhurāmiva śikṣayantam||20||",
    "948:82": "āpādapadmamabhitaḥ pravijṛmbhitābhi",
    "948:83": "rambhojarāgapatapatakābharaṇaprabhābhiḥ|",
    "948:84": "tasmāt prabhṛtyuparibhāvimunitvamudrāṃ",
    "948:85": "kāṣāyadhāraṇakalāmiva śīlayantam||21||",
    "948:86": "saṃkrāntasaudhavalabhīmaṇiputrakeṇa",
    "948:87": "vakṣaḥ kavāṭaphalakena manohareṇa|",
    "948:88": "sākṣāduraḥsthalavihārisamudrarāja",
    "948:89": "kanyasya kaiṭabharipoḥ kalayantamābhām||22||",
    "948:90": "niṣyandamānamakarandanirantareṇa",
    "948:91": "raktotpalena karapaṅkajalālitena|",
    "948:92": "sadyo vipāṭanagaladrudhirāruṇena",
    "948:93": "netrotpalena śivirājamivopalakṣyam||23||",
    "948:94": "ālepacandanavisṛtvaragandhalobhā",
    "948:95": "dālīyamānamalināmabhito nikāyam|",
    "948:96": "ajñānagāḍhatimiraudhamivāntarasthaṃ",
    "948:97": "tenaiva dikṣu nitarāmapasārayantam||24||",
    "948:98": "āśāmukhaprasṛmarairarabhinandanīyai-",
    "948:99": "rāścaryasaṃhananakāntisudhāpravāhaiḥ|",
    "948:100": "āplāvayantamiva nirjararājaloka-",
    "948:101": "mātmapratāpatapanāturamantikastham||25||",
    "948:102": "ālokabāhuvalayaskhalanāravāra-",
    "948:103": "vācālitākhilaharinmukhamaṇḍalībhiḥ|",
    "948:104": "ārādamartyapuravāravilāsinībhi-",
    "948:105": "rādhūyamānasitacāmaracakravālam||26||",
    "948:106": "vakṣaḥsthalena valamānamanojñahāra-",
    "948:107": "tārāvalīvalayinā gaganopamena|",
    "948:108": "ākāśasindhulaharīparirabhyamāṇa-",
    "948:109": "mābhāsayantamamarādritaṭāvalepam||27||",
    "948:110": "ambhoruhākṛtimabhaṅgarapadmarāga-",
    "948:111": "bhaṅgībhirāracitamadbhutapādapīṭham|",
    "948:112": "dānābhibhūtanatapadmanidhiprakāśaṃ",
    "948:113": "savyetareṇa caraṇena parāmṛśantam||28||",
    "948:114": "aṃghreraktalakarasadyutihāriṇībhi-",
    "948:115": "rabhyudgatābhiraruṇāṃgulidīdhitībhiḥ|",
    "948:116": "vandārudevavadanāmbujabodhanāya",
    "948:117": "bālātapaprasaravarṣamivācarantam||29||",
    "948:118": "nakṣatranāthakarakandalamāṃsalena",
    "948:119": "navyena pādanakhadīdhitijālakena|",
    "948:120": "niṣyandamānasuranirjhariṇīmaranda-",
    "948:121": "dhārābhirāmacaraṇābjamivābjanābham||30||",
    "948:122": "saṃsāreghoraparitāpajuṣāṃ janānāṃ",
    "948:123": "saṃrakṣaṇāya kimayaṃ samayo na veti|",
    "948:124": "jijñāsayā kṣaṇamivāvatarītukāmaṃ",
    "948:125": "māṇikyakuṭṭimatalapratimānibhena||31||",
    "948:126": "dṛṣṭvā jagattrayaguruṃ śirasā praṇemu-",
    "948:127": "rdūrānatena tuṣitālayapārijātam|",
    "948:128": "vācāmatītya padavīmabhivartamāna-",
    "948:129": "mārebhire stutibhirarcayituṃ ca devāḥ||32||",
    "948:130": "dīnāvalokanadaśāntarajṛmbhamāṇa-",
    "948:131": "kāruṇyapūraparivāhamahāpraṇālaiḥ|",
    "948:132": "asmānapāṅgaya vinidrasarojamudrā-",
    "948:133": "karṇejapaistava surendra ! kaṭākṣapātaiḥ||33||",
    "948:134": "svairojjihānasuṣamābharadugdhasindhu-",
    "948:135": "kallolakandalakarambitagātrayaṣṭe !",
    "948:136": "cūḍāvataṃsa ! tuṣitālayadevatānāṃ",
    "948:137": "tubhyaṃ namaḥ paramakāruṇikavratāya||34||",
    "948:138": "prajñāpradhānamahiṣīpadapaṭṭabandha-",
    "948:139": "sambhāvanātiśayasambhṛtanirvṛtāya|",
    "948:140": "sarvottarāśramakathāmṛtapānalīlā-",
    "948:141": "goṣṭhīparāya guṇavāridhaye namaste||35||",
    "948:142": "maitrīkalatrakucabhārapaṭīrapaṅka-",
    "948:143": "patrāvalīmakarikāṅkaramaṇḍitāya|",
    "948:144": "tejastaraṅgitadigantarakandarāya,",
    "948:145": "trailokyabhāgyaparipākabhuve namaste||36||",
    "948:146": "mārapratāpabaḍavānalakīlajāla-",
    "948:147": "jājvalyamānajananārṇavadharmanāve|",
    "948:148": "divapālaśekharitaśāsanapatrikāya",
    "948:149": "dikyānubhāva ! jagadekaguro ! namaste||37||",
    "948:150": "niṣyandamānanirapāyakṛpāpravāha-",
    "948:151": "vīcīviṭaṅkavalamānaviśāladṛṣṭe !",
    "948:152": "dhyānāmṛtadravataraṅgitacittavṛtte !",
    "948:153": "devādideva ! jagadekadṛśe namaste||38||",
    "948:154": "nirvyājakṛttagalanirgaladasradhārā-",
    "948:155": "nirvāpitakṣudhitarākṣasajāṭharāgne !",
    "948:156": "nirvāṇakelikṛtinirmitisūtradhāra !",
    "948:157": "netrābhirāma ! surarāja ! namo namaste||39||",
    "948:158": "gandharvarājamahilājanagīyamāna-",
    "948:159": "kīrttipravāhaparivāhitadiṅmukhāya|",
    "948:160": "bhavyānurakṣaṇaparāya phalonmukhīna-",
    "948:161": "bhāgyādhikāya bhagavan ! bhavate praṇāmaḥ||40||",
    "948:162": "nityapravṛttaniravadyamahāpradāna-",
    "948:163": "śobhāparājitasuradruimakāmadheno|",
    "948:164": "śuddhāśayāya sucaritravibhūṣaṇāya",
    "948:165": "tubhyaṃ namastuṣitalokadhurandharāya||41||",
    "948:166": "rākāsudhākiraṇabimbamano'bhirāma-",
    "948:167": "vaktrāvadhūtavaravārijavaibhavāya|",
    "948:168": "śāntāśayāya śapharadhvajabāhuvīrya-",
    "948:169": "muṣṭindhayāya munimānyadhiye namaste||42||",
    "948:170": "śṛṅgāritāyatadigantamadāvalendra-",
    "948:171": "śuṇḍārakāṇḍaparibhāvukabāhudaṇḍam|",
    "948:172": "saundaryakandalitacārumukhāravindaṃ",
    "948:173": "vandāmahe varadarāja ! bhavantameva||43||",
    "948:174": "vīra ! tvameva vijitākhiladiṅmukhasya",
    "948:175": "mīnadhvajasya vinipātavidhau vidagghaḥ|",
    "948:176": "siṃhād ṛte jagati kaḥ khalu dhīracetā",
    "948:177": "dantāvalaṃ jayati jarjaritādrikūṭam||44||",
    "948:178": "vidveṣatāpamakhilaṃ jagatāṃ vinetuṃ",
    "948:179": "śaktastvameva śaraṇāgatapuṇyarāśe !",
    "948:180": "dhārādharaṃ taralavidyutamantareṇa",
    "948:181": "dāvānalaṃ śamayituṃ bhuviḥ kaḥ kṣameta !||45||",
    "948:182": "mohāndhakāramuṣitāni jagattrayāṇi",
    "948:183": "puṇyādhika ! tvamasi bodhayituṃ pravīṇaḥ|",
    "948:184": "ko vā vikāsayitumarhati kokabandhuṃ",
    "948:185": "bhānuṃ vinā śaradi paṅkajakānanāni||46||",
    "948:186": "tṛṣṇāpravāhamavaśoṣayituṃ janānāṃ",
    "948:187": "tejasvināmadhipa ! dakṣatarastvameva|",
    "948:188": "kalpāvasānabaḍavānalamantareṇa",
    "948:189": "kaḥ pārayejjagati pātumapāmadhīśam||47||",
    "948:190": "dhīra ! tvameva jananāmbunidhestrilokīṃ",
    "948:191": "pāraṃ paraṃ gamayituṃ paṭutāmupaiṣi|",
    "948:192": "ko vā vihāya bhuvena kuhanāvarāhaṃ",
    "948:193": "kṣoṇīsamuddhṛtividhau kuśalaḥ payodheḥ||48||",
    "948:194": "itthaṃ suparvavihitāṃ stutimādareṇa",
    "948:195": "śrutvā prasannahṛdayastuṣitādhirājaḥ|",
    "948:196": "gambhīravāridharagarjitamandareṇa",
    "948:197": "tān pratyuvāca vacasā madhurākṣareṇa||49||",
    "948:198": "bho bhoḥpurandaramukhā haridantapālāḥ",
    "948:199": "sambhūya yūyamiha sādaramāgatāḥ kim !",
    "948:200": "kārya mayā kimapi ced bhavatāmabhīṣṭa",
    "948:201": "māvedyatāmalamiha stutisampadeti||50||",
    "948:202": "te'pi prasannamanasaḥ praṇipatya tasmai",
    "948:203": "vyajñāpayan vinayanamritapūrvakāyāḥ|",
    "948:204": "devādhideva ! jagatāmavabodhanāya,",
    "948:205": "santiṣṭhate samucito'vasarastaveti||51||",
    "948:206": "ākarṇya tadvacanamaśrutapūrvameṣāṃ",
    "948:207": "kālādicintanaparaḥ kṣaṇameṣa bhūtvā|",
    "948:208": "niścitya tat sakalameva nidhirguṇānāṃ",
    "948:209": "pratyabravīt punaramūn prathitāpadānaḥ||52||",
    "948:210": "śuddhodanasya sutatāmahametya satyaṃ",
    "948:211": "sambodhanaṃ trijagatāṃ niyataṃ kariṣye|",
    "948:212": "aṅgairdhanairasubhirapyahametadeva",
    "948:213": "samprārthya puṇyanicayaṃ kṛtavān pureti||53||",
    "948:214": "iti kṛtavati tasmin satyasandhe pratijñāṃ",
    "948:215": "parahitaparabhāve pāramīpāraniṣṭhe|",
    "948:216": "pramuditamanasaste sphītaromāñcadaṇḍa-",
    "948:217": "pracayaniculitāṅgāḥ pratyagacchan yatheccham||54||",
    "948:218": "atha kānicideva vāsarāṇi kṣapayitvā tridive sa devarājaḥ|",
    "948:219": "vidadhe vividhavratojjvalāyāṃ pratisandhiṃ pṛthivīpatermahiṣyām||55||",
    "948:220": "iti buddhaghoṣaviracite padyacūḍāmaṇināmni mahākāvye dvitīyaḥ sargaḥ||"
}