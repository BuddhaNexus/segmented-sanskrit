{
    "909:0": "vajrayoginyāḥ piṇḍārthastutiḥ",
    "909:1": "siddhācārya-virūpādaviaracitā",
    "909:2": "om namo vajrayoginyai |",
    "909:3": "om namo buddhadharmasaṃghebhyaḥ |",
    "909:4": "om namo gurubuddhabodhisattvebhyaḥ |",
    "909:5": "om namo locanādidaśavajravilāsinībhyaḥ |",
    "909:6": "namo yamāntakādidaśakroḍha(dha) vīrebhyaḥ |",
    "909:7": "vārāhī śauṇḍinī caiva caṇḍālī ḍombinī tathā |",
    "909:8": "naṭanī rajakī brāhmī kapālinī ca sāsa(śāśva) tā || 1 ||",
    "909:9": "amṛtā'mṛtakuṇḍalī ca jñānajyotiḥprakāśikā |",
    "909:10": "sarvāśāvīradevīnāmiyamekā mahāsukhā || 2 ||",
    "909:11": "śūnyatā gīyate cāsau parāśaktiḥ parātparā |",
    "909:12": "amṛtordhvamanā divyā upāyā nityavāhinī || 3 ||",
    "909:13": "khecarī bhūcarī caiva pātālavāsinī tathā |",
    "909:14": "pravi(ti)ṣṭhāpūraṇī nityaṃ trailokyakṣobhatī(bhiṇī) tathā || 4 ||",
    "909:15": "bindunādakalā devī candrasūryātmikā hi sā",
    "909:16": "(nityā) nairmāṇikī caiva saṃbhāgī ca mahāsukhā || 5 ||",
    "909:17": "bindunādakalātītā prajñāpāramitā matā |",
    "909:18": "sarvabhāvasvabhāvā hi sarvabhāvavivarjitā || 6 ||",
    "909:19": "pralayotpattihīnā ca pralayotpattikāriṇī |",
    "909:20": "śāśvatatvāt sthitā proktā śāśvatena ca varjitā || 7 ||",
    "909:21": "gambhīrā''liṅgitodārā mahārthā svadhimuktikā |",
    "909:22": "śūnyatātrayahīnā ca prabhāsvarasvarūpiṇī || 8 ||",
    "909:23": "ekārākṣararūpā ca vaṃkārākṣarasaṃgatā |",
    "909:24": "vicitrādikṣaṇairyuktā caturānandarūpiṇī || 9 ||",
    "909:25": "bāhyamaṇḍalacakre'pi sphurantī ca trikāyataḥ |",
    "909:26": "kāyavākacittabhāveṣu kāyavākacittabhūṣaṇī || 10 ||",
    "909:27": "atītya kāyavākacittaiḥ samatvena ca madhyagā |",
    "909:28": "nairātmyarūpiṇī devī tathatāyāṃ pratiṣṭhitā || 11 ||",
    "909:29": "kamalakuliśākrāntaśūnyatātrayarūpiṇī |",
    "909:30": "lalanārasanāyogādavadhūtī mahāsukhā || 12 ||",
    "909:31": "saṃsāratāraṇī caiṣāṃ tathā tāsāṃ pratītyajā |",
    "909:32": "yāṃ labdhvā yoginīṃ muktā bhavasaṃsārabandhanāt || 13 ||",
    "909:33": "ityeṣā ṛddhidā proktā siddhidā caiva yoginī |",
    "909:34": "mokṣadvāraparā caiva satatābhyāsakāriṇām || 14 ||",
    "909:35": "vajravat kurute dehaṃ rasasiddhiṃ dadāti ca |",
    "909:36": "gurupādaprasādena labdheyaṃ vajrayoginī || 15 ||",
    "909:37": "ya(e) tasyāḥ pāṭhamātreṇa puṇyasaṃbhāramādarāt |",
    "909:38": "prāpnoti satataṃ yogī jñānasaṃbhārasaṃbhṛtaḥ || 16 ||",
    "909:39": "śrīguhyasamayatantre piṇḍārthāḥ ṣoḍaśaślokāstrikāyavajra-",
    "909:40": "yoginyāḥ samāptāḥ |",
    "909:41": "kṛtiriyaṃ siddhācāryaśrīvirupādānāmiti |"
}