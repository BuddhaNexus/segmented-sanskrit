{
  "SA05n904:0": "kalyāṇatriṃśatikāstotram",
  "SA05n904:1": "evaṃkārasamāpannā kāyavākacittabuddhitaḥ |",
  "SA05n904:2": "karomi satataṃ tasyā nutiṃ pūjāṃ pradakṣiṇām || 1 ||",
  "SA05n904:3": "yā devī sarvasattvānāṃ sṛṣṭisaṃhārakāriṇī |",
  "SA05n904:4": "namastasyai namastasyai namastasyai namo namaḥ || 2 ||",
  "SA05n904:5": "yā devī sarvabhūtānāṃ pratipāle pratiṣṭhitā |",
  "SA05n904:6": "namastasyai namastasyai namastasyai namo namaḥ || 3 ||",
  "SA05n904:7": "yā devī devadevīnāṃ devatārūpiṇī sthitā |",
  "SA05n904:8": "namastasyai namastasyai namastasyai namo namaḥ || 4 ||",
  "SA05n904:9": "yā devī daityadurdāntadāharūpā bhayānakā |",
  "SA05n904:10": "namastasyai namastasyai namastasyai namo namaḥ || 5 ||",
  "SA05n904:11": "yā devī sarvanāgānāṃ sahasramukhanāginī |",
  "SA05n904:12": "namastasyai namastasyai namastasyai namo namaḥ || 6 ||",
  "SA05n904:13": "yā devī saptapātāle śāntarūpeṇa saṃsthitā |",
  "SA05n904:14": "namastasyai namastasyai namastasyai namo namaḥ || 7 ||",
  "SA05n904:15": "yā devī naranārīṇāṃ vedamātā cidambikā |",
  "SA05n904:16": "namastasyai namastasyai namastasyai namo namaḥ || 8 ||",
  "SA05n904:17": "yā devī kṣatriṇīvaiśyāśūdrījātiprapūjitā |",
  "SA05n904:18": "namastasyai namastasyai namastasyai namo namaḥ || 9 ||",
  "SA05n904:19": "yā devī pretalokānāṃ pālanāya maharddhikā |",
  "SA05n904:20": "namastasyai namastasyai namastasyai namo namaḥ || 10 ||",
  "SA05n904:21": "yā devī sarvatiryakṣu tāriṇī tāpanāśinī |",
  "SA05n904:22": "namastasyai namastasyai namastasyai namo namaḥ || 11 ||",
  "SA05n904:23": "yā devī nārakīyāṇāṃ duḥkhabhājāṃ ca mātṛkā |",
  "SA05n904:24": "namastasyai namastasyai namastasyai namo namaḥ || 12 ||",
  "SA05n904:25": "yā devī ṣoḍaśasaṃkhyanarakāṇāṃ vināśinī |",
  "SA05n904:26": "namastasyai namastasyai namastasyai namo namaḥ || 13 ||",
  "SA05n904:27": "yā devī dveṣirāgīṇāṃ mohināṃ durdurīkṛtā |",
  "SA05n904:28": "namastasyai namastasyai namastasyai namo namaḥ || 14 ||",
  "SA05n904:29": "yā devī kāmakrodhābhyāṃ krodharūpeṇa saṃsthitā |",
  "SA05n904:30": "namastasyai namastasyai namastasyai namo namaḥ || 15 ||",
  "SA05n904:31": "yā devī lobhalābhānāṃ laṅghanāya vilambitā |",
  "SA05n904:32": "namastasyai namastasyai namastasyai namo namaḥ || 16 ||",
  "SA05n904:33": "yā devī māyāyā mātā mātṝṇāṃ vajrayogiṇī |",
  "SA05n904:34": "namastasyai namastasyai namastasyai namo namaḥ || 17 ||",
  "SA05n904:35": "yā devī rājacaurāgnisiṃhaśatruvināśinī |",
  "SA05n904:36": "namastasyai namastasyai namastasyai namo namaḥ || 18 ||",
  "SA05n904:37": "yā devī jalanāgāhidurbhikṣabhayatāriṇī |",
  "SA05n904:38": "namastasyai namastasyai namastasyai namo namaḥ || 19 ||",
  "SA05n904:39": "yā devī pañcabhūtānāṃ candre sūrye pratiṣṭhitā |",
  "SA05n904:40": "namastasyai namastasyai namastasyai namo namaḥ || 20 ||",
  "SA05n904:41": "yā devī bhautikī velā ālikālivicāriṇī |",
  "SA05n904:42": "namastasyai namastasyai namastasyai namo namaḥ || 21 ||",
  "SA05n904:43": "yā devī sarvapīṭheśī kṣetrarūpopachandikā |",
  "SA05n904:44": "namastasyai namastasyai namastasyai namo namaḥ || 22 ||",
  "SA05n904:45": "yā devī pīlavākhyātā upapīlavamīlitā |",
  "SA05n904:46": "namastasyai namastasyai namastasyai namo namaḥ || 23 ||",
  "SA05n904:47": "yā devī dvādaśe cakre rāśilagnavibhūṣiṇī |",
  "SA05n904:48": "namastasyai namastasyai namastasyai namo namaḥ || 24 ||",
  "SA05n904:49": "yā devī ṣaḍaṃge deśe prodbuddhamukhacandrikā |",
  "SA05n904:50": "namastasyai namastasyai namastasyai namo namaḥ || 25 ||",
  "SA05n904:51": "yā devī caṇḍacakreṣu gāminī parameśvarī |",
  "SA05n904:52": "namastasyai namastasyai namastasyai namo namaḥ || 26 ||",
  "SA05n904:53": "yā devī kāyavākacitte mantrarūpeṇa gāminī |",
  "SA05n904:54": "namastasyai namastasyai namastasyai namo namaḥ || 27 ||",
  "SA05n904:55": "yā devī nirmalātmā śrīrṛddhisiddhibalapradā |",
  "SA05n904:56": "namastasyai namastasyai namastasyai namo namaḥ || 28 ||",
  "SA05n904:57": "yā devo sarvajantūnāṃ sadā maṅgalakāriṇī |",
  "SA05n904:58": "namastasyai namastasyai namastasyai namo namaḥ || 29 ||",
  "SA05n904:59": "ya idaṃ paṭhate dhīmān devyā bhaktisamanvitaḥ |",
  "SA05n904:60": "buddhatvaṃ labhate śīghraṃ kalyāṇaṃ maṅgalaṃ śivam || 30 ||",
  "SA05n904:61": "śrī trikāyanivāsinīvajradevyāḥ kalyāṇatriṃśatikāstotraṃ samāptam |"
}