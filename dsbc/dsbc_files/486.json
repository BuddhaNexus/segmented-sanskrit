    "486:270": "duḥkhe'vaimukhyam |",
    "486:271": "saṃskārāṇāmanādānatā |",
    "486:272": "bhūtavarṇe'saṅgaḥ |",
    "486:273": "abhūtavarṇe'dhivāsanatā |",
    "486:274": "gṛhasthapravrajitairasaṃstavaḥ |",
    "486:275": "agocaravivarjanam |",
    "486:276": "gocarapracāraḥ |",
    "486:277": "ācārasaṃpat |",
    "486:278": "anācāravivarjanatā |",
    "486:279": "kulānāmadūṣaṇatā |",
    "486:280": "śāsanasyārakṣaṇatā |",
    "486:281": "alpabhāṣaṇatā |",
    "486:282": "mitabhāṣaṇatā |",
    "486:283": "prativacanakauśalyam |",
    "486:284": "pratyarthikanigrahaḥ |",
    "486:285": "kālapratikramaṇatā |",
    "486:286": "akālavivarjanatā |",
    "486:287": "pṛthagjaneṣvaviśvāsaḥ |",
    "486:288": "duḥkhitānāmaparibhavanatā,",
    "486:289": "tebhyaśca dhanapradānam |",
