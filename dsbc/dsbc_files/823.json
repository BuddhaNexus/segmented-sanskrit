{
    "823:0": "potalakāṣṭakam",
    "823:1": "jaya siddhasurāsuralokaguruṃ",
    "823:2": "kamalāsanasaṃsthitapādayugam |",
    "823:3": "daśapāramitādikapadmadharaṃ",
    "823:4": "praṇamāmi sadā varakāruṇikam || 1 ||",
    "823:5": "bahukoṭinarādhipalabdhavaraṃ",
    "823:6": "jaya susthitasusthitanāthamunim |",
    "823:7": "bhavasāgarapāragatādhikaraṃ",
    "823:8": "śaraṇāgatasattvahitārthakaram || 2 ||",
    "823:9": "kamalāsanacārusurupadharaṃ",
    "823:10": "amitābhatathāgatamaulidharam |",
    "823:11": "kamalojjvalabhūṣitagātravaraṃ",
    "823:12": "navabhāskaraśobhanadīptikaram || 3 ||",
    "823:13": "mṛdutāmranakhāṅguliśobhamukhaṃ",
    "823:14": "śaśibimbasamaprabhasaumyamukham |",
    "823:15": "ṣaḍabhijñaguṇākaravandyatanuṃ",
    "823:16": "varasusthitanūpurahārayutam || 4 ||",
    "823:17": "maṇikuṇḍalamaṇḍitagaṇḍayugaṃ",
    "823:18": "ghanagarjitatulyagabhīrarutam |",
    "823:19": "girigahvaradyotanivāsakṛtaṃ",
    "823:20": "vṛṣavāhanahaṃsamṛgāyuśatam || 5 ||",
    "823:21": "ṛṣidevagaṇādhipasaṃstutikaṃ",
    "823:22": "jinarūpadharātmakabodhiparam |",
    "823:23": "dvipadottamanāthasurādhipatiṃ",
    "823:24": "bahusattvavimocanamuktikaram || 6 ||",
    "823:25": "dhvajachatrasuśobhitavāmakaraṃ",
    "823:26": "malayāgiricandanadhūparatim |",
    "823:27": "kamalotpalapāṭalidāmadharaṃ",
    "823:28": "praṇamāmyatha pāṭalivṛddhikaram || 7 ||",
    "823:29": "jvararogavivarjitakuṣṭhaharaṃ",
    "823:30": "bahubodhimanāghanabhadrakaram |",
    "823:31": "navadurgatināśanamārgapradaṃ",
    "823:32": "karuṇā-tathatādvayaviśvapatim || 8 ||",
    "823:33": "śrīāryāvalokiteśvarasya potalakāṣṭakaṃ samāptam |"
}