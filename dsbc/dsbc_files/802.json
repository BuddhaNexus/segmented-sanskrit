{
    "802:0": "ādibuddhadvādaśakastotram",
    "802:1": "om nama ādibuddhāya",
    "802:2": "namaste buddharūpāya dharmarūpāya te namaḥ|",
    "802:3": "namaste saṃgharūpāya pañcabuddhātmane namaḥ|| 1||",
    "802:4": "pṛthvīrūpāyābrūpāya tejorūpāya te namaḥ|",
    "802:5": "namaste vāyurūpāyākāśarūpāya te namaḥ|| 2||",
    "802:6": "brahmaṇe sattvarūpāya rajorūpāya viṣṇave|",
    "802:7": "tamorūpamaheśāya jñānarūpāya te namaḥ|| 3||",
    "802:8": "prajñopāyātmarūpāya guhyarūpāya te namaḥ|",
    "802:9": "digrūpalokapālāya viśvarūpāya te namaḥ|| 4||",
    "802:10": "cakṣūrūpāya karṇāya ghrāṇarūpāya jihvake|",
    "802:11": "kāyarūpāya śrīdharmarūpāya manase namaḥ|| 5||",
    "802:12": "namaste rūparūpāya rasarūpāya te namaḥ|",
    "802:13": "gandharūpa-śabdarūpa-sparśarūpāya te namaḥ|| 6||",
    "802:14": "dharmarūpadhārakāya ṣaḍindriyātmane namaḥ|",
    "802:15": "māṃsāsthimedamajjānāṃ saṃghātarūpiṇe namaḥ|| 7||",
    "802:16": "rūpāya jaṅgamānāṃ te sthāvarāṇāṃ ca murtaye|",
    "802:17": "tiraścāṃ moharūpāya rūpāyāścaryamūrtaye|| 8||",
    "802:18": "sṛṣṭikartre janmarūpa kālarūpāya mṛtyave|",
    "802:19": "bhavyāya vṛddharūpāya bālāya te namo namaḥ|| 9||",
    "802:20": "prāṇāpānasamānodānavyānamūrtaye namaḥ|",
    "802:21": "varṇāpavarṇarūpāya bhoktre tanmūrtaye namaḥ|| 10||",
    "802:22": "dinarūpāya sūryāya candrāya rātrirūpiṇe|",
    "802:23": "tithirūpāya nakṣatrayogavārādimurtaye|| 11||",
    "802:24": "bāhyābhyantararūpāya laukikāya namonamaḥ|",
    "802:25": "nairvāṇāya namastubhyaṃ bahurūpāya te namaḥ|| 12||",
    "802:26": "ādibuddhadvādaśakaṃ puṇyaṃ prātaḥ paṭhiṣyati|",
    "802:27": "yadicchati labhennūnaṃ manujo nityaniścayaḥ|| 13||",
    "802:28": "śrīmañjuśrīkṛtamādibuddhadvādaśakastotraṃ samāptam|"
}