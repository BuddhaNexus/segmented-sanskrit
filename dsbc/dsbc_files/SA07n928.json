{
  "SA07n928:0": "CANTO XIV",
  "SA07n928:1": "tato mārabalaṃ jitvā",
  "SA07n928:2": "dhairyeṇa ca śamena ca |",
  "SA07n928:3": "paramārtha vijijñāsuḥ",
  "SA07n928:4": "sa dadhyau dhyānakovidaḥ ||1 ||",
  "SA07n928:5": "sarveṣu dhyānavidhiṣu",
  "SA07n928:6": "prāpya caiśvaryamuttamam |",
  "SA07n928:7": "sasmāra prathame yāme",
  "SA07n928:8": "pūrvajanmaparaṃparām ||2 ||",
  "SA07n928:9": "amutrāhamayaṃ nāma",
  "SA07n928:10": "cyutastasmādihāgataḥ |",
  "SA07n928:11": "uiti janmasahastrāṇi",
  "SA07n928:12": "sasmārānubhavanniva ||3 ||",
  "SA07n928:13": "smṛtvā janma ca mṛtyuṃ ca",
  "SA07n928:14": "tāsu tāsūpapattiṣu |",
  "SA07n928:15": "tataḥ sattveṣu kāruṃṇyaṃ",
  "SA07n928:16": "cakāra karuṇātmakaḥ ||4 ||",
  "SA07n928:17": "kṛtveha svajanotsarga",
  "SA07n928:18": "punaranyatra ca kriyāḥ |",
  "SA07n928:19": "atrāṇaḥ khalu loko'yaṃ",
  "SA07n928:20": "paribhramati cakravat ||5 ||",
  "SA07n928:21": "ityevaṃ smaratastasya",
  "SA07n928:22": "babhūva niyatātmanaḥ |",
  "SA07n928:23": "kadalīgarbhaniḥsāraḥ |",
  "SA07n928:24": "saṃsāra iti niścayaḥ ||6 ||",
  "SA07n928:25": "dvitīye tvāgate yāme",
  "SA07n928:26": "so'dvitīyaparākramaḥ |",
  "SA07n928:27": "divyaṃ lebhe paraṃ cakṣuḥ",
  "SA07n928:28": "sarvacakṣuṣmatāṃ varaḥ ||7 ||",
  "SA07n928:29": "tatastena sa divyena",
  "SA07n928:30": "pariśuddhena cakṣuṣā |",
  "SA07n928:31": "dadarśa nikhilaṃ loka-",
  "SA07n928:32": "mādarśa iva nirmale ||8 ||",
  "SA07n928:33": "sattvānāṃ paśyatastasya",
  "SA07n928:34": "nikṛṣṭotkṛṣṭakarmaṇām |",
  "SA07n928:35": "pracyutiṃ copapattiṃ ca",
  "SA07n928:36": "vavṛdhe karuṇātmatā ||9 ||",
  "SA07n928:37": "ime duṣkṛtakarmāṇaḥ",
  "SA07n928:38": "prāṇino yāni durgatim |",
  "SA07n928:39": "ime'nye śubhakarmāṇaḥ",
  "SA07n928:40": "pratiṣṭhante tripiṣṭape ||10 ||",
  "SA07n928:41": "upapannāḥ pratibhaye",
  "SA07n928:42": "narake bhṛśadāruṇe |",
  "SA07n928:43": "amī duḥkhairbahuvidhaiḥ",
  "SA07n928:44": "pīḍyante kṛpaṇaṃ bata ||11 ||",
  "SA07n928:45": "pāyyante kvathitaṃ keci-",
  "SA07n928:46": "dagnivarṇamayorasam |",
  "SA07n928:47": "āropyante ruvānto'nye",
  "SA07n928:48": "niṣṭaptastambhamāyasam ||12 ||",
  "SA07n928:49": "pacyante piṣṭavatkeci-",
  "SA07n928:50": "dayaskumbhīṣvavāṅmukhāḥ |",
  "SA07n928:51": "dahyante karuṇaṃ keci-",
  "SA07n928:52": "ddīpteṣvaṅgārarāśiṣu ||13 ||",
  "SA07n928:53": "kecittīkṣṇairayodaṃṣṭrai-",
  "SA07n928:54": "rbhakṣyante dāruṇaiḥ śvabhiḥ |",
  "SA07n928:55": "keciddhṛṣṭairayastuṇḍai-",
  "SA07n928:56": "rvāyasairāyasairiva ||14 ||",
  "SA07n928:57": "keciddāhapariśrāntāḥ",
  "SA07n928:58": "śītacchāyābhikāṅikṣaṇaḥ |",
  "SA07n928:59": "asipattravanaṃ nīlaṃ",
  "SA07n928:60": "baddhā iva viśantyamī ||15 ||",
  "SA07n928:61": "pāṭyante dāruvatkeci-",
  "SA07n928:62": "tkuṭhārairbaddhabāhavaḥ |",
  "SA07n928:63": "duḥkhe'pi na vipacyante",
  "SA07n928:64": "karmabhirdhāritāsavaḥ ||16 ||",
  "SA07n928:65": "sukhaṃ syāditi yatkarma",
  "SA07n928:66": "kṛtaṃ duḥkhanivṛttaye |",
  "SA07n928:67": "phalaṃ tasyedamavaśai-",
  "SA07n928:68": "rduḥkhamevopabhujyate ||17 ||",
  "SA07n928:69": "sukhārthamaśubhaṃ kṛtvā",
  "SA07n928:70": "ya ete bhṛśaduḥkhitāḥ |",
  "SA07n928:71": "āsvādaḥ sa kimeteṣāṃ",
  "SA07n928:72": "karoti sukhamaṇvapi ||18 ||",
  "SA07n928:73": "hasadbhiryatkṛtaṃ karma",
  "SA07n928:74": "kaluṣaṃ kaluṣātmabhiḥ |",
  "SA07n928:75": "etatpariṇate kāle",
  "SA07n928:76": "krośadbhiranubhūyate ||19 ||",
  "SA07n928:77": "yadyevaṃ pāpakarmāṇaḥ",
  "SA07n928:78": "paśyeyuḥ karmaṇāṃ phalam |",
  "SA07n928:79": "vameyuruṣṇaṃ rudhiraṃ",
  "SA07n928:80": "marmasvabhihatā iva ||20 ||",
  "SA07n928:81": "ime'nye karmabhiścitrai-",
  "SA07n928:82": "ścittavispandasaṃbhavaiḥ |",
  "SA07n928:83": "tiryagyonau vicitrāyāḥ-",
  "SA07n928:84": "mupapannāstapasvinaḥ ||21 ||",
  "SA07n928:85": "māṃsatvagbāladantārtha",
  "SA07n928:86": "vairādapi madādapi |",
  "SA07n928:87": "hanyante kṛpaṇaṃ yatra",
  "SA07n928:88": "bandhūnāṃ paśyatāmapi ||22 ||",
  "SA07n928:89": "aśaknuvanto'pyavaśāḥ",
  "SA07n928:90": "kṣuttarṣaśramapīḍitāḥ |",
  "SA07n928:91": "go'śvabhūtāśca vāhyante",
  "SA07n928:92": "pratodakṣatamūrtayaḥ ||23 ||",
  "SA07n928:93": "vāhyante gajabhūtāśca",
  "SA07n928:94": "valīyāṃso'pi durbalaiḥ |",
  "SA07n928:95": "aṅkaśakliṣṭamūrdhāna-",
  "SA07n928:96": "stāḍitāḥ pādapāṣṇibhiḥ ||24 ||",
  "SA07n928:97": "satsvapyanyeṣu duḥkheṣu",
  "SA07n928:98": "duḥkhaṃ yatra viśeṣataḥ |",
  "SA07n928:99": "parasparavirodhācca",
  "SA07n928:100": "parādhīnatayaiva ca ||25 ||",
  "SA07n928:101": "khasthāḥ khasthairhi bādhyante",
  "SA07n928:102": "jalasthā jalacāribhiḥ |",
  "SA07n928:103": "sthalasthāḥ sthalasaṃsthaiśca",
  "SA07n928:104": "prāpya caivetaretaraiḥ ||26 ||",
  "SA07n928:105": "upapannāstathā ceme",
  "SA07n928:106": "mātsaryākrāntacetasaḥ |",
  "SA07n928:107": "pitṛloke nirāloke",
  "SA07n928:108": "kṛpaṇaṃ bhuñjate phalam ||27 ||",
  "SA07n928:109": "sūcīchidropamamukhāḥ",
  "SA07n928:110": "parvatopamakukṣayaḥ |",
  "SA07n928:111": "kṣuttarṣajanitairduḥkhai",
  "SA07n928:112": "pīḍyante duḥkhabhāginaḥ ||28 ||",
  "SA07n928:113": "āśayā samatikrāntā",
  "SA07n928:114": "dhāryamāṇāḥ svakarmabhiḥ |",
  "SA07n928:115": "labhante na hyamī bhoktuṃ",
  "SA07n928:116": "praviddhānyaśucīnyapi ||29 ||",
  "SA07n928:117": "puruṣo yadi jānīta",
  "SA07n928:118": "mātsaryasyedṛśaṃ phalam |",
  "SA07n928:119": "sarvathā śibivaddadyā-",
  "SA07n928:120": "ccharīrāvayavānapi ||30 ||",
  "SA07n928:121": "ime'nye narakaprakhye",
  "SA07n928:122": "garbhasaṃjñe'śucihrade |",
  "SA07n928:123": "upapannā manuṣyeṣu",
  "SA07n928:124": "duḥkhamarchanti jantavaḥ ||31 ||"
}