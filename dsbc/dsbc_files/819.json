{
    "819:0": "lokanāthastotram",
    "819:1": "kalpādike bhavasi ko hi mamāgabhāva",
    "819:2": "sarvasvasāra karuṇāmaya viśvamūrte |",
    "819:3": "kāryādike praṇamatīti samantakaṃ tvāṃ",
    "819:4": "śrīlokanātha tava pādayugaṃ name'ham || 1 ||",
    "819:5": "ākṛṣṇakena rajasā vinivartamāna-",
    "819:6": "ścāyāsi saumya sakalaḥ prativāsare ca |",
    "819:7": "hemasvarūparathakena samujjvalena",
    "819:8": "śrīlokanātha tava pādayugaṃ name'ham || 2 ||",
    "819:9": "brahmā tvameva hi sa viprakulaprasiddho",
    "819:10": "viṣṇuśca vaiṣṇavamate varadharmaketuḥ |",
    "819:11": "sarvajñako'si vimate prabhavo'vyayaśca",
    "819:12": "śrīlokanātha tava pādayugaṃ name'ham || 3 ||",
    "819:13": "bauddhānvaye bhavasi vajrakasūryarūpo",
    "819:14": "yogeśvaro hi śubhayogakamārgakeṣu |",
    "819:15": "gaṅgādharo bhavabhayasya vināśakāri",
    "819:16": "śrīlokanātha tava pādayugaṃ name'ham || 4 ||",
    "819:17": "kāruṇyabhāvahṛdayaḥ sahajaḥ saroci-",
    "819:18": "rvicchinnakalmaṣacayo guṇasāgaraśca |",
    "819:19": "cintāmaṇistvamasi lokaguruḥ kṛpeśa",
    "819:20": "śrīlokanātha tava pādayugaṃ name'ham || 5 ||",
    "819:21": "bandhūkavarṇa bahurūpa viśālanetra",
    "819:22": "sarvaprasūtikṛtaniṣkṛtikaḥ sudanta |",
    "819:23": "tvaṃ padmapāṇi vimalottama mitrarūpaḥ",
    "819:24": "śrīlokanātha tava pādayugaṃ name'ham || 6 ||",
    "819:25": "tava bahulacaritraṃ kaḥ samartho'sti vaktuṃ",
    "819:26": "tadapi mukharabhāvaiḥ stūyase tvaṃ mayātra |",
    "819:27": "yadapi padamaśuddhaṃ sarvametat kṣamasva",
    "819:28": "stutiriti kusumasrak bhaktimātrārcanaṃ syāt || 7 ||",
    "819:29": "śrīmadāryāvalokiteśvarabhaṭṭārakasya stotraṃ samāptam |"
}