{
    "1193:0": "6. prajñāpāramitāsamāsaḥ",
    "1193:1": "puṇyāni dānaprabhṛtīnyamūni",
    "1193:2": "prajñāsanāthānyadhikaṃ vibhānti|",
    "1193:3": "hiraṇmayānīva vibhūṣaṇāni",
    "1193:4": "pratyuptaratnadyutibhāsvarāṇi || 1 ||",
    "1193:5": "kriyāsu sāmarthyaguṇaṃ hi teṣāṃ",
    "1193:6": "prajñaiva vistāriṇamādadhāti|",
    "1193:7": "svārthapravṛttau viśadakramāṇāṃ",
    "1193:8": "yathā manaḥsaṃtatirindriyāṇāṃ || 2 ||",
    "1193:9": "kriyāsvayogyāni śarīrayantrāṇyā-",
    "1193:10": "yurviyuktāni yathā na bhānti|",
    "1193:11": "tathaiva kāryāṇi na bhānti loke",
    "1193:12": "prajñāviyogena jadīkṛtāni || 3 ||",
    "1193:13": "śraddhādikānāmapi cendriyāṇāṃ",
    "1193:14": "prajñāgraṇī buddhirivendriyāṇām|",
    "1193:15": "guṇāguṇān vetti hi tatsanāthaḥ",
    "1193:16": "kleśakṣaye naipuṇametyataśca || 4 ||",
    "1193:17": "prajñāviyogāt phalalālasānāṃ",
    "1193:18": "naiva svatodānaviśuddhirasti|",
    "1193:19": "tyāgaṃ parārthaṃ hi vadanti dānaṃ",
    "1193:20": "śeṣastu vṛddhyārthamiva prayogaḥ || 5 ||",
    "1193:21": "prajñāsamunmīlitacakṣuṣastu",
    "1193:22": "dattvā svamāṃsānyapi bodhisattvāḥ|",
    "1193:23": "naivonnatiṃ nāvanatiṃ prayānti",
    "1193:24": "bhaiṣajyavṛkṣā iva nirviklapāḥ || 6 ||",
    "1193:25": "evaṃ sa bhūmiṃ prathamāmupaiti",
    "1193:26": "lokottarasyārthavidhipratiṣṭhām|",
    "1193:27": "akrodhanaḥ prītisamṛddhacetā",
    "1193:28": "dānairmahadbhirjagadarthacetāḥ || 7 ||",
    "1193:29": "prāyeṇa yasyāṃ balacakravartī",
    "1193:30": "bhavatyasaṃhāryamatiśca bodheḥ|",
    "1193:31": "prajñāguṇādeśitasatpatho'tha",
    "1193:32": "karmeṇa bhūmiṃ vimalāmupaiti || 8 ||",
    "1193:33": "yasyāṃ prakṛtyaiva viśuddhaśīlaśca-",
    "1193:34": "turmahādvīpapatiḥ sa bhūtvā|",
    "1193:35": "narendracūḍāmaṇisatkṛtājñaḥ",
    "1193:36": "sūryārhatāmeti yathā munīndraḥ || 9 ||",
    "1193:37": "tataḥ paraṃ kāmiṣu daivateṣu",
    "1193:38": "loke'pi ca dvitrisahasrasaṃkhye|",
    "1193:39": "aiśvaryamāpnoti tataḥ paraṃ ca",
    "1193:40": "bhūmiṃ viśodhya prabhavāṃ prabhāyāḥ || 10 ||",
    "1193:41": "śīlasya śuddhiḥ kuta eva tasya",
    "1193:42": "yaḥ prajñayā nāpahṛtāndhakāraḥ|",
    "1193:43": "prāyeṇa śīlāni hi tadviyogād",
    "1193:44": "āmarṣadoṣaiḥ kaluṣīkriyante || 11 ||",
    "1193:45": "nātmārthamapyasti tu yasya śīlaṃ",
    "1193:46": "prājñasya tasyāsti kathaṃ parārtham|",
    "1193:47": "yo dṛṣṭadoṣo bhavabandhanānāṃ",
    "1193:48": "lokān samastāṃstata ujjihīrṣuḥ || 12 ||",
    "1193:49": "prajñāvipakṣairhṛdi soparāge",
    "1193:50": "kṣamāguṇaḥ kena dhṛtiṃ labheta|",
    "1193:51": "guṇāguṇāavekṣaṇakātarākṣe",
    "1193:52": "khyāto guṇairvīrā iva kṣitīśe || 13 ||",
    "1193:53": "prajñānvitānāṃ tu parāpakārāḥ",
    "1193:54": "kṣamāguṇāḥ sthairyakarā bhavanti|",
    "1193:55": "bhadrātmakānāmiva vāraṇānāṃ",
    "1193:56": "karamāśrayā naikavidhā viṣeṣāḥ || 14 ||",
    "1193:57": "niṣkevalaṃ vīryamapi śramāya",
    "1193:58": "prajñāsanāthasya tu tasya kārye|",
    "1193:59": "anuttaraḥ siddhiguṇo'bhyudeti",
    "1193:60": "hartā tadutthasya pariśramasya || 15 ||",
    "1193:61": "yasmāt paraṃ sūkṣamataraṃ na kiṃ cid",
    "1193:62": "yannaipuṇānāṃ paramaḥ prakarṣaḥ|",
    "1193:63": "yatkāmadoṣādibhirāvṛtānāṃ",
    "1193:64": "manaḥpathaṃ naiva kadā cideti || 16 ||",
    "1193:65": "tad dhyānamekāntasukhābhirāmaṃ",
    "1193:66": "kathaṃ pravekṣyantyastāṃ manāṃsi|",
    "1193:67": "sthūlāni doṣopacayairmahadbhiḥ",
    "1193:68": "prajñotpathaṃ nyāyamivāśritānī || 17 ||",
    "1193:69": "prajñānirudyogamaterhi dṛṣṭirnā-",
    "1193:70": "yāti śuddhiṃ tadṛte na śīlam|",
    "1193:71": "samyakasmādhis tadṛte na labhyo",
    "1193:72": "duḥkhakṣayastadvirahāttathaiva || 18 ||",
    "1193:73": "prājñastu doṣādbhayamīkṣamāṇaḥ",
    "1193:74": "sukhānubaddhaṃ ca sukhaṃ guṇebhyaḥ|",
    "1193:75": "vihāya doṣāñjagadarthakāmo",
    "1193:76": "guṇābhirāmeṇa pathā prayāti || 19 ||",
    "1193:77": "samudyatastena samādhimetya",
    "1193:78": "prāpnoti vākkāyamanoviśuddhīḥ|",
    "1193:79": "ato'navadyena balena yuktaḥ",
    "1193:80": "pravartate lokahitodayeṣu || 20 ||",
    "1193:81": "dānena cābhīpsitabhūyasaiva",
    "1193:82": "priyairadīnairvacanāmṛtaiśca|",
    "1193:83": "naiṣkāraṇorjasvalayā ca vṛttyā",
    "1193:84": "parārthacaryāsu samaṃ samantāt || 21 ||",
    "1193:85": "sāmānyamartheṣu ca darśayitvā",
    "1193:86": "premṇā vaśīkṛtya manāṃsi teṣām|",
    "1193:87": "karoti nirvāṇasukhe pratiṣṭhāṃ",
    "1193:88": "prajñāguṇāvyāhatadharmacakraḥ || 22 ||",
    "1193:89": "prajñādyarogaiśca balairamībhira-",
    "1193:90": "dhyāsitaṃ nābhyupayātumīsā|",
    "1193:91": "ajīvikādurgatimṛtyunindā-",
    "1193:92": "śāradyadoṣāśrayaṇī bhayārtiḥ || 23 ||",
    "1193:93": "bhayāni sarvāṇi hi doṣajāni",
    "1193:94": "prajñā na doṣaiḥ sahavāsameti|",
    "1193:95": "śaradvyapoḍhābhragavākṣapakṣā",
    "1193:96": "bhā bhāskarasyeva tamaḥpratānaiḥ || 24 ||",
    "1193:97": "sahasraraśmerudaye'pi yāni",
    "1193:98": "tamāṃsi rundhanti jagadgatāni|",
    "1193:99": "nāmaikaśeṣāṇi karoti tāni",
    "1193:100": "prajñāprabhāyāḥ prasaraprabhāvaḥ || 25 ||",
    "1193:101": "na tatra bhūyaḥ karaṇīyam asti",
    "1193:102": "yatra prabhā sā balatāmupaiti|",
    "1193:103": "yugāntakālānalasaṃhṛte hi",
    "1193:104": "loke na dagdhavyakathāḥ prathante || 26 ||",
    "1193:105": "jyotīṃṣi sarvāṇyapi saṃhitāni",
    "1193:106": "prajñāprabhāṃ nālamathopayātum|",
    "1193:107": "atastayā nāsti parātivṛddhirga-",
    "1193:108": "rīyasī vāparihāṇijātiḥ || 27 ||",
    "1193:109": "saṃpūrṇatāṃ yāti sukhena śikṣā",
    "1193:110": "śīlāya cittapraśamāya caiva|",
    "1193:111": "prajñābhiyuktasya yatastato'syāṃ",
    "1193:112": "sarvābhisāreṇa parākrameta || 28 ||",
    "1193:113": "yā skandhadhātvāyatanapravṛttau",
    "1193:114": "satyāśrayā pratyayitā parīkṣā|",
    "1193:115": "kālatraye'pyeṣa samāsayuktyā",
    "1193:116": "prajñāvadātairviṣayapraveśaḥ || 29 ||",
    "1193:117": "kīrtiṃ vitanvanti jinātmajānāṃ",
    "1193:118": "prajñāvadātāścaritapradeśāḥ|",
    "1193:119": "guṇadvīṣāmapyatiduṣkuhāṇāṃ",
    "1193:120": "romāñcitā vismayapāratantryāt || 30 ||",
    "1193:121": "prajñābalaṃ dīptataraprabhāvaṃ",
    "1193:122": "nālaṃ prasoḍhuṃ sabalo'pi māraḥ|",
    "1193:123": "prajñāṃśavo vibhramayānti cakṣurna",
    "1193:124": "draṣṭum īśo hi yataḥ sa eva || 31 ||",
    "1193:125": "kandarpanārācanipātasāhī",
    "1193:126": "prajñāmayaṃ varma vitatya citte|",
    "1193:127": "vyūḍhāni rūpaprabhṛtīnyanekānyeko-",
    "1193:128": "'pi nirbhīrabhibhūya yāti || 32 ||",
    "1193:129": "adhīrasātmyaṃ bhayaviklavaṃ vā",
    "1193:130": "mūḍhocitaṃ śokaparigrahaṃ vā|",
    "1193:131": "svalpātmacitteṣvavagāḍhamūlaṃ",
    "1193:132": "roṣoparāgaṃ parijihmitaṃ vā || 33 ||",
    "1193:133": "dīneṣu kārpaṇyamalīmasatvaṃ",
    "1193:134": "kṛtāspadaṃ rāgiṣu cāpalaṃ vā|",
    "1193:135": "tejovihīneṣvalasatvasattvaṃ",
    "1193:136": "samuddhateṣvapraśamātmakatvam || 34 ||",
    "1193:137": "tāṃstāṃśca līnānapi doṣaleśān",
    "1193:138": "pṛthagvidhiṣvāśrayagahvareṣu|",
    "1193:139": "samudbhavantyeva parākaroti",
    "1193:140": "prajñā pratijñeva jagaddhitārthā || 35 ||",
    "1193:141": "niveśya doṣakṣayadhīrasaumyāṃ",
    "1193:142": "bhavasya tasyopari dṛṣṭilakṣmīm|",
    "1193:143": "svayaṃ munīndrairabhiṣicyate yat",
    "1193:144": "prahlādinā vyākaraṇāmṛtena || 36 ||",
    "1193:145": "ūrṇāprabhābhiśca mahāmunīnāṃ",
    "1193:146": "niśīthacandradyutihāsinībhiḥ|",
    "1193:147": "yadājyadhārābhirivādhvaragnirvi-",
    "1193:148": "bhāti mūrdhanyabhiṣicyamānaḥ || 37 ||",
    "1193:149": "avāpya yasmān muniyauvarājyaṃ",
    "1193:150": "samaṃ samantād visṛtātmabhāvaḥ|",
    "1193:151": "lokasya duḥkhaṃ praśamatyayatnād",
    "1193:152": "rajo mahāmegha iva pravṛṣṭaḥ || 38 ||",
    "1193:153": "prajñāprabhāvopanataḥ sa sarvaḥ",
    "1193:154": "prabhāvisāraḥ sugatātmajānām|",
    "1193:155": "ko vismayo vātra sutapriyāyā",
    "1193:156": "mātuḥ samīyādyadiyaṃ vibhūtiḥ || 39 ||",
    "1193:157": "daśaprakāro'pi yadā munīnāṃ",
    "1193:158": "tadāśrayādeva balaprakarṣaḥ|",
    "1193:159": "udetyasādhāraṇasundaraśca",
    "1193:160": "śeṣo'pyasaṃkhyo guṇaratnarāśiḥ || 40 ||",
    "1193:161": "śāstrāṇi cakṣuḥpratimāni loke",
    "1193:162": "nidhānabhūtāṃśca kalāviśeṣān|",
    "1193:163": "mantrān paritrāṇakṛto vicitrān",
    "1193:164": "dharmavyavasthāśca pṛthagviśeṣāḥ || 41 ||",
    "1193:165": "paryāyacitraṃ ca vimokṣamārgaṃ",
    "1193:166": "tattacca lokasya hitopapādi|",
    "1193:167": "yadbodhisattvāḥ pravidarśayanti",
    "1193:168": "prajñāprabhāvābhyudayaḥ sa sarvaḥ || 42 ||",
    "1193:169": "divyapratispardhibhirindriyārthai-",
    "1193:170": "rnarendrabhāve'pi hi bodhisattvāḥ|",
    "1193:171": "na yadvirūpāṃ prakṛtiṃ vrajanti",
    "1193:172": "prajñā guṇāmātyasanāthatā sā || 43 ||",
    "1193:173": "paropakāraikarasā ca maitrī",
    "1193:174": "rāgoparāgaprativarjitā ca|",
    "1193:175": "parasya duḥkheṣu parā dayā ca",
    "1193:176": "na śokabhārālasatāṃ gatā ca || 44 ||",
    "1193:177": "anuddhātatvaṃ mudite'pi citte",
    "1193:178": "tamonirārambhamupekṣitaṃ ca|",
    "1193:179": "te te guṇā<ś>cābhyadhikaṃ vibhānti",
    "1193:180": "prajñāniruddhapratipakṣamārgāḥ || 45 ||",
    "1193:181": "ko nāma lokasya parārthasādhurduḥkhai-",
    "1193:182": "kahetūni tamāṃsi hanyāt|",
    "1193:183": "avyāhatā jñānaśayāśayeṣu",
    "1193:184": "prajñā na cet syādatisūryādīptiḥ || 46 ||",
    "1193:185": "tatprāptaye śrutam aśītivikalpacitraṃ",
    "1193:186": "saṃceyam āśrayasahaṃ gurumabhyupetya|",
    "1193:187": "dvātriṃśatā tadadhigamya vivardhayeta",
    "1193:188": "samyaṅmanaḥ samavadhānakṛtairviśeṣaiḥ || 47 ||",
    "1193:189": "alpaśruto'ndha iva vetti na bhāvanāyā",
    "1193:190": "mārgaṃ vicintayati kāni ca tadvihīnaḥ|",
    "1193:191": "tasmācchrutaṃ prati yateta tadāśrayā hi",
    "1193:192": "prajñā samudbhavati cintanabhāvanābhyām || 48 ||",
    "1193:193": "praśnairavigrahamukhaiśca kathāviśeṣairmī-",
    "1193:194": "māṃsayārthagativīkṣaṇayā svayaṃ ca|",
    "1193:195": "prajñāvivṛddhimabhitaḥ prayateta nityaṃ",
    "1193:196": "dhyānena tadguṇavivṛddhikareṇa caiva || 49 ||",
    "1193:197": "prajñābhyupāyavidhireṣa samāsatastu",
    "1193:198": "dhyānaṃ tadarthaniyataḥ śrutivistaraśca|",
    "1193:199": "tābhyāṃ samudbhavati hi prabhavo guṇānāṃ",
    "1193:200": "prajñāprabhāsamudayo'gnirivāraṇībhyāṃ || 50 ||",
    "1193:201": "vidvajjanācaritamārgasamāśrayācca",
    "1193:202": "saṃmohahetugahanāni vivarjayeta|",
    "1193:203": "tairāvṛto na hi vibhātyudayasthito'pi",
    "1193:204": "toyāvalambijaladāntaritaḥ śaśīva || 51 ||",
    "1193:205": "ālasyajṛmbhitamatitvam asatsahāyā",
    "1193:206": "nidrānivṛttiraviniścayaśīlatā ca|",
    "1193:207": "jñāne muneriva kutūhalitānivṛttirmithyā-",
    "1193:208": "bhimānaparisaṃkucitāśca pṛcchāḥ || 52 ||",
    "1193:209": "dainyena cātmaparitāpasamudbhavena",
    "1193:210": "vidvajjanābhigamanādarakātaratvam|",
    "1193:211": "mithyāvikalpapaṭutā vitathā ca dṛṣṭirmohāya",
    "1193:212": "tatpraśamanāya tu tadvipakṣāḥ || 53 ||",
    "1193:213": "skandheṣu sāyatanadhātuṣu satyayuktyo",
    "1193:214": "hetudbhaveṣu śucayānavinirṇaye ca|",
    "1193:215": "dharmeṣu kauśalamaśeṣata eva yacca",
    "1193:216": "prajñāprayogaviṣayo'ṣṭavikalpa eṣaḥ || 54 ||",
    "1193:217": "niḥsāraphenanicayairaviśeṣi rūpaṃ",
    "1193:218": "tisro'pi budbudalavā iva vedanāśca|",
    "1193:219": "saṃjñāpi kāmaguṇaviprasṛtān satṛṣṇān",
    "1193:220": "bālān mṛgāniva vilobhayate marīciḥ || 55 ||",
    "1193:221": "saṃskārajātirapi tulyaguṇā kadalyā",
    "1193:222": "vijñānato'pi na ca yuktatarāsti māyā|",
    "1193:223": "yanniśrayādbhramati naikavikalpaceṣṭaṃ",
    "1193:224": "bhūtābhibhūtakuṇapapratimaṃ śarīram || 56 ||",
    "1193:225": "nātmā tadīyamapi cātra na kiṃ cid asti",
    "1193:226": "saṃghāta eṣa vividhāśūcisaṃnidhānaḥ|",
    "1193:227": "bālān pralambhayata eva ca sattvasaṃjñā",
    "1193:228": "svacchandaceṣṭa iva yantravidhau suyukte || 57 ||",
    "1193:229": "ātmā na cakṣurapi ca kṣaṇabhaṅguratvāt",
    "1193:230": "tadvan na cakṣuṣi na cātra yathaiva cakṣuḥ|",
    "1193:231": "ādhyātmikāyatanaśeṣam aśeṣam evam",
    "1193:232": "ātmīyavastuviṣayo'pi ca tadvivekī || 58 ||",
    "1193:233": "bāhyeṣu dhātuṣu śarīrasamāśritānāṃ",
    "1193:234": "nālpo'pi lakṣaṇavirodhakṛto'sti bhedaḥ|",
    "1193:235": "vijñānadhāturapi ca kṣaṇikaḥ sa nātmā",
    "1193:236": "tasmātparo'pi ca nabhaḥkusumaiḥ samānaḥ || 59 ||",
    "1193:237": "ityetadudbhavati kevalameva duḥkhaṃ",
    "1193:238": "tṛṣṇāvimūḍhamanaso vigmāttu tasyāḥ|",
    "1193:239": "śāntiḥ parā bhavati tarṣaharastu mārgaḥ",
    "1193:240": "śīlaṃ samādhipariśuddhatayā ca dṛṣṭiḥ || 60 ||",
    "1193:241": "tattatpratītya bhavatīti viśuddha-",
    "1193:242": "dṛṣṭirnāstyasti veti samupaiti sa naiva kiṃ cit|",
    "1193:243": "māyāmayaṃ jagadidaṃ pratibhāti tasya",
    "1193:244": "tasmāt sukhādiṣu bhavatyavikāradhīraḥ || 61 ||",
    "1193:245": "āsīdbhaviṣyati ca yat tad apīdṛgeva",
    "1193:246": "kaḥ saṃbhavo yadasukhaṃ na bhaved bhavebhyaḥ|",
    "1193:247": "evaṃ vyatītaviṣayeṣvapi vītarāgo",
    "1193:248": "naivābhinandati bhavāṃśca bhaviṣyato'pi || 62 ||",
    "1193:249": "ākārabhedaparuṣe puruṣo'parādhī",
    "1193:250": "ko nāma gūḍhanakharasphuṭadṛṣṭicihne|",
    "1193:251": "tatpraiṣyavṛttikapaṭānyanucintya rajyed",
    "1193:252": "viśvāsameva ca yathocitamatra yāyāt || 63 ||",
    "1193:253": "evaṃ vimuktamatirapyanukampakastu",
    "1193:254": "kleśāntaraṃ jagadanāthamavekṣamāṇaḥ|",
    "1193:255": "hīneṣu niṣpraṇayabuddhirudārabhāvān",
    "1193:256": "nirvātumicchati na buddhaguṇānalabdhvā || 64 ||",
    "1193:257": "lokārthasādhanavidhāvasamartharūpaṃ",
    "1193:258": "yānadvayaṃ samavādhūya sa pūrvameva|",
    "1193:259": "kāruṇyadeśitapatho munirājayānam",
    "1193:260": "ātasthivān parahitaikarasasvabhāvam || 65 ||",
    "1193:261": "hīnociteṣu na matirnamati praṇītā",
    "1193:262": "saṃtiṣṭhate mahati nāmahatī kadā cit|",
    "1193:263": "saṃsyandate śucibhireva śucisvabhāvaṃ",
    "1193:264": "tulyaistathānyadapi śāsvata eṣa yogaḥ || 66 ||",
    "1193:265": "svapnopamāni vigaṇayya sukhāsukhāni",
    "1193:266": "saṃmohadoṣakṛpaṇāṃ janatāṃ ca teṣu|",
    "1193:267": "ātmārtha eva gurutāṃ katham asya yāyād",
    "1193:268": "vyāpārabhāramavadhūya parārtharamyam || 67 ||",
    "1193:269": "yaḥ sarvalokahitakāraṇasarvaceṣṭastya-",
    "1193:270": "ktvātmadṛṣṭiviṣayaṃ vitathābhimānam|",
    "1193:271": "sarvatra śāntamatiradvayamārgacārī",
    "1193:272": "so'tyadbhutaścaritanirvṛta eva loke || 68 ||",
    "1193:273": "prajñāviśuddhikaramuttamayānametat",
    "1193:274": "sarvajñatā tadudayā hi mahāmunīnām|",
    "1193:275": "lokasya yā nayanatāmiva saṃprayāti",
    "1193:276": "dīptāṃśumaṇḍalatalotpatitā prabheva || 69 ||",
    "1193:277": "saṃsāradoṣabharanirmathito'pi naiva",
    "1193:278": "prajñāvivecanatayā parikhīdyate yaḥ|",
    "1193:279": "nātmābhikhedapariviklavatāṃ sa yāti",
    "1193:280": "yānasya buddhaguṇasaṃjananasya loke || 70 ||",
    "1193:281": "paśyanti cābhutamayaṃ sugataprabhāvaṃ",
    "1193:282": "romāñcakañcukitasarvaśarīradeśāḥ|",
    "1193:283": "tadgāminaṃ pariharanti ca yānamārgaṃ",
    "1193:284": "kiṃ nāma kāraṇamṛte śaṭhaceṣṭitebhyaḥ || 71 ||",
    "1193:285": "ko nāma mārakalinānabhibhūtacetāḥ",
    "1193:286": "saṃbuddhadharmaguṇaratnanidhānabhūtam|",
    "1193:287": "sarvajñayānamapayānam anarthapaṅkād",
    "1193:288": "ākroṣṭumarhati na cejjagato'sya vairam || 72 ||",
    "1193:289": "lokārthasādhanapare jinarājavaṃśe",
    "1193:290": "prajñānimīlitanayeṣu pariskhalatsu|",
    "1193:291": "cittaṃ narasya karuṇāmṛdu kasya na syāt",
    "1193:292": "tanmohadoṣaśamanāya dṛḍhaṃ ca vīryam || 73 ||",
    "1193:293": "prajñāyā janayati yaḥ parāṃ viśuddhiṃ",
    "1193:294": "nirmokṣaḥ kathamiva tasya dūrataḥ syāt|",
    "1193:295": "naivāsmātparataramasti śīlam anyat",
    "1193:296": "tattasmād bhajata vimokṣakāṅkṣiṇo hi || 74 ||",
    "1193:297": "|| prajñāpāramitāsa[māsa]ścāyaṃ pāramitāsamāsaḥ ||",
    "1193:298": "viśuddhamaunīndramanastaḍāga-",
    "1193:299": "prasūtasūtrāntasaroruhebhyaḥ|",
    "1193:300": "ādāya śurabhramareṇa samyag",
    "1193:301": "madhūrjitaṃ pāramitāsamāse ||"
}