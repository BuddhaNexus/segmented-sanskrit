{
    "778:0": "saptamo binduḥ",
    "778:1": "pratītyasamutpādaḥ",
    "778:2": "1 | dvādaśa pratyayāḥ |",
    "778:3": "avidyā saṃskārāḥ vijñānaṃ nāmarūpaṃ ṣaḍāyatanaṃ sparśaḥ vedanā tṛṣṇā upādānaṃ bhavaḥ jātiḥ jarāmaraṇaṃ ||",
    "778:4": "2 | ete dvādaśa pratyayā strividhā bhavanti |",
    "778:5": "kleśaḥ karma duḥkhaṃ ca ||",
    "778:6": "kleśa strividhaḥ |",
    "778:7": "avidyā tṛṣṇā upādānaṃ ca ||",
    "778:8": "dvividhaṃ karma |",
    "778:9": "saṃskārāḥ bhavaśca ||",
    "778:10": "saptavidhaṃ duḥkhaṃ |",
    "778:11": "vijñānaṃ nāmarūpaṃ ṣaḍāyatanaṃ sparśaḥ vedanā jātirjarāmaraṇaṃ ca ||",
    "778:12": "dvau (pratyayau)",
    "778:13": "atītasaṃgṛhītau |",
    "778:14": "dvau (pratyayau)",
    "778:15": "anāgatasaṃgṛhītau |",
    "778:16": "aṣṭau (pratyayāḥ)",
    "778:17": "pratyutpannasaṃgṛhītāḥ ||",
    "778:18": "3 | kleśa karmahetuḥ |",
    "778:19": "karma duḥkhahetuḥ |",
    "778:20": "duḥkhaṃ kleśahetuḥ |",
    "778:21": "kleśaḥ kleśahetuḥ |",
    "778:22": "kleśaḥ karmahetuḥ |",
    "778:23": "karma duḥkhahetuḥ |",
    "778:24": "duḥkhaṃ duḥkhahetuḥ |",
    "778:25": "ityevaṃ krameṇotpādaḥ ||",
    "778:26": "4 | atītāvidyāsahakṛtasarvakleśasaṃprayuktā bhavatyavidyā ||",
    "778:27": "etāṃ pratītya kriyate karma |",
    "778:28": "karmakaraṇād bhavati lokaphalaṃ |",
    "778:29": "ityucyate saṃskāraḥ ||",
    "778:30": "etān saṃskārān pratītya saṃkliṣṭaṃ cittaṃ labhate kāyendriyavijñānāni |",
    "778:31": "tathāhi |",
    "778:32": "vatsaḥ vijānāti mātaraṃ |",
    "778:33": "iti vijñānaṃ ||",
    "778:34": "etadvijñānasahajāścatvāro&#39;",
    "778:35": "rūpiskaṃdhāḥ (tat-)",
    "778:36": "saṃtānajaṃ cāpi rūpam - iti nāmarūpaṃ ||",
    "778:37": "cakṣurādīndriyagocarāśrayaṃ bhavati ṣaḍāyatanaṃ ||",
    "778:38": "indriyaviṣayavijñānānāṃ saṃnipātād bhavati sparśaḥ ||",
    "778:39": "sparśājjāyate vedanā - iti vedanā ||",
    "778:40": "vedanāsaṃgena bhavati tṛṣṇā ||",
    "778:41": "tṛṣṇāyāḥ kleśeṇodyama ityupādānaṃ ||",
    "778:42": "udyamena karoti karmeti bhavaḥ ||",
    "778:43": "anāgataphalamucyate jātiḥ ||",
    "778:44": "jātirutpādayati duḥkhamaparimeyamiti jarāmaraṇaṃ ||",
    "778:45": "5 | punaḥ khalu na jānātyavidyā catvāri satyāni |",
    "778:46": "adhyātmabahirdhā dharmān atītānāgatapratyutpannabuddhadharmān sarvahetupratyayān evaṃbhūtān vividhān satyadharmān na jānātītyucyate&#39;vidyā ||",
    "778:47": "6 | mūḍhaḥ pudgala ścarati trividhāṃ caryāṃ |",
    "778:48": "śubhacaryāṃ aśubhacaryāṃ akṣobhyacaryāṃ ||",
    "778:49": "katamā śubhācaryā |",
    "778:50": "yayā prāpyate kuśalaphalaṃ ||",
    "778:51": "katamā aśubhacaryā |",
    "778:52": "yayā prāpyate&#39;",
    "778:53": "kuśalaphalaṃ ||",
    "778:54": "katamā akṣobhyacaryā |",
    "778:55": "rūpārūpadhātūpapattiḥ ||",
    "778:56": "7 | punaḥ khalu dānaṃ śīlaṃ dhyānaṃ (nāma śubhacaryā)||",
    "778:57": "dānaṃ katamat |",
    "778:58": "dānaṃ dvividhaṃ |",
    "778:59": "prathamamāmiṣadānaṃ dvitīyaṃ dharmadānaṃ ||",
    "778:60": "paṃcavidhaṃ śīlagrahaṇaṃ |",
    "778:61": "gṛhṇāti cec chīlaṃ yāvadantaṃ |",
    "778:62": "vyavadānayatyakuśalacittamalam |",
    "778:63": "sarvadā smṛtaḥ,",
    "778:64": "saṃprajanyaḥ,",
    "778:65": "na gaveṣayanti lokaphalam ||",
    "778:66": "dhyānaṃ (nāma)",
    "778:67": "aśucibhāvanā ānāpānādismṛtiḥ sarvasāsravakuśalasamādhidharmāḥ ||",
    "778:68": "iti śubhacaryā ||",
    "778:69": "8 | aśubhacaryā (punaḥ)katamā |",
    "778:70": "trīṇyakuśalamūlāni daśakuśalakarmapathādayo vividhāni pāpānītyucyate&#39;",
    "778:71": "śubhacaryā ||",
    "778:72": "akṣobhyācaryā (punaḥ)katamā |",
    "778:73": "prathamadhyānād yāvat naiva saṃjñānāsaṃjñā(yatana)",
    "778:74": "samādhirityucyate&#39;",
    "778:75": "kṣobhyacaryā ||",
    "778:76": "9 | traihetukasāsravavijñānamupādāya prathamād yāvat saptamaṃ bhavam - ityucyate vijñānaṃ |",
    "778:77": "vijñānād bhavati nāmarūpaṃ |",
    "778:78": "vedanāsaṃjñāsaṃskāravijñānaskaṃdhā iti nāma |",
    "778:79": "catvāri mahābhūtāni tatkṛtaṃ rūpaṃ ceti rūpaṃ |",
    "778:80": "nāmarūpājjāyate ṣaḍāyatanaṃ |",
    "778:81": "ṣaḍāyatanājjāyate sparśaḥ |",
    "778:82": "sparśaḥ ṣaḍvidhaḥ kāyotthapratigha-mānasotthādhivacana (-bhedena)",
    "778:83": "dviprakāraḥ |",
    "778:84": "ṣaḍvijñānavikalpahetoḥ ṣaḍvidhaḥ sparśaḥ ||"
}