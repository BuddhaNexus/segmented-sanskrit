{
  "SA05n826:0": "rūpastavaḥ",
  "SA05n826:1": "sarvabhūtamaṇikampita tubhyaṃ",
  "SA05n826:2": "kṣāntimeva caritaṃ tava rūpam |",
  "SA05n826:3": "sarvarūpavaradivyasurūpaṃ",
  "SA05n826:4": "taṃ namāmi daśabalavararūpam || 1 ||",
  "SA05n826:5": "bhṛṅgarājaśikhimūrdhni sukeśaṃ",
  "SA05n826:6": "kokilābhaśikhidivyasukeśam |",
  "SA05n826:7": "snigdhanīlamṛdukuñcitakeśaṃ",
  "SA05n826:8": "taṃ namāmi daśabalavarakeśam || 2 ||",
  "SA05n826:9": "śaṅkhakundakumudaṃ vimaromaṃ",
  "SA05n826:10": "janmaduḥkhavigataṃ vimaromam |",
  "SA05n826:11": "rogaśokavimate vimaromaṃ",
  "SA05n826:12": "taṃ namāmi daśabalavararomam || 3 ||",
  "SA05n826:13": "pūrṇacandradyutiśobhitavaktraṃ",
  "SA05n826:14": "tuṅganāsamaṇibhāśubhavaktram |",
  "SA05n826:15": "buddhapaṅkajinabhāmaravaktraṃ",
  "SA05n826:16": "taṃ namāmi daśabalavaravaktram || 4 ||",
  "SA05n826:17": "raśmisahasravicitrasumūrdhni",
  "SA05n826:18": "puṣpavarṣaśatapūjitamūrdhni |",
  "SA05n826:19": "ratnavarṣaśatapūjitamūrdhni",
  "SA05n826:20": "taṃ namāmi daśabalavaramūrdhni || 5 ||",
  "SA05n826:21": "netrapatākavilambitaśīrṣaṃ",
  "SA05n826:22": "kāñcanachatravaropitaśīrṣam |",
  "SA05n826:23": "netravitānasuśobhitaśīrṣaṃ",
  "SA05n826:24": "taṃ namāmi daśabalavaraśīrṣam || 6 ||",
  "SA05n826:25": "divyāmbaramukuṭaṃ maṇimukuṭaṃ",
  "SA05n826:26": "candraprabhāmukuṭāmaṇimukuṭam |",
  "SA05n826:27": "āgataśīrṣamakhaṇḍitamukuṭaṃ",
  "SA05n826:28": "taṃ namāmi daśabalavaramukuṭam || 7 ||",
  "SA05n826:29": "cārudalāyatapaṅkajanetraṃ",
  "SA05n826:30": "divyajñānavipulāyatanetram |",
  "SA05n826:31": "dhyānamokṣaśubhasaṃskṛtanetraṃ",
  "SA05n826:32": "taṃ namāmi daśabalavaranetram || 8 ||",
  "SA05n826:33": "śuddhakarṇaśubhaśuddhasukarṇaṃ",
  "SA05n826:34": "śuddhaprajñavaraśobhitakarṇam |",
  "SA05n826:35": "śuddhajñānavaraśuddhasukarṇaṃ",
  "SA05n826:36": "taṃ namāmi daśabalavarakarṇam || 9 ||",
  "SA05n826:37": "śubhrakundaśaśipaṅkajadantaṃ",
  "SA05n826:38": "tīkṣṇasulakṣaṇanirmaladantam |",
  "SA05n826:39": "viśvapañcadaśapañcasudantaṃ",
  "SA05n826:40": "taṃ namāmi daśabalavaradantam || 10 ||",
  "SA05n826:41": "uccavākyasasurāsurajihvāṃ",
  "SA05n826:42": "dharmajñānakṛtapāpadajihvām |",
  "SA05n826:43": "gandhadhūpasasurāsurajihvāṃ",
  "SA05n826:44": "taṃ namāmi daśabalavarajihvām || 11 ||",
  "SA05n826:45": "meghasudundubhināditaghoṣaṃ",
  "SA05n826:46": "satyadharmanayasuviditaghoṣam |",
  "SA05n826:47": "divyavādiparavāditaghoṣaṃ",
  "SA05n826:48": "taṃ namāmi daśabalavaraghoṣam || 12 ||",
  "SA05n826:49": "grīvagrīvadaśagrīvasugrīvaṃ",
  "SA05n826:50": "kṣāntivīryaṃ tava grīvasugrīvam |",
  "SA05n826:51": "hemavarṇamaṇigrīvasugrīvaṃ",
  "SA05n826:52": "taṃ namāmi daśabalavaragrīvam || 13 ||",
  "SA05n826:53": "siṃhakāyahimakundasukāyaṃ",
  "SA05n826:54": "dhyānakāyaguṇasāgarakāyam |",
  "SA05n826:55": "aṣṭasulakṣaṇanirmalakāyaṃ",
  "SA05n826:56": "taṃ namāmi daśabalavarakāyam || 14 ||",
  "SA05n826:57": "śvetaraktaśubhaśobhitavastraṃ",
  "SA05n826:58": "nīlapītaharitāyitavastram |",
  "SA05n826:59": "hāradhavalaśubhaśobhitavastraṃ",
  "SA05n826:60": "taṃ namāmi daśabalavaravastram || 15 ||",
  "SA05n826:61": "hemanāgakarasaṃskṛtabāhū",
  "SA05n826:62": "puṣpadānakṛtaśobhitabāhū |",
  "SA05n826:63": "dhīravīryasumanoharabāhū",
  "SA05n826:64": "taṃ (tau) namāmi daśabalavarabāhū || 16 ||",
  "SA05n826:65": "cāmaracakrasuśobhitahastaṃ",
  "SA05n826:66": "vimalasukomalapaṅkajahastam |",
  "SA05n826:67": "bhūtapretaśaraṇāgatahastaṃ",
  "SA05n826:68": "taṃ namāmi daśabalavarahastam || 17 ||",
  "SA05n826:69": "rāgadveṣatanuvarjitacittaṃ",
  "SA05n826:70": "kalpakoṭisamabhāvitacittam |",
  "SA05n826:71": "śīlajñānasamabhāvitacittaṃ",
  "SA05n826:72": "taṃ namāmi daśabalavaracittam || 18 ||",
  "SA05n826:73": "padmanābhaśubhaśobhitanābhaṃ",
  "SA05n826:74": "padmayoniśubhaśobhitanābham |",
  "SA05n826:75": "brahmajyotisuśobhitanābhaṃ",
  "SA05n826:76": "taṃ namāmi daśabalavaranābham || 19 ||",
  "SA05n826:77": "cakrasulakṣaṇabhūṣitapādaṃ",
  "SA05n826:78": "aṅkuśaśaktivirājitapādam |",
  "SA05n826:79": "nāgayakṣagaṇavanditapādaṃ",
  "SA05n826:80": "taṃ namāmi daśabalavarapādam || 20 ||",
  "SA05n826:81": "bhāskaradyutirājitapādaṃ",
  "SA05n826:82": "devadānavagaṇavanditapādam |",
  "SA05n826:83": "sarvadevagaṇavanditapādaṃ",
  "SA05n826:84": "taṃ namāmi daśabalavarapādam || 21 ||",
  "SA05n826:85": "sarvadevagaṇapūjitaśīrṣaṃ",
  "SA05n826:86": "sūryakoṭisamabhāvitaśīrṣam |",
  "SA05n826:87": "divyajvālajvalitārpitaśīrṣaṃ",
  "SA05n826:88": "taṃ namāmi daśabalavaraśīrṣam || 22 ||",
  "SA05n826:89": "puṣpadhūpaśatapūjitaśīrṣaṃ",
  "SA05n826:90": "dīpagandhaśatapūjitaśīrṣam |",
  "SA05n826:91": "mālyavastraśatapūjitaśīrṣaṃ",
  "SA05n826:92": "taṃ namāmi daśabalavaraśīrṣam || 23 ||",
  "SA05n826:93": "tribhuvanadharmakalābhyuṣṇīṣa-",
  "SA05n826:94": "ratnachatramukuṭādipūjitam |",
  "SA05n826:95": "ye paṭhanti sugatastavamenaṃ",
  "SA05n826:96": "te labhanti śubhamokṣapathādhigamam || 24 ||",
  "SA05n826:97": "śrīmadāryāvalokiteśvarabhaṭṭārakasya rūpastavaṃ samāptam |"
}