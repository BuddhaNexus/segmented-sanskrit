{
    "920:0": "CANTO VI",
    "920:1": "tato muhūrtābhyudite",
    "920:2": "jagaccakṣuṣi bhāskare|",
    "920:3": "bhārgavasyāśramapadaṃ",
    "920:4": "sa dadarśa nṛṇāṃ varaḥ||1||",
    "920:5": "suptaviśvastahariṇaṃ",
    "920:6": "svasthasthitavihaṅgamam|",
    "920:7": "viśrānta iva yaddṛṣṭvā",
    "920:8": "kṛtārtha iva cābhavat||2||",
    "920:9": "sa vismayanivṛttyartha",
    "920:10": "tapaḥpūjārthameva ca|",
    "920:11": "svāṃ cānuvartitā rakṣa-",
    "920:12": "nnaśvapṛṣṭhādavatārat||3||",
    "920:13": "avatīrya ca pasparśa",
    "920:14": "nistīrṇamiti vājinam|",
    "920:15": "chandakaṃ cāvravītprītaḥ",
    "920:16": "snāpayanniva cakṣuṣā||4||",
    "920:17": "imaṃ tārkṣyopamajavaṃ",
    "920:18": "turaṅgamanugacchatā|",
    "920:19": "darśitā saumya madbhakti-",
    "920:20": "rvikramaścāyamātmanaḥ||5||",
    "920:21": "sarvathāsmyanyakāryo'pi",
    "920:22": "gṛhīto bhavatā hṛdi|",
    "920:23": "bhartusnehaśca yasyāya-",
    "920:24": "mīdṛśaḥ śaktireva ca||6||",
    "920:25": "asnigdho'pi samartho'sti",
    "920:26": "niḥsāmarthyo'pi bhaktimān|",
    "920:27": "bhaktimāṃścaiva śaktaśca",
    "920:28": "durlabhastvadvidho bhuvi||7||",
    "920:29": "tatprīto'smi tavānena",
    "920:30": "mahābhāgena karmaṇā|",
    "920:31": "yasya te mayi bhāvo'yaṃ",
    "920:32": "phalebhyo'pi parāṅmukhaḥ||8||",
    "920:33": "ko janasya phalasthasya",
    "920:34": "na syādabhimukho janaḥ|",
    "920:35": "janībhavati bhūyiṣṭhaṃ",
    "920:36": "svajano'pi viparyaye||9||",
    "920:37": "kulārtha dhāryate putraḥ",
    "920:38": "poṣārtha sevyate pitā|",
    "920:39": "āśayācchilaṣyati jaga-",
    "920:40": "nnāsti niṣkāraṇā svatā||10||",
    "920:41": "kimuktvā bahu saṃkṣepā-",
    "920:42": "tkṛtaṃ me sumahatpriyam|",
    "920:43": "nivartasvāśvamādāya",
    "920:44": "saṃprāpto'smīpsitaṃ padam||11||",
    "920:45": "ityuktvā sa mahābāhu-",
    "920:46": "ranuśaṃsacikīrṣayā|",
    "920:47": "bhūṣaṇānyavamucyāsmai",
    "920:48": "saṃtaptamanase dadau||12||",
    "920:49": "mukuṭāddīpakarmāṇaṃ",
    "920:50": "maṇīmādāya bhāsvaram|",
    "920:51": "bruvanvākyamidaṃ tasthau",
    "920:52": "sāṃditya iva mandaraḥ||13||",
    "920:53": "anena maṇinā chanda",
    "920:54": "praṇamya bahuśo nṛpaḥ|",
    "920:55": "vijñāpyo'muktaviśrambhaṃ",
    "920:56": "saṃtāpavinivṛttaye||14||",
    "920:57": "janmamaraṇanāśārtha",
    "920:58": "praviṣṭo'smi tapovanam|",
    "920:59": "na khalu svargatarṣeṇa",
    "920:60": "nāsnehena na manyunā||15||",
    "920:61": "tadevamabhiniṣkrāntaṃ",
    "920:62": "na māṃ śocitumarhasi|",
    "920:63": "bhūtvāpi hi ciraṃ śleṣaḥ",
    "920:64": "kālena na bhaviṣyati||16||",
    "920:65": "dhruvo yasmācca viśleṣa-",
    "920:66": "stasmānmokṣāya me matiḥ|",
    "920:67": "viprayogaḥ kathaṃ na syād",
    "920:68": "bhūyo'pi svajanāditi||17||",
    "920:69": "śokatyāgāya niṣkrāntaṃ",
    "920:70": "na māṃ śocitumarhasi|",
    "920:71": "śokahetuṣu kāmeṣu",
    "920:72": "saktāḥ śocyāstu rāgiṇaḥ||18||",
    "920:73": "ayaṃ ca kila pūrveṣā-",
    "920:74": "masmākaṃ niścayaḥ sthiraḥ|",
    "920:75": "iti dāyādyabhūtena",
    "920:76": "na śocyo'smi pathā vrajan||19||",
    "920:77": "bhavanti hyarthadāyādāḥ",
    "920:78": "puruṣasya viparyaye|",
    "920:79": "pṛthivyāṃ dharmadāyādāḥ",
    "920:80": "durlabhāstu na santi vā||20||",
    "920:81": "yadapi syādasamaye",
    "920:82": "yāto vanamasāviti|",
    "920:83": "akālo nāsti dharmasya",
    "920:84": "jīvite cañcale sati||21||",
    "920:85": "tasmādadyaiva me śreya-",
    "920:86": "ścetavyamiti niścayaḥ|",
    "920:87": "jīvite ko hi viśrambho",
    "920:88": "mṛtyau pratyarthini sthite||22||",
    "920:89": "evamādi tvayā saumya",
    "920:90": "vijñāpyo vasudhādhipaḥ|",
    "920:91": "prayatethāstathā caiva",
    "920:92": "yathā māṃ na smaredāpi||23||",
    "920:93": "api nairguṇyamasmākaṃ",
    "920:94": "vācyaṃ narapatau tvayā|",
    "920:95": "nairguṇyāttyajyate snehaḥ",
    "920:96": "snehatyāgānna śocyate||24||",
    "920:97": "iti vākyamidaṃ śrutvā",
    "920:98": "chandaḥ saṃtāpaviklavaḥ|",
    "920:99": "bāṣpagrathitayā vācā",
    "920:100": "pratyuvāca kṛtāñjaliḥ||25||",
    "920:101": "anena ta va bhāvena",
    "920:102": "bāndhavāyāsadāyinā|",
    "920:103": "bhartaḥ sīdati me ceto",
    "920:104": "nadīpaṅka iva dvipaḥ||26||",
    "920:105": "kasya notpādayedbāṣpaṃ",
    "920:106": "niścayaste'yamīdṛśaḥ|",
    "920:107": "ayomaye'pi hṛdaye",
    "920:108": "kiṃ punaḥ snehaviklave||27||",
    "920:109": "vimānaśayanārhaṃ hi",
    "920:110": "saukumāryamidaṃ kva ca|",
    "920:111": "kharadarbhāṅkuravatī",
    "920:112": "tapovanamahī kva ca||28||",
    "920:113": "śrutvā tu vyavasāyaṃ te",
    "920:114": "yadaśvo'yaṃ mayāhṛtaḥ|",
    "920:115": "balātkāreṇa tannātha",
    "920:116": "daivenaivāsmi kāritaḥ||29||",
    "920:117": "kathaṃ hyātmavaśo jānan",
    "920:118": "vyavasāyamimaṃ tava|",
    "920:119": "upānayeyaṃ turagaṃ",
    "920:120": "śokaṃ kapilavāstunaḥ||30||",
    "920:121": "tannārhasi mahābāho",
    "920:122": "vihātuṃ putralālasam|",
    "920:123": "snigdhaṃ vṛddhaṃ ca rājānaṃ",
    "920:124": "saddharmamiva nāstikaḥ||31||",
    "920:125": "saṃvardhanapariśrāntāṃ",
    "920:126": "dvitīyāṃ tāṃ ca mātaram|",
    "920:127": "devīṃ nārhasi vismartu",
    "920:128": "kṛtaghna iva satkriyām||32||",
    "920:129": "bālaputrāṃ guṇavartī",
    "920:130": "kulaślādhyāṃ pativratām|",
    "920:131": "devīmarhasi na tyaktuṃ",
    "920:132": "klībaḥ prāptāmiva śriyam||33||",
    "920:133": "putraṃ yāśodharaṃ ślādhyaṃ",
    "920:134": "yaśodharmabhṛtāṃ varam|",
    "920:135": "bālamarhasi na tyaktuṃ",
    "920:136": "vyasanīvottamaṃ yaśaḥ||34||",
    "920:137": "atha bandhuṃ ca rājyaṃ ca",
    "920:138": "tyaktumeva kṛtā matiḥ|",
    "920:139": "māṃ nārhasi vibho tyaktuṃ",
    "920:140": "tvatpādau hi gatirmama||35||",
    "920:141": "nāsmi yātuṃ puraṃ śakto",
    "920:142": "dahyamānena cetasā|",
    "920:143": "tvāmaraṇye parityajya",
    "920:144": "sumantra iva rāghavam||36||",
    "920:145": "kiṃ hi vakṣyati māṃ rājā",
    "920:146": "tvadṛte nagaraṃ gatam|",
    "920:147": "vakṣyāmyucitadarśitvā-",
    "920:148": "tkiṃ tavāntaḥpurāṇi vā||37||",
    "920:149": "yadapyātthāpi nairguṇyaṃ",
    "920:150": "vācyaṃ narapatāviti|",
    "920:151": "kiṃ tadvakṣyāmyabhūtaṃ te",
    "920:152": "nirdoṣasya muneriva||38||",
    "920:153": "hṛdayena salajjena",
    "920:154": "jivhayā sajjamānayā|",
    "920:155": "ahaṃ yadapi vā brūyāṃ",
    "920:156": "kastacchraddhātumarhati||39||",
    "920:157": "yo hi candramasastaikṣṇyaṃ",
    "920:158": "kathayecchraddadhīta vā|",
    "920:159": "sa doṣāṃstava doṣajña",
    "920:160": "kathayecchraddadhīta vā||40||",
    "920:161": "sānukrośasya satataṃ",
    "920:162": "nityaṃ karuṇavedinaḥ|",
    "920:163": "snigdhatyāgo na sadṛśo",
    "920:164": "nivartasva prasīda me||41||",
    "920:165": "iti śokābhibhūtasya",
    "920:166": "śrutvā chandasya bhāṣitam|",
    "920:167": "svasthaḥ paramayā dhṛtyā",
    "920:168": "jagāda vadatāṃ varaḥ||42||",
    "920:169": "madviyogaṃ prati cchanda",
    "920:170": "saṃtāpastyajyatāmayam|",
    "920:171": "nānābhāvo hi niyataṃ",
    "920:172": "pṛthagjātiṣu dehiṣu||43||",
    "920:173": "svajanaṃ yadyapi snehā-",
    "920:174": "nna tyajeyamahaṃ svayam|",
    "920:175": "mṛtyuranyonyamavaśā-",
    "920:176": "nasmān saṃtyājayiṣyati||44||",
    "920:177": "mahatyā tṛṣṇayā duḥkhai-",
    "920:178": "rgarbheṇāsmi yayā dhṛtaḥ|",
    "920:179": "tasyā niṣphalayatnāyāḥ",
    "920:180": "kvāhaṃ mātuḥ kva sā mama||45||",
    "920:181": "vāsavṛkṣe samāgamya",
    "920:182": "vigacchanti yathāṇḍajāḥ|",
    "920:183": "niyataṃ viprayogānta-",
    "920:184": "stathā bhūtasamāgamaḥ||46||",
    "920:185": "sametya ca yathā bhūyo",
    "920:186": "vyapayānti balāhakāḥ|",
    "920:187": "saṃyogo viprayogaśca",
    "920:188": "tathā me prāṇināṃ mataḥ||47||",
    "920:189": "yasmādyāti ca loko'yaṃ",
    "920:190": "vipralabhya paraṃparam|",
    "920:191": "mamattvaṃ na kṣamaṃ tasmā-",
    "920:192": "tsvapnabhūte samāgame||48||",
    "920:193": "sahajena viyujyante",
    "920:194": "parṇarāgeṇa pādapāḥ|",
    "920:195": "anyenānyasya viśleṣaḥ",
    "920:196": "kiṃ punarna bhaviṣyati||49||",
    "920:197": "tadevaṃ sati saṃtāpaṃ",
    "920:198": "mā kārṣī saumya gamyatām|",
    "920:199": "lambate yadi tu sneho",
    "920:200": "gatvāpi punarāvraja||50||",
    "920:201": "brūyāścāsmatkṛtāpekṣaṃ",
    "920:202": "janaṃ kapilavāstuni|",
    "920:203": "tyajyatāṃ tagdataḥ snehaḥ|",
    "920:204": "śrūyatāṃ cāsya niścayaḥ||51||",
    "920:205": "kṣiprameṣyati vā kṛtvā",
    "920:206": "janmamṛtyukṣayaṃ kila|",
    "920:207": "akṛtārtho nirārambho",
    "920:208": "nidhanaṃ yāsyatīti vā||52||",
    "920:209": "iti tasya vacaḥ śrutvā",
    "920:210": "kanthakasturagottamaḥ|",
    "920:211": "jivhayā lilihe pādau",
    "920:212": "bāṣpamuṣṇaṃ mumoca ca||53||",
    "920:213": "jālinā svastikāṅkena",
    "920:214": "cakramadhyena pāṇinā|",
    "920:215": "āmamarśa kumārastaṃ",
    "920:216": "babhāṣe ca vayasyavat||54||",
    "920:217": "muñca kanthaka mā bāṣpaṃ",
    "920:218": "darśiteyaṃ sadaśvatā|",
    "920:219": "mṛṣyatāṃ saphalaḥ śīghraṃ",
    "920:220": "śramaste'yaṃ bhaviṣyati||55||",
    "920:221": "maṇitsaruṃ chandakahastasaṃsthaṃ",
    "920:222": "tataḥ sa dhīro niśitaṃ gṛhītvā",
    "920:223": "kośādasiṃ kañcanabhakticitraṃ",
    "920:224": "bilādivaśīviṣamudbabarha||56||",
    "920:225": "niṣkāsya taṃ cotpalapattranīlaṃ",
    "920:226": "ciccheda citraṃ mukuṭaṃ sakeśam|",
    "920:227": "vikīryamāṇāṃśukamantarīkṣe",
    "920:228": "cikṣepa cainaṃ sarasīva haṃsam||57||",
    "920:229": "pūjābhilāṣeṇa ca bāhumānyā-",
    "920:230": "ddivaukasastaṃ jagṛhuḥ praviddham|",
    "920:231": "yathāvadenaṃ divi devasaṅghā",
    "920:232": "divyairviśeṣairmahayāṃ ca cakruḥ||58||",
    "920:233": "muktvā tvalaṃkārakalatravattāṃ",
    "920:234": "śrīvipravāsaṃ śirasaśca kṛtvā|",
    "920:235": "dṛṣṭvāṃśukaṃ kāñcanahaṃsacinhaṃ",
    "920:236": "vanyaṃ sa dhīro'bhicakāṅkṣa vāsaḥ||59||",
    "920:237": "tato mṛgavyādhanapurdivaukā",
    "920:238": "bhāvaṃ viditvāsya viśuddhabhāvaḥ|",
    "920:239": "kāṣāyavastro'bhiyayau samīpaṃ",
    "920:240": "taṃ śākyarājaprabhavo'bhyuvāca||60||",
    "920:241": "śivaṃ ca kāṣāyamṛṣidhvajaste",
    "920:242": "na yujyate hiṃsramidaṃ dhanuśca|",
    "920:243": "tatsaumya yadyasti na saktiratra",
    "920:244": "mahyaṃ prayacchedamidaṃ gṛhāṇa||61||",
    "920:245": "vyādho'bravītkāmada kāmamārā-",
    "920:246": "danena viśvāsya mṛgāgnihanmi|",
    "920:247": "arthastu śakropama yadyanena",
    "920:248": "hanta pratīcchānaya śuklametat||62||",
    "920:249": "pareṇa harṣeṇa tataḥ sa vanyaṃ",
    "920:250": "jagrāha vāso'śukamutsasarja|",
    "920:251": "vyādhastu divyaṃ vapureva bibhra-",
    "920:252": "ttacchuklamādāya divaṃ jagāma||63||",
    "920:253": "tataḥ kumāraśca sa cāśvagopa-",
    "920:254": "stasmiṃstathā yāti visismiyāte|",
    "920:255": "āraṇyake vāsasi caiva bhūya-",
    "920:256": "stasminnakārṣṭā bahumānamāśu||64||",
    "920:257": "chandaṃ tataḥ sāśrumukhaṃ visṛjya",
    "920:258": "kāṣāyasaṃbhṛddhṛtikīrtibhṛtsaḥ|",
    "920:259": "yenāśramastena yayau mahātmā",
    "920:260": "saṃdhyābhrasaṃvīta ivoḍurājaḥ||65||",
    "920:261": "tatastathā bhartari rājyaniḥspṛhe",
    "920:262": "tapovanaṃ yāti vivarṇavāsasi|",
    "920:263": "bhujau samutkṣipya tataḥ sa vājibhṛd",
    "920:264": "bhṛśaṃ vicukrośa papāta ca kṣitau||66||",
    "920:265": "vilokya bhūyaśca ruroda sasvaraṃ",
    "920:266": "hayaṃ bhujābhyāmupaguhya kanthakam|",
    "920:267": "tato nirāśo vilapananmuhurmuhu-",
    "920:268": "ryayau śarīreṇa puraṃ na cetasā||67||",
    "920:269": "kvacitpradadhyau vilalāpa ca kvacit",
    "920:270": "kvacitpracaskhāla papāta ca kvacit|",
    "920:271": "ato vrajan bhaktivaśena duḥkhita-",
    "920:272": "ścacāra bavhīravaśaḥ pathi kriyāḥ||68||",
    "920:273": "iti buddhacarite mahākāvye",
    "920:274": "chandakanivartano nāma ṣaṣṭhaḥ sargaḥ||6||"
}