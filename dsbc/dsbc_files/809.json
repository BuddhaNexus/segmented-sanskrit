{
    "809:0": "(ārya)tārāstutiḥ",
    "809:1": "ācārya candradāsakṛtā",
    "809:2": "namastārāyai",
    "809:3": "sukhadacakracārucūḍāmaṇiruciramarīcisaṃcaya-",
    "809:4": "pracuraśikhāpracāraparicumbitacarcitacaraṇacandrake|",
    "809:5": "jagati carācare'pi sācīkṛtacakitakṛpālulocane",
    "809:6": "stutivacanopacāramucitācaratiṃ racayāmi devi te|| 1||",
    "809:7": "nakharakaṭhorakoṭikuṭṭitakarikaṭataṭapāṭanotkaṭāḥ",
    "809:8": "pravikaṭarudhirapaṭalapaṭapālitā aṭavīviluṇṭhakāḥ|",
    "809:9": "vikaṭasaṭāṭṭahāsaghaṭitāvaṭataṭaghaṭānodbhaṭāstvajjuṣi",
    "809:10": "jhaṭiti yānti saṭinaḥ kuṇṭhādapi kuṇṭhaśaktitām|| 2||",
    "809:11": "malinamahākapolatalavigalitamadajalamalanavihvalāḥ",
    "809:12": "skhaladalijālabahalakolāhalalitavilāsalāsinaḥ|",
    "809:13": "vicalitakarṇatālapavanāhatalalitavilepadhūlayaḥ",
    "809:14": "pathi na gajāścalanti kulaśailatulāstava lā(nā)malālitāḥ|| 3||",
    "809:15": "mārutaghātajātarabhasocchaladanalaśikhākadambaka-",
    "809:16": "pratihatapurapurandhrihāhāravatvaritadigantabhairavaḥ|",
    "809:17": "uddhatadhūmradhūmadhūlidhutabaddhaghanāndhakārakaṃ",
    "809:18": "tvannatinītigatireti śamaṃ bahuśo hutāśanaḥ|| 4||",
    "809:19": "calitalatāvitānakuṭilodgamadurgamagahanavartinaḥ",
    "809:20": "sapadi puro narasya tāreti manāgapi nāmadhāriṇaḥ|",
    "809:21": "sphītaphaṇaughaghoraphūtkāraparisphuratānalasphuṭa-",
    "809:22": "sphuraduruvisphuliṅgavisphāriṇi phaṇini viṣaṃ vinaśyati|| 5||",
    "809:23": "kṣaṇakṛtakopakampakarakarṣitakharakaravālanirmala-",
    "809:24": "vyatikarakarakaravālavikarālamahābalabhujārgalaḥ|",
    "809:25": "prasthitapathikanikaṭakaṭavighaṭanapaṭuratiniṣṭhurāśayo",
    "809:26": "bhagavati bhaktivantumupasarpati tava na vane'pi taskaraḥ|| 6||",
    "809:27": "yo'pi narendravīrahuṅkārakacagrahanigraho",
    "809:28": "grahagrasta iva rajjuhiñjīravajarjaritāṅgapañjaraḥ|",
    "809:29": "pratipadakhanakhanāyamānamukharīkṛtakharakharaśṛṅkhalāvali-",
    "809:30": "stvaccaraṇāravindamabhivandya sa nandati muktabandhanaḥ|| 7||",
    "809:31": "kalakalakalilalolakallolajalollalatkālikānilā-",
    "809:32": "sphālitavipulabahalavelākulakūlatamālapallavāt|",
    "809:33": "sarabhasamakaranikarakharanakharasudustarato'pi sāgarāt",
    "809:34": "tāriṇi taralatārataratārakamāturametya rakṣasi|| 8||",
    "809:35": "sūkṣmavirāvasārasaraghotkaranirbharaghoraghargharaghrāṇā",
    "809:36": "ghṛṇāṅaghripāṇyasravisrīkṛtakvathitaśarīrapañjarā|",
    "809:37": "yatkṣaṇamahāprasādāveśatvatpraṇatā tāriṇi kāmarūpiṇā",
    "809:38": "tatkṣaṇalabdhalolakiraṇamaṇikuṇḍalamaṇḍitagaṇḍamaṇḍalā|| 9||",
    "809:39": "yūkavikīrṇaśīrṇapaṭakarpaṭakaṭitaṭaveṣṭanodbhaṭaḥ",
    "809:40": "saṃkaṭapeṭṭapūramātrārjanaparapurapiṇḍatarkaṇaḥ|",
    "809:41": "yadi tava nāmakaṃ hṛdi karoti hi rākṣasaikakaḥ",
    "809:42": "prauḍhavadhūvidhūtacāmīkarakhacitavicitracāmaram|| 10||",
    "809:43": "śrīcandradāsaviracitā''ryatārāstutiḥ samāptā|"
}