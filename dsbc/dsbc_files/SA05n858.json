{
  "SA05n858:0": "pañcatathāgatastutigāthā",
  "SA05n858:1": "om namaḥ śrīpañcabhūtāntāya |",
  "SA05n858:2": "namaste varadarājāgra bhūtakoṭi namostu te |",
  "SA05n858:3": "namaste śūnyatāgarbha buddhabodhi namo'stu te || 1 ||",
  "SA05n858:4": "buddharāga namaste'stu buddhakāma namo'stu te |",
  "SA05n858:5": "buddhaprīti namaste'stu buddhamoda namo namaḥ || 2 ||",
  "SA05n858:6": "buddhasmita namastubhyaṃ buddhahāsa namo namaḥ |",
  "SA05n858:7": "buddhavāca namaste'stu buddhabhāva namo namaḥ || 3 ||",
  "SA05n858:8": "abhavodbhava namaste'stu namaste buddhasaṃbhava |",
  "SA05n858:9": "gaganodbhava namaste'stu namaste jñānasaṃbhava || 4 ||",
  "SA05n858:10": "māyājāla namastubhyaṃ namaste buddhanāyaka |",
  "SA05n858:11": "namaste sarvasarvebhyo jñānakāya namo'stu te |",
  "SA05n858:12": "śrī pañcatathāgatastutigāthā samāptā |"
}