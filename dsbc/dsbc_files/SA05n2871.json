{
  "SA05n2871:3": "Bhagavatstutiḥ",
  "SA05n2871:10": "praṇidhāya manaḥ sahendriyairvidhivad vāk ca",
  "SA05n2871:11": "śarīrameva ca |",
  "SA05n2871:15": "guṇabhūta mahāguṇodadhestava vakṣyāmi guṇaikadeśatām  ||1 ||",
  "SA05n2871:23": "paramapravaro'smi vādināmanavadyaḥ",
  "SA05n2871:24": "susamāhitendriyaḥ |",
  "SA05n2871:28": "paramārthavidaprakampitaḥ prayataiḥ",
  "SA05n2871:29": "sarvaparapravādibhiḥ ||2 ||",
  "SA05n2871:36": "caraṇaṁ susamāptameva te susamāptavrata",
  "SA05n2871:37": "sādhitavrataḥ |",
  "SA05n2871:41": "balavāṁśca samādhiravyayastava nārāyaṇaśailarājavat  ||3 ||",
  "SA05n2871:49": "puruṣaṣabha nāsti te vyathā na viṣādo na bhayaṁ",
  "SA05n2871:50": "na ca klamaḥ |",
  "SA05n2871:54": "na ca te vyasanaṁ kutaḥ kalirna ca bhūteṣu",
  "SA05n2871:55": "kadācidakṣamā ||4 ||",
  "SA05n2871:62": "na ca dhāvasi nātilīyase na ca santapyasi nāpi",
  "SA05n2871:63": "hṛṣyase |",
  "SA05n2871:67": "satataṁ śubhameva te manaḥ satataṁ",
  "SA05n2871:68": "merurivācalādhipaḥ ||5 ||",
  "SA05n2871:75": "munipuṁgava sarvadhātubhirvipulaṁ jñānamapāvṛtaṁ",
  "SA05n2871:76": "tava |",
  "SA05n2871:80": "aparāhatamakṣayavyayaṁ vividheṣvāyataneṣu",
  "SA05n2871:81": "vartase ||6 ||",
  "SA05n2871:88": "na ca te'sti mune kathaṁ kathā vimatirnāsti na",
  "SA05n2871:89": "saṁśayaḥ kvacit |",
  "SA05n2871:93": "svayameva na te parāparaṁ viditaṁ sarvamavedi",
  "SA05n2871:94": "vidyayā ||7 ||",
  "SA05n2871:101": "priyadarśanasādhudarśanapriyasādhupriyapaṇḍitapriyaḥ",
  "SA05n2871:105": "samameva hi te priyāpriyaṁ satataṁ",
  "SA05n2871:106": "prītikarastatheryase ||8 ||",
  "SA05n2871:113": "madhurapratibhānavānasi smitavākyaḥ smṛtamān",
  "SA05n2871:114": "viśāradaḥ |",
  "SA05n2871:118": "vividānumataṁ prabhāṣase triṣu lokeṣu ca te stṛtaṁ",
  "SA05n2871:119": "yaśaḥ ||9 ||",
  "SA05n2871:126": "nṛsurāsurayakṣarākṣasā bahavastvāmiha lokapaṇḍitāḥ|",
  "SA05n2871:131": "upagamya mune punaḥ punaḥ paripṛcchānti na",
  "SA05n2871:132": "cābhiṣūyase ||10 ||",
  "SA05n2871:139": "svavacaḥ paritoṣitāstvayā naradevāḥ surayakṣarākṣasāḥ|",
  "SA05n2871:144": "pratiyānti vinītasaṁśayāścaraṇau vandya ca te",
  "SA05n2871:145": "mahāmuni ||11 ||",
  "SA05n2871:152": "sthitamāsitamāgataṁ gataṁ śayitaṁ",
  "SA05n2871:153": "maunamathābhyudīritam |",
  "SA05n2871:157": "atha cīvarapātradhāraṇaṁ ruciraṁ gautama",
  "SA05n2871:158": "sarvameva te ||12 ||",
  "SA05n2871:165": "avilambitamadrutaṁ samaṁ svaramādhuryaguṇaiḥ",
  "SA05n2871:166": "samanvitam |",
  "SA05n2871:170": "vacanaṁ punaruktavarjitaṁ samaye vyāharase",
  "SA05n2871:171": "narottama ||13 ||",
  "SA05n2871:178": "balavānasi lokaviśrutaḥ puruṣajñaḥ puruṣarṣabhaḥ",
  "SA05n2871:179": "prabhuḥ |",
  "SA05n2871:183": "na ca manyasi nāvamanyase sakalaṁ lokamimaṁ",
  "SA05n2871:184": "sadaivatam ||14 ||",
  "SA05n2871:191": "na ca vismayase kadācana prakṛtistheṣu calācaleṣu",
  "SA05n2871:192": "ca |",
  "SA05n2871:196": "suhṛdeṣu [ ca ] durhṛdeṣu ca pratikūleṣvanulomavatsu ca ||15 ||",
  "SA05n2871:203": "paridevyamadīnanisvanaṁ",
  "SA05n2871:204": "srutalālārpitasannināditam |",
  "SA05n2871:208": "paridāhavidāhasaṁyutaṁ bhayarogajvaraśokakarṣitam  ||16 ||",
  "SA05n2871:216": "prasamīkṣya jagat samākulaṁ vividhairduḥkhaśatairupadrutam|",
  "SA05n2871:221": "cirarātramanāthamutsukaṁ bhavatṛṣṇāprasṛtaṁ",
  "SA05n2871:222": "tamovṛtam ||17 ||",
  "SA05n2871:229": "avatārya munermahākṛpāṁ vividhāṁ cātmagatāṁ",
  "SA05n2871:230": "prabhāvatām |",
  "SA05n2871:234": "svayameva hi śākyapuṅgava vyathitānmocayituṁ",
  "SA05n2871:235": "tvayocyate ||19 ||",
  "SA05n2871:242": "ativīrya mahāvināyakapravarāṇāmanuvādināṁ vara|",
  "SA05n2871:247": "na ca te sadṛśaḥ kuto'dhikastriṣu lokeṣvapi",
  "SA05n2871:248": "naiva vidyate ||20 ||",
  "SA05n2871:255": "pratighānunayā na santi te na ca te santi muni",
  "SA05n2871:256": "parisravāḥ |",
  "SA05n2871:260": "anurodhavirodhavigrahāḥ satataṁ suvrata naiva",
  "SA05n2871:261": "naiva te ||21 ||",
  "SA05n2871:268": "apahāya mune priyāpriye sukhaduḥkhe vi [ ṣamaṁ samaṁ tathā ]",
  "SA05n2871:272": "aratiṁ ca ratiṁ viparyayannupaśāntaścarasīha saṁyatah  ||22 ||",
  "SA05n2871:280": "vyasane na ca nāma nirmanā na ca nāmonnamase",
  "SA05n2871:281": "praśaṁsayā |",
  "SA05n2871:285": "ayaśaśca yaśaśca te samaṁ samamākruṣṭamathāpi",
  "SA05n2871:286": "vanditam ||23 ||",
  "SA05n2871:293": "ativākyamatho titikṣase paruṣaṁ",
  "SA05n2871:294": "pāpajanairudīritam |",
  "SA05n2871:298": "samarāgragato viṣāṇavān bhṛśamuktāniva kuñjaraḥ",
  "SA05n2871:299": "śarān ||24 ||",
  "SA05n2871:306": "suvacastvamṛṣi vacaḥ kṣamaḥ sudurukteṣvapi",
  "SA05n2871:307": "nābhiṣūyase |",
  "SA05n2871:311": "samameva ca varttase mune paribhāṣāsu śubhāsu",
  "SA05n2871:312": "vākṣu ca ||25 ||",
  "SA05n2871:319": "satataṁ ca varārha pūjyase naradevāsurayakṣarākṣasaiḥ|",
  "SA05n2871:324": "ṛṣibhiśca sadā mahātmabhirna ca te vikriyate",
  "SA05n2871:325": "sthiraṁ manaḥ ||26 ||",
  "SA05n2871:332": "pravaro'hamito na manyase'tyavaro'hamito na",
  "SA05n2871:333": "manyase |",
  "SA05n2871:337": "sadṛśo'hamito na manyase trividhā mānavidhā na",
  "SA05n2871:338": "santi te ||27 ||",
  "SA05n2871:345": "dhṛtimān samaloṣṭakāñcanaḥ samavaiḍuryakaṭhallaśarkaraḥ|",
  "SA05n2871:350": "tṛṇakāṣthasamaṁ mahāmune carasīdaṁ hi sadevakaṁ",
  "SA05n2871:351": "jagat ||28 ||",
  "SA05n2871:358": "himabhāskararaśmisaṁgamācchiśiroṣṇaṁ pavanaṁ",
  "SA05n2871:359": "samudvahan |",
  "SA05n2871:363": "adhivāsayase nagendravanna ca te pravyathate",
  "SA05n2871:364": "sthiraṁ manaḥ ||29 ||",
  "SA05n2871:371": "śayanāsanapānabhojanaṁ vividhaṁ",
  "SA05n2871:372": "cīvaramuttarādharam |",
  "SA05n2871:376": "adhivāsayase nagendravanna ca te pravyathate",
  "SA05n2871:377": "mune nirāsravam ||30 ||",
  "SA05n2871:384": "na ha śocāsi nātha nāsti me na ca te santi mune",
  "SA05n2871:385": "parigrahāḥ |",
  "SA05n2871:389": "asito'si suvākyanirmamḥ parimukto vividhairupadravaiḥ  ||31 ||",
  "SA05n2871:397": "na ca lābhamavāpya hṛṣyase tadalābhācca layaṁ",
  "SA05n2871:398": "na gacchasi |",
  "SA05n2871:402": "avamānamatho titikṣase na ca",
  "SA05n2871:403": "sammānamihabhinandasi ||32 ||",
  "SA05n2871:410": "kṣatajopamamagracandanaṁ",
  "SA05n2871:411": "surabhūmandarasānusambhavam |",
  "SA05n2871:415": "asayo niśitāḥ śarāśca te na vikurvanti manaḥ",
  "SA05n2871:416": "kadācana ||33 ||",
  "SA05n2871:423": "na kathāṁ kathayasyanarthikāṁ na ca yā",
  "SA05n2871:424": "durjanasevitā kathā |",
  "SA05n2871:428": "na [ ca lābhakathā na ] sāmiṣā na ca yā nānumatā mahātmabhiḥ ||34 ||",
  "SA05n2871:435": "pravivekakathāḥ sukhāvahāḥ praśamaṁ pravadanti",
  "SA05n2871:436": "kevalam |",
  "SA05n2871:440": "kathayasyatidevatāḥ kathāḥ kathitā yā vinayanti",
  "SA05n2871:441": "kilviṣam ||35 ||",
  "SA05n2871:448": "madhurāṇi ca saṅgatāni ca svabhinītāni ca",
  "SA05n2871:449": "sāravanti ca |",
  "SA05n2871:453": "vacanāni mune prabhāṣase jagadarthāya viniṣcitāni",
  "SA05n2871:454": "ca ||36 ||",
  "SA05n2871:461": "abudhā viparītadarśanāścapalāḥ sāhasikāḥ",
  "SA05n2871:462": "priyanvitāḥ |",
  "SA05n2871:466": "piśunāḥ paruṣā śaṭhāśca ye bhagavaṁstaiḥ saha",
  "SA05n2871:467": "saṅgataṁ na te ||37 ||",
  "SA05n2871:474": "aśaṭhā ṛjavaśca ye narāḥ śucayaḥ satyaratā",
  "SA05n2871:475": "jitendriyāḥ |",
  "SA05n2871:479": "satataṁ ca samīkṣyakāriṇo bhagavaṁstaiḥ saha saṅgataṁ",
  "SA05n2871:480": "tava ||38 ||",
  "SA05n2871:487": "dharmajña nayajña pudralajña tvāṁ vande ṣaḍabhijña",
  "SA05n2871:488": "sarvadaiva |",
  "SA05n2871:492": "kṣetrajña mune parāparajña tvāṁ vande śirasā",
  "SA05n2871:493": "nayānayajña ||39 ||",
  "SA05n2871:500": "pṛthumapi samīkṣate guṇaṁ [ tava ] parikathito hi māa guṇaikadeśaḥ |",
  "SA05n2871:504": "na tava guṇamahārṇavasya pāraṁ jagati",
  "SA05n2871:505": "pumānadhigantumārya śaktaḥ ||40 ||",
  "SA05n2871:512": "[ iti bhagavatstutiḥ || ]"
}