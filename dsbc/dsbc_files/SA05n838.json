{
  "SA05n838:0": "ṣaṭtriṃśatsaṃvarastutiḥ",
  "SA05n838:1": "śrīsaṃvaraṃ mahāvīraṃ vārāhīṃ cāpi yoginīm |",
  "SA05n838:2": "namāmi sarvabhāvena yoginījālanāyakam || 1 ||",
  "SA05n838:3": "ḍākinīṃ ca tathā lāmāṃ khaṇḍarohāṃ ca rūpiṇīm |",
  "SA05n838:4": "caturo'mṛtabhāṇḍāṃśca bodhicittena pūritān || 2 ||",
  "SA05n838:5": "pullīramalayodbhūtaṃ khaṇḍakapālasaṃvaram |",
  "SA05n838:6": "pracaṇḍāliṅgaṇaṃ vīraṃ namāmi śirasi sthitam || 3 ||",
  "SA05n838:7": "jālandharātsamutpannaṃ mahākaṃkālasaṃvaram |",
  "SA05n838:8": "caṇḍālyāliṅgaṇaṃ vīraṃ śikhādeśe sthitaṃ name || 4 ||",
  "SA05n838:9": "oḍiyānātsamudbhūtaṃ kaṃkālanāma saṃvaram |",
  "SA05n838:10": "prabhāvatīsamāpannaṃ savyakarṇasthitaṃ name || 5 ||",
  "SA05n838:11": "arbudapīṭhamadhyasthaṃ saṃvaraṃ vikaṭadaṃṣṭriṇam |",
  "SA05n838:12": "mahānāsāsamāpannaṃ pṛṣṭhadeśe sthitaṃ name || 6 ||",
  "SA05n838:13": "godāvarīsamudbhūtaṃ surāvīriṇasaṃvaram |",
  "SA05n838:14": "saṃpuṭaṃ vīramatyākhyā vāmakarṇasthitaṃ name || 7 ||",
  "SA05n838:15": "rāmeśvarodbhavaṃ vīraṃ amitābhākhyasaṃvaram |",
  "SA05n838:16": "kharvarosaṃpuṭaṃ nāthaṃ bhruvormadhyasthitaṃ name || 8 ||",
  "SA05n838:17": "devīkoṭodbhavaṃ nāthaṃ vajraprabhākhyasaṃvaram |",
  "SA05n838:18": "laṅkeśvarīsamāpannaṃ namāmi netrakoṇagam || 9 ||",
  "SA05n838:19": "māravākhyātsamutpannaṃ vajradehākhyasaṃvaram |",
  "SA05n838:20": "drumacchāyāsamāpannaṃ namāmi skandhadeśagam || 10 ||",
  "SA05n838:21": "kāmarūpodbhavaṃ vīraṃ aṅgulikākhyasaṃvaram |",
  "SA05n838:22": "airāvatīsamāpannaṃ namāmi kakṣadeśagam || 11 ||",
  "SA05n838:23": "okārasthānasaṃjātaṃ vajrajaṭilasaṃvaram |",
  "SA05n838:24": "bhairavāliṅgaṇaṃ vīraṃ tanuyugodbhavaṃ name || 12 ||",
  "SA05n838:25": "triśakunyadbhavaṃ nāthaṃ mahāvīrākhyasaṃvaram |",
  "SA05n838:26": "vāyuvegāsamāpannaṃ nābhideśasthitaṃ name || 13 ||",
  "SA05n838:27": "kauśalāyāṃ samudbhūtaṃ vajrahūkārasaṃvaram |",
  "SA05n838:28": "surābhakṣīsamāpannaṃ namāmi nāsikāgrajam || 14 ||",
  "SA05n838:29": "kaliṅgapīṭhasaṃjātaṃ suprabhānāma saṃvaram |",
  "SA05n838:30": "śyāmādevī samāpannaṃ namāmi mukhabhāgajam || 15 ||",
  "SA05n838:31": "lampākasthānasaṃbhūtaṃ vajraprabhākhyasaṃvaram |",
  "SA05n838:32": "subhadrāliṅgaṇaṃ vīraṃ kaṇṭhadeśodbhavaṃ name || 16 ||",
  "SA05n838:33": "kāñcyākhyapīṭhasaṃjātaṃ mahābhairavasaṃvaram |",
  "SA05n838:34": "hayakarṇāsamāpannaṃ namāmi hṛdimadhyagam || 17 ||",
  "SA05n838:35": "himālayodbhavaṃ vīraṃ virūpākṣākhyasaṃvaram |",
  "SA05n838:36": "khagānanāsamāpannaṃ meḍhrasthānagataṃ name || 18 ||",
  "SA05n838:37": "pretapuryāṃ samudbhūtaṃ mahābalākhyasaṃvaram |",
  "SA05n838:38": "cakravegāsamāpannaṃ liṅgasthānagataṃ name || 19 ||",
  "SA05n838:39": "guhadeśātsamudbhūtaṃ ratnavajrākhyasaṃvaram |",
  "SA05n838:40": "khaṇḍarohāsamāpannaṃ namāmi gudamadhyagam || 20 ||",
  "SA05n838:41": "saurāṣṭradeśasaṃbhūtaṃ hayagrīvākhyasaṃvaram |",
  "SA05n838:42": "śauṇḍinyāliṅgaṇaṃ vīraṃ namāmi urumadhyagam || 21 ||",
  "SA05n838:43": "suvarṇadvīpasaṃjātam ākāśagarbhasaṃvaram |",
  "SA05n838:44": "cakravarmiṇīsamāpannaṃ jaṅghāmadhyagataṃ name || 22 ||",
  "SA05n838:45": "nagarapīṭhamadhyasthaṃ śrīherukākhyasaṃvaram |",
  "SA05n838:46": "suvīrāliṅgaṇaṃ vīraṃ namāmyaṅgulivartinam || 23 ||",
  "SA05n838:47": "sindhudeśasamudbhūtaṃ padmanṛtyākhyasaṃvaram |",
  "SA05n838:48": "mahābalāsamāpannaṃ pādapṛṣṭhagataṃ name || 24 ||",
  "SA05n838:49": "marutīpīṭhasaṃjātaṃ vairocanākhyasaṃvaram |",
  "SA05n838:50": "cakravartinīsamāpannaṃ namāmyaṅguṣṭhamadhyagam || 25 ||",
  "SA05n838:51": "kulatāyāḥ samudbhūtaṃ vajrasattvākhyasaṃvaram |",
  "SA05n838:52": "mahāvīryāsamāpannaṃ jānudvayagata name || 26 ||",
  "SA05n838:53": "pūrvadvāre sthitaṃ vīraṃ vajracaṇḍākhyasaṃvaram |",
  "SA05n838:54": "kākāsyāliṅgaṇaṃ vīraṃ namāmi suranāyakam || 27 ||",
  "SA05n838:55": "uttaradvāramadhyasthaṃ vajrānalākhyasaṃvaram |",
  "SA05n838:56": "ulūkāsyāsamāpannaṃ namāmi yakṣanāyakam || 28 ||",
  "SA05n838:57": "paścimadvāramadhyasthaṃ vajroṣṇīṣākhyasaṃvaram |",
  "SA05n838:58": "śvānāsyāsampuṭaṃ vīraṃ namāmi pannagādhipam || 29 ||",
  "SA05n838:59": "dakṣiṇadvāramadhyasthaṃ vajrakuṇḍalisaṃvaram |",
  "SA05n838:60": "śūkarāsyāsamāpannaṃ namāmi yamanāyakam || 30 ||",
  "SA05n838:61": "āgneyadigvimātrasthaṃ vajrayakṣākhyasaṃvaram |",
  "SA05n838:62": "yamadāḍhīsamāpannaṃ namāmi vahnināyakam || 31 ||",
  "SA05n838:63": "nairṛtyadigvibhāṣasthaṃ vajrakīlākhyasaṃvaram |",
  "SA05n838:64": "yamadūtīsamāpannaṃ namāmi rākṣasādhipam || 32 ||",
  "SA05n838:65": "vāyavyadigvibhāgasthaṃ vajramahābalāhvayam |",
  "SA05n838:66": "yamadaṃṣṭrīsamāpannaṃ namāmi pavanādhipam || 33 ||",
  "SA05n838:67": "īśānadigvibhāgasthaṃ vajrabhīṣaṇasaṃvaram |",
  "SA05n838:68": "yamamathanīsamāpannaṃ namāmi bhūtanāyakam || 34 ||",
  "SA05n838:69": "etāndevān namasyāmi digvidikṣu susaṃsthitān |",
  "SA05n838:70": "vīrān vīreśvarīḥ sarvā herukaṃ parameśvaram || 35 ||",
  "SA05n838:71": "sahajānandātmakaṃ devaṃ viśuddhaṃ tāṇḍavānvitam |",
  "SA05n838:72": "ḍākinījālamadhyasthaṃ pratyātmavedyagocaram || 36 ||",
  "SA05n838:73": "stutvedaṃ devatīcakraṃ yanmayopārjitaṃ śubham |",
  "SA05n838:74": "tena puṇyena loko'stu vajraḍākaparāyaṇaḥ || 37 ||",
  "SA05n838:75": "śrī ṣaṭtriṃśatsaṃvaragaṇacakramaṇḍalastutiḥ samāptāḥ ||"
}