{
    "855:0": "guhyeśvarīstotram",
    "855:1": "bhagavati bahurūpe nirvikāre nirañje",
    "855:2": "nimitanikhilarūpe niścayātītarūpe|",
    "855:3": "akhilanigamapāre nityanityasvabhāve",
    "855:4": "caraṇakamalayugmaṃ naumi devi tvadīyam|| 1||",
    "855:5": "hasitamukhaśaśāṅkaṃ jyotsnayā rātribhūtaṃ",
    "855:6": "daśaśatakiraṇodyadvaktramādhyandinaṃ te|",
    "855:7": "aruṇavadanaśobhā audayī cāstakāle",
    "855:8": "caraṇakamalayugmaṃ naumi devi tvadīyam|| 2||",
    "855:9": "galajaṭharapadeṣu svargamartyāhilokāḥ",
    "855:10": "sakalabhagaṇadantā romarājī drumāste|",
    "855:11": "giraya iva nitambā raktaśuklāḥ samudrā-",
    "855:12": "ścaraṇakamalayugmaṃ naumi devi tvadīyam|| 3||",
    "855:13": "pavanadahanavegāḥ śvāsapraśvāsa eva",
    "855:14": "pralayaprabhavakālo mīlanonmīlanābhyām|",
    "855:15": "dvidaśatapanabhūtā bhīmavaktrāstvadīyā-",
    "855:16": "ścaraṇakamalayugmaṃ naumi devi tvadīyam|| 4||",
    "855:17": "anupamatanudehaṃ vyāpyamānaṃ samantā-",
    "855:18": "nnikhilanigamasāraṃ darśayan devi divyam|",
    "855:19": "tribhuvanamakhilaṃ te darśitumeti buddhi-",
    "855:20": "ścaraṇakamalayugmaṃ naumi devi tvadīyam|| 5||",
    "855:21": "vidhimukhavibudhāste kiṃkarāḥ pādasaṃsthā",
    "855:22": "mukuṭavidhṛtabuddhe sarvamārgā hi śuddhe|",
    "855:23": "kimu tava mahimānaṃ varṇaye guhyadevi",
    "855:24": "caraṇakamalayugmaṃ naumi devi tvadīyam|| 6||",
    "855:25": "anupaṭhati samāptau pūjane bhaktimān yo",
    "855:26": "nutimati vitagoti spaṣṭamevāsya buddhiḥ|",
    "855:27": "sakalajanajananyā bhaktisampattimuccaiḥ",
    "855:28": "karatalavaśagāstāḥ siddhipuṣṭayau labhante|| 7||",
    "855:29": "śrī svayaṃbhūpurāṇoddhṛtaṃ mañjunāthakṛtaṃ",
    "855:30": "guhyeśvarīstotraṃ samāptam|"
}