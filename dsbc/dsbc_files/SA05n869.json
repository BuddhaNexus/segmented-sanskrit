{
  "SA05n869:0": "Buddhagadyaya (buddhagajjaya)",
  "SA05n869:1": "namo buddhāya |",
  "SA05n869:2": "1. namas samantabhadrāya sarvagocaracakṣue",
  "SA05n869:3": "karuṇāmṛtakallola­ sindhave sūryyabandhave |",
  "SA05n869:4": "2.jaya jaya dharmmarāja janakṛtaśarmmapūja",
  "SA05n869:5": "asadṛśabuddhalīla atipariśuddhaśila",
  "SA05n869:6": "3.sphuṭitagabhastijāla bahuvidhavisvapala",
  "SA05n869:7": "jagati nirastadosa- praṇamitaśastaveṣa",
  "SA05n869:8": "4. ruciravimuktibhūṣa magadhaviśiṣṭabhāṣa",
  "SA05n869:9": "daṇḍitabahumāra khaṇḍitabhavabhāra",
  "SA05n869:10": "5. maṇḍitaśubhagātha paṇḍitajananātha",
  "SA05n869:11": "añcitapadapāta sañcitahitajāta",
  "SA05n869:12": "6. sāraharivaraśayana tāmarasanibhanayana",
  "SA05n869:13": "lokajanasukhakaraṇa lolasuranatacaraṇa",
  "SA05n869:14": "7. kālajayacaturatara śiladharadivasakara",
  "SA05n869:15": "bālajanakumatihara ālavakadamanakara",
  "SA05n869:16": "8. suranarapūjanīyāryya budhajanabodhanāvīryya",
  "SA05n869:17": "parahitasādhanākāryya tribhuvanaśāsanācāryya",
  "SA05n869:18": "9. dānavāripadmāsanastha dhīrapūtacitrāsanastha",
  "SA05n869:19": "vītamoharudrāsanastha bodhimūlavajrāsanastha",
  "SA05n869:20": "10. yajñādhīśaguṇapraṇatijña vijñādhāravarasphuṭavijña",
  "SA05n869:21": "ajñānāribalapraharajña prajñādhānamahatṣaḍabhijña",
  "SA05n869:22": "11. dhiṣaṇāmayasārasāyaka vipadākulahetuhāyaka",
  "SA05n869:23": "ajarāmarabhūtidāyaka sacarācaralokanāyaka",
  "SA05n869:24": "12. praśrutamṛduvācācandana niśrutasurapūjāvindana",
  "SA05n869:25": "suśrutajanamāyānandana viśrutakula-māyānandana",
  "SA05n869:26": "13. iṣṭavinaṣṭasamujjhitakopa tuṣṭavariṣṭhavivarjjitapāpa",
  "SA05n869:27": "śiṣṭaviśiṣṭavinirjjitatāpa aṣṭacatuṣṭayalakṣaṇarūpa",
  "SA05n869:28": "14. yogyabhāvagaṇanāhṛtasīma bhāgyadāyajanamodanarāma",
  "SA05n869:29": "vākyaleśapariśoṣitakāma sākyarājakulasāgarasoma",
  "SA05n869:30": "15. preritaśāsananītiviśāla dhāritabodhimanoharamūla",
  "SA05n869:31": "vārita-māravadhūjanalīla pūritapāramitādaśaśīla",
  "SA05n869:32": "16. kāmaprabhātārarocirddineśa hemaprabhāśobhamānapradeśa",
  "SA05n869:33": "śyāmaprabhāpūraṇānarghakeśa vyāmaprabhālokakaprakāśa",
  "SA05n869:34": "17. cīrṇākhilajñānayogarddhicandra pūrṇādharaccedanerāddhacandra",
  "SA05n869:35": "pūrṇāgamakhyātadhīrārddhacandra ūrṇārucicchedabālārddhacandra",
  "SA05n869:36": "18. kumbhodayādhīśaviśvatyarāja dambholiśālīśavistutyarāja",
  "SA05n869:37": "ambhodhinīkāśasatkarmmarāja śambhodhivāgīśasaddharmmarāja",
  "SA05n869:38": "19. nikuñcadbhavādīnavāśeṣabuddha sadañcadguṇāveśaśīlādiśuddha",
  "SA05n869:39": "viriñcatprabhābhedaśobhādisiddha udañcadmahāketumālānubaddha",
  "SA05n869:40": "20. anusamayaśubhasārabhāṣaṇa atulaguṇadhara vītabhīṣaṇa",
  "SA05n869:41": "atimalinabhavarāgaroṣaṇa aparamitavaraśīlabhūṣaṇa",
  "SA05n869:42": "21. prasuta prahita prasava pracala prathita praguṇa pramada praphala",
  "SA05n869:43": "praṇatapracura prabhavapradala prakaṭapraṇaya pratima prabala",
  "SA05n869:44": "22. nistārivikhyātaviśvajanaśaraṇa astārisaddhīravṛndaśivakaraṇa",
  "SA05n869:45": "śastārisattyāgasiddhakaracaraṇa vistāriṣaḍbhedavarṇavarakiraṇa",
  "SA05n869:46": "23. vyaktikaravinyāsasādhujanasaṅga bhaktikarasammānanādimadhubhṛṅga",
  "SA05n869:47": "bhaktidharasaṃsārabhedaraṇaraṅga muktipurakalyāṇamodamunituṅga",
  "SA05n869:48": "24. sarasijaniśrutaviracitatantra sapaditiraskṛtabhavabhayayantra",
  "SA05n869:49": "budhajanaśikṣitabahuvidhatantra tribhuvanaviśrutanavaguṇamantra",
  "SA05n869:50": "25. bahulolakathanīyarāvādhirāva guṇaśīlamahanīyasevādhiseva",
  "SA05n869:51": "avipākaramaṇīyabhāvādhibhāva jagadekagaraṇīyadevādhideva",
  "SA05n869:52": "26. tapanaśaśiśikhimahassatyāvabodhana anayakulamayatapassatyāvabādhana",
  "SA05n869:53": "aparimitaguṇavacassatyāvasādhana sakalajanahitacatussatyāvabodhana",
  "SA05n869:54": "27. agatilakatalanarmmacakrapramarddana sugatikarabahukarmmacakrapravarddhana",
  "SA05n869:55": "dugatimayabhavakarmmacakrapramarddana jagatikṛtavaradharmmacakrapravarttana",
  "SA05n869:56": "28. siddhāgamasadbhāvanasujñāpitapāka buddhāgamavikhyāpanavijñāpitanāka",
  "SA05n869:57": "śuddhāgamavisphāraṇavidhmāpitaśoka ṛddhyāgamavistāraṇavijñāpitaloka",
  "SA05n869:58": "29. mṛtasamṛnmayānnapātralabdhadevatāñcana vatsalatvadhāmagātraluptaśobhakāñcana",
  "SA05n869:59": "utsavṛndaśītamañjucitrajātivāñcana sat-samantakūṭaśṛṅgadattapādalāñchana",
  "SA05n869:60": "30. prathitacaritasaṅgagītivītakadana amitavimaladharmmaśīlanītisadana",
  "SA05n869:61": "bahuladhavalapuñjahāracāruradana śiśirakiraṇamaṇḍalābhirāmavadana",
  "SA05n869:62": "31. jātadivasīyamahamoditapitāmaha vītamalalokahitapūtakuśalāvaha",
  "SA05n869:63": "bāhubalaghoraraṇa -mārabahuvīraha rāhunayanāviṣayapādasarasīruha",
  "SA05n869:64": "32. premapūgasīmabhūrisomasāravācana bhīmabhāvabhāmarāganāmarūpasūdana",
  "SA05n869:65": "vāmadeva-kāmanāthasāmapādarodhana hemarāśidhāmacārunāmarūpaśobhana",
  "SA05n869:66": "33. vādabaddhavisphuratkudṛṣṭidṛṣṭisañcaya bhedacittasambhavaprapañcapattisañcaya",
  "SA05n869:67": "pādapadmasañcaratpraphullapadmasañcaya vedasiddhaśemuṣīprabhagnabhedyasañcaya",
  "SA05n869:68": "34. akhilalokapūjitaprahitasarvaśocana sakalatāpavedanaprabhavasaṅgamocana",
  "SA05n869:69": "madhurasāraśītalaprakaṭabhavyavācana nirvaśeṣagocarapratatabuddhilocana",
  "SA05n869:70": "35. kiraṇanivahapramaṇḍitavitatapuṣkara kaluṣaharaṇasphurannavalalitapuṣkara",
  "SA05n869:71": "gamanasamayasvayannadadamitapuṣkara sakalabhuvanapriyaṅkaracaraṇapuṣkara",
  "SA05n869:72": "36. nityapeśalanityadakṣiṇanityabhāvatathāgata nityabhāsuranityadeśitanityabhāvatathāgata",
  "SA05n869:73": "nityavismayanityavatsalanityamodatathāgata nityamaṅgalanityasundaranityapūjyatathāgata",
  "SA05n869:74": "37. trividhaśubhacalanavarajalajanibhayugala vinayamayavividhamanusamitaduritagala",
  "SA05n869:75": "mathitabahuviṣayabharabharitabhavanigala praṇatasuramakuṭamaṇighaṭitapadayugala",
  "SA05n869:76": "38. helāviracitadolāyitajanamūlāgamavidhidīpanavāṭa hālāhalanibhaśālābhavaśubhalīlāvidalanaśāsanajhāṭa",
  "SA05n869:77": "bālānayamatijālādaraharaśīlāyudhadharavāridakūṭa kolāhalakara-nālāgirigajakālākṛtimadavegavighāṭa",
  "SA05n869:78": "39. vanamuditavispaṣṭamadhurataranāda janahṛdayamaddhyasthakamalamṛdupāda",
  "SA05n869:79": "vinayabalaviccinnaduritakulabheda anavaratasaddharmmavitaraṇavinoda",
  "SA05n869:80": "40. aruṇapadatalasakalaviṣayakavimaladaśavidhamatibala karuṇajalanidhilahariparikarasatatakuśalitamahitala",
  "SA05n869:81": "viśadadinamaṇikiraṇavikasitakamalamṛdutalakaratala daśadigupagatasurabhikulavaravadanavikasitaparimala.",
  "SA05n869:82": "41. anagham atimanojñaṃ dharmmarājasya gadyaṃ likhati paṭhati yo vā sādaraṃ yaś śruṇoti",
  "SA05n869:83": "tribhuvanabhavanāntaḥ prāpya bhogān aśeṣān anubhavati sa nityaṃ śāntanirvāṇasaukhyam.",
  "SA05n869:84": "siddhir astu. śubham astu.",
  "SA05n869:85": "buddhagadyaya nimi."
}