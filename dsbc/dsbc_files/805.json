{
    "805:0": "ākāśagarbhanāmāṣṭottaraśatastotram",
    "805:1": "om namo buddhāya",
    "805:2": "ākāśagarbha sattvārtha mahāsattva mahādyute|",
    "805:3": "mahāratna suratnāgrya vajraratna namo'stu te|| 1||",
    "805:4": "abhiṣekamahāratna mahāśuddha mahāśubha|",
    "805:5": "buddharatna viśuddhāṅga ratnaratna namo'stu te|| 2||",
    "805:6": "ākāśākāśasaṃbhūta sarvākāśa mahānabha|",
    "805:7": "ākāśadhātusarvāśa sarvāśāgrya namo'stu te|| 3||",
    "805:8": "ratnasaṃbhava ratnorṇa buddhorṇa sutathāgata|",
    "805:9": "sarvaratna susarvāgrya ratnakārya namo'stu te|| 4||",
    "805:10": "ratnaratnāgrya ratnogra ratnasarva tathāgata|",
    "805:11": "ratnottama mahākāśa samākāśa namo'stu te|| 5||",
    "805:12": "alaṅkāramahāśobha śobhākara suśobhaka|",
    "805:13": "śuddhasarvārtha śuddhārtha dānacarya namo'stu te|| 6||",
    "805:14": "dharmaratna viśuddhāgrya saṅgharatna tathāgata|",
    "805:15": "mahābhiṣeka lokārtha pramodārtha namo'stu te|| 7||",
    "805:16": "dāna pradāna dānāgrya tyāga tyāgāgrya dāyaka |",
    "805:17": "sarvasattvārtha tattvārtha mahārthārtha namo'stu te || 8 ||",
    "805:18": "cintārāja mahāteja dānapāramitānaya|",
    "805:19": "tathāgata mahāsattva sarvabuddha namo'stu te|| 9||",
    "805:20": "tathāgata mahāratna tathāgata mahāprabha|",
    "805:21": "tathāgata mahāketo mahāhāsa namo'stu te|| 10||",
    "805:22": "tathāgatābhiṣekājña mahābhiṣeka mahāvibho|",
    "805:23": "lokanātha trilokāgrya lokasūrya namo'stu te|| 11||",
    "805:24": "ratnādhikādhikatara ratnabhūṣaṇa ratnadhṛk|",
    "805:25": "ratnāloka mahāloka ratnakīrte namo'stu te|| 12||",
    "805:26": "ratnotkara suratnottha maṇe vajramaṇe guṇa|",
    "805:27": "ratnākara sudīptāṅga sarvaratna namo'stu te|| 13||",
    "805:28": "mahātmayaṣṭi ratneśa sarvāśāparipūraka|",
    "805:29": "sarvābhiprāyasaṃprāptiratnarāśi namo'stu te|| 14||",
    "805:30": "abhvagrya vyāpi sarvātma varaprada mahāvara|",
    "805:31": "vibhūte sarvasaṃpatte vajragarbhaṃ namo'stu te|| 15||",
    "805:32": "yaḥ kaścid dhārayen nāmnāmidante'ṣṭaśataṃ śivam|",
    "805:33": "sarvabuddhābhiṣekaṃ tu sa prāpnotyanaghaḥ kṣaṇāt|| 16||",
    "805:34": "śrī ākāśagarbhanāmāṣṭaśatādhyeṣaṇāstotraṃ saṃpūrṇam|"
}