{
    "816:0": "avalokiteśvarastotram",
    "816:1": "vāsukināgarājakṛtam",
    "816:2": "om namo'valokiteśvarāya",
    "816:3": "jaṭādharaṃ saumyaviśālalocanaṃ",
    "816:4": "sadāprasannānanacandramaṇḍalam |",
    "816:5": "surāsurairvanditapādapaṅkajaṃ",
    "816:6": "namāmi nāthaṃ maṇipadmasaṃbhavam ||1||",
    "816:7": "sarojapatrāyatadivyalocanaṃ",
    "816:8": "kudṛṣṭisaṃśodhitaśuddhalocanam |",
    "816:9": "kṛpāmṛtārdraṃ jagadekalocanaṃ",
    "816:10": "namāmi nāthaṃ maṇipadmasaṃbhavam ||2||",
    "816:11": "hārenduhārārdhahimādhikojjavalaṃ",
    "816:12": "nighṛṣṭagaṇḍāmalalolakuṇḍalam |",
    "816:13": "gabhastimālākulakoṭisaṃkulaṃ",
    "816:14": "namāmi nāthaṃ maṇipadmasaṃbhavam ||3||",
    "816:15": "prabuddhadharmādhvani dharmadhātukaṃ",
    "816:16": "sahasrabāhuṃ dvicatuśca ṣaḍbhujam |",
    "816:17": "khadhātunā tulyamanantabāhukaṃ",
    "816:18": "namāmi nāthaṃ maṇipadmasaṃbhavam ||4||",
    "816:19": "upāyaprajñodadhimanthanodbhavaṃ",
    "816:20": "tridhātusaṃrakṣaṇahetusaṃbhavam |",
    "816:21": "jinendramauliṃ jinadhātusaṃbhavaṃ",
    "816:22": "namāmi nāthaṃ maṇipadmasaṃbhavam ||5||",
    "816:23": "padmopari gataṃ nāthaṃ padmahastaṃ jaṭādharam |",
    "816:24": "āryāvalokitaṃ vande sarvasattvānukampakam ||6||",
    "816:25": "śrīvāsukināgarājakṛtamāryāvalokiteśvarastotraṃ samāptam |"
}