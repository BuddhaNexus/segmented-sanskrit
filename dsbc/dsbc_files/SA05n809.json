{
  "SA05n809:0": "(ārya)tārāstutiḥ",
  "SA05n809:1": "ācārya candradāsakṛtā",
  "SA05n809:2": "namastārāyai",
  "SA05n809:3": "sukhadacakracārucūḍāmaṇiruciramarīcisaṃcaya-",
  "SA05n809:4": "pracuraśikhāpracāraparicumbitacarcitacaraṇacandrake |",
  "SA05n809:5": "jagati carācare'pi sācīkṛtacakitakṛpālulocane",
  "SA05n809:6": "stutivacanopacāramucitācaratiṃ racayāmi devi te || 1 ||",
  "SA05n809:7": "nakharakaṭhorakoṭikuṭṭitakarikaṭataṭapāṭanotkaṭāḥ",
  "SA05n809:8": "pravikaṭarudhirapaṭalapaṭapālitā aṭavīviluṇṭhakāḥ |",
  "SA05n809:9": "vikaṭasaṭāṭṭahāsaghaṭitāvaṭataṭaghaṭānodbhaṭāstvajjuṣi",
  "SA05n809:10": "jhaṭiti yānti saṭinaḥ kuṇṭhādapi kuṇṭhaśaktitām || 2 ||",
  "SA05n809:11": "malinamahākapolatalavigalitamadajalamalanavihvalāḥ",
  "SA05n809:12": "skhaladalijālabahalakolāhalalitavilāsalāsinaḥ |",
  "SA05n809:13": "vicalitakarṇatālapavanāhatalalitavilepadhūlayaḥ",
  "SA05n809:14": "pathi na gajāścalanti kulaśailatulāstava lā(nā)malālitāḥ || 3 ||",
  "SA05n809:15": "mārutaghātajātarabhasocchaladanalaśikhākadambaka-",
  "SA05n809:16": "pratihatapurapurandhrihāhāravatvaritadigantabhairavaḥ |",
  "SA05n809:17": "uddhatadhūmradhūmadhūlidhutabaddhaghanāndhakārakaṃ",
  "SA05n809:18": "tvannatinītigatireti śamaṃ bahuśo hutāśanaḥ || 4 ||",
  "SA05n809:19": "calitalatāvitānakuṭilodgamadurgamagahanavartinaḥ",
  "SA05n809:20": "sapadi puro narasya tāreti manāgapi nāmadhāriṇaḥ |",
  "SA05n809:21": "sphītaphaṇaughaghoraphūtkāraparisphuratānalasphuṭa-",
  "SA05n809:22": "sphuraduruvisphuliṅgavisphāriṇi phaṇini viṣaṃ vinaśyati || 5 ||",
  "SA05n809:23": "kṣaṇakṛtakopakampakarakarṣitakharakaravālanirmala-",
  "SA05n809:24": "vyatikarakarakaravālavikarālamahābalabhujārgalaḥ |",
  "SA05n809:25": "prasthitapathikanikaṭakaṭavighaṭanapaṭuratiniṣṭhurāśayo",
  "SA05n809:26": "bhagavati bhaktivantumupasarpati tava na vane'pi taskaraḥ || 6 ||",
  "SA05n809:27": "yo'pi narendravīrahuṅkārakacagrahanigraho",
  "SA05n809:28": "grahagrasta iva rajjuhiñjīravajarjaritāṅgapañjaraḥ |",
  "SA05n809:29": "pratipadakhanakhanāyamānamukharīkṛtakharakharaśṛṅkhalāvali-",
  "SA05n809:30": "stvaccaraṇāravindamabhivandya sa nandati muktabandhanaḥ || 7 ||",
  "SA05n809:31": "kalakalakalilalolakallolajalollalatkālikānilā-",
  "SA05n809:32": "sphālitavipulabahalavelākulakūlatamālapallavāt |",
  "SA05n809:33": "sarabhasamakaranikarakharanakharasudustarato'pi sāgarāt",
  "SA05n809:34": "tāriṇi taralatārataratārakamāturametya rakṣasi || 8 ||",
  "SA05n809:35": "sūkṣmavirāvasārasaraghotkaranirbharaghoraghargharaghrāṇā",
  "SA05n809:36": "ghṛṇāṅaghripāṇyasravisrīkṛtakvathitaśarīrapañjarā |",
  "SA05n809:37": "yatkṣaṇamahāprasādāveśatvatpraṇatā tāriṇi kāmarūpiṇā",
  "SA05n809:38": "tatkṣaṇalabdhalolakiraṇamaṇikuṇḍalamaṇḍitagaṇḍamaṇḍalā || 9 ||",
  "SA05n809:39": "yūkavikīrṇaśīrṇapaṭakarpaṭakaṭitaṭaveṣṭanodbhaṭaḥ",
  "SA05n809:40": "saṃkaṭapeṭṭapūramātrārjanaparapurapiṇḍatarkaṇaḥ |",
  "SA05n809:41": "yadi tava nāmakaṃ hṛdi karoti hi rākṣasaikakaḥ",
  "SA05n809:42": "prauḍhavadhūvidhūtacāmīkarakhacitavicitracāmaram || 10 ||",
  "SA05n809:43": "śrīcandradāsaviracitā''ryatārāstutiḥ samāptā |"
}