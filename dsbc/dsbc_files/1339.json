{
    "1339:0": "yānā'nuttaryaparicchedaḥ pañcamaḥ",
    "1339:1": "ānuttaryaṃprapattau hi punarālambane matam|",
    "1339:2": "samudāgama uddiṣṭaṃ pratipattistu ṣaḍ vidhā||1||",
    "1339:3": "paramā'tha manaskāre anudharme'ntavarjane|",
    "1339:4": "viśiṣṭā cāviśiṣṭā ca paramā dvādaśātmikā||2||",
    "1339:5": "audāryamāyatatvañca adhikaro'kṣayātmatā|",
    "1339:6": "nairantaryamakṛcchratvaṃ vittatvañca parigrahaḥ||3||",
    "1339:7": "ārambhaprāptiniṣyandaniṣpattiḥ paramā matā|",
    "1339:8": "tataśca paramārthena daśa pāramitā matāḥ||4||",
    "1339:9": "dānaṃ śīlaṃ kṣamā vīryaṃ dhyānaṃ prajñā upāyatā|",
    "1339:10": "praṇidhānaṃ balaṃ jñānametāḥ pāramitā daśa||5||",
    "1339:11": "anugraho'vighātaśca karma tasya ca marṣaṇam|",
    "1339:12": "guṇavṛddhiśca sāmarthyamavatāravimocane||6||",
    "1339:13": "akṣayatvaṃ sadā vṛttirniyataṃ bhogapācane|",
    "1339:14": "yathāprajñaptito dharmamahāyānamanaskriyā||7||",
    "1339:15": "bodhisattvasya satataṃ prajñayā triprakārayā|",
    "1339:16": "dhātupuṣṭayaipraveśāya cārthasiddhyai bhavatyasau||8||",
    "1339:17": "saṃyuktā dharmacaritaiḥ sā jñeyā daśabhiḥ punaḥ|",
    "1339:18": "lekhanā pūjanā dānaṃ śravaṇaṃ vācanod grahaḥ||9||",
    "1339:19": "prakāśanā'tha svādhyāyaścintanā bhāvanā ca tat|",
    "1339:20": "ameyapuṇyaskandhaṃ hi caritaṃ tad daśātmakam||10||",
    "1339:21": "viśeṣādakṣayatvācca parānugrahato'śamāt|",
    "1339:22": "avikṣiptā'viparyāsapraṇatā cā'nudhārmikī||11||",
    "1339:23": "vyutthānaṃ viṣaye sārastathāsvādalayoddhavaḥ|",
    "1339:24": "sambhāvanā'bhisandhiśca manaskāre'pyahaṃkṛtiḥ||12||",
    "1339:25": "hīnacittañca vikṣepaḥ parijñeyo hi dhīmatā|",
    "1339:26": "vyañjanā'rthamanaskāre'visāre lakṣaṇadvaye||13||",
    "1339:27": "aśuddhaśuddhāvāgantukatve'trāsitā'nunnatau|",
    "1339:28": "saṃyogāt saṃstavāccaiva viyogādapyasaṃstavāt||14||",
    "1339:29": "arthasattvamasattvañca vyañjane so'viparyayaḥ|",
    "1339:30": "dvayena pratibhāsatvaṃ tathā cā'vidyamānatā||15||",
    "1339:31": "arthe sa cā'viparyāsaḥ sadasattvena varjitaḥ|",
    "1339:32": "tajjalpabhāvito jalpamanaskārastadāśrayaḥ||16||",
    "1339:33": "manaskāre'viparyāso dvayaprakhyānakāraṇe|",
    "1339:34": "māyādivadasattvañca sattvañcā'rthasya tanmatam||17||",
    "1339:35": "so'visāre'viparyāso bhāvā'bhāvā'visārataḥ|",
    "1339:36": "sarvasya nāmamātratvaṃ sarvakalpā'pravṛttaye||18||",
    "1339:37": "svalakṣaṇe'viparyāsaḥ paramārthe svalakṣaṇe|",
    "1339:38": "dharmadhātuvinirmukto yasmād dharmo na vidyate||19||",
    "1339:39": "sāmānyalakṣaṇantasmāt sa ca tatrā'viparyayaḥ|",
    "1339:40": "viparyastamanaskārā'vihāniparihāṇitaḥ||20||",
    "1339:41": "tadaśuddhirviśuddhiśca sa ca tatrā'viparyayaḥ|",
    "1339:42": "dharmadhātorviśuddhatvāt prakṛtyā vyomavatpunaḥ||21||",
    "1339:43": "dvayasyāgantukatvaṃ hi sa ca tatrā'viparyayaḥ|",
    "1339:44": "saṃkleśaśca viśuddhiśca dharmapudgalayorna hi||22||",
    "1339:45": "asattvāt trāsatāmānau nā'taḥ so'trā'viparyayaḥ|",
    "1339:46": "pṛthaktvaikatvamantaśca tīrthyaśrāvakayorapi||23||",
    "1339:47": "samāropā'pavādā'nto dvidhā pudgaladharmayoḥ|",
    "1339:48": "vipakṣapratipakṣā'ntaḥ śāśvatocchedasaṃjñitaḥ||24||",
    "1339:49": "grāhyagrāhakasaṃkleśavyavadāne dvidhā tridhā|",
    "1339:50": "vikalpadvayatā'ntaśca sa ca saptavidho mataḥ||25||",
    "1339:51": "bhāvā'bhāve praśāmye'tha śamane trāsyatadbhaye|",
    "1339:52": "grāhyagrāhe'tha samyaktvamithyātve vyāpṛtau na ca||26||",
    "1339:53": "ajanmasamakālatve sa vikalpadvayā'ntatā|",
    "1339:54": "viśiṣṭā cā'viśiṣṭā ca jñeyā daśasu bhūmiṣu||27||",
    "1339:55": "vyavasthānantato dhātuḥ sādhyasādhanadhāraṇā|",
    "1339:56": "avadhārapradhārā ca prativedhaḥ pratānatā||28||",
    "1339:57": "pragamaḥ praśaṭhatvañca prakarṣālambanammatam|",
    "1339:58": "avaikalyā'pratikṣepo'vikṣepaśca prapūraṇā||29||",
    "1339:59": "samutpādo nirūḍhiśca karmaṇyatvā'pratiṣṭhitā|",
    "1339:60": "nirāvaraṇatā tasyā'praśrabdhisamudāgamaḥ||30||",
    "1339:61": "śāstraṃ madhyavibhāgaṃ hi gūḍhasārārthameva ca|",
    "1339:62": "mahārthañcaiva sarvārthaṃ sarvā'narthapraṇodanam||31||",
    "1339:63": "||iti yānānuttarya paricchedaḥ pañcamaḥ||",
    "1339:64": "samāptā madhyāntavibhāgakārikāḥ"
}