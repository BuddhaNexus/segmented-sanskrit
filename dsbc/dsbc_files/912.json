{
    "912:0": "vasudhārāstotram",
    "912:1": "yā samyaguktavidhibhiḥ paripathyamānā",
    "912:2": "lakṣmīṃ dadāti vipulāṃ sugatopabhogān |",
    "912:3": "tāṃ dhāraṇīṃ sakalasattvahitaikacittāṃ",
    "912:4": "bhaktyā namāmi satataṃ vasudhārasaṃjñām || 1 ||",
    "912:5": "yā saṃsmṛtā sucirasusthataraṃ pravṛddhaṃ",
    "912:6": "dāridrayaduḥkhaduritaṃ śamate narāṇām |",
    "912:7": "tāṃ kalpavṛkṣasadṛśīṃ vasudhārasaṃjñāṃ",
    "912:8": "nityaṃ namāmi śirasā jagatāṃ hitāya || 2 ||",
    "912:9": "ratnākarīṃ ratnanidhānakoṣāṃ",
    "912:10": "vicitraratnāṃ pratibhāsavarṣām |",
    "912:11": "ratnāvalīṃ ratnamayīṃ vicitrāṃ",
    "912:12": "namāmi cāryāṃ vasudhāradhārām || 3 ||",
    "912:13": "tāṃ ratnapāṇisaphalāmavabhāsayā te",
    "912:14": "tyāgāvataṃsadayaśekharamudvahantīm |",
    "912:15": "hārārdhahāracalakuṇḍalabhūṣitāṅgī-",
    "912:16": "māryāṃ namāmi vasuvṛṣṭikarīṃ śaraṇyām || 4 ||",
    "912:17": "śrīvasudhārākalpoddhṛtaṃ vasudhārāstotraṃ samāptam |"
}