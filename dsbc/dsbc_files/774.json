{
    "774:0": "tṛtīyo binduḥ",
    "774:1": "sthityāhārabhavāḥ",
    "774:2": "1 | catasro vijñānasthitayaḥ |",
    "774:3": "katamāścatasraḥ |",
    "774:4": "rūpa (vijñānasthitiḥ)",
    "774:5": "vedanā (vijñānasthitiḥ)",
    "774:6": "saṃjñā (vijñānasthitiḥ)",
    "774:7": "saṃskāra(vijñānasthitiśca)|",
    "774:8": "kāmadhātau rūpadhātau ca bhūyo rūpālambanā vijñānasthitiḥ |",
    "774:9": "ākāśānantyāyatane vijñānānantyāyatane ca bhūyo vedanālambanā vijñānasthitiḥ |",
    "774:10": "ākiṃcanyāyatane bhūyaḥ saṃjñālambanā vijñānasthitiḥ |",
    "774:11": "naiva saṃjñānāsaṃjñāyatane bhūyaḥ saṃskārālambanā vijñānasthitiḥ ||",
    "774:12": "2 | sattvasthitivṛddhihetava āhārāścaturvidhāḥ |",
    "774:13": "āhārāścatvāraḥ katame |",
    "774:14": "kavalīkāra āhāraḥ prathamaḥ |",
    "774:15": "sparśāhāro dvitīyaḥ |",
    "774:16": "manaḥsaṃcetanikāhārastṛtīyaḥ |",
    "774:17": "vijñānāhāraścaturthaḥ ||",
    "774:18": "3 | kavalīkārāhārasya triṣu gaṃdharasasparśāyataneṣu saṃgrahaḥ |",
    "774:19": "kasmādrūpāyatane na saṃgrahaḥ |",
    "774:20": "yasmāccakṣurdarśanāhāreṇa sattvanikāyasya mahābhūtānāṃ nopacayastasmāt ||",
    "774:21": "4 | kavalīkārāhāro dvividhaḥ |",
    "774:22": "kharo mṛduśca |",
    "774:23": "katamaḥ kharaḥ |",
    "774:24": "sarvo hi bhaktāpūpādiḥ |",
    "774:25": "katamo mṛduḥ |",
    "774:26": "peyaṃ surabhikāyānuvilepanādi ||",
    "774:27": "5 | katamaḥ sparśāhāraḥ |",
    "774:28": "cakṣuḥ sparśāhāraḥ śrotra ghrāṇa jihvā kāyasparśāhāraḥ |",
    "774:29": "sāsravamanaḥsparśāhāraśca |",
    "774:30": "(ta ime)santānānucchedena pare&#39;",
    "774:31": "pi loke pravartante ||",
    "774:32": "6 | pakṣiṇāṃ haṃsādīnāṃ bhūyaḥ sparśāhāraḥ |",
    "774:33": "jalakīṭāṇḍajamatsyādīnāṃ bhūyo manaḥsaṃcetanikāhāraḥ |",
    "774:34": "naivasaṃjñānāsaṃjñāyatanāntarābhavasattvānāṃ bhūyo vijñānāhāraḥ ||",
    "774:35": "7 | kāmadhātau bhūyaḥ kavalokārāhāraḥ |",
    "774:36": "(anye)traya āhārā bhūyasā rūpārūpadhātvoḥ ||",
    "774:37": "8 | ādyaḥ kavalīkārāhāraḥ kharaḥ |",
    "774:38": "sparśāhāro mṛduḥ |",
    "774:39": "manaḥsaṃcetanikāhāro mṛdutaraḥ |",
    "774:40": "vijñānāhāro mṛdutamaḥ ||",
    "774:41": "9 | caturvidhāḥ sattvāḥ |",
    "774:42": "aṃḍajā jarāyujāḥ saṃsvedajā aupapādukāḥ |",
    "774:43": "narakā devā antarābhavāśceti sarva aupapādukāḥ |",
    "774:44": "pretānāmasurāṇāṃ ca dvividhaṃ janma jarāyujamaupapādukaṃ ca |",
    "774:45": "anyeṣāṃ sattvānāṃ caturvidha janma ||",
    "774:46": "aupapādukāḥ sattvā yugapallabhante ṣaḍindriyāṇi |",
    "774:47": "anyāsu tisṛṣūpapattiṣu kāyendriyajīvendriyayoḥ prathamaṃ lābhaḥ |",
    "774:48": "anyeṣāmindriyāṇāṃ krameṇa lābhaḥ ||",
    "774:49": "10 | catvāro bhavāḥ |",
    "774:50": "upapattibhavaḥ |",
    "774:51": "mṛtyubhavaḥ |",
    "774:52": "mūlabhavaḥ |",
    "774:53": "antarābhavaḥ ||",
    "774:54": "11 | mṛtyupapattyorantare (vartamānāḥ)",
    "774:55": "sūkṣmāḥ paṃcaskandhā antarābhavo nāma |",
    "774:56": "antarābhava upapattibhavaśca tulyamudrāvarṇau tathāhi pitā putraśca ||",
    "774:57": "12 | arūpadhātuṃ vihāyānyadhātusattvā labhante&#39;",
    "774:58": "ntarābhavaṃ ||",
    "774:59": "arūpadhātusattvāścyavantaḥ kāmadhātāvutpatsyamānā labhante&#39;",
    "774:60": "ntarābhavaṃ ||",
    "774:61": "tathā hyanāgāmibhave&#39;",
    "774:62": "ntarābhavaḥ |",
    "774:63": "anyāsūpapattiṣvapyevamantarābhavopapattibhavau ||",
    "774:64": "[ityabhidharmāmṛtaśāstre sthityāhārabhavanirdeśo nāma tṛtīyo binduḥ ||]"
}