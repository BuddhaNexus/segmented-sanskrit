{
  "SA05n2888:3": "Dvādaśakāranayastotram",
  "SA05n2888:10": "namaḥ śākyamunaye buddhāya (1)",
  "SA05n2888:23": "upāyakauśalyakṛpāluśākya-",
  "SA05n2888:27": "vaṃśyaṃ hatānyāhatamārasenam |",
  "SA05n2888:31": "suvarṇamerūpamacārukāya-",
  "SA05n2888:35": "menaṃ hi śākyādhipatiṃ namāmi ||",
  "SA05n2888:42": "ādau samutpāditabodhicitto",
  "SA05n2888:46": "yo jñānapuṇyobhayasaṃbhṛtaḥ san |",
  "SA05n2888:50": "udāralīlābhirihādya kāle",
  "SA05n2888:54": "jātaṃ jagannāthamahaṃ stavīmi ||1 || (2)",
  "SA05n2888:67": "devān vināyyātha vinītakālaṃ",
  "SA05n2888:71": "jñātvā'vatīryāpi ca devalokāt |",
  "SA05n2888:75": "kukṣiṃ viśantaṃ gajavad hi māyā-",
  "SA05n2888:79": "devyā vilokyaiva kulaṃ namāmi ||2 || (3)",
  "SA05n2888:92": "janiṃ bhajantaṃ khalu lumbinīvane",
  "SA05n2888:96": "śākyātmajaṃ vai daśamāsapūrtau |",
  "SA05n2888:100": "brahmendravandyaṃ varalakṣaṇaṃ ca",
  "SA05n2888:104": "bodheḥ kule taṃ niyataṃ namāmi ||3 ||",
  "SA05n2888:111": "nṛsiṃhamaṅge magadhe ca śilpaṃ",
  "SA05n2888:115": "pradarśayantaṃ balinaṃ kumāram |",
  "SA05n2888:119": "nigṛhya cāhaṃkṛtinaḥ samastān",
  "SA05n2888:123": "atulyakarmāntakaraṃ namāmi ||4 || (4)",
  "SA05n2888:136": "dharmānuvṛttyai jagato hyavadya-",
  "SA05n2888:140": "hānyāyupāye kuśalatvapūrvam |",
  "SA05n2888:144": "antaḥpure yaḥ kṛtasaṃnivāsaḥ",
  "SA05n2888:148": "taṃ rājyarakṣāpravaṇaṃ namāmi ||5 ||",
  "SA05n2888:155": "niḥsāratāṃ sāṃsṛtikīṃ vilokya",
  "SA05n2888:159": "tyaktvā gṛhaṃ vyomapathena gatvā |",
  "SA05n2888:163": "pravrājayantaṃ nijamātmanā taṃ",
  "SA05n2888:167": "viśuddhacaityābhimukhaṃ namāmi ||6 || (5)",
  "SA05n2888:180": "matvodyamenaiva hi bodhisiddhiṃ",
  "SA05n2888:184": "varṣāṇi ṣaḍ duṣkaracāriṇaṃ tam |",
  "SA05n2888:188": "nairañjanānte khalu vīryapāraṃ",
  "SA05n2888:192": "dhyānottamaprāptamahaṃ namāmi ||7 ||",
  "SA05n2888:199": "anādikaṃ sārthayituṃ prayāsam ,",
  "SA05n2888:203": "bodhidrumānmāgadhikād adhastāt |",
  "SA05n2888:207": "paryaṅkabaddhaṃ hyacalābhibodhim",
  "SA05n2888:211": "sambodhisamprāptikaraṃ namāmi ||8 || (6)",
  "SA05n2888:224": "śīghraṃ jagat kāruṇiko'valokya",
  "SA05n2888:228": "vārāṇasītyādivarasthaleṣu |",
  "SA05n2888:232": "vineyakān vartitadharmacakraṃ",
  "SA05n2888:236": "praveśayantaṃ trinaye namāmi ||9 ||",
  "SA05n2888:243": "ṣaṭ tīrthyaśāstṝnatha nigrihītuṃ",
  "SA05n2888:247": "yo devadattādyaparān kudṛṣṭīn |",
  "SA05n2888:251": "vārāṇasīye'damayad hi māraṃ ,",
  "SA05n2888:255": "muniṃ jayantaṃ samare namāmi ||10 || (7)",
  "SA05n2888:268": "guṇaiḥ pradarśya tribhave'tulārhaṃ",
  "SA05n2888:272": "śrāvastike prātihāryam mahad yaḥ |",
  "SA05n2888:276": "supūjito devamanuṣyayonibhiḥ |",
  "SA05n2888:280": "taṃ śāsanasyarddhikaraṃ namāmi ||11 ||",
  "SA05n2888:287": "kusīdinaḥ prerayituṃ suśīghraṃ ,",
  "SA05n2888:291": "pravitrakauśīnagarīyabhūmau |",
  "SA05n2888:295": "tyaktvāmaraṃ vajrasamānakāyaṃ",
  "SA05n2888:299": "namāmi yātaṃ parinirvṛtiṃ tam ||12 || (8)",
  "SA05n2888:312": "samyaktayā santamapi hyamartyaṃ ,",
  "SA05n2888:316": "puṇyaṃ labheranniti bhāvisattvāḥ |",
  "SA05n2888:320": "vikṛtya tatkālamanekadhātūn",
  "SA05n2888:324": "aṣṭāṃśadhātūn dadataṃ namāmi ||13 ||",
  "SA05n2888:331": "evaṃ prakārairbhagavat praśāstṛ-",
  "SA05n2888:335": "līlāsamāsastutijātapuṇyaiḥ | (9)",
  "SA05n2888:348": "samastasattvācaraṇaṃ hyapīdaṃ",
  "SA05n2888:352": "yathaiva bhūyāt sugatasya caryā ||14 ||",
  "SA05n2888:358": "(10)"
}