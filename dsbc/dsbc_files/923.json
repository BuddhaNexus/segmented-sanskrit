{
    "923:0": "CANTO IX",
    "923:1": "tatastadā mantripurohitau tau",
    "923:2": "bāṣpapratodābhihatau nṛpeṇa|",
    "923:3": "viddhau sadaśvāviva sarvayatnā-",
    "923:4": "tsauhārdaśīghraṃ yayaturvanaṃ tat||1||",
    "923:5": "tamāśramaṃ jātapariśramau tā-",
    "923:6": "vupetya kāle sadṛśānuyātrau|",
    "923:7": "rājarddhimutsṛjya vinītaceṣṭā-",
    "923:8": "vupeyaturbhārgavadhiṣṇyameva||2||",
    "923:9": "tau nyāyatastaṃ pratipūjya vipraṃ",
    "923:10": "tenārcitau tāvapi cānurūpam|",
    "923:11": "kṛtāsanau bhārgavamāsanasthaṃ",
    "923:12": "chittvā kathāmūcaturātmakṛtyam||3||",
    "923:13": "śuddhaujasaḥ śuddhaviśālakīrte-",
    "923:14": "rikṣvākuvaṃśaprabhavasya rājñaḥ|",
    "923:15": "imaṃ janaṃ vettu bhavānadhītaṃ",
    "923:16": "śrutagrahe mantraparigrahe ca||4||",
    "923:17": "tasyendrakalpasya jayantakalpaḥ",
    "923:18": "putro jarāmṛtyubhayaṃ titīrṣuḥ|",
    "923:19": "ihābhyutepaḥ kila tasya heto-",
    "923:20": "rāvāmupetau bhagavānavaitu||5||",
    "923:21": "tau so'bravīdasti sa dīrghabāhuḥ",
    "923:22": "prāptaḥ kumāro na tu nāvabuddhaḥ|",
    "923:23": "dharmo'yamāvartaka ityavetya",
    "923:24": "yātastvarāḍābhimukho mumukṣuḥ||6||",
    "923:25": "tasmāttatastāvupalabhya tattvaṃ",
    "923:26": "taṃ vipramāmantrya tadaiva sadyaḥ|",
    "923:27": "khinnāvakhinnāviva rājabhaktyā",
    "923:28": "prasasratustena yataḥ sa yātaḥ||7||",
    "923:29": "yāntau tatastau mṛjayā vihīna-",
    "923:30": "mapaśyatāṃ taṃ vapuṣojjvalantam|",
    "923:31": "upopaviṣṭaṃ pathi vṛkṣamūle",
    "923:32": "sūrya ghanābhogamiva praviṣṭam||8||",
    "923:33": "yānaṃ vihāyopayayau tatastaṃ",
    "923:34": "purohito mantradhareṇa sārdham|",
    "923:35": "yathā vanasthaṃ sahavāmadevo",
    "923:36": "rāmaṃ didṛkṣurmunirāvairvaśeyaḥ||9||",
    "923:37": "tāvarcayāmāsaturarhatastaṃ",
    "923:38": "divīva śukrāṅgirasau mahendram|",
    "923:39": "pratyarcayāmāsa sa cārhatastau",
    "923:40": "divīva śukrāṅgirasau mahendraḥ||10||",
    "923:41": "kṛtābhyanujñāvabhitastatastau",
    "923:42": "niṣedatuḥ śākyakuladhvajasya|",
    "923:43": "virejatustasya ca saṃnikarṣe",
    "923:44": "punarvasū yogagatāvivendoḥ||11||",
    "923:45": "taṃ vṛkṣamūlasthamabhijvalantaṃ",
    "923:46": "purohito rājasutaṃ babhāṣe|",
    "923:47": "yathopaviṣṭaṃ divi pārijāte",
    "923:48": "bṛhaspatiḥ śakrasutaṃ jayantam||12||",
    "923:49": "tvacchokaśalye hṛdayāvagāḍhe",
    "923:50": "mohaṃ gato bhūmitale muhūrtam|",
    "923:51": "kumāra rājā nayanāmbuvarṣo",
    "923:52": "yattvāmavocattadidaṃ nibodha||13||",
    "923:53": "jānāmi dharma prati niścayaṃ te",
    "923:54": "paraimi te bhāvinametamartham|",
    "923:55": "ahaṃ tvakāle vanasaṃśrayātte",
    "923:56": "śokāgnināgnipratimena dahye||14||",
    "923:57": "tadehi dharmapriya matpriyārtha",
    "923:58": "dharmārthameva tyaja buddhimetām|",
    "923:59": "ayaṃ hi mā śokarayaḥ pravṛddhau",
    "923:60": "nadīrayaḥ kūlamivābhihanti||15||",
    "923:61": "meghāmbukakṣādriṣu yā hi vṛttiḥ",
    "923:62": "samīraṇārkāgnimahāśanīnām|",
    "923:63": "tāṃ vṛttimasmāsu karoti śoko",
    "923:64": "vikarṣaṇocchoṣaṇadāhabhedaiḥ||16||",
    "923:65": "tadbhuṅkṣva tāvadvasudhādhipatyaṃ",
    "923:66": "kāle vanaṃ yāsyāsi śāstradṛṣṭe|",
    "923:67": "aniṣṭabandhau kuru mayyapekṣāṃ",
    "923:68": "sarveṣu bhūteṣu dayā hi dharmaḥ||17||",
    "923:69": "na caiṣa dharmo vana eva siddhaḥ",
    "923:70": "pure'pi siddhirniyatā yatīnām|",
    "923:71": "buddhiśca yatnaśca nimittamatra",
    "923:72": "vana ca liṅgaṃ ca hi bhīrucinham||18||",
    "923:73": "maulīdharairasaviṣaktahāraiḥ",
    "923:74": "keyūraviṣṭabdhabhujairnarendraiḥ|",
    "923:75": "lakṣbhyaṅkamadhye parivartamānaiḥ",
    "923:76": "prāpto gṛhasthairapi mokṣadharmaḥ||19||",
    "923:77": "dhruvānujau yau balivajrabāhū",
    "923:78": "vaibhrājamāṣāḍhamathāntidevam|",
    "923:79": "videharājaṃ janakaṃ tathaiva",
    "923:80": "śālvadrumaṃ senajitaśca rājñaḥ||20||",
    "923:81": "etān gṛhasthānnṛpatīnavehi",
    "923:82": "naiḥśreyase dharmavidhau vinītān|",
    "923:83": "ubhe'pi tasmādyugapadbhajasva",
    "923:84": "cittādhipatyaṃ ca nṛpaśriyaṃ ca||21||",
    "923:85": "icchāmi hi tvāmupaguhya gāḍhaṃ",
    "923:86": "kṛtābhiṣekaṃ salilārdrameva|",
    "923:87": "dhṛtātapattraṃ samudīkṣamāṇa-",
    "923:88": "stenaiva harṣeṇa vanaṃ praveṣṭum||22||",
    "923:89": "ityabravīdbhūmipatirbhavantaṃ",
    "923:90": "vākyena bāṣpagrathitākṣareṇa|",
    "923:91": "śrutvā bhavānarhati tatpriyārtha",
    "923:92": "snehena tasnehamanuprayātum||23||",
    "923:93": "śokāmbhasi tvatprabhave hyagādhe",
    "923:94": "duḥkhārṇave majjati śākyarājaḥ|",
    "923:95": "tasmāttamuttāraya nāthahīnaṃ",
    "923:96": "nirāśrayaṃ magnamivārṇave nauḥ||24||",
    "923:97": "bhīṣmeṇa gaṅgodarasaṃbhavena",
    "923:98": "rāmeṇa rāmeṇa ca bhārgaveṇa|",
    "923:99": "śrutvā kṛtaṃ karma pituḥ priyārtha",
    "923:100": "pitustvamaopyarhasi kartumiṣṭam||25||",
    "923:101": "saṃvardhayitrī samavehi devī-",
    "923:102": "magastyajuṣṭāṃ diśamaprayātām|",
    "923:103": "pranaṣṭavatsāmiva vatsalāṃ gā-",
    "923:104": "majasramārtā karuṇaṃ rudantīm||26||",
    "923:105": "haṃsena haṃsīmiva viprayuktāṃ",
    "923:106": "tyaktāṃ gajeneva vane kareṇum|",
    "923:107": "artā sanāthāmapi nāthahīnāṃ",
    "923:108": "trātuṃ vadhūmarhasi darśanena||27||",
    "923:109": "ekaṃ sutaṃ bālamanarhaduḥkhaṃ",
    "923:110": "saṃtāpamantargatamudvahantam|",
    "923:111": "taṃ rāhulaṃ mokṣaya bandhuśokā-",
    "923:112": "drāhūpasargādeva pūrṇacandram||28||",
    "923:113": "śokāgninā tvadvirahendhanena|",
    "923:114": "niḥsvāsadhūmena tamaḥśikhena|",
    "923:115": "tvaddarśanāmbvicchati dahyamāna-",
    "923:116": "mantaḥpuraṃ caiva puraṃ ca kṛtsnam||29||",
    "923:117": "sa bodhisattvaḥ paripūrṇasattvaḥ",
    "923:118": "śrutvā vacastasya purohitasya|",
    "923:119": "dhyātvā muhūrtaṃ guṇavadguṇajñaḥ",
    "923:120": "pratyuttaraṃ praśritamityuvāca||30||",
    "923:121": "avaimi bhāvaṃ tanaye pitṝṇāṃ",
    "923:122": "viśeṣato yo mayi bhūmipasya|",
    "923:123": "jānannapi vyādhijarāvipadbhyo",
    "923:124": "bhītastvagatyā svajanaṃ tyajāmi||31||",
    "923:125": "draṣṭuṃ priyaṃ kaḥ svajanaṃ hi necche-",
    "923:126": "nnānte yadi syātpriyaviprayogaḥ|",
    "923:127": "yadā tu bhūtvāpi ciraṃ viyoga-",
    "923:128": "stato guruaṃ snindhamapi tyajāmi||32||",
    "923:129": "maddhetukaṃ yattu narādhipasya",
    "923:130": "śokaṃ bhavānāha na tatpriyaṃ me|",
    "923:131": "yatsvapnabhūteṣu samāgameṣu",
    "923:132": "saṃtapyate bhāvini viprayoge||33||",
    "923:133": "evaṃ ca te niścayametu buddhi-",
    "923:134": "rdṛṣṭvā vicitraṃ jagataḥ pracāram|",
    "923:135": "saṃtāpaheturna suto na bandhu-",
    "923:136": "rajñānanaimittika eṣa tāpaḥ||34||",
    "923:137": "yathādhvagānamiha saṃgatānāṃ",
    "923:138": "kāle viyogo niyataḥ prajānām|",
    "923:139": "prājño janaḥ ko tu bhajeta śokaṃ",
    "923:140": "bandhupratijñātajanairvihīnaḥ||35||",
    "923:141": "ihaiti hitvā svajanaṃ paratra",
    "923:142": "pralabhya cehāpi punaḥ prayāti|",
    "923:143": "gatvāpi tatrāpyaparatra gaccha-",
    "923:144": "tyevaṃ jane tyāgini ko'nurodhaḥ||36||",
    "923:145": "yadā ca garbhātprabhṛti pravṛttaḥ",
    "923:146": "sarvāsvavasthāsu vadhāya mṛtyuḥ|",
    "923:147": "kasmādakāle vanasaṃśrayaṃ me",
    "923:148": "putrapriyastatrabhavānavocat||37||",
    "923:149": "bhavatyakālo viṣayābhipattau",
    "923:150": "kālastathaivārthavidhau pradiṣṭaḥ|",
    "923:151": "kālo jagatkarṣati sarvakālā-",
    "923:152": "nnirvāhake śreyasi nāsti kālaḥ||38||",
    "923:153": "rājyaṃ mumukṣurmayi yacca rājā",
    "923:154": "tadapyudāraṃ sadṛśaṃ pituśca|",
    "923:155": "pratigrahītuṃ mama na kṣamaṃ tu",
    "923:156": "lobhādapathyānnamivāturasya||39||",
    "923:157": "kathaṃ nu mohāyatanaṃ nṛpatvaṃ",
    "923:158": "kṣamaṃ prapattuṃ viduṣā nareṇa|",
    "923:159": "sodvegatā yatra madaḥ śramaśca",
    "923:160": "parāpacāreṇa ca dharmapīḍā||40||",
    "923:161": "jāmbūnadaṃ harmyamiva pradīptaṃ",
    "923:162": "viṣeṇa saṃyuktamivottamānnam|",
    "923:163": "grāhākulaṃ cāmbviva sāravindaṃ",
    "923:164": "rājyaṃ hi ramyaṃ vyasanāśrayaṃ ca||41||",
    "923:165": "itthaṃ ca rājyaṃ na sukhaṃ na dharmaḥ",
    "923:166": "pūrve yathā jātaghṛṇā narendrāḥ|",
    "923:167": "vayaḥprakarṣe'parihāryaduḥkhe",
    "923:168": "rājyāni muktvā vanameva jagmuḥ||42||",
    "923:169": "varaṃ hi bhuktāni tṛṇānyaraṇye",
    "923:170": "toṣaṃ paraṃ ratnamivopagṛhya|",
    "923:171": "sahoṣitaṃ śrīsulabhairna caiva",
    "923:172": "doṣairadṛśyairiva kṛṣṇasarpaiḥ||43||",
    "923:173": "ślādhyaṃ hi rājyāni vihāya rājñāṃ",
    "923:174": "dharmābhilāṣeṇa vanaṃ praveṣṭum|",
    "923:175": "bhagnapratijñasya na tūpapannaṃ",
    "923:176": "vanaṃ parityajya gṛhaṃ praveṣṭum||44||",
    "923:177": "jātaḥ kule ko hi naraḥ sasattvo",
    "923:178": "dharmābhilāṣeṇa vanaṃ praviṣṭaḥ|",
    "923:179": "kāṣāyamutsṛjya vimuktalajjaḥ",
    "923:180": "puraṃdarasyāpi puraṃ śrayeta||45||",
    "923:181": "lobhāddhi mohādathavā bhayena",
    "923:182": "yo vāntamannaṃ punarādadīta|",
    "923:183": "lobhātsa mohādathavā bhayena",
    "923:184": "saṃtyajya kāmān punarādadīta||46||",
    "923:185": "yaśca pradīptāccharaṇātkathaṃci-",
    "923:186": "nniṣkramya bhūyaḥ praviśettadeva|",
    "923:187": "gārhasthyamutsṛjya sa dṛṣṭadoṣo",
    "923:188": "mohena bhūyo'bhilaṣedgrahītum||47||",
    "923:189": "yā ca śrutirmokṣamavāptavanto",
    "923:190": "nṛpā gṛhasthā iti naitadasti|",
    "923:191": "śamapradhānaḥ kva ca mokṣadharmo",
    "923:192": "daṇḍapradhānaḥ kva ca rājadharmaḥ||48||",
    "923:193": "śame ratiścecchithilaṃ ca rājyaṃ",
    "923:194": "rājye matiścecchamaviplavaśca|",
    "923:195": "śamaśca taikṣṇyaṃ ca hi nopapannaṃ",
    "923:196": "śītoṣṇayoraikyamivodakāgnyoḥ||49||",
    "923:197": "tanniścayādvā vasudhādhipāste",
    "923:198": "rājyāni muktvā śamamāptavantaḥ|",
    "923:199": "rājyāṅgitā vā nibhṛtendriyatvā-",
    "923:200": "danaiṣṭhike mokṣakṛtābhimānāḥ||50||",
    "923:201": "teṣāṃ ca rājye'stu śamo yathāva-",
    "923:202": "tprāpto vanaṃ nāhamaniścayena|",
    "923:203": "chittvā hi pāśaṃ gṛhabandhusaṃjñaṃ",
    "923:204": "muktaḥ punarna pravivikṣurasmi||51||",
    "923:205": "ityātmavijñānaguṇānurūpaṃ",
    "923:206": "muktaspṛhaṃ hetumadūrjitaṃ ca|",
    "923:207": "śrutvā narendrātmajamuktavantaṃ",
    "923:208": "pratyuttaraṃ mantradharo'pyuvāca||52||",
    "923:209": "yo niścayo dharmavidhau tavāyaṃ",
    "923:210": "nāyaṃ na yukto na tu kālayuktaḥ|",
    "923:211": "śokāya dattvā pitaraṃ vayaḥsthaṃ",
    "923:212": "syāddharmakāmasya hi te na dharmaḥ||53||",
    "923:213": "nūnaṃ ca buddhistava nātisūkṣmā",
    "923:214": "dharmārthakāmeṣvavicakṣaṇā vā|",
    "923:215": "hetoradṛṣṭasya phalasya yastvaṃ",
    "923:216": "pratyakṣamartha paribhūya yāsi||54||",
    "923:217": "punarbhavo'stīti ca kecidāhu-",
    "923:218": "rnāstīti kecinniyatapratijñāḥ|",
    "923:219": "evaṃ yadā saṃśayito'yamartha-",
    "923:220": "stasmātkṣamaṃ bhoktumupasthitā śrīḥ||55||",
    "923:221": "bhūyaḥ pravṛttiryadi kācidasti",
    "923:222": "raṃsyāmahe tatra yathopapattau|",
    "923:223": "atha pravṛttiḥ parato na kāci-",
    "923:224": "tsiddhau'prayatnājjagato'sya mokṣaḥ||56||",
    "923:225": "astīti kecitparalokamāhu-",
    "923:226": "rmokṣasya yogaṃ na tu varṇayanti|",
    "923:227": "agneryathā hyauṣṇyamapāṃ dravatvaṃ",
    "923:228": "tadvatpravṛttau prakṛtiṃ vadanti||57||",
    "923:229": "kecitsvabhāvāditi varṇayanti",
    "923:230": "śubhāśubhaṃ caiva bhavābhavau ca|",
    "923:231": "svābhāvikaṃ sarvamidaṃ ca yasmā-",
    "923:232": "dato'pi mogho bhavati prayatnaḥ||58||",
    "923:233": "yadindriyāṇāṃ niyataḥ pracāraḥ",
    "923:234": "priyāpriyatvaṃ viṣayeṣu caiva|",
    "923:235": "saṃyujyate yajjarayārtibhiśca",
    "923:236": "kastatra yatno nanu sa svabhāvaḥ||59||",
    "923:237": "adbhirhutāśaḥ śamamabhyupaiti",
    "923:238": "tejāṃsi cāpo gamayanti śoṣam|",
    "923:239": "bhinnāni bhūtāni śarīrasaṃsthā-",
    "923:240": "nyaikyaṃ ca gatvā jagadudvahanti||60||",
    "923:241": "yatpāṇipādodarapṛṣṭhamūrdhnā",
    "923:242": "nirvartate garbhagatasya bhāvaḥ|",
    "923:243": "yadātmanastasya ca tena yogaḥ",
    "923:244": "svābhāvikaṃ tatkathayanti tajjñāḥ||61||",
    "923:245": "kaḥ kaṇṭakasya prakaroti taikṣṇyaṃ",
    "923:246": "vicitrabhāvaṃ mṛgapakṣiṇāṃ vā|",
    "923:247": "svabhāvataḥ sarvamidaṃ pravṛttaṃ",
    "923:248": "na kāmakāro'sti kutaḥ prayatnaḥ||62||",
    "923:249": "sarga vadantīśvaratastathānye",
    "923:250": "tatra paryatne puruṣasya ko'rthaḥ|",
    "923:251": "ya eva heturjagataḥ pravṛttau",
    "923:252": "heturnivṛttau niyataḥ sa eva||63||",
    "923:253": "kecidvadantyātmanimittameva",
    "923:254": "prādurbhavaṃ caiva bhavakṣayaṃ ca|",
    "923:255": "prādurbhavaṃ tu pravadantyayatnā-",
    "923:256": "dyatnena mokṣādhigamaṃ bruvanti||64||",
    "923:257": "naraḥ pitṛṇāmanṛṇaḥ prajābhi-",
    "923:258": "rvedaiṛṣīṇāṃ kratubhiḥ surāṇām|",
    "923:259": "utpadyate sārdhamṛṇaistribhistai-",
    "923:260": "ryasyāsti mokṣaḥ kila tasya mokṣaḥ||65||",
    "923:261": "ityevametena vidhikrameṇa",
    "923:262": "mokṣaṃ sayatnasya vadanti tajjñāḥ|",
    "923:263": "prayatnavanto'pi hi vikrameṇa",
    "923:264": "mumukṣavaḥ khedamavāpnuvanti||66||",
    "923:265": "tatsaumya mokṣe yadi bhaktirasti",
    "923:266": "nyāyena sevasva vidhiṃ yathoktam|",
    "923:267": "evaṃ bhaviṣyatyupapattirasya",
    "923:268": "saṃtāpanāśaśca narādhipasya||67||",
    "923:269": "yā ca pravṛttā tava doṣabuddhi-",
    "923:270": "stapovanebhyo bhavanaṃ praveṣṭum|",
    "923:271": "tatrāpi cintā tava tāta mā bhūta",
    "923:272": "pūrve'pi jagmuḥ svagṛhānvanebhyaḥ||68||",
    "923:273": "tapovanastho'pi vṛtaḥ prajābhi-",
    "923:274": "rjagāma rājā puramambarīṣaḥ|",
    "923:275": "tathā mahīṃ viprakṛtāmanāryai-",
    "923:276": "stapovanādetya rarakṣa rāmaḥ||69||",
    "923:277": "tathaiva śālvādhipatirdrumākhyo",
    "923:278": "vanātsasūranurnagaraṃ viveśa|",
    "923:279": "brahmārṣibhūtaśca munervasiṣṭhā-",
    "923:280": "ddadhre śriyaṃ sāṃkṛtirantidevaḥ||70||",
    "923:281": "evaṃvidhā dharmayaśaḥpradīptā",
    "923:282": "vanāni hitvā bhavanānyatīyuḥ|",
    "923:283": "tasmānna doṣo'sti gṛhaṃ prayātuṃ",
    "923:284": "tapovanāddharmanimittameva ||71||",
    "923:285": "tato vacastasya niśamya mantriṇaḥ",
    "923:286": "priyaṃ hitaṃ caiva nṛpasya cakṣuṣaḥ|",
    "923:287": "anūnamavyastamasaktamadrutaṃ",
    "923:288": "dhṛtau sthito rājasuto'bravīdvacaḥ||72||",
    "923:289": "ihāsti nāstīti ya eṣa saṃśayaḥ",
    "923:290": "parasya vākyairna mamātra niścayaḥ|",
    "923:291": "avetya tattvaṃ tapasā śamena ca",
    "923:292": "svayaṃ grahīṣyāmi yadatra niścitam||73||",
    "923:293": "na me kṣamaṃ saṃśayajaṃ hi darśanaṃ",
    "923:294": "grahītumavyaktaparasparāhatam|",
    "923:295": "budhaḥ parapratyayato hi ko vraje-",
    "923:296": "jjano'ndhakāre'ndha ivāndhadeśikaḥ||74||",
    "923:297": "adṛṣṭatattvasya sato'pi kiṃ tu me",
    "923:298": "śubhāśubhe saṃśayitu śubhe matiḥ|",
    "923:299": "vṛthāpi khedo hi varaṃ śubhātmanaḥ",
    "923:300": "sukhaṃ na tattve'pi vigarhitātmanaḥ||75||",
    "923:301": "imaṃ tu dṛṣṭvāgamamavyavasthitaṃ",
    "923:302": "yaduktamāpttaistadavehi sādhviti|",
    "923:303": "prahīṇadoṣatvamavehi cāptatāṃ",
    "923:304": "prahīṇadoṣo hyanṛtaṃ na vakṣyati||76||",
    "923:305": "gṛhapraveśaṃ prati yacca me bhavā-",
    "923:306": "nuvāca rāmaprabhṛtīnnidarśanam|",
    "923:307": "na te pramāṇaṃ na hi dharmaniścayai-",
    "923:308": "ṣvalaṃ pramāṇāya parikṣatavratāḥ||77||",
    "923:309": "tadevamapyeva ravirmahī pate-",
    "923:310": "dapi sthiratvaṃ himavān giristyajet|",
    "923:311": "adṛṣṭatattvo viṣayonmukhendriyaḥ",
    "923:312": "śrayeya na tveva gṛhān pṛthagjanaḥ||78||",
    "923:313": "ahaṃ viśeyaṃ jvalitaṃ hutāśanaṃ",
    "923:314": "na cākṛtārthaḥ praviśeyamālayam|",
    "923:315": "iti pratijñāṃ sa cakāra garvito",
    "923:316": "yatheṣṭamutthāya ca nirmamo yayau||79||",
    "923:317": "tataḥ sabāṣpau sacivadvijābubhau",
    "923:318": "niśamya tasya sthirameva niścayam|",
    "923:319": "viṣaṇṇavaktrāvanugamya duḥkhitau",
    "923:320": "śanairagatyā purameva jagmatuḥ||80||",
    "923:321": "tatsnehādatha nṛpateśca bhaktitastau",
    "923:322": "sāpekṣaṃ pratiyayatuśca tasthatuśca|",
    "923:323": "durdharṣa ravimiva dīptamātmabhāsā",
    "923:324": "taṃ draṣṭuṃ na hi pathi śekaturna moktum||81||",
    "923:325": "tau jñātuṃ paramagatergatiṃ tu tasya",
    "923:326": "pracchannāṃścarapuruṣāñchucīnvidhāya|",
    "923:327": "rājānaṃ priyasutalālasaṃ nu gatvā",
    "923:328": "drakṣyāvaḥ kathamiti jagmatuḥ kathaṃcitu||82||",
    "923:329": "iti buddhacarite mahākāvye",
    "923:330": "kumārānveṣaṇoṃ nāma navamaḥ sargaḥ||9||"
}