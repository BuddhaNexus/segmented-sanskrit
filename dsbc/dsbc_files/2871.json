{
    "2871:0": "normal;mso-pagination:",
    "2871:1": "none;mso-layout-grid-align:",
    "2871:2": "none;text-autospace:",
    "2871:3": "none“> Bhagavatstutiḥ",
    "2871:4": "normal;mso-pagination:",
    "2871:5": "none;mso-layout-grid-align:",
    "2871:6": "none;text-autospace:none“>",
    "2871:7": "normal;mso-pagination:",
    "2871:8": "none;mso-layout-grid-align:",
    "2871:9": "none;text-autospace:",
    "2871:10": "none“> praṇidhāya manaḥ sahendriyairvidhivad vāk ca",
    "2871:11": "śarīrameva ca |",
    "2871:12": "normal;mso-pagination:",
    "2871:13": "none;mso-layout-grid-align:",
    "2871:14": "none;text-autospace:",
    "2871:15": "none“> guṇabhūta mahāguṇodadhestava vakṣyāmi guṇaikadeśatām",
    "2871:16": "||1||",
    "2871:17": "normal;mso-pagination:",
    "2871:18": "none;mso-layout-grid-align:",
    "2871:19": "none;text-autospace:none“>",
    "2871:20": "normal;mso-pagination:",
    "2871:21": "none;mso-layout-grid-align:",
    "2871:22": "none;text-autospace:",
    "2871:23": "none“> paramapravaro'smi vādināmanavadyaḥ",
    "2871:24": "susamāhitendriyaḥ |",
    "2871:25": "normal;mso-pagination:",
    "2871:26": "none;mso-layout-grid-align:",
    "2871:27": "none;text-autospace:",
    "2871:28": "none“> paramārthavidaprakampitaḥ prayataiḥ",
    "2871:29": "sarvaparapravādibhiḥ ||2||",
    "2871:30": "normal;mso-pagination:",
    "2871:31": "none;mso-layout-grid-align:",
    "2871:32": "none;text-autospace:none“>",
    "2871:33": "normal;mso-pagination:",
    "2871:34": "none;mso-layout-grid-align:",
    "2871:35": "none;text-autospace:",
    "2871:36": "none“> caraṇaṁ susamāptameva te susamāptavrata",
    "2871:37": "sādhitavrataḥ |",
    "2871:38": "normal;mso-pagination:",
    "2871:39": "none;mso-layout-grid-align:",
    "2871:40": "none;text-autospace:",
    "2871:41": "none“> balavāṁśca samādhiravyayastava nārāyaṇaśailarājavat",
    "2871:42": "||3||",
    "2871:43": "normal;mso-pagination:",
    "2871:44": "none;mso-layout-grid-align:",
    "2871:45": "none;text-autospace:none“>",
    "2871:46": "normal;mso-pagination:",
    "2871:47": "none;mso-layout-grid-align:",
    "2871:48": "none;text-autospace:",
    "2871:49": "none“> puruṣaṣabha nāsti te vyathā na viṣādo na bhayaṁ",
    "2871:50": "na ca klamaḥ |",
    "2871:51": "normal;mso-pagination:",
    "2871:52": "none;mso-layout-grid-align:",
    "2871:53": "none;text-autospace:",
    "2871:54": "none“> na ca te vyasanaṁ kutaḥ kalirna ca bhūteṣu",
    "2871:55": "kadācidakṣamā ||4||",
    "2871:56": "normal;mso-pagination:",
    "2871:57": "none;mso-layout-grid-align:",
    "2871:58": "none;text-autospace:none“>",
    "2871:59": "normal;mso-pagination:",
    "2871:60": "none;mso-layout-grid-align:",
    "2871:61": "none;text-autospace:",
    "2871:62": "none“> na ca dhāvasi nātilīyase na ca santapyasi nāpi",
    "2871:63": "hṛṣyase |",
    "2871:64": "normal;mso-pagination:",
    "2871:65": "none;mso-layout-grid-align:",
    "2871:66": "none;text-autospace:",
    "2871:67": "none“> satataṁ śubhameva te manaḥ satataṁ",
    "2871:68": "merurivācalādhipaḥ ||5||",
    "2871:69": "normal;mso-pagination:",
    "2871:70": "none;mso-layout-grid-align:",
    "2871:71": "none;text-autospace:none“>",
    "2871:72": "normal;mso-pagination:",
    "2871:73": "none;mso-layout-grid-align:",
    "2871:74": "none;text-autospace:",
    "2871:75": "none“> munipuṁgava sarvadhātubhirvipulaṁ jñānamapāvṛtaṁ",
    "2871:76": "tava |",
    "2871:77": "normal;mso-pagination:",
    "2871:78": "none;mso-layout-grid-align:",
    "2871:79": "none;text-autospace:",
    "2871:80": "none“> aparāhatamakṣayavyayaṁ vividheṣvāyataneṣu",
    "2871:81": "vartase ||6||",
    "2871:82": "normal;mso-pagination:",
    "2871:83": "none;mso-layout-grid-align:",
    "2871:84": "none;text-autospace:none“>",
    "2871:85": "normal;mso-pagination:",
    "2871:86": "none;mso-layout-grid-align:",
    "2871:87": "none;text-autospace:",
    "2871:88": "none“> na ca te'sti mune kathaṁ kathā vimatirnāsti na",
    "2871:89": "saṁśayaḥ kvacit |",
    "2871:90": "normal;mso-pagination:",
    "2871:91": "none;mso-layout-grid-align:",
    "2871:92": "none;text-autospace:",
    "2871:93": "none“> svayameva na te parāparaṁ viditaṁ sarvamavedi",
    "2871:94": "vidyayā ||7||",
    "2871:95": "normal;mso-pagination:",
    "2871:96": "none;mso-layout-grid-align:",
    "2871:97": "none;text-autospace:none“>",
    "2871:98": "normal;mso-pagination:",
    "2871:99": "none;mso-layout-grid-align:",
    "2871:100": "none;text-autospace:",
    "2871:101": "none“> priyadarśanasādhudarśanapriyasādhupriyapaṇḍitapriyaḥ",
    "2871:102": "normal;mso-pagination:",
    "2871:103": "none;mso-layout-grid-align:",
    "2871:104": "none;text-autospace:",
    "2871:105": "none“> samameva hi te priyāpriyaṁ satataṁ",
    "2871:106": "prītikarastatheryase ||8||",
    "2871:107": "normal;mso-pagination:",
    "2871:108": "none;mso-layout-grid-align:",
    "2871:109": "none;text-autospace:none“>",
    "2871:110": "normal;mso-pagination:",
    "2871:111": "none;mso-layout-grid-align:",
    "2871:112": "none;text-autospace:",
    "2871:113": "none“> madhurapratibhānavānasi smitavākyaḥ smṛtamān",
    "2871:114": "viśāradaḥ |",
    "2871:115": "normal;mso-pagination:",
    "2871:116": "none;mso-layout-grid-align:",
    "2871:117": "none;text-autospace:",
    "2871:118": "none“> vividānumataṁ prabhāṣase triṣu lokeṣu ca te stṛtaṁ",
    "2871:119": "yaśaḥ ||9||",
    "2871:120": "normal;mso-pagination:",
    "2871:121": "none;mso-layout-grid-align:",
    "2871:122": "none;text-autospace:none“>",
    "2871:123": "normal;mso-pagination:",
    "2871:124": "none;mso-layout-grid-align:",
    "2871:125": "none;text-autospace:",
    "2871:126": "none“> nṛsurāsurayakṣarākṣasā bahavastvāmiha lokapaṇḍitāḥ",
    "2871:127": "|",
    "2871:128": "normal;mso-pagination:",
    "2871:129": "none;mso-layout-grid-align:",
    "2871:130": "none;text-autospace:",
    "2871:131": "none“> upagamya mune punaḥ punaḥ paripṛcchānti na",
    "2871:132": "cābhiṣūyase ||10||",
    "2871:133": "normal;mso-pagination:",
    "2871:134": "none;mso-layout-grid-align:",
    "2871:135": "none;text-autospace:none“>",
    "2871:136": "normal;mso-pagination:",
    "2871:137": "none;mso-layout-grid-align:",
    "2871:138": "none;text-autospace:",
    "2871:139": "none“> svavacaḥ paritoṣitāstvayā naradevāḥ surayakṣarākṣasāḥ",
    "2871:140": "|",
    "2871:141": "normal;mso-pagination:",
    "2871:142": "none;mso-layout-grid-align:",
    "2871:143": "none;text-autospace:",
    "2871:144": "none“> pratiyānti vinītasaṁśayāścaraṇau vandya ca te",
    "2871:145": "mahāmuni ||11||",
    "2871:146": "normal;mso-pagination:",
    "2871:147": "none;mso-layout-grid-align:",
    "2871:148": "none;text-autospace:none“>",
    "2871:149": "normal;mso-pagination:",
    "2871:150": "none;mso-layout-grid-align:",
    "2871:151": "none;text-autospace:",
    "2871:152": "none“> sthitamāsitamāgataṁ gataṁ śayitaṁ",
    "2871:153": "maunamathābhyudīritam |",
    "2871:154": "normal;mso-pagination:",
    "2871:155": "none;mso-layout-grid-align:",
    "2871:156": "none;text-autospace:",
    "2871:157": "none“> atha cīvarapātradhāraṇaṁ ruciraṁ gautama",
    "2871:158": "sarvameva te ||12||",
    "2871:159": "normal;mso-pagination:",
    "2871:160": "none;mso-layout-grid-align:",
    "2871:161": "none;text-autospace:none“>",
    "2871:162": "normal;mso-pagination:",
    "2871:163": "none;mso-layout-grid-align:",
    "2871:164": "none;text-autospace:",
    "2871:165": "none“> avilambitamadrutaṁ samaṁ svaramādhuryaguṇaiḥ",
    "2871:166": "samanvitam |",
    "2871:167": "normal;mso-pagination:",
    "2871:168": "none;mso-layout-grid-align:",
    "2871:169": "none;text-autospace:",
    "2871:170": "none“> vacanaṁ punaruktavarjitaṁ samaye vyāharase",
    "2871:171": "narottama ||13||",
    "2871:172": "normal;mso-pagination:",
    "2871:173": "none;mso-layout-grid-align:",
    "2871:174": "none;text-autospace:none“>",
    "2871:175": "normal;mso-pagination:",
    "2871:176": "none;mso-layout-grid-align:",
    "2871:177": "none;text-autospace:",
    "2871:178": "none“> balavānasi lokaviśrutaḥ puruṣajñaḥ puruṣarṣabhaḥ",
    "2871:179": "prabhuḥ |",
    "2871:180": "normal;mso-pagination:",
    "2871:181": "none;mso-layout-grid-align:",
    "2871:182": "none;text-autospace:",
    "2871:183": "none“> na ca manyasi nāvamanyase sakalaṁ lokamimaṁ",
    "2871:184": "sadaivatam ||14||",
    "2871:185": "normal;mso-pagination:",
    "2871:186": "none;mso-layout-grid-align:",
    "2871:187": "none;text-autospace:none“>",
    "2871:188": "normal;mso-pagination:",
    "2871:189": "none;mso-layout-grid-align:",
    "2871:190": "none;text-autospace:",
    "2871:191": "none“> na ca vismayase kadācana prakṛtistheṣu calācaleṣu",
    "2871:192": "ca |",
    "2871:193": "normal;mso-pagination:",
    "2871:194": "none;mso-layout-grid-align:",
    "2871:195": "none;text-autospace:",
    "2871:196": "none“> suhṛdeṣu [ ca ] durhṛdeṣu ca pratikūleṣvanulomavatsu ca ||15||",
    "2871:197": "normal;mso-pagination:",
    "2871:198": "none;mso-layout-grid-align:",
    "2871:199": "none;text-autospace:none“>",
    "2871:200": "normal;mso-pagination:",
    "2871:201": "none;mso-layout-grid-align:",
    "2871:202": "none;text-autospace:",
    "2871:203": "none“> paridevyamadīnanisvanaṁ",
    "2871:204": "srutalālārpitasannināditam |",
    "2871:205": "normal;mso-pagination:",
    "2871:206": "none;mso-layout-grid-align:",
    "2871:207": "none;text-autospace:",
    "2871:208": "none“> paridāhavidāhasaṁyutaṁ bhayarogajvaraśokakarṣitam",
    "2871:209": "||16||",
    "2871:210": "normal;mso-pagination:",
    "2871:211": "none;mso-layout-grid-align:",
    "2871:212": "none;text-autospace:none“>",
    "2871:213": "normal;mso-pagination:",
    "2871:214": "none;mso-layout-grid-align:",
    "2871:215": "none;text-autospace:",
    "2871:216": "none“> prasamīkṣya jagat samākulaṁ vividhairduḥkhaśatairupadrutam",
    "2871:217": "|",
    "2871:218": "normal;mso-pagination:",
    "2871:219": "none;mso-layout-grid-align:",
    "2871:220": "none;text-autospace:",
    "2871:221": "none“> cirarātramanāthamutsukaṁ bhavatṛṣṇāprasṛtaṁ",
    "2871:222": "tamovṛtam ||17||",
    "2871:223": "normal;mso-pagination:",
    "2871:224": "none;mso-layout-grid-align:",
    "2871:225": "none;text-autospace:none“>",
    "2871:226": "normal;mso-pagination:",
    "2871:227": "none;mso-layout-grid-align:",
    "2871:228": "none;text-autospace:",
    "2871:229": "none“> avatārya munermahākṛpāṁ vividhāṁ cātmagatāṁ",
    "2871:230": "prabhāvatām |",
    "2871:231": "normal;mso-pagination:",
    "2871:232": "none;mso-layout-grid-align:",
    "2871:233": "none;text-autospace:",
    "2871:234": "none“> svayameva hi śākyapuṅgava vyathitānmocayituṁ",
    "2871:235": "tvayocyate ||19||",
    "2871:236": "normal;mso-pagination:",
    "2871:237": "none;mso-layout-grid-align:",
    "2871:238": "none;text-autospace:none“>",
    "2871:239": "normal;mso-pagination:",
    "2871:240": "none;mso-layout-grid-align:",
    "2871:241": "none;text-autospace:",
    "2871:242": "none“> ativīrya mahāvināyakapravarāṇāmanuvādināṁ vara",
    "2871:243": "|",
    "2871:244": "normal;mso-pagination:",
    "2871:245": "none;mso-layout-grid-align:",
    "2871:246": "none;text-autospace:",
    "2871:247": "none“> na ca te sadṛśaḥ kuto'dhikastriṣu lokeṣvapi",
    "2871:248": "naiva vidyate ||20||",
    "2871:249": "normal;mso-pagination:",
    "2871:250": "none;mso-layout-grid-align:",
    "2871:251": "none;text-autospace:none“>",
    "2871:252": "normal;mso-pagination:",
    "2871:253": "none;mso-layout-grid-align:",
    "2871:254": "none;text-autospace:",
    "2871:255": "none“> pratighānunayā na santi te na ca te santi muni",
    "2871:256": "parisravāḥ |",
    "2871:257": "normal;mso-pagination:",
    "2871:258": "none;mso-layout-grid-align:",
    "2871:259": "none;text-autospace:",
    "2871:260": "none“> anurodhavirodhavigrahāḥ satataṁ suvrata naiva",
    "2871:261": "naiva te ||21||",
    "2871:262": "normal;mso-pagination:",
    "2871:263": "none;mso-layout-grid-align:",
    "2871:264": "none;text-autospace:none“>",
    "2871:265": "normal;mso-pagination:",
    "2871:266": "none;mso-layout-grid-align:",
    "2871:267": "none;text-autospace:",
    "2871:268": "none“> apahāya mune priyāpriye sukhaduḥkhe vi [ ṣamaṁ samaṁ tathā ]",
    "2871:269": "normal;mso-pagination:",
    "2871:270": "none;mso-layout-grid-align:",
    "2871:271": "none;text-autospace:",
    "2871:272": "none“> aratiṁ ca ratiṁ viparyayannupaśāntaścarasīha saṁyatah",
    "2871:273": "||22||",
    "2871:274": "normal;mso-pagination:",
    "2871:275": "none;mso-layout-grid-align:",
    "2871:276": "none;text-autospace:none“>",
    "2871:277": "normal;mso-pagination:",
    "2871:278": "none;mso-layout-grid-align:",
    "2871:279": "none;text-autospace:",
    "2871:280": "none“> vyasane na ca nāma nirmanā na ca nāmonnamase",
    "2871:281": "praśaṁsayā |",
    "2871:282": "normal;mso-pagination:",
    "2871:283": "none;mso-layout-grid-align:",
    "2871:284": "none;text-autospace:",
    "2871:285": "none“> ayaśaśca yaśaśca te samaṁ samamākruṣṭamathāpi",
    "2871:286": "vanditam ||23||",
    "2871:287": "normal;mso-pagination:",
    "2871:288": "none;mso-layout-grid-align:",
    "2871:289": "none;text-autospace:none“>",
    "2871:290": "normal;mso-pagination:",
    "2871:291": "none;mso-layout-grid-align:",
    "2871:292": "none;text-autospace:",
    "2871:293": "none“> ativākyamatho titikṣase paruṣaṁ",
    "2871:294": "pāpajanairudīritam |",
    "2871:295": "normal;mso-pagination:",
    "2871:296": "none;mso-layout-grid-align:",
    "2871:297": "none;text-autospace:",
    "2871:298": "none“> samarāgragato viṣāṇavān bhṛśamuktāniva kuñjaraḥ",
    "2871:299": "śarān ||24||",
    "2871:300": "normal;mso-pagination:",
    "2871:301": "none;mso-layout-grid-align:",
    "2871:302": "none;text-autospace:none“>",
    "2871:303": "normal;mso-pagination:",
    "2871:304": "none;mso-layout-grid-align:",
    "2871:305": "none;text-autospace:",
    "2871:306": "none“> suvacastvamṛṣi vacaḥ kṣamaḥ sudurukteṣvapi",
    "2871:307": "nābhiṣūyase |",
    "2871:308": "normal;mso-pagination:",
    "2871:309": "none;mso-layout-grid-align:",
    "2871:310": "none;text-autospace:",
    "2871:311": "none“> samameva ca varttase mune paribhāṣāsu śubhāsu",
    "2871:312": "vākṣu ca ||25||",
    "2871:313": "normal;mso-pagination:",
    "2871:314": "none;mso-layout-grid-align:",
    "2871:315": "none;text-autospace:none“>",
    "2871:316": "normal;mso-pagination:",
    "2871:317": "none;mso-layout-grid-align:",
    "2871:318": "none;text-autospace:",
    "2871:319": "none“> satataṁ ca varārha pūjyase naradevāsurayakṣarākṣasaiḥ",
    "2871:320": "|",
    "2871:321": "normal;mso-pagination:",
    "2871:322": "none;mso-layout-grid-align:",
    "2871:323": "none;text-autospace:",
    "2871:324": "none“> ṛṣibhiśca sadā mahātmabhirna ca te vikriyate",
    "2871:325": "sthiraṁ manaḥ ||26||",
    "2871:326": "normal;mso-pagination:",
    "2871:327": "none;mso-layout-grid-align:",
    "2871:328": "none;text-autospace:none“>",
    "2871:329": "normal;mso-pagination:",
    "2871:330": "none;mso-layout-grid-align:",
    "2871:331": "none;text-autospace:",
    "2871:332": "none“> pravaro'hamito na manyase'tyavaro'hamito na",
    "2871:333": "manyase |",
    "2871:334": "normal;mso-pagination:",
    "2871:335": "none;mso-layout-grid-align:",
    "2871:336": "none;text-autospace:",
    "2871:337": "none“> sadṛśo'hamito na manyase trividhā mānavidhā na",
    "2871:338": "santi te ||27||",
    "2871:339": "normal;mso-pagination:",
    "2871:340": "none;mso-layout-grid-align:",
    "2871:341": "none;text-autospace:none“>",
    "2871:342": "normal;mso-pagination:",
    "2871:343": "none;mso-layout-grid-align:",
    "2871:344": "none;text-autospace:",
    "2871:345": "none“> dhṛtimān samaloṣṭakāñcanaḥ samavaiḍuryakaṭhallaśarkaraḥ",
    "2871:346": "|",
    "2871:347": "normal;mso-pagination:",
    "2871:348": "none;mso-layout-grid-align:",
    "2871:349": "none;text-autospace:",
    "2871:350": "none“> tṛṇakāṣthasamaṁ mahāmune carasīdaṁ hi sadevakaṁ",
    "2871:351": "jagat ||28||",
    "2871:352": "normal;mso-pagination:",
    "2871:353": "none;mso-layout-grid-align:",
    "2871:354": "none;text-autospace:none“>",
    "2871:355": "normal;mso-pagination:",
    "2871:356": "none;mso-layout-grid-align:",
    "2871:357": "none;text-autospace:",
    "2871:358": "none“> himabhāskararaśmisaṁgamācchiśiroṣṇaṁ pavanaṁ",
    "2871:359": "samudvahan |",
    "2871:360": "normal;mso-pagination:",
    "2871:361": "none;mso-layout-grid-align:",
    "2871:362": "none;text-autospace:",
    "2871:363": "none“> adhivāsayase nagendravanna ca te pravyathate",
    "2871:364": "sthiraṁ manaḥ ||29||",
    "2871:365": "normal;mso-pagination:",
    "2871:366": "none;mso-layout-grid-align:",
    "2871:367": "none;text-autospace:none“>",
    "2871:368": "normal;mso-pagination:",
    "2871:369": "none;mso-layout-grid-align:",
    "2871:370": "none;text-autospace:",
    "2871:371": "none“> śayanāsanapānabhojanaṁ vividhaṁ",
    "2871:372": "cīvaramuttarādharam |",
    "2871:373": "normal;mso-pagination:",
    "2871:374": "none;mso-layout-grid-align:",
    "2871:375": "none;text-autospace:",
    "2871:376": "none“> adhivāsayase nagendravanna ca te pravyathate",
    "2871:377": "mune nirāsravam ||30||",
    "2871:378": "normal;mso-pagination:",
    "2871:379": "none;mso-layout-grid-align:",
    "2871:380": "none;text-autospace:none“>",
    "2871:381": "normal;mso-pagination:",
    "2871:382": "none;mso-layout-grid-align:",
    "2871:383": "none;text-autospace:",
    "2871:384": "none“> na ha śocāsi nātha nāsti me na ca te santi mune",
    "2871:385": "parigrahāḥ |",
    "2871:386": "normal;mso-pagination:",
    "2871:387": "none;mso-layout-grid-align:",
    "2871:388": "none;text-autospace:",
    "2871:389": "none“> asito'si suvākyanirmamḥ parimukto vividhairupadravaiḥ",
    "2871:390": "||31||",
    "2871:391": "normal;mso-pagination:",
    "2871:392": "none;mso-layout-grid-align:",
    "2871:393": "none;text-autospace:none“>",
    "2871:394": "normal;mso-pagination:",
    "2871:395": "none;mso-layout-grid-align:",
    "2871:396": "none;text-autospace:",
    "2871:397": "none“> na ca lābhamavāpya hṛṣyase tadalābhācca layaṁ",
    "2871:398": "na gacchasi |",
    "2871:399": "normal;mso-pagination:",
    "2871:400": "none;mso-layout-grid-align:",
    "2871:401": "none;text-autospace:",
    "2871:402": "none“> avamānamatho titikṣase na ca",
    "2871:403": "sammānamihabhinandasi ||32||",
    "2871:404": "normal;mso-pagination:",
    "2871:405": "none;mso-layout-grid-align:",
    "2871:406": "none;text-autospace:none“>",
    "2871:407": "normal;mso-pagination:",
    "2871:408": "none;mso-layout-grid-align:",
    "2871:409": "none;text-autospace:",
    "2871:410": "none“> kṣatajopamamagracandanaṁ",
    "2871:411": "surabhūmandarasānusambhavam |",
    "2871:412": "normal;mso-pagination:",
    "2871:413": "none;mso-layout-grid-align:",
    "2871:414": "none;text-autospace:",
    "2871:415": "none“> asayo niśitāḥ śarāśca te na vikurvanti manaḥ",
    "2871:416": "kadācana ||33||",
    "2871:417": "normal;mso-pagination:",
    "2871:418": "none;mso-layout-grid-align:",
    "2871:419": "none;text-autospace:none“>",
    "2871:420": "normal;mso-pagination:",
    "2871:421": "none;mso-layout-grid-align:",
    "2871:422": "none;text-autospace:",
    "2871:423": "none“> na kathāṁ kathayasyanarthikāṁ na ca yā",
    "2871:424": "durjanasevitā kathā |",
    "2871:425": "normal;mso-pagination:",
    "2871:426": "none;mso-layout-grid-align:",
    "2871:427": "none;text-autospace:",
    "2871:428": "none“> na [ ca lābhakathā na ] sāmiṣā na ca yā nānumatā mahātmabhiḥ ||34||",
    "2871:429": "normal;mso-pagination:",
    "2871:430": "none;mso-layout-grid-align:",
    "2871:431": "none;text-autospace:none“>",
    "2871:432": "normal;mso-pagination:",
    "2871:433": "none;mso-layout-grid-align:",
    "2871:434": "none;text-autospace:",
    "2871:435": "none“> pravivekakathāḥ sukhāvahāḥ praśamaṁ pravadanti",
    "2871:436": "kevalam |",
    "2871:437": "normal;mso-pagination:",
    "2871:438": "none;mso-layout-grid-align:",
    "2871:439": "none;text-autospace:",
    "2871:440": "none“> kathayasyatidevatāḥ kathāḥ kathitā yā vinayanti",
    "2871:441": "kilviṣam ||35||",
    "2871:442": "normal;mso-pagination:",
    "2871:443": "none;mso-layout-grid-align:",
    "2871:444": "none;text-autospace:none“>",
    "2871:445": "normal;mso-pagination:",
    "2871:446": "none;mso-layout-grid-align:",
    "2871:447": "none;text-autospace:",
    "2871:448": "none“> madhurāṇi ca saṅgatāni ca svabhinītāni ca",
    "2871:449": "sāravanti ca |",
    "2871:450": "normal;mso-pagination:",
    "2871:451": "none;mso-layout-grid-align:",
    "2871:452": "none;text-autospace:",
    "2871:453": "none“> vacanāni mune prabhāṣase jagadarthāya viniṣcitāni",
    "2871:454": "ca ||36||",
    "2871:455": "normal;mso-pagination:",
    "2871:456": "none;mso-layout-grid-align:",
    "2871:457": "none;text-autospace:none“>",
    "2871:458": "normal;mso-pagination:",
    "2871:459": "none;mso-layout-grid-align:",
    "2871:460": "none;text-autospace:",
    "2871:461": "none“> abudhā viparītadarśanāścapalāḥ sāhasikāḥ",
    "2871:462": "priyanvitāḥ |",
    "2871:463": "normal;mso-pagination:",
    "2871:464": "none;mso-layout-grid-align:",
    "2871:465": "none;text-autospace:",
    "2871:466": "none“> piśunāḥ paruṣā śaṭhāśca ye bhagavaṁstaiḥ saha",
    "2871:467": "saṅgataṁ na te ||37||",
    "2871:468": "normal;mso-pagination:",
    "2871:469": "none;mso-layout-grid-align:",
    "2871:470": "none;text-autospace:none“>",
    "2871:471": "normal;mso-pagination:",
    "2871:472": "none;mso-layout-grid-align:",
    "2871:473": "none;text-autospace:",
    "2871:474": "none“> aśaṭhā ṛjavaśca ye narāḥ śucayaḥ satyaratā",
    "2871:475": "jitendriyāḥ |",
    "2871:476": "normal;mso-pagination:",
    "2871:477": "none;mso-layout-grid-align:",
    "2871:478": "none;text-autospace:",
    "2871:479": "none“> satataṁ ca samīkṣyakāriṇo bhagavaṁstaiḥ saha saṅgataṁ",
    "2871:480": "tava ||38||",
    "2871:481": "normal;mso-pagination:",
    "2871:482": "none;mso-layout-grid-align:",
    "2871:483": "none;text-autospace:none“>",
    "2871:484": "normal;mso-pagination:",
    "2871:485": "none;mso-layout-grid-align:",
    "2871:486": "none;text-autospace:",
    "2871:487": "none“> dharmajña nayajña pudralajña tvāṁ vande ṣaḍabhijña",
    "2871:488": "sarvadaiva |",
    "2871:489": "normal;mso-pagination:",
    "2871:490": "none;mso-layout-grid-align:",
    "2871:491": "none;text-autospace:",
    "2871:492": "none“> kṣetrajña mune parāparajña tvāṁ vande śirasā",
    "2871:493": "nayānayajña ||39||",
    "2871:494": "normal;mso-pagination:",
    "2871:495": "none;mso-layout-grid-align:",
    "2871:496": "none;text-autospace:none“>",
    "2871:497": "normal;mso-pagination:",
    "2871:498": "none;mso-layout-grid-align:",
    "2871:499": "none;text-autospace:",
    "2871:500": "none“> pṛthumapi samīkṣate guṇaṁ [ tava ] parikathito hi māa guṇaikadeśaḥ |",
    "2871:501": "normal;mso-pagination:",
    "2871:502": "none;mso-layout-grid-align:",
    "2871:503": "none;text-autospace:",
    "2871:504": "none“> na tava guṇamahārṇavasya pāraṁ jagati",
    "2871:505": "pumānadhigantumārya śaktaḥ ||40||",
    "2871:506": "normal;mso-pagination:",
    "2871:507": "none;mso-layout-grid-align:",
    "2871:508": "none;text-autospace:none“>",
    "2871:509": "normal;mso-pagination:",
    "2871:510": "none;mso-layout-grid-align:",
    "2871:511": "none;text-autospace:",
    "2871:512": "none“> [ iti bhagavatstutiḥ|| ]"
}