{
    "858:0": "pañcatathāgatastutigāthā",
    "858:1": "om namaḥ śrīpañcabhūtāntāya |",
    "858:2": "namaste varadarājāgra bhūtakoṭi namostu te |",
    "858:3": "namaste śūnyatāgarbha buddhabodhi namo'stu te || 1 ||",
    "858:4": "buddharāga namaste'stu buddhakāma namo'stu te |",
    "858:5": "buddhaprīti namaste'stu buddhamoda namo namaḥ || 2 ||",
    "858:6": "buddhasmita namastubhyaṃ buddhahāsa namo namaḥ |",
    "858:7": "buddhavāca namaste'stu buddhabhāva namo namaḥ || 3 ||",
    "858:8": "abhavodbhava namaste'stu namaste buddhasaṃbhava |",
    "858:9": "gaganodbhava namaste'stu namaste jñānasaṃbhava || 4 ||",
    "858:10": "māyājāla namastubhyaṃ namaste buddhanāyaka |",
    "858:11": "namaste sarvasarvebhyo jñānakāya namo'stu te |",
    "858:12": "śrī pañcatathāgatastutigāthā samāptā |"
}