{
  "SA05n855:0": "guhyeśvarīstotram",
  "SA05n855:1": "bhagavati bahurūpe nirvikāre nirañje",
  "SA05n855:2": "nimitanikhilarūpe niścayātītarūpe |",
  "SA05n855:3": "akhilanigamapāre nityanityasvabhāve",
  "SA05n855:4": "caraṇakamalayugmaṃ naumi devi tvadīyam || 1 ||",
  "SA05n855:5": "hasitamukhaśaśāṅkaṃ jyotsnayā rātribhūtaṃ",
  "SA05n855:6": "daśaśatakiraṇodyadvaktramādhyandinaṃ te |",
  "SA05n855:7": "aruṇavadanaśobhā audayī cāstakāle",
  "SA05n855:8": "caraṇakamalayugmaṃ naumi devi tvadīyam || 2 ||",
  "SA05n855:9": "galajaṭharapadeṣu svargamartyāhilokāḥ",
  "SA05n855:10": "sakalabhagaṇadantā romarājī drumāste |",
  "SA05n855:11": "giraya iva nitambā raktaśuklāḥ samudrā-",
  "SA05n855:12": "ścaraṇakamalayugmaṃ naumi devi tvadīyam || 3 ||",
  "SA05n855:13": "pavanadahanavegāḥ śvāsapraśvāsa eva",
  "SA05n855:14": "pralayaprabhavakālo mīlanonmīlanābhyām |",
  "SA05n855:15": "dvidaśatapanabhūtā bhīmavaktrāstvadīyā-",
  "SA05n855:16": "ścaraṇakamalayugmaṃ naumi devi tvadīyam || 4 ||",
  "SA05n855:17": "anupamatanudehaṃ vyāpyamānaṃ samantā-",
  "SA05n855:18": "nnikhilanigamasāraṃ darśayan devi divyam |",
  "SA05n855:19": "tribhuvanamakhilaṃ te darśitumeti buddhi-",
  "SA05n855:20": "ścaraṇakamalayugmaṃ naumi devi tvadīyam || 5 ||",
  "SA05n855:21": "vidhimukhavibudhāste kiṃkarāḥ pādasaṃsthā",
  "SA05n855:22": "mukuṭavidhṛtabuddhe sarvamārgā hi śuddhe |",
  "SA05n855:23": "kimu tava mahimānaṃ varṇaye guhyadevi",
  "SA05n855:24": "caraṇakamalayugmaṃ naumi devi tvadīyam || 6 ||",
  "SA05n855:25": "anupaṭhati samāptau pūjane bhaktimān yo",
  "SA05n855:26": "nutimati vitagoti spaṣṭamevāsya buddhiḥ |",
  "SA05n855:27": "sakalajanajananyā bhaktisampattimuccaiḥ",
  "SA05n855:28": "karatalavaśagāstāḥ siddhipuṣṭayau labhante || 7 ||",
  "SA05n855:29": "śrī svayaṃbhūpurāṇoddhṛtaṃ mañjunāthakṛtaṃ",
  "SA05n855:30": "guhyeśvarīstotraṃ samāptam |"
}